rm adzi140.4ad

if [ -f "toolbar_adzi140_zh_CN.4tb" ]
then
  rm $ERP/cfg/4tb/toolbar_adzi140_zh_CN.4tb
fi

if [ -f "toolbar_adzi140_zh_TW.4tb" ]
then
  rm $ERP/cfg/4tb/toolbar_adzi140_zh_TW.4tb
fi

cp toolbar_adzi140_zh_CN.4tb $ERP/cfg/4tb
