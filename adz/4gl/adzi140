rm toolbar_adzi140_zh_TW.4tb
rm toolbar_adzi140_vi_VN.4tb

if [ -f "adzi140_zh_CN.4tm" ]
then
  rm $ERP/adz/4tm/zh_CN/adzi140.4tm
fi

if [ -f "adzi140_zh_TW.4tm" ]
then
  rm $ERP/adz/4tm/zh_TW/adzi140.4tm
fi

if [ -f "adzi140_vi_VN.4tm" ]
then
  rm $ERP/adz/4tm/vi_VN/adzi140.4tm
fi

if [ -f "adzi140_en_US.4tm" ]
then
  rm $ERP/adz/4tm/en_US/adzi140.4tm
fi

cp adzi140_zh_CN.4tm $ERP/adz/4tm/zh_CN/adzi140.4tm
cp adzi140_zh_TW.4tm $ERP/adz/4tm/zh_TW/adzi140.4tm
cp adzi140_vi_VN.4tm $ERP/adz/4tm/vi_VN/adzi140.4tm
cp adzi140_en_US.4tm $ERP/adz/4tm/en_US/adzi140.4tm

rm adzi140_zh_CN.4tm 
rm adzi140_zh_TW.4tm
rm adzi140_vi_VN.4tm
rm adzi140_en_US.4tm

cd ../4fd
