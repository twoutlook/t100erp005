   {</Local define>}
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_dzde_m.dzde001 IS NULL
      #key2
   OR g_dzde_m.dzde002 IS NULL


   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
    SELECT UNIQUE dzde001,dzde002,dzde003,dzdestus,dzdemodu,dzdedate,dzdeoriu,dzdeorid,dzdeuser,dzdedept,dzdebuid
 INTO g_dzde_m.dzde001,g_dzde_m.dzde002,g_dzde_m.dzde003,g_dzde_m.dzdestus,g_dzde_m.dzdemodu,g_dzde_m.dzdedate,g_dzde_m.dzdeoriu,g_dzde_m.dzdeorid,g_dzde_m.dzdeuser,g_dzde_m.dzdedept,g_dzde_m.dzdebuid
 FROM dzde_t
 WHERE dzde001 = g_dzde_m.dzde001 AND dzde002 = g_dzde_m.dzde002
   BEGIN WORK
   
   
 
   OPEN adzi452_cl USING g_dzde_m.dzde001
      #key2
                                                       ,g_dzde_m.dzde002


   IF STATUS THEN
      CALL cl_err("OPEN adzi452_cl:", STATUS, 1)
      CLOSE adzi452_cl
      ROLLBACK WORK
      RETURN
   END IF
 
   FETCH adzi452_cl INTO g_dzde_m.dzde001,g_dzde_m.dzde002,g_dzde_m.dzde003,g_dzde_m.dzdestus,g_dzde_m.dzdemodu,g_dzde_m.modu_desc,g_dzde_m.dzdedate,g_dzde_m.dzdeoriu,g_dzde_m.oriu_desc,g_dzde_m.dzdeorid,g_dzde_m.orid_desc,g_dzde_m.dzdeuser,g_dzde_m.user_desc,g_dzde_m.dzdedept,g_dzde_m.dept_desc,g_dzde_m.dzdebuid              # 鎖住將被更改或取消的資料
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_dzde_m.dzde001,SQLCA.sqlcode,0)          #資料被他人LOCK
      ROLLBACK WORK
      RETURN
   END IF
 
   CALL adzi452_show()
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      INITIALIZE g_doc.* TO NULL         
      LET g_doc.column1 = "dzde001"       
      LET g_doc.value1 = g_dzde_m.dzde001     
      CALL cl_del_doc()   
      
      #add-point:單頭刪除前
      {<point name="delete.head.b_delete"/>}
      #end add-point
  
      #資料備份
      LET g_dzde001_t = g_dzde_m.dzde001
      #key2
      LET g_dzde002_t = g_dzde_m.dzde002


      
      DELETE FROM dzde_t
       WHERE  dzde001 = g_dzde_m.dzde001
      #key2
         AND dzde002 = g_dzde_m.dzde002


      
      IF SQLCA.sqlcode THEN
         CALL cl_err(g_dzde_m.dzde001,SQLCA.sqlcode,0) 
         ROLLBACK WORK
         RETURN
      END IF
      
      #add-point:單頭刪除後
      {<point name="delete.head.a_delete"/>}
      #end add-point
  
      
      
      #add-point:單身刪除前
      {<point name="delete.body.b_delete"/>}
      #end add-point
      
      DELETE FROM dzdf_t
       WHERE  dzdf001 = g_dzde_m.dzde001
      #key2
         AND  = g_dzde_m.dzde002


 
      IF SQLCA.sqlcode THEN
         CALL cl_err("dzdf_t",SQLCA.sqlcode,0) 
         ROLLBACK WORK
         RETURN
      END IF                                                           
                                                               

                                                               
      #add-point:單身刪除後
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
      CLEAR FORM
      CALL g_dzdf_d.clear() 

     
      CALL adzi452_ui_browser_refresh()  
      CALL adzi452_ui_headershow()  
      CALL adzi452_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL adzi452_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         CALL adzi452_browser_fill("first")
      END IF
       
   END IF
 
   CLOSE adzi452_cl
   COMMIT WORK
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_dzde_m.dzde001,'D')
    
