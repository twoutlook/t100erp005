#+ 資料修改
PRIVATE FUNCTION adzi452_modify()
   {<Local define>}
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   {</Local define>}
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_dzde_m.dzde001 IS NULL
      #key2
   OR g_dzde_m.dzde002 IS NULL


   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
    SELECT UNIQUE dzde001,dzde002,dzde003,dzdestus,dzdemodu,dzdedate,dzdeoriu,dzdeorid,dzdeuser,dzdedept,dzdebuid
 INTO g_dzde_m.dzde001,g_dzde_m.dzde002,g_dzde_m.dzde003,g_dzde_m.dzdestus,g_dzde_m.dzdemodu,g_dzde_m.dzdedate,g_dzde_m.dzdeoriu,g_dzde_m.dzdeorid,g_dzde_m.dzdeuser,g_dzde_m.dzdedept,g_dzde_m.dzdebuid
 FROM dzde_t
 WHERE dzde001 = g_dzde_m.dzde001 AND dzde002 = g_dzde_m.dzde002
 
   ERROR ""
  
   LET g_dzde001_t = g_dzde_m.dzde001
      #key2
   LET g_dzde002_t = g_dzde_m.dzde002


   BEGIN WORK
   
   OPEN adzi452_cl USING g_dzde_m.dzde001
      #key2
                                                       ,g_dzde_m.dzde002


   IF STATUS THEN
      CALL cl_err("OPEN adzi452_cl:", STATUS, 1)
      CLOSE adzi452_cl
      ROLLBACK WORK
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH adzi452_cl INTO g_dzde_m.dzde001,g_dzde_m.dzde002,g_dzde_m.dzde003,g_dzde_m.dzdestus,g_dzde_m.dzdemodu,g_dzde_m.modu_desc,g_dzde_m.dzdedate,g_dzde_m.dzdeoriu,g_dzde_m.oriu_desc,g_dzde_m.dzdeorid,g_dzde_m.orid_desc,g_dzde_m.dzdeuser,g_dzde_m.user_desc,g_dzde_m.dzdedept,g_dzde_m.dept_desc,g_dzde_m.dzdebuid
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_dzde_m.dzde001,SQLCA.sqlcode,0)
      CLOSE adzi452_cl
      ROLLBACK WORK
      RETURN
   END IF
   
   
 
   CALL adzi452_show()
   WHILE TRUE
      LET g_dzde001_t = g_dzde_m.dzde001
      #key2
      LET g_dzde002_t = g_dzde_m.dzde002


      LET g_dzde_m.dzdemodu = g_user
      LET g_dzde_m.dzdedate = cl_get_current()
 
      CALL adzi452_input("u")     #欄位更改
 
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_dzde_m.* = g_dzde_m_t.*
         CALL adzi452_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_dzde_m.dzde001 != g_dzde001_t 
      #key2
      OR g_dzde_m.dzde002 != g_dzde002_t 


      THEN
         BEGIN WORK
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update"/>}
         #end add-point
         
         #更新單身key值
         UPDATE dzdf_t SET dzdf001 = g_dzde_m.dzde001
      #key2
                                      , = g_dzde_m.dzde002


          WHERE  dzdf001 = g_dzde001_t
      #key2
            AND  = g_dzde002_t


         IF SQLCA.sqlcode THEN
             CALL cl_err("",SQLCA.sqlcode,1) 
             ROLLBACK WORK
             CONTINUE WHILE
         END IF
         

         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
         #UPDATE 多語言table key值
         

         COMMIT WORK
      END IF
      
      EXIT WHILE
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_used_modified_record(g_dzde_m.dzde001,g_site) THEN 
      ROLLBACK WORK
   END IF
 
   CLOSE adzi452_cl
   COMMIT WORK
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_dzde_m.dzde001,'U')
 
END FUNCTION   
 
 
