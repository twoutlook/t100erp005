#+ 資料複製
PRIVATE FUNCTION adzi452_reproduce()
   {<Local define>}
   DEFINE l_newno     LIKE dzde_t.dzde001 
   DEFINE l_oldno     LIKE dzde_t.dzde001 
      #key2
   DEFINE l_newno02     LIKE dzde_t.dzde002 
   DEFINE l_oldno02     LIKE dzde_t.dzde002 


   DEFINE l_master    RECORD LIKE dzde_t.*
   DEFINE l_detail    RECORD LIKE dzdf_t.*
   DEFINE l_cnt       LIKE type_t.num5
   {</Local define>}
   #add-point:reproduce段define
   {<point name="reproduce.define"/>}
   #end add-point   
 
   IF g_dzde_m.dzde001 IS NULL
      #key2
   OR g_dzde_m.dzde002 IS NULL


   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
    
   LET g_before_input_done = FALSE
   CALL adzi452_set_entry('a')
   LET g_before_input_done = TRUE
 
   CALL cl_set_head_visible("","YES")
 
   INPUT l_newno   # FROM
      #key2
         ,l_newno02


    FROM dzde001 
      #key2
         ,dzde002 


      BEFORE INPUT
 
      AFTER FIELD dzde001 
         IF l_newno IS NULL THEN
            NEXT FIELD CURRENT
         END IF
         #add-point:AFTER FIELD dzde001
         {<point name="reproduce.a.dzde001" />}
         #end add-point
         
      #key2
      AFTER FIELD dzde002 
         IF l_newno IS NULL THEN
            NEXT FIELD CURRENT
         END IF
         #add-point:AFTER FIELD dzde002
         {<point name="reproduce.a.dzde002" />}
         #end add-point


            
      AFTER INPUT
         #確定該key值是否有重複定義
         LET l_cnt = 0
         SELECT COUNT(*) INTO l_cnt FROM dzde_t 
          WHERE  dzde001 = l_newno
      #key2
            AND dzde002 = l_newno02


         IF l_cnt > 0 THEN
            CALL cl_err("Reproduce","std-00006",1)
            #NEXT FIELD dzde001 
         END IF
 
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE INPUT
   END INPUT
   
   IF INT_FLAG OR l_newno IS NULL THEN
      LET INT_FLAG = 0
      RETURN
   END IF
   
   BEGIN WORK
 
   SELECT * INTO l_master.* FROM dzde_t 
    WHERE  dzde001 = g_dzde_m.dzde001
      #key2
      AND dzde002 = g_dzde_m.dzde002


   LET l_master.dzde001 = l_newno
      #key2
   LET l_master.dzde002 = l_newno02


   
   LET l_master.dzdeuser = g_user
   LET l_master.dzdedept = g_dept
   LET l_master.dzdebuid = cl_get_current()
   LET l_master.dzdeoriu = g_user 
   LET l_master.dzdeorid = g_dept 
   LET l_master.dzdemodu = g_user
   LET l_master.dzdedate = cl_get_current()
   LET l_master.dzdestus = 'Y'
   
   #add-point:單頭複製前
   {<point name="reproduce.head.b_insert"/>}
   #end add-point
   
   INSERT INTO dzde_t VALUES (l_master.*) #複製單頭        
   IF SQLCA.sqlcode THEN
      CALL cl_err("dzde_t",SQLCA.sqlcode,1)
      ROLLBACK WORK
      RETURN
   END IF
   
   #add-point:單頭複製後
   {<point name="reproduce.head.a_insert"/>}
   #end add-point
   
   LET g_sql = "SELECT * FROM dzdf_t WHERE  ",
               " dzdf001 = '",g_dzde_m.dzde001,"'"
      #key2
               ," AND  = '",g_dzde_m.dzde002,"'"


   DECLARE adzi452_reproduce CURSOR FROM g_sql
 
   FOREACH adzi452_reproduce INTO l_detail.*
      LET l_detail.dzdf001 = l_newno
      #key2
      LET l_detail. = l_newno02


  
      #add-point:單身複製前
      {<point name="reproduce.body.b_insert"/>}
      #end add-point
      
      INSERT INTO dzdf_t VALUES (l_detail.*) #複製單身
      IF SQLCA.sqlcode THEN
         CALL cl_err('Insert error!',SQLCA.sqlcode,1)
         ROLLBACK WORK
         RETURN
      END IF
 
      #add-point:單身複製後
      {<point name="reproduce.body.a_insert"/>}
      #end add-point
      
   END FOREACH
   
   COMMIT WORK
   ERROR 'ROW(',l_newno,') O.K'
   LET l_oldno = g_dzde_m.dzde001
      #key2
   LET l_oldno02 = g_dzde_m.dzde002


   
   SELECT dzde001,dzde002,dzde003,dzdestus,dzdemodu,dzdedate,dzdeoriu,dzdeorid,dzdeuser,dzdedept,dzdebuid INTO g_dzde_m.dzde001,g_dzde_m.dzde002,g_dzde_m.dzde003,g_dzde_m.dzdestus,g_dzde_m.dzdemodu,g_dzde_m.dzdedate,g_dzde_m.dzdeoriu,g_dzde_m.dzdeorid,g_dzde_m.dzdeuser,g_dzde_m.dzdedept,g_dzde_m.dzdebuid FROM dzde_t 
    WHERE  dzde001 = l_newno
      #key2
      AND dzde002 = l_newno02


      
   CALL adzi452_show()
   LET g_dzde_m.dzde001 = l_oldno
      #key2
   LET g_dzde_m.dzde002 = l_oldno02


   
   SELECT dzde001,dzde002,dzde003,dzdestus,dzdemodu,dzdedate,dzdeoriu,dzdeorid,dzdeuser,dzdedept,dzdebuid INTO g_dzde_m.dzde001,g_dzde_m.dzde002,g_dzde_m.dzde003,g_dzde_m.dzdestus,g_dzde_m.dzdemodu,g_dzde_m.dzdedate,g_dzde_m.dzdeoriu,g_dzde_m.dzdeorid,g_dzde_m.dzdeuser,g_dzde_m.dzdedept,g_dzde_m.dzdebuid FROM dzde_t 
    WHERE  dzde001 = g_dzde_m.dzde001
      #key2
      AND dzde002 = g_dzde_m.dzde002


   CALL adzi452_show()
 
   DISPLAY BY NAME g_dzde_m.dzde001
      #key2
   DISPLAY BY NAME g_dzde_m.dzde002


   
