PRIVATE FUNCTION adzi452_b_fill()
   {<Local define>}
   DEFINE p_wc2      STRING
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_dzdf_d.clear()    #g_dzdf_d 單頭及單身 

 
   LET g_sql = "SELECT dzdf002 FROM dzdf_t",    
               "",
               " WHERE dzdf001=?"
 
   IF NOT cl_null(g_wc_table1) THEN
      LET g_sql = g_sql CLIPPED, " AND ", g_wc_table1 CLIPPED
   END IF
 
   LET g_sql = g_sql, " ORDER BY dzdf_t.dzdf002"
 
   PREPARE adzi452_pb FROM g_sql
