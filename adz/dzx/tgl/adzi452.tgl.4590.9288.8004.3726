   {</Local define>}
   #add-point:input段define
   {<point name="input.define"/>}
   #end add-point    
 
   CALL cl_set_head_visible("","YES")  
 
   LET l_insert = FALSE
   LET g_action_choice = ""
 
   LET g_forupd_sql = "SELECT dzdf002 FROM dzdf_t WHERE dzdf001=? AND dzdf002=? FOR UPDATE"
   LET g_forupd_sql = cl_forupd_sql(g_forupd_sql)
   DECLARE adzi452_bcl CURSOR FROM g_forupd_sql
   

 
   LET l_allow_insert = cl_detail_input_auth("insert")
   LET l_allow_delete = cl_detail_input_auth("delete")
   LET g_qryparam.state = 'i'
 
   DISPLAY BY NAME g_dzde_m.dzde001,g_dzde_m.dzde002,g_dzde_m.dzde003,g_dzde_m.dzdestus,g_dzde_m.dzdemodu,g_dzde_m.dzdedate,g_dzde_m.dzdeoriu,g_dzde_m.dzdeorid,g_dzde_m.dzdeuser,g_dzde_m.dzdedept,g_dzde_m.dzdebuid
   
