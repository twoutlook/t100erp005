      LET g_detail_idx = g_curr_diag.getCurrentRow("s_detail1")
      IF g_detail_idx > g_dzdf_d.getLength() THEN
         LET g_detail_idx = g_dzdf_d.getLength()
      END IF
	  IF g_detail_idx = 0 AND g_dzdf_d.getLength() <> 0 THEN
	     LET g_detail_idx = 1
	  END IF
      DISPLAY g_detail_idx TO FORMONLY.idx
      DISPLAY g_dzdf_d.getLength() TO FORMONLY.cnt
   END IF
   

 
END FUNCTION
 
   
