#+ 單頭資料重新顯示及單身資料重抓
PRIVATE FUNCTION adzi452_show()
   {<Local define>}
   DEFINE l_ac_t    LIKE type_t.num5
   DEFINE l_sql     STRING
   {</Local define>}
   #add-point:show段define
   {<point name="show.define"/>}
   #end add-point  
 
   #add-point:show段之前
   {<point name="show.before"/>}
   #end add-point
   
   LET g_dzde_m_t.* = g_dzde_m.*      #保存單頭舊值
  
   DISPLAY BY NAME g_dzde_m.dzde001,g_dzde_m.dzde002,g_dzde_m.dzde003,g_dzde_m.dzdestus,g_dzde_m.dzdemodu,g_dzde_m.modu_desc,g_dzde_m.dzdedate,g_dzde_m.dzdeoriu,g_dzde_m.oriu_desc,g_dzde_m.dzdeorid,g_dzde_m.orid_desc,g_dzde_m.dzdeuser,g_dzde_m.user_desc,g_dzde_m.dzdedept,g_dzde_m.dept_desc,g_dzde_m.dzdebuid
    
   CASE g_dzde_m.dzdestus
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "authstatus/valid.png")
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "authstatus/void.png")
      WHEN "X"
         CALL gfrm_curr.setElementImage("statechange", "authstatus/invalid.png")
   END CASE
  
   CALL adzi452_b_fill()                 #單身
     
   #帶出預設欄位之值
   LET g_dzde_m.modu_desc = cl_get_username(g_dzde_m.dzdemodu)
   LET g_dzde_m.oriu_desc = cl_get_username(g_dzde_m.dzdeoriu)
   LET g_dzde_m.user_desc = cl_get_username(g_dzde_m.dzdeuser)
   LET g_dzde_m.dept_desc = cl_get_deptname(g_dzde_m.dzdedept)
   LET g_dzde_m.orid_desc = cl_get_deptname(g_dzde_m.dzdeorid)
 
   LET l_ac_t = l_ac
   
   #讀入ref值(單頭)
    
   
   #add-point:show段reference
   {<point name="show.head.reference"/>}
   #end add-point
   
   #讀入ref值(單身)
   FOR l_ac = 1 TO g_dzdf_d.getLength()
       
      #add-point:show段單身reference
      {<point name="show.body.reference"/>}
      #end add-point
   END FOR

   
    
   
