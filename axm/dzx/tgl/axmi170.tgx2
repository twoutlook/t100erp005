   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code = "std-00003" 
      LET g_errparam.popup = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_xmia001_t = g_xmia_m.xmia001
 
   CALL s_transaction_begin()
   
   OPEN axmi170_cl USING g_enterprise,g_xmia_m.xmia001
   IF SQLCA.SQLCODE THEN   #(ver:78)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN axmi170_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE   #(ver:78)
      LET g_errparam.popup = TRUE 
      CLOSE axmi170_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE axmi170_master_referesh USING g_xmia_m.xmia001 INTO g_xmia_m.xmia001,g_xmia_m.xmia002,g_xmia_m.xmia016, 
       g_xmia_m.xmia003,g_xmia_m.xmia004,g_xmia_m.xmia005,g_xmia_m.xmia006,g_xmia_m.xmia007,g_xmia_m.xmia008, 
       g_xmia_m.xmia009,g_xmia_m.xmia017,g_xmia_m.xmiastus,g_xmia_m.xmia010,g_xmia_m.xmia011,g_xmia_m.xmia013, 
       g_xmia_m.xmia014,g_xmia_m.xmia012,g_xmia_m.xmia015,g_xmia_m.xmiaownid,g_xmia_m.xmiaowndp,g_xmia_m.xmiacrtid, 
       g_xmia_m.xmiacrtdp,g_xmia_m.xmiacrtdt,g_xmia_m.xmiamodid,g_xmia_m.xmiamoddt,g_xmia_m.xmia004_desc, 
       g_xmia_m.xmia005_desc,g_xmia_m.xmia008_desc,g_xmia_m.xmiaownid_desc,g_xmia_m.xmiaowndp_desc,g_xmia_m.xmiacrtid_desc, 
       g_xmia_m.xmiacrtdp_desc,g_xmia_m.xmiamodid_desc
   
   #檢查是否允許此動作
   IF NOT axmi170_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_xmia_m_mask_o.* =  g_xmia_m.*
   CALL axmi170_xmia_t_mask()
   LET g_xmia_m_mask_n.* =  g_xmia_m.*
   
   
   
   #add-point:modify段show之前 name="modify.before_show"
   {<point name="modify.before_show"/>}
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
 
   
   CALL axmi170_show()
   #add-point:modify段show之後 name="modify.after_show"
   {<point name="modify.after_show"/>}
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
 
 
    
   WHILE TRUE
      LET g_xmia001_t = g_xmia_m.xmia001
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_xmia_m.xmiamodid = g_user 
LET g_xmia_m.xmiamoddt = cl_get_current()
LET g_xmia_m.xmiamodid_desc = cl_get_username(g_xmia_m.xmiamodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
