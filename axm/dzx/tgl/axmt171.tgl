   LET g_forupd_sql = " SELECT xmic001,'',xmic002,xmic003,'',xmic004,'',xmic005,'',xmic006,'',xmic007, 
       '',xmicstus,xmicownid,'',xmicowndp,'',xmiccrtid,'',xmiccrtdp,'',xmiccrtdt,xmicmodid,'',xmicmoddt, 
       xmiccnfid,'',xmiccnfdt", 
                      " FROM xmic_t",
                      " WHERE xmicent= ? AND xmic001=? AND xmic002=? AND xmic003=? AND xmic004=? AND  
                          xmic005=? AND xmic006=? FOR UPDATE"
   #add-point:SQL_define name="main.after_define_sql"
   {<point name="main.after_define_sql"/>}
   #end add-point
