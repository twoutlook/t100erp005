         xmid001 = ps_keys_bak[1] AND xmid002 = ps_keys_bak[2] AND xmid003 = ps_keys_bak[3] AND xmid004 = ps_keys_bak[4] AND xmid005 = ps_keys_bak[5] AND xmid006 = ps_keys_bak[6] AND xmid007 = ps_keys_bak[7] AND xmid008 = ps_keys_bak[8] AND xmid009 = ps_keys_bak[9] AND xmid010 = ps_keys_bak[10] AND xmid011 = ps_keys_bak[11]
