   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE axmt171_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.xmic001,t0.xmic002,t0.xmic003,t0.xmic004,t0.xmic005,t0.xmic006,t0.xmic007, 
       t0.xmicstus,t0.xmicownid,t0.xmicowndp,t0.xmiccrtid,t0.xmiccrtdp,t0.xmiccrtdt,t0.xmicmodid,t0.xmicmoddt, 
       t0.xmiccnfid,t0.xmiccnfdt,t1.xmial003 ,t2.ooefl003 ,t3.ooefl003 ,t4.ooag011 ,t5.ooail003 ,t6.ooag011 , 
