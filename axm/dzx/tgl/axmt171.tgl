   #將新增的資料併入搜尋條件中
   LET g_xmic001_t = g_xmic_m.xmic001
   LET g_xmic002_t = g_xmic_m.xmic002
   LET g_xmic003_t = g_xmic_m.xmic003
   LET g_xmic004_t = g_xmic_m.xmic004
   LET g_xmic005_t = g_xmic_m.xmic005
   LET g_xmic006_t = g_xmic_m.xmic006
 
   
   #組合新增資料的條件
   LET g_add_browse = " xmicent = " ||g_enterprise|| " AND",
                      " xmic001 = '", g_xmic_m.xmic001, "' "
                      ," AND xmic002 = '", g_xmic_m.xmic002, "' "
                      ," AND xmic003 = '", g_xmic_m.xmic003, "' "
                      ," AND xmic004 = '", g_xmic_m.xmic004, "' "
                      ," AND xmic005 = '", g_xmic_m.xmic005, "' "
                      ," AND xmic006 = '", g_xmic_m.xmic006, "' "
 
                      
   #add-point:組合新增資料的條件後 name="insert.after.add_browse"
   {<point name="insert.after.add_browse"/>}
   #end add-point
      
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
