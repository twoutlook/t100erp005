{<section id="axmt171.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION axmt171_b_fill()
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   {<point name="b_fill.define_customerization" edit="c" mark="Y"/>}
   #end add-point     
   DEFINE p_wc2      STRING
   DEFINE li_idx     LIKE type_t.num10
   #add-point:b_fill段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   {<point name="b_fill.define"/>} 
   #end add-point     
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   {<point name="b_fill.pre_function"/>}
   #end add-point
   
   #清空第一階單身
   CALL g_xmid_d.clear()
   CALL g_xmid2_d.clear()
   CALL g_xmid3_d.clear()
 
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF axmt171_fill_chk(1) THEN
      #切換上下筆時不重組SQL
      IF (g_action_choice = "query" OR cl_null(g_action_choice))
      #add-point:b_fill段long_sql_if name="b_fill.long_sql_if"
      {<point name="b_fill.long_sql_if"/>}
      #end add-point
      THEN
         LET g_sql = "SELECT  DISTINCT xmid007,xmid008,xmid010,xmid009,xmid014,xmid011,xmid015,xmid007, 
             xmid008,xmid010,xmid009,xmid011,xmid018,xmid019,xmid007,xmid008,xmid010,xmid009,xmid011, 
             xmid016,xmid017,xmid020,xmid021  FROM xmid_t",   
                     " INNER JOIN xmic_t ON xmicent = " ||g_enterprise|| " AND xmic001 = xmid001 ",
                     " AND xmic002 = xmid002 ",
                     " AND xmic003 = xmid003 ",
                     " AND xmic004 = xmid004 ",
                     " AND xmic005 = xmid005 ",
                     " AND xmic006 = xmid006 ",
 
                     #"",
                     
                     "",
                     #下層單身所需的join條件
 
                     
                     " WHERE xmident=? AND xmid001=? AND xmid002=? AND xmid003=? AND xmid004=? AND xmid005=? AND xmid006=?"
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before name="b_fill.body.fill_sql"
         {<point name="b_fill.body.fill_sql"/>}
         #end add-point
         IF NOT cl_null(g_wc2_table1) THEN
            LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY xmid_t.xmid007,xmid_t.xmid008,xmid_t.xmid009,xmid_t.xmid010,xmid_t.xmid011"
         
         #add-point:單身填充控制 name="b_fill.sql"
         {<point name="b_fill.sql"/>}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE axmt171_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR axmt171_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
   #  OPEN b_fill_cs USING g_enterprise,g_xmic_m.xmic001,g_xmic_m.xmic002,g_xmic_m.xmic003,g_xmic_m.xmic004,g_xmic_m.xmic005,g_xmic_m.xmic006   #(ver:78)
                                               
      FOREACH b_fill_cs USING g_enterprise,g_xmic_m.xmic001,g_xmic_m.xmic002,g_xmic_m.xmic003,g_xmic_m.xmic004, 
          g_xmic_m.xmic005,g_xmic_m.xmic006 INTO g_xmid_d[l_ac].xmid007,g_xmid_d[l_ac].xmid008,g_xmid_d[l_ac].xmid010, 
          g_xmid_d[l_ac].xmid009,g_xmid_d[l_ac].xmid014,g_xmid_d[l_ac].xmid011,g_xmid_d[l_ac].xmid015, 
          g_xmid2_d[l_ac].xmid007,g_xmid2_d[l_ac].xmid008,g_xmid2_d[l_ac].xmid010,g_xmid2_d[l_ac].xmid009, 
          g_xmid2_d[l_ac].xmid011,g_xmid2_d[l_ac].xmid018,g_xmid2_d[l_ac].xmid019,g_xmid3_d[l_ac].xmid007, 
          g_xmid3_d[l_ac].xmid008,g_xmid3_d[l_ac].xmid010,g_xmid3_d[l_ac].xmid009,g_xmid3_d[l_ac].xmid011, 
          g_xmid3_d[l_ac].xmid016,g_xmid3_d[l_ac].xmid017,g_xmid3_d[l_ac].xmid020,g_xmid3_d[l_ac].xmid021  
            #(ver:78)
         IF SQLCA.SQLCODE THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code = SQLCA.SQLCODE 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充 name="b_fill.fill"
         {<point name="b_fill.fill"/>}
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code = 9035 
               LET g_errparam.popup = TRUE 
               CALL cl_err()
            END IF
            EXIT FOREACH
         END IF
         
         LET l_ac = l_ac + 1
      END FOREACH
      LET g_error_show = 0
   
   END IF
    
 
   
   #add-point:browser_fill段其他table處理 name="browser_fill.other_fill"
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_xmid_d.deleteElement(g_xmid_d.getLength())
   CALL g_xmid2_d.deleteElement(g_xmid2_d.getLength())
   CALL g_xmid3_d.deleteElement(g_xmid3_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE axmt171_pb
 
   
   LET li_idx = l_ac
   
   #遮罩相關處理
   FOR l_ac = 1 TO g_xmid_d.getLength()
      LET g_xmid_d_mask_o[l_ac].* =  g_xmid_d[l_ac].*
      CALL axmt171_xmid_t_mask()
      LET g_xmid_d_mask_n[l_ac].* =  g_xmid_d[l_ac].*
   END FOR
   
   LET g_xmid2_d_mask_o.* =  g_xmid2_d.*
   FOR l_ac = 1 TO g_xmid2_d.getLength()
      LET g_xmid2_d_mask_o[l_ac].* =  g_xmid2_d[l_ac].*
      CALL axmt171_xmid_t_mask()
      LET g_xmid2_d_mask_n[l_ac].* =  g_xmid2_d[l_ac].*
   END FOR
   LET g_xmid3_d_mask_o.* =  g_xmid3_d.*
   FOR l_ac = 1 TO g_xmid3_d.getLength()
      LET g_xmid3_d_mask_o[l_ac].* =  g_xmid3_d[l_ac].*
      CALL axmt171_xmid_t_mask()
      LET g_xmid3_d_mask_n[l_ac].* =  g_xmid3_d[l_ac].*
   END FOR
 
   
   LET l_ac = li_idx
   
   CALL cl_ap_performance_next_end()
 
END FUNCTION

