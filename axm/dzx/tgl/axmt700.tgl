            CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
            CALL axmt700_set_act_visible()   
            CALL axmt700_set_act_no_visible()
            IF NOT (g_xmfo_m.xmfodocno IS NULL
 
