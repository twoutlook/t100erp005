      #寫入修改者/修改日期資訊(單頭)
      LET g_xmfo_m.xmfomodid = g_user 
LET g_xmfo_m.xmfomoddt = cl_get_current()
LET g_xmfo_m.xmfomodid_desc = cl_get_username(g_xmfo_m.xmfomodid)
      
