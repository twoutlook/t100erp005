         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE xmae_t SET (xmaemodid,xmaemoddt) = (g_xmae_m.xmaemodid,g_xmae_m.xmaemoddt)
       WHERE xmaeent = g_enterprise AND xmae001 = g_xmae001_t
