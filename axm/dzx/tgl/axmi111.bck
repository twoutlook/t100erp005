       g_xmae_m.xmaecrtdp_desc,g_xmae_m.xmaemodid_desc
 
   #檢查是否允許此動作
   IF NOT axmi111_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
