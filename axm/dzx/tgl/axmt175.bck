          g_xmii3_d[l_ac].xmii008,g_xmii3_d[l_ac].xmii012,g_xmii3_d[l_ac].xmii013,g_xmii3_d[l_ac].xmii011, 
          g_xmii3_d[l_ac].xmii009,g_xmii3_d[l_ac].xmii010,g_xmii3_d[l_ac].xmii017,g_xmii3_d[l_ac].xmii014, 
          g_xmii_d[l_ac].xmii007_desc,g_xmii_d[l_ac].xmii006_desc,g_xmii_d[l_ac].xmii008_desc,g_xmii_d[l_ac].xmii012_desc, 
          g_xmii_d[l_ac].xmii013_desc,g_xmii_d[l_ac].xmii011_desc,g_xmii_d[l_ac].xmii009_desc,g_xmii_d[l_ac].xmii017_desc  
            #(ver:78)
