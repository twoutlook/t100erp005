         LET g_sql = "SELECT  DISTINCT xmii005,xmii007,xmii006,xmii008,xmii012,xmii013,xmii011,xmii009, 
             xmii010,xmii017,xmii014,xmii005,xmii007,xmii006,xmii008,xmii012,xmii013,xmii011,xmii009, 
             xmii010,xmii017,xmii014,xmii005,xmii007,xmii006,xmii008,xmii012,xmii013,xmii011,xmii009, 
             xmii010,xmii017,xmii014 ,t1.ooefl003 ,t2.ooefl003 ,t3.ooag011 ,t4.pmaal004 ,t5.oojdl003 , 
             t6.rtaxl003 ,t7.imaal003 ,t8.oocal003 FROM xmii_t",   
                     " INNER JOIN xmih_t ON xmihent = " ||g_enterprise|| " AND xmih001 = xmii001 ",
                     " AND xmih002 = xmii002 ",
