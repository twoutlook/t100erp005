      CALL cl_doc_remove()  
 
 
 
  
  
      #資料備份
      LET g_xmdtdocno_t = g_xmdt_m.xmdtdocno
 
 
      DELETE FROM xmdt_t
       WHERE xmdtent = g_enterprise AND xmdtdocno = g_xmdt_m.xmdtdocno
 
       
