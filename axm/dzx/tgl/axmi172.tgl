            IF g_xmie_d.getLength() = 0 THEN
               NEXT FIELD xmieseq
            END IF
            #add-point:input段after input  name="input.body.after_input"
            {<point name="input.body.after_input"/>}
            #end add-point    
            
         ON ACTION controlo   
