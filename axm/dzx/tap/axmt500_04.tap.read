  <point name="input.before_input" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[            IF p_cmd ='u' THEN
               
               CALL s_transaction_begin()
               
               OPEN axmt500_04_cl USING g_enterprise,g_xmdcdocno,g_xmdcseq
               IF STATUS THEN
                  CALL cl_err("OPEN axmt500_04_cl:", STATUS, 1)
                  CLOSE axmt500_04_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
    
               #鎖住將被更改或取消的資料
               FETCH axmt500_04_cl INTO g_xmdc_m.xmdcdocno,g_xmdc_m.xmdcseq,g_xmdc_m.xmdc027,g_xmdc_m.xmdc028,g_xmdc_m.xmdc028_desc,
