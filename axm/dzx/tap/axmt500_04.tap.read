  <point name="function.axmt500_04_xmdc029_chk" order="8" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#儲位檢查
PRIVATE FUNCTION axmt500_04_xmdc029_chk()
DEFINE r_success  LIKE type_t.num5

        LET r_success = TRUE
        IF NOT cl_null(g_xmdc_m.xmdc029) THEN 

           #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
           INITIALIZE g_chkparam.* TO NULL
           
           #設定g_chkparam.*的參數
           LET g_chkparam.arg1 = g_site
           LET g_chkparam.arg2 = g_xmdc_m.xmdc028
           LET g_chkparam.arg3 = g_xmdc_m.xmdc029

              
           #呼叫檢查存在並帶值的library
           IF NOT cl_chk_exist("v_inab002_1") THEN
              LET r_success = FALSE
              RETURN r_success              
           END IF
        END IF
        RETURN r_success        
END FUNCTION]]>
