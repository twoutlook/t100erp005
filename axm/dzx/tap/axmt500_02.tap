       xmdr005          LIKE xmdr_t.xmdr005,
       xmdr003          LIKE xmdr_t.xmdr003,
       xmdr006          LIKE xmdr_t.xmdr006,
       xmdr007          LIKE xmdr_t.xmdr007
                     END RECORD]]>
  </point>
