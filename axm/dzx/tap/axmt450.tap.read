               IF SQLCA.sqlcode THEN
                  CALL cl_err("xmeb_t",SQLCA.sqlcode,1)
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               ELSE
                  LET g_rec_b = g_rec_b-1
                  
                  #add-point:單身刪除後
                  {<point name="input.body.a_delete"/>}
                  #end add-point
                  CALL s_transaction_end('Y','0')
               END IF 
               CLOSE axmt450_bcl
               LET l_count = g_xmeb_d.getLength()
            END IF 
            
                           INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_xmea_m.xmeadocno
               LET gs_keys[2] = g_xmea_m.xmea900
               LET gs_keys[3] = g_xmeb_d[g_detail_idx].xmebseq
 
              
         AFTER DELETE 
            #add-point:單身刪除後2
            {<point name="input.body.after_delete"/>}
            #end add-point
                           CALL axmt450_delete_b('xmeb_t',gs_keys,"'1'")
 
                  #此段落由子樣板a01產生
         BEFORE FIELD xmebsite
            #add-point:BEFORE FIELD xmebsite
            {<point name="input.b.page1.xmebsite" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD xmebsite
            
            #add-point:AFTER FIELD xmebsite
            {<point name="input.a.page1.xmebsite" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE xmebsite
            #add-point:ON CHANGE xmebsite
            {<point name="input.g.page1.xmebsite" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD xmebseq
            #此段落由子樣板a15產生
            IF NOT cl_ap_chk_Range(g_xmeb_d[l_ac].xmebseq,"0","0","","","azz-00079",1) THEN
               NEXT FIELD xmebseq
            END IF
 
 
            #add-point:AFTER FIELD xmebseq
            {<point name="input.a.page1.xmebseq" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD xmebseq
            #add-point:BEFORE FIELD xmebseq
            {<point name="input.b.page1.xmebseq" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE xmebseq
            #add-point:ON CHANGE xmebseq
            {<point name="input.g.page1.xmebseq" />}
            #END add-point
 
