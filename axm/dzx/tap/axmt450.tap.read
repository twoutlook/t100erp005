  <section id="axmt450.main" ver="17" status="" src="s">
    <![CDATA[#+ 此段落由子樣板a26產生
#OPTIONS SHORT CIRCUIT
#+ 作業開始 
MAIN
   #add-point:main段define
   {<point name="main.define"/>}
   #end add-point   
 
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("axm","")
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT xmeadocno,xmea900,xmea902,xmea002,'','',xmea000,xmea003,'',xmea901,xmeadocdt, 
       xmeastus,xmea004,'',xmeaacti,xmea005,'',xmea006,'',xmea007,xmea008,xmea009,'',xmea011,'',xmea030, 
       xmea016,xmea017,xmea018,xmea019,xmea020,xmea010,xmea012,xmea001,xmea014,xmea015,xmeasite,xmea903, 
       '',xmea904,xmeaownid,'',xmeaowndp,'',xmeacrtid,'',xmeacrtdp,'',xmeacrtdt,xmeamodid,'',xmeamoddt, 
       xmeacnfid,'',xmeacnfdt", 
                      " FROM xmea_t",
                      " WHERE xmeaent= ? AND xmeadocno=? AND xmea900=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE axmt450_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE xmeadocno,xmea900,xmea902,xmea002,xmea000,xmea003,xmea901,xmeadocdt,xmeastus, 
       xmea004,xmeaacti,xmea005,xmea006,xmea007,xmea008,xmea009,xmea011,xmea030,xmea016,xmea017,xmea018, 
       xmea019,xmea020,xmea010,xmea012,xmea001,xmea014,xmea015,xmeasite,xmea903,xmea904,xmeaownid,xmeaowndp, 
       xmeacrtid,xmeacrtdp,xmeacrtdt,xmeamodid,xmeamoddt,xmeacnfid,xmeacnfdt,t1.oofa011 ,t2.pmaal004 , 
       t3.ooefl003 ,t4.ooail003 ,t5.ooibl004 ,t6.oocql004 ,t7.oocql004 ,t8.oofa011 ,t9.ooefl003 ,t10.oofa011 , 
       t11.ooefl003 ,t12.oofa011 ,t13.oofa011",
               " FROM xmea_t",
                              " LEFT JOIN oofa_t t1 ON t1.oofaent='"||g_enterprise||"' AND t1.oofa001=xmea002  ",
               " LEFT JOIN pmaal_t t2 ON t2.pmaalent='"||g_enterprise||"' AND t2.pmaal001=xmea003 AND t2.pmaal002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent='"||g_enterprise||"' AND t3.ooefl001=xmea004 AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooail_t t4 ON t4.ooailent='"||g_enterprise||"' AND t4.ooail001=xmea005 AND t4.ooail002='"||g_dlang||"' ",
               " LEFT JOIN ooibl_t t5 ON t5.ooiblent='"||g_enterprise||"' AND t5.ooibl002=xmea009 AND t5.ooibl003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t6 ON t6.oocqlent='"||g_enterprise||"' AND t6.oocql001='238' AND t6.oocql002=xmea011 AND t6.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t7 ON t7.oocqlent='"||g_enterprise||"' AND t7.oocql001='282' AND t7.oocql002=xmea903 AND t7.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t8 ON t8.oofaent='"||g_enterprise||"' AND t8.oofa002='2' AND t8.oofa003=xmeaownid  ",
               " LEFT JOIN ooefl_t t9 ON t9.ooeflent='"||g_enterprise||"' AND t9.ooefl001=xmeaowndp AND t9.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t10 ON t10.oofaent='"||g_enterprise||"' AND t10.oofa002='2' AND t10.oofa003=xmeacrtid  ",
               " LEFT JOIN ooefl_t t11 ON t11.ooeflent='"||g_enterprise||"' AND t11.ooefl001=xmeacrtdp AND t11.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t12 ON t12.oofaent='"||g_enterprise||"' AND t12.oofa002='2' AND t12.oofa003=xmeamodid  ",
               " LEFT JOIN oofa_t t13 ON t13.oofaent='"||g_enterprise||"' AND t13.oofa002='2' AND t13.oofa003=xmeacnfid  ",
 
               " WHERE xmeaent = '" ||g_enterprise|| "' AND xmeadocno = ? AND xmea900 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE axmt450_master_referesh FROM g_sql
 
 
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_axmt450 WITH FORM cl_ap_formpath("axm",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL axmt450_init()   
 
      #進入選單 Menu (="N")
      CALL axmt450_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_axmt450
      
   END IF 
   
   CLOSE axmt450_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
   
END MAIN
 
 
]]>
