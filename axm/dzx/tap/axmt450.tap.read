  <section id="axmt450.b_fill" ver="18" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION axmt450_b_fill()
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_xmeb_d.clear()    #g_xmeb_d 單頭及單身 
   CALL g_xmeb2_d.clear()
   CALL g_xmeb3_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF axmt450_fill_chk(1) THEN
   
      LET g_sql = "SELECT  UNIQUE xmebsite,xmebseq,xmeb002,xmeb003,xmeb005,xmeb006,xmeb007,xmeb014,xmeb008, 
          xmeb009,xmeb010,xmeb024,xmeb011,xmeb012,xmeb017,xmeb018,xmeb019,xmeb013,xmeb004,xmeb030,xmeb901, 
          xmeb902,xmeb903,xmeb001,xmebseq,xmeb020,xmeb021,xmeb022,xmeb023 ,t1.imaal003 ,t2.oocql004 , 
          t3.oocql004 ,t4.oocal003 ,t5.oocql004 ,t6.oocql004 FROM xmeb_t",   
                  " INNER JOIN xmea_t ON xmeadocno = xmebdocno ",
                  " AND xmea900 = xmeb900 ",
 
                  #"",
                  " LEFT JOIN xmec_t ON xmebent = xmecent AND xmebdocno = xmecdocno AND xmeb900 = xmec900 AND xmebseq = xmecseq ",
                  "",
                                 " LEFT JOIN imaal_t t1 ON t1.imaalent='"||g_enterprise||"' AND t1.imaal001=xmeb002 AND t1.imaal002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t2 ON t2.oocqlent='"||g_enterprise||"' AND t2.oocql001='221' AND t2.oocql002=xmeb006 AND t2.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t3 ON t3.oocqlent='"||g_enterprise||"' AND t3.oocql001='277' AND t3.oocql002=xmeb014 AND t3.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t4 ON t4.oocalent='"||g_enterprise||"' AND t4.oocal001=xmeb008 AND t4.oocal002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t5 ON t5.oocqlent='"||g_enterprise||"' AND t5.oocql001='263' AND t5.oocql002=xmeb013 AND t5.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t6 ON t6.oocqlent='"||g_enterprise||"' AND t6.oocql001='282' AND t6.oocql002=xmeb902 AND t6.oocql003='"||g_dlang||"' ",
 
                  " WHERE xmebent=? AND xmebdocno=? AND xmeb900=?"
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      #add-point:b_fill段sql_before
      {<point name="b_fill.body.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table1) THEN
         LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
      END IF
      
      #子單身的WC
         IF NOT cl_null(g_wc2_table2) THEN 
      LET g_sql = g_sql CLIPPED," AND ", g_wc2_table2 CLIPPED
   END IF 
 
      
      LET g_sql = g_sql, " ORDER BY xmeb_t.xmebseq"
      
      #add-point:單身填充控制
      {<point name="b_fill.sql"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE axmt450_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR axmt450_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_xmea_m.xmeadocno,g_xmea_m.xmea900
                                               
      FOREACH b_fill_cs INTO g_xmeb_d[l_ac].xmebsite,g_xmeb_d[l_ac].xmebseq,g_xmeb_d[l_ac].xmeb002,g_xmeb_d[l_ac].xmeb003, 
          g_xmeb_d[l_ac].xmeb005,g_xmeb_d[l_ac].xmeb006,g_xmeb_d[l_ac].xmeb007,g_xmeb_d[l_ac].xmeb014, 
          g_xmeb_d[l_ac].xmeb008,g_xmeb_d[l_ac].xmeb009,g_xmeb_d[l_ac].xmeb010,g_xmeb_d[l_ac].xmeb024, 
          g_xmeb_d[l_ac].xmeb011,g_xmeb_d[l_ac].xmeb012,g_xmeb_d[l_ac].xmeb017,g_xmeb_d[l_ac].xmeb018, 
          g_xmeb_d[l_ac].xmeb019,g_xmeb_d[l_ac].xmeb013,g_xmeb_d[l_ac].xmeb004,g_xmeb_d[l_ac].xmeb030, 
          g_xmeb_d[l_ac].xmeb901,g_xmeb_d[l_ac].xmeb902,g_xmeb_d[l_ac].xmeb903,g_xmeb_d[l_ac].xmeb001, 
          g_xmeb2_d[l_ac].xmebseq,g_xmeb2_d[l_ac].xmeb020,g_xmeb2_d[l_ac].xmeb021,g_xmeb2_d[l_ac].xmeb022, 
          g_xmeb2_d[l_ac].xmeb023,g_xmeb_d[l_ac].xmeb002_desc,g_xmeb_d[l_ac].xmeb006_desc,g_xmeb_d[l_ac].xmeb014_desc, 
          g_xmeb_d[l_ac].xmeb008_desc,g_xmeb_d[l_ac].xmeb013_desc,g_xmeb_d[l_ac].xmeb902_desc
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               CALL cl_err( '', 9035, 1)
            END IF
            EXIT FOREACH
         END IF
         
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
 
   
   #add-point:browser_fill段其他table處理
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_xmeb_d.deleteElement(g_xmeb_d.getLength())
   CALL g_xmeb2_d.deleteElement(g_xmeb2_d.getLength())
   CALL g_xmeb3_d.deleteElement(g_xmeb3_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE axmt450_pb
 
   
END FUNCTION
]]>
  </section>
  <section id="axmt450.b_fill2" ver="10" status="" src="s">
    <![CDATA[#+ 單身陣列填充2
PRIVATE FUNCTION axmt450_b_fill2(pi_idx)
   DEFINE pi_idx          LIKE type_t.num5
   DEFINE li_ac           LIKE type_t.num5
   DEFINE ls_chk          LIKE type_t.chr1
   #add-point:b_fill2段define
   {<point name="b_fill2.define"/>}
   #end add-point
   
   LET li_ac = l_ac 
   
   IF g_detail_idx <= 0 THEN
      RETURN
   END IF
   
   IF axmt450_fill_chk(2) THEN
      IF (pi_idx = 2 OR pi_idx = 0 ) AND g_xmeb_d.getLength() > 0 THEN
               CALL g_xmeb3_d.clear()
 
         LET g_sql = "SELECT  UNIQUE xmecseq,xmecseq1,xmec001,xmec002,xmec003,xmec004,xmec005,xmec901, 
             xmec902,xmec903 ,t8.oocql004 FROM xmec_t",    
                     "",
                                    " LEFT JOIN oocql_t t8 ON t8.oocqlent='"||g_enterprise||"' AND t8.oocql001='282' AND t8.oocql002=xmec902 AND t8.oocql003='"||g_dlang||"' ",
 
                     " WHERE xmecent=? AND xmecdocno=? AND xmec900=? AND xmecseq=?"
         
         IF NOT cl_null(g_wc2_table2) THEN
            LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
         END IF
         
         LET g_sql = g_sql, " ORDER BY  xmec_t.xmecseq1" 
                            
         #add-point:單身填充前
         {<point name="b_fill2.before_fill2" />}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE axmt450_pb2 FROM g_sql
         DECLARE b_fill_curs2 CURSOR FOR axmt450_pb2
         
         OPEN b_fill_curs2 USING g_enterprise,
                                        g_xmea_m.xmeadocno,g_xmea_m.xmea900,g_xmeb_d[g_detail_idx].xmebseq 

         LET l_ac = 1
         FOREACH b_fill_curs2 INTO g_xmeb3_d[l_ac].xmecseq,g_xmeb3_d[l_ac].xmecseq1,g_xmeb3_d[l_ac].xmec001, 
             g_xmeb3_d[l_ac].xmec002,g_xmeb3_d[l_ac].xmec003,g_xmeb3_d[l_ac].xmec004,g_xmeb3_d[l_ac].xmec005, 
             g_xmeb3_d[l_ac].xmec901,g_xmeb3_d[l_ac].xmec902,g_xmeb3_d[l_ac].xmec903,g_xmeb3_d[l_ac].xmec902_desc  

            IF SQLCA.sqlcode THEN
               CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
               EXIT FOREACH
            END IF
            
            #add-point:b_fill段資料填充
            {<point name="b_fill2.fill2"/>}
            #end add-point
           
            LET l_ac = l_ac + 1
            IF l_ac > g_max_rec THEN
               CALL cl_err( '', 9035, 0 )
               EXIT FOREACH
            END IF
            
         END FOREACH
               CALL g_xmeb3_d.deleteElement(g_xmeb3_d.getLength())
 
      END IF
   END IF
 
 
      
   #add-point:單身填充後
   {<point name="b_fill2.after_fill" />}
   #end add-point
    
   LET l_ac = li_ac
   
   CALL axmt450_detail_show()
   
END FUNCTION
]]>
  </section>
  <section id="axmt450.before_delete" ver="1" status="" src="s">
    <![CDATA[#+ 單身db資料刪除
PRIVATE FUNCTION axmt450_before_delete()
   #add-point:before_delete段define
   {<point name="before_delete.define"/>}
   #end add-point 
   
   #add-point:單筆刪除前
   {<point name="delete.body.b_single_delete" mark="Y"/>}
   #end add-point
   
   DELETE FROM xmeb_t
    WHERE xmebent = g_enterprise AND  = g_xmea_m.xmeadocno AND
 
          xmebdocno = g_xmeb_d_t.xmebdocno
