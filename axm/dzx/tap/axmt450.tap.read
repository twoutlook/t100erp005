  <point name="function.axmt450_xmeadocno_desc" order="17" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION axmt450_xmeadocno_desc()
DEFINE l_flag            LIKE type_t.num5
DEFINE l_site            LIKE ooef_t.ooef005
DEFINE l_slip            LIKE oobal_t.oobal002
DEFINE l_ooef004         LIKE ooef_t.ooef004

   LET g_xmea_m.xmeadocno_desc = ''
   DISPLAY BY NAME g_xmea_m.xmeadocno_desc

   IF cl_null(g_xmea_m.xmeadocno) THEN
      RETURN
   END IF


   #抓取單據別
   CALL s_aooi200_get_slip(g_xmea_m.xmeadocno) RETURNING l_flag,l_slip
   IF NOT l_flag THEN
      RETURN
   END IF

   #抓取單據別說明
   SELECT oobxl003 INTO g_xmea_m.xmeadocno_desc
     FROM oobxl_t
    WHERE oobxlent = g_enterprise
      AND oobxl001 = l_slip
      AND oobxl002 = g_dlang

   DISPLAY BY NAME g_xmea_m.xmeadocno_desc
END FUNCTION]]>
  </point>
