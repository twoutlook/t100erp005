   END IF

   #呼叫檢查存在並帶值的library
   IF NOT cl_chk_exist("v_xmdxdocno") THEN
      #檢查失敗時後續處理
      LET r_success = FALSE
      RETURN r_success
   END IF

   #同一張合約單不能同時有兩張以上未確認的變更單存在
