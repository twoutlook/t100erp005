  <section id="axmt450.filter" ver="6" status="" src="s">
    <![CDATA[#+ 此段落由子樣板a50產生
#+ filter過濾功能
PRIVATE FUNCTION axmt450_filter()
   #add-point:filter段define
   {<point name="filter.define"/>}
   #end add-point   
 
   #切換畫面
   IF NOT g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",1)
      CALL gfrm_curr.setElementHidden("worksheet",0)
      LET g_main_hidden = 1
   END IF   
 
   LET INT_FLAG = 0
 
   LET g_qryparam.state = 'c'
 
   LET g_wc_filter_t = g_wc_filter.trim()
   LET g_wc_t = g_wc
 
   LET g_wc = cl_replace_str(g_wc, g_wc_filter_t, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON xmeadocno,xmea902,xmea900,xmea002,xmea003,xmea004,xmea005,xmea014,xmea015 

                          FROM s_browse[1].b_xmeadocno,s_browse[1].b_xmea902,s_browse[1].b_xmea900,s_browse[1].b_xmea002, 
                              s_browse[1].b_xmea003,s_browse[1].b_xmea004,s_browse[1].b_xmea005,s_browse[1].b_xmea014, 
                              s_browse[1].b_xmea015
 
         BEFORE CONSTRUCT
               DISPLAY axmt450_filter_parser('xmeadocno') TO s_browse[1].b_xmeadocno
            DISPLAY axmt450_filter_parser('xmea902') TO s_browse[1].b_xmea902
            DISPLAY axmt450_filter_parser('xmea900') TO s_browse[1].b_xmea900
            DISPLAY axmt450_filter_parser('xmea002') TO s_browse[1].b_xmea002
            DISPLAY axmt450_filter_parser('xmea003') TO s_browse[1].b_xmea003
            DISPLAY axmt450_filter_parser('xmea004') TO s_browse[1].b_xmea004
            DISPLAY axmt450_filter_parser('xmea005') TO s_browse[1].b_xmea005
            DISPLAY axmt450_filter_parser('xmea014') TO s_browse[1].b_xmea014
            DISPLAY axmt450_filter_parser('xmea015') TO s_browse[1].b_xmea015
      
         #add-point:filter段cs_ctrl
         {<point name="filter.cs_ctrl"/>}
         #end add-point
      
      END CONSTRUCT
 
      #add-point:filter段add_cs
      {<point name="filter.add_cs"/>}
      #end add-point
 
      BEFORE DIALOG
         #add-point:filter段b_dialog
         {<point name="filter.b_dialog"/>}
         #end add-point  
      
      ON ACTION accept
         ACCEPT DIALOG
 
      ON ACTION cancel
         LET INT_FLAG = 1
         EXIT DIALOG 
 
      #交談指令共用ACTION
      &include "common_action.4gl" 
         CONTINUE DIALOG
   
   END DIALOG
 
   IF NOT INT_FLAG THEN
      LET g_wc_filter = "   AND   ", g_wc_filter, "   "
      LET g_wc = g_wc , g_wc_filter
   ELSE
      LET g_wc_filter = g_wc_filter_t
      LET g_wc = g_wc_t
   END IF
 
      CALL axmt450_filter_show('xmeadocno')
   CALL axmt450_filter_show('xmea902')
   CALL axmt450_filter_show('xmea900')
   CALL axmt450_filter_show('xmea002')
   CALL axmt450_filter_show('xmea003')
   CALL axmt450_filter_show('xmea004')
   CALL axmt450_filter_show('xmea005')
   CALL axmt450_filter_show('xmea014')
   CALL axmt450_filter_show('xmea015')
 
END FUNCTION
