  <point name="function.axmt450_change_xmea011" order="20" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION axmt450_change_xmea011(p_cmd)
DEFINE p_cmd             LIKE type_t.chr1
DEFINE l_i               LIKE type_t.num10
DEFINE l_success         LIKE type_t.num5
DEFINE l_xmebseq         LIKE xmeb_t.xmebseq
DEFINE l_xmeb002         LIKE xmeb_t.xmeb002
DEFINE l_xmeb009         LIKE xmeb_t.xmeb009
DEFINE l_xmeb010         LIKE xmeb_t.xmeb010
DEFINE l_xmeb011         LIKE xmeb_t.xmeb011
DEFINE l_xmeb012         LIKE xmeb_t.xmeb012
DEFINE l_xmeb017         LIKE xmeb_t.xmeb017
DEFINE l_xmeb018         LIKE xmeb_t.xmeb018
DEFINE l_xmeb019         LIKE xmeb_t.xmeb019

   #單頭新增時，因單身資料還沒insert到資料庫，所以只改變畫面上的顯示
   IF p_cmd = 'a' THEN
      FOR l_i = 1 TO g_xmeb_d.getLength()
         IF g_tax_app = '2' THEN
            #依料件設定
            CALL s_tax_chktype(g_xmea_m.xmeasite,g_xmea_m.xmea004,g_xmeb_d[l_i].xmeb002,
                               '1',g_pmab089)
                 RETURNING l_success,g_xmeb_d[l_i].xmeb011,g_xmeb_d[l_i].xmeb012
         ELSE
            #依正常稅率
            LET g_xmeb_d[l_i].xmeb011 = g_xmea_m.xmea006
            LET g_xmeb_d[l_i].xmeb012 = g_xmea_m.xmea007
         END IF

         #重計未稅金額、含稅金額、稅額
         CALL axmt450_get_amount(g_xmeb_d[l_i].xmebseq,g_xmeb_d[l_i].xmeb009,
                                 g_xmeb_d[l_i].xmeb010,g_xmeb_d[l_i].xmeb011)
              RETURNING g_xmeb_d[l_i].xmeb017,g_xmeb_d[l_i].xmeb018,g_xmeb_d[l_i].xmeb019
      END FOR
   ELSE
      DECLARE sel_xmeb_cs CURSOR FOR
       SELECT xmebseq,xmeb002,xmeb009,xmeb010
         FROM xmeb_t
        WHERE xmebent = g_enterprise
          AND xmebdocno = g_xmea_m.xmeadocno
          AND xmeb900 = g_xmea_m.xmea900
         
      FOREACH sel_xmeb_cs INTO l_xmebseq,l_xmeb002,l_xmeb009,l_xmeb010
         IF g_tax_app = '2' THEN
            #依料件設定
            CALL s_tax_chktype(g_xmea_m.xmeasite,g_xmea_m.xmea004,l_xmeb002,
                               '1',g_pmab089)
                 RETURNING l_success,l_xmeb011,l_xmeb012
         ELSE
            #依正常稅率
            LET l_xmeb011 = g_xmea_m.xmea006
            LET l_xmeb012 = g_xmea_m.xmea007
         END IF

         #重計未稅金額、含稅金額、稅額
         CALL axmt450_get_amount(l_xmebseq,l_xmeb009,
                                 l_xmeb010,l_xmeb011)
              RETURNING l_xmeb017,l_xmeb018,l_xmeb019

         UPDATE xmeb_t SET xmeb011 = l_xmeb011,
                           xmeb012 = l_xmeb012,
                           xmeb017 = l_xmeb017,
                           xmeb018 = l_xmeb018,
                           xmeb019 = l_xmeb019
          WHERE xmebent = g_enterprise
            AND xmebdocno = g_xmea_m.xmeadocno
            AND xmeb900 = g_xmea_m.xmea900
            AND xmebseq = l_xmebseq
         IF SQLCA.sqlcode THEN
            CALL cl_err("xmeb_t:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF            
      END FOREACH
      CALL axmt450_b_fill()
   END IF
END FUNCTION]]>
  </point>
  <point name="function.axmt450_get_amount" order="21" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION axmt450_get_amount(p_xmebseq,p_xmeb009,p_xmeb010,p_xmeb011)
DEFINE p_xmebseq         LIKE xmeb_t.xmebseq
DEFINE p_xmeb009         LIKE xmeb_t.xmeb009
DEFINE p_xmeb010         LIKE xmeb_t.xmeb010
