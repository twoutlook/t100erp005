#                : 
# Return code....: r_success      TRUE/FALSE
#                : 
# Date & Author..: 2014/12/03 By Polly
# Modify.........:
################################################################################
PRIVATE FUNCTION axmt510_xmei_ins_xmek(p_xmei_d)
DEFINE p_xmei_d          type_g_xmeg4_d
DEFINE r_success         LIKE type_t.num5
DEFINE l_xmds            RECORD LIKE xmds_t.*
DEFINE l_flag            LIKE type_t.num5      #單身是否有修改

   LET r_success = TRUE

   IF p_xmei_d.xmei901 = '1' THEN       #未變更不需寫入歷程檔內  
      RETURN r_success
   END IF

   INITIALIZE l_xmds.* TO NULL
   LET l_flag = FALSE

   IF p_xmei_d.xmei901 = '2' THEN     
      SELECT xmdsseq,xmds001,xmds002,xmds003,xmds004,
             xmds005,xmds006 
        INTO l_xmds.xmdsseq,l_xmds.xmds001,l_xmds.xmds002,l_xmds.xmds003,l_xmds.xmds004,
             l_xmds.xmds005,l_xmds.xmds006       
