  <point name="input.body.before_insert" order="" ver="3" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[            CALL s_transaction_begin()        
            LET g_xmeg_d[l_ac].xmegsite = g_site           
            LET g_xmeg_d[l_ac].xmegunit = g_site
            LET g_xmeg_d[l_ac].xmegorga = g_site
            LET g_xmeg_d[l_ac].xmeg017 = g_xmee_m.xmee012
            LET g_xmeg_d[l_ac].xmeg023 = g_xmee_m.xmee022
            LET g_xmeg_d[l_ac].xmeg046 = 0
            LET g_xmeg_d[l_ac].xmeg047 = 0
            LET g_xmeg2_d[l_ac].xmeg025 = g_xmee_m.xmee025        
            LET g_xmeg2_d[l_ac].xmeg026 = g_xmee_m.xmee026
            LET g_xmeg2_d[l_ac].xmeg032 = "1"
            LET g_xmeg2_d[l_ac].xmeg035 = "1"
            LET g_xmeg2_d[l_ac].xmeg040 = "1"           
            #判斷據點參數若不使用產品特徵時，則產品特徵不可以維護，需給空白
            IF cl_get_para(g_enterprise,g_site,'S-BAS-0036') = 'N' THEN
               LET g_xmeg_d[l_ac].xmeg002 = ' '
            END IF  
            #判斷單據別參數品質檢驗否(單據別參數:D-BAS-0076)
            IF cl_get_doc_para(g_enterprise,g_site,l_slip,'D-BAS-0076') = 'N' THEN
               LET g_xmeg_d[l_ac].xmeg052 = "N"
            END IF
            
            CALL axmt510_xmee003_ref(g_xmeg_d[l_ac].xmegunit) RETURNING g_xmeg_d[l_ac].xmegunit_desc
            CALL axmt510_xmee003_ref(g_xmeg_d[l_ac].xmegorga) RETURNING g_xmeg_d[l_ac].xmegorga_desc
            DISPLAY BY NAME g_xmeg_d[l_ac].xmegunit_desc,g_xmeg_d[l_ac].xmegorga_desc
                   
            LET g_xmeg2_d[l_ac].xmeg031 = g_xmee_m.xmee020
            CALL s_desc_get_acc_desc('263',g_xmeg2_d[l_ac].xmeg031) RETURNING g_xmeg2_d[l_ac].xmeg031_desc
            DISPLAY BY NAME g_xmeg2_d[l_ac].xmeg031_desc
                
            #項次加1
            SELECT MAX(xmegseq)+1 INTO g_xmeg_d[l_ac].xmegseq FROM xmeg_t
             WHERE xmegent = g_enterprise AND xmegdocno = g_xmee_m.xmeedocno
               AND xmeg900 = g_xmee_m.xmee900
            IF cl_null(g_xmeg_d[l_ac].xmegseq) OR g_xmeg_d[l_ac].xmegseq = 0 THEN
               LET g_xmeg_d[l_ac].xmegseq = 1
            END IF
            LET g_xmeg_d[l_ac].xmeg901 = '3'  #單身新增
            LET g_xmeg2_d[l_ac].xmegseq = g_xmeg_d[l_ac].xmegseq
            
           #新增時，若稅別應用=1.正常稅率，則單身的稅別、稅率預設為單頭的稅別、稅率
            IF g_tax_app = '1' THEN
               LET g_xmeg_d[l_ac].xmeg016 = g_xmee_m.xmee011
               LET g_xmeg_d[l_ac].xmeg016_desc = g_xmee_m.xmee011_desc
               LET g_xmeg_d[l_ac].xmeg017 = g_xmee_m.xmee012
            END IF
            LET g_xmeg_d_t.* = g_xmeg_d[l_ac].*
            LET g_xmeg_d_o.* = g_xmeg_d[l_ac].*         ]]>
  </point>
  <point name="input.body.before_row" order="" ver="4" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[            #單身結案則不可變更
            LET g_detail_idx = ARR_CURR()
            IF g_xmeg_d[g_detail_idx].xmeg045 MATCHES "[234]" THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.extend = g_xmee_m.xmeedocno
               LET g_errparam.replace[1] = g_xmeg_d[g_detail_idx].xmegseq
               LET g_errparam.code   = "axm-00554"
               LET g_errparam.popup  = TRUE
               CALL cl_err()
               LET l_cnt = 0
               CASE
                 WHEN g_detail_idx < g_rec_b
                   FOR l_i = g_detail_idx + 1 TO g_rec_b
                     IF NOT g_xmeg_d[l_i].xmeg045 MATCHES "[234]" THEN
                        LET l_cnt = l_i
                        EXIT FOR
                     END IF
                   END FOR
                 WHEN g_detail_idx > g_rec_b
                   FOR l_i = g_rec_b-1 TO 1 STEP -1
                       IF NOT g_xmeg_d[l_i].xmeg045 MATCHES "[234]" THEN
                          LET l_cnt = l_i
                          EXIT FOR
                       END IF
                   END FOR
                 WHEN g_detail_idx = g_rec_b
                    LET l_cnt = g_detail_idx + 1
               END CASE
               IF l_cnt = 0 THEN
                  LET l_cnt = g_rec_b + 1
               END IF
               CALL FGL_SET_ARR_CURR(l_cnt)
            END IF         
            IF l_cmd = 'u' THEN
               LET g_xmeg_d_o.* = g_xmeg_d[l_ac].*
            ELSE
               INITIALIZE g_xmeg_d_o.* TO NULL
            END IF]]>
