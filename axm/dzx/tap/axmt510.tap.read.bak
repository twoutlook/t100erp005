  <point name="function.axmt510_xmee004_chk" order="9" ver="3" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#客戶檢查
PRIVATE FUNCTION axmt510_xmee004_chk(p_xmee004)
DEFINE p_xmee004    LIKE xmee_t.xmee004
DEFINE l_success    LIKE type_t.num5
DEFINE l_flag       LIKE type_t.num5
DEFINE r_success    LIKE type_t.num5

       LET r_success = TRUE
       
       IF NOT cl_null(p_xmee004) THEN 
          #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
          INITIALIZE g_chkparam.* TO NULL
          
          #設定g_chkparam.*的參數
          LET g_chkparam.arg1 = p_xmee004
          LET g_chkparam.arg2 = g_site
   
          #呼叫檢查存在並帶值的library
          IF NOT cl_chk_exist("v_pmaa001_3") THEN
             LET r_success = FALSE
             RETURN r_success
          END IF
          
          #判斷輸入的客戶編號是否在銷售控制組限制的供應商範圍內，若不在限制內則不允許跟此客戶訂購  
          CALL s_control_chk_group('1','2',g_user,g_dept,p_xmee004,'','','','') RETURNING l_success,l_flag
          IF NOT l_success THEN      #處理狀況
             LET r_success = FALSE
             RETURN r_success
          ELSE
             IF NOT l_flag THEN      #是否存在
                INITIALIZE g_errparam TO NULL
                LET g_errparam.code = 'axm-00007'
                LET g_errparam.extend = p_xmee004
                LET g_errparam.popup = TRUE
                CALL cl_err()

                LET r_success = FALSE
                RETURN r_success
             END IF 
          END IF
          
          #檢核輸入的客戶是否在單據別限制範圍內，若不在限制內則不允許跟此客戶訂購
          CALL s_control_chk_doc('3',g_xmee_m.xmeedocno,p_xmee004,'','','','') RETURNING l_success,l_flag
          IF NOT l_success THEN      #處理狀況
             LET r_success = FALSE
             RETURN r_success
          ELSE
             IF NOT l_flag THEN      #是否存在
                INITIALIZE g_errparam TO NULL
                LET g_errparam.code = 'apm-00239'
                LET g_errparam.extend = p_xmee004
                LET g_errparam.popup = TRUE
                CALL cl_err()

                LET r_success = FALSE
                RETURN r_success
             END IF 
          END IF
       END IF 
       RETURN r_success
END FUNCTION]]>
  </point>
  <point name="function.axmt510_xmee004_desc" order="10" ver="5" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#根據客戶編號帶出其他欄位值
PRIVATE FUNCTION axmt510_xmee004_desc()
  DEFINE l_n           LIKE type_t.num5
  DEFINE l_success     LIKE type_t.num5
  DEFINE l_sql         STRING
  DEFINE l_controlno   LIKE ooha_t.ooha001
  DEFINE l_pmaa027     LIKE pmaa_t.pmaa027
  DEFINE l_oodbl004    LIKE oodbl_t.oodbl004  #稅別名稱
  DEFINE l_oodb005     LIKE oodb_t.oodb005    #含稅否
  DEFINE l_oodb006     LIKE oodb_t.oodb006    #稅率 
  DEFINE l_oodb011     LIKE oodb_t.oodb011    #取得稅別類型1:正常稅率2:依料件設定
  DEFINE l_pmaa096     LIKE pmaa_t.pmaa096    #2015/03/03 by stellar add
  DEFINE l_pmaa097     LIKE pmaa_t.pmaa097    #2015/03/03 by stellar add
  DEFINE l_xmee002_o   LIKE xmee_t.xmee002    #2015/03/04 by stellar add
  DEFINE l_xmee003_o   LIKE xmee_t.xmee003    #2015/03/04 by stellar add
  
      #2015/03/03 by stellar add ----- (S)
      LET l_xmee002_o = g_xmee_m.xmee002
      LET l_xmee003_o = g_xmee_m.xmee003
      LET g_xmee_m.xmee002 = ''
      LET g_xmee_m.xmee002_desc = ''
      LET g_xmee_m.xmee003 = ''
      LET g_xmee_m.xmee003_desc = ''
      #2015/03/03 by stellar add ----- (E)
   
      LET g_xmee_m.xmee007 = '1'
      LET g_xmee_m.xmee008 = ''
      LET g_xmee_m.xmee009 = ''
      LET g_xmee_m.xmee009_desc = ''
      LET g_xmee_m.xmee010 = ''
      LET g_xmee_m.xmee010_desc = ''
      LET g_xmee_m.xmee011 = ''
      LET g_xmee_m.xmee011_desc = ''
      LET g_xmee_m.xmee012 = ''
      LET g_xmee_m.xmee013 = ''
      LET g_xmee_m.xmee015 = ''
      LET g_xmee_m.xmee015_desc = ''
      LET g_xmee_m.xmee016 = ''
      LET g_xmee_m.xmee017 = ''
      LET g_xmee_m.xmee017_desc = ''
      LET g_xmee_m.xmee020 = ''
      LET g_xmee_m.xmee020_desc = ''
      LET g_xmee_m.xmee023 = ''
      LET g_xmee_m.xmee023_desc = ''
      LET g_xmee_m.xmee024 = ''
      LET g_xmee_m.xmee024_desc = ''
      LET g_xmee_m.xmee035 = ''
      LET g_xmee_m.xmee035_desc = ''
      LET g_xmee_m.xmee037 = ''
      LET g_xmee_m.xmee037_desc = ''
      LET g_xmee_m.xmee038 = ''
      LET g_xmee_m.xmee038_desc = ''
      
      #先判斷這個客戶是否有設多個當前銷售控制組範圍內的客戶預設條件，則開窗，讓user選擇帶哪一個控制組的資料
      LET l_controlno = ''
      #2015/03/04 by stellar modify ----- (S)
#      CALL s_control_get_group('2',g_xmee_m.xmee002,g_xmee_m.xmee003) RETURNING l_success,l_controlno
      CALL s_control_get_group('2',g_user,g_dept) RETURNING l_success,l_controlno
      #2015/03/04 by stellar modify ----- (E)
      
      IF NOT cl_null(l_controlno) THEN
         #判斷客戶是否有設置訂單控制組客戶預設條件(axmi110)，若有則抓取相關的預設條件default到訂購單上
         SELECT COUNT(*) INTO l_n FROM xmae_t 
          WHERE xmaeent = g_enterprise AND xmae001 = g_xmee_m.xmee004 
            AND xmae002 = l_controlno 
         IF l_n > 0 THEN
            SELECT xmae006,xmae020,xmae004,xmae003,xmae021,
                   xmae011,xmae008,xmae009,xmae022,xmae012,
                   xmae013,xmae022
                  ,xmae019,xmae025   #2015/03/04 by stellar add
              INTO g_xmee_m.xmee009,g_xmee_m.xmee010,g_xmee_m.xmee011,g_xmee_m.xmee015,g_xmee_m.xmee017,
                   g_xmee_m.xmee020,g_xmee_m.xmee023,g_xmee_m.xmee024,g_xmee_m.xmee035,g_xmee_m.xmee037,
                   g_xmee_m.xmee038,g_xmee_m.xmee035
                  ,g_xmee_m.xmee002,g_xmee_m.xmee003   #2015/03/04 by stellar add
              FROM xmae_t 
             WHERE xmaeent = g_enterprise AND xmae001 = g_xmee_m.xmee004 
               AND xmae002 = l_controlno 
         END IF
      END IF
      
      #若沒有設置銷售控制組客戶預設條件資料則改抓客戶據點預設條件(axmm202)資料  
      IF cl_null(l_controlno) OR l_n = 0 THEN
         SELECT pmab087,pmab103,pmab084,pmab106,pmab083,
                pmab104,pmab090,pmab091,pmab092,pmab088,
                pmab089,pmab106,pmab107,pmab108
               ,pmab081,pmab109   #2015/03/04 by stellar add
           INTO g_xmee_m.xmee009,g_xmee_m.xmee010,g_xmee_m.xmee011,g_xmee_m.xmee035,g_xmee_m.xmee015,
                g_xmee_m.xmee017,g_xmee_m.xmee020,g_xmee_m.xmee037,g_xmee_m.xmee038,g_xmee_m.xmee023,
                g_xmee_m.xmee024,g_xmee_m.xmee035,g_xmee_m.xmee048,g_xmee_m.xmee049
               ,g_xmee_m.xmee002,g_xmee_m.xmee003   #2015/03/04 by stellar add
           FROM pmab_t WHERE pmabent = g_enterprise AND pmabsite = g_site AND pmab001 = g_xmee_m.xmee004         
      END IF
      
      #2015/03/04 by stellar add ----- (S)
      IF cl_null(g_xmee_m.xmee002) OR cl_null(g_xmee_m.xmee003) THEN
         LET l_pmaa096 = ''
         LET l_pmaa097 = ''
         SELECT pmaa096,pmaa097 INTO l_pmaa096,l_pmaa097
           FROM pmaa_t
          WHERE pmaaent = g_enterprise
            AND pmaa001 = g_xmee_m.xmee004
         IF cl_null(g_xmee_m.xmee002) THEN
            LET g_xmee_m.xmee002 = l_pmaa096
         END IF
         IF cl_null(g_xmee_m.xmee003) THEN
            LET g_xmee_m.xmee003 = l_pmaa097
         END IF
      END IF
      
      IF NOT cl_null(g_xmee_m.xmee002) AND cl_null(g_xmee_m.xmee003) THEN
         SELECT ooag003 INTO g_xmee_m.xmee003 
           FROM ooag_t 
          WHERE ooagent = g_enterprise 
            AND ooag001 = g_xmee_m.xmee002
      END IF
      
      IF cl_null(g_xmee_m.xmee002) THEN
         LET g_xmee_m.xmee002 = l_xmee002_o
      END IF
      
      IF cl_null(g_xmee_m.xmee003) THEN
         LET g_xmee_m.xmee003 = l_xmee003_o
      END IF
      
      CALL s_desc_get_person_desc(g_xmee_m.xmee002) RETURNING g_xmee_m.xmee002_desc
      CALL s_desc_get_department_desc(g_xmee_m.xmee003) RETURNING g_xmee_m.xmee003_desc
      
      DISPLAY BY NAME g_xmee_m.xmee002,g_xmee_m.xmee002_desc,
                      g_xmee_m.xmee003,g_xmee_m.xmee003_desc
      #2015/03/04 by stellar add ----- (E)
      
      #取得銷售分類
      LET g_pmab089 = g_xmee_m.xmee024
      #取稅率、單價含稅否
      IF NOT cl_null(g_xmee_m.xmee011) THEN
         LET l_oodb005 = ''
         LET l_oodb006 = ''
         LET l_oodb011 = ''
         CALL s_tax_chk(g_site,g_xmee_m.xmee011)
           RETURNING l_success,l_oodbl004,l_oodb005,l_oodb006,l_oodb011
           
         IF l_success THEN
            LET g_tax_app = l_oodb011
            LET g_xmee_m.xmee012 = l_oodb006
            LET g_xmee_m.xmee013 = l_oodb005
            LET g_xmee_m.xmee011_desc = l_oodbl004
         END IF           
      END IF
      
      #內外銷
      IF cl_null(g_xmee_m.xmee048) THEN
         LET g_xmee_m.xmee048 = '1'
      END IF 
      #依內外銷決定匯率      
      CALL s_axmt540_get_exchange(g_xmee_m.xmee048,g_xmee_m.xmee015) RETURNING g_xmee_m.xmee016
      DISPLAY BY NAME g_xmee_m.xmee016       
      #匯率計算基準
      IF cl_null(g_xmee_m.xmee049) THEN
         LET g_xmee_m.xmee049 = '1'
      END IF 
                      
      CALL s_desc_get_ooib002_desc(g_xmee_m.xmee009) RETURNING g_xmee_m.xmee009_desc
      CALL s_desc_get_acc_desc('238',g_xmee_m.xmee010) RETURNING g_xmee_m.xmee010_desc
      CALL s_desc_get_tax_desc1(g_site,g_xmee_m.xmee011) RETURNING g_xmee_m.xmee011_desc         
      CALL s_desc_get_currency_desc(g_xmee_m.xmee015) RETURNING g_xmee_m.xmee015_desc
      CALL axmt510_xmee017_ref(g_xmee_m.xmee017) RETURNING g_xmee_m.xmee017_desc
      CALL s_desc_get_acc_desc('263',g_xmee_m.xmee020) RETURNING g_xmee_m.xmee020_desc      
      CALL s_desc_get_acc_desc('275',g_xmee_m.xmee023) RETURNING g_xmee_m.xmee023_desc
      CALL axmt510_xmee035_ref(g_xmee_m.xmee035) RETURNING g_xmee_m.xmee035_desc
      CALL s_desc_get_acc_desc('295',g_xmee_m.xmee024) RETURNING g_xmee_m.xmee024_desc 
      CALL s_apmi011_location_ref(g_xmee_m.xmee020,g_xmee_m.xmee037) RETURNING g_xmee_m.xmee037_desc
      CALL s_apmi011_location_ref(g_xmee_m.xmee020,g_xmee_m.xmee038) RETURNING g_xmee_m.xmee038_desc
      CALL axmt510_xmee018_ref(g_xmee_m.xmee018) RETURNING g_xmee_m.xmee018_desc
      CALL axmt510_xmee035_ref(g_xmee_m.xmee035) RETURNING g_xmee_m.xmee035_desc

      DISPLAY BY NAME g_xmee_m.xmee009,g_xmee_m.xmee009_desc,g_xmee_m.xmee010,g_xmee_m.xmee010_desc,g_xmee_m.xmee011,
                      g_xmee_m.xmee011_desc,g_xmee_m.xmee012,g_xmee_m.xmee013,g_xmee_m.xmee015,g_xmee_m.xmee015_desc,
                      g_xmee_m.xmee016,g_xmee_m.xmee017,g_xmee_m.xmee017_desc,g_xmee_m.xmee020,g_xmee_m.xmee020_desc,
                      g_xmee_m.xmee023,g_xmee_m.xmee023_desc,g_xmee_m.xmee024,g_xmee_m.xmee024_desc,g_xmee_m.xmee035,
                      g_xmee_m.xmee035_desc,g_xmee_m.xmee037,g_xmee_m.xmee037_desc,g_xmee_m.xmee038,g_xmee_m.xmee038_desc,
                      g_xmee_m.xmee018,g_xmee_m.xmee018_desc,g_xmee_m.xmee035,g_xmee_m.xmee035_desc

      
      #收款客戶
      LET g_xmee_m.xmee021 = ''
      LET g_xmee_m.xmee021_desc = ''
      CALL s_axmt540_client_partner(g_xmee_m.xmeedocno,g_xmee_m.xmee004,'1') RETURNING g_xmee_m.xmee021
      LET g_xmee_m_o.xmee021 = g_xmee_m.xmee021
      CALL s_desc_get_trading_partner_abbr_desc(g_xmee_m.xmee021) RETURNING g_xmee_m.xmee021_desc
      DISPLAY BY NAME g_xmee_m.xmee021,g_xmee_m.xmee021_desc      

      #依收款客戶抓取預設帳款地址簡碼
