  <point name="function.axmt510_xmeedocno_xmei" order="60" ver="7" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 依據訂單單號，帶出訂單費用資料值
# Memo...........:
# Usage..........: CALL axmt510_xmeedocno_xmei()
#                  RETURNING r_success 
# Input parameter: 
# Return code....: r_success   TRUE/FALSE
# Date & Author..: 20141203 By Polly
# Modify.........:
################################################################################
PRIVATE FUNCTION axmt510_xmeedocno_xmei()
DEFINE l_sql             STRING
DEFINE r_success         LIKE type_t.num5

   LET r_success = TRUE

   IF cl_null(g_xmee_m.xmeedocno) THEN
      LET r_success = FALSE
      RETURN r_success
   END IF
   
   LET l_sql = "SELECT xmdssite,xmdsseq,xmds001,xmds002,xmds003, ",
               "       xmds004,xmds005,xmds006 ",
               "  FROM xmds_t ",
               " WHERE xmdsent = '",g_enterprise,"'", 
               "   AND xmdsdocno = '",g_xmee_m.xmeedocno,"'",         
               " ORDER BY xmdsseq "

   PREPARE sel_xmds_pre FROM l_sql
   DECLARE sel_xmds_cur CURSOR FOR sel_xmds_pre

   LET l_ac = 1               
   FOREACH sel_xmds_cur INTO g_xmeg4_d[l_ac].xmeisite,g_xmeg4_d[l_ac].xmeiseq,g_xmeg4_d[l_ac].xmei001,g_xmeg4_d[l_ac].xmei002,g_xmeg4_d[l_ac].xmei003,
                             g_xmeg4_d[l_ac].xmei004,g_xmeg4_d[l_ac].xmei005,g_xmeg4_d[l_ac].xmei006 

      LET g_xmeg4_d[l_ac].xmei901 = '1'   #未變更 
      CALL s_desc_get_item_desc(g_xmeg4_d[l_ac].xmei001) RETURNING g_xmeg4_d[l_ac].xmei001_desc,g_xmeg4_d[l_ac].xmei001_desc_desc 
      CALL s_desc_get_currency_desc(g_xmeg4_d[l_ac].xmei002) RETURNING g_xmeg4_d[l_ac].xmei002_desc
      CALL s_desc_get_trading_partner_abbr_desc(g_xmeg4_d[l_ac].xmei005) RETURNING g_xmeg4_d[l_ac].xmei005_desc


      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 9035
         LET g_errparam.extend = ''
         LET g_errparam.popup = FALSE
         CALL cl_err()
         EXIT FOREACH
      END IF        
    END FOREACH

    CALL g_xmeg4_d.deleteElement(g_xmeg4_d.getLength())
    
    LET g_rec_b = l_ac - 1
    LET l_ac = 0
    FREE sel_xmds_pre

