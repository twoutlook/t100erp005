          LET l_xmeg_d.xmeg901 = '3'      

          IF cl_null(l_xmeg2_d.xmeg034) THEN LET l_xmeg2_d.xmeg034 = 0 END IF          
          #依交易對象、料號、產品特徵回傳客戶料號
          CALL s_apmi070_get_pmao004(g_xmee_m.xmee004,l_xmeg_d.xmeg001,l_xmeg_d.xmeg002)
            RETURNING l_success,l_pmao004
          IF l_success THEN
             LET l_xmeg_d.xmeg027 = l_pmao004
          END IF
          
          LET l_xmeg_d.xmeg007 = g_inam[l_i].inam004          
          #自動推算參考數量
          IF NOT cl_null(l_xmeg_d.xmeg006) AND NOT cl_null(l_xmeg_d.xmeg008) THEN
             LET l_xmeg_d.xmeg009 = 0
             CALL s_aooi250_convert_qty(l_xmeg_d.xmeg001,l_xmeg_d.xmeg006,l_xmeg_d.xmeg008,l_xmeg_d.xmeg007)
               RETURNING l_success,l_xmeg_d.xmeg009
