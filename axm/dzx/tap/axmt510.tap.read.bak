  <point name="statechange.b_update" order="" ver="8" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   CALL cl_err_collect_init()
   
   IF lc_state = 'Y' THEN
      CALL s_axmt510_conf_chk(g_xmee_m.xmeedocno,g_xmee_m.xmee900) RETURNING l_success
      IF NOT l_success THEN
         CALL cl_err_collect_show()
         RETURN
      ELSE
         IF NOT cl_ask_confirm('aim-00108') THEN
            CALL cl_err_collect_show()
            RETURN
         ELSE
            CALL s_transaction_begin()           
            CALL s_axmt510_conf_upd(g_xmee_m.xmeedocno,g_xmee_m.xmee900) RETURNING l_success
            IF NOT l_success THEN
               CALL s_transaction_end('N','0')
               CALL cl_err_collect_show()
               RETURN
            ELSE
               LET g_xmee_m.xmeecnfid = g_user
               CALL axmt510_xmee002_ref(g_xmee_m.xmeecnfid) RETURNING g_xmee_m.xmeecnfid_desc
               LET g_xmee_m.xmeecnfdt = cl_get_current()
               DISPLAY BY NAME g_xmee_m.xmeecnfid,g_xmee_m.xmeecnfid_desc,g_xmee_m.xmeecnfdt               
               CALL s_transaction_end('Y','0')
            END IF
         END IF
      END IF
   END IF
   IF lc_state = 'X' THEN
      CALL s_axmt510_invalid_chk(g_xmee_m.xmeedocno,g_xmee_m.xmee900) RETURNING l_success
      IF NOT l_success THEN
         CALL cl_err_collect_show()
         RETURN
      ELSE
         IF NOT cl_ask_confirm('aim-00109') THEN
            CALL cl_err_collect_show()
            RETURN
         ELSE
            CALL s_transaction_begin()
            CALL s_axmt510_invalid_upd(g_xmee_m.xmeedocno,g_xmee_m.xmee900) RETURNING l_success
            IF NOT l_success THEN
               CALL s_transaction_end('N','0')
               CALL cl_err_collect_show()
               RETURN
            ELSE
               CALL s_transaction_end('Y','0')
            END IF
         END IF
      END IF
   END IF
   
   CALL cl_err_collect_show()
   ]]>
  </point>
