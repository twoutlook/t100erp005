  <point name="init.init" order="" ver="3" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   CALL cl_set_combo_scc('xmeg035','2039') 
   CALL cl_set_combo_scc('xmeg040','2067') 
   CALL cl_set_combo_scc_part('xmeg019','2055','1,2,3,9')
   CALL cl_set_combo_scc('xmeh003','2055')
   CALL cl_set_combo_scc('xmeh009','2057')   
   CALL cl_set_combo_scc('xmeh017','2058')
   
   #計算金額建立tmptable
   CALL s_tax_recount_tmp()    
   
   #判斷據點參數若不使用參考單位時，則參考單位、數量需隱藏不可以維護(據點參數:S-BAS-0028)
   IF cl_get_para(g_enterprise,g_site,'S-BAS-0028') = 'N' THEN
      CALL cl_set_comp_visible("xmeg008,xmeg008_desc,xmeg009,xmdd024,xmdd024_desc,xmdd025",FALSE)
   END IF

   #判斷據點參數若不使用產品特徵時，則產品特徵、特徵值說明欄位需隱藏不可以維護(據點參數:S-BAS-0036)
   IF cl_get_para(g_enterprise,g_site,'S-BAS-0036') = 'N' THEN
      CALL cl_set_comp_visible("xmeg002,xmeg002_desc",FALSE) 
      CALL cl_set_comp_visible("imaa005_1,imaa005_1_desc",FALSE)  
      CALL cl_set_comp_visible("xmdd002,xmdd002_desc",FALSE)        
