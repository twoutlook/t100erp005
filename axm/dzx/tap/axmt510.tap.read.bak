# Return code....: r_success            TRUE/FALSE
#                :
# Date & Author..: 2014/12/03 By Polly
# Modify.........:
################################################################################
PRIVATE FUNCTION axmt510_reason_chk(p_oocq001,p_oocq002)
DEFINE p_oocq001         LIKE oocq_t.oocq001
DEFINE p_oocq002         LIKE oocq_t.oocq002
DEFINE r_success         LIKE type_t.num5
DEFINE l_success         LIKE type_t.num5
DEFINE l_flag            LIKE type_t.num5

   LET r_success = TRUE
   IF cl_null(p_oocq001) OR cl_null(p_oocq002) THEN
      RETURN r_success
   END IF

   IF NOT s_azzi650_chk_exist(p_oocq001,p_oocq002) THEN
      LET r_success = FALSE
      RETURN r_success
   END IF           
   #檢核輸入的理由碼是否在單據別限制範圍內，若不在限制內則不允許使用此理由碼
   CALL s_control_chk_doc('8',g_xmee_m.xmeedocno,p_oocq002,'','','','') 
     RETURNING l_success,l_flag
   IF NOT l_success THEN
