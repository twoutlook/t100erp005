  <point name="function.axmt510_xmeg001_chk" order="12" ver="3" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#料件編號檢查
PRIVATE FUNCTION axmt510_xmeg001_chk(p_xmeg001)
DEFINE p_xmeg001     LIKE xmeg_t.xmeg001
DEFINE l_flag        LIKE type_t.num5
DEFINE l_success     LIKE type_t.num5
DEFINE r_success     LIKE type_t.num5

       LET r_success = TRUE
       
       IF NOT cl_null(p_xmeg001) THEN 
          #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
          INITIALIZE g_chkparam.* TO NULL
          
          #設定g_chkparam.*的參數
          LET g_chkparam.arg1 = p_xmeg001
             
          #呼叫檢查存在並帶值的library
          IF cl_chk_exist("v_imaa001_5") THEN
             IF NOT cl_chk_exist("v_imaf001_1") THEN
                LET r_success = FALSE
                RETURN r_success
             END IF
          ELSE
             LET r_success = FALSE
             RETURN r_success
          END IF
          
          #判斷輸入的料件編號是否在控制組限制的產品範圍內，若不在限制內則不允許訂購此料
          CALL s_control_chk_group('3','2',g_user,g_dept,p_xmeg001,'','','','') RETURNING l_success,l_flag
          IF NOT l_success THEN      #處理狀態
             LET r_success = FALSE
             RETURN r_success
          ELSE
             IF NOT l_flag THEN      #是否存在
                INITIALIZE g_errparam TO NULL
                LET g_errparam.code = 'axm-00046'
                LET g_errparam.extend = p_xmeg001
                LET g_errparam.popup = TRUE
                CALL cl_err()

                LET r_success = FALSE
                RETURN r_success
             END IF 
          END IF
          
          #檢核輸入的料件的生命週期是否在單據別限制範圍內，若不在限制內則不允許請購此料
          CALL s_control_chk_doc('4',g_xmee_m.xmeedocno,p_xmeg001,'','','','') RETURNING l_success,l_flag
          IF NOT l_success THEN      #處理狀態
             LET r_success = FALSE
             RETURN r_success
          ELSE
             IF NOT l_flag THEN      #是否存在
                INITIALIZE g_errparam TO NULL
                LET g_errparam.code = 'ain-00015'
                LET g_errparam.extend = p_xmeg001
                LET g_errparam.popup = TRUE
                CALL cl_err()

                LET r_success = FALSE
                RETURN r_success
             END IF 
          END IF
          
          #檢核輸入的料件的產品分類是否在單據別限制範圍內，若不在限制內則不允訂請購此料
          CALL s_control_chk_doc('5',g_xmee_m.xmeedocno,p_xmeg001,'','','','') RETURNING l_success,l_flag
          IF NOT l_success THEN      #處理狀態
             LET r_success = FALSE
             RETURN r_success
          ELSE
             IF NOT l_flag THEN      #是否存在
                LET r_success = FALSE
                #CALL cl_err(p_xmeg001,'apm-00238',1)
                RETURN r_success
             END IF 
          END IF
       END IF
       RETURN r_success
END FUNCTION]]>
  </point>
  <point name="function.axmt510_xmeg001_desc" order="13" ver="5" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#根據料件編號、供應商、銷售控制組帶出欄位預設值
PRIVATE FUNCTION axmt510_xmeg001_desc()
DEFINE l_n           LIKE type_t.num5
DEFINE l_success     LIKE type_t.num5
DEFINE l_controlno   LIKE ooha_t.ooha001
DEFINE l_imaf158     LIKE imaf_t.imaf158
DEFINE l_imaf122     LIKE imaf_t.imaf122
DEFINE l_pmao004     LIKE pmao_t.pmao004
DEFINE l_xmeg016     LIKE xmeg_t.xmeg016
DEFINE l_xmeg017     LIKE xmeg_t.xmeg017
DEFINE l_xmegunit    LIKE xmeg_t.xmegunit    #出貨據點
 
      LET l_n = 0
      LET g_xmeg2_d[l_ac].xmeg003 = ''              #包裝容器
      LET g_xmeg2_d[l_ac].xmeg003_desc = ''
      LET g_xmeg_d[l_ac].xmeg006 = ''               #銷售單位
      LET g_xmeg_d[l_ac].xmeg006_desc = ''   
      LET g_xmeg_d[l_ac].xmeg008 = ''               #參考單位
      LET g_xmeg_d[l_ac].xmeg008_desc = ''         
      LET g_xmeg_d[l_ac].xmeg010 = ''               #計價單位
      LET g_xmeg_d[l_ac].xmeg010_desc = ''    
      LET g_xmeg2_d[l_ac].xmeg028 = ''              #限定庫位
      LET g_xmeg2_d[l_ac].xmeg028_desc = ''
      LET g_xmeg2_d[l_ac].xmeg029 = ''              #限定儲位
      LET g_xmeg2_d[l_ac].xmeg029_desc = ''    

   
      #先判斷這個供應商是否有設多個當前銷售控制組範圍內的客戶預設條件，則開窗，讓user 選擇帶哪一個控制組的資料
      LET l_controlno = ''
      #2015/03/04 by stellar add ----- (S)
#      CALL s_control_get_group('2',g_xmee_m.xmee002,g_xmee_m.xmee003) RETURNING l_success,l_controlno
      CALL s_control_get_group('2',g_user,g_dept) RETURNING l_success,l_controlno
      #2015/03/04 by stellar add ----- (E)
      
      IF NOT cl_null(l_controlno) THEN
         SELECT COUNT(*) INTO l_n FROM xmaf_t 
            WHERE xmafent = g_enterprise AND xmafsite = g_site AND xmaf001 = g_xmee_m.xmee004
              AND xmaf002 = l_controlno AND xmaf003 = g_xmeg_d[l_ac].xmeg001 AND xmaf004 = g_xmeg_d[l_ac].xmeg002
         #若訂購料件有設置客戶料件預設條件'(axmi121)時，則需將設置的預設條件值預設到訂購單對應欄位
         IF l_n > 0 THEN  
            LET l_xmegunit = ''                               #出貨據點
            LET g_xmeg_d[l_ac].xmegunit_desc = ''  
            LET g_xmeg_d[l_ac].xmegorga_desc = ''  
            SELECT xmaf005,xmaf006,xmaf007,xmaf008,xmaf009,
                   xmaf010,xmaf011
              INTO g_xmeg2_d[l_ac].xmeg003,g_xmeg_d[l_ac].xmeg006,g_xmeg_d[l_ac].xmeg008,g_xmeg_d[l_ac].xmeg010,l_xmegunit,
                   g_xmeg2_d[l_ac].xmeg028,g_xmeg2_d[l_ac].xmeg029
              FROM xmaf_t 
             WHERE xmafent = g_enterprise 
               AND xmafsite = g_site 
               AND xmaf001 = g_xmee_m.xmee004
               AND xmaf002 = l_controlno 
               AND xmaf003 = g_xmeg_d[l_ac].xmeg001 
               AND xmaf004 = g_xmeg_d[l_ac].xmeg002
            #單頭多角性質為"3:統銷統收"、"4:統銷自收"時，才依客戶預設條件帶出預設值
            IF g_xmee_m.xmee006 MATCHES '[34]' AND NOT cl_null(l_xmegunit) THEN
               LET g_xmeg_d[l_ac].xmegunit = l_xmegunit
            END IF            
            IF g_xmee_m.xmee006 = '4' THEN
               LET g_xmeg_d[l_ac].xmegorga = g_xmeg_d[l_ac].xmegunit
            END IF           
            CALL axmt510_xmee003_ref(g_xmeg_d[l_ac].xmegunit) RETURNING g_xmeg_d[l_ac].xmegunit_desc
            CALL axmt510_xmee003_ref(g_xmeg_d[l_ac].xmegorga) RETURNING g_xmeg_d[l_ac].xmegorga_desc
         END IF
      END IF
      
      IF cl_null(l_controlno) OR l_n = 0 THEN        
         #沒有設置'客戶控制組料件預設條件改抓料件進銷存檔預設的條件
         SELECT imaf112,imaf015,imaf113,imaf091,imaf092,
                imaf123
           INTO g_xmeg_d[l_ac].xmeg006,g_xmeg_d[l_ac].xmeg008,g_xmeg_d[l_ac].xmeg010,g_xmeg2_d[l_ac].xmeg028,g_xmeg2_d[l_ac].xmeg029,
                g_xmeg2_d[l_ac].xmeg003
           FROM imaf_t
         WHERE imafent = g_enterprise AND imafsite = g_site AND imaf001 = g_xmeg_d[l_ac].xmeg001  
      END IF
     
      CALL axmt510_xmeg003_ref(g_xmeg2_d[l_ac].xmeg003) RETURNING  g_xmeg2_d[l_ac].xmeg003_desc
      CALL s_desc_get_unit_desc(g_xmeg_d[l_ac].xmeg006) RETURNING g_xmeg_d[l_ac].xmeg006_desc 
      CALL s_desc_get_unit_desc(g_xmeg_d[l_ac].xmeg008) RETURNING g_xmeg_d[l_ac].xmeg008_desc       
      CALL s_desc_get_unit_desc(g_xmeg_d[l_ac].xmeg010) RETURNING g_xmeg_d[l_ac].xmeg010_desc         
      CALL s_desc_get_stock_desc(g_site,g_xmeg2_d[l_ac].xmeg028) RETURNING g_xmeg2_d[l_ac].xmeg028_desc
      CALL s_desc_get_locator_desc(g_site,g_xmeg2_d[l_ac].xmeg028,g_xmeg2_d[l_ac].xmeg029)
        RETURNING g_xmeg2_d[l_ac].xmeg029_desc
      
      DISPLAY BY NAME g_xmeg2_d[l_ac].xmeg003,g_xmeg2_d[l_ac].xmeg003_desc,
                      g_xmeg_d[l_ac].xmeg006,g_xmeg_d[l_ac].xmeg006_desc,
                      g_xmeg_d[l_ac].xmeg008,g_xmeg_d[l_ac].xmeg008_desc,
                      g_xmeg_d[l_ac].xmeg010,g_xmeg_d[l_ac].xmeg010_desc,
                      g_xmeg_d[l_ac].xmegunit,g_xmeg_d[l_ac].xmegunit_desc,
                      g_xmeg_d[l_ac].xmegorga,g_xmeg_d[l_ac].xmegorga_desc,
                      g_xmeg2_d[l_ac].xmeg028,g_xmeg2_d[l_ac].xmeg028_desc,
                      g_xmeg2_d[l_ac].xmeg029,g_xmeg2_d[l_ac].xmeg029_desc

            
      #若稅別應用為2.依料件設定，呼叫應用元件帶出稅別、稅率
      IF g_tax_app = '2' AND NOT cl_null(g_pmab089) THEN
         #依料件檢核是否有依交易分類設定稅別
         CALL s_tax_chktype(g_xmee_m.xmeesite,g_xmee_m.xmee004,g_xmeg_d[l_ac].xmeg001,'1',g_pmab089)
           RETURNING l_success,l_xmeg016,l_xmeg017            
         IF l_success THEN
            LET g_xmeg_d[l_ac].xmeg016 = l_xmeg016
            LET g_xmeg_d[l_ac].xmeg017 = l_xmeg017
            CALL s_desc_get_tax_desc1(g_site,g_xmeg_d[l_ac].xmeg016) RETURNING g_xmeg_d[l_ac].xmeg016_desc
         ELSE
            #檢查失敗時後續處理
            LET g_xmeg_d[l_ac].xmeg016 = ''
            LET g_xmeg_d[l_ac].xmeg016_desc = ''
            LET g_xmeg_d[l_ac].xmeg017 = ''
         END IF           
      ELSE
         LET g_xmeg_d[l_ac].xmeg016 = g_xmee_m.xmee011
         LET g_xmeg_d[l_ac].xmeg017 = g_xmee_m.xmee012
      END IF             
      DISPLAY BY NAME g_xmeg_d[l_ac].xmeg016,g_xmeg_d[l_ac].xmeg016_desc,g_xmeg_d[l_ac].xmeg017
      
      #若[T:料件進銷存檔].[C:接單拆解方式(訂單)]的值為'1:自動CKD'或是'2:自動SKD'時，則[C:子件特性]的值預設'2:CKD'或是'3:SKD'
      SELECT imaf122 INTO l_imaf122 FROM imaf_t
       WHERE imafent = g_enterprise AND imafsite = g_site AND imaf001 = g_xmeg_d[l_ac].xmeg001
      IF l_imaf122 = '1' THEN
         LET g_xmeg_d[l_ac].xmeg019 = '2'
      ELSE
         IF l_imaf122 = '2' THEN
            LET g_xmeg_d[l_ac].xmeg019 = '3'
         END IF
      END IF      
      DISPLAY BY NAME g_xmeg_d[l_ac].xmeg019

      #若訂單料件有設置'交易對象對應料號'(apmi070)有客戶料號時，需將對應的料號抓出顯示在[C:客戶料號]欄位上，若有設置多筆對應料號時以有勾選主要對應料那一筆為預設值
      IF cl_null(g_xmeg_d[l_ac].xmeg027) THEN
         SELECT pmao004 INTO l_pmao004
           FROM pmao_t
          WHERE pmaoent = g_enterprise
            AND pmao001 = g_xmee_m.xmee004
            AND pmao002 = g_xmeg_d[l_ac].xmeg001
            AND pmao003 = g_xmeg_d[l_ac].xmeg002
            AND pmao007 = 'Y'
         IF cl_null(l_pmao004) THEN
            SELECT pmao004 INTO l_pmao004
              FROM pmao_t
             WHERE pmaoent = g_enterprise
               AND pmao001 = g_xmee_m.xmee004
               AND pmao002 = g_xmeg_d[l_ac].xmeg001
               AND pmao003 = g_xmeg_d[l_ac].xmeg002
               AND rownum = 1
         END IF
         LET g_xmeg_d[l_ac].xmeg027 =  l_pmao004
         DISPLAY BY NAME g_xmeg_d[l_ac].xmeg027        
       END IF  
END FUNCTION]]>
  </point>
