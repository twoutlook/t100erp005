  <point name="set_no_entry_b.set_no_entry_b" order="" ver="4" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   CALL cl_set_comp_entry("xmehseq",FALSE)  
   CALL cl_set_comp_entry("xmef001",FALSE)  
   
   #單頭多角性質為"3:統銷統收"、"4:統銷自收"時，出貨據點才可維護
   IF g_xmee_m.xmee006 NOT MATCHES '[34]' THEN
      CALL cl_set_comp_entry("xmegunit",FALSE)
   END IF
   
   
   #若1.正常稅率，則單身稅別不可輸入
   IF g_tax_app = '1' THEN
      CALL cl_set_comp_entry("xmeg016",FALSE)
   END IF
   #單頭取價方式的基本資料設置不允許修改單價
   IF g_xmah003 <> 'Y' THEN
      CALL cl_set_comp_entry("xmeg015",FALSE)
   END IF

   IF cl_null(g_xmeg_d[l_ac].xmeg001) THEN
      CALL cl_set_comp_entry("xmeg002",FALSE)
   ELSE

      #料件主檔無特徵類別，不能維護產品特徵
      IF cl_null(g_imaa005) THEN
         CALL cl_set_comp_entry("xmeg002",FALSE)
      ELSE
         #若料件有使用產品特徵時，則此欄位不允許空白
         CALL cl_set_comp_required("xmeg002",TRUE)
      END IF
      LET l_imaf015 = ''
      LET l_imaf061 = ''
      LET l_imaf113 = ''
      SELECT imaf015,imaf061,imaf113
        INTO l_imaf015,l_imaf061,l_imaf113
        FROM imaf_t
       WHERE imafent = g_enterprise
         AND imafsite = g_site
         AND imaf001 = g_xmeg_d[l_ac].xmeg001
      #[T:料件據點進銷存檔].[C:批號控管]=1或3時才可輸入
      IF l_imaf061 NOT MATCHES '[13]' THEN
         LET g_xmeg2_d[l_ac].xmeg030 = ' '
         CALL cl_set_comp_entry("xmeg030",FALSE)
      END IF
      #[T:料件據點進銷存檔]未設參考單位，參考單位、參考數量不允許輸入
      IF cl_null(l_imaf015) THEN
         CALL cl_set_comp_entry("xmeg009",FALSE)
      ELSE
         #若有使用參考單位管理時，則不允許空白
         CALL cl_set_comp_required("xmeg008",TRUE)         
      END IF
      #[T:料件據點進銷存檔]有設計價單位，計價單位不允許空白
      IF cl_get_para(g_enterprise,g_site,'S-BAS-0007') = "Y" AND NOT cl_null(l_imaf113) THEN
         CALL cl_set_comp_required("xmeg010",TRUE)
      END IF 
   END IF 
   
   #為多交期時，約定交貨日期、預計簽收日期不可維護
   IF g_xmeg_d[l_ac].xmeg024 = 'Y' THEN
      CALL cl_set_comp_entry("xmeg012,xmeg013",FALSE)
   END IF 
   #多期預付款頁籤：已經有應收帳款與已收金額的資料不可以進行修改
   IF (NOT cl_null(g_xmeg5_d_t.xmef007)) AND (g_xmeg5_d_t.xmef008> 0 OR g_xmeg5_d_t.xmef009 > 0)  OR  NOT g_flag1 THEN
      CALL cl_set_comp_entry("xmef001,xmef002,xmef003,xmef004,xmef005,xmef006",FALSE)
   END IF  
   #多期預付款頁籤：依照單價含稅否開收未稅、含稅欄位輸入
   IF g_xmee_m.xmee031 = 'Y' THEN
      CALL cl_set_comp_entry("xmef005",FALSE)
   ELSE
      CALL cl_set_comp_entry("xmef006",FALSE)
   END IF    ]]>
  </point>
