               RETURNING l_success,l_xmeg_d.xmeg011 
          END IF                     
          #取出價格
          CALL s_sale_price_get(g_xmee_m.xmee017,g_xmee_m.xmee004,l_xmeg_d.xmeg001,l_xmeg_d.xmeg002,g_xmee_m.xmee015,
