      IF NOT s_axmt510_ins_xmek(0,0,p_xmei_d.xmeiseq,"xmdsseq",p_xmei_d.xmei901,l_xmds.xmdsseq,p_xmei_d.xmeiseq,g_xmee_m.xmeedocno,g_xmee_m.xmee900) THEN
         LET r_success = FALSE
         RETURN r_success
      END IF
      LET l_flag = TRUE
   ELSE
      #資料一樣，表示無變更，將變更歷程檔刪除
      IF NOT s_axmt510_del_xmek(0,0,p_xmei_d.xmeiseq,"xmdsseq",g_xmee_m.xmeedocno,g_xmee_m.xmee900) THEN
         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF
 
   #費用料號
   IF p_xmei_d.xmei901 = '3' OR (p_xmei_d.xmei901 = '2' AND
      ((p_xmei_d.xmei001 <> l_xmds.xmds001) OR
      (p_xmei_d.xmei001 IS NULL AND l_xmds.xmds001 IS NOT NULL) OR
      (p_xmei_d.xmei001 IS NOT NULL AND l_xmds.xmds001 IS NULL))) THEN
      #其說明的SQL
