         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = 'sel xmds_t'
         LET g_errparam.popup = TRUE
         CALL cl_err()
         LET r_success = FALSE
         RETURN r_success
      END IF 
   END IF                    

   #項次
   IF p_xmei_d.xmei901 = '3' OR (p_xmei_d.xmei901 = '2' AND
      ((p_xmei_d.xmeiseq <> l_xmds.xmdsseq) OR
