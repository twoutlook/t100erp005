  <point name="delete.head.a_delete" order="" ver="3" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[      #刪除單號
      CALL s_aooi360_del('6',g_prog,g_xmee_m.xmeedocno,'','','','','','','','','4') RETURNING l_success
      IF NOT l_success THEN
         CALL s_transaction_end('N','0')
         RETURN
      END IF 
      #刪除訂單多交期匯總檔
      LET l_cnt = 0
      SELECT COUNT(*) INTO l_cnt
        FROM xmej_t 
       WHERE xmejent = g_enterprise 
         AND xmejdocno = g_xmee_m.xmeedocno
         AND xmej900 = g_xmee_m.xmee900
      IF l_cnt > 0 THEN   
         DELETE FROM xmej_t 
          WHERE xmejent = g_enterprise 
            AND xmejdocno = g_xmee_m.xmeedocno
            AND xmej900 = g_xmee_m.xmee900
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "xmej_t"
            LET g_errparam.popup = FALSE
            CALL cl_err()
            CALL s_transaction_end('N','0')
            RETURN
         END IF
      END IF
      #刪除交易稅明細檔
      LET l_cnt = 0
      SELECT COUNT(*) INTO l_cnt
        FROM xrcd_t
       WHERE xrcdent = g_enterprise
         AND xrcddocno = g_xmee_m.xmeedocno 
         AND xrcdseq2 = g_xmee_m.xmee900         
      IF l_cnt > 0 THEN               
         DELETE FROM xrcd_t
          WHERE xrcdent = g_enterprise
            AND xrcddocno = g_xmee_m.xmeedocno
            AND xrcdseq2 = g_xmee_m.xmee900
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.extend = "DELETE xrcd_t"
            LET g_errparam.code   = SQLCA.sqlcode
            LET g_errparam.popup  = FALSE
            CALL cl_err()
            CALL s_transaction_end('N','0')
            RETURN
         END IF    
      END IF]]>
