  <point name="function.axmt510_change_xmeg016" order="25" ver="7" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 單頭稅別修改時，單身稅別一併修改
# Memo...........:
# Usage..........: CALL axmt510_change_xmeg016(p_cmd)
#                  
# Input parameter: p_cmd     #輸入模式
#                : 
# Return code....: 
#                : 
# Date & Author..: 2014/03/25 By polly
# Modify.........:
################################################################################
PRIVATE FUNCTION axmt510_change_xmeg016(p_cmd)
  DEFINE p_cmd             LIKE type_t.chr1
  DEFINE l_i               LIKE type_t.num10
  DEFINE l_success         LIKE type_t.num5
  DEFINE l_xmegseq         LIKE xmeg_t.xmegseq
  DEFINE l_xmeg001         LIKE xmeg_t.xmeg001
  DEFINE l_xmeg011         LIKE xmeg_t.xmeg011
  DEFINE l_xmeg015         LIKE xmeg_t.xmeg015
  DEFINE l_xmeg016         LIKE xmeg_t.xmeg016
  DEFINE l_xmeg017         LIKE xmeg_t.xmeg017
  DEFINE l_xmeg046         LIKE xmeg_t.xmeg046
  DEFINE l_xmeg047         LIKE xmeg_t.xmeg047
  DEFINE l_xmeg048         LIKE xmeg_t.xmeg048
  DEFINE l_xmeg901         LIKE xmeg_t.xmeg901    #變更類型
  DEFINE l_xmeg016_o       LIKE xmeg_t.xmeg016    #稅別(舊值)
  DEFINE l_xmeg017_o       LIKE xmeg_t.xmeg017    #稅率(舊值)
  DEFINE l_xmeg046_o       LIKE xmeg_t.xmeg046    #未稅金額(舊值)
  DEFINE l_xmeg047_o       LIKE xmeg_t.xmeg047    #含稅金額(舊值)
  DEFINE l_xmeg048_o       LIKE xmeg_t.xmeg048    #稅額(舊值)
                          


   #單頭新增時，因單身資料還沒insert到資料庫，所以只改變畫面上的顯示
   IF p_cmd = 'a' THEN
      FOR l_i = 1 TO g_xmeg_d.getLength()
         IF g_xmeg_d[l_i].xmeg045 MATCHES '[234]' THEN
            CONTINUE FOR
         END IF       
         LET l_xmeg016_o = g_xmeg_d[l_i].xmeg016
         LET l_xmeg017_o = g_xmeg_d[l_i].xmeg017
         LET l_xmeg046_o = g_xmeg_d[l_i].xmeg046
         LET l_xmeg047_o = g_xmeg_d[l_i].xmeg047
         LET l_xmeg048_o = g_xmeg_d[l_i].xmeg048         
         IF g_tax_app = '2' THEN
            #依料件設定
            CALL s_tax_chktype(g_xmee_m.xmeesite,g_xmee_m.xmee004,g_xmeg_d[l_i].xmeg001,'1',g_pmab089)
              RETURNING l_success,l_xmeg016,l_xmeg017
            #稅別檢查失敗，將稅別、稅率清空
            IF NOT l_success THEN                   
               LET l_xmeg016 = ''
               LET l_xmeg017 = ''
            END IF                 
         ELSE
            #依正常稅率
            LET l_xmeg016 = g_xmee_m.xmee011
            LET l_xmeg017 = g_xmee_m.xmee012
         END IF
         #重計未稅金額、含稅金額、稅額
         CALL s_axmt510_get_amount(g_xmee_m.xmee900,g_xmee_m.xmeedocno,g_xmeg_d[l_i].xmegseq,g_xmeg_d[l_i].xmeg011,g_xmeg_d[l_i].xmeg015,l_xmeg016,g_xmee_m.xmee015,g_xmee_m.xmee016)
           RETURNING l_xmeg046,l_xmeg047,l_xmeg048 
         LET g_xmeg_d[l_i].xmeg016 = l_xmeg016
         LET g_xmeg_d[l_i].xmeg017 = l_xmeg017
         LET g_xmeg_d[l_i].xmeg046 = l_xmeg046
         LET g_xmeg_d[l_i].xmeg047 = l_xmeg047
         LET g_xmeg_d[l_i].xmeg048 = l_xmeg048  
         #將稅別、稅率、未稅金額、含稅金額、稅額寫入歷程檔中      
         IF g_xmeg_d[l_i].xmeg016 <> l_xmeg016_o THEN
            LET g_xmeg_d[l_i].xmeg901 = '2'       #單身修改
            CALL s_axmt510_ins_xmek(g_xmeg_d[l_i].xmegseq,0,0,"xmdc016",g_xmeg_d[l_i].xmeg901,l_xmeg016_o,g_xmeg_d[l_i].xmeg016,g_xmee_m.xmeedocno,g_xmee_m.xmee900) 
              RETURNING l_success
         ELSE
            CALL s_axmt510_del_xmek(g_xmeg_d[l_i].xmegseq,0,0,"xmdc016",g_xmee_m.xmeedocno,g_xmee_m.xmee900) 
              RETURNING l_success
         END IF
         IF g_xmeg_d[l_i].xmeg017 <> l_xmeg017_o THEN
            LET g_xmeg_d[l_i].xmeg901 = '2'       #單身修改
            CALL s_axmt510_ins_xmek(g_xmeg_d[l_i].xmegseq,0,0,"xmdc017",g_xmeg_d[l_i].xmeg901,l_xmeg017_o,g_xmeg_d[l_i].xmeg017,g_xmee_m.xmeedocno,g_xmee_m.xmee900) 
              RETURNING l_success
         ELSE
            CALL s_axmt510_del_xmek(g_xmeg_d[l_i].xmegseq,0,0,"xmdc017",g_xmee_m.xmeedocno,g_xmee_m.xmee900) 
              RETURNING l_success
         END IF         
         IF g_xmeg_d[l_i].xmeg046 <> l_xmeg046_o THEN
            LET g_xmeg_d[l_i].xmeg901 = '2'       #單身修改
            CALL s_axmt510_ins_xmek(g_xmeg_d[l_i].xmegseq,0,0,"xmdc046",g_xmeg_d[l_i].xmeg901,l_xmeg046_o,g_xmeg_d[l_i].xmeg046,g_xmee_m.xmeedocno,g_xmee_m.xmee900) 
              RETURNING l_success
         ELSE
            CALL s_axmt510_del_xmek(g_xmeg_d[l_i].xmegseq,0,0,"xmdc046",g_xmee_m.xmeedocno,g_xmee_m.xmee900) 
              RETURNING l_success         
         END IF
         IF g_xmeg_d[l_i].xmeg047 <> l_xmeg047_o THEN
            LET g_xmeg_d[l_i].xmeg901 = '2'       #單身修改
            CALL s_axmt510_ins_xmek(g_xmeg_d[l_i].xmegseq,0,0,"xmdc047",g_xmeg_d[l_i].xmeg901,l_xmeg047_o,g_xmeg_d[l_i].xmeg047,g_xmee_m.xmeedocno,g_xmee_m.xmee900) 
              RETURNING l_success 
         ELSE
            CALL s_axmt510_del_xmek(g_xmeg_d[l_i].xmegseq,0,0,"xmdc047",g_xmee_m.xmeedocno,g_xmee_m.xmee900) 
              RETURNING l_success               
         END IF
         IF g_xmeg_d[l_i].xmeg048 <> l_xmeg048_o THEN
            LET g_xmeg_d[l_i].xmeg901 = '2'       #單身修改
            CALL s_axmt510_ins_xmek(g_xmeg_d[l_i].xmegseq,0,0,"xmdc048",g_xmeg_d[l_i].xmeg901,l_xmeg048_o,g_xmeg_d[l_i].xmeg048,g_xmee_m.xmeedocno,g_xmee_m.xmee900) 
              RETURNING l_success
         ELSE
            CALL s_axmt510_del_xmek(g_xmeg_d[l_i].xmegseq,0,0,"xmdc048",g_xmee_m.xmeedocno,g_xmee_m.xmee900) 
              RETURNING l_success               
         END IF  
      END FOR
   ELSE
      DECLARE sel_xmeg016_cs CURSOR FOR
       SELECT xmegseq,xmeg001,xmeg011,xmeg015,xmeg016,xmeg017,
              xmeg046,xmeg047,xmeg048
         FROM xmee_t,xmeg_t
        WHERE xmeeent = g_enterprise
          AND xmeedocno = g_xmee_m.xmeedocno
          AND xmee900 = g_xmee_m.xmee900
