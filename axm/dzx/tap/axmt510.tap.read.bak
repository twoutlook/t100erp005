   #預估金額
   IF p_xmei_d.xmei901 = '3' OR (p_xmei_d.xmei901 = '2' AND
      ((p_xmei_d.xmei004 <> l_xmds.xmds004) OR
      (p_xmei_d.xmei004 IS NULL AND l_xmds.xmds004 IS NOT NULL) OR
      (p_xmei_d.xmei004 IS NOT NULL AND l_xmds.xmds004 IS NULL))) THEN
