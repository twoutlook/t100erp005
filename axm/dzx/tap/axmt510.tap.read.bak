         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF
   
   #建議廠商
   IF p_xmei_d.xmei901 = '3' OR (p_xmei_d.xmei901 = '2' AND
      ((p_xmei_d.xmei005 <> l_xmds.xmds005) OR
      (p_xmei_d.xmei005 IS NULL AND l_xmds.xmds005 IS NOT NULL) OR
