  <point name="menu.open_apmi004_01" order="" ver="3" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[               #若輸入供應商的法人類型為'2:一次性交易'或是'4:內部員工'時，則自動串apmi004_01
               #維護一次性交易對項基本資料，維護完基本資料後會回傳一個一次性交易對象識別碼，
               #將識別碼值預設給xmee028欄位
               IF NOT cl_null(g_xmee_m.xmee004) THEN
                  LET l_pmaa004 = ''
                  SELECT pmaa004 INTO l_pmaa004 FROM pmaa_t WHERE pmaaent = g_enterprise AND pmaa001 = g_xmee_m.xmee004
                  IF l_pmaa004 = '2' THEN   #一次性交易對象
                     CALL apmi004_01('1','',g_xmee_m.xmee004,g_xmee_m.xmeedocno) RETURNING g_xmee_m.xmee028
                  END IF
                  IF l_pmaa004 = '4' THEN   #內部員工
                     CALL apmi004_01('2','',g_xmee_m.xmee004,g_xmee_m.xmeedocno) RETURNING g_xmee_m.xmee028
                  END IF                      
               END IF   ]]>
  </point>
  <point name="menu.open_axmt510_03" order="" ver="4" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[               IF cl_null(g_detail_idx) OR g_detail_idx = 0 THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = 'agl-00066'
                  LET g_errparam.extend = ""
                  LET g_errparam.popup = TRUE
                  CALL cl_err()
                  EXIT DIALOG
               END IF
               IF g_xmeg_d[g_detail_idx].xmeg045 MATCHES "[234]" THEN    #行狀態等於結案的
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.extend = g_xmee_m.xmeedocno
                  LET g_errparam.replace[1] = g_xmeg_d[g_detail_idx].xmegseq
                  LET g_errparam.code   = "axm-00554"
                  LET g_errparam.popup  = TRUE
                  CALL cl_err()                  
                  EXIT DIALOG
               END IF 
               CALL s_transaction_begin()
               CALL axmt510_03(g_xmee_m.xmeedocno,g_xmeg_d[g_detail_idx].xmegseq,g_xmeg_d[g_detail_idx].xmeg007,g_xmee_m.xmee900)
                 RETURNING l_xmeg012,l_xmeg013
               #xmeg012、xmeg013其中一個有值,表示有多交期資料,就將抓到的值寫回xmeg_t
               IF NOT (cl_null(l_xmeg012) AND cl_null(l_xmeg013)) THEN
                  #修改約定交貨日、預定簽收日
                  UPDATE xmeg_t
                     SET xmeg012 = l_xmeg012,
                         xmeg013 = l_xmeg013,
                         xmeg024 = 'Y',
                         xmeg901 = '2'  
                   WHERE xmegent = g_enterprise
                     AND xmegdocno = g_xmee_m.xmeedocno
                     AND xmegseq = g_xmeg_d[g_detail_idx].xmegseq
                     AND xmeg900 = g_xmee_m.xmee900
                  IF SQLCA.sqlcode THEN
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code   = SQLCA.sqlcode
                     LET g_errparam.extend = "UPDATE xmeg012,xmeg013"
                     LET g_errparam.popup  = FALSE
                     CALL cl_err()                  
                     CALL s_transaction_end('N','0')
                     EXIT DIALOG
                  END IF  
               END IF               
               #修改後，需重新產生交期明細資料
               IF NOT s_axmt510_gen_xmeh(g_xmee_m.xmeedocno,g_xmeg_d[g_detail_idx].xmegseq,g_xmee_m.xmee900) THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = SQLCA.sqlcode
                  LET g_errparam.extend = "xmeh_t"
                  LET g_errparam.popup = TRUE
                  CALL cl_err()
                  CALL s_transaction_end('N','0')
                  EXIT DIALOG
               END IF
               CALL s_transaction_end('Y','0')
               CALL axmt510_b_fill()]]>
  </point>
