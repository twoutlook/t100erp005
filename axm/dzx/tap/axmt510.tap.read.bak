      IF NOT s_axmt510_ins_xmek(0,0,p_xmei_d.xmeiseq,"xmds004",p_xmei_d.xmei901,l_xmds.xmds004,p_xmei_d.xmei004,g_xmee_m.xmeedocno,g_xmee_m.xmee900) THEN
