  <point name="menu.prog_xmee008" order="" ver="8" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[               #應用 a41 樣板自動產生(Version:2)
               #使用JSON格式組合參數與作業編號(串查)
               #抓取單據別
               LET l_slip = ''
               LET l_prog = ''
               LET l_gzzc002 = ''
               IF NOT cl_null(g_xmee_m.xmee008) THEN
                  CALL s_aooi200_get_slip(g_xmee_m.xmee008) RETURNING l_success,l_slip
                  IF NOT cl_null(l_slip) THEN
                     SELECT oobx004 INTO l_prog
                       FROM oobx_t
                      WHERE oobxent = g_enterprise
                        AND oobx001 = l_slip
                  END IF
                  IF NOT cl_null(l_prog) THEN
                     SELECT MAX(gzzc002) INTO l_gzzc002
                       FROM gzzc_t
                      WHERE gzzc001 = l_prog
                     IF cl_null(l_gzzc002) THEN
                        LET l_gzzc002 = 1
                     ELSE 
                        LET l_gzzc002 = l_gzzc002 - 1
                     END IF
                     INITIALIZE la_param.* TO NULL
                     LET la_param.prog     = l_prog
                     LET la_param.param[l_gzzc002] = g_xmee_m.xmee008
                     
                     LET ls_js = util.JSON.stringify(la_param)
                     CALL cl_cmdrun_wait(ls_js)
                  END IF
               END IF

]]>
