   ELSE
      #資料一樣，表示無變更，將變更歷程檔刪除
      IF NOT s_axmt510_del_xmek(0,0,p_xmei_d.xmeiseq,"xmds002",g_xmee_m.xmeedocno,g_xmee_m.xmee900) THEN
         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF  
 
    #匯率
   IF p_xmei_d.xmei901 = '3' OR (p_xmei_d.xmei901 = '2' AND
      ((p_xmei_d.xmei003 <> l_xmds.xmds003) OR
