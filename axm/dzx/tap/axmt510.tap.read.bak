      END IF
      LET l_flag = TRUE
   ELSE
      #資料一樣，表示無變更，將變更歷程檔刪除
      IF NOT s_axmt510_del_xmek(0,0,p_xmei_d.xmeiseq,"xmds001",g_xmee_m.xmeedocno,g_xmee_m.xmee900) THEN
         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF   
 
   #幣別
   IF p_xmei_d.xmei901 = '3' OR (p_xmei_d.xmei901 = '2' AND
      ((p_xmei_d.xmei002 <> l_xmds.xmds002) OR
      (p_xmei_d.xmei002 IS NULL AND l_xmds.xmds002 IS NOT NULL) OR
      (p_xmei_d.xmei002 IS NOT NULL AND l_xmds.xmds002 IS NULL))) THEN
      #其說明的SQL
      LET g_xmek007 = "SELECT ooail003 FROM ooail_t WHERE ooailent='"||g_enterprise||"' AND ooail001='"||l_xmds.xmds002||"' AND ooail002=? "
      #新增資料到變更歷程檔
      IF NOT s_axmt510_ins_xmek(0,0,p_xmei_d.xmeiseq,"xmds002",p_xmei_d.xmei901,l_xmds.xmds002,p_xmei_d.xmei002,g_xmee_m.xmeedocno,g_xmee_m.xmee900) THEN
         LET r_success = FALSE
