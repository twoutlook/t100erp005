   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON xmeedocno,xmee902,xmee900,xmee002,xmee003,xmee004
                          FROM s_browse[1].b_xmeedocno,s_browse[1].b_xmee902,s_browse[1].b_xmee900,s_browse[1].b_xmee002, 
                              s_browse[1].b_xmee003,s_browse[1].b_xmee004
 
         BEFORE CONSTRUCT
               DISPLAY axmt510_filter_parser('xmeedocno') TO s_browse[1].b_xmeedocno
            DISPLAY axmt510_filter_parser('xmee902') TO s_browse[1].b_xmee902
            DISPLAY axmt510_filter_parser('xmee900') TO s_browse[1].b_xmee900
