         DISPLAY BY NAME g_xmee_m.xmee025_desc,g_xmee_m.oofb017
      END IF
      
      ##發票客戶
      #LET g_xmee_m.xmee203 = ''
      #LET g_xmee_m.xmee203_desc = ''      
      #CALL s_axmt540_client_partner(g_xmee_m.xmeedocno,g_xmee_m.xmee004,'3') RETURNING g_xmee_m.xmee203
      #LET g_xmee_m_o.xmee203 = g_xmee_m.xmee203
      #CALL s_desc_get_trading_partner_abbr_desc(g_xmee_m.xmee203) RETURNING g_xmee_m.xmee203_desc
      #DISPLAY BY NAME g_xmee_m.xmee203,g_xmee_m.xmee203_desc      


      #客戶連絡人
      LET g_xmee_m.xmee027 = ''
      LET g_xmee_m.xmee027_desc = ''
      LET l_sql = "SELECT pmaj002 FROM pmaj_t ",
                  " WHERE pmajent = ",g_enterprise, 
                  "   AND pmaj001 = '",g_xmee_m.xmee004,"' ",
                  "   AND pmajstus = 'Y' ",
                  "   AND pmaj004 = 'Y' "
      PREPARE axmt510_pmaj002_pre FROM l_sql
      DECLARE axmt510_pmaj002_cur SCROLL CURSOR FOR axmt510_pmaj002_pre
      OPEN axmt510_pmaj002_cur
      FETCH FIRST axmt510_pmaj002_cur INTO g_xmee_m.xmee027            
      CALL axmt510_xmee027_ref(g_xmee_m.xmee027) RETURNING g_xmee_m.xmee027_desc
      DISPLAY BY NAME g_xmee_m.xmee027,g_xmee_m.xmee027_desc
END FUNCTION]]>
