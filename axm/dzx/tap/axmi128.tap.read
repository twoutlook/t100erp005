  <section id="axmi128.modify" ver="14" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION axmi128_modify()
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point  
   #add-point:modify段define
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point    
   
   IF g_xmau_m.xmau001 IS NULL
   OR g_xmau_m.xmau002 IS NULL
   OR g_xmau_m.xmau003 IS NULL
   OR g_xmau_m.xmau005 IS NULL
   OR g_xmau_m.xmau006 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
   LET g_xmau001_t = g_xmau_m.xmau001
   LET g_xmau002_t = g_xmau_m.xmau002
   LET g_xmau003_t = g_xmau_m.xmau003
   LET g_xmau005_t = g_xmau_m.xmau005
   LET g_xmau006_t = g_xmau_m.xmau006
 
   CALL s_transaction_begin()
   
   OPEN axmi128_cl USING g_enterprise,g_xmau_m.xmau001,g_xmau_m.xmau002,g_xmau_m.xmau003,g_xmau_m.xmau005,g_xmau_m.xmau006
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN axmi128_cl:" 
      LET g_errparam.code   =  STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE axmi128_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE axmi128_master_referesh USING g_xmau_m.xmau001,g_xmau_m.xmau002,g_xmau_m.xmau003,g_xmau_m.xmau005, 
       g_xmau_m.xmau006 INTO g_xmau_m.xmau001,g_xmau_m.xmau002,g_xmau_m.xmau003,g_xmau_m.xmau005,g_xmau_m.xmau006, 
       g_xmau_m.xmau001_desc,g_xmau_m.xmau002_desc,g_xmau_m.xmau003_desc,g_xmau_m.xmau005_desc
   
   #遮罩相關處理
   LET g_xmau_m_mask_o.* =  g_xmau_m.*
   CALL axmi128_xmau_t_mask()
   LET g_xmau_m_mask_n.* =  g_xmau_m.*
   
   CALL s_transaction_end('Y','0')
 
   CALL axmi128_show()
   WHILE TRUE
      LET g_xmau001_t = g_xmau_m.xmau001
      LET g_xmau002_t = g_xmau_m.xmau002
      LET g_xmau003_t = g_xmau_m.xmau003
      LET g_xmau005_t = g_xmau_m.xmau005
      LET g_xmau006_t = g_xmau_m.xmau006
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL axmi128_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_xmau_m.* = g_xmau_m_t.*
         CALL axmi128_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更(更新單身table的key欄位值)
      IF g_xmau_m.xmau001 != g_xmau001_t 
      OR g_xmau_m.xmau002 != g_xmau002_t 
      OR g_xmau_m.xmau003 != g_xmau003_t 
      OR g_xmau_m.xmau005 != g_xmau005_t 
      OR g_xmau_m.xmau006 != g_xmau006_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("modify,modify_detail,delete,reproduce", TRUE)
   CALL axmi128_set_act_visible()
   CALL axmi128_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " xmauent = '" ||g_enterprise|| "' AND",
                      " xmau001 = '", g_xmau_m.xmau001, "' "
                      ," AND xmau002 = '", g_xmau_m.xmau002, "' "
                      ," AND xmau003 = '", g_xmau_m.xmau003, "' "
                      ," AND xmau005 = '", g_xmau_m.xmau005, "' "
                      ," AND xmau006 = '", g_xmau_m.xmau006, "' "
 
   #填到對應位置
   CALL axmi128_browser_fill("")
 
   CALL axmi128_idx_chk()
 
   CLOSE axmi128_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL axmi128_msgcentre_notify('modify')
   
END FUNCTION   
]]>
  </section>
