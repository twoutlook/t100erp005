  <section id="axmi128.delete" ver="16" status="" src="s" readonly="">
    <![CDATA[#+ 資料刪除
PRIVATE FUNCTION axmi128_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define" edit="s"/>}
   #end add-point     
   #add-point:delete段define
   {<point name="delete.define_customerization" edit="c"/>}
   #end add-point
   
   IF g_xmau_m.xmau001 IS NULL
   OR g_xmau_m.xmau002 IS NULL
   OR g_xmau_m.xmau003 IS NULL
   OR g_xmau_m.xmau005 IS NULL
   OR g_xmau_m.xmau006 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   
 
   OPEN axmi128_cl USING g_enterprise,g_xmau_m.xmau001,g_xmau_m.xmau002,g_xmau_m.xmau003,g_xmau_m.xmau005,g_xmau_m.xmau006
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN axmi128_cl:" 
      LET g_errparam.code   =  STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE axmi128_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE axmi128_master_referesh USING g_xmau_m.xmau001,g_xmau_m.xmau002,g_xmau_m.xmau003,g_xmau_m.xmau005, 
       g_xmau_m.xmau006 INTO g_xmau_m.xmau001,g_xmau_m.xmau002,g_xmau_m.xmau003,g_xmau_m.xmau005,g_xmau_m.xmau006, 
       g_xmau_m.xmau001_desc,g_xmau_m.xmau002_desc,g_xmau_m.xmau003_desc,g_xmau_m.xmau005_desc
   
   #遮罩相關處理
   LET g_xmau_m_mask_o.* =  g_xmau_m.*
   CALL axmi128_xmau_t_mask()
   LET g_xmau_m_mask_n.* =  g_xmau_m.*
   
   CALL axmi128_show()
 
   #單頭多語言資料備份
   
   
   IF cl_ask_del_master() THEN              #確認一下
      #應用 a47 樣板自動產生(Version:2)
      #刪除相關文件
      CALL axmi128_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM xmau_t WHERE xmauent = g_enterprise AND xmau001 = g_xmau_m.xmau001
                                                               AND xmau002 = g_xmau_m.xmau002
                                                               AND xmau003 = g_xmau_m.xmau003
                                                               AND xmau005 = g_xmau_m.xmau005
                                                               AND xmau006 = g_xmau_m.xmau006
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "xmau_t" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      #單頭多語言刪除
      
      
      #單身多語言刪除
      
 
 
   
      #add-point:多語言刪除
      {<point name="delete.lang.delete"/>}
      #end add-point
      
      #頭先不紀錄
      #IF NOT cl_log_modified_record('','') THEN 
      #   CLOSE axmi128_cl
      #   CALL s_transaction_end('N','0')
      #   RETURN 
      #END IF
      
      CLEAR FORM
      CALL g_xmau_d.clear() 
      CALL g_xmau2_d.clear()       
 
     
      CALL axmi128_ui_browser_refresh()  
      #CALL axmi128_ui_headershow()  
      #CALL axmi128_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL axmi128_fetch('P')
      ELSE
         #LET g_wc = " 1=1"
         #LET g_wc2 = " 1=1"
         #CALL axmi128_browser_fill("F")
