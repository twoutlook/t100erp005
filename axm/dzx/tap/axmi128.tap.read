  <point name="input.c.page1.xmau018" order="" ver="8" cite_std="N" new="N" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[            #此段落由子樣板a07產生            
            #開窗i段      
            
            LET l_chk = FALSE                 #避免輸入料件編號出現二次開窗
            
            #若料件編號有值
            IF NOT cl_null(g_xmau_d[l_ac].xmau011) THEN
