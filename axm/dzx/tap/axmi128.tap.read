  <point name="function.axmi128_set_visible" order="19" ver="8" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION axmi128_set_visible()
DEFINE l_xmas    RECORD LIKE xmas_t.*

   SELECT * INTO l_xmas.* FROM xmas_t
    WHERE xmasent = g_enterprise AND xmas001 = g_xmas001
   CALL cl_set_comp_visible("xmau012,xmau013,xmau014,xmau015,xmau016,xmau017,xmau019,xmau020,xmau021",TRUE)
   CALL cl_set_comp_visible("xmau012_2,xmau013_2,xmau014_2,xmau015_2,xmau016_2,xmau017_2,xmau019_2,xmau020_2,xmau021_2",TRUE)
   CALL cl_set_comp_visible("xmau013_desc,xmau014_desc,xmau015_desc,xmau016_desc,xmau017_desc,xmau019_desc,oodb005,xmau020_desc,xmau021_desc",TRUE)
   IF l_xmas.xmas002 = 'N' THEN CALL cl_set_comp_visible("xmau012,xmau012_desc",FALSE) END IF
   IF l_xmas.xmas003 = 'N' THEN CALL cl_set_comp_visible("xmau013,xmau013_desc,xmau013_2",FALSE) END IF
   IF l_xmas.xmas004 = 'N' THEN CALL cl_set_comp_visible("xmau014,xmau014_desc,xmau014_2",FALSE) END IF
   IF l_xmas.xmas005 = 'N' THEN CALL cl_set_comp_visible("xmau015,xmau015_desc,xmau015_2",FALSE) END IF
   IF l_xmas.xmas006 = 'N' THEN CALL cl_set_comp_visible("xmau016,xmau016_desc,xmau016_2",FALSE) END IF
   IF l_xmas.xmas007 = 'N' THEN CALL cl_set_comp_visible("xmau017,xmau017_desc,xmau017_2",FALSE) END IF
   IF l_xmas.xmas008 = 'N' THEN CALL cl_set_comp_visible("xmau019,xmau019_desc,oodb005,xmau019_2",FALSE) END IF
   IF l_xmas.xmas009 = 'N' THEN CALL cl_set_comp_visible("xmau020,xmau020_desc,xmau020_2",FALSE) END IF
   IF l_xmas.xmas010 = 'N' THEN CALL cl_set_comp_visible("xmau021,xmau021_desc,xmau021_2",FALSE) END IF
   #2015-06-10----dorislai----add
   
   LET g_column_chk = 4   #僅有一欄位顯示
   #依axmi127隱藏欄位
   IF l_xmas.xmas011 = 'N' THEN 
      CALL cl_set_comp_visible("xmau011,xmau011_desc,xmau011_desc_1",FALSE) 
   END IF
   IF l_xmas.xmas012 = 'N' THEN  
      CALL cl_set_comp_visible("xmau031,xmau031_desc",FALSE) 
   END IF
   IF l_xmas.xmas013 = 'N' THEN
      CALL cl_set_comp_visible("xmau032,xmau032_desc",FALSE) 
   END IF
   #用來跳訊息的
   IF l_xmas.xmas011 = 'N' AND l_xmas.xmas012 = 'Y' AND l_xmas.xmas013 = 'Y' THEN 
      LET g_column_chk = 1
   END IF
   IF l_xmas.xmas012 = 'N' AND l_xmas.xmas011 = 'Y' AND l_xmas.xmas013 = 'Y' THEN
      LET g_column_chk = 2   
   END IF
   IF l_xmas.xmas013 = 'N' AND l_xmas.xmas011 = 'Y' AND l_xmas.xmas012 = 'Y' THEN
      LET g_column_chk = 3  
   END IF
   IF (l_xmas.xmas013 = 'Y' AND l_xmas.xmas011 = 'Y' AND l_xmas.xmas012 = 'Y')
       OR (cl_null(l_xmas.xmas013) AND cl_null(l_xmas.xmas011) AND cl_null(l_xmas.xmas012)) THEN
      LET g_column_chk = 0  
   END IF
   CALL axmi128_set_no_required_b()
   #2015-06-10----dorislai----end

END FUNCTION]]>
  </point>
