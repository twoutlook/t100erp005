   DEFINE l_newno05     LIKE xmau_t.xmau006 
   DEFINE l_oldno05     LIKE xmau_t.xmau006 
 
   DEFINE l_master    RECORD LIKE xmau_t.*
   DEFINE l_detail    RECORD LIKE xmau_t.*
 
   DEFINE l_cnt       LIKE type_t.num10
   #add-point:reproduce段define
   {<point name="reproduce.define" edit="s"/>}
   #end add-point
   #add-point:reproduce段define
   {<point name="reproduce.define_customerization" edit="c"/>}
   #end add-point
 
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF     
 
   IF g_xmau_m.xmau001 IS NULL
      OR g_xmau_m.xmau002 IS NULL
      OR g_xmau_m.xmau003 IS NULL
      OR g_xmau_m.xmau005 IS NULL
      OR g_xmau_m.xmau006 IS NULL
 
      THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
   
   LET g_xmau001_t = g_xmau_m.xmau001
   LET g_xmau002_t = g_xmau_m.xmau002
   LET g_xmau003_t = g_xmau_m.xmau003
   LET g_xmau005_t = g_xmau_m.xmau005
   LET g_xmau006_t = g_xmau_m.xmau006
 
   
   LET g_xmau_m.xmau001 = ""
   LET g_xmau_m.xmau002 = ""
   LET g_xmau_m.xmau003 = ""
   LET g_xmau_m.xmau005 = ""
   LET g_xmau_m.xmau006 = ""
 
   LET g_master_insert = FALSE
   CALL axmi128_set_entry('a')
   CALL axmi128_set_no_entry('a')
   
   CALL cl_set_head_visible("","YES")
   CALL s_transaction_begin()
   
   #add-point:複製輸入前
   {<point name="reproduce.head.b_input"/>}
   #end add-point
   
   #清空key欄位的desc
      LET g_xmau_m.xmau001_desc = ''
   DISPLAY BY NAME g_xmau_m.xmau001_desc
   LET g_xmau_m.xmau002_desc = ''
   DISPLAY BY NAME g_xmau_m.xmau002_desc
   LET g_xmau_m.xmau003_desc = ''
   DISPLAY BY NAME g_xmau_m.xmau003_desc
   LET g_xmau_m.xmau005_desc = ''
   DISPLAY BY NAME g_xmau_m.xmau005_desc
 
   
   CALL axmi128_input("r")
    
   IF INT_FLAG AND NOT g_master_insert THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = '' 
      LET g_errparam.code   = 9001 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
      DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
      INITIALIZE g_xmau_m.* TO NULL
      INITIALIZE g_xmau_d TO NULL
      INITIALIZE g_xmau2_d TO NULL
 
      CALL axmi128_show()
      LET INT_FLAG = 0
      RETURN 
   END IF
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("modify,modify_detail,delete,reproduce", TRUE)
   CALL axmi128_set_act_visible()
   CALL axmi128_set_act_no_visible()
 
   #將新增的資料併入搜尋條件中
   LET g_state = "insert"
   
   LET g_xmau001_t = g_xmau_m.xmau001
   LET g_xmau002_t = g_xmau_m.xmau002
   LET g_xmau003_t = g_xmau_m.xmau003
   LET g_xmau005_t = g_xmau_m.xmau005
   LET g_xmau006_t = g_xmau_m.xmau006
 
   
   #組合新增資料的條件
   LET g_add_browse = " xmauent = '" ||g_enterprise|| "' AND",
                      " xmau001 = '", g_xmau_m.xmau001, "' "
                      ," AND xmau002 = '", g_xmau_m.xmau002, "' "
                      ," AND xmau003 = '", g_xmau_m.xmau003, "' "
                      ," AND xmau005 = '", g_xmau_m.xmau005, "' "
                      ," AND xmau006 = '", g_xmau_m.xmau006, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL axmi128_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   LET g_curr_diag = ui.DIALOG.getCurrent()
   CALL axmi128_idx_chk()
   
   #add-point:完成複製段落後
   {<point name="reproduce.after_reproduce" />}
