  <section id="axmi128.b_fill" ver="15" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION axmi128_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define" edit="s"/>}
   #end add-point     
   #add-point:b_fill段define
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
   
   #先清空單身變數內容
   CALL g_xmau_d.clear()
   CALL g_xmau2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE xmaustus,xmau011,xmau012,xmau031,xmau032,xmau013,xmau014,xmau015,xmau016, 
       xmau017,xmau018,xmau019,xmau020,xmau021,xmau022,xmau100,xmau011,xmau012,xmau013,xmau014,xmau015, 
       xmau016,xmau017,xmau018,xmau019,xmau020,xmau021,xmauownid,xmauowndp,xmaucrtid,xmaucrtdp,xmaucrtdt, 
       xmaumodid,xmaumoddt,t1.imaal003 ,t2.oocel003 ,t3.oocgl003 ,t4.oocil004 ,t5.oocql004 ,t6.oocql004 , 
       t7.oocal003 ,t8.oodbl004 ,t9.ooibl003 ,t10.oocql004 ,t11.ooag011 ,t12.ooefl003 ,t13.ooag011 , 
       t14.ooefl003 ,t15.ooag011 FROM xmau_t",   
               "",
               
                              " LEFT JOIN imaal_t t1 ON t1.imaalent='"||g_enterprise||"' AND t1.imaal001=xmau011 AND t1.imaal002='"||g_dlang||"' ",
               " LEFT JOIN oocel_t t2 ON t2.oocelent='"||g_enterprise||"' AND t2.oocel001=xmau013 AND t2.oocel002='"||g_dlang||"' ",
               " LEFT JOIN oocgl_t t3 ON t3.oocglent='"||g_enterprise||"' AND t3.oocgl001=xmau014 AND t3.oocgl002='"||g_dlang||"' ",
               " LEFT JOIN oocil_t t4 ON t4.oocilent='"||g_enterprise||"' AND t4.oocil001=xmau014 AND t4.oocil002=xmau015 AND t4.oocil003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t5 ON t5.oocqlent='"||g_enterprise||"' AND t5.oocql001='283' AND t5.oocql002=xmau016 AND t5.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t6 ON t6.oocqlent='"||g_enterprise||"' AND t6.oocql001='281' AND t6.oocql002=xmau017 AND t6.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t7 ON t7.oocalent='"||g_enterprise||"' AND t7.oocal001=xmau018 AND t7.oocal002='"||g_dlang||"' ",
               " LEFT JOIN oodbl_t t8 ON t8.oodblent='"||g_enterprise||"' AND t8.oodbl001='' AND t8.oodbl002=xmau019 AND t8.oodbl003='"||g_dlang||"' ",
               " LEFT JOIN ooibl_t t9 ON t9.ooiblent='"||g_enterprise||"' AND t9.ooibl002=xmau020 AND t9.ooibl003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t10 ON t10.oocqlent='"||g_enterprise||"' AND t10.oocql001='238' AND t10.oocql002=xmau021 AND t10.oocql003='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t11 ON t11.ooagent='"||g_enterprise||"' AND t11.ooag001=xmauownid  ",
               " LEFT JOIN ooefl_t t12 ON t12.ooeflent='"||g_enterprise||"' AND t12.ooefl001=xmauowndp AND t12.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t13 ON t13.ooagent='"||g_enterprise||"' AND t13.ooag001=xmaucrtid  ",
               " LEFT JOIN ooefl_t t14 ON t14.ooeflent='"||g_enterprise||"' AND t14.ooefl001=xmaucrtdp AND t14.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t15 ON t15.ooagent='"||g_enterprise||"' AND t15.ooag001=xmaumodid  ",
 
               " WHERE xmauent= ? AND xmau001=? AND xmau002=? AND xmau003=? AND xmau005=? AND xmau006=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("xmau_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF axmi128_fill_chk(1) THEN
      IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
         LET g_sql = g_sql, " ORDER BY xmau_t.xmau011,xmau_t.xmau012,xmau_t.xmau013,xmau_t.xmau014,xmau_t.xmau015,xmau_t.xmau016,xmau_t.xmau017,xmau_t.xmau018,xmau_t.xmau019,xmau_t.xmau020,xmau_t.xmau021,xmau_t.xmau031,xmau_t.xmau032"
         #add-point:b_fill段fill_before
         {<point name="b_fill.fill_before"/>}
         #end add-point
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE axmi128_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR axmi128_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_xmau_m.xmau001,g_xmau_m.xmau002,g_xmau_m.xmau003,g_xmau_m.xmau005,g_xmau_m.xmau006
                                               
      FOREACH b_fill_cs INTO g_xmau_d[l_ac].xmaustus,g_xmau_d[l_ac].xmau011,g_xmau_d[l_ac].xmau012,g_xmau_d[l_ac].xmau031, 
          g_xmau_d[l_ac].xmau032,g_xmau_d[l_ac].xmau013,g_xmau_d[l_ac].xmau014,g_xmau_d[l_ac].xmau015, 
          g_xmau_d[l_ac].xmau016,g_xmau_d[l_ac].xmau017,g_xmau_d[l_ac].xmau018,g_xmau_d[l_ac].xmau019, 
          g_xmau_d[l_ac].xmau020,g_xmau_d[l_ac].xmau021,g_xmau_d[l_ac].xmau022,g_xmau_d[l_ac].xmau100, 
          g_xmau2_d[l_ac].xmau011,g_xmau2_d[l_ac].xmau012,g_xmau2_d[l_ac].xmau013,g_xmau2_d[l_ac].xmau014, 
          g_xmau2_d[l_ac].xmau015,g_xmau2_d[l_ac].xmau016,g_xmau2_d[l_ac].xmau017,g_xmau2_d[l_ac].xmau018, 
          g_xmau2_d[l_ac].xmau019,g_xmau2_d[l_ac].xmau020,g_xmau2_d[l_ac].xmau021,g_xmau2_d[l_ac].xmauownid, 
          g_xmau2_d[l_ac].xmauowndp,g_xmau2_d[l_ac].xmaucrtid,g_xmau2_d[l_ac].xmaucrtdp,g_xmau2_d[l_ac].xmaucrtdt, 
          g_xmau2_d[l_ac].xmaumodid,g_xmau2_d[l_ac].xmaumoddt,g_xmau_d[l_ac].xmau011_desc,g_xmau_d[l_ac].xmau013_desc, 
          g_xmau_d[l_ac].xmau014_desc,g_xmau_d[l_ac].xmau015_desc,g_xmau_d[l_ac].xmau016_desc,g_xmau_d[l_ac].xmau017_desc, 
          g_xmau_d[l_ac].xmau018_desc,g_xmau_d[l_ac].xmau019_desc,g_xmau_d[l_ac].xmau020_desc,g_xmau_d[l_ac].xmau021_desc, 
          g_xmau2_d[l_ac].xmauownid_desc,g_xmau2_d[l_ac].xmauowndp_desc,g_xmau2_d[l_ac].xmaucrtid_desc, 
          g_xmau2_d[l_ac].xmaucrtdp_desc,g_xmau2_d[l_ac].xmaumodid_desc
                             
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #應用 a12 樣板自動產生(Version:3)
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF 
            EXIT FOREACH
         END IF
      
         LET l_ac = l_ac + 1
         
      END FOREACH
 
            CALL g_xmau_d.deleteElement(g_xmau_d.getLength())
      CALL g_xmau2_d.deleteElement(g_xmau2_d.getLength())
 
      
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_xmau_d.getLength()
      LET g_xmau_d_mask_o[l_ac].* =  g_xmau_d[l_ac].*
      CALL axmi128_xmau_t_mask()
      LET g_xmau_d_mask_n[l_ac].* =  g_xmau_d[l_ac].*
   END FOR
   
   LET g_xmau2_d_mask_o.* =  g_xmau2_d.*
   FOR l_ac = 1 TO g_xmau2_d.getLength()
      LET g_xmau2_d_mask_o[l_ac].* =  g_xmau2_d[l_ac].*
      CALL axmi128_xmau_t_mask()
      LET g_xmau2_d_mask_n[l_ac].* =  g_xmau2_d[l_ac].*
   END FOR
 
 
   FREE axmi128_pb   
   
END FUNCTION
]]>
  </section>
