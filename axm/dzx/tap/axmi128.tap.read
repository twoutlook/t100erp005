  <section id="axmi128.main" ver="11" status="" src="s" readonly="">
    <![CDATA[#應用 a26 樣板自動產生(Version:4)
#+ 作業開始(主程式類型)
MAIN
   #add-point:main段define
   {<point name="main.define" edit="s"/>}
   #end add-point   
   #add-point:main段define(客製用)
   {<point name="main.define_customerization" edit="c"/>}
   #end add-point   
 
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("axm","")
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT xmau001,'',xmau002,'',xmau003,'',xmau005,'',xmau006", 
                      " FROM xmau_t",
                      " WHERE xmauent= ? AND xmau001=? AND xmau002=? AND xmau003=? AND xmau005=? AND  
                          xmau006=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE axmi128_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE t0.xmau001,t0.xmau002,t0.xmau003,t0.xmau005,t0.xmau006,t1.ooall004 ,t2.oohal003 , 
       t3.ooail003 ,t4.oocql004",
               " FROM xmau_t t0",
                              " LEFT JOIN ooall_t t1 ON t1.ooallent='"||g_enterprise||"' AND t1.ooall001='15' AND t1.ooall002=t0.xmau001 AND t1.ooall003='"||g_dlang||"' ",
               " LEFT JOIN oohal_t t2 ON t2.oohalent='"||g_enterprise||"' AND t2.oohal001=t0.xmau002 AND t2.oohal002='"||g_dlang||"' ",
               " LEFT JOIN ooail_t t3 ON t3.ooailent='"||g_enterprise||"' AND t3.ooail001=t0.xmau003 AND t3.ooail002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t4 ON t4.oocqlent='"||g_enterprise||"' AND t4.oocql001='275' AND t4.oocql002=t0.xmau005 AND t4.oocql003='"||g_dlang||"' ",
 
               " WHERE t0.xmauent = '" ||g_enterprise|| "' AND t0.xmau001 = ? AND t0.xmau002 = ? AND t0.xmau003 = ? AND t0.xmau005 = ? AND t0.xmau006 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE axmi128_master_referesh FROM g_sql
 
    
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_axmi128 WITH FORM cl_ap_formpath("axm",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL axmi128_init()   
 
      #進入選單 Menu (="N")
      CALL axmi128_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_axmi128
      
   END IF 
   
   CLOSE axmi128_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
END MAIN
 
 
]]>
