  <section id="axmi128.detail_reproduce" ver="10" status="" src="s" readonly="">
    <![CDATA[#+ 單身自動複製
PRIVATE FUNCTION axmi128_detail_reproduce()
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE xmau_t.*
 
 
   #add-point:delete段define
   {<point name="detail_reproduce.define" edit="s"/>}
   #end add-point    
   #add-point:delete段define
   {<point name="detail_reproduce.define_customerization" edit="c"/>}
   #end add-point 
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE axmi128_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE axmi128_detail AS ",
                "SELECT * FROM xmau_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO axmi128_detail SELECT * FROM xmau_t 
                                         WHERE xmauent = g_enterprise AND xmau001 = g_xmau001_t
                                         AND xmau002 = g_xmau002_t
                                         AND xmau003 = g_xmau003_t
                                         AND xmau005 = g_xmau005_t
                                         AND xmau006 = g_xmau006_t
 
   
   #將key修正為調整後   
   UPDATE axmi128_detail 
      #更新key欄位
      SET xmau001 = g_xmau_m.xmau001
          , xmau002 = g_xmau_m.xmau002
          , xmau003 = g_xmau_m.xmau003
          , xmau005 = g_xmau_m.xmau005
          , xmau006 = g_xmau_m.xmau006
 
      #更新共用欄位
      #應用 a13 樣板自動產生(Version:3)
       , xmauownid = g_user 
       , xmauowndp = g_dept
       , xmaucrtid = g_user
       , xmaucrtdp = g_dept 
       , xmaucrtdt = ld_date
       , xmaumodid = g_user
       , xmaumoddt = ld_date
      #, xmaustus = "Y" 
 
 
                                       
   #將資料塞回原table   
   INSERT INTO xmau_t SELECT * FROM axmi128_detail
   
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "reproduce" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
