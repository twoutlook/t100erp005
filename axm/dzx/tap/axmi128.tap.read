  <point name="function.axmi128_xmau_unit" order="26" ver="8" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 系列、產品分類的單位確認
# Memo...........:
# Usage..........: CALL axmi128_xmau_unit()
#                  RETURNING TURE/FALSE
# Input parameter: 
# Return code....: 回傳 TURE/FALSE
# Date & Author..: 2015-06-22 By dorislai
# Modify.........:
################################################################################
PRIVATE FUNCTION axmi128_xmau_unit()
   DEFINE  l_sql                 STRING                               #抓取sql語法
   DEFINE  l_chk                 LIKE  type_t.num5                    #判斷是否有抓到符合的值
   DEFINE  l_imaa001             LIKE  type_t.chr100                  #接收抓取到的料件編號值
   #先依欄位抓取sql語法    
       #---用系列去抓料件編號
   IF NOT cl_null(g_xmau_d[l_ac].xmau031) THEN
      LET l_sql = "SELECT imaa001 FROM imaa_t ",
                  "WHERE imaaent = '"||g_enterprise||"' AND imaa127 = '"||g_xmau_d[l_ac].xmau031||"'",
                  "ORDER BY imaa001"
   END IF
       #----用產品分類去抓料件編號
   IF NOT cl_null(g_xmau_d[l_ac].xmau032) THEN
      LET l_sql = "SELECT imaa001 FROM imaa_t ",
                  "WHERE imaaent = '"||g_enterprise||"' AND imaa009 = '"||g_xmau_d[l_ac].xmau032||"'",
                  "ORDER BY imaa001"
   END IF          
   #校驗          
   PREPARE axmi128_get_imaa001_chk_pre FROM l_sql
   DECLARE axmi128_get_imaa001_chk_curs CURSOR FOR axmi128_get_imaa001_chk_pre
   LET l_imaa001 =''
   LET l_chk = FALSE #判斷是否有抓到符合的資料
   #一筆一筆比對是否有符合任一 料件編號中的可使用單位
   FOREACH axmi128_get_imaa001_chk_curs INTO l_imaa001 
      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_xmau_d[l_ac].xmau018
      #以COUNT來計算，並判斷是否有比對到符合的資料
      CALL ap_ref_array2(g_ref_fields,"SELECT COUNT(*) FROM imao_t WHERE imaoent='"||g_enterprise||"' AND imao002 = ? AND imao001 = '"||l_imaa001||"'","") RETURNING g_rtn_fields
      IF g_rtn_fields[1] > 0 THEN
         LET l_chk = TRUE 
         EXIT FOREACH
      END IF 
   END FOREACH
   FREE  axmi128_get_imaa001_chk_pre  #已使用完畢，釋放
   #若料件編號中無相對應的可使用單位，跳出錯誤訊息提示
   IF NOT l_chk THEN
      RETURN FALSE
   END IF
   RETURN TRUE
END FUNCTION]]>
