  <section id="axmi128.before_delete" ver="8" status="" src="s" readonly="">
    <![CDATA[#+ 單身db資料刪除
PRIVATE FUNCTION axmi128_before_delete()
   #add-point:before_delete段define
   {<point name="before_delete.define" edit="s"/>}
   #end add-point 
   #add-point:before_delete段define
   {<point name="before_delete.define_customerization" edit="c"/>}
   #end add-point 
   #add-point:單筆刪除前
   {<point name="delete.body.b_single_delete" mark="Y"/>}
   #end add-point
   
   DELETE FROM xmau_t
    WHERE xmauent = g_enterprise AND xmau001 = g_xmau_m.xmau001 AND
                              xmau002 = g_xmau_m.xmau002 AND
                              xmau003 = g_xmau_m.xmau003 AND
                              xmau005 = g_xmau_m.xmau005 AND
                              xmau006 = g_xmau_m.xmau006 AND
 
          xmau011 = g_xmau_d_t.xmau011
      AND xmau012 = g_xmau_d_t.xmau012
      AND xmau013 = g_xmau_d_t.xmau013
      AND xmau014 = g_xmau_d_t.xmau014
      AND xmau015 = g_xmau_d_t.xmau015
      AND xmau016 = g_xmau_d_t.xmau016
      AND xmau017 = g_xmau_d_t.xmau017
      AND xmau018 = g_xmau_d_t.xmau018
      AND xmau019 = g_xmau_d_t.xmau019
      AND xmau020 = g_xmau_d_t.xmau020
      AND xmau021 = g_xmau_d_t.xmau021
