  <point name="function.axmi128_area_exists" order="9" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION axmi128_area_exists()
DEFINE l_n     LIKE type_t.num5
DEFINE l_sql   STRING
DEFINE l_case  LIKE type_t.chr1
   
   #洲別、國家、州省有值
   IF NOT cl_null(g_xmau_d[l_ac].xmau013) AND NOT cl_null(g_xmau_d[l_ac].xmau014) AND NOT cl_null(g_xmau_d[l_ac].xmau015) THEN
      LET l_sql = "SELECT COUNT(*) FROM ooce_t,oocg_t,ooci_t",
                  " WHERE ooceent = oocgent AND ooce001 = oocg002",
                  "   AND oocgent = oocient AND oocg001 = ooci001",
                  "   AND oocgent = ",g_enterprise,
                  "   AND ooce001 = '",g_xmau_d[l_ac].xmau013,"'",
                  "   AND oocg001 = '",g_xmau_d[l_ac].xmau014,"'",
                  "   AND ooci002 = '",g_xmau_d[l_ac].xmau015,"'"
      LET l_case = '1'
   END IF
   
   #洲別、國家有值、州省沒有值
   IF NOT cl_null(g_xmau_d[l_ac].xmau013) AND NOT cl_null(g_xmau_d[l_ac].xmau014) AND cl_null(g_xmau_d[l_ac].xmau015) THEN
      LET l_sql = "SELECT COUNT(*) FROM ooce_t,oocg_t",
                  " WHERE ooceent = oocgent AND ooce001 = oocg002",
                  "   AND ooceent = ",g_enterprise,
                  "   AND ooce001 = '",g_xmau_d[l_ac].xmau013,"'",
                  "   AND oocg001 = '",g_xmau_d[l_ac].xmau014,"'"
      LET l_case = '2'
   END IF
   
   #洲別沒有值、國家、州省有值
   IF cl_null(g_xmau_d[l_ac].xmau013) AND NOT cl_null(g_xmau_d[l_ac].xmau014) AND NOT cl_null(g_xmau_d[l_ac].xmau015) THEN
      LET l_sql = "SELECT COUNT(*) FROM oocg_t,ooci_t",
                  " WHERE oocgent = oocient AND oocg001 = ooci001",
                  "   AND oocgent = ",g_enterprise,
                  "   AND oocg001 = '",g_xmau_d[l_ac].xmau014,"'",
                  "   AND ooci002 = '",g_xmau_d[l_ac].xmau015,"'"
      LET l_case = '3'
   END IF
   
   #洲別、州省有值、國家沒有值
   IF NOT cl_null(g_xmau_d[l_ac].xmau013) AND cl_null(g_xmau_d[l_ac].xmau014) AND NOT cl_null(g_xmau_d[l_ac].xmau015) THEN
      LET l_sql = "SELECT COUNT(*) FROM ooce_t,oocg_t,ooci_t",
                  " WHERE ooci001 = oocg001  AND oocg002 = ooce001 ",
                  "   AND ooceent = ",g_enterprise,
                  "   AND ooce001 = '",g_xmau_d[l_ac].xmau013,"'",
                  "   AND ooci002 = '",g_xmau_d[l_ac].xmau015,"'"
      LET l_case = '4'
   END IF
   PREPARE sel_area_prep FROM l_sql
   
   LET l_n = 0
   EXECUTE sel_area_prep INTO l_n
   IF l_n = 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'apm-00314'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()

      CASE l_case
         WHEN '1'
            LET g_xmau_d[l_ac].xmau013 = g_xmau_d_t.xmau013
            LET g_xmau_d[l_ac].xmau014 = g_xmau_d_t.xmau014
            LET g_xmau_d[l_ac].xmau015 = g_xmau_d_t.xmau015
         WHEN '2'
            LET g_xmau_d[l_ac].xmau013 = g_xmau_d_t.xmau013
            LET g_xmau_d[l_ac].xmau014 = g_xmau_d_t.xmau014
         WHEN '3'
            LET g_xmau_d[l_ac].xmau014 = g_xmau_d_t.xmau014
            LET g_xmau_d[l_ac].xmau015 = g_xmau_d_t.xmau015
         WHEN '4'
            LET g_xmau_d[l_ac].xmau013 = g_xmau_d_t.xmau013
            LET g_xmau_d[l_ac].xmau015 = g_xmau_d_t.xmau015
      END CASE
      CALL axmi128_xmau013_desc()
      CALL axmi128_xmau014_desc()
      CALL axmi128_xmau015_desc()
      RETURN FALSE
   END IF
   RETURN TRUE
END FUNCTION]]>
  </point>
