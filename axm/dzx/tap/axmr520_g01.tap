  <point name="ins_data.foreach" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[       #銷售通路
       SELECT oocql004 INTO sr_s.l_xmdg026_desc
         FROM oocql_t
        WHERE oocqlent = sr_s.xmdgent 
          AND oocql001 = '275'
          AND oocql002 = sr_s.xmdg026
          AND oocql003 = g_dlang

       #運輸方式
       SELECT oocql004 INTO sr_s.l_xmdg018_desc
         FROM oocql_t
        WHERE oocqlent = sr_s.xmdgent 
          AND oocql001 = '263'
          AND oocql002 = sr_s.xmdg018
          AND oocql003 = g_dlang
          
       #送貨地址
       LET l_pmaa027 = ''
       SELECT pmaa027 INTO l_pmaa027
         FROM pmaa_t
        WHERE pmaaent = sr_s.xmdgent
          AND pmaa001 = sr_s.xmdg007  
       
       IF NOT cl_null(sr_s.xmdg017) THEN             
          CALL s_aooi350_get_address(l_pmaa027,sr_s.xmdg017,g_dlang)RETURNING l_success,sr_s.l_address_desc
       END IF
       
       #起運地帶出參考欄位
       LET l_oocq019 = ''               #運輸方式清空
       LET sr_s.l_xmdg019_desc = ''

       IF NOT cl_null(sr_s.xmdg018) THEN
       SELECT oocq019 INTO l_oocq019 FROM oocq_t 
        WHERE oocqent = sr_s.xmdgent 
          AND oocq001 ='263' 
          AND oocq002 = sr_s.xmdg018
       END IF

             CASE
                WHEN l_oocq019 = '1' OR    #陸運
                     l_oocq019 = '4'       #其他

                   DECLARE axmr520_xmdg_cs SCROLL CURSOR FOR
                      SELECT oockl005 FROM oockl_t
                       WHERE oocklent = sr_s.xmdgent  
                         AND oockl003 = sr_s.xmdg019
                         AND oockl004 = g_dlang 
                       ORDER BY oockl001 DESC
                       OPEN axmr520_xmdg_cs
                   FETCH FIRST axmr520_xmdg_cs INTO sr_s.l_xmdg019_desc
                                      
                WHEN l_oocq019 = '2'       #海運

                   SELECT oocql004 INTO sr_s.l_xmdg019_desc FROM oocql_t 
                    WHERE oocqlent = sr_s.xmdgent
                      AND oocql001 ='262' 
                      AND oocql002 = sr_s.xmdg019
                      AND oocql003 = g_dlang
             
                WHEN l_oocq019 = '3'       #空運
             
                   SELECT oocql004 INTO sr_s.l_xmdg019_desc FROM oocql_t 
                    WHERE oocqlent = sr_s.xmdgent
                      AND oocql001 = '276' 
                      AND oocql002 = sr_s.xmdg019
                      AND oocql003 = g_dlang
             END CASE

       #目的地帶出參考欄位
       LET l_oocq019 = '' 
       LET sr_s.l_xmdg020_desc = ''
       
       IF NOT cl_null(sr_s.xmdg018) THEN
       SELECT oocq019 INTO l_oocq019
         FROM oocq_t 
        WHERE oocq001 = '263' 
          AND oocq002 = sr_s.xmdg018
       END IF
       
          CASE
             WHEN l_oocq019 = '1' OR    #陸運
                  l_oocq019 = '4'       #其他

                DECLARE axmr520_xmdg_cs1 SCROLL CURSOR FOR
                   SELECT oockl005 FROM oockl_t
                    WHERE oocklent = sr_s.xmdgent  
                      AND oockl003 = sr_s.xmdg020
                      AND oockl004 = g_dlang 
                    ORDER BY oockl001 DESC
                    OPEN axmr520_xmdg_cs1
                FETCH FIRST axmr520_xmdg_cs1 INTO sr_s.l_xmdg020_desc             
             
             WHEN l_oocq019 = '2'       #海運
             
                SELECT oocql004 INTO sr_s.l_xmdg020_desc FROM oocql_t 
                 WHERE oocqlent = sr_s.xmdgent
                   AND oocql001 ='262' 
                   AND oocql002 = sr_s.xmdg020
                   AND oocql003 = g_dlang
             
             WHEN l_oocq019 = '3'       #空運
             
                SELECT oocql004 INTO sr_s.l_xmdg020_desc FROM oocql_t 
                 WHERE oocqlent = sr_s.xmdgent
                   AND oocql001 = '276' 
                   AND oocql002 = sr_s.xmdg020
                   AND oocql003 = g_dlang
          END CASE]]>
  </point>
