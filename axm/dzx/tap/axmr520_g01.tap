  <section id="axmr520_g01.sel_prep" ver="16" status="" src="s">
    <![CDATA[#+ 選單功能實際執行處
PRIVATE FUNCTION axmr520_g01_sel_prep()
   #add-point:sel_prep段define
   {<point name="sel_prep.define"/>}
   #end add-point
 
   #add-point:sel_prep before
   {<point name="sel_prep.before" mark="Y"/>}
   #end add-point
   
   #add-point:sel_prep g_select
   {<point name="sel_prep.g_select" mark="Y"/>}
   #end add-point
   LET g_select = " SELECT xmdg001,xmdg002,xmdg003,xmdg004,xmdg005,xmdg006,xmdg007,xmdg008,xmdg009,xmdg010, 
       xmdg011,xmdg012,xmdg013,xmdg014,xmdg015,xmdg016,xmdg017,xmdg018,xmdg019,xmdg020,xmdg021,xmdg022, 
       xmdg023,xmdg024,xmdg025,xmdg026,xmdg027,xmdg028,xmdg030,xmdg031,xmdg053,xmdg054,xmdg055,xmdgdocdt, 
       xmdgdocno,xmdgent,xmdgsite,xmdgstus,xmdgunit,xmdh001,xmdh002,xmdh003,xmdh004,xmdh005,xmdh006, 
       xmdh007,xmdh008,xmdh009,xmdh010,xmdh011,xmdh012,xmdh013,xmdh014,xmdh015,xmdh016,xmdh017,xmdh018, 
       xmdh019,xmdh020,xmdh021,xmdh022,xmdh023,xmdh024,xmdh025,xmdh026,xmdh027,xmdh028,xmdh030,xmdh031, 
       xmdh032,xmdh033,xmdh034,xmdh036,xmdh050,xmdh051,xmdh052,xmdhseq,xmdhsite,oofa_t.oofa011,ooefl_t.ooefl003, 
       pmaal_t.pmaal004,t5.pmaal004,t6.pmaal004,t8.pmaal004,ooail_t.ooail003,oofb_t.oofb011,oobal_t.oobal004, 
       x.t10_oocal003,x.t11_oocal003,x.t13_oocal003,oocal_t.oocal004,x.imaal_t_imaal003,x.inaa_t_inaa002, 
       x.inab_t_inab003,trim(xmdg003)||'.'||trim(ooefl_t.ooefl003),trim(xmdg005)||'.'||trim(pmaal_t.pmaal004), 
       trim(xmdg006)||'.'||trim(t5.pmaal004),trim(xmdg007)||'.'||trim(t6.pmaal004),trim(xmdg016)||'.'||trim(t8.pmaal004), 
       trim(xmdg002)||'.'||trim(oofa_t.oofa011),trim(xmdh013)||'.'||trim(x.inab_t_inab003),trim(xmdh012)||'.'||trim(x.inaa_t_inaa002), 
       NULL,NULL,NULL,NULL,NULL,NULL,NULL"
 
   #add-point:sel_prep g_from
   {<point name="sel_prep.g_from" mark="Y"/>}
   #end add-point
    LET g_from = " FROM xmdg_t LEFT OUTER JOIN oofa_t ON oofa_t.oofa002 = '2' AND oofa_t.oofa003 = xmdg_t.xmdg002 AND oofa_t.oofaent = xmdg_t.xmdgent             LEFT OUTER JOIN oocal_t ON oocal_t.oocal002 = xmdg_t.xmdg020 AND oocal_t.oocalent = xmdg_t.xmdgent AND oocal_t.oocal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oobal_t ON oobal_t.oobal002 = xmdg_t.xmdg010 AND oobal_t.oobalent = xmdg_t.xmdgent AND oobal_t.oobal003 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN ooefl_t ON ooefl_t.ooefl001 = xmdg_t.xmdg003 AND ooefl_t.ooeflent = xmdg_t.xmdgent AND ooefl_t.ooefl002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN pmaal_t ON pmaal_t.pmaal001 = xmdg_t.xmdg005 AND pmaal_t.pmaalent = xmdg_t.xmdgent AND pmaal_t.pmaal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN pmaal_t t5 ON t5.pmaal001 = xmdg_t.xmdg006 AND t5.pmaalent = xmdg_t.xmdgent AND t5.pmaal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN pmaal_t t6 ON t6.pmaal001 = xmdg_t.xmdg007 AND t6.pmaalent = xmdg_t.xmdgent AND t6.pmaal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN ooail_t ON ooail_t.ooail001 = xmdg_t.xmdg014 AND ooail_t.ooailent = xmdg_t.xmdgent AND ooail_t.ooail002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN pmaal_t t8 ON t8.pmaal001 = xmdg_t.xmdg016 AND t8.pmaalent = xmdg_t.xmdgent AND t8.pmaal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oofb_t ON oofb_t.oofb001 = xmdg_t.xmdg017 AND oofb_t.oofbent = xmdg_t.xmdgent LEFT OUTER JOIN ( SELECT xmdh_t.*, 
        t10.oocal003 t10_oocal003,t11.oocal003 t11_oocal003,t13.oocal003 t13_oocal003,imaal_t.imaal003 imaal_t_imaal003, 
        inaa_t.inaa002 inaa_t_inaa002,inab_t.inab003 inab_t_inab003 FROM xmdh_t LEFT OUTER JOIN oocal_t t10 ON t10.oocal001 = xmdh_t.xmdh018 AND t10.oocalent = xmdh_t.xmdhent AND t10.oocal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oocal_t t11 ON t11.oocal001 = xmdh_t.xmdh015 AND t11.oocalent = xmdh_t.xmdhent AND t11.oocal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN inab_t ON inab_t.inab001 = xmdh_t.xmdh012 AND inab_t.inab002 = xmdh_t.xmdh013 AND inab_t.inabent = xmdh_t.xmdhent             LEFT OUTER JOIN inaa_t ON inaa_t.inaa001 = xmdh_t.xmdh012 AND inaa_t.inaaent = xmdh_t.xmdhent             LEFT OUTER JOIN imaal_t ON imaal_t.imaal001 = xmdh_t.xmdh006 AND imaal_t.imaalent = xmdh_t.xmdhent AND imaal_t.imaal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oocal_t t13 ON t13.oocal001 = xmdh_t.xmdh020 AND t13.oocalent = xmdh_t.xmdhent AND t13.oocal002 = '" , 
        g_dlang,"'" ," ) x  ON xmdg_t.xmdgent = x.xmdhent AND xmdg_t.xmdgdocno = x.xmdhdocno"
 
   #add-point:sel_prep g_where
   {<point name="sel_prep.g_where" mark="Y"/>}
   #end add-point
    LET g_where = " WHERE " ,tm.wc CLIPPED
 
   #add-point:sel_prep g_order
   {<point name="sel_prep.g_order" mark="Y"/>}
   #end add-point
    LET g_order = " ORDER BY xmdgdocno,xmdhseq"
 
   #add-point:sel_prep.sql.before
   {<point name="sel_prep.sql.before mark="Y""/>}
   #end add-point:sel_prep.sql.before
 
   LET g_sql = g_select CLIPPED ," ",g_from CLIPPED ," ",g_where CLIPPED ," ",g_order CLIPPED 
 
   #add-point:sel_prep.sql.after
   {<point name="sel_prep.sql.after mark="Y""/>}
   #end add-point
   PREPARE axmr520_g01_prepare FROM g_sql
   IF STATUS THEN
      CALL cl_err('prepare:',STATUS,1)
      CALL cl_ap_exitprogram("0")
   END IF
   DECLARE axmr520_g01_curs CURSOR FOR axmr520_g01_prepare
 
END FUNCTION
]]>
