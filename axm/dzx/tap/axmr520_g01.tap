################################################################################
PRIVATE REPORT axmr520_g01_subrep06(sr4)
    DEFINE sr4  sr4_r
    
    ORDER EXTERNAL BY sr4.inaodocno,sr4.inaoseq,sr4.inaoseq1,sr4.inaoseq2
    
