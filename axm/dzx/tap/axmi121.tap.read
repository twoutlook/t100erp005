   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE axmi121_detail AS ",
                "SELECT * FROM xmaf_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO axmi121_detail SELECT * FROM xmaf_t 
                                         WHERE xmafent = g_enterprise AND xmafsite = g_site AND xmaf001 = g_xmaf001_t
                                         AND xmaf002 = g_xmaf002_t
 
   
