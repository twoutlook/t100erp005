   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE axmt610_detail AS ",
                "SELECT * FROM xmem_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO axmt610_detail SELECT * FROM xmem_t 
                                         WHERE xmement = g_enterprise AND xmemdocno = g_xmeldocno_t
 
   
