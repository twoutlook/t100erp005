                   xmelownid,xmelowndp,xmelcrtid,xmelcrtdp,xmelcrtdt,xmelmodid,xmelmoddt,xmelcnfid,xmelcnfdt) = (g_xmel_m.xmeldocno, 
                   g_xmel_m.xmeldocdt,g_xmel_m.xmel003,g_xmel_m.xmel001,g_xmel_m.xmel002,g_xmel_m.xmelstus, 
