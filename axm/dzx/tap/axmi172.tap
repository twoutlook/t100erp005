  <section id="axmi172.b_fill" ver="14" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION axmi172_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_xmie_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
      LET g_sql = "SELECT  UNIQUE xmieseq,xmie002,'',xmie003,'','',xmie008,'',xmie004,'',xmie005,'', 
          xmie006,'',xmie007,'' FROM xmie_t",   
                  "",
                  
                  " WHERE xmieent= ? AND xmie001=?"  
                  
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF axmi172_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY xmie_t.xmieseq"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      PREPARE axmi172_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR axmi172_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_xmie_m.xmie001
 
                                               
      FOREACH b_fill_cs INTO g_xmie_d[l_ac].xmieseq,g_xmie_d[l_ac].xmie002,g_xmie_d[l_ac].xmie002_desc, 
          g_xmie_d[l_ac].xmie003,g_xmie_d[l_ac].xmie003_desc,g_xmie_d[l_ac].xmie003_desc1,g_xmie_d[l_ac].xmie008, 
          g_xmie_d[l_ac].xmie008_desc,g_xmie_d[l_ac].xmie004,g_xmie_d[l_ac].xmie004_desc,g_xmie_d[l_ac].xmie005, 
          g_xmie_d[l_ac].xmie005_desc,g_xmie_d[l_ac].xmie006,g_xmie_d[l_ac].xmie006_desc,g_xmie_d[l_ac].xmie007, 
          g_xmie_d[l_ac].xmie007_desc 
                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_xmie_d.deleteElement(g_xmie_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE axmi172_pb   
   
END FUNCTION
]]>
</section>
