  <section id="axmi172.delete" ver="8" status="" src="s">
<![CDATA[#+ 資料刪除
PRIVATE FUNCTION axmi172_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_xmie_m.xmie001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE axmi172_master_referesh USING g_xmie_m.xmie001 INTO g_xmie_m.xmie001
   
   CALL axmi172_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN axmi172_cl USING g_enterprise,g_xmie_m.xmie001
 
   IF STATUS THEN
      CALL cl_err("OPEN axmi172_cl:", STATUS, 1)
      CLOSE axmi172_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH axmi172_cl INTO g_xmie_m.xmie001,g_xmie_m.xmie001_desc
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_xmie_m.xmie001,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL axmi172_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM xmie_t WHERE xmieent = g_enterprise AND xmie001 = g_xmie_m.xmie001
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("xmie_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_xmie_d.clear() 
 
     
      CALL axmi172_ui_browser_refresh()  
      CALL axmi172_ui_headershow()  
      CALL axmi172_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL axmi172_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL axmi172_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE axmi172_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_xmie_m.xmie001,'D')
    
END FUNCTION
]]>
</section>
