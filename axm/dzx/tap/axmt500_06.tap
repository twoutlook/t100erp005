  <point name="function.axmt500_06_fill" order="1" ver="3" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION axmt500_06_fill()
DEFINE l_sql      STRING
DEFINE l_ac       LIKE type_t.num5
DEFINE i          LIKE type_t.num5
DEFINE l_cnt      LIKE type_t.num5
DEFINE l_success  LIKE type_t.num5
DEFINE l_inan010  LIKE inan_t.inan010




   LET l_sql = "SELECT xmdr001,xmdr002,     '',     '',xmdr008, ",
               "       xmdr009,     '',xmdr010,     '',xmdr004, ",
               "            '',xmdr005,     '',xmdr003,xmdr006, ",
               "            '',xmdr007,     '' ",
               "  FROM xmdr_t ",
               "  WHERE xmdrent = '",g_enterprise,"' ",
               "    AND xmdrdocno = '",g_xmdddocno,"' ",
               "    AND xmdrseq = '",g_xmddseq,"' ",
               "    AND xmdrseq1 = '",g_xmddseq1,"' ",
               "    AND xmdrseq2 = '",g_xmddseq2,"' "

   PREPARE axmt500_06_sel FROM l_sql
   DECLARE axmt500_06_per CURSOR FOR axmt500_06_sel 
   
   CALL g_xmdr_d.clear()
   LET l_ac = 1
   
   #先抓取對應的xmdr_t資料
   FOREACH axmt500_06_per INTO g_xmdr_d[l_ac].*
        IF SQLCA.sqlcode THEN
           INITIALIZE g_errparam TO NULL
           LET g_errparam.code = SQLCA.sqlcode
           LET g_errparam.extend = "FOREACH:"
           LET g_errparam.popup = TRUE
           CALL cl_err()

           EXIT FOREACH
        END IF
        LET g_xmdr_d[l_ac].sel = 'Y'
        
        
        #抓取對應inag_t資料的庫存量(inag008)
        LET g_xmdr_d[l_ac].inag008 = 0
        SELECT inag008 INTO g_xmdr_d[l_ac].inag008
          FROM inag_t 
         WHERE inagent = g_enterprise
           AND inagsite = g_site
           AND inag001 = g_xmdr_d[l_ac].xmdr001
           AND inag002 = g_xmdr_d[l_ac].xmdr002       
           AND inag003 = g_xmdr_d[l_ac].xmdr003
           AND inag004 = g_xmdr_d[l_ac].xmdr004
           AND inag005 = g_xmdr_d[l_ac].xmdr005
           AND inag006 = g_xmdr_d[l_ac].xmdr006            
           AND inag007 = g_xmdr_d[l_ac].xmdr007
        IF cl_null(g_xmdr_d[l_ac].inag008) THEN LET g_xmdr_d[l_ac].inag008 = 0 END IF
        #已在揀量inan010+已備置量inan010
        LET l_inan010 = 0
        SELECT SUM(inan010)  INTO l_inan010
          FROM inan_t
         WHERE inanent = g_enterprise
           AND inansite = g_site
           AND inan001 = g_xmdr_d[l_ac].xmdr001
           AND inan002 = g_xmdr_d[l_ac].xmdr002       
           AND inan003 = g_xmdr_d[l_ac].xmdr003
           AND inan004 = g_xmdr_d[l_ac].xmdr004
           AND inan005 = g_xmdr_d[l_ac].xmdr005
           AND inan006 = g_xmdr_d[l_ac].xmdr006            
           AND inan007 = g_xmdr_d[l_ac].xmdr007
        IF cl_null(l_inan010) THEN LET l_inan010 = 0 END IF
        #可備置量 = 庫存量(inag008)-已在揀量inan010-已備置量inan010        
        LET g_xmdr_d[l_ac].yxmdr008 = g_xmdr_d[l_ac].inag008 - l_inan010
        
        #轉換備置單位的數量
        IF NOT cl_null(g_xmdr_d[l_ac].xmdr007) THEN
           CALL s_aooi250_convert_qty(g_xmdr_d[l_ac].xmdr001,g_xmdr_d[l_ac].xmdr007,g_xmdr_d[l_ac].xmdr010,g_xmdr_d[l_ac].yxmdr008)
             RETURNING l_success,g_xmdr_d[l_ac].yxmdr008    
        END IF  
        #可備置量
        IF g_xmdr_d[l_ac].yxmdr008 = 0 THEN
           CONTINUE FOREACH
        END IF         
        CALL axmt500_06_xmdr004_ref(g_xmdr_d[l_ac].xmdr004) RETURNING g_xmdr_d[l_ac].xmdr004_desc 
        CALL axmt500_06_xmdr005_ref(g_xmdr_d[l_ac].xmdr004,g_xmdr_d[l_ac].xmdr005) RETURNING g_xmdr_d[l_ac].xmdr005_desc 
        CALL axmt500_06_unit_ref(g_xmdr_d[l_ac].xmdr007) RETURNING g_xmdr_d[l_ac].xmdr007_desc         
        CALL axmt500_06_unit_ref(g_xmdr_d[l_ac].xmdr010) RETURNING g_xmdr_d[l_ac].xmdr010_desc      
        CALL s_feature_description(g_xmdr_d[l_ac].xmdr001,g_xmdr_d[l_ac].xmdr002)
             RETURNING l_success,g_xmdr_d[l_ac].xmdr002_desc
       
        LET l_ac = l_ac + 1
        IF l_ac > g_max_rec THEN
           INITIALIZE g_errparam TO NULL
           LET g_errparam.code =  9035
           LET g_errparam.extend =  ''
           LET g_errparam.popup = FALSE
           CALL cl_err()

           EXIT FOREACH
        END IF        
   END FOREACH
   #再抓取inag_t資料，排除已存在xmdr_t的資料，
   LET l_sql = "SELECT inag001,inag002,'',     '',      0, ",
               "             0,      0,'',     '',inag004, ",
               "            '',inag005,'',inag003,inag006, ",
               "       inag008,inag007,'' ",
               "  FROM inag_t ",
               "  WHERE inagent = '",g_enterprise,"' ",
               "    AND inagsite = '",g_site,"' ",
               "    AND inag001 = '",g_xmdd001,"' ",
               "    AND inag002 = '",g_xmdd002,"' ",
               "    AND inag001||inag002||inag003||inag004||inag005||inag006||inag007 NOT IN ",
               "     (SELECT xmdr001||xmdr002||xmdr003||xmdr004||xmdr005||xmdr006||xmdr007 ",
               "        FROM xmdr_t ",
               "       WHERE xmdrent = '",g_enterprise,"' ",
               "         AND xmdrdocno = '",g_xmdddocno,"' ",
               "         AND xmdrseq = '",g_xmddseq,"' ",
               "         AND xmdrseq1 = '",g_xmddseq1,"' ",
               "         AND xmdrseq2 = '",g_xmddseq2,"') "        
   PREPARE axmt500_inag_sel FROM l_sql
   DECLARE axmt500_inag_per CURSOR FOR axmt500_inag_sel 
   FOREACH axmt500_inag_per INTO g_xmdr_d[l_ac].*
        IF SQLCA.sqlcode THEN
           INITIALIZE g_errparam TO NULL
           LET g_errparam.code = SQLCA.sqlcode
           LET g_errparam.extend = "FOREACH:"
           LET g_errparam.popup = TRUE
           CALL cl_err()

           EXIT FOREACH
        END IF
        #排除可備置量(庫存量-已在揀量-已備置量)小於等於0的資料
        #已在揀量inan010+已備置量inan010
        LET l_inan010 = 0
        SELECT SUM(inan010)  INTO l_inan010
          FROM inan_t
         WHERE inanent = g_enterprise
           AND inansite = g_site
           AND inan001 = g_xmdr_d[l_ac].xmdr001
           AND inan002 = g_xmdr_d[l_ac].xmdr002       
           AND inan003 = g_xmdr_d[l_ac].xmdr003
           AND inan004 = g_xmdr_d[l_ac].xmdr004
           AND inan005 = g_xmdr_d[l_ac].xmdr005
           AND inan006 = g_xmdr_d[l_ac].xmdr006            
           AND inan007 = g_xmdr_d[l_ac].xmdr007
        IF cl_null(l_inan010) THEN LET l_inan010 = 0 END IF  
        IF g_xmdr_d[l_ac].inag008 - l_inan010 = 0 THEN
           CONTINUE FOREACH
        END IF        
        LET g_xmdr_d[l_ac].sel = 'N'
        
        #備置量
        LET g_xmdr_d[l_ac].xmdr008 = 0
        #備置已沖銷量
        LET g_xmdr_d[l_ac].xmdr009 = 0
        #備置單位
        LET g_xmdr_d[l_ac].xmdr010 = g_xmdd004
        
        #可備置量 = 庫存量(inag008)-已在揀量inan010-已備置量inan010
        LET g_xmdr_d[l_ac].yxmdr008 = g_xmdr_d[l_ac].inag008 - l_inan010
        
        #轉換備置單位的數量
        IF NOT cl_null(g_xmdr_d[l_ac].xmdr007) THEN
           CALL s_aooi250_convert_qty(g_xmdr_d[l_ac].xmdr001,g_xmdr_d[l_ac].xmdr007,g_xmdr_d[l_ac].xmdr010,g_xmdr_d[l_ac].yxmdr008)
             RETURNING l_success,g_xmdr_d[l_ac].yxmdr008 
        END IF
        #可備置量
        IF g_xmdr_d[l_ac].yxmdr008 = 0 THEN
           CONTINUE FOREACH
        END IF        
        CALL axmt500_06_xmdr004_ref(g_xmdr_d[l_ac].xmdr004) RETURNING g_xmdr_d[l_ac].xmdr004_desc 
        CALL axmt500_06_xmdr005_ref(g_xmdr_d[l_ac].xmdr004,g_xmdr_d[l_ac].xmdr005) RETURNING g_xmdr_d[l_ac].xmdr005_desc 
        CALL axmt500_06_unit_ref(g_xmdr_d[l_ac].xmdr007) RETURNING g_xmdr_d[l_ac].xmdr007_desc         
        CALL axmt500_06_unit_ref(g_xmdr_d[l_ac].xmdr010) RETURNING g_xmdr_d[l_ac].xmdr010_desc
        
        LET l_ac = l_ac + 1
        IF l_ac > g_max_rec THEN
           INITIALIZE g_errparam TO NULL
           LET g_errparam.code =  9035
           LET g_errparam.extend =  ''
           LET g_errparam.popup = FALSE
           CALL cl_err()

           EXIT FOREACH
        END IF 
    END FOREACH  
   CALL g_xmdr_d.deleteElement(g_xmdr_d.getLength())
   LET g_rec_b = l_ac - 1     
END FUNCTION]]>
  </point>
  <point name="function.axmt500_06_unit_ref" order="2" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#單位名稱顯示
PRIVATE FUNCTION axmt500_06_unit_ref(p_xmdr007)
DEFINE p_xmdr007      LIKE xmdr_t.xmdr007
DEFINE r_xmdr007_desc LIKE oocal_t.oocal003

       INITIALIZE g_ref_fields TO NULL
       LET g_ref_fields[1] = p_xmdr007
       CALL ap_ref_array2(g_ref_fields,"SELECT oocal003 FROM oocal_t WHERE oocalent='"||g_enterprise||"' AND oocal001=? AND oocal002='"||g_dlang||"'","") RETURNING g_rtn_fields
       LET r_xmdr007_desc = '', g_rtn_fields[1] , ''
       RETURN r_xmdr007_desc
END FUNCTION]]>
  </point>
  <point name="function.axmt500_06_xmdr004_ref" order="3" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
