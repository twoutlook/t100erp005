  <point name="input.a.page1.xmdr001" order="" ver="1" cite_std="" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[            #此段落由子樣板a05產生
            IF  g_xmdr_d[g_detail_idx].xmdrdocno IS NOT NULL AND g_xmdr_d[g_detail_idx].xmdrseq IS NOT NULL AND g_xmdr_d[g_detail_idx].xmdrseq1 IS NOT NULL AND g_xmdr_d[g_detail_idx].xmdrseq2 IS NOT NULL AND g_xmdr_d[g_detail_idx].xmdr001 IS NOT NULL AND g_xmdr_d[g_detail_idx].xmdr002 IS NOT NULL AND g_xmdr_d[g_detail_idx].xmdr003 IS NOT NULL AND g_xmdr_d[g_detail_idx].xmdr004 IS NOT NULL AND g_xmdr_d[g_detail_idx].xmdr005 IS NOT NULL AND g_xmdr_d[g_detail_idx].xmdr006 IS NOT NULL AND g_xmdr_d[g_detail_idx].xmdr007 IS NOT NULL THEN 
               IF l_cmd = 'a' OR ( l_cmd = 'u' AND (g_xmdr_d[g_detail_idx].xmdrdocno != g_xmdr_d_t.xmdrdocno OR g_xmdr_d[g_detail_idx].xmdrseq != g_xmdr_d_t.xmdrseq OR g_xmdr_d[g_detail_idx].xmdrseq1 != g_xmdr_d_t.xmdrseq1 OR g_xmdr_d[g_detail_idx].xmdrseq2 != g_xmdr_d_t.xmdrseq2 OR g_xmdr_d[g_detail_idx].xmdr001 != g_xmdr_d_t.xmdr001 OR g_xmdr_d[g_detail_idx].xmdr002 != g_xmdr_d_t.xmdr002 OR g_xmdr_d[g_detail_idx].xmdr003 != g_xmdr_d_t.xmdr003 OR g_xmdr_d[g_detail_idx].xmdr004 != g_xmdr_d_t.xmdr004 OR g_xmdr_d[g_detail_idx].xmdr005 != g_xmdr_d_t.xmdr005 OR g_xmdr_d[g_detail_idx].xmdr006 != g_xmdr_d_t.xmdr006 OR g_xmdr_d[g_detail_idx].xmdr007 != g_xmdr_d_t.xmdr007)) THEN 
