  <point name="function.axmi410_xmfa001_chk" cite_std="N" status="" ver="1" src="s" new="Y" order="5" mark_hard="N">
<![CDATA[
################################################################################
# Descriptions...: 版本狀態碼檢查
# Memo...........:
# Usage..........: CALL axmi410_xmfa001_chk()
#                  RETURNING r_success
# Input parameter: 
#                : 
# Return code....: r_success      TRUE/FALSE
#                : 
# Date & Author..: 14/06/19 By emma
# Modify.........:
################################################################################
PRIVATE FUNCTION axmi410_xmfa001_chk()
DEFINE r_success         LIKE type_t.num5

   LET r_success = TRUE
   
   IF cl_null(g_xmfa_m.xmfadocno) AND cl_null(g_xmfa_m.xmfa001) THEN
      RETURN r_success
   END IF
   
   #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
   INITIALIZE g_chkparam.* TO NULL
   #設定g_chkparam.*的參數
   LET g_chkparam.arg1 = g_xmfa_m.xmfadocno
   #呼叫檢查存在並帶值的library
   IF NOT cl_chk_exist("v_xmfa001") THEN
      #檢查失敗時後續處理
      LET r_success = FALSE
      RETURN r_success
   END IF
   
   RETURN r_success
END FUNCTION]]>
</point>
