            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE xmfb005
            #add-point:ON CHANGE xmfb005
            {<point name="input.g.page1.xmfb005" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD xmfb006
            #此段落由子樣板a15產生
            IF NOT cl_ap_chk_Range(g_xmfb_d[l_ac].xmfb006,"0.000","0","","","azz-00079",1) THEN
               NEXT FIELD xmfb006
            END IF
 
 
            #add-point:AFTER FIELD xmfb006
            {<point name="input.a.page1.xmfb006" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD xmfb006
            #add-point:BEFORE FIELD xmfb006
            {<point name="input.b.page1.xmfb006" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE xmfb006
            #add-point:ON CHANGE xmfb006
            {<point name="input.g.page1.xmfb006" />}
            #END add-point
 
 
                  #Ctrlp:input.c.page1.xmfbsite
         ON ACTION controlp INFIELD xmfbsite
            #add-point:ON ACTION controlp INFIELD xmfbsite
            {<point name="input.c.page1.xmfbsite" />}
            #END add-point
 
         #Ctrlp:input.c.page1.xmfb002
         ON ACTION controlp INFIELD xmfb002
            #add-point:ON ACTION controlp INFIELD xmfb002
            {<point name="input.c.page1.xmfb002" />}
            #END add-point
 
         #Ctrlp:input.c.page1.xmfb003
         ON ACTION controlp INFIELD xmfb003
            #add-point:ON ACTION controlp INFIELD xmfb003
            {<point name="input.c.page1.xmfb003" />}
            #END add-point
 
         #Ctrlp:input.c.page1.xmfb004
