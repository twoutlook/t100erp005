  <point name="process.pre_process" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[   IF cl_null(g_master.wc) THEN      
      LET g_master.wc=" 1=1 "
   END IF
   CASE g_master.radio01
      WHEN '0'
   
      WHEN '1'
         LET g_master.wc = g_master.wc CLIPPED," AND pmaa047 = 'N' "
      WHEN '2'
         LET g_master.wc = g_master.wc CLIPPED," AND pmaa047 = 'Y' "
   END CASE
   LET g_master.wc = g_master.wc CLIPPED ,"  AND xmdkent = ",g_enterprise," AND xmdksite = '",g_site,"' "   
   IF NOT cl_null(g_master.year01) THEN
      IF NOT cl_null(g_master.year02) THEN
         IF g_master.year01 <= g_master.year02 THEN
           LET g_master.wc = g_master.wc CLIPPED ,"  AND to_char(xmdkdocdt,'YYYY') BETWEEN ",g_master.year01," AND ",g_master.year02," "
         ELSE
           LET g_master.wc = g_master.wc CLIPPED ,"  AND to_char(xmdkdocdt,'YYYY') BETWEEN ",g_master.year02," AND ",g_master.year01," "
         END IF
      ELSE
         LET g_master.wc = g_master.wc CLIPPED ,"  AND to_char(xmdkdocdt,'YYYY') = ",g_master.year01," "
      END IF
   ELSE
      IF NOT cl_null(g_master.year02) THEN
         LET g_master.wc = g_master.wc CLIPPED ,"  AND to_char(xmdkdocdt,'YYYY') = ",g_master.year02," "
      END IF
   END IF       
   --
   CALL axmr002_x01(g_master.wc,g_master.radio02)]]>
  </point>
