   #add-point:sel_prep g_select
   {<point name="sel_prep.g_select" mark="Y"/>}
   #end add-point
   LET g_select = " SELECT xmev001,xmev002,xmev003,xmev004,xmev005,xmev006,xmev007,xmev008,xmev009,xmev010, 
       xmev011,xmev012,xmev013,xmev014,xmev015,xmev016,xmev017,xmev018,xmev019,xmevdocdt,xmevdocno,xmevent, 
       xmevsite,xmevstus,xmew001,xmew002,xmew003,xmew004,xmew005,xmew006,xmew007,xmew008,xmew009,xmew010, 
       xmew011,xmew012,xmew013,xmew014,xmew015,xmew016,xmewseq,xmewsite,ooag_t.ooag011,ooefl_t.ooefl003, 
       pmaal_t.pmaal004,x.t1_pmaal004,xmesl_t.xmesl003,ooail_t.ooail003,oocal_t.oocal003,x.t2_oocal003, 
       x.imaal_t_imaal003,trim(xmev004)||'.'||trim(xmesl_t.xmesl003),trim(xmev001)||'.'||trim(ooag_t.ooag011), 
       trim(xmev003)||'.'||trim(pmaal_t.pmaal003),trim(xmev002)||'.'||trim(ooefl_t.ooefl003),trim(xmew013)||'.'||trim(x.t1_pmaal004), 
