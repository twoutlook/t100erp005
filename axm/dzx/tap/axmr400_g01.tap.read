   #end add-point
    LET g_from = " FROM xmev_t LEFT OUTER JOIN ooefl_t ON ooefl_t.ooefl001 = xmev_t.xmev002 AND ooefl_t.ooeflent = xmev_t.xmevent AND ooefl_t.ooefl002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oocal_t ON oocal_t.oocal001 = xmev_t.xmev012 AND oocal_t.oocalent = xmev_t.xmevent AND oocal_t.oocal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN ooail_t ON ooail_t.ooail001 = xmev_t.xmev006 AND ooail_t.ooailent = xmev_t.xmevent AND ooail_t.ooail002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN xmesl_t ON xmesl_t.xmesldocno = xmev_t.xmev004 AND xmesl_t.xmeslent = xmev_t.xmevent AND xmesl_t.xmesl002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN pmaal_t ON pmaal_t.pmaal001 = xmev_t.xmev003 AND pmaal_t.pmaalent = xmev_t.xmevent AND pmaal_t.pmaal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN ooag_t ON ooag_t.ooag001 = xmev_t.xmev001 AND ooag_t.ooagent = xmev_t.xmevent LEFT OUTER JOIN ( SELECT xmew_t.*, 
        t1.pmaal004 t1_pmaal004,t2.oocal003 t2_oocal003,imaal_t.imaal003 imaal_t_imaal003,imaal_t.imaal004 imaal_t_imaal004 FROM xmew_t             LEFT OUTER JOIN pmaal_t t1 ON t1.pmaal001 = xmew_t.xmew013 AND t1.pmaalent = xmew_t.xmewent AND t1.pmaal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oocal_t t2 ON t2.oocal001 = xmew_t.xmew006 AND t2.oocalent = xmew_t.xmewent AND t2.oocal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN imaal_t ON imaal_t.imaal001 = xmew_t.xmew003 AND imaal_t.imaalent = xmew_t.xmewent AND imaal_t.imaal002 = '" , 
        g_dlang,"'" ," ) x  ON xmev_t.xmevent = x.xmewent AND xmev_t.xmevdocno = x.xmewdocno"
 
   #add-point:sel_prep g_where
   {<point name="sel_prep.g_where" mark="Y"/>}
   #end add-point
    LET g_where = " WHERE " ,tm.wc CLIPPED 
 
   #add-point:sel_prep g_order
   {<point name="sel_prep.g_order" mark="Y"/>}
   #end add-point
    LET g_order = " ORDER BY xmevdocno,xmewseq"
 
   #add-point:sel_prep.sql.before
   {<point name="sel_prep.sql.before" mark="Y"/>}
   #end add-point:sel_prep.sql.before
   LET g_where = g_where ,cl_sql_add_filter("xmev_t")   #資料過濾功能
   LET g_sql = g_select CLIPPED ," ",g_from CLIPPED ," ",g_where CLIPPED ," ",g_order CLIPPED
   LET g_sql = cl_sql_add_mask(g_sql)    #遮蔽特定資料, 若寫至add-point也需複製此段 
 
   #add-point:sel_prep.sql.after
   {<point name="sel_prep.sql.after" mark="Y"/>}
   #end add-point
   PREPARE axmr400_g01_prepare FROM g_sql
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.extend = 'prepare:'
      LET g_errparam.code   = STATUS
      LET g_errparam.popup  = TRUE
      CALL cl_err()   
      LET g_rep_success = 'N'    
   END IF
   DECLARE axmr400_g01_curs CURSOR FOR axmr400_g01_prepare
 
END FUNCTION
