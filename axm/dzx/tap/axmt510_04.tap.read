  <point name="input.before_input" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[            IF p_cmd ='u' THEN
               
               CALL s_transaction_begin()
               
               OPEN axmt510_04_cl USING g_enterprise,g_xmegdocno,g_xmegseq,g_xmeg900
               IF STATUS THEN
                  CALL cl_err("OPEN axmt510_04_cl:", STATUS, 1)
                  CLOSE axmt510_04_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
    
               #鎖住將被更改或取消的資料
               FETCH axmt510_04_cl INTO g_xmeg_m.xmegdocno,g_xmeg_m.xmegseq,g_xmeg_m.xmeg027,g_xmeg_m.xmeg028,g_xmeg_m.xmeg028_desc,
