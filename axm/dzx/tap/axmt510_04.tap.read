    <![CDATA[#儲位檢查
PRIVATE FUNCTION axmt510_04_xmeg029_chk()
DEFINE r_success  LIKE type_t.num5

        LET r_success = TRUE
        IF NOT cl_null(g_xmeg_m.xmeg029) THEN 

