  <point name="function.axmt171_xmid007_chk" order="4" ver="13" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 檢查料號是否可以重複
# Memo...........:
# Usage..........: CALL axmt171_xmid007_chk(p_cmd)
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION axmt171_xmid007_chk(p_cmd)
DEFINE   p_cmd         LIKE type_t.chr1
DEFINE   l_xmia010     LIKE xmia_t.xmia010
DEFINE   l_xmia011     LIKE xmia_t.xmia011
DEFINE   l_xmia012     LIKE xmia_t.xmia012
DEFINE   l_xmia013     LIKE xmia_t.xmia013
DEFINE   l_xmia014     LIKE xmia_t.xmia014
DEFINE   l_xmia015     LIKE xmia_t.xmia015
DEFINE   l_cnt         LIKE type_t.num5
DEFINE   l_sql         STRING
   IF cl_null(g_xmid_d[l_ac].xmid007) THEN
      RETURN TRUE
   END IF 
   
　 SELECT xmia010,xmia011,xmia012,xmia013,xmia014,xmia015 
     INTO l_xmia010,l_xmia011,l_xmia012,l_xmia013,l_xmia014,l_xmia015 
     FROM xmia_t
    WHERE xmiaent = g_enterprise
      AND xmia001 = g_xmic_m.xmic001   
      
   #預測營運據點 
   IF l_xmia010 = 'Y' AND g_xmic_m.xmic004 IS NULL THEN   
      RETURN TRUE
   END IF
   IF l_xmia011 = 'Y' AND g_xmic_m.xmic005 IS NULL THEN 
      RETURN TRUE
   END IF 
   IF l_xmia012 = 'Y' AND g_xmic_m.xmic006 IS NULL THEN 
      RETURN TRUE
   END IF 
   IF l_xmia013 = 'Y' AND g_xmid_d[l_ac].xmid009 IS NULL THEN 
      RETURN TRUE
   END IF   
   IF l_xmia014 = 'Y' AND g_xmid_d[l_ac].xmid010 IS NULL THEN 
      RETURN TRUE
   END IF
   IF l_xmia015 = 'Y' AND g_xmid_d[l_ac].xmid008 IS NULL THEN 
      RETURN TRUE
   END IF      

   LET l_cnt = 0
   LET l_sql = " SELECT COUNT(*) FROM xmid_t,xmic_t",
               "  WHERE xmicent = xmident ",
               "    AND xmic001 = xmid001 ",
               "    AND xmic002 = xmid002 ",
               "    AND xmic003 = xmid003 ",
               "    AND xmic004 = xmid004 ",
               "    AND xmic005 = xmid005 ",
               "    AND xmic006 = xmid006 ",
               "    AND xmident = '",g_enterprise,"'",
               "    AND xmid001 = '",g_xmic_m.xmic001,"'",
               "    AND xmid002 = '",g_xmic_m.xmic002,"'",
               "    AND xmid003 = '",g_xmic_m.xmic003,"'",
               "    AND xmid007 = '",g_xmid_d[l_ac].xmid007,"'"
                   
   IF l_xmia010 = 'Y' AND g_xmic_m.xmic004 IS NOT NULL THEN 
      LET l_sql = l_sql," AND xmid004 = '",g_xmic_m.xmic004,"'"
   END IF
   IF l_xmia011 = 'Y' AND g_xmic_m.xmic005 IS NOT NULL THEN 
      LET l_sql = l_sql," AND xmid005 = '",g_xmic_m.xmic005,"'"
   END IF   
   IF l_xmia012 = 'Y' AND g_xmic_m.xmic006 IS NOT NULL THEN 
      LET l_sql = l_sql," AND xmid006 = '",g_xmic_m.xmic006,"'"
   END IF 
   IF l_xmia013 = 'Y' AND g_xmid_d[l_ac].xmid009 IS NOT NULL THEN 
      LET l_sql = l_sql," AND xmid009 = '",g_xmid_d[l_ac].xmid009,"'"
   END IF   
   IF l_xmia014 = 'Y' AND g_xmid_d[l_ac].xmid010 IS NOT NULL THEN 
      LET l_sql = l_sql," AND xmid010 = '",g_xmid_d[l_ac].xmid010,"'"
   END IF 
   IF l_xmia015 = 'Y' AND g_xmid_d[l_ac].xmid008 IS NOT NULL THEN 
      LET l_sql = l_sql," AND xmid008 = '",g_xmid_d[l_ac].xmid008,"'"
   END IF
   PREPARE axmt171_cnt FROM l_sql
   EXECUTE axmt171_cnt INTO l_cnt    

   IF (p_cmd = 'a' AND l_cnt > 0) OR (p_cmd = 'u' AND l_cnt > g_xmic_m.xmia002)  THEN 
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'std-00004'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()

      RETURN FALSE
   END IF            
   RETURN TRUE      
END FUNCTION]]>
