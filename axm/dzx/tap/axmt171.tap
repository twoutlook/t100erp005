       g_xmic_m.xmiccrtdp,g_xmic_m.xmiccrtdp_desc,g_xmic_m.xmiccrtdt,g_xmic_m.xmicmodid,g_xmic_m.xmicmodid_desc, 
       g_xmic_m.xmicmoddt,g_xmic_m.xmiccnfid,g_xmic_m.xmiccnfid_desc,g_xmic_m.xmiccnfdt
   
