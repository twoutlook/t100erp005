         WHEN "W"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/signing.png")
         
      END CASE
 
 
   
   #清空key欄位的desc
      LET g_xmic_m.xmic001_desc = ''
   DISPLAY BY NAME g_xmic_m.xmic001_desc
   LET g_xmic_m.xmic004_desc = ''
   DISPLAY BY NAME g_xmic_m.xmic004_desc
   LET g_xmic_m.xmic005_desc = ''
   DISPLAY BY NAME g_xmic_m.xmic005_desc
   LET g_xmic_m.xmic006_desc = ''
   DISPLAY BY NAME g_xmic_m.xmic006_desc
 
   
   CALL axmt171_input("r")
   
   IF INT_FLAG AND NOT g_master_insert THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = '' 
      LET g_errparam.code   = 9001 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      CALL s_transaction_end('N','0')
      LET INT_FLAG = 0
