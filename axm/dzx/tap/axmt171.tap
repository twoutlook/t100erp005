  <point name="function.axmt171_get_xmic003" order="8" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 取得最新版本号
# Memo...........:
# Usage..........: CALL axmt171_get_xmic003(p_cmd)
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION axmt171_get_xmic003(p_cmd)
DEFINE   p_cmd    LIKE type_t.chr1
DEFINE   l_cnt    LIKE type_t.num5  

   IF NOT cl_null(g_xmic_m.xmic001) AND NOT cl_null(g_xmic_m.xmic002) AND g_xmic_m.xmic004 IS NOT NULL AND
      g_xmic_m.xmic005 IS NOT NULL AND g_xmic_m.xmic006 IS NOT NULL THEN
      #是否有未確認的前版本
      SELECT COUNT(*) INTO l_cnt FROM xmic_t
       WHERE xmicent = g_enterprise
         AND xmic001 = g_xmic_m.xmic001
         AND xmic002 = g_xmic_m.xmic002
         AND xmic004 = g_xmic_m.xmic004
         AND xmic005 = g_xmic_m.xmic005
         AND xmic006 = g_xmic_m.xmic006  
         AND xmicstus = 'N'
      IF cl_null(l_cnt) THEN LET l_cnt = 0 END IF
      IF (p_cmd = 'a' AND l_cnt > 0) OR (p_cmd = 'u' AND l_cnt > 1) THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'axm-00259'
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()

         RETURN FALSE
      END IF      
      SELECT MAX(xmic003)+1 INTO g_xmic_m.xmic003 FROM xmic_t
       WHERE xmicent = g_enterprise
         AND xmic001 = g_xmic_m.xmic001
         AND xmic002 = g_xmic_m.xmic002
         AND xmic004 = g_xmic_m.xmic004
         AND xmic005 = g_xmic_m.xmic005
         AND xmic006 = g_xmic_m.xmic006
      IF cl_null(g_xmic_m.xmic003) THEN
         LET g_xmic_m.xmic003 = 1
      END IF   
      DISPLAY BY NAME g_xmic_m.xmic003        
   END IF 
   RETURN TRUE    
END FUNCTION]]>
  </point>
  <point name="function.axmt171_xmic002_chk" order="9" ver="3" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 檢查起始日是否跟預測編號的起始日一致
# Memo...........:
# Usage..........: CALL axmt171_xmic002_chk()
#                  RETURNING TRUE/FALSE
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION axmt171_xmic002_chk()
   DEFINE  l_xmia003             LIKE xmia_t.xmia003
   DEFINE  l_xmia016             LIKE xmia_t.xmia016   
   DEFINE  l_dd                  LIKE xmia_t.xmia003
   DEFINE  l_xmic002             LIKE xmic_t.xmic002
   DEFINE  l_msg                 STRING
   LET l_msg  = ''
    IF NOT cl_null(g_xmic_m.xmic002) AND NOT cl_null(g_xmic_m.xmic001) THEN 
       SELECT xmia003,xmia016 INTO l_xmia003,l_xmia016 FROM xmia_t
        WHERE xmiaent = g_enterprise
          AND xmia001 = g_xmic_m.xmic001 
       IF l_xmia016 = '1' THEN   #指定日   
          #取得起始日    
          LET l_dd = DAY(g_xmic_m.xmic002)     
          IF l_xmia003 <> l_dd THEN     
             CALL cl_getmsg('axm-00247',g_lang) RETURNING l_msg
             LET l_msg = l_msg,l_xmia003     
             INITIALIZE g_errparam TO NULL
             LET g_errparam.code = 'axm-00169'
             LET g_errparam.extend = l_msg
             LET g_errparam.popup = TRUE
             CALL cl_err()
          
             RETURN FALSE
          END IF  
       END IF    
       IF l_xmia016 = '2' THEN   #指定周 
          CALL s_date_get_monday(g_xmic_m.xmic002) RETURNING l_xmic002  #日期_取得传入日期所在周的星期一的日期值
          IF l_xmia003 = 0 THEN 
             LET l_xmic002 = l_xmic002 - 1
          END IF          
          IF l_xmia003 = 2 THEN
             LET l_xmic002 = l_xmic002 + 1
          END IF   
          IF l_xmia003 = 3 THEN
             LET l_xmic002 = l_xmic002 + 2
          END IF    
          IF l_xmia003 = 4 THEN
             LET l_xmic002 = l_xmic002 + 3
          END IF     
          IF l_xmia003 = 5 THEN
             LET l_xmic002 = l_xmic002 + 4
          END IF   
          IF l_xmia003 = 6 THEN
             LET l_xmic002 = l_xmic002 + 5
          END IF       
          IF l_xmic002 <> g_xmic_m.xmic002 THEN
             CALL cl_getmsg('axm-00414',g_lang) RETURNING l_msg
             LET l_msg = l_msg,l_xmia003     
             INITIALIZE g_errparam TO NULL
             LET g_errparam.code = 'axm-00169'
             LET g_errparam.extend = l_msg
             LET g_errparam.popup = TRUE
             CALL cl_err()          
             RETURN FALSE             
          END IF          
       END IF       
    END IF
    RETURN TRUE
END FUNCTION]]>
  </point>
