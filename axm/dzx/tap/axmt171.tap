  <point name="statechange.b_update" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[      CALL s_transaction_begin()
      IF lc_state = 'Y' THEN 
         CALL axmt171_conf_chk()
            RETURNING r_success,g_errno
         IF NOT r_success THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = g_errno
            LET g_errparam.extend = g_xmic_m.xmic001
            LET g_errparam.popup = TRUE
            CALL cl_err()

            RETURN
         ELSE
            IF NOT cl_ask_confirm('aim-00108') THEN
               RETURN
            ELSE
               CALL axmt171_conf_upd() RETURNING r_success
               IF NOT r_success THEN
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','0')
               END IF
            END IF
         END IF
      END IF
      IF lc_state = 'N' THEN
         CALL axmt171_unconfirm_chk() 
              RETURNING r_success,g_errno
         IF NOT r_success THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = g_errno
            LET g_errparam.extend = g_xmic_m.xmic001
            LET g_errparam.popup = TRUE
            CALL cl_err()

            RETURN
         ELSE
            IF NOT cl_ask_confirm('axm-00233') THEN
               RETURN
            ELSE
               CALL axmt171_unconfirm_upd() RETURNING r_success
               IF NOT r_success THEN
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','0')
               END IF
            END IF
         END IF
      END IF
      IF lc_state = 'C' THEN
         CALL axmt171_closed_chk() 
              RETURNING r_success,g_errno
         IF NOT r_success THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = g_errno
            LET g_errparam.extend = g_xmic_m.xmic001
            LET g_errparam.popup = TRUE
            CALL cl_err()

            RETURN
         ELSE
            IF NOT cl_ask_confirm('axm-00201') THEN
               RETURN
            ELSE
               CALL axmt171_closed_upd() 
                    RETURNING r_success
               IF NOT r_success THEN
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','0')
               END IF
            END IF
         END IF
      END IF ]]>
