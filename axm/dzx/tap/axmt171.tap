    <![CDATA[
################################################################################
# Descriptions...: 取消確認
# Memo...........:
# Usage..........: CALL axmt171_unconfirm_upd()
#                  RETURNING r_success
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION axmt171_unconfirm_upd()
DEFINE  r_success        LIKE type_t.num5
DEFINE  l_xmia009        LIKE xmia_t.xmia009
   LET r_success = TRUE
   #检查:应在事物中的
   IF NOT s_transaction_chk('Y','0') THEN
      LET r_success = FALSE
      RETURN r_success
   END IF   
   UPDATE xmic_t SET xmicstus = 'N',
                     xmiccnfid = '',
                     xmiccnfdt = ''   
    WHERE xmicent = g_enterprise
      AND xmic001 = g_xmic_m.xmic001
      AND xmic002 = g_xmic_m.xmic002
      AND xmic003 = g_xmic_m.xmic003
      AND xmic004 = g_xmic_m.xmic004
      AND xmic005 = g_xmic_m.xmic005
      AND xmic006 = g_xmic_m.xmic006
   IF SQLCA.sqlerrd[3] = 0 THEN
      LET r_success = FALSE
   END IF
   SELECT xmia009 INTO l_xmia009 FROM xmia_t
    WHERE xmiaent = g_enterprise
      AND xmia001 = g_xmic_m.xmic001
   IF l_xmia009 = 'N' THEN   
      DELETE FROM xmig_t WHERE xmigent = g_enterprise
                           AND xmigsite = g_xmic_m.xmic004
                           AND xmig001 =  g_xmic_m.xmic001
                           AND xmig002 =  g_xmic_m.xmic002
                           AND xmig003 =  g_xmic_m.xmic003
                           AND xmig004 =  g_xmic_m.xmic005
                           AND xmig005 =  g_xmic_m.xmic006
      IF SQLCA.sqlerrd[3] = 0 THEN
         LET r_success = FALSE
      END IF  
   END IF      
   RETURN r_success  
END FUNCTION]]>
