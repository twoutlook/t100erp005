   LET g_forupd_sql = "SELECT xmid007,xmid008,xmid010,xmid009,xmid014,xmid011,xmid015,xmid007,xmid008, 
       xmid010,xmid009,xmid011,xmid018,xmid019,xmid007,xmid008,xmid010,xmid009,xmid011,xmid016,xmid017, 
       xmid020,xmid021 FROM xmid_t WHERE xmident=? AND xmid001=? AND xmid002=? AND xmid003=? AND xmid004=?  
       AND xmid005=? AND xmid006=? AND xmid007=? AND xmid008=? AND xmid009=? AND xmid010=? AND xmid011=?  
       FOR UPDATE"
   #add-point:input段define_sql
