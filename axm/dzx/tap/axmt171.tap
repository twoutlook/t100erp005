  <section id="axmt171.insert" ver="29" status="" src="s" readonly="">
    <![CDATA[#+ 資料新增
PRIVATE FUNCTION axmt171_insert()
   #add-point:insert段define
   {<point name="insert.define" edit="s"/>}
   #end add-point    
   #add-point:insert段define(客製用)
   {<point name="insert.define_customerization" edit="c"/>}
   #end add-point    
   
   #清畫面欄位內容
   CLEAR FORM                    
   CALL g_xmid_d.clear()   
   CALL g_xmid2_d.clear()  
   CALL g_xmid3_d.clear()  
 
 
   INITIALIZE g_xmic_m.* LIKE xmic_t.*             #DEFAULT 設定
   
   LET g_xmic001_t = NULL
   LET g_xmic002_t = NULL
   LET g_xmic003_t = NULL
   LET g_xmic004_t = NULL
   LET g_xmic005_t = NULL
   LET g_xmic006_t = NULL
 
   
   LET g_master_insert = FALSE
   
   #add-point:insert段before
   {<point name="insert.before"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   WHILE TRUE
      #公用欄位給值(單頭)
      #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_xmic_m.xmicownid = g_user
      LET g_xmic_m.xmicowndp = g_dept
      LET g_xmic_m.xmiccrtid = g_user
      LET g_xmic_m.xmiccrtdp = g_dept 
      LET g_xmic_m.xmiccrtdt = cl_get_current()
      LET g_xmic_m.xmicmodid = g_user
      LET g_xmic_m.xmicmoddt = cl_get_current()
      LET g_xmic_m.xmicstus = 'N'
 
 
 
      #append欄位給值
      
     
      #一般欄位給值
            LET g_xmic_m.xmicstus = "N"
 
  
      #add-point:單頭預設值
      {<point name="insert.default"/>}
      #end add-point 
      
      #保存單頭舊值(用於資料輸入錯誤還原預設值時使用)
      LET g_xmic_m_t.* = g_xmic_m.*
      LET g_xmic_m_o.* = g_xmic_m.*
      
      #顯示狀態(stus)圖片
            #應用 a21 樣板自動產生(Version:2)
	  #根據當下狀態碼顯示圖片
      CASE g_xmic_m.xmicstus 
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "C"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/closed.png")
         WHEN "A"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/approved.png")
         WHEN "D"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/withdraw.png")
         WHEN "R"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/rejection.png")
         WHEN "W"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/signing.png")
         
      END CASE
 
 
    
      CALL axmt171_input("a")
      
      #add-point:單頭輸入後
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
      END IF
      
      IF NOT g_master_insert THEN
         DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
         DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
         INITIALIZE g_xmic_m.* TO NULL
         INITIALIZE g_xmid_d TO NULL
         INITIALIZE g_xmid2_d TO NULL
         INITIALIZE g_xmid3_d TO NULL
 
         #add-point:取消新增後
         {<point name="insert.cancel"/>}
         #end add-point 
         CALL axmt171_show()
         RETURN
      END IF
      
      LET INT_FLAG = 0
      #CALL g_xmid_d.clear()
      #CALL g_xmid2_d.clear()
      #CALL g_xmid3_d.clear()
 
 
      LET g_rec_b = 0
      CALL s_transaction_end('Y','0')
      EXIT WHILE
        
   END WHILE
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL axmt171_set_act_visible()   
   CALL axmt171_set_act_no_visible()
   
   #將新增的資料併入搜尋條件中
   LET g_xmic001_t = g_xmic_m.xmic001
   LET g_xmic002_t = g_xmic_m.xmic002
   LET g_xmic003_t = g_xmic_m.xmic003
   LET g_xmic004_t = g_xmic_m.xmic004
   LET g_xmic005_t = g_xmic_m.xmic005
   LET g_xmic006_t = g_xmic_m.xmic006
 
   
   #組合新增資料的條件
   LET g_add_browse = " xmicent = '" ||g_enterprise|| "' AND",
                      " xmic001 = '", g_xmic_m.xmic001, "' "
                      ," AND xmic002 = '", g_xmic_m.xmic002, "' "
                      ," AND xmic003 = '", g_xmic_m.xmic003, "' "
                      ," AND xmic004 = '", g_xmic_m.xmic004, "' "
                      ," AND xmic005 = '", g_xmic_m.xmic005, "' "
                      ," AND xmic006 = '", g_xmic_m.xmic006, "' "
 
                      
   #add-point:組合新增資料的條件後
   {<point name="insert.after.add_browse"/>}
   #end add-point
      
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL axmt171_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   CLOSE axmt171_cl
   
   CALL axmt171_idx_chk()
   
   #撈取異動後的資料(主要是帶出reference)
   EXECUTE axmt171_master_referesh USING g_xmic_m.xmic001,g_xmic_m.xmic002,g_xmic_m.xmic003,g_xmic_m.xmic004, 
       g_xmic_m.xmic005,g_xmic_m.xmic006 INTO g_xmic_m.xmic001,g_xmic_m.xmic002,g_xmic_m.xmic003,g_xmic_m.xmic004, 
       g_xmic_m.xmic005,g_xmic_m.xmic006,g_xmic_m.xmic007,g_xmic_m.xmicstus,g_xmic_m.xmicownid,g_xmic_m.xmicowndp, 
       g_xmic_m.xmiccrtid,g_xmic_m.xmiccrtdp,g_xmic_m.xmiccrtdt,g_xmic_m.xmicmodid,g_xmic_m.xmicmoddt, 
       g_xmic_m.xmiccnfid,g_xmic_m.xmiccnfdt,g_xmic_m.xmic001_desc,g_xmic_m.xmic004_desc,g_xmic_m.xmic005_desc, 
       g_xmic_m.xmic006_desc,g_xmic_m.xmic007_desc,g_xmic_m.xmicownid_desc,g_xmic_m.xmicowndp_desc,g_xmic_m.xmiccrtid_desc, 
       g_xmic_m.xmiccrtdp_desc,g_xmic_m.xmicmodid_desc,g_xmic_m.xmiccnfid_desc
   
   #遮罩相關處理
   LET g_xmic_m_mask_o.* =  g_xmic_m.*
   CALL axmt171_xmic_t_mask()
   LET g_xmic_m_mask_n.* =  g_xmic_m.*
   
   #將資料顯示到畫面上
   DISPLAY BY NAME g_xmic_m.xmic001,g_xmic_m.xmic001_desc,g_xmic_m.xmic002,g_xmic_m.xmic003,g_xmic_m.xmia002, 
       g_xmic_m.xmic004,g_xmic_m.xmic004_desc,g_xmic_m.xmic005,g_xmic_m.xmic005_desc,g_xmic_m.xmic006, 
       g_xmic_m.xmic006_desc,g_xmic_m.xmic007,g_xmic_m.xmic007_desc,g_xmic_m.xmicstus,g_xmic_m.xmicownid, 
       g_xmic_m.xmicownid_desc,g_xmic_m.xmicowndp,g_xmic_m.xmicowndp_desc,g_xmic_m.xmiccrtid,g_xmic_m.xmiccrtid_desc, 
       g_xmic_m.xmiccrtdp,g_xmic_m.xmiccrtdp_desc,g_xmic_m.xmiccrtdt,g_xmic_m.xmicmodid,g_xmic_m.xmicmodid_desc, 
       g_xmic_m.xmicmoddt,g_xmic_m.xmiccnfid,g_xmic_m.xmiccnfid_desc,g_xmic_m.xmiccnfdt
   
   #add-point:新增結束後
   {<point name="insert.after"/>}
   #end add-point 
   
   #功能已完成,通報訊息中心
   CALL axmt171_msgcentre_notify('insert')
   
END FUNCTION
]]>
  </section>
