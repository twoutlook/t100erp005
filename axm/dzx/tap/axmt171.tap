  <section id="axmt171.modify" ver="31" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION axmt171_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   DEFINE l_wc2_table1          STRING
 
 
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point    
   #add-point:modify段define(客製用)
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point    
   
   #保存單頭舊值
   LET g_xmic_m_t.* = g_xmic_m.*
   LET g_xmic_m_o.* = g_xmic_m.*
   
   IF g_xmic_m.xmic001 IS NULL
   OR g_xmic_m.xmic002 IS NULL
   OR g_xmic_m.xmic003 IS NULL
   OR g_xmic_m.xmic004 IS NULL
   OR g_xmic_m.xmic005 IS NULL
   OR g_xmic_m.xmic006 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_xmic001_t = g_xmic_m.xmic001
   LET g_xmic002_t = g_xmic_m.xmic002
   LET g_xmic003_t = g_xmic_m.xmic003
   LET g_xmic004_t = g_xmic_m.xmic004
   LET g_xmic005_t = g_xmic_m.xmic005
   LET g_xmic006_t = g_xmic_m.xmic006
 
   CALL s_transaction_begin()
   
   OPEN axmt171_cl USING g_enterprise,g_xmic_m.xmic001,g_xmic_m.xmic002,g_xmic_m.xmic003,g_xmic_m.xmic004,g_xmic_m.xmic005,g_xmic_m.xmic006
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN axmt171_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE axmt171_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE axmt171_master_referesh USING g_xmic_m.xmic001,g_xmic_m.xmic002,g_xmic_m.xmic003,g_xmic_m.xmic004, 
       g_xmic_m.xmic005,g_xmic_m.xmic006 INTO g_xmic_m.xmic001,g_xmic_m.xmic002,g_xmic_m.xmic003,g_xmic_m.xmic004, 
       g_xmic_m.xmic005,g_xmic_m.xmic006,g_xmic_m.xmic007,g_xmic_m.xmicstus,g_xmic_m.xmicownid,g_xmic_m.xmicowndp, 
       g_xmic_m.xmiccrtid,g_xmic_m.xmiccrtdp,g_xmic_m.xmiccrtdt,g_xmic_m.xmicmodid,g_xmic_m.xmicmoddt, 
       g_xmic_m.xmiccnfid,g_xmic_m.xmiccnfdt,g_xmic_m.xmic001_desc,g_xmic_m.xmic004_desc,g_xmic_m.xmic005_desc, 
       g_xmic_m.xmic006_desc,g_xmic_m.xmic007_desc,g_xmic_m.xmicownid_desc,g_xmic_m.xmicowndp_desc,g_xmic_m.xmiccrtid_desc, 
       g_xmic_m.xmiccrtdp_desc,g_xmic_m.xmicmodid_desc,g_xmic_m.xmiccnfid_desc
   
   #檢查是否允許此動作
   IF NOT axmt171_action_chk() THEN
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_xmic_m_mask_o.* =  g_xmic_m.*
   CALL axmt171_xmic_t_mask()
   LET g_xmic_m_mask_n.* =  g_xmic_m.*
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
 
   
   CALL axmt171_show()
   #add-point:modify段show之後
   {<point name="modify.after_show"/>}
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
 
 
    
   WHILE TRUE
      LET g_xmic001_t = g_xmic_m.xmic001
      LET g_xmic002_t = g_xmic_m.xmic002
      LET g_xmic003_t = g_xmic_m.xmic003
      LET g_xmic004_t = g_xmic_m.xmic004
      LET g_xmic005_t = g_xmic_m.xmic005
      LET g_xmic006_t = g_xmic_m.xmic006
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_xmic_m.xmicmodid = g_user 
LET g_xmic_m.xmicmoddt = cl_get_current()
LET g_xmic_m.xmicmodid_desc = cl_get_username(g_xmic_m.xmicmodid)
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      CALL axmt171_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE xmic_t SET (xmicmodid,xmicmoddt) = (g_xmic_m.xmicmodid,g_xmic_m.xmicmoddt)
          WHERE xmicent = g_enterprise AND xmic001 = g_xmic001_t
            AND xmic002 = g_xmic002_t
            AND xmic003 = g_xmic003_t
            AND xmic004 = g_xmic004_t
            AND xmic005 = g_xmic005_t
            AND xmic006 = g_xmic006_t
 
      END IF
    
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_xmic_m.* = g_xmic_m_t.*
         CALL axmt171_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_xmic_m.xmic001 != g_xmic001_t 
      OR g_xmic_m.xmic002 != g_xmic002_t 
      OR g_xmic_m.xmic003 != g_xmic003_t 
      OR g_xmic_m.xmic004 != g_xmic004_t 
      OR g_xmic_m.xmic005 != g_xmic005_t 
      OR g_xmic_m.xmic006 != g_xmic006_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE xmid_t SET xmid001 = g_xmic_m.xmic001
                                       ,xmid002 = g_xmic_m.xmic002
                                       ,xmid003 = g_xmic_m.xmic003
                                       ,xmid004 = g_xmic_m.xmic004
                                       ,xmid005 = g_xmic_m.xmic005
                                       ,xmid006 = g_xmic_m.xmic006
 
          WHERE xmident = g_enterprise AND xmid001 = g_xmic001_t
            AND xmid002 = g_xmic002_t
            AND xmid003 = g_xmic003_t
            AND xmid004 = g_xmic004_t
            AND xmid005 = g_xmic005_t
            AND xmid006 = g_xmic006_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "xmid_t" 
               LET g_errparam.code   = "std-00009" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "xmid_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
         
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL axmt171_set_act_visible()   
   CALL axmt171_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " xmicent = '" ||g_enterprise|| "' AND",
                      " xmic001 = '", g_xmic_m.xmic001, "' "
                      ," AND xmic002 = '", g_xmic_m.xmic002, "' "
                      ," AND xmic003 = '", g_xmic_m.xmic003, "' "
                      ," AND xmic004 = '", g_xmic_m.xmic004, "' "
                      ," AND xmic005 = '", g_xmic_m.xmic005, "' "
                      ," AND xmic006 = '", g_xmic_m.xmic006, "' "
 
   #填到對應位置
   CALL axmt171_browser_fill("")
 
   CLOSE axmt171_cl
   
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL axmt171_msgcentre_notify('modify')
 
END FUNCTION   
]]>
