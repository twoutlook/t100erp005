  <section id="axmt171.update_b" ver="18" status="" src="s" readonly="">
    <![CDATA[#+ 修改單身後其他table連動
PRIVATE FUNCTION axmt171_update_b(ps_table,ps_keys,ps_keys_bak,ps_page)
   DEFINE ps_table         STRING
   DEFINE ps_page          STRING
   DEFINE ps_keys          DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ps_keys_bak      DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ls_group         STRING
   DEFINE li_idx           LIKE type_t.num10 
   DEFINE lb_chk           BOOLEAN
   DEFINE l_new_key        DYNAMIC ARRAY OF STRING
   DEFINE l_old_key        DYNAMIC ARRAY OF STRING
   DEFINE l_field_key      DYNAMIC ARRAY OF STRING
   #add-point:update_b段define
   {<point name="update_b.define" edit="s"/>}
   #end add-point   
   #add-point:update_b段define(客製用)
   {<point name="update_b.define_customerization" edit="c"/>}
   #end add-point   
   
   LET g_update = TRUE   
   
   #判斷key是否有改變
   LET lb_chk = TRUE
   FOR li_idx = 1 TO ps_keys.getLength()
      IF ps_keys[li_idx] <> ps_keys_bak[li_idx] THEN
         LET lb_chk = FALSE
         EXIT FOR
      END IF
   END FOR
   
   #不需要做處理
   IF lb_chk THEN
      RETURN
   END IF
   
   #判斷是否是同一群組的table
   LET ls_group = "'1','2','3',"
   IF ls_group.getIndexOf(ps_page,1) > 0 AND ps_table <> "xmid_t" THEN
      #add-point:update_b段修改前
      {<point name="update_b.before_update" mark="Y"/>}
      #end add-point 
      
      #將遮罩欄位還原
      CALL axmt171_xmid_t_mask_restore('restore_mask_o')
               
      UPDATE xmid_t 
         SET (xmid001,xmid002,xmid003,xmid004,xmid005,xmid006,
              xmid007,xmid008,xmid009,xmid010,xmid011
              ,xmid014,xmid015,xmid018,xmid019,xmid016,xmid017,xmid020,xmid021) 
              = 
             (ps_keys[1],ps_keys[2],ps_keys[3],ps_keys[4],ps_keys[5],ps_keys[6],ps_keys[7],ps_keys[8],ps_keys[9],ps_keys[10],ps_keys[11]
              ,g_xmid_d[g_detail_idx].xmid014,g_xmid_d[g_detail_idx].xmid015,g_xmid2_d[g_detail_idx].xmid018, 
                  g_xmid2_d[g_detail_idx].xmid019,g_xmid3_d[g_detail_idx].xmid016,g_xmid3_d[g_detail_idx].xmid017, 
                  g_xmid3_d[g_detail_idx].xmid020,g_xmid3_d[g_detail_idx].xmid021) 
         WHERE xmident = g_enterprise AND xmid001 = ps_keys_bak[1] AND xmid002 = ps_keys_bak[2] AND xmid003 = ps_keys_bak[3] AND xmid004 = ps_keys_bak[4] AND xmid005 = ps_keys_bak[5] AND xmid006 = ps_keys_bak[6] AND xmid007 = ps_keys_bak[7] AND xmid008 = ps_keys_bak[8] AND xmid009 = ps_keys_bak[9] AND xmid010 = ps_keys_bak[10] AND xmid011 = ps_keys_bak[11]
      #add-point:update_b段修改中
      {<point name="update_b.m_update"/>}
      #end add-point   
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "xmid_t" 
            LET g_errparam.code   = "std-00009" 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            CALL s_transaction_end('N','0')
         WHEN SQLCA.sqlcode #其他錯誤
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "xmid_t" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            CALL s_transaction_end('N','0')
         OTHERWISE
            
      END CASE
      
      #將遮罩欄位進行遮蔽
      CALL axmt171_xmid_t_mask_restore('restore_mask_n')
               
      #add-point:update_b段修改後
      {<point name="update_b.after_update"/>}
      #end add-point  
   END IF
   
 
   
 
   
   #add-point:update_b段other
   {<point name="update_b.other"/>}
   #end add-point  
   
END FUNCTION
]]>
  </section>
