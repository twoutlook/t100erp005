PRIVATE FUNCTION axmt171_xmic002_desc()
DEFINE l_dd                 LIKE type_t.num5
DEFINE l_gzze003            LIKE gzze_t.gzze003
DEFINE l_xmia016            LIKE xmia_t.xmia016
DEFINE l_msg                STRING

   LET l_msg =''
   LET l_gzze003 = ''

   SELECT xmia003,xmia016 INTO l_dd,l_xmia016 FROM xmia_t
    WHERE xmiaent = g_enterprise
      AND xmia001 = g_xmic_m.xmic001  
