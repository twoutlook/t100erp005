  <point name="detail_reproduce.body.table1.m_insert" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   SELECT xmia002 INTO l_xmia002_o FROM xmia_t
    WHERE xmiaent = g_enterprise
      AND xmia001 = g_xmic001_t

   SELECT xmia002 INTO l_xmia002_n FROM xmia_t
    WHERE xmiaent = g_enterprise
      AND xmia001 = g_xmic_m.xmic001
      
   IF l_xmia002_o > l_xmia002_n THEN
      DELETE FROM xmid_t WHERE xmident = g_enterprise
                           AND xmid001 = g_xmic_m.xmic001
                           AND xmid002 = g_xmic_m.xmic002
                           AND xmid003 = g_xmic_m.xmic003
                           AND xmid004 = g_xmic_m.xmic004
                           AND xmid005 = g_xmic_m.xmic005
                           AND xmid006 = g_xmic_m.xmic006
                           AND xmid011 > l_xmia002_n    
            IF SQLCA.sqlcode THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = SQLCA.sqlcode
               LET g_errparam.extend = "reproduce"
               LET g_errparam.popup = TRUE
               CALL cl_err()

               RETURN
            END IF                              
   END IF   
   
   IF l_xmia002_o >= l_xmia002_n THEN  
      FOR i = 1 TO l_xmia002_n
          UPDATE xmid_t SET xmid012 = g_xmib[i].b_date,
                            xmid013 = g_xmib[i].e_date
                      WHERE xmident = g_enterprise      
                        AND xmid001 = g_xmic_m.xmic001
                        AND xmid002 = g_xmic_m.xmic002
                        AND xmid003 = g_xmic_m.xmic003
                        AND xmid004 = g_xmic_m.xmic004
                        AND xmid005 = g_xmic_m.xmic005
                        AND xmid006 = g_xmic_m.xmic006 
                        AND xmid011 = i   
            IF SQLCA.sqlcode THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = SQLCA.sqlcode
               LET g_errparam.extend = "reproduce"
               LET g_errparam.popup = TRUE
               CALL cl_err()

               RETURN
            END IF                           
      END FOR       
   END IF
   IF l_xmia002_o < l_xmia002_n THEN    
      LET l_sql = " SELECT DISTINCT xmid007,xmid008,xmid009,xmid010 FROM xmid_t ",  
                  "  WHERE xmident = '",g_enterprise,"'",
                  "    AND xmid001 = '",g_xmic_m.xmic001,"'",   
                  "    AND xmid002 = '",g_xmic_m.xmic002,"'",
                  "    AND xmid003 = '",g_xmic_m.xmic003,"'",
                  "    AND xmid004 = '",g_xmic_m.xmic004,"'",
                  "    AND xmid005 = '",g_xmic_m.xmic005,"'",   
                  "    AND xmid006 = '",g_xmic_m.xmic006,"'"  

      PREPARE axmt171_pb_02 FROM l_sql
      DECLARE b_fill_cs_02 CURSOR FOR axmt171_pb_02
      LET n = l_xmia002_o+1      
      FOREACH b_fill_cs_02 INTO l_xmid007,l_xmid008,l_xmid009,l_xmid010
         FOR j = n TO l_xmia002_n
            INSERT INTO xmid_t
                       (xmident,
                        xmid001,xmid002,xmid003,xmid004,xmid005,xmid006,
                        xmid007,xmid008,xmid009,xmid010,xmid011,xmid012,xmid013,xmid015,xmid016,xmid017,xmid018,xmid019,xmid020,xmid021)
                 VALUES (g_enterprise,g_xmic_m.xmic001,g_xmic_m.xmic002,g_xmic_m.xmic003,g_xmic_m.xmic004,g_xmic_m.xmic005,
                         g_xmic_m.xmic006,l_xmid007,l_xmid008,l_xmid009,l_xmid010,n,g_xmib[n].b_date,g_xmib[n].e_date,0,0,0,0,0,0,0)
            IF SQLCA.sqlcode THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = SQLCA.sqlcode
               LET g_errparam.extend = "reproduce"
               LET g_errparam.popup = TRUE
               CALL cl_err()

               RETURN
            END IF                         
         END FOR         
      END FOREACH      
   END IF]]>
  </point>
