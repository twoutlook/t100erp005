       g_xmae_m.xmaemodid,g_xmae_m.xmaemoddt,g_xmae_m.xmae002_desc,g_xmae_m.xmae001_desc,g_xmae_m.xmae003_desc, 
       g_xmae_m.xmae020_desc,g_xmae_m.xmae021_desc,g_xmae_m.xmae006_desc,g_xmae_m.xmae008_desc,g_xmae_m.xmae009_desc, 
       g_xmae_m.xmae011_desc,g_xmae_m.xmae012_desc,g_xmae_m.xmae013_desc,g_xmae_m.xmae014_desc,g_xmae_m.xmae017_desc, 
       g_xmae_m.xmae019_desc,g_xmae_m.xmaeownid_desc,g_xmae_m.xmaeowndp_desc,g_xmae_m.xmaecrtid_desc, 
       g_xmae_m.xmaecrtdp_desc,g_xmae_m.xmaemodid_desc
 
   ERROR ""
  
   LET g_xmae001_t = g_xmae_m.xmae001
   LET g_xmae002_t = g_xmae_m.xmae002
 
   
   CALL s_transaction_begin()
   
   OPEN axmi111_cl USING g_enterprise,g_xmae_m.xmae001,g_xmae_m.xmae002
   IF STATUS THEN
      CALL cl_err("OPEN axmi111_cl:", STATUS, 1)
      CLOSE axmi111_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE axmi111_master_referesh USING g_xmae_m.xmae001,g_xmae_m.xmae002 INTO g_xmae_m.xmae002,g_xmae_m.xmae001, 
       g_xmae_m.xmae003,g_xmae_m.xmae004,g_xmae_m.xmae020,g_xmae_m.xmae021,g_xmae_m.xmae022,g_xmae_m.xmae006, 
