                  IF cl_null(g_xmae_m.xmae001) THEN
                     LET g_xmae_m.xmae001_desc=NULL
                     DISPLAY g_xmae_m.xmae001_desc TO xmae001_desc
                  END IF
                  NEXT FIELD CURRENT
               END IF
            END IF
