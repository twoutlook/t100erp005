  <point name="function.axmt580_01_b_fill" order="4" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION axmt580_01_b_fill()
   DEFINE l_sql        STRING
   
   LET l_sql = "SELECT xmdmdocno,xmdmseq,xmdmseq1,",
               "       xmdm001,xmdm002,xmdm003,xmdm004,xmdm005,",
               "       xmdm006,xmdm007,xmdm008,xmdm009,xmdm010,",
               "       xmdm011,",
               "       xmdm031,xmdm032",
               "  FROM axmt580_01_temp",
               " WHERE xmdment = '",g_enterprise,"'",
               "   AND xmdmsite = '",g_site,"'",
               "   AND xmdmdocno = '",g_xmdl.xmdldocno,"'",
               "   AND xmdmseq = '",g_xmdl.xmdlseq,"'"

   PREPARE axmt580_01_b_pre FROM l_sql
   DECLARE axmt580_01_b_cs CURSOR FOR axmt580_01_b_pre

   CALL g_xmdm_d.clear()
   LET l_ac = 1
   FOREACH axmt580_01_b_cs INTO g_xmdm_d[l_ac].xmdmdocno,g_xmdm_d[l_ac].xmdmseq,
                                g_xmdm_d[l_ac].xmdmseq1,g_xmdm_d[l_ac].xmdm001,g_xmdm_d[l_ac].xmdm002,g_xmdm_d[l_ac].xmdm003,
                                g_xmdm_d[l_ac].xmdm004,g_xmdm_d[l_ac].xmdm005,g_xmdm_d[l_ac].xmdm006,g_xmdm_d[l_ac].xmdm007,
                                g_xmdm_d[l_ac].xmdm008,g_xmdm_d[l_ac].xmdm009,g_xmdm_d[l_ac].xmdm010,g_xmdm_d[l_ac].xmdm011,
                                g_xmdm_d[l_ac].xmdm031,g_xmdm_d[l_ac].xmdm032
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF

      CALL axmt580_01_xmdm005_ref()
      CALL axmt580_01_xmdm006_ref()
      CALL axmt580_01_xmdm008_ref()
      CALL axmt580_01_xmdm010_ref()

      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
   END FOREACH
   
   CALL g_xmdm_d.deleteElement(g_xmdm_d.getLength())
   LET g_rec_b = l_ac - 1
   DISPLAY g_rec_b TO FORMONLY.cnt
   CLOSE axmt580_01_b_cs
   FREE axmt580_01_b_pre
END FUNCTION]]>
  </point>
