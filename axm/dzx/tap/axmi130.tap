  <point name="function.axmi130_xmah001_ref" cite_std="N" status="" ver="1" src="s" new="Y" order="1" cite_ver="" mark_hard="N">
<![CDATA[################################################################################
# Descriptions...: 帶出價格編號說明和助記碼
# Memo...........:
# Usage..........: CALL axmi130_xmah001_ref()
# Date & Author..: 日期 By 作者
# Date & Author..: 2014/01/23 By lixh
################################################################################
PRIVATE FUNCTION axmi130_xmah001_ref()
        INITIALIZE g_ref_fields TO NULL
        LET g_ref_fields[1] = g_xmah_m.xmah001
        CALL ap_ref_array2(g_ref_fields,"SELECT xmahl003,xmahl004 FROM xmahl_t WHERE xmahlent='"||g_enterprise||"' AND xmahl001=? AND xmahl002 = '"||g_dlang||"'","") RETURNING g_rtn_fields
        LET g_xmah_m.xmahl003 = '', g_rtn_fields[1] , ''
        LET g_xmah_m.xmahl004 = '', g_rtn_fields[1] , ''
        DISPLAY BY NAME g_xmah_m.xmahl003,g_xmah_m.xmahl004
END FUNCTION]]>
</point>
