  <section id="axmt440.insert" ver="7" status="" src="s">
    <![CDATA[#+ 資料新增
PRIVATE FUNCTION axmt440_insert()
   #add-point:insert段define
   {<point name="insert.define"/>}
   #end add-point    
   
   #清畫面欄位內容
   CLEAR FORM                    
   CALL g_xmdy_d.clear()   
   CALL g_xmdy2_d.clear()  
