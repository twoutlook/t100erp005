                       " FROM xmdy_t ",
                            "  ",
                            "  ",
                       " WHERE xmdyent = '" ||g_enterprise|| "' AND ", g_wc
   END IF
   
   #定義翻頁CURSOR
   LET g_sql= "SELECT '',xmdydocno,xmdyseq FROM (",l_sql_rank,") WHERE ",
