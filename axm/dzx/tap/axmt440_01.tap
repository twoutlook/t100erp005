<![CDATA[
PRIVATE FUNCTION axmt440_01_xmdy_show()
DEFINE l_xmdxstus  LIKE xmdx_t.xmdxstus

   LET g_xmdy_m.xmdydocno = g_xmdydocno
   LET g_xmdy_m.xmdyseq = g_xmdyseq 
   LET g_xmdy_m.xmdy002 = g_xmdy002 
   LET g_xmdy_m.xmdy003 = g_xmdy003 
   LET g_xmdy_m.xmdy004 = g_xmdy004 
   LET g_xmdy_m.xmdy006 = g_xmdy006 
   LET g_xmdy_m.xmdy007 = g_xmdy007 
   LET g_xmdy_m.xmdy008 = g_xmdy008 
   LET g_xmdy_m.xmdysite = g_site
   LET l_xmdxstus = ''
   SELECT xmdx005,xmdxstus INTO g_xmdy_m.xmdx005,l_xmdxstus FROM xmdx_t 
    WHERE xmdxent = g_enterprise
      AND xmdxdocno = g_xmdydocno
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_xmdy_m.xmdy002
   CALL ap_ref_array2(g_ref_fields,"SELECT imaal003,imaal004 FROM imaal_t WHERE imaalent='"||g_enterprise||"' AND imaal001=? AND imaal002='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_xmdy_m.imaal003 = '', g_rtn_fields[1] , ''
   LET g_xmdy_m.imaal004 = '', g_rtn_fields[2] , ''
   DISPLAY BY NAME g_xmdy_m.imaal003,g_xmdy_m.imaal004
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_xmdy_m.xmdy006
   CALL ap_ref_array2(g_ref_fields,"SELECT oocql004 FROM oocql_t WHERE oocqlent='"||g_enterprise||"' AND oocql001='221' AND oocql002=? AND oocql003='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_xmdy_m.xmdy006_desc = '', g_rtn_fields[1] , ''
   DISPLAY BY NAME g_xmdy_m.xmdy006_desc

   DISPLAY BY NAME g_xmdy_m.xmdydocno,g_xmdy_m.xmdyseq,g_xmdy_m.xmdy008,g_xmdy_m.xmdy002,g_xmdy_m.imaal003, 
       g_xmdy_m.imaal004,g_xmdy_m.xmdy003,g_xmdy_m.xmdy004,g_xmdy_m.xmdy006,g_xmdy_m.xmdy006_desc,g_xmdy_m.xmdy007, 
       g_xmdy_m.xmdysite,g_xmdy_m.xmdx005
       
    
    CALL cl_set_act_visible("modify",TRUE)
    IF l_xmdxstus != 'N' THEN
       CALL cl_set_act_visible("modify",FALSE)
    END IF       
END FUNCTION]]>
</point>
