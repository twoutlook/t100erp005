  <point name="function.axmt440_01_xmdz001_round" cite_std="N" status="" ver="1" src="s" new="Y" order="1" mark_hard="N">
<![CDATA[
PRIVATE FUNCTION axmt440_01_xmdz001_round()
DEFINE l_success   LIKE type_t.num5,
       l_ooca002   LIKE ooca_t.ooca002,
       l_ooca004   LIKE ooca_t.ooca004

   IF NOT cl_null(g_xmdy_m.xmdy008) AND NOT cl_null(g_xmdz_d[l_ac].xmdz001) THEN 
      CALL s_aooi250_get_msg(g_xmdy_m.xmdy008) RETURNING l_success,l_ooca002,l_ooca004
      IF l_success = TRUE THEN 
         CALL s_num_round(l_ooca004,g_xmdz_d[l_ac].xmdz001,l_ooca002) RETURNING g_xmdz_d[l_ac].xmdz001
      END IF
   END IF
END FUNCTION]]>
</point>
  <point name="function.axmt440_01_xmdz001" cite_std="N" status="" ver="1" src="s" new="Y" order="2" mark_hard="N">
<![CDATA[
PRIVATE FUNCTION axmt440_01_xmdz001()
DEFINE l_xmdy009   LIKE xmdy_t.xmdy009
DEFINE l_max       LIKE xmdy_t.xmdy009
DEFINE r_success   LIKE type_t.num5
   
   LET r_success = TRUE
   SELECT MAX(xmdz001) INTO l_max 
     FROM xmdz_t
    WHERE xmdzent = g_enterprise
      AND xmdzdocno = g_xmdy_m.xmdydocno
      AND xmdzseq = g_xmdy_m.xmdyseq
      AND xmdzseq1 <> g_xmdz_d[l_ac].xmdzseq1
   IF cl_null(l_max) THEN LET l_max = 0 END IF
   IF l_max >= g_xmdz_d[l_ac].xmdz001 THEN 
      CALL cl_err(g_xmdz_d[l_ac].xmdz001,'axm-00224',1)
      LET r_success = FALSE
      RETURN r_success
   END IF
   
   SELECT xmdy009 INTO l_xmdy009 
     FROM xmdy_t
    WHERE xmdyent = g_enterprise
      AND xmdydocno = g_xmdy_m.xmdydocno
      AND xmdyseq = g_xmdy_m.xmdyseq
   IF cl_null(l_xmdy009) THEN 
      LET l_xmdy009 = g_xmdy009
   END IF
   IF l_xmdy009 < g_xmdz_d[l_ac].xmdz001 THEN 
      CALL cl_err(g_xmdz_d[l_ac].xmdz001,'axm-00223',1)
      LET r_success = FALSE
      RETURN r_success
   END IF

   CALL axmt440_01_xmdz001_round() 
   RETURN r_success
   
END FUNCTION]]>
</point>
