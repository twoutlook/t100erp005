   #end add-point
 
END FUNCTION
 
 
]]>
</section>
  <section id="axmt440_01.modify" ver="8" status="" src="s">
<![CDATA[#+ 資料修改
PRIVATE FUNCTION axmt440_01_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_xmdy_m.xmdydocno IS NULL
   OR g_xmdy_m.xmdyseq IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE axmt440_01_master_referesh USING g_xmdy_m.xmdydocno,g_xmdy_m.xmdyseq INTO g_xmdy_m.xmdydocno, 
       g_xmdy_m.xmdyseq,g_xmdy_m.xmdy002,g_xmdy_m.xmdy003,g_xmdy_m.xmdy004,g_xmdy_m.xmdy006,g_xmdy_m.xmdy007, 
       g_xmdy_m.xmdysite,g_xmdy_m.xmdy008
 
   ERROR ""
  
   LET g_xmdydocno_t = g_xmdy_m.xmdydocno
   LET g_xmdyseq_t = g_xmdy_m.xmdyseq
 
   CALL s_transaction_begin()
   
   OPEN axmt440_01_cl USING g_enterprise,g_xmdy_m.xmdydocno
                                                       ,g_xmdy_m.xmdyseq
 
   IF STATUS THEN
      CALL cl_err("OPEN axmt440_01_cl:", STATUS, 1)
      CLOSE axmt440_01_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH axmt440_01_cl INTO g_xmdy_m.xmdydocno,g_xmdy_m.xmdyseq,g_xmdy_m.xmdx005,g_xmdy_m.xmdy002,g_xmdy_m.imaal003, 
       g_xmdy_m.imaal004,g_xmdy_m.xmdy003,g_xmdy_m.xmdy004,g_xmdy_m.xmdy006,g_xmdy_m.xmdy006_desc,g_xmdy_m.xmdy007, 
       g_xmdy_m.xmdysite,g_xmdy_m.xmdy008
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_xmdy_m.xmdydocno,SQLCA.sqlcode,0)
      CLOSE axmt440_01_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   CALL axmt440_01_show()
   WHILE TRUE
      LET g_xmdydocno_t = g_xmdy_m.xmdydocno
      LET g_xmdyseq_t = g_xmdy_m.xmdyseq
 
      
      #寫入修改者/修改日期資訊(單頭)
      
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL axmt440_01_input("u")     #欄位更改
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_xmdy_m.* = g_xmdy_m_t.*
         CALL axmt440_01_show()
         CALL cl_err('',9001,0)
         CALL s_transaction_end('N','0')
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_xmdy_m.xmdydocno != g_xmdydocno_t 
      OR g_xmdy_m.xmdyseq != g_xmdyseq_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE xmdz_t SET xmdzdocno = g_xmdy_m.xmdydocno
                                      ,xmdzseq = g_xmdy_m.xmdyseq
 
          WHERE xmdzent = g_enterprise AND xmdzdocno = g_xmdydocno_t
            AND xmdzseq = g_xmdyseq_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("xmdz_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("xmdz_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_xmdy_m.xmdydocno,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE axmt440_01_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_xmdy_m.xmdydocno,'U')
 
END FUNCTION   
]]>
</section>
