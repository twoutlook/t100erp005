  <section id="axmt440_01.b_fill" ver="4" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION axmt440_01_b_fill()
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_xmdz_d.clear()    #g_xmdz_d 單頭及單身 
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF axmt440_01_fill_chk(1) THEN
   
      LET g_sql = "SELECT  UNIQUE xmdzsite,xmdzseq1,xmdz001,xmdz002,xmdz003,xmdz004,xmdz005 FROM xmdz_t", 
             
                  " INNER JOIN xmdy_t ON xmdydocno = xmdzdocno ",
                  " AND xmdyseq = xmdzseq ",
 
                  #"",
                  
                  "",
                  " WHERE"
      #add-point:b_fill段sql_before
      {<point name="b_fill.body.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table1) THEN
         LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
      END IF
      
      #子單身的WC
      
      
      LET g_sql = g_sql, " ORDER BY xmdz_t.xmdzseq1"
      
      #add-point:單身填充控制
      {<point name="b_fill.sql"/>}
      #end add-point
      
      PREPARE axmt440_01_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR axmt440_01_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_xmdy_m.xmdydocno
                                               ,g_xmdy_m.xmdyseq
 
                                               
      FOREACH b_fill_cs INTO g_xmdz_d[l_ac].xmdzsite,g_xmdz_d[l_ac].xmdzseq1,g_xmdz_d[l_ac].xmdz001, 
          g_xmdz_d[l_ac].xmdz002,g_xmdz_d[l_ac].xmdz003,g_xmdz_d[l_ac].xmdz004,g_xmdz_d[l_ac].xmdz005 

         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               CALL cl_err( '', 9035, 1)
            END IF
            EXIT FOREACH
         END IF
         
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
 
   
   #add-point:browser_fill段其他table處理
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_xmdz_d.deleteElement(g_xmdz_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE axmt440_01_pb
 
   
END FUNCTION
]]>
</section>
