           LET g_sql = " SELECT ooff013 FROM ooff_t WHERE ooffstus='Y' and ooff001='6' AND ooff012='1' AND ooffent = '", 
                sr1.xmfdent CLIPPED ,"'", " AND  ooff002 = '", sr1.xmfddocno CLIPPED ,"'"
 
           LET l_cnt = 0
