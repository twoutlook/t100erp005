               LET gs_keys_bak[4] = g_xmag_d_t.xmag003
               LET gs_keys[5] = g_xmag_d[g_detail_idx].xmag004
               LET gs_keys_bak[5] = g_xmag_d_t.xmag004
               LET gs_keys[6] = g_xmag_d[g_detail_idx].xmag005
               LET gs_keys_bak[6] = g_xmag_d_t.xmag005
               LET gs_keys[7] = g_xmag_d[g_detail_idx].xmag006
               LET gs_keys_bak[7] = g_xmag_d_t.xmag006
