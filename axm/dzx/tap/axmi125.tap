                 AND xmag006 = g_xmag_d_t.xmag006  
                 AND xmag007 = g_xmag_d_t.xmag007  
                 AND xmag009 = g_xmag_d_t.xmag009  
 
                 
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
               
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("xmag_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                  #WHEN SQLCA.sqlcode #其他錯誤
                  #   CALL cl_err("xmag_t",SQLCA.sqlcode,1)  
                  #   CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_xmag_m.xmag000
               LET gs_keys_bak[1] = g_xmag000_t
               LET gs_keys[2] = g_xmag_m.xmag001
               LET gs_keys_bak[2] = g_xmag001_t
               LET gs_keys[3] = g_xmag_d[g_detail_idx].xmag002
               LET gs_keys_bak[3] = g_xmag_d_t.xmag002
