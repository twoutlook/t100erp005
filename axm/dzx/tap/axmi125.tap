                WHERE xmagent = g_enterprise AND xmagsite = g_site AND xmag000 = g_xmag_m.xmag000 
                 AND xmag001 = g_xmag_m.xmag001 
 
                 AND xmag002 = g_xmag_d_t.xmag002 #項次   
                 AND xmag003 = g_xmag_d_t.xmag003  
                 AND xmag004 = g_xmag_d_t.xmag004  
