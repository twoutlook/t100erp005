  <section id="axmi125.main" ver="5" status="" src="s">
<![CDATA[#+ 此段落由子樣板a26產生
#OPTIONS SHORT CIRCUIT
#+ 作業開始 
MAIN
   #add-point:main段define
   {<point name="main.define"/>}
   #end add-point   
 
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("axm","")
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT xmag000,xmag001,'','',''", 
                      " FROM xmag_t",
                      " WHERE xmagent= ? AND xmagsite= ? AND xmag000=? AND xmag001=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE axmi125_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE xmag000,xmag001",
               " FROM xmag_t",
               
               " WHERE xmagent = '" ||g_enterprise|| "' AND xmagsite = '" ||g_site|| "' AND xmag000 = ? AND xmag001 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE axmi125_master_referesh FROM g_sql
 
 
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_axmi125 WITH FORM cl_ap_formpath("axm",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL axmi125_init()   
 
      #進入選單 Menu (="N")
      CALL axmi125_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_axmi125
      
   END IF 
   
   CLOSE axmi125_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
   
END MAIN
 
 
]]>
</section>
  <section id="axmi125.modify" ver="4" status="" src="s">
<![CDATA[#+ 資料修改
PRIVATE FUNCTION axmi125_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_xmag_m.xmag000 IS NULL
   OR g_xmag_m.xmag001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE axmi125_master_referesh USING g_xmag_m.xmag000,g_xmag_m.xmag001 INTO g_xmag_m.xmag000,g_xmag_m.xmag001 

 
   ERROR ""
  
   LET g_xmag000_t = g_xmag_m.xmag000
   LET g_xmag001_t = g_xmag_m.xmag001
 
   CALL s_transaction_begin()
   
   OPEN axmi125_cl USING g_enterprise, g_site,g_xmag_m.xmag000
                                                       ,g_xmag_m.xmag001
 
   IF STATUS THEN
      CALL cl_err("OPEN axmi125_cl:", STATUS, 1)
      CLOSE axmi125_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH axmi125_cl INTO g_xmag_m.xmag000,g_xmag_m.xmag001,g_xmag_m.pmaal003,g_xmag_m.pmaal004,g_xmag_m.pmaa003 

 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_xmag_m.xmag000,SQLCA.sqlcode,0)
      CLOSE axmi125_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL axmi125_show()
   WHILE TRUE
      LET g_xmag000_t = g_xmag_m.xmag000
      LET g_xmag001_t = g_xmag_m.xmag001
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL axmi125_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_xmag_m.* = g_xmag_m_t.*
         CALL axmi125_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_xmag_m.xmag000 != g_xmag000_t 
      OR g_xmag_m.xmag001 != g_xmag001_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE xmag_t SET xmag000 = g_xmag_m.xmag000
                                       , xmag001 = g_xmag_m.xmag001
 
          WHERE xmagent = g_enterprise AND xmagsite = g_site AND xmag000 = g_xmag000_t
            AND xmag001 = g_xmag001_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("xmag_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("xmag_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_xmag_m.xmag000,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE axmi125_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_xmag_m.xmag000,'U')
 
   CALL axmi125_b_fill("1=1")
   
