  <section id="axmi125.b_fill" ver="6" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION axmi125_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_xmag_d.clear()
   CALL g_xmag2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE xmagstus,xmag002,xmag003,xmag004,xmag005,xmag006,xmag007,xmag008,xmag009, 
       xmag010,xmag011,xmag012,xmag013,xmag014,xmag015,xmag017,xmag016,xmag018,xmag019,xmag020,xmag002, 
       xmag003,xmag004,xmag005,xmag006,xmag007,xmag009,xmagownid,xmagowndp,xmagcrtid,xmagcrtdp,xmagcrtdt, 
       xmagmodid,xmagmoddt,xmag000,t1.imaal003 ,t2.imaal004 ,t3.oocql004 ,t4.oocal003 ,t5.ooail003 , 
       t6.oodbl004 ,t7.oocal003 ,t8.oofa011 ,t9.oofa011 ,t10.ooefl003 ,t11.oofa011 ,t12.ooefl003 ,t13.oofa011 FROM xmag_t", 
          
               "",
               
                              " LEFT JOIN imaal_t t1 ON t1.imaalent='"||g_enterprise||"' AND t1.imaal001=xmag002 AND t1.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t2 ON t2.imaalent='"||g_enterprise||"' AND t2.imaal001=xmag002 AND t2.imaal002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t3 ON t3.oocqlent='"||g_enterprise||"' AND t3.oocql001='221' AND t3.oocql002=xmag004 AND t3.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t4 ON t4.oocalent='"||g_enterprise||"' AND t4.oocal001=xmag006 AND t4.oocal002='"||g_dlang||"' ",
               " LEFT JOIN ooail_t t5 ON t5.ooailent='"||g_enterprise||"' AND t5.ooail001=xmag007 AND t5.ooail002='"||g_dlang||"' ",
               " LEFT JOIN oodbl_t t6 ON t6.oodblent='"||g_enterprise||"' AND t6.oodbl002=xmag009 AND t6.oodbl003='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t7 ON t7.oocalent='"||g_enterprise||"' AND t7.oocal001=xmag014 AND t7.oocal002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t8 ON t8.oofaent='"||g_enterprise||"' AND t8.oofa002='2' AND t8.oofa003=xmag020  ",
               " LEFT JOIN oofa_t t9 ON t9.oofaent='"||g_enterprise||"' AND t9.oofa002='2' AND t9.oofa003=xmagownid  ",
               " LEFT JOIN ooefl_t t10 ON t10.ooeflent='"||g_enterprise||"' AND t10.ooefl001=xmagowndp AND t10.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t11 ON t11.oofaent='"||g_enterprise||"' AND t11.oofa002='2' AND t11.oofa003=xmagcrtid  ",
               " LEFT JOIN ooefl_t t12 ON t12.ooeflent='"||g_enterprise||"' AND t12.ooefl001=xmagcrtdp AND t12.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t13 ON t13.oofaent='"||g_enterprise||"' AND t13.oofa002='2' AND t13.oofa003=xmagmodid  ",
 
               " WHERE xmagent= ? AND xmagsite= ? AND xmag000=? AND xmag001=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("xmag_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF axmi125_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY xmag_t.xmag002,xmag_t.xmag003,xmag_t.xmag004,xmag_t.xmag005,xmag_t.xmag006,xmag_t.xmag007,xmag_t.xmag009"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE axmi125_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR axmi125_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise, g_site,g_xmag_m.xmag000,g_xmag_m.xmag001
                                               
      FOREACH b_fill_cs INTO g_xmag_d[l_ac].xmagstus,g_xmag_d[l_ac].xmag002,g_xmag_d[l_ac].xmag003,g_xmag_d[l_ac].xmag004, 
          g_xmag_d[l_ac].xmag005,g_xmag_d[l_ac].xmag006,g_xmag_d[l_ac].xmag007,g_xmag_d[l_ac].xmag008, 
          g_xmag_d[l_ac].xmag009,g_xmag_d[l_ac].xmag010,g_xmag_d[l_ac].xmag011,g_xmag_d[l_ac].xmag012, 
          g_xmag_d[l_ac].xmag013,g_xmag_d[l_ac].xmag014,g_xmag_d[l_ac].xmag015,g_xmag_d[l_ac].xmag017, 
          g_xmag_d[l_ac].xmag016,g_xmag_d[l_ac].xmag018,g_xmag_d[l_ac].xmag019,g_xmag_d[l_ac].xmag020, 
          g_xmag2_d[l_ac].xmag002,g_xmag2_d[l_ac].xmag003,g_xmag2_d[l_ac].xmag004,g_xmag2_d[l_ac].xmag005, 
          g_xmag2_d[l_ac].xmag006,g_xmag2_d[l_ac].xmag007,g_xmag2_d[l_ac].xmag009,g_xmag2_d[l_ac].xmagownid, 
          g_xmag2_d[l_ac].xmagowndp,g_xmag2_d[l_ac].xmagcrtid,g_xmag2_d[l_ac].xmagcrtdp,g_xmag2_d[l_ac].xmagcrtdt, 
          g_xmag2_d[l_ac].xmagmodid,g_xmag2_d[l_ac].xmagmoddt,g_xmag2_d[l_ac].xmag000,g_xmag_d[l_ac].xmag002_desc, 
          g_xmag_d[l_ac].xmag002_desc_desc,g_xmag_d[l_ac].xmag004_desc,g_xmag_d[l_ac].xmag006_desc,g_xmag_d[l_ac].xmag007_desc, 
          g_xmag_d[l_ac].xmag009_desc,g_xmag_d[l_ac].xmag014_desc,g_xmag_d[l_ac].xmag020_desc,g_xmag2_d[l_ac].xmagownid_desc, 
          g_xmag2_d[l_ac].xmagowndp_desc,g_xmag2_d[l_ac].xmagcrtid_desc,g_xmag2_d[l_ac].xmagcrtdp_desc, 
          g_xmag2_d[l_ac].xmagmodid_desc
                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_xmag2_d[l_ac].xmagownid_desc = cl_get_username(g_xmag2_d[l_ac].xmagownid)
      #LET g_xmag2_d[l_ac].xmagowndp_desc = cl_get_deptname(g_xmag2_d[l_ac].xmagowndp)
      #LET g_xmag2_d[l_ac].xmagcrtid_desc = cl_get_username(g_xmag2_d[l_ac].xmagcrtid)
      #LET g_xmag2_d[l_ac].xmagcrtdp_desc = cl_get_deptname(g_xmag2_d[l_ac].xmagcrtdp)
      #LET g_xmag2_d[l_ac].xmagmodid_desc = cl_get_username(g_xmag2_d[l_ac].xmagmodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_xmag_d.deleteElement(g_xmag_d.getLength())
      CALL g_xmag2_d.deleteElement(g_xmag2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE axmi125_pb   
   
END FUNCTION
]]>
</section>
