  <section id="axmi125.delete" ver="5" status="" src="s">
<![CDATA[#+ 資料刪除
PRIVATE FUNCTION axmi125_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_xmag_m.xmag000 IS NULL
   OR g_xmag_m.xmag001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE axmi125_master_referesh USING g_xmag_m.xmag000,g_xmag_m.xmag001 INTO g_xmag_m.xmag000,g_xmag_m.xmag001 

   
   CALL axmi125_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN axmi125_cl USING g_enterprise, g_site,g_xmag_m.xmag000
                                                       ,g_xmag_m.xmag001
 
   IF STATUS THEN
      CALL cl_err("OPEN axmi125_cl:", STATUS, 1)
      CLOSE axmi125_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH axmi125_cl INTO g_xmag_m.xmag000,g_xmag_m.xmag001,g_xmag_m.pmaal003,g_xmag_m.pmaal004,g_xmag_m.pmaa003 

   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_xmag_m.xmag000,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL axmi125_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM xmag_t WHERE xmagent = g_enterprise AND xmagsite = g_site AND xmag000 = g_xmag_m.xmag000 

                                                               AND xmag001 = g_xmag_m.xmag001
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("xmag_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_xmag_d.clear() 
      CALL g_xmag2_d.clear()       
 
     
      CALL axmi125_ui_browser_refresh()  
      CALL axmi125_ui_headershow()  
      CALL axmi125_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL axmi125_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL axmi125_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE axmi125_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_xmag_m.xmag000,'D')
    
END FUNCTION
]]>
</section>
