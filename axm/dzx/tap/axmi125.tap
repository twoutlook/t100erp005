  <section id="axmi125.detail_reproduce" ver="3" status="" src="s">
<![CDATA[#+ 單身自動複製
PRIVATE FUNCTION axmi125_detail_reproduce()
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE xmag_t.*
 
 
   #add-point:delete段define
   {<point name="detail_reproduce.define"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE axmi125_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE axmi125_detail AS ",
                "SELECT * FROM xmag_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO axmi125_detail SELECT * FROM xmag_t 
                                         WHERE xmagent = g_enterprise AND xmagsite = g_site AND xmag000 = g_xmag000_t
                                         AND xmag001 = g_xmag001_t
 
   
   #將key修正為調整後   
   UPDATE axmi125_detail 
      #更新key欄位
      SET xmag000 = g_xmag_m.xmag000
          , xmag001 = g_xmag_m.xmag001
 
      #更新共用欄位
      #此段落由子樣板a13產生
       , xmagownid = g_user
       , xmagowndp = g_dept
       , xmagcrtid = g_user
       , xmagcrtdp = g_dept 
       , xmagcrtdt = ld_date
       , xmagmodid = "" 
       , xmagmoddt = "" 
      #, xmagstus = "Y"
 
 
                                       
  
   #將資料塞回原table   
   INSERT INTO xmag_t SELECT * FROM axmi125_detail
   
   IF SQLCA.sqlcode THEN
      CALL cl_err("reproduce",SQLCA.sqlcode,1)
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE axmi125_detail
   
   #add-point:單身複製後1
   {<point name="detail_reproduce.body.table1.a_insert"/>}
   #end add-point
 
 
   
 
   
   #多語言複製段落
   
   
   CALL s_transaction_end('Y','0')
   
   #已新增完, 調整資料內容(修改時使用)
   LET g_xmag000_t = g_xmag_m.xmag000
   LET g_xmag001_t = g_xmag_m.xmag001
 
   
   DROP TABLE axmi125_detail
   
END FUNCTION
]]>
