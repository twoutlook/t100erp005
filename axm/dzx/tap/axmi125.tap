  <point name="function.axmi125_xmag001_chk" cite_std="N" status="" ver="1" src="s" new="Y" order="8" mark_hard="N">
<![CDATA[################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL axmi125_xmag001_chk(p_xmag001)
#                  RETURNING 回传参数
# Input parameter: p_xmag001      客戶編號
# Return code....: r_success      檢查是否通過
# Date & Author..: 2014/01/22 By lixh
################################################################################
PRIVATE FUNCTION axmi125_xmag001_chk(p_xmag001)
DEFINE p_xmag001    LIKE xmag_t.xmag001
DEFINE r_success    LIKE type_t.num5
       
       LET r_success = TRUE
       
       IF NOT cl_null(p_xmag001) THEN
          IF NOT ap_chk_notDup("","SELECT COUNT(*) FROM xmag_t WHERE "||"xmagent = '" ||g_enterprise|| "' AND xmagsite = '" ||g_site|| "' AND "||"xmag000 = '"||g_xmag_m.xmag000 ||"' AND "|| "xmag001 = '"||p_xmag001 ||"'",'std-00004',1) THEN 
             LET r_success = FALSE
             RETURN r_success
          END IF

          INITIALIZE g_chkparam.* TO NULL         
          #設定g_chkparam.*的參數
          LET g_chkparam.arg1 = p_xmag001           
          #呼叫檢查存在並帶值的library
          IF cl_chk_exist("v_pmaa001_3") THEN
             #檢查成功時後續處理
          ELSE
             #檢查失敗時後續處理
             LET r_success = FALSE
             RETURN r_success
          END IF
       END IF 
       RETURN r_success
END FUNCTION]]>
</point>
