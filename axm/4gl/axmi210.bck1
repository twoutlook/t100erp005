################################################################################
# Descriptions...: 取類別
# Usage..........: CALL axmi210_class()
# Input parameter:
# Return code....:
# Date & Author..: 2014/9/19 By shiunyo
################################################################################
PRIVATE FUNCTION axmi210_class()
   DEFINE l_sql   STRING
   DEFINE l_sql2  STRING
   LET l_sql = "  SELECT gzcbl002",
               "    FROM gzcbl_t",
               "   WHERE gzcbl001 = 2102",
               "     AND gzcbl003 = '",g_lang,"'",
               "ORDER BY gzcbl002"
      
   PREPARE axmi210_pb2 FROM l_sql
   DECLARE b_fill_cs2 CURSOR FOR axmi210_pb2
   LET g_cnt = l_ac
   LET l_ac = 1                             
   FOREACH b_fill_cs2 INTO g_class[l_ac].*
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
   
         EXIT FOREACH
      END IF
     
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
   
      LET l_ac = l_ac + 1
   END FOREACH
   
   LET l_sql2 = "  SELECT gzcbl002,gzcbl004",
                "    FROM gzcbl_t",
                "   WHERE gzcbl001 = 2101",
                "     AND gzcbl003 = '",g_lang,"'",
                "ORDER BY gzcbl002"
      
   PREPARE axmi210_pb3 FROM l_sql2
   DECLARE b_fill_cs3 CURSOR FOR axmi210_pb3
   LET g_cnt = l_ac
   LET l_ac = 1                             
   FOREACH b_fill_cs3 INTO g_gzcbl[l_ac].*
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
   
         EXIT FOREACH
      END IF
     
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
   
      LET l_ac = l_ac + 1
   END FOREACH
   
   CALL g_class.deleteElement(g_class.getLength())
   CALL g_gzcbl.deleteElement(g_gzcbl.getLength())
END FUNCTION

