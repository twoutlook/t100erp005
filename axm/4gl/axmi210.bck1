#+ 資料刪除
PRIVATE FUNCTION axmi210_delete()
   #add-point:delete段define(客製用) name="delete.define_customerization"
   
   #end add-point     
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="delete.define"
   
   #end add-point     
   
   #add-point:Function前置處理  name="delete.pre_function"
   
   #end add-point
   
   IF g_xmao_m.xmao001 IS NULL
   OR g_xmao_m.xmao002 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code = "std-00003" 
      LET g_errparam.popup = FALSE 
      CALL cl_err()
      RETURN
   END IF
   
   LET g_master_multi_table_t.xmaol001 = g_xmao_m.xmao001
LET g_master_multi_table_t.xmaol002 = g_xmao_m.xmao002
LET g_master_multi_table_t.xmaol004 = g_xmao_m.xmaol004
 
   
   CALL s_transaction_begin()
 
   OPEN axmi210_cl USING g_enterprise,g_xmao_m.xmao001,g_xmao_m.xmao002
   IF SQLCA.SQLCODE THEN   #(ver:78)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN axmi210_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE   #(ver:78)
      LET g_errparam.popup = TRUE 
      CLOSE axmi210_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE axmi210_master_referesh USING g_xmao_m.xmao001,g_xmao_m.xmao002 INTO g_xmao_m.xmao001,g_xmao_m.xmao002, 
       g_xmao_m.xmaostus,g_xmao_m.xmaoownid,g_xmao_m.xmaoowndp,g_xmao_m.xmaocrtid,g_xmao_m.xmaocrtdp, 
       g_xmao_m.xmaocrtdt,g_xmao_m.xmaomodid,g_xmao_m.xmaomoddt,g_xmao_m.xmao003,g_xmao_m.xmao004,g_xmao_m.xmao001_desc, 
       g_xmao_m.xmaoownid_desc,g_xmao_m.xmaoowndp_desc,g_xmao_m.xmaocrtid_desc,g_xmao_m.xmaocrtdp_desc, 
       g_xmao_m.xmaomodid_desc
   
   
   #檢查是否允許此動作
   IF NOT axmi210_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_xmao_m_mask_o.* =  g_xmao_m.*
   CALL axmi210_xmao_t_mask()
   LET g_xmao_m_mask_n.* =  g_xmao_m.*
   
   CALL axmi210_show()
   
   #add-point:delete段before ask name="delete.before_ask"
   
   #end add-point 
 
   IF cl_ask_del_master() THEN              #確認一下
   
      #add-point:單頭刪除前 name="delete.head.b_delete"
      
      #end add-point   
      
      #應用 a47 樣板自動產生(Version:4)
      #刪除相關文件
      CALL axmi210_set_pk_array()
      #add-point:相關文件刪除前 name="delete.befroe.related_document_remove"
      
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
  
  
      #資料備份
      LET g_xmao001_t = g_xmao_m.xmao001
      LET g_xmao002_t = g_xmao_m.xmao002
 
 
      DELETE FROM xmao_t
       WHERE xmaoent = g_enterprise AND xmao001 = g_xmao_m.xmao001
         AND xmao002 = g_xmao_m.xmao002
 
       
      #add-point:單頭刪除中 name="delete.head.m_delete"
      
      #end add-point
       
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = g_xmao_m.xmao001,":",SQLERRMESSAGE  
         LET g_errparam.code = SQLCA.SQLCODE 
         LET g_errparam.popup = FALSE 
         CALL s_transaction_end('N','0')
         CALL cl_err()
         RETURN
      END IF
      
      #add-point:單頭刪除後 name="delete.head.a_delete"
      
      #end add-point
  
      #add-point:單身刪除前 name="delete.body.b_delete"
      
      #end add-point
      
      DELETE FROM xmap_t
       WHERE xmapent = g_enterprise AND xmap001 = g_xmao_m.xmao001
         AND xmap002 = g_xmao_m.xmao002
 
 
      #add-point:單身刪除中 name="delete.body.m_delete"
      
      #end add-point
         
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "xmap_t:",SQLERRMESSAGE 
         LET g_errparam.code = SQLCA.SQLCODE 
         LET g_errparam.popup = FALSE 
         CALL s_transaction_end('N','0')
         CALL cl_err()
         RETURN
      END IF    
 
      #add-point:單身刪除後 name="delete.body.a_delete"
      
      #end add-point
      
            
                                                               
 
 
 
      
      #修改歷程記錄(刪除)
      LET g_log1 = util.JSON.stringify(g_xmao_m)   #(ver:78)
      IF NOT cl_log_modified_record(g_log1,'') THEN    #(ver:78)
         CLOSE axmi210_cl
         CALL s_transaction_end('N','0')
         RETURN
      END IF
             
      CLEAR FORM
      CALL g_xmap_d.clear() 
 
     
      CALL axmi210_ui_browser_refresh()  
      #CALL axmi210_ui_headershow()  
      #CALL axmi210_ui_detailshow()
 
      #add-point:多語言刪除 name="delete.lang.before_delete"
      
      #end add-point
      
      #單頭多語言刪除
      INITIALIZE l_var_keys_bak TO NULL 
   INITIALIZE l_field_keys   TO NULL 
   LET l_var_keys_bak[01] = g_enterprise
   LET l_field_keys[01] = 'xmaolent'
   LET l_var_keys_bak[02] = g_master_multi_table_t.xmaol001
   LET l_field_keys[02] = 'xmaol001'
   LET l_var_keys_bak[03] = g_master_multi_table_t.xmaol002
   LET l_field_keys[03] = 'xmaol002'
   CALL cl_multitable_delete(l_field_keys,l_var_keys_bak,'xmaol_t')
 
      
      #單身多語言刪除
      
 
   
      #add-point:多語言刪除 name="delete.lang.delete"
 
      #end add-point
      
      IF g_browser_cnt > 0 THEN 
         #CALL axmi210_browser_fill("")
         CALL axmi210_fetch('P')
         DISPLAY g_browser_cnt TO FORMONLY.h_count   #總筆數的顯示
         DISPLAY g_browser_cnt TO FORMONLY.b_count   #總筆數的顯示
      ELSE
         CLEAR FORM
      END IF
      
      CALL s_transaction_end('Y','0')
   ELSE
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE axmi210_cl
 
   #功能已完成,通報訊息中心
   CALL axmi210_msgcentre_notify('delete')
    
