{<section id="axmi210.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION axmi210_b_fill()
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   
   #end add-point     
   DEFINE p_wc2      STRING
   DEFINE li_idx     LIKE type_t.num10
   #add-point:b_fill段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   DEFINE l_sql      STRING
   DEFINE l_count    LIKE type_t.num5
   #end add-point     
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   #清空第一階單身
   CALL g_xmap_d.clear()
 
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   IF g_wc2_search_first THEN
      LET l_sql = "SELECT xmap003 FROM xmap_t ",
                  " WHERE xmapent = ",g_enterprise,
                  "   AND xmap001 = '",g_xmao_m.xmao001,"'",
                  "   AND xmap002 = '",g_xmao_m.xmao002,"'",
                  "   AND ",g_wc2_table1,
                  " ORDER BY xmap003"
      PREPARE xmap003_pre FROM l_sql
      DECLARE xmap003_cs CURSOR FOR xmap003_pre
      FOREACH xmap003_cs INTO g_class_search
         EXIT FOREACH
      END FOREACH
   END IF
   #end add-point
   
   #判斷是否填充
   IF axmi210_fill_chk(1) THEN
      #切換上下筆時不重組SQL
      IF (g_action_choice = "query" OR cl_null(g_action_choice))
      #add-point:b_fill段long_sql_if name="b_fill.long_sql_if"
         OR g_action_choice = "fetch" #170214-00038#1 add
      #end add-point
      THEN
         LET g_sql = "SELECT  DISTINCT xmap003,xmap004,xmap005,xmap006  FROM xmap_t",   
                     " INNER JOIN xmao_t ON xmaoent = " ||g_enterprise|| " AND xmao001 = xmap001 ",
                     " AND xmao002 = xmap002 ",
 
                     #"",
                     
                     "",
                     #下層單身所需的join條件
 
                     
                     " WHERE xmapent=? AND xmap001=? AND xmap002=?"
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before name="b_fill.body.fill_sql"
      IF NOT cl_null(g_class_search) THEN
         LET g_sql = g_sql," AND xmap003 = '",g_class_search,"'"
      ELSE
         SELECT gzcbl002 INTO g_class_search
           FROM gzcbl_t
          WHERE gzcbl001 = 2102
          ORDER BY gzcbl002
         LET g_sql = g_sql," AND xmap003 = '",g_class_search,"'"
      END IF
         #end add-point
         IF NOT cl_null(g_wc2_table1) THEN
            LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY xmap_t.xmap003,xmap_t.xmap004"
         
         #add-point:單身填充控制 name="b_fill.sql"
 
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE axmi210_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR axmi210_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
   #  OPEN b_fill_cs USING g_enterprise,g_xmao_m.xmao001,g_xmao_m.xmao002   #(ver:78)
                                               
      FOREACH b_fill_cs USING g_enterprise,g_xmao_m.xmao001,g_xmao_m.xmao002 INTO g_xmap_d[l_ac].xmap003, 
          g_xmap_d[l_ac].xmap004,g_xmap_d[l_ac].xmap005,g_xmap_d[l_ac].xmap006   #(ver:78)
         IF SQLCA.SQLCODE THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code = SQLCA.SQLCODE 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充 name="b_fill.fill"
 
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code = 9035 
               LET g_errparam.popup = TRUE 
               CALL cl_err()
            END IF
            EXIT FOREACH
         END IF
         
         LET l_ac = l_ac + 1
      END FOREACH
      LET g_error_show = 0
   
   END IF
    
 
   
   #add-point:browser_fill段其他table處理 name="browser_fill.other_fill"
#   CALL axmi210_class()
#      SELECT COUNT(*) INTO l_count
#        FROM xmap_t
#       WHERE xmapent = g_enterprise
#         AND xmap001 = g_xmao_m.xmao001
#         AND xmap002 = g_xmao_m.xmao002
#         AND xmap003 = g_class_search
#      IF l_count = 0 THEN
#         LET g_cnt = 1
#      ELSE
#         LET g_cnt = l_count
#      END IF
   #end add-point
   
   CALL g_xmap_d.deleteElement(g_xmap_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE axmi210_pb
 
   
   LET li_idx = l_ac
   
   #遮罩相關處理
   FOR l_ac = 1 TO g_xmap_d.getLength()
      LET g_xmap_d_mask_o[l_ac].* =  g_xmap_d[l_ac].*
      CALL axmi210_xmap_t_mask()
      LET g_xmap_d_mask_n[l_ac].* =  g_xmap_d[l_ac].*
   END FOR
   
 
   
   LET l_ac = li_idx
   
   CALL cl_ap_performance_next_end()
 
END FUNCTION
 
