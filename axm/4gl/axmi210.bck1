{<section id="axmi210.state_change" >}
   #應用 a09 樣板自動產生(Version:17)
#+ 確認碼變更 
PRIVATE FUNCTION axmi210_statechange()
   #add-point:statechange段define(客製用) name="statechange.define_customerization"
   
   #end add-point  
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="statechange.define"
   
   #end add-point  
   
   #add-point:Function前置處理 name="statechange.before"
   
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_xmao_m.xmao001 IS NULL
      OR g_xmao_m.xmao002 IS NULL
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN axmi210_cl USING g_enterprise,g_xmao_m.xmao001,g_xmao_m.xmao002
   IF STATUS THEN
      CLOSE axmi210_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN axmi210_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE axmi210_master_referesh USING g_xmao_m.xmao001,g_xmao_m.xmao002 INTO g_xmao_m.xmao001,g_xmao_m.xmao002, 
       g_xmao_m.xmaostus,g_xmao_m.xmaoownid,g_xmao_m.xmaoowndp,g_xmao_m.xmaocrtid,g_xmao_m.xmaocrtdp, 
       g_xmao_m.xmaocrtdt,g_xmao_m.xmaomodid,g_xmao_m.xmaomoddt,g_xmao_m.xmao003,g_xmao_m.xmao004,g_xmao_m.xmao001_desc, 
       g_xmao_m.xmaoownid_desc,g_xmao_m.xmaoowndp_desc,g_xmao_m.xmaocrtid_desc,g_xmao_m.xmaocrtdp_desc, 
       g_xmao_m.xmaomodid_desc
   
 
   #檢查是否允許此動作
   IF NOT axmi210_action_chk() THEN
      CLOSE axmi210_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_xmao_m.xmao001,g_xmao_m.xmao001_desc,g_xmao_m.xmao002,g_xmao_m.xmaol004,g_xmao_m.xmaostus, 
       g_xmao_m.xmaoownid,g_xmao_m.xmaoownid_desc,g_xmao_m.xmaoowndp,g_xmao_m.xmaoowndp_desc,g_xmao_m.xmaocrtid, 
       g_xmao_m.xmaocrtid_desc,g_xmao_m.xmaocrtdp,g_xmao_m.xmaocrtdp_desc,g_xmao_m.xmaocrtdt,g_xmao_m.xmaomodid, 
       g_xmao_m.xmaomodid_desc,g_xmao_m.xmaomoddt,g_xmao_m.xmao003,g_xmao_m.xmao004
 
   CASE g_xmao_m.xmaostus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_xmao_m.xmaostus
            
            WHEN "N"
               HIDE OPTION "inactive"
            WHEN "Y"
               HIDE OPTION "active"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      
      #end add-point
      
      
	  
      ON ACTION inactive
         IF cl_auth_chk_act("inactive") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.inactive"
            
            #end add-point
         END IF
         EXIT MENU
      ON ACTION active
         IF cl_auth_chk_act("active") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.active"
            
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      ) OR 
      g_xmao_m.xmaostus = lc_state OR cl_null(lc_state) THEN
      CLOSE axmi210_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   
   #end add-point
   
   LET g_xmao_m.xmaomodid = g_user
   LET g_xmao_m.xmaomoddt = cl_get_current()
   LET g_xmao_m.xmaostus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE xmao_t 
      SET (xmaostus,xmaomodid,xmaomoddt) 
        = (g_xmao_m.xmaostus,g_xmao_m.xmaomodid,g_xmao_m.xmaomoddt)     
    WHERE xmaoent = g_enterprise AND xmao001 = g_xmao_m.xmao001
      AND xmao002 = g_xmao_m.xmao002
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE axmi210_master_referesh USING g_xmao_m.xmao001,g_xmao_m.xmao002 INTO g_xmao_m.xmao001, 
          g_xmao_m.xmao002,g_xmao_m.xmaostus,g_xmao_m.xmaoownid,g_xmao_m.xmaoowndp,g_xmao_m.xmaocrtid, 
          g_xmao_m.xmaocrtdp,g_xmao_m.xmaocrtdt,g_xmao_m.xmaomodid,g_xmao_m.xmaomoddt,g_xmao_m.xmao003, 
          g_xmao_m.xmao004,g_xmao_m.xmao001_desc,g_xmao_m.xmaoownid_desc,g_xmao_m.xmaoowndp_desc,g_xmao_m.xmaocrtid_desc, 
          g_xmao_m.xmaocrtdp_desc,g_xmao_m.xmaomodid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_xmao_m.xmao001,g_xmao_m.xmao001_desc,g_xmao_m.xmao002,g_xmao_m.xmaol004,g_xmao_m.xmaostus, 
          g_xmao_m.xmaoownid,g_xmao_m.xmaoownid_desc,g_xmao_m.xmaoowndp,g_xmao_m.xmaoowndp_desc,g_xmao_m.xmaocrtid, 
          g_xmao_m.xmaocrtid_desc,g_xmao_m.xmaocrtdp,g_xmao_m.xmaocrtdp_desc,g_xmao_m.xmaocrtdt,g_xmao_m.xmaomodid, 
          g_xmao_m.xmaomodid_desc,g_xmao_m.xmaomoddt,g_xmao_m.xmao003,g_xmao_m.xmao004
   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
   
   #end add-point
 
   #add-point:statechange段結束前 name="statechange.after"
   
   #end add-point  
 
   CLOSE axmi210_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL axmi210_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION
 
 
 
 
