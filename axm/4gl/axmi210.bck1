PRIVATE FUNCTION axmi210_set_pk_array_b()
   IF l_ac <= 0 OR cl_null (l_ac) THEN
      RETURN
   END IF
   CALL g_pk_array.clear()
   LET g_pk_array[1].values = g_xmao_m.xmao001
   LET g_pk_array[1].column = 'xmap001'
   LET g_pk_array[2].values = g_xmao_m.xmao002
   LET g_pk_array[2].column = 'xmap002'
   LET g_pk_array[3].values = g_xmap_d[l_ac].xmap003
   LET g_pk_array[3].column = 'xmap003'
   LET g_pk_array[4].values = g_xmap_d[l_ac].xmap004
   LET g_pk_array[4].column = 'xmap004'
   
END FUNCTION

