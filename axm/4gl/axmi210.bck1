                      " xmao001 = '", g_xmao_m.xmao001, "' "
                      ," AND xmao002 = '", g_xmao_m.xmao002, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL axmi210_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   #add-point:完成複製段落後 name="reproduce.after_reproduce"
   
   #end add-point
   
   CALL axmi210_idx_chk()
   
   LET g_data_owner = g_xmao_m.xmaoownid      
   LET g_data_dept  = g_xmao_m.xmaoowndp
   
   #功能已完成,通報訊息中心
   CALL axmi210_msgcentre_notify('reproduce')
 
