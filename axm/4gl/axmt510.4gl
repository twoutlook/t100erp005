PRIVATE FUNCTION axmt510_xmeedocno_xmeh()
DEFINE l_sql             STRING
DEFINE r_success         LIKE type_t.num5

   LET r_success = TRUE

   IF cl_null(g_xmee_m.xmeedocno) THEN
      LET r_success = FALSE
      RETURN r_success
   END IF
   
   LET l_sql = "SELECT xmddsite,xmddseq,xmddseq1,xmdd003, ",
               "       xmdd001,xmdd002,xmdd004,xmdd005,xmddseq2, ",
               "       xmdd006,xmdd024,xmdd025,xmdd011,xmdd012, ",
