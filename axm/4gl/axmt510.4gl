# Descriptions...: 檢查料件是否為附屬零件或可選件
# Memo...........:
# Usage..........: CALL axmt510_get_bmab(p_xmeg001,p_xmegunit,p_xmeg062)
#                  RETURNING TRUE/FALSE
# Input parameter: p_xmeg001      料號
#                : p_xmegunit     出貨據點
# Return code....: TRUE/FALSE
# Date & Author..: 
# Modify.........: #161031-00049#1 By 02040
################################################################################
PRIVATE FUNCTION axmt510_get_bmab(p_xmeg001,p_xmegunit,p_xmeg062)
   DEFINE p_xmeg001    LIKE xmeg_t.xmeg001
   DEFINE p_xmegunit   LIKE xmeg_t.xmegunit 
   DEFINE l_imae037    LIKE imae_t.imae037     #預設BOM特性 
   DEFINE l_cnt        LIKE type_t.num5
   DEFINE l_cnt2       LIKE type_t.num5   
   DEFINE l_sql        STRING
   DEFINE p_xmeg062    LIKE xmeg_t.xmeg062    

   LET l_cnt = 0
   SELECT COUNT(1) INTO l_cnt FROM imaf_t
    WHERE imafent = g_enterprise
      AND imafsite = g_site
      AND imaf001 = p_xmeg001
      AND imaf018 = 'Y'
   IF l_cnt > 0 THEN   
      #抓取料件特性
      LET l_cnt2 = 0       

      LET l_imae037 = p_xmeg062    
           
      LET l_sql = "SELECT COUNT(1) FROM ( ",  
                  "   SELECT bmba020,bmba025 FROM bmaa_t,bmba_t ",
                  "    WHERE bmaa001 = bmba001 AND bmaa002 = bmba002 ",
                  "      AND bmaaent = bmbaent AND bmaasite = bmbasite ",
                  "      AND bmba005 <= SYSDATE ",    
                  "      AND (bmba006 >= SYSDATE OR bmba006 IS NULL) ",
                  "   Connect by bmba001 = prior bmba003  AND bmba002 = prior bmba002 ",
                  "              AND bmbasite = prior bmbasite ",    
                  "   Start with bmaa001 = '",p_xmeg001,"' ",
                  "          AND bmaa002 = '",l_imae037,"' ",
                  "          AND bmbasite = '",p_xmegunit,"' ",
                  "   ) ", 
                  " WHERE (bmba025 = 'Y' OR bmba020 = 'Y') "   
      PREPARE axmt510_BOM_p FROM l_sql
      EXECUTE axmt510_BOM_p INTO l_cnt2
      IF l_cnt2 > 0 THEN
         RETURN TRUE
      ELSE
         RETURN FALSE
      END IF
   ELSE
      RETURN FALSE   
   END IF

END FUNCTION
