               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 'adb-00080'
               LET g_errparam.extend = ''
               LET g_errparam.popup = FALSE
               CALL cl_err()
               NEXT FIELD CURRENT
            END IF


         AFTER FIELD xmef005
            IF NOT cl_ap_chk_Range(g_xmeg5_d[l_ac].xmef005,"0.000","0","","","azz-00079",1) THEN
               NEXT FIELD xmef005
            END IF
            IF NOT cl_null(g_xmeg5_d[l_ac].xmef005) THEN
               IF l_cmd = 'a' OR (l_cmd = 'u' AND (g_xmeg5_d[l_ac].xmef005 != g_xmeg5_d_o.xmef005 OR g_xmeg5_d_o.xmef005 IS NULL )) THEN            
                  LET l_xmef005 = 0
