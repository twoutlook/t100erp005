                  " WHERE t0.xmeeent = " ||g_enterprise|| " AND ",l_wc, cl_sql_add_filter("xmee_t")
   END IF
   #add-point:browser_fill,sql wc name="browser_fill.fill_sqlwc"
   #161031-00025#24-s
   IF NOT cl_null(g_wc2_i36001) AND g_wc2_i36001 <> " 1=1" THEN
