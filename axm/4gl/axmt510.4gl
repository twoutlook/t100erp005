         AFTER FIELD xmef006
            IF NOT cl_ap_chk_Range(g_xmeg5_d[l_ac].xmef006,"0.000","0","","","azz-00079",1) THEN
               NEXT FIELD xmef006
            END IF
            IF NOT cl_null(g_xmeg5_d[l_ac].xmef006) THEN
               IF l_cmd = 'a' OR (l_cmd = 'u' AND (g_xmeg5_d[l_ac].xmef006 != g_xmeg5_d_o.xmef006 OR g_xmeg5_d_o.xmef006 IS NULL )) THEN                   
