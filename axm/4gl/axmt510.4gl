                  #end add-point
 
                  "  LEFT JOIN xmfw_t ON xmfwent = xmeeent AND xmeedocno = xmfwdocno AND xmee900 = xmfw900", "  ", 
                  #add-point:browser_fill段sql(xmfw_t1) name="browser_fill.join.xmfw_t1"
                  #" AND xmfwseq = xmeeseq AND xmfw012 = '4'  ",          #161031-00049#1 add  #161031-00025#24 mark
                  " AND xmfwseq = xmegseq AND xmfw012 = '4'  ",          #161031-00025#24 add  
                  " LEFT JOIN ooff_t ON ooffent = xmeeent AND ooff001 = '7' 
                          AND ooff002 = '",g_prog,"'  AND xmeedocno = ooff003  AND ooff004 = xmegseq
                          AND ooff005 = xmee900 ",  "  ",   #161031-00025#24 add                  
                  #end add-point
 
 
 
                  " ", 
                  " ",                      
 
                  " ",                      
 
                  " ",                      
 
