{<section id="axmt510.set_no_entry_b" >}
#+ 單身欄位關閉設定
PRIVATE FUNCTION axmt510_set_no_entry_b(p_cmd)
   #add-point:set_no_entry_b段define(客製用) name="set_no_entry_b.define_customerization"
   
   #end add-point    
   DEFINE p_cmd   LIKE type_t.chr1   
   #add-point:set_no_entry_b段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="set_no_entry_b.define"
   DEFINE l_imaf015 LIKE imaf_t.imaf015
   DEFINE l_imaf061 LIKE imaf_t.imaf061
   DEFINE l_imaf113 LIKE imaf_t.imaf113
   DEFINE l_imaa005 LIKE imaa_t.imaa005  #160623-00017 by whitney add
   DEFINE l_cnt     LIKE type_t.num5     #160706-00017#1-add
   #end add-point    
   
   #add-point:Function前置處理  name="set_no_entry_b.pre_function"
   
   #end add-point
   
   IF p_cmd = 'u' AND g_chkey = 'N' THEN
      CALL cl_set_comp_entry("",FALSE)
      #add-point:set_no_entry_b段欄位控制 name="set_no_entry_b.field_control"
      
      #end add-point 
   END IF 
   
   #add-point:set_no_entry_b段 name="set_no_entry_b.set_no_entry_b"
   CALL cl_set_comp_entry("xmehseq",FALSE)  
   CALL cl_set_comp_entry("xmef001",FALSE)  
   
   #單頭多角性質為"3:統銷統收"、"4:統銷自收"時，出貨據點才可維護
   IF g_xmee_m.xmee006 NOT MATCHES '[34]' THEN
      CALL cl_set_comp_entry("xmegunit",FALSE)
   END IF
   
   
   #若1.正常稅率，則單身稅別不可輸入
   IF g_tax_app = '1' THEN
      CALL cl_set_comp_entry("xmeg016",FALSE)
   END IF
   #單頭取價方式的基本資料設置不允許修改單價
   #160705-00008#2 20160721 -----(S) 
   #IF g_xmah003 <> 'Y' THEN
   #   CALL cl_set_comp_entry("xmeg015",FALSE)
   #END IF
   #161202-00006#2-mark-s   變更單不受取價條件可否改單價的控管，單價欄位開放可修改
#   IF (g_xmeg2_d[l_ac].xmeg043 > 0 OR g_xmah002 <> 'Y') AND g_xmah003 <> 'Y' THEN
#      CALL cl_set_comp_entry("xmeg015",FALSE)
#   END IF
   #161202-00006#2-mark-e
   #160705-00008#2 20160721 -----(E) 

   IF cl_null(g_xmeg_d[l_ac].xmeg001) THEN
      CALL cl_set_comp_entry("xmeg002",FALSE)
   ELSE
      CALL s_axmt500_get_imaa005(g_enterprise,g_xmeg_d[l_ac].xmeg001) RETURNING l_imaa005  #160623-00017 by whitney add
      #料件主檔無特徵類別，不能維護產品特徵
      IF cl_null(l_imaa005) THEN  #160623-00017 by whitney modify g_imaa005 -> l_imaa005
         CALL cl_set_comp_entry("xmeg002",FALSE)
      ELSE
         #若料件有使用產品特徵時，則此欄位不允許空白
         CALL cl_set_comp_required("xmeg002",TRUE)
      END IF
      
      LET g_imaf015 = ''
      LET g_imaf061 = ''
      LET g_imaf113 = ''
      SELECT imaf015,imaf061,imaf113
        INTO l_imaf015,l_imaf061,l_imaf113
        FROM imaf_t
       WHERE imafent = g_enterprise
         AND imafsite = g_site
         AND imaf001 = g_xmeg_d[l_ac].xmeg001
      #[T:料件據點進銷存檔].[C:批號控管]=1或3時才可輸入
      IF l_imaf061 NOT MATCHES '[13]' THEN
         LET g_xmeg2_d[l_ac].xmeg030 = ' '
         CALL cl_set_comp_entry("xmeg030",FALSE)
      END IF
     #--151012--polly--add--(S)     
      #[T:料件據點進銷存檔]未設參考單位，參考單位、參考數量不允許輸入
      #IF cl_null(l_imaf015) THEN
      #   CALL cl_set_comp_entry("xmeg009",FALSE)
      #ELSE
      #   #若有使用參考單位管理時，則不允許空白
      #   CALL cl_set_comp_required("xmeg008",TRUE)         
      #END IF
      ##[T:料件據點進銷存檔]有設計價單位，計價單位不允許空白
      #IF cl_get_para(g_enterprise,g_site,'S-BAS-0007') = "Y" AND NOT cl_null(l_imaf113) THEN
      #   CALL cl_set_comp_required("xmeg010",TRUE)
      #END IF 
    
      #計價單位、計價數量
      IF cl_get_para(g_enterprise,g_site,'S-BAS-0007') = 'N' THEN
         LET g_xmeg_d[l_ac].xmeg010 = g_xmeg_d[l_ac].xmeg006
         LET g_xmeg_d[l_ac].xmeg011 = g_xmeg_d[l_ac].xmeg007
      ELSE
         IF cl_null(l_imaf113) THEN
            CALL cl_set_comp_entry("xmeg010,xmeg011",FALSE)   
            LET g_xmeg_d[l_ac].xmeg010 = g_xmeg_d[l_ac].xmeg006
            LET g_xmeg_d[l_ac].xmeg011 = g_xmeg_d[l_ac].xmeg007  
         END IF            
      END IF      
      #參考單位、參考數量      
      IF cl_get_para(g_enterprise,g_site,'S-BAS-0028') = 'N' THEN
         LET g_xmeg_d[l_ac].xmeg008 = ''
         LET g_xmeg_d[l_ac].xmeg009 = ''        
      ELSE 
         IF cl_null(l_imaf015) THEN
            CALL cl_set_comp_entry("xmeg008,xmeg009",FALSE)  
            LET g_xmeg_d[l_ac].xmeg008 = ''
            LET g_xmeg_d[l_ac].xmeg009 = ''           
         END IF   
      END IF      
     #--151012--polly--add--(E) 
   END IF 
   
   #為多交期時，約定交貨日期、預計簽收日期不可維護
   IF g_xmeg_d[l_ac].xmeg024 = 'Y' THEN
      CALL cl_set_comp_entry("xmeg012,xmeg013",FALSE)
   END IF 
   #多期預付款頁籤：已經有應收帳款與已收金額的資料不可以進行修改
   IF (NOT cl_null(g_xmeg5_d_t.xmef007)) AND (g_xmeg5_d_t.xmef008> 0 OR g_xmeg5_d_t.xmef009 > 0)  OR  NOT g_flag1 THEN
      CALL cl_set_comp_entry("xmef001,xmef002,xmef003,xmef004,xmef005,xmef006",FALSE)
   END IF  
   #多期預付款頁籤：依照單價含稅否開收未稅、含稅欄位輸入
   #IF g_xmee_m.xmee031 = 'Y' THEN     #161122-00057#1  mark
   IF g_xmee_m.xmee013 = 'Y' THEN     #161122-00057#1  add
      CALL cl_set_comp_entry("xmef005",FALSE)
   ELSE
      CALL cl_set_comp_entry("xmef006",FALSE)
   END IF    
   
   #add by lixiang 2015/06/29--s---
   #依訂單單別參數（D-BAS-0095）設定的"專案類型"抓取專案與訂單連動(pjaa003)、專案編號與訂單單號一致(pjaa004)資料
   #pjaa003="Y" AND pjaa004="Y" 則專案編號欄位不可輸，預設帶入訂單單號
   LET g_pjaa001 = ''
   LET g_pjaa001 =  cl_get_doc_para(g_enterprise,g_site,g_slip,'D-BAS-0095')
   IF NOT cl_null(g_pjaa001) THEN
      LET g_pjaa003 = ''
      LET g_pjaa004 = ''
      SELECT pjaa003,pjaa004 INTO g_pjaa003,g_pjaa004 FROM pjaa_t WHERE pjaaent = g_enterprise AND pjaa001 = g_pjaa001
      IF g_pjaa003 = 'Y' AND g_pjaa004 = 'Y' THEN
         LET g_xmeg2_d[l_ac].xmeg036 = g_xmee_m.xmeedocno
         DISPLAY BY NAME g_xmeg2_d[l_ac].xmeg036
         CALL cl_set_comp_entry("xmeg036",FALSE)
      END IF
   END IF
   #add by lixiang 2015/06/29--e----
   
   #160706-00017#1-add-(S)
   LET l_cnt = 0
   SELECT x1.cnt+x2.cnt INTO l_cnt
     FROM (SELECT COUNT(1) cnt FROM xmdg_t,xmdh_t WHERE xmdgent = xmdhent AND xmdgsite = xmdhsite AND xmdgdocno = xmdhdocno 
                                                AND xmdgent = g_enterprise AND xmdgsite = g_site AND xmdh001 = g_xmee_m.xmeedocno) x1,
          (SELECT COUNT(1) cnt FROM xmdk_t,xmdl_t WHERE xmdkent = xmdlent AND xmdksite = xmdlsite AND xmdkdocno=xmdldocno
                                                AND xmdkent = g_enterprise AND xmdksite = g_site AND xmdl003 = g_xmee_m.xmeedocno) x2
   IF l_cnt > 0 THEN
      CALL cl_set_comp_entry("xmeg006",FALSE)
   END IF
   #160706-00017#1-add-(E)
   #end add-point     
END FUNCTION
 
