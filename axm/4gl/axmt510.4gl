#根據料件編號、供應商、銷售控制組帶出欄位預設值
PRIVATE FUNCTION axmt510_xmeg001_desc()
DEFINE l_n           LIKE type_t.num5
DEFINE l_success     LIKE type_t.num5
DEFINE l_controlno   LIKE ooha_t.ooha001
DEFINE l_imaf158     LIKE imaf_t.imaf158
DEFINE l_imaf122     LIKE imaf_t.imaf122
DEFINE l_pmao004     LIKE pmao_t.pmao004
DEFINE l_xmeg016     LIKE xmeg_t.xmeg016
DEFINE l_xmeg017     LIKE xmeg_t.xmeg017
DEFINE l_xmegunit    LIKE xmeg_t.xmegunit    #出貨據點
 
      LET l_n = 0
      LET g_xmeg2_d[l_ac].xmeg003 = ''              #包裝容器
      LET g_xmeg2_d[l_ac].xmeg003_desc = ''
      LET g_xmeg_d[l_ac].xmeg006 = ''               #銷售單位
      LET g_xmeg_d[l_ac].xmeg006_desc = ''   
      LET g_xmeg_d[l_ac].xmeg008 = ''               #參考單位
      LET g_xmeg_d[l_ac].xmeg008_desc = ''         
      LET g_xmeg_d[l_ac].xmeg010 = ''               #計價單位
      LET g_xmeg_d[l_ac].xmeg010_desc = ''    
#151001 by whiteny mark start - 變更料號不重帶庫儲
#      LET g_xmeg2_d[l_ac].xmeg028 = ''              #限定庫位
#      LET g_xmeg2_d[l_ac].xmeg028_desc = ''
#      LET g_xmeg2_d[l_ac].xmeg029 = ''              #限定儲位
#      LET g_xmeg2_d[l_ac].xmeg029_desc = ''    
#151001 by whiteny mark end

   
      #先判斷這個供應商是否有設多個當前銷售控制組範圍內的客戶預設條件，則開窗，讓user 選擇帶哪一個控制組的資料
      LET l_controlno = ''
      #2015/03/04 by stellar add ----- (S)
#      CALL s_control_get_group('2',g_xmee_m.xmee002,g_xmee_m.xmee003) RETURNING l_success,l_controlno
      CALL s_control_get_group('2',g_user,g_dept) RETURNING l_success,l_controlno
      #2015/03/04 by stellar add ----- (E)
      
      IF NOT cl_null(l_controlno) THEN
         SELECT COUNT(1) INTO l_n FROM xmaf_t 
            WHERE xmafent = g_enterprise AND xmafsite = g_site AND xmaf001 = g_xmee_m.xmee004
              AND xmaf002 = l_controlno AND xmaf003 = g_xmeg_d[l_ac].xmeg001 AND xmaf004 = g_xmeg_d[l_ac].xmeg002
         #若訂購料件有設置客戶料件預設條件'(axmi121)時，則需將設置的預設條件值預設到訂購單對應欄位
         IF l_n > 0 THEN  
            LET l_xmegunit = ''                               #出貨據點
            LET g_xmeg_d[l_ac].xmegunit_desc = ''  
            LET g_xmeg_d[l_ac].xmegorga_desc = ''  
            SELECT xmaf005,xmaf006,xmaf007,xmaf008,xmaf009
#                   ,xmaf010,xmaf011  #151001 by whiteny mark
              INTO g_xmeg2_d[l_ac].xmeg003,g_xmeg_d[l_ac].xmeg006,g_xmeg_d[l_ac].xmeg008,g_xmeg_d[l_ac].xmeg010,l_xmegunit
#                   ,g_xmeg2_d[l_ac].xmeg028,g_xmeg2_d[l_ac].xmeg029  #151001 by whiteny mark
              FROM xmaf_t 
             WHERE xmafent = g_enterprise 
               AND xmafsite = g_site 
               AND xmaf001 = g_xmee_m.xmee004
               AND xmaf002 = l_controlno 
               AND xmaf003 = g_xmeg_d[l_ac].xmeg001 
               AND xmaf004 = g_xmeg_d[l_ac].xmeg002
            #單頭多角性質為"3:統銷統收"、"4:統銷自收"時，才依客戶預設條件帶出預設值
            IF g_xmee_m.xmee006 MATCHES '[34]' AND NOT cl_null(l_xmegunit) THEN
               LET g_xmeg_d[l_ac].xmegunit = l_xmegunit
            END IF            
            IF g_xmee_m.xmee006 = '4' THEN
               LET g_xmeg_d[l_ac].xmegorga = g_xmeg_d[l_ac].xmegunit
            END IF           
            CALL axmt510_xmee003_ref(g_xmeg_d[l_ac].xmegunit) RETURNING g_xmeg_d[l_ac].xmegunit_desc
            CALL axmt510_xmee003_ref(g_xmeg_d[l_ac].xmegorga) RETURNING g_xmeg_d[l_ac].xmegorga_desc
         END IF
      END IF
      
      IF cl_null(l_controlno) OR l_n = 0 THEN        
         #沒有設置'客戶控制組料件預設條件改抓料件進銷存檔預設的條件
         SELECT imaf112,imaf015,imaf113,#imaf091,imaf092,  #151001 by whiteny mark
                imaf123
           INTO g_xmeg_d[l_ac].xmeg006,g_xmeg_d[l_ac].xmeg008,g_xmeg_d[l_ac].xmeg010,#g_xmeg2_d[l_ac].xmeg028,g_xmeg2_d[l_ac].xmeg029,  #151001 by whiteny mark
                g_xmeg2_d[l_ac].xmeg003
           FROM imaf_t
         WHERE imafent = g_enterprise AND imafsite = g_site AND imaf001 = g_xmeg_d[l_ac].xmeg001  
      END IF
     
      CALL axmt510_xmeg003_ref(g_xmeg2_d[l_ac].xmeg003) RETURNING  g_xmeg2_d[l_ac].xmeg003_desc
      CALL s_desc_get_unit_desc(g_xmeg_d[l_ac].xmeg006) RETURNING g_xmeg_d[l_ac].xmeg006_desc 
      CALL s_desc_get_unit_desc(g_xmeg_d[l_ac].xmeg008) RETURNING g_xmeg_d[l_ac].xmeg008_desc       
      CALL s_desc_get_unit_desc(g_xmeg_d[l_ac].xmeg010) RETURNING g_xmeg_d[l_ac].xmeg010_desc         
