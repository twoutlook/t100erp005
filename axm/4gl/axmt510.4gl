               END IF
               LET g_xmeg5_d_o.xmef006 = g_xmeg5_d[l_ac].xmef006
            END IF
            
          AFTER FIELD xmef902
             LET g_xmeg5_d[l_ac].xmef902_desc = ''
             DISPLAY BY NAME g_xmeg5_d[l_ac].xmef902_desc
             IF NOT cl_null(g_xmeg5_d[l_ac].xmef902) THEN
                IF NOT axmt510_reason_chk(g_acc2,g_xmeg5_d[l_ac].xmef902) THEN
                   LET g_xmeg5_d[l_ac].xmef902 = g_xmeg5_d_o.xmef902
                   CALL s_desc_get_acc_desc(g_acc2,g_xmeg5_d[l_ac].xmef902) RETURNING g_xmeg5_d[l_ac].xmef902_desc
                   DISPLAY BY NAME g_xmeg5_d[l_ac].xmef902_desc
