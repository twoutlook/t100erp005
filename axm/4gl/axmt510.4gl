# Memo...........:
# Usage..........: CALL axmt510_icac003_control()
#                  RETURNING r_success
# Input parameter: 
#                : 
# Return code....: 
#                : 
# Date & Author..: 160106 By earl
# Modify.........: #150818-00014#1 By earl
################################################################################
PRIVATE FUNCTION axmt510_icac003_control()
   DEFINE r_success      LIKE type_t.num5
   DEFINE l_slip         LIKE oobal_t.oobal002
   DEFINE l_success      LIKE type_t.num5
   DEFINE l_cnt          LIKE type_t.num10

   LET r_success = FALSE  #不控卡

   IF g_aic_doc THEN
      IF NOT cl_null(g_xmee_m.xmeedocno) THEN
         CALL s_aooi200_get_slip(g_xmee_m.xmeedocno) RETURNING l_success,l_slip

         IF l_success THEN
            LET l_cnt = 0
            SELECT COUNT(1) INTO l_cnt
              FROM icab_t,icac_t
             WHERE icabent = icacent AND icacent = g_enterprise
               AND icab001 = icac001
               AND icab002 = icac002
               AND icab003 = g_site
               AND icac003 = l_slip

            IF l_cnt > 0 THEN
               LET r_success = TRUE  #控卡
            END IF
         END IF

      END IF
   END IF
   
   RETURN r_success
END FUNCTION

################################################################################
# Descriptions...: 帶入附屬零件明細檔
# Memo...........:
# Usage..........: CALL axmt510_xmeedocno_xmfw()
#                  RETURNING r_success
# Input parameter: 
# Return code....: r_success  TRUE/FALSE 
# Date & Author..: #161031-00049#1 By 02040
# Modify.........:
################################################################################
PRIVATE FUNCTION axmt510_xmeedocno_xmfw()
DEFINE l_sql             STRING
DEFINE r_success         LIKE type_t.num5

   LET r_success = TRUE

   IF cl_null(g_xmee_m.xmeedocno) THEN
      LET r_success = FALSE
      RETURN r_success
   END IF
   
   LET l_sql = "SELECT xmdqseq,xmdqseq1, ",
               "       xmdq001,xmdq006,xmdq007,xmdq008,xmdq009, ",
               "       xmdq002,xmdq004,xmdq005 ",
               "  FROM xmdq_t ",
               " WHERE xmdqent = ",g_enterprise,
               "   AND xmdqdocno = '",g_xmee_m.xmeedocno,"'", 
               "   AND xmdq012= '4' ",
               " ORDER BY xmdqseq,xmdqseq1 "
               
   PREPARE sel_xmdq_pre FROM l_sql
   DECLARE sel_xmdq_cur CURSOR FOR sel_xmdq_pre               
   LET l_ac = 1 
   FOREACH sel_xmdq_cur INTO g_xmeg6_d[l_ac].xmfwseq,g_xmeg6_d[l_ac].xmfwseq1,
                             g_xmeg6_d[l_ac].xmfw001,g_xmeg6_d[l_ac].xmfw006,g_xmeg6_d[l_ac].xmfw007,g_xmeg6_d[l_ac].xmfw008,g_xmeg6_d[l_ac].xmfw009, 
                             g_xmeg6_d[l_ac].xmfw002,g_xmeg6_d[l_ac].xmfw004,g_xmeg6_d[l_ac].xmfw005
     

      LET g_xmeg6_d[l_ac].xmfw901 = '1'    #單頭修改
      CALL s_desc_get_item_desc(g_xmeg6_d[l_ac].xmfw001) RETURNING g_xmeg6_d[l_ac].xmfw001_desc,g_xmeg6_d[l_ac].xmfw001_desc_1
      CALL s_desc_get_item_desc(g_xmeg6_d[l_ac].xmfw002) RETURNING g_xmeg6_d[l_ac].xmfw002_desc,g_xmeg6_d[l_ac].xmfw002_desc_1
      CALL s_desc_get_unit_desc(g_xmeg6_d[l_ac].xmfw008) RETURNING g_xmeg6_d[l_ac].xmfw008_desc

      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 9035
         LET g_errparam.extend = ''
         LET g_errparam.popup = FALSE
         CALL cl_err()

         EXIT FOREACH
      END IF
        
    END FOREACH
    
    CALL g_xmeg6_d.deleteElement(g_xmeg6_d.getLength())
    LET g_rec_b = l_ac - 1
    LET l_ac = 0
    FREE sel_xmdb_pre 
    
    DISPLAY ARRAY g_xmeg6_d TO s_detail6.* ATTRIBUTES(COUNT=g_rec_b) 
       BEFORE DISPLAY
          EXIT DISPLAY
    END DISPLAY
    
    RETURN r_success    
   
END FUNCTION

