#      CALL s_desc_get_stock_desc(g_site,g_xmeg2_d[l_ac].xmeg028) RETURNING g_xmeg2_d[l_ac].xmeg028_desc
#      CALL s_desc_get_locator_desc(g_site,g_xmeg2_d[l_ac].xmeg028,g_xmeg2_d[l_ac].xmeg029)
#        RETURNING g_xmeg2_d[l_ac].xmeg029_desc
#151001 by whiteny mark end
      
      DISPLAY BY NAME g_xmeg2_d[l_ac].xmeg003,g_xmeg2_d[l_ac].xmeg003_desc,
                      g_xmeg_d[l_ac].xmeg006,g_xmeg_d[l_ac].xmeg006_desc,
                      g_xmeg_d[l_ac].xmeg008,g_xmeg_d[l_ac].xmeg008_desc,
                      g_xmeg_d[l_ac].xmeg010,g_xmeg_d[l_ac].xmeg010_desc,
                      g_xmeg_d[l_ac].xmegunit,g_xmeg_d[l_ac].xmegunit_desc,
                      g_xmeg_d[l_ac].xmegorga,g_xmeg_d[l_ac].xmegorga_desc
