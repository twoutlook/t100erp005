                  IF g_xmeg5_d[l_ac].xmef005 > l_xmef005 THEN
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code = 'adb-00086'
                     LET g_errparam.extend = ''
                     LET g_errparam.popup = TRUE
                     CALL cl_err()
                     LET g_xmeg5_d[l_ac].xmef005 = g_xmeg5_d_o.xmef005
