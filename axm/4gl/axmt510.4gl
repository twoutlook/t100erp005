   LET g_qryparam.state = 'c'
   
   #add-point:cs段開始前 name="cs.before_construct"
                  
   #end add-point 
   
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
      
      #單頭
      CONSTRUCT BY NAME g_wc ON xmeedocno,xmee900,xmee902,xmee001,xmee901,xmeedocdt,xmee002,xmee003, 
          xmeestus,xmeeacti,xmee004,xmee005,xmee006,xmee007,xmee008,xmee033,xmee027,xmee009,xmee010, 
          xmee011,xmee012,xmee013,xmee035,xmee015,xmee016,xmee017,xmee018,xmee019,xmee023,xmee032,xmee071, 
          xmee050,xmee903,xmee904,xmee021,xmee022,xmee034,xmee203,xmee024,xmee048,xmee049,xmee044,xmee030, 
