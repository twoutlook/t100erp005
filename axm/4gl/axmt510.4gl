# Memo...........: 重新取價
# Usage..........: axmt510_change_xmeg015(p_cmd)
#                  
# Input parameter: p_cmd     輸入模式 
#                : 
# Return code....: 
#                : 
# Date & Author..: 2014/07/10 By Polly
# Modify.........:
################################################################################
PRIVATE FUNCTION axmt510_change_xmeg015(p_cmd)
  DEFINE p_cmd             LIKE type_t.chr1
  DEFINE l_i               LIKE type_t.num10
  DEFINE l_success         LIKE type_t.num5
  DEFINE l_xmegseq     LIKE xmeg_t.xmegseq    #項次
  DEFINE l_xmeg001     LIKE xmeg_t.xmeg001    #料件
  DEFINE l_xmeg002     LIKE xmeg_t.xmeg002    #產品特徵
  DEFINE l_xmeg010     LIKE xmeg_t.xmeg010    #單位
  DEFINE l_xmeg011     LIKE xmeg_t.xmeg011    #數量
  DEFINE l_xmeg015     LIKE xmeg_t.xmeg015    #單價
  DEFINE l_xmeg016     LIKE xmeg_t.xmeg016    #稅別
  DEFINE l_xmeg040     LIKE xmeg_t.xmeg040    #取價來源
  DEFINE l_xmeg041     LIKE xmeg_t.xmeg041    #價格參考單號
  DEFINE l_xmeg042     LIKE xmeg_t.xmeg042    #價格參考項次
  DEFINE l_xmeg043     LIKE xmeg_t.xmeg043    #取出價格
  DEFINE l_xmeg046     LIKE xmeg_t.xmeg046    #未稅金額
  DEFINE l_xmeg047     LIKE xmeg_t.xmeg047    #含稅金額
  DEFINE l_xmeg048     LIKE xmeg_t.xmeg048    #稅額
  DEFINE l_xmeg901     LIKE xmeg_t.xmeg901    #變更類型
  DEFINE l_xmeg015_o   LIKE xmeg_t.xmeg015    #單價(舊值)
  DEFINE l_xmeg046_o   LIKE xmeg_t.xmeg046    #未稅金額(舊值)
  DEFINE l_xmeg047_o   LIKE xmeg_t.xmeg047    #含稅金額(舊值)
  DEFINE l_xmeg048_o   LIKE xmeg_t.xmeg048    #稅額(舊值)  
  
  
  #單頭新增時，因單身資料還沒insert到資料庫，所以只改變畫面上的顯示
  IF p_cmd = 'a' THEN  
      FOR l_i = 1 TO g_xmeg_d.getLength()    
          IF g_xmeg_d[l_i].xmeg045 MATCHES '[234]' THEN
             CONTINUE FOR
          END IF          
          LET l_xmeg015_o = g_xmeg_d[l_i].xmeg015 
          LET l_xmeg046_o = g_xmeg_d[l_i].xmeg046
          LET l_xmeg047_o = g_xmeg_d[l_i].xmeg047
          LET l_xmeg048_o = g_xmeg_d[l_i].xmeg048
          #單身重新取價
          CALL s_sale_price_get(g_xmee_m.xmee017,g_xmee_m.xmee004,g_xmeg_d[l_i].xmeg001,g_xmeg_d[l_i].xmeg002,g_xmee_m.xmee015,
                                g_xmeg_d[l_i].xmeg016,g_xmee_m.xmee009,g_xmee_m.xmee010,g_xmee_m.xmee023,g_xmee_m.xmeedocno,
                                g_xmee_m.xmeedocdt,g_xmeg_d[l_i].xmeg010,g_xmeg_d[l_i].xmeg011,g_site,'1',g_xmee_m.xmee048)
             RETURNING g_xmeg2_d[l_i].xmeg040,g_xmeg2_d[l_i].xmeg043,g_xmeg2_d[l_i].xmeg041,g_xmeg2_d[l_i].xmeg042
          LET g_xmeg_d[l_i].xmeg015 = g_xmeg2_d[l_i].xmeg043         
         #重計未稅金額、含稅金額、稅額
         CALL s_axmt510_get_amount(g_xmee_m.xmee900,g_xmee_m.xmeedocno,g_xmeg_d[l_i].xmegseq,g_xmeg_d[l_i].xmeg011,g_xmeg_d[l_i].xmeg015,g_xmeg_d[l_i].xmeg016,g_xmee_m.xmee015,g_xmee_m.xmee016)
           RETURNING l_xmeg046,l_xmeg047,l_xmeg048 
         LET g_xmeg_d[l_i].xmeg046 = l_xmeg046
         LET g_xmeg_d[l_i].xmeg047 = l_xmeg047
         LET g_xmeg_d[l_i].xmeg048 = l_xmeg048       
         #將單價、未稅金額、含稅金額、稅額寫入歷程檔中         
         IF g_xmeg_d[l_i].xmeg015 <> l_xmeg015_o THEN
            LET g_xmeg_d[l_i].xmeg901 = '2'       #單身修改
            CALL s_axmt510_ins_xmek(g_xmeg_d[l_i].xmegseq,0,0,"xmdc015",g_xmeg_d[l_i].xmeg901,l_xmeg015_o,g_xmeg_d[l_i].xmeg015,g_xmee_m.xmeedocno,g_xmee_m.xmee900) 
              RETURNING l_success
         ELSE
            CALL s_axmt510_del_xmek(g_xmeg_d[l_i].xmegseq,0,0,"xmdc015",g_xmee_m.xmeedocno,g_xmee_m.xmee900) 
              RETURNING l_success
         END IF
         IF g_xmeg_d[l_i].xmeg046 <> l_xmeg046_o THEN
            LET g_xmeg_d[l_i].xmeg901 = '2'       #單身修改
            CALL s_axmt510_ins_xmek(g_xmeg_d[l_i].xmegseq,0,0,"xmdc046",g_xmeg_d[l_i].xmeg901,l_xmeg046_o,g_xmeg_d[l_i].xmeg046,g_xmee_m.xmeedocno,g_xmee_m.xmee900) 
              RETURNING l_success
         ELSE
            CALL s_axmt510_del_xmek(g_xmeg_d[l_i].xmegseq,0,0,"xmdc046",g_xmee_m.xmeedocno,g_xmee_m.xmee900) 
              RETURNING l_success         
         END IF
         IF g_xmeg_d[l_i].xmeg047 <> l_xmeg047_o THEN
            LET g_xmeg_d[l_i].xmeg901 = '2'       #單身修改
            CALL s_axmt510_ins_xmek(g_xmeg_d[l_i].xmegseq,0,0,"xmdc047",g_xmeg_d[l_i].xmeg901,l_xmeg047_o,g_xmeg_d[l_i].xmeg047,g_xmee_m.xmeedocno,g_xmee_m.xmee900) 
              RETURNING l_success 
         ELSE
            CALL s_axmt510_del_xmek(g_xmeg_d[l_i].xmegseq,0,0,"xmdc047",g_xmee_m.xmeedocno,g_xmee_m.xmee900) 
              RETURNING l_success               
         END IF
         IF g_xmeg_d[l_i].xmeg048 <> l_xmeg048_o THEN
            LET g_xmeg_d[l_i].xmeg901 = '2'       #單身修改
            CALL s_axmt510_ins_xmek(g_xmeg_d[l_i].xmegseq,0,0,"xmdc048",g_xmeg_d[l_i].xmeg901,l_xmeg048_o,g_xmeg_d[l_i].xmeg048,g_xmee_m.xmeedocno,g_xmee_m.xmee900) 
              RETURNING l_success
         ELSE
            CALL s_axmt510_del_xmek(g_xmeg_d[l_i].xmegseq,0,0,"xmdc048",g_xmee_m.xmeedocno,g_xmee_m.xmee900) 
              RETURNING l_success               
         END IF         
      END FOR  
  ELSE

#160620-00034#1-s
