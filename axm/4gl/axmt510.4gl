            #其他table進行unlock
            CLOSE axmt510_bcl9    #161031-00049#1 axmt510_bcl5 mod axmt510_bcl9
            CALL s_transaction_end('Y','0')  
            
         AFTER INPUT
            #多帳期預付款金額加總不可超過整張訂購單總訂金額(含未稅金額均不能超過)
            LET l_xmef005 = 0
