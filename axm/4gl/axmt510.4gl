      CALL cl_set_act_visible("open_axmt510_03,open_axmt510_01,open_axmt510_05", FALSE) #161031-00049#1 add axmt510_05
   END IF
   
   IF NOT cl_null(g_xmee_m.xmee004) THEN
      LET l_pmaa004 = ''
      SELECT pmaa004 INTO l_pmaa004 FROM pmaa_t WHERE pmaaent = g_enterprise AND pmaa001 = g_xmee_m.xmee004
      IF l_pmaa004 NOT MATCHES '[24]' THEN
         CALL cl_set_act_visible("open_apmi004_01", FALSE)
      END IF
   ELSE
      CALL cl_set_act_visible("open_apmi004_01", FALSE)
   END IF
   
   CALL s_control_chk_group('5','2',g_user,g_dept,g_site,'','','','') RETURNING l_success,l_flag
