      CALL cl_set_act_visible("insert", FALSE)
      CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", FALSE)
      CALL cl_set_act_visible("open_apmi004_01,open_xmef", FALSE)
      CALL cl_set_act_visible("open_axmt510_03,open_axmt510_01,open_axmt510_05", FALSE) #161031-00049#1 add axmt510_05
   END IF
   
   #150721-00001#1  2016/01/08 By earl add e
   
   #end add-point   
END FUNCTION
 
