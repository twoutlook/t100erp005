PRIVATE FUNCTION axmt510_feature()
   DEFINE  l_xmegseq    LIKE xmeg_t.xmegseq
   DEFINE  l_xmeg_d     type_g_xmeg_d
   DEFINE  l_xmeg2_d    type_g_xmeg2_d
   DEFINE  r_success    LIKE type_t.num5
   DEFINE  l_success    LIKE type_t.num5
   DEFINE  l_i          LIKE type_t.num5
   DEFINE  l_pmao004    LIKE pmao_t.pmao004
   
      LET l_xmegseq = ''
      LET r_success = TRUE   
       
 
      INITIALIZE l_xmeg_d.* TO NULL
      INITIALIZE l_xmeg2_d.* TO NULL
             
      SELECT MAX(xmegseq) INTO l_xmegseq
        FROM xmeg_t
       WHERE xmegent = g_enterprise
         AND xmegdocno = g_xmee_m.xmeedocno
         AND xmeg900 = g_xmee_m.xmee900
      IF cl_null(l_xmegseq) THEN
         LET l_xmegseq = 0
      END IF     
      
      FOR l_i = 2 TO g_inam.getLength() 
          LET l_xmeg_d.* = g_xmeg_d[l_ac].*
          LET l_xmeg2_d.* = g_xmeg2_d[l_ac].*
          
          LET l_xmegseq = l_xmegseq + 1
          LET l_xmeg_d.xmegseq = l_xmegseq       
          #220150212--polly--(S)
          LET l_xmeg_d.xmeg001 = g_inam[l_i].inam001
          LET l_xmeg_d.xmeg002 = g_inam[l_i].inam002
          #220150212--polly--(E)           
          LET l_xmeg_d.xmeg024 = 'N'                                        #多交期
          LET l_xmeg_d.xmeg901 = '3'   #單身新增     

          IF cl_null(l_xmeg2_d.xmeg034) THEN LET l_xmeg2_d.xmeg034 = 0 END IF          
          #依交易對象、料號、產品特徵回傳客戶料號
          #161221-00064#16 mod-S
#          CALL s_apmi070_get_pmao004(g_xmee_m.xmee004,l_xmeg_d.xmeg001,l_xmeg_d.xmeg002)
#            RETURNING l_success,l_pmao004
          CALL s_apmi070_get_pmao004_2(g_xmee_m.xmee004,l_xmeg_d.xmeg001,l_xmeg_d.xmeg002,'2')
            RETURNING l_success,l_pmao004
          #161221-00064#16 mod-E
          IF l_success THEN
             LET l_xmeg_d.xmeg027 = l_pmao004
          END IF
          
          LET l_xmeg_d.xmeg007 = g_inam[l_i].inam004          
          #自動推算參考數量
          IF NOT cl_null(l_xmeg_d.xmeg006) AND NOT cl_null(l_xmeg_d.xmeg008) THEN
             LET l_xmeg_d.xmeg009 = 0
             CALL s_aooi250_convert_qty(l_xmeg_d.xmeg001,l_xmeg_d.xmeg006,l_xmeg_d.xmeg008,l_xmeg_d.xmeg007)
               RETURNING l_success,l_xmeg_d.xmeg009
             #參考數量取位
             CALL s_aooi250_take_decimals(l_xmeg_d.xmeg008,l_xmeg_d.xmeg009)
               RETURNING l_success,l_xmeg_d.xmeg009                         
          END IF       
          #自動推算計價數量          
          IF NOT cl_null(l_xmeg_d.xmeg006) AND NOT cl_null(l_xmeg_d.xmeg010) THEN
             LET l_xmeg_d.xmeg011 = 0
             CALL s_aooi250_convert_qty(l_xmeg_d.xmeg001,l_xmeg_d.xmeg006,l_xmeg_d.xmeg010,l_xmeg_d.xmeg007)
               RETURNING l_success,l_xmeg_d.xmeg011
             #對計價數量進行取位
             CALL s_aooi250_take_decimals(l_xmeg_d.xmeg010,l_xmeg_d.xmeg011)
               RETURNING l_success,l_xmeg_d.xmeg011 
          END IF                     
          #取出價格
          CALL s_sale_price_get(g_xmee_m.xmee017,g_xmee_m.xmee004,l_xmeg_d.xmeg001,l_xmeg_d.xmeg002,g_xmee_m.xmee015,
                                l_xmeg_d.xmeg016,g_xmee_m.xmee009,g_xmee_m.xmee010,g_xmee_m.xmee023,g_xmee_m.xmeedocno,
                                g_xmee_m.xmeedocdt,l_xmeg_d.xmeg010,l_xmeg_d.xmeg011,g_site,'1',g_xmee_m.xmee048)
            RETURNING l_xmeg2_d.xmeg040,l_xmeg2_d.xmeg043,l_xmeg2_d.xmeg041,l_xmeg2_d.xmeg042
          IF l_xmeg2_d.xmeg040 <> 'X' THEN      
             LET l_xmeg_d.xmeg015 = l_xmeg2_d.xmeg043                 
          END IF              
          
                        
          
          #取得未稅金額、含稅金額、稅額  
          CALL s_axmt510_get_amount(g_xmee_m.xmee900,g_xmee_m.xmeedocno,l_xmeg_d.xmegseq,l_xmeg_d.xmeg007,l_xmeg_d.xmeg015,l_xmeg_d.xmeg016,g_xmee_m.xmee015,g_xmee_m.xmee016)
            RETURNING l_xmeg_d.xmeg046,l_xmeg_d.xmeg047,l_xmeg_d.xmeg048          

         INSERT INTO xmeg_t
                     (xmegent,xmegdocno,xmeg900,xmegseq,xmegsite,
                      xmeg027,xmeg001,xmeg002,
                      xmeg006,xmeg007,xmeg008,xmeg009,xmeg024,
                      xmeg012,xmeg013,xmeg045,xmeg016,xmeg017,
                      xmeg010,xmeg011,xmeg015,xmeg046,xmeg047,
                      xmeg048,xmeg023,xmeg019,xmeg020,xmeg021,
                      xmeg022,xmegunit,xmegorga,xmeg052,xmeg049,
                      xmeg053,xmeg050,xmeg901,xmeg902,xmeg903,
                      xmeg028,xmeg029,xmeg030,xmeg025,xmeg026,
                      xmeg031,xmeg032,xmeg033,xmeg034,xmeg003,
                      xmeg036,xmeg037,xmeg038,xmeg039,xmeg035,
                      xmeg040,xmeg041,xmeg042,xmeg043,xmeg044)
               VALUES(g_enterprise,g_xmee_m.xmeedocno,g_xmee_m.xmee900,l_xmegseq,g_xmee_m.xmeesite,
                      l_xmeg_d.xmeg027,l_xmeg_d.xmeg001,l_xmeg_d.xmeg002,
                      l_xmeg_d.xmeg006,l_xmeg_d.xmeg007,l_xmeg_d.xmeg008,l_xmeg_d.xmeg009,l_xmeg_d.xmeg024,
                      l_xmeg_d.xmeg012,l_xmeg_d.xmeg013,l_xmeg_d.xmeg045,l_xmeg_d.xmeg016,l_xmeg_d.xmeg017,
                      l_xmeg_d.xmeg010,l_xmeg_d.xmeg011,l_xmeg_d.xmeg015,l_xmeg_d.xmeg046,l_xmeg_d.xmeg047,
                      l_xmeg_d.xmeg048,l_xmeg_d.xmeg023,l_xmeg_d.xmeg019,l_xmeg_d.xmeg020,l_xmeg_d.xmeg021,
                      l_xmeg_d.xmeg022,l_xmeg_d.xmegunit,l_xmeg_d.xmegorga,l_xmeg_d.xmeg052,l_xmeg_d.xmeg049,
                      l_xmeg_d.xmeg053,l_xmeg_d.xmeg050,l_xmeg_d.xmeg901,l_xmeg_d.xmeg902,l_xmeg_d.xmeg903,
                      l_xmeg2_d.xmeg028,l_xmeg2_d.xmeg029,l_xmeg2_d.xmeg030,l_xmeg2_d.xmeg025,l_xmeg2_d.xmeg026,
                      l_xmeg2_d.xmeg031,l_xmeg2_d.xmeg032,l_xmeg2_d.xmeg033,l_xmeg2_d.xmeg034,l_xmeg2_d.xmeg003,
                      l_xmeg2_d.xmeg036,l_xmeg2_d.xmeg037,l_xmeg2_d.xmeg038,l_xmeg2_d.xmeg039,l_xmeg2_d.xmeg035,
                      l_xmeg2_d.xmeg040,l_xmeg2_d.xmeg041,l_xmeg2_d.xmeg042,l_xmeg2_d.xmeg043,l_xmeg2_d.xmeg044)
                      
         IF SQLCA.sqlcode THEN
             INITIALIZE g_errparam TO NULL
             LET g_errparam.code = SQLCA.sqlcode
             LET g_errparam.extend = "xmeg_t"
             LET g_errparam.popup = TRUE
             CALL cl_err()

             LET r_success = FALSE
             EXIT FOR
         END IF
         CALL s_axmt510_gen_xmeh(g_xmee_m.xmeedocno,l_xmegseq,g_xmee_m.xmee900) RETURNING l_success
         IF NOT l_success THEN
             INITIALIZE g_errparam TO NULL
             LET g_errparam.code = SQLCA.sqlcode
             LET g_errparam.extend = "xmeh_t"
             LET g_errparam.popup = TRUE
             CALL cl_err()
             LET r_success = FALSE
             EXIT FOR
         END IF   

         #新增變更歷程檔(產品特微)
         CALL axmt510_xmeg_ins_xmek(l_xmeg_d.*,l_xmeg2_d.*)
              RETURNING r_success
         IF NOT r_success THEN
            EXIT FOR
         END IF  
      END FOR 
      CALL axmt510_b_fill()
      RETURN r_success

END FUNCTION
