  <point name="function.apsq800_fetch_page" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 指定PK後抓取單頭其他資料
# Memo...........:
# Usage..........: CALL apsq800_fetch_page(p_flag)
# Input parameter: p_flag
# Return code....: no
# Date & Author..: 140425 By whitney
# Modify.........:
################################################################################
PRIVATE FUNCTION apsq800_fetch_page(p_flag)
DEFINE p_flag     LIKE type_t.chr1      #處理方式
DEFINE ls_msg     STRING
DEFINE l_wc       STRING
   CASE p_flag
      WHEN 'N' FETCH NEXT     apsq800_page_cs INTO g_psga_m.psga001,g_psga_m.psga002,g_psga_m.psga003
      WHEN 'P' FETCH PREVIOUS apsq800_page_cs INTO g_psga_m.psga001,g_psga_m.psga002,g_psga_m.psga003
      WHEN 'F' FETCH FIRST    apsq800_page_cs INTO g_psga_m.psga001,g_psga_m.psga002,g_psga_m.psga003
      WHEN 'L' FETCH LAST     apsq800_page_cs INTO g_psga_m.psga001,g_psga_m.psga002,g_psga_m.psga003
      WHEN '/'
         IF (NOT g_no_ask) THEN
            CALL cl_set_act_visible("accept,cancel", TRUE)    
            CALL cl_getmsg('fetch',g_lang) RETURNING ls_msg
            LET INT_FLAG = 0
            PROMPT ls_msg CLIPPED,':' FOR g_jump
               #交談指令共用ACTION
               &include "common_action.4gl" 
            END PROMPT
            CALL cl_set_act_visible("accept,cancel", FALSE)    
            IF INT_FLAG THEN
               LET INT_FLAG = 0
               EXIT CASE
            END IF
         END IF
         LET g_no_ask = FALSE
         FETCH ABSOLUTE g_jump apsq800_page_cs INTO g_psga_m.psga001,g_psga_m.psga002,g_psga_m.psga003
   END CASE

   IF SQLCA.sqlcode THEN
      CALL cl_err("psga_t",SQLCA.sqlcode,1)
      INITIALIZE g_psga_m.* TO NULL
      RETURN
   ELSE
      CASE p_flag
         WHEN 'F' LET g_curs_index = 1
         WHEN 'P' LET g_curs_index = g_curs_index - 1
         WHEN 'N' LET g_curs_index = g_curs_index + 1
         WHEN 'L' LET g_curs_index = g_row_count
         WHEN '/' LET g_curs_index = g_jump
      END CASE
      CALL cl_navigator_setting( g_curs_index, g_row_count )
   END IF
   SELECT UNIQUE psga001,psga002,psga003
     INTO g_psga_m.psga001,g_psga_m.psga002,g_psga_m.psga003
     FROM psga_t 
    WHERE psga001 = g_psga_m.psga001
      AND psga002 = g_psga_m.psga002
      AND psga003 = g_psga_m.psga003
      AND psgaent = g_enterprise
   IF SQLCA.sqlcode THEN
      CALL cl_err("psga_t",SQLCA.sqlcode,1)
      INITIALIZE g_psga_m.* TO NULL
      RETURN
   END IF
   LET g_wc3 = "psga001='",g_psga_m.psga001,"' AND psga002='",g_psga_m.psga002,"' AND psga003='",g_psga_m.psga003,"'"
   DISPLAY BY NAME g_psga_m.psga001,g_psga_m.psga002,g_psga_m.psga003
   INITIALIZE g_ref_fields TO NULL 
   LET g_ref_fields[1] = g_psga_m.psga002
   CALL ap_ref_array2(g_ref_fields,"SELECT imaal003 FROM imaal_t WHERE imaalent='"||g_enterprise||"' AND imaal001=? AND imaal002='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_psga_m.imaal003 = '', g_rtn_fields[1] , '' 
   DISPLAY g_psga_m.imaal003 TO imaal003
   CALL ap_ref_array2(g_ref_fields,"SELECT imaal004 FROM imaal_t WHERE imaalent='"||g_enterprise||"' AND imaal001=? AND imaal002='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_psga_m.imaal004 = '', g_rtn_fields[1] , '' 
   DISPLAY g_psga_m.imaal004 TO imaal004
   DISPLAY g_curs_index TO FORMONLY.page
   CALL apsq800_b_fill()
END FUNCTION]]>
  </point>
  <point name="b_fill.sql_after" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter, " AND ", g_wc3
   
   LET g_sql = "SELECT  UNIQUE psga001,psga002,psga003,psga004,psga005,psga006,psga007,psga008,psga009, 
       psga010,psga011,psga012,psga013,psga014,psga015,psga016,psga017,psga018,psga019,psga020,psga021, 
       psga022,psga023,psga024 FROM psga_t",
 
               " LEFT JOIN psgb_t ON psgbent = psgaent AND psga001 = psgb001 AND psga002 = psgb002 AND psga003 = psgb003 AND psga004 = psgb004",
 
 
               "",
               " WHERE psgaent= ? AND 1=1 AND ", ls_wc
    
   LET g_sql = g_sql, " ORDER BY psga_t.psga001,psga_t.psga002,psga_t.psga003,psga_t.psga004"]]>
  </point>
  <point name="b_fill.sql_before" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[]]>
  </point>
  <point name="cs.after_construct" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[   
   LET l_page_sql = "SELECT DISTINCT psga001,psga002,psga003 FROM psga_t",
                    " WHERE psgaent = '",g_enterprise,"' ",
                    "   AND ",g_wc_filter CLIPPED,
                    "   AND ",g_wc CLIPPED,
                    "   AND ",g_wc2 CLIPPED,
                    "   AND ",l_wc CLIPPED,
                    " GROUP BY psga_t.psga001,psga_t.psga002,psga_t.psga003 ",
                    " ORDER BY psga_t.psga001,psga_t.psga002,psga_t.psga003 "
   PREPARE apsq800_page_pb FROM l_page_sql
   DECLARE apsq800_page_cs SCROLL CURSOR WITH HOLD FOR apsq800_page_pb
   LET l_page_sql = "SELECT COUNT(*) FROM (",l_page_sql,")"
   PREPARE apsq800_cnt_pb FROM l_page_sql
   DECLARE apsq800_cnt_cs CURSOR FOR apsq800_cnt_pb
   OPEN apsq800_page_cs
   OPEN apsq800_cnt_cs
   FETCH apsq800_cnt_cs INTO g_row_count
   DISPLAY g_row_count TO FORMONLY.tot_page
   CALL apsq800_fetch_page('F')]]>
  </point>
