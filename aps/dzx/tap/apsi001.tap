  <section id="apsi001.input.body" ver="17" status="" src="s" readonly="">
    <![CDATA[   
      #Page1 預設值產生於此處
      INPUT ARRAY g_psbe_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,WITHOUT DEFAULTS, #MAXCOUNT = g_max_rec,
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            #add-point:資料輸入前
            {<point name="input.body.before_input2"/>}
            #end add-point
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_psbe_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL apsi001_b_fill()
            #如果一直都在單身1則控制筆數位置
            IF g_loc = 'm' AND g_rec_b != 0 THEN
               CALL FGL_SET_ARR_CURR(g_detail_idx)
            END IF
            LET g_loc = 'm'
            LET g_rec_b = g_psbe_d.getLength()
            #add-point:資料輸入前
            {<point name="input.d.before_input"/>}
            #end add-point
         
         BEFORE ROW
            #add-point:modify段before row2
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET l_insert = FALSE
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET g_detail_idx = l_ac
            LET g_current_page = 1
            
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
         
            CALL s_transaction_begin()
            OPEN apsi001_cl USING g_enterprise,g_psba_m.psba001
            IF STATUS THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "OPEN apsi001_cl:" 
               LET g_errparam.code   = STATUS 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CLOSE apsi001_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            
            LET g_rec_b = g_psbe_d.getLength()
            
            IF g_rec_b >= l_ac 
               AND g_psbe_d[l_ac].psbe002 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_psbe_d_t.* = g_psbe_d[l_ac].*  #BACKUP
               LET g_psbe_d_o.* = g_psbe_d[l_ac].*  #BACKUP
               CALL apsi001_set_entry_b(l_cmd)
               #add-point:modify段after_set_entry_b
               {<point name="input.body.after_set_entry_b"/>}
               #end add-point  
               CALL apsi001_set_no_entry_b(l_cmd)
               IF NOT apsi001_lock_b("psbe_t","'1'") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH apsi001_bcl INTO g_psbe_d[l_ac].psbe002,g_psbe_d[l_ac].psbe003
                  IF SQLCA.sqlcode THEN
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = g_psbe_d_t.psbe002 
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     LET l_lock_sw = "Y"
                  END IF
                  
                  #遮罩相關處理
                  LET g_psbe_d_mask_o[l_ac].* =  g_psbe_d[l_ac].*
                  CALL apsi001_psbe_t_mask()
                  LET g_psbe_d_mask_n[l_ac].* =  g_psbe_d[l_ac].*
                  
                  LET g_bfill = "N"
                  CALL apsi001_show()
                  LET g_bfill = "Y"
                  
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
        
         BEFORE INSERT
            
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_psbe_d[l_ac].* TO NULL 
            INITIALIZE g_psbe_d_t.* TO NULL 
            INITIALIZE g_psbe_d_o.* TO NULL 
            #公用欄位給值(單身)
            
            #自定義預設值
            
            #add-point:modify段before備份
            {<point name="input.body.insert.before_bak"/>}
            #end add-point
            LET g_psbe_d_t.* = g_psbe_d[l_ac].*     #新輸入資料
            LET g_psbe_d_o.* = g_psbe_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL apsi001_set_entry_b(l_cmd)
            #add-point:modify段after_set_entry_b
            {<point name="input.body.insert.after_set_entry_b"/>}
            #end add-point
            CALL apsi001_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_psbe_d[li_reproduce_target].* = g_psbe_d[li_reproduce].*
 
               LET g_psbe_d[li_reproduce_target].psbe002 = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            #add-point:單身新增
            {<point name="input.body.b_a_insert"/>}
            #end add-point
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM psbe_t 
             WHERE psbeent = g_enterprise AND psbe001 = g_psba_m.psba001
 
               AND psbe002 = g_psbe_d[l_ac].psbe002
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前
               {<point name="input.body.b_insert"/>}
               #end add-point
            
               #同步新增到同層的table
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_psba_m.psba001
               LET gs_keys[2] = g_psbe_d[g_detail_idx].psbe002
               CALL apsi001_insert_b('psbe_t',gs_keys,"'1'")
                           
               #add-point:單身新增後
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE    
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = 'INSERT' 
               LET g_errparam.code   = "std-00006" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               INITIALIZE g_psbe_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "psbe_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL apsi001_b_fill()
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert2"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               #ERROR 'INSERT O.K'
               LET g_rec_b = g_rec_b + 1
            END IF
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' THEN
               LET l_cmd='d'
               #add-point:單身刪除後(=d)
               {<point name="input.body.after_delete_d"/>}
               #end add-point
            ELSE
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "" 
                  LET g_errparam.code   = -263 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  CANCEL DELETE
               END IF
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete" mark="Y"/>}
               #end add-point 
               
               #取得該筆資料key值
               INITIALIZE gs_keys TO NULL
               LET gs_keys[01] = g_psba_m.psba001
 
               LET gs_keys[gs_keys.getLength()+1] = g_psbe_d_t.psbe002
 
            
               #刪除同層單身
               IF NOT apsi001_delete_b('psbe_t',gs_keys,"'1'") THEN
                  CALL s_transaction_end('N','0')
                  CLOSE apsi001_bcl
                  CANCEL DELETE
               END IF
    
               #刪除下層單身
               IF NOT apsi001_key_delete_b(gs_keys,'psbe_t') THEN
                  CALL s_transaction_end('N','0')
                  CLOSE apsi001_bcl
                  CANCEL DELETE
               END IF
               
               #刪除多語言
               
               
               #add-point:單身刪除中
               {<point name="input.body.m_delete"/>}
               #end add-point 
               
               CALL s_transaction_end('Y','0')
               CLOSE apsi001_bcl
            
               LET g_rec_b = g_rec_b-1
               #add-point:單身刪除後
               {<point name="input.body.a_delete"/>}
               #end add-point
               LET l_count = g_psbe_d.getLength()
               
               #add-point:單身刪除後(<>d)
               {<point name="input.body.after_delete"/>}
               #end add-point
            END IF
 
         AFTER DELETE
            #如果是最後一筆
            IF l_ac = (g_psbe_d.getLength() + 1) THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
            END IF
 
                  #應用 a01 樣板自動產生(Version:1)
         BEFORE FIELD psbe002
            #add-point:BEFORE FIELD psbe002
            {<point name="input.b.page1.psbe002" />}
            #END add-point
 
         #應用 a02 樣板自動產生(Version:1)
         AFTER FIELD psbe002
            
            #add-point:AFTER FIELD psbe002
            {<point name="input.a.page1.psbe002" />}
            #END add-point
            
 
         #應用 a04 樣板自動產生(Version:2)
         ON CHANGE psbe002
            #add-point:ON CHANGE psbe002
            {<point name="input.g.page1.psbe002" />}
            #END add-point 
 
         #應用 a01 樣板自動產生(Version:1)
         BEFORE FIELD psbe003
            #add-point:BEFORE FIELD psbe003
            {<point name="input.b.page1.psbe003" />}
            #END add-point
 
         #應用 a02 樣板自動產生(Version:1)
         AFTER FIELD psbe003
            
            #add-point:AFTER FIELD psbe003
            {<point name="input.a.page1.psbe003" />}
            #END add-point
            
 
         #應用 a04 樣板自動產生(Version:2)
         ON CHANGE psbe003
            #add-point:ON CHANGE psbe003
            {<point name="input.g.page1.psbe003" />}
            #END add-point 
 
 
                  #Ctrlp:input.c.page1.psbe002
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD psbe002
            #add-point:ON ACTION controlp INFIELD psbe002
            {<point name="input.c.page1.psbe002" />}
            #END add-point
 
         #Ctrlp:input.c.page1.psbe003
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD psbe003
            #add-point:ON ACTION controlp INFIELD psbe003
            {<point name="input.c.page1.psbe003" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               LET g_psbe_d[l_ac].* = g_psbe_d_t.*
               CLOSE apsi001_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = g_psbe_d[l_ac].psbe002 
               LET g_errparam.code   = -263 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               LET g_psbe_d[l_ac].* = g_psbe_d_t.*
            ELSE
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               #寫入修改者/修改日期資訊(單身)
               
      
               #將遮罩欄位還原
               CALL apsi001_psbe_t_mask_restore('restore_mask_o')
      
               UPDATE psbe_t SET (psbe001,psbe002,psbe003) = (g_psba_m.psba001,g_psbe_d[l_ac].psbe002, 
                   g_psbe_d[l_ac].psbe003)
                WHERE psbeent = g_enterprise AND psbe001 = g_psba_m.psba001 
 
                  AND psbe002 = g_psbe_d_t.psbe002 #項次   
 
                  
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "psbe_t" 
                     LET g_errparam.code   = "std-00009" 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     CALL s_transaction_end('N','0')
                     LET g_psbe_d[l_ac].* = g_psbe_d_t.*
                  WHEN SQLCA.sqlcode #其他錯誤
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "psbe_t" 
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()                   
                     CALL s_transaction_end('N','0')
                     LET g_psbe_d[l_ac].* = g_psbe_d_t.*  
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_psba_m.psba001
               LET gs_keys_bak[1] = g_psba001_t
               LET gs_keys[2] = g_psbe_d[g_detail_idx].psbe002
               LET gs_keys_bak[2] = g_psbe_d_t.psbe002
               CALL apsi001_update_b('psbe_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
               END CASE
 
               #將遮罩欄位進行遮蔽
               CALL apsi001_psbe_t_mask_restore('restore_mask_n')
               
               #判斷key是否有改變
               INITIALIZE gs_keys TO NULL
               IF NOT(g_psbe_d[g_detail_idx].psbe002 = g_psbe_d_t.psbe002 
 
                  ) THEN
                  LET gs_keys[01] = g_psba_m.psba001
 
                  LET gs_keys[gs_keys.getLength()+1] = g_psbe_d_t.psbe002
 
                  CALL apsi001_key_update_b(gs_keys,'psbe_t')
               END IF
               
               #修改歷程記錄(單身修改)
               LET g_log1 = util.JSON.stringify(g_psba_m),util.JSON.stringify(g_psbe_d_t)
               LET g_log2 = util.JSON.stringify(g_psba_m),util.JSON.stringify(g_psbe_d[l_ac])
               IF NOT cl_log_modified_record_d(g_log1,g_log2) THEN 
                  CALL s_transaction_end('N','0')
               END IF
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            #add-point:單身after_row
            {<point name="input.body.after_row"/>}
            #end add-point
            CALL apsi001_unlock_b("psbe_t","'1'")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock
            #add-point:單身after_row2
            {<point name="input.body.after_row2"/>}
            #end add-point
              
         AFTER INPUT
            #add-point:input段after input 
            {<point name="input.body.after_input"/>}
            #end add-point 
 
         ON ACTION controlo    
            CALL FGL_SET_ARR_CURR(g_psbe_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_psbe_d.getLength()+1
            
         #ON ACTION cancel
         #   LET INT_FLAG = 1
         #   LET g_detail_idx = 1
         #   EXIT DIALOG 
 
