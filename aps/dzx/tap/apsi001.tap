                  (psbeent,
                   psbe001,
                   psbe002
                   ,psbe003) 
            VALUES(g_enterprise,
                   ps_keys[1],ps_keys[2]
                   ,g_psbe_d[g_detail_idx].psbe003)
      #add-point:insert_b段資料新增中
