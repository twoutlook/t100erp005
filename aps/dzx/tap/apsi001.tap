  <section id="apsi001.main" ver="35" status="" src="s" readonly="">
    <![CDATA[#應用 a26 樣板自動產生(Version:4)
#+ 作業開始(主程式類型)
MAIN
   #add-point:main段define
   {<point name="main.define" edit="s"/>}
   #end add-point   
   #add-point:main段define(客製用)
   {<point name="main.define_customerization" edit="c"/>}
   #end add-point   
 
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("aps","")
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT psba001,'','',psbastus,psba002,psba003,psba004,psba005,psba007,'',psba008, 
       psba009,psba010,psba011,psba012,psba006,psba013,psba014,psba015,psba017,psba018,psba019,psba016, 
       psba020,psba021,psba022,psba023,psba024,psba025,psba026,psba027,psba028,psba029,psba030,psbaownid, 
       '',psbaowndp,'',psbacrtid,'',psbacrtdp,'',psbacrtdt,psbamodid,'',psbamoddt", 
                      " FROM psba_t",
                      " WHERE psbaent= ? AND psba001=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE apsi001_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE t0.psba001,t0.psbastus,t0.psba002,t0.psba003,t0.psba004,t0.psba005,t0.psba007, 
       t0.psba008,t0.psba009,t0.psba010,t0.psba011,t0.psba012,t0.psba006,t0.psba013,t0.psba014,t0.psba015, 
       t0.psba017,t0.psba018,t0.psba019,t0.psba016,t0.psba020,t0.psba021,t0.psba022,t0.psba023,t0.psba024, 
       t0.psba025,t0.psba026,t0.psba027,t0.psba028,t0.psba029,t0.psba030,t0.psbaownid,t0.psbaowndp,t0.psbacrtid, 
       t0.psbacrtdp,t0.psbacrtdt,t0.psbamodid,t0.psbamoddt,t1.xmial003 ,t2.ooag011 ,t3.ooefl003 ,t4.ooag011 , 
       t5.ooefl003 ,t6.ooag011",
               " FROM psba_t t0",
                              " LEFT JOIN xmial_t t1 ON t1.xmialent='"||g_enterprise||"' AND t1.xmial001=t0.psba007 AND t1.xmial002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t2 ON t2.ooagent='"||g_enterprise||"' AND t2.ooag001=t0.psbaownid  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent='"||g_enterprise||"' AND t3.ooefl001=t0.psbaowndp AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t4 ON t4.ooagent='"||g_enterprise||"' AND t4.ooag001=t0.psbacrtid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=t0.psbacrtdp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent='"||g_enterprise||"' AND t6.ooag001=t0.psbamodid  ",
 
               " WHERE t0.psbaent = '" ||g_enterprise|| "' AND t0.psba001 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE apsi001_master_referesh FROM g_sql
 
    
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_apsi001 WITH FORM cl_ap_formpath("aps",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL apsi001_init()   
 
      #進入選單 Menu (="N")
      CALL apsi001_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_apsi001
      
   END IF 
   
   CLOSE apsi001_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
END MAIN
 
 
]]>
  </section>
