  <section id="apsi001.fetch" ver="25" status="" src="s" readonly="">
    <![CDATA[#+ 指定PK後抓取單頭其他資料
PRIVATE FUNCTION apsi001_fetch(p_flag)
   DEFINE p_flag     LIKE type_t.chr1
   DEFINE ls_msg     STRING
   #add-point:fetch段define
   {<point name="fetch.define" edit="s"/>}
   #end add-point    
   #add-point:fetch段define(客製用)
   {<point name="fetch.define_customerization" edit="c"/>}
   #end add-point    
   
   IF g_browser_cnt = 0 THEN
      RETURN
   END IF
 
   CALL cl_ap_performance_next_start()
   CASE p_flag
      WHEN 'F' 
         LET g_current_idx = 1
      WHEN 'L'  
         LET g_current_idx = g_browser.getLength()              
      WHEN 'P'
         IF g_current_idx > 1 THEN               
            LET g_current_idx = g_current_idx - 1
         END IF 
      WHEN 'N'
         IF g_current_idx < g_header_cnt THEN
            LET g_current_idx =  g_current_idx + 1
         END IF        
      WHEN '/'
         IF (NOT g_no_ask) THEN    
            CALL cl_set_act_visible("accept,cancel", TRUE)    
            CALL cl_getmsg('fetch',g_lang) RETURNING ls_msg
            LET INT_FLAG = 0
 
            PROMPT ls_msg CLIPPED,':' FOR g_jump
               #交談指令共用ACTION
               &include "common_action.4gl" 
            END PROMPT
 
            CALL cl_set_act_visible("accept,cancel", FALSE)    
            IF INT_FLAG THEN
                LET INT_FLAG = 0
                EXIT CASE  
            END IF           
         END IF
         
         IF g_jump > 0 AND g_jump <= g_browser.getLength() THEN
             LET g_current_idx = g_jump
         END IF
         LET g_no_ask = FALSE  
   END CASE 
 
   CALL g_curr_diag.setCurrentRow("s_browse", g_current_idx) #設定browse 索引
   
   LET g_current_row = g_current_idx
   LET g_detail_cnt = g_header_cnt                  
   
   #單身總筆數顯示
   IF g_detail_cnt > 0 THEN
      #若單身有資料時, idx至少為1
      IF g_detail_idx <= 0 THEN
         LET g_detail_idx = 1
      END IF
      DISPLAY g_detail_idx TO FORMONLY.idx  
   ELSE
      LET g_detail_idx = 0
      DISPLAY '' TO FORMONLY.idx    
   END IF
   
   #瀏覽頁筆數顯示
   LET g_browser_idx = g_pagestart+g_current_idx-1
   DISPLAY g_browser_idx TO FORMONLY.b_index   #當下筆數
   DISPLAY g_browser_idx TO FORMONLY.h_index   #當下筆數
   
   CALL cl_navigator_setting( g_current_idx, g_browser_cnt )
 
   #代表沒有資料
   IF g_current_idx = 0 OR g_browser.getLength() = 0 THEN
      RETURN
   END IF
   
   #超出範圍
   IF g_current_idx > g_browser.getLength() THEN
      LET g_current_idx = g_browser.getLength()
   END IF
   
   LET g_psba_m.psba001 = g_browser[g_current_idx].b_psba001
 
   
   #重讀DB,因TEMP有不被更新特性
   EXECUTE apsi001_master_referesh USING g_psba_m.psba001 INTO g_psba_m.psba001,g_psba_m.psbastus,g_psba_m.psba002, 
       g_psba_m.psba003,g_psba_m.psba004,g_psba_m.psba005,g_psba_m.psba007,g_psba_m.psba008,g_psba_m.psba009, 
       g_psba_m.psba010,g_psba_m.psba011,g_psba_m.psba012,g_psba_m.psba006,g_psba_m.psba013,g_psba_m.psba014, 
       g_psba_m.psba015,g_psba_m.psba017,g_psba_m.psba018,g_psba_m.psba019,g_psba_m.psba016,g_psba_m.psba020, 
       g_psba_m.psba021,g_psba_m.psba022,g_psba_m.psba023,g_psba_m.psba024,g_psba_m.psba025,g_psba_m.psba026, 
       g_psba_m.psba027,g_psba_m.psba028,g_psba_m.psba029,g_psba_m.psba030,g_psba_m.psbaownid,g_psba_m.psbaowndp, 
       g_psba_m.psbacrtid,g_psba_m.psbacrtdp,g_psba_m.psbacrtdt,g_psba_m.psbamodid,g_psba_m.psbamoddt, 
       g_psba_m.psba007_desc,g_psba_m.psbaownid_desc,g_psba_m.psbaowndp_desc,g_psba_m.psbacrtid_desc, 
       g_psba_m.psbacrtdp_desc,g_psba_m.psbamodid_desc
   
   #遮罩相關處理
   LET g_psba_m_mask_o.* =  g_psba_m.*
   CALL apsi001_psba_t_mask()
   LET g_psba_m_mask_n.* =  g_psba_m.*
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL apsi001_set_act_visible()   
   CALL apsi001_set_act_no_visible()
   
   #add-point:fetch段action控制
   {<point name="fetch.action_control"/>}
   #end add-point  
   
   
   
   #add-point:fetch結束前
   {<point name="fetch.after" />}
   #end add-point
   
   #保存單頭舊值
   LET g_psba_m_t.* = g_psba_m.*
   LET g_psba_m_o.* = g_psba_m.*
   
   LET g_data_owner = g_psba_m.psbaownid      
   LET g_data_dept  = g_psba_m.psbaowndp
   
   #重新顯示   
   CALL apsi001_show()
 
   
 
END FUNCTION
]]>
