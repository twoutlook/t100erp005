   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point    
   
   #保存單頭舊值
   LET g_psba_m_t.* = g_psba_m.*
   LET g_psba_m_o.* = g_psba_m.*
   
   IF g_psba_m.psba001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_psba001_t = g_psba_m.psba001
 
   CALL s_transaction_begin()
   
   OPEN apsi001_cl USING g_enterprise,g_psba_m.psba001
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN apsi001_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE apsi001_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE apsi001_master_referesh USING g_psba_m.psba001 INTO g_psba_m.psba001,g_psba_m.psbastus,g_psba_m.psba002, 
       g_psba_m.psba003,g_psba_m.psba004,g_psba_m.psba005,g_psba_m.psba007,g_psba_m.psba008,g_psba_m.psba009, 
       g_psba_m.psba010,g_psba_m.psba011,g_psba_m.psba012,g_psba_m.psba006,g_psba_m.psba013,g_psba_m.psba014, 
       g_psba_m.psba015,g_psba_m.psba017,g_psba_m.psba018,g_psba_m.psba019,g_psba_m.psba016,g_psba_m.psba020, 
       g_psba_m.psba021,g_psba_m.psba022,g_psba_m.psba023,g_psba_m.psba024,g_psba_m.psba025,g_psba_m.psba026, 
       g_psba_m.psba027,g_psba_m.psba028,g_psba_m.psba029,g_psba_m.psba030,g_psba_m.psbaownid,g_psba_m.psbaowndp, 
       g_psba_m.psbacrtid,g_psba_m.psbacrtdp,g_psba_m.psbacrtdt,g_psba_m.psbamodid,g_psba_m.psbamoddt, 
       g_psba_m.psba007_desc,g_psba_m.psbaownid_desc,g_psba_m.psbaowndp_desc,g_psba_m.psbacrtid_desc, 
       g_psba_m.psbacrtdp_desc,g_psba_m.psbamodid_desc
   
   #檢查是否允許此動作
   IF NOT apsi001_action_chk() THEN
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_psba_m_mask_o.* =  g_psba_m.*
   CALL apsi001_psba_t_mask()
   LET g_psba_m_mask_n.* =  g_psba_m.*
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
 
   
   CALL apsi001_show()
   #add-point:modify段show之後
   {<point name="modify.after_show"/>}
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
 
 
    
   WHILE TRUE
      LET g_psba001_t = g_psba_m.psba001
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_psba_m.psbamodid = g_user 
LET g_psba_m.psbamoddt = cl_get_current()
LET g_psba_m.psbamodid_desc = cl_get_username(g_psba_m.psbamodid)
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      CALL apsi001_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE psba_t SET (psbamodid,psbamoddt) = (g_psba_m.psbamodid,g_psba_m.psbamoddt)
          WHERE psbaent = g_enterprise AND psba001 = g_psba001_t
 
      END IF
    
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_psba_m.* = g_psba_m_t.*
         CALL apsi001_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_psba_m.psba001 != g_psba001_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE psbe_t SET psbe001 = g_psba_m.psba001
 
          WHERE psbeent = g_enterprise AND psbe001 = g_psba001_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "psbe_t" 
               LET g_errparam.code   = "std-00009" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "psbe_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL apsi001_set_act_visible()   
   CALL apsi001_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " psbaent = '" ||g_enterprise|| "' AND",
                      " psba001 = '", g_psba_m.psba001, "' "
 
   #填到對應位置
   CALL apsi001_browser_fill("")
 
   CLOSE apsi001_cl
   
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL apsi001_msgcentre_notify('modify')
 
END FUNCTION   
]]>
