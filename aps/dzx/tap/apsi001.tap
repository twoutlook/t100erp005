  <point name="function.apsi001_psba007_ref" order="1" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#指定預測編號說明
PRIVATE FUNCTION apsi001_psba007_ref(p_psba007)
DEFINE p_psba007   LIKE psba_t.psba007
DEFINE r_xmial003  LIKE xmial_t.xmial003

            INITIALIZE g_ref_fields TO NULL
            LET g_ref_fields[1] = g_psba_m.psba007
            CALL ap_ref_array2(g_ref_fields,"SELECT xmial003 FROM xmial_t WHERE xmialent='"||g_enterprise||"' AND xmial001=? AND xmial002='"||g_dlang||"'","") RETURNING g_rtn_fields
            LET r_xmial003 = '', g_rtn_fields[1] , ''
            RETURN r_xmial003
END FUNCTION]]>
  </point>
  <point name="function.apsi001_psbe003_chk" order="2" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#自訂時距檢查
PRIVATE FUNCTION apsi001_psbe003_chk()
DEFINE l_i    LIKE type_t.num5

   IF l_ac > 1 THEN                 #判斷下一期別的期間資料是否正確
      CASE g_psbe_d[l_ac-1].psbe003
         WHEN '1'
            IF g_psbe_d[l_ac].psbe003 != '1' AND g_psbe_d[l_ac].psbe003 != '3' THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 'axm-00123'
               LET g_errparam.extend = ''
               LET g_errparam.popup = TRUE
               CALL cl_err()

               RETURN FALSE
            END IF
         WHEN  '2'
            IF g_psbe_d[l_ac].psbe003 != '2' AND g_psbe_d[l_ac].psbe003 != '3' THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 'axm-00124'
               LET g_errparam.extend = ''
               LET g_errparam.popup = TRUE
               CALL cl_err()

               RETURN FALSE
            END IF
         WHEN '3'
            IF g_psbe_d[l_ac].psbe003 != '3' THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 'axm-00125'
               LET g_errparam.extend = ''
               LET g_errparam.popup = TRUE
               CALL cl_err()

               RETURN FALSE
            END IF
      END CASE
   END IF
   IF l_ac < g_psbe_d.getLength() THEN            #若修改中間期別的期間,要反向管控後面期別的期間資料是否符合
      FOR l_i = l_ac + 1 TO g_psbe_d.getLength()
          IF g_psbe_d[l_ac].psbe003 = '3' THEN
             IF g_psbe_d[l_i].psbe003 = '2' OR g_psbe_d[l_i].psbe003 = '1' THEN
                INITIALIZE g_errparam TO NULL
                LET g_errparam.code = 'axm-00126'
                LET g_errparam.extend = ''
                LET g_errparam.popup = TRUE
                CALL cl_err()

               RETURN FALSE
             END IF
          END IF
          IF g_psbe_d[l_ac].psbe003 = '2' THEN
             IF g_psbe_d[l_i].psbe003 = '1' THEN
                INITIALIZE g_errparam TO NULL
                LET g_errparam.code = 'axm-00127'
                LET g_errparam.extend = ''
                LET g_errparam.popup = TRUE
                CALL cl_err()

               RETURN FALSE
             END IF
          END IF
      END FOR
   END IF
   RETURN TRUE
END FUNCTION]]>
  </point>
