  <section id="apsi001.state_change" ver="15" status="" src="s" readonly="">
    <![CDATA[   #應用 a09 樣板自動產生(Version:12)
#+ 確認碼變更 
PRIVATE FUNCTION apsi001_statechange()
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define
   {<point name="statechange.define" edit="s"/>}
   #end add-point  
   #add-point:statechange段define(客製用)
   {<point name="statechange.define_customerization" edit="c"/>}
   #end add-point  
   
   #add-point:statechange段開始前
   {<point name="statechange.before"/>}
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_psba_m.psba001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN apsi001_cl USING g_enterprise,g_psba_m.psba001
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN apsi001_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE apsi001_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE apsi001_master_referesh USING g_psba_m.psba001 INTO g_psba_m.psba001,g_psba_m.psbastus,g_psba_m.psba002, 
       g_psba_m.psba003,g_psba_m.psba004,g_psba_m.psba005,g_psba_m.psba007,g_psba_m.psba008,g_psba_m.psba009, 
       g_psba_m.psba010,g_psba_m.psba011,g_psba_m.psba012,g_psba_m.psba006,g_psba_m.psba013,g_psba_m.psba014, 
       g_psba_m.psba015,g_psba_m.psba017,g_psba_m.psba018,g_psba_m.psba019,g_psba_m.psba016,g_psba_m.psba020, 
       g_psba_m.psba021,g_psba_m.psba022,g_psba_m.psba023,g_psba_m.psba024,g_psba_m.psba025,g_psba_m.psba026, 
       g_psba_m.psba027,g_psba_m.psba028,g_psba_m.psba029,g_psba_m.psba030,g_psba_m.psbaownid,g_psba_m.psbaowndp, 
       g_psba_m.psbacrtid,g_psba_m.psbacrtdp,g_psba_m.psbacrtdt,g_psba_m.psbamodid,g_psba_m.psbamoddt, 
       g_psba_m.psba007_desc,g_psba_m.psbaownid_desc,g_psba_m.psbaowndp_desc,g_psba_m.psbacrtid_desc, 
       g_psba_m.psbacrtdp_desc,g_psba_m.psbamodid_desc
 
   #檢查是否允許此動作
   IF NOT apsi001_action_chk() THEN
      CLOSE apsi001_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_psba_m.psba001,g_psba_m.psbal003,g_psba_m.psbal004,g_psba_m.psbastus,g_psba_m.psba002, 
       g_psba_m.psba003,g_psba_m.psba004,g_psba_m.psba005,g_psba_m.psba007,g_psba_m.psba007_desc,g_psba_m.psba008, 
       g_psba_m.psba009,g_psba_m.psba010,g_psba_m.psba011,g_psba_m.psba012,g_psba_m.psba006,g_psba_m.psba013, 
       g_psba_m.psba014,g_psba_m.psba015,g_psba_m.psba017,g_psba_m.psba018,g_psba_m.psba019,g_psba_m.psba016, 
       g_psba_m.psba020,g_psba_m.psba021,g_psba_m.psba022,g_psba_m.psba023,g_psba_m.psba024,g_psba_m.psba025, 
       g_psba_m.psba026,g_psba_m.psba027,g_psba_m.psba028,g_psba_m.psba029,g_psba_m.psba030,g_psba_m.psbaownid, 
       g_psba_m.psbaownid_desc,g_psba_m.psbaowndp,g_psba_m.psbaowndp_desc,g_psba_m.psbacrtid,g_psba_m.psbacrtid_desc, 
       g_psba_m.psbacrtdp,g_psba_m.psbacrtdp_desc,g_psba_m.psbacrtdt,g_psba_m.psbamodid,g_psba_m.psbamodid_desc, 
       g_psba_m.psbamoddt
 
   CASE g_psba_m.psbastus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
      
   END CASE
 
   #add-point:資料刷新後
   {<point name="statechange.after_refresh"/>}
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_psba_m.psbastus
            
            WHEN "N"
               HIDE OPTION "inactive"
            WHEN "Y"
               HIDE OPTION "active"
         END CASE
     
      #add-point:menu前
      {<point name="statechange.before_menu"/>}
      #end add-point
      
      
	  
      ON ACTION inactive
         IF cl_auth_chk_act("inactive") THEN
            LET lc_state = "N"
            #add-point:action控制
            {<point name="statechange.inactive"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION active
         IF cl_auth_chk_act("active") THEN
            LET lc_state = "Y"
            #add-point:action控制
            {<point name="statechange.active"/>}
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制
      {<point name="statechange.more_control"/>}
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      ) OR 
      g_psba_m.psbastus = lc_state OR cl_null(lc_state) THEN
      CLOSE apsi001_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前
   {<point name="statechange.b_update"/>}
   #end add-point
   
   LET g_psba_m.psbamodid = g_user
   LET g_psba_m.psbamoddt = cl_get_current()
   LET g_psba_m.psbastus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE psba_t 
      SET (psbastus,psbamodid,psbamoddt) 
        = (g_psba_m.psbastus,g_psba_m.psbamodid,g_psba_m.psbamoddt)     
    WHERE psbaent = g_enterprise AND psba001 = g_psba_m.psba001
 
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE apsi001_master_referesh USING g_psba_m.psba001 INTO g_psba_m.psba001,g_psba_m.psbastus, 
          g_psba_m.psba002,g_psba_m.psba003,g_psba_m.psba004,g_psba_m.psba005,g_psba_m.psba007,g_psba_m.psba008, 
          g_psba_m.psba009,g_psba_m.psba010,g_psba_m.psba011,g_psba_m.psba012,g_psba_m.psba006,g_psba_m.psba013, 
          g_psba_m.psba014,g_psba_m.psba015,g_psba_m.psba017,g_psba_m.psba018,g_psba_m.psba019,g_psba_m.psba016, 
          g_psba_m.psba020,g_psba_m.psba021,g_psba_m.psba022,g_psba_m.psba023,g_psba_m.psba024,g_psba_m.psba025, 
          g_psba_m.psba026,g_psba_m.psba027,g_psba_m.psba028,g_psba_m.psba029,g_psba_m.psba030,g_psba_m.psbaownid, 
          g_psba_m.psbaowndp,g_psba_m.psbacrtid,g_psba_m.psbacrtdp,g_psba_m.psbacrtdt,g_psba_m.psbamodid, 
          g_psba_m.psbamoddt,g_psba_m.psba007_desc,g_psba_m.psbaownid_desc,g_psba_m.psbaowndp_desc,g_psba_m.psbacrtid_desc, 
          g_psba_m.psbacrtdp_desc,g_psba_m.psbamodid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_psba_m.psba001,g_psba_m.psbal003,g_psba_m.psbal004,g_psba_m.psbastus,g_psba_m.psba002, 
          g_psba_m.psba003,g_psba_m.psba004,g_psba_m.psba005,g_psba_m.psba007,g_psba_m.psba007_desc, 
          g_psba_m.psba008,g_psba_m.psba009,g_psba_m.psba010,g_psba_m.psba011,g_psba_m.psba012,g_psba_m.psba006, 
          g_psba_m.psba013,g_psba_m.psba014,g_psba_m.psba015,g_psba_m.psba017,g_psba_m.psba018,g_psba_m.psba019, 
          g_psba_m.psba016,g_psba_m.psba020,g_psba_m.psba021,g_psba_m.psba022,g_psba_m.psba023,g_psba_m.psba024, 
          g_psba_m.psba025,g_psba_m.psba026,g_psba_m.psba027,g_psba_m.psba028,g_psba_m.psba029,g_psba_m.psba030, 
          g_psba_m.psbaownid,g_psba_m.psbaownid_desc,g_psba_m.psbaowndp,g_psba_m.psbaowndp_desc,g_psba_m.psbacrtid, 
          g_psba_m.psbacrtid_desc,g_psba_m.psbacrtdp,g_psba_m.psbacrtdp_desc,g_psba_m.psbacrtdt,g_psba_m.psbamodid, 
          g_psba_m.psbamodid_desc,g_psba_m.psbamoddt
   END IF
 
   #add-point:stus修改後
   {<point name="statechange.a_update"/>}
   #end add-point
 
   #add-point:statechange段結束前
   {<point name="statechange.after"/>}
   #end add-point  
 
   CLOSE apsi001_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL apsi001_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION
 
 
]]>
