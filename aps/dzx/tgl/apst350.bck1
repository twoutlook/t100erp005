{<section id="apst350.default_search" >}
#+ 外部參數搜尋
PRIVATE FUNCTION apst350_default_search()
   #add-point:default_search段define(客製用) name="default_search.define_customerization"
   {<point name="default_search.define_customerization" edit="c" mark="Y"/>}
   #end add-point  
   DEFINE li_idx     LIKE type_t.num10
   DEFINE li_cnt     LIKE type_t.num10
   DEFINE ls_wc      STRING
   DEFINE la_wc      DYNAMIC ARRAY OF RECORD
          tableid    STRING,
          wc         STRING
          END RECORD
   DEFINE ls_where   STRING
   #add-point:default_search段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="default_search.define"
   {<point name="default_search.define"/>} 
   #end add-point  
   
   #add-point:Function前置處理 name="default_search.before"
   {<point name="default_search.before"/>}
   #end add-point  
   
   LET g_pagestart = 1
   
   IF cl_null(g_order) THEN
      LET g_order = "ASC"
   END IF
   
   IF NOT cl_null(g_argv[01]) THEN
      LET ls_wc = ls_wc, " psiasite = '", g_argv[01], "' AND "
   END IF
   
   IF NOT cl_null(g_argv[02]) THEN
      LET ls_wc = ls_wc, " psia001 = '", g_argv[02], "' AND "
   END IF
   IF NOT cl_null(g_argv[03]) THEN
      LET ls_wc = ls_wc, " psia002 = '", g_argv[03], "' AND "
   END IF
   IF NOT cl_null(g_argv[04]) THEN
      LET ls_wc = ls_wc, " psia003 = '", g_argv[04], "' AND "
   END IF
   IF NOT cl_null(g_argv[05]) THEN
      LET ls_wc = ls_wc, " psia004 = '", g_argv[05], "' AND "
   END IF
   IF NOT cl_null(g_argv[06]) THEN
      LET ls_wc = ls_wc, " psia005 = '", g_argv[06], "' AND "
   END IF
 
   
   #add-point:default_search段after sql name="default_search.after_sql"
   {<point name="default_search.after_sql"/>}
   #end add-point  
   
   IF NOT cl_null(ls_wc) THEN
      LET g_wc = ls_wc.subString(1,ls_wc.getLength()-5)
      LET g_default = TRUE
   ELSE
      #若無外部參數則預設為1=2
      LET g_default = FALSE
      
      #預設查詢條件
      CALL cl_qbe_get_default_qryplan() RETURNING ls_where
      IF NOT cl_null(ls_where) THEN
         CALL util.JSON.parse(ls_where, la_wc)
         INITIALIZE g_wc, g_wc2,g_wc2_table1,g_wc2_extend TO NULL
 
         FOR li_idx = 1 TO la_wc.getLength()
            CASE
               WHEN la_wc[li_idx].tableid = "psia_t" 
                  LET g_wc = la_wc[li_idx].wc
               WHEN la_wc[li_idx].tableid = "psib_t" 
                  LET g_wc2_table1 = la_wc[li_idx].wc
 
               WHEN la_wc[li_idx].tableid = "EXTENDWC"
                  LET g_wc2_extend = la_wc[li_idx].wc
            END CASE
         END FOR
         IF NOT cl_null(g_wc) OR NOT cl_null(g_wc2_table1) 
 
            OR NOT cl_null(g_wc2_extend)
            THEN
            #組合g_wc2
            IF g_wc2_table1 <> " 1=1" AND NOT cl_null(g_wc2_table1) THEN
               LET g_wc2 = g_wc2_table1
            END IF
 
            IF g_wc2_extend <> " 1=1" AND NOT cl_null(g_wc2_extend) THEN
               LET g_wc2 = g_wc2 ," AND ", g_wc2_extend
            END IF
         
            IF g_wc2.subString(1,5) = " AND " THEN
               LET g_wc2 = g_wc2.subString(6,g_wc2.getLength())
            END IF
         END IF
      END IF
    
      IF cl_null(g_wc) AND cl_null(g_wc2) THEN
         LET g_wc = " 1=2"
      END IF
   END IF
   
   #add-point:default_search段結束前 name="default_search.after"
   {<point name="default_search.after"/>}
   #end add-point  
 
   IF g_wc.getIndexOf(" 1=2", 1) THEN
      LET g_default = TRUE
   END IF
 
 
END FUNCTION

{</section>}

{<section id="apst350.state_change" >}
   #應用 a09 樣板自動產生(Version:17)
#+ 確認碼變更 
PRIVATE FUNCTION apst350_statechange()
   #add-point:statechange段define(客製用) name="statechange.define_customerization"
   {<point name="statechange.define_customerization" edit="c" mark="Y"/>}
   #end add-point  
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="statechange.define"
   {<point name="statechange.define"/>}
   #end add-point  
   
   #add-point:Function前置處理 name="statechange.before"
   {<point name="statechange.before"/>}
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_psia_m.psiasite IS NULL
      OR g_psia_m.psia001 IS NULL      OR g_psia_m.psia002 IS NULL      OR g_psia_m.psia003 IS NULL      OR g_psia_m.psia004 IS NULL      OR g_psia_m.psia005 IS NULL
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN apst350_cl USING g_enterprise,g_psia_m.psiasite,g_psia_m.psia001,g_psia_m.psia002,g_psia_m.psia003,g_psia_m.psia004,g_psia_m.psia005
   IF STATUS THEN
      CLOSE apst350_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN apst350_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE apst350_master_referesh USING g_psia_m.psiasite,g_psia_m.psia001,g_psia_m.psia002,g_psia_m.psia003, 
       g_psia_m.psia004,g_psia_m.psia005 INTO g_psia_m.psia001,g_psia_m.psia002,g_psia_m.psia003,g_psia_m.psia004, 
       g_psia_m.psia005,g_psia_m.psiasite,g_psia_m.psiastus,g_psia_m.psiaownid,g_psia_m.psiaowndp,g_psia_m.psiacrtid, 
       g_psia_m.psiacrtdp,g_psia_m.psiacrtdt,g_psia_m.psiamodid,g_psia_m.psiamoddt,g_psia_m.psiacnfid, 
       g_psia_m.psiacnfdt,g_psia_m.psia001_desc,g_psia_m.psia003_desc,g_psia_m.psia004_desc,g_psia_m.psiaownid_desc, 
       g_psia_m.psiaowndp_desc,g_psia_m.psiacrtid_desc,g_psia_m.psiacrtdp_desc,g_psia_m.psiamodid_desc, 
       g_psia_m.psiacnfid_desc
   
 
   #檢查是否允許此動作
   IF NOT apst350_action_chk() THEN
      CLOSE apst350_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_psia_m.psia001,g_psia_m.psia001_desc,g_psia_m.psia002,g_psia_m.psia003,g_psia_m.psia003_desc, 
       g_psia_m.psia004,g_psia_m.psia004_desc,g_psia_m.psia005,g_psia_m.psiasite,g_psia_m.psiastus,g_psia_m.psiaownid, 
       g_psia_m.psiaownid_desc,g_psia_m.psiaowndp,g_psia_m.psiaowndp_desc,g_psia_m.psiacrtid,g_psia_m.psiacrtid_desc, 
       g_psia_m.psiacrtdp,g_psia_m.psiacrtdp_desc,g_psia_m.psiacrtdt,g_psia_m.psiamodid,g_psia_m.psiamodid_desc, 
       g_psia_m.psiamoddt,g_psia_m.psiacnfid,g_psia_m.psiacnfid_desc,g_psia_m.psiacnfdt
 
   CASE g_psia_m.psiastus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
      WHEN "X"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   {<point name="statechange.after_refresh"/>}
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_psia_m.psiastus
            
            WHEN "N"
               HIDE OPTION "unconfirmed"
            WHEN "Y"
               HIDE OPTION "confirmed"
            WHEN "X"
               HIDE OPTION "invalid"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      {<point name="statechange.before_menu"/>}
      #end add-point
      
      
	  
      ON ACTION unconfirmed
         IF cl_auth_chk_act("unconfirmed") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.unconfirmed"
            {<point name="statechange.unconfirmed"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION confirmed
         IF cl_auth_chk_act("confirmed") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.confirmed"
            {<point name="statechange.confirmed"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION invalid
         IF cl_auth_chk_act("invalid") THEN
            LET lc_state = "X"
            #add-point:action控制 name="statechange.invalid"
            {<point name="statechange.invalid"/>}
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      {<point name="statechange.more_control"/>}
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      AND lc_state <> "X"
      ) OR 
      g_psia_m.psiastus = lc_state OR cl_null(lc_state) THEN
      CLOSE apst350_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   {<point name="statechange.b_update"/>}
   #end add-point
   
   LET g_psia_m.psiamodid = g_user
   LET g_psia_m.psiamoddt = cl_get_current()
   LET g_psia_m.psiastus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE psia_t 
      SET (psiastus,psiamodid,psiamoddt) 
        = (g_psia_m.psiastus,g_psia_m.psiamodid,g_psia_m.psiamoddt)     
    WHERE psiaent = g_enterprise AND psiasite = g_psia_m.psiasite
      AND psia001 = g_psia_m.psia001      AND psia002 = g_psia_m.psia002      AND psia003 = g_psia_m.psia003      AND psia004 = g_psia_m.psia004      AND psia005 = g_psia_m.psia005
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE apst350_master_referesh USING g_psia_m.psiasite,g_psia_m.psia001,g_psia_m.psia002,g_psia_m.psia003, 
          g_psia_m.psia004,g_psia_m.psia005 INTO g_psia_m.psia001,g_psia_m.psia002,g_psia_m.psia003, 
          g_psia_m.psia004,g_psia_m.psia005,g_psia_m.psiasite,g_psia_m.psiastus,g_psia_m.psiaownid,g_psia_m.psiaowndp, 
          g_psia_m.psiacrtid,g_psia_m.psiacrtdp,g_psia_m.psiacrtdt,g_psia_m.psiamodid,g_psia_m.psiamoddt, 
          g_psia_m.psiacnfid,g_psia_m.psiacnfdt,g_psia_m.psia001_desc,g_psia_m.psia003_desc,g_psia_m.psia004_desc, 
          g_psia_m.psiaownid_desc,g_psia_m.psiaowndp_desc,g_psia_m.psiacrtid_desc,g_psia_m.psiacrtdp_desc, 
          g_psia_m.psiamodid_desc,g_psia_m.psiacnfid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_psia_m.psia001,g_psia_m.psia001_desc,g_psia_m.psia002,g_psia_m.psia003,g_psia_m.psia003_desc, 
          g_psia_m.psia004,g_psia_m.psia004_desc,g_psia_m.psia005,g_psia_m.psiasite,g_psia_m.psiastus, 
          g_psia_m.psiaownid,g_psia_m.psiaownid_desc,g_psia_m.psiaowndp,g_psia_m.psiaowndp_desc,g_psia_m.psiacrtid, 
          g_psia_m.psiacrtid_desc,g_psia_m.psiacrtdp,g_psia_m.psiacrtdp_desc,g_psia_m.psiacrtdt,g_psia_m.psiamodid, 
          g_psia_m.psiamodid_desc,g_psia_m.psiamoddt,g_psia_m.psiacnfid,g_psia_m.psiacnfid_desc,g_psia_m.psiacnfdt 

   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
   {<point name="statechange.a_update"/>}
   #end add-point
 
   #add-point:statechange段結束前 name="statechange.after"
   {<point name="statechange.after"/>}
   #end add-point  
 
   CLOSE apst350_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL apst350_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION
 
 
 

{</section>}

