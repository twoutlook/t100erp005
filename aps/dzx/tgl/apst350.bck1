         ON ROW CHANGE
            IF INT_FLAG THEN
               LET INT_FLAG = 0
               LET g_psib_d[l_ac].* = g_psib_d_t.*
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CLOSE apst350_bcl
               CALL s_transaction_end('N','0')
               CALL cl_err()
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = g_psib_d[l_ac].psib006 
               LET g_errparam.code   = -263 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               LET g_psib_d[l_ac].* = g_psib_d_t.*
            ELSE
            
               #add-point:單身修改前 name="input.body.b_update"
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               #寫入修改者/修改日期資訊(單身)
               
      
               #將遮罩欄位還原
               CALL apst350_psib_t_mask_restore('restore_mask_o')
      
               UPDATE psib_t SET (psibsite,psib001,psib002,psib003,psib004,psib005,psib006,psib007,psib012, 
                   psib013,psib009,psib010,psib008,psib011) = (g_psia_m.psiasite,g_psia_m.psia001,g_psia_m.psia002, 
                   g_psia_m.psia003,g_psia_m.psia004,g_psia_m.psia005,g_psib_d[l_ac].psib006,g_psib_d[l_ac].psib007, 
                   g_psib_d[l_ac].psib012,g_psib_d[l_ac].psib013,g_psib_d[l_ac].psib009,g_psib_d[l_ac].psib010, 
                   g_psib_d[l_ac].psib008,g_psib_d[l_ac].psib011)
                WHERE psibent = g_enterprise AND psibsite = g_psia_m.psiasite 
                  AND psib001 = g_psia_m.psia001 
                  AND psib002 = g_psia_m.psia002 
                  AND psib003 = g_psia_m.psia003 
                  AND psib004 = g_psia_m.psia004 
                  AND psib005 = g_psia_m.psia005 
 
                  AND psib006 = g_psib_d_t.psib006 #項次   
                  AND psib007 = g_psib_d_t.psib007  
                  AND psib008 = g_psib_d_t.psib008  
                  AND psib013 = g_psib_d_t.psib013  
 
                  
               #add-point:單身修改中 name="input.body.m_update"
               {<point name="input.body.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     LET g_psib_d[l_ac].* = g_psib_d_t.*
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "psib_t" 
                     LET g_errparam.code   = "std-00009" 
                     LET g_errparam.popup  = TRUE 
                     CALL s_transaction_end('N','0')
                     CALL cl_err()
                     
                  WHEN SQLCA.sqlcode #其他錯誤
                     LET g_psib_d[l_ac].* = g_psib_d_t.*  
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "psib_t:",SQLERRMESSAGE 
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL s_transaction_end('N','0')
                     CALL cl_err()                   
                     
                  OTHERWISE
                     #資料多語言用-增/改
                     
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_psia_m.psiasite
               LET gs_keys_bak[1] = g_psiasite_t
               LET gs_keys[2] = g_psia_m.psia001
               LET gs_keys_bak[2] = g_psia001_t
               LET gs_keys[3] = g_psia_m.psia002
               LET gs_keys_bak[3] = g_psia002_t
               LET gs_keys[4] = g_psia_m.psia003
               LET gs_keys_bak[4] = g_psia003_t
               LET gs_keys[5] = g_psia_m.psia004
               LET gs_keys_bak[5] = g_psia004_t
               LET gs_keys[6] = g_psia_m.psia005
               LET gs_keys_bak[6] = g_psia005_t
               LET gs_keys[7] = g_psib_d[g_detail_idx].psib006
               LET gs_keys_bak[7] = g_psib_d_t.psib006
               LET gs_keys[8] = g_psib_d[g_detail_idx].psib007
               LET gs_keys_bak[8] = g_psib_d_t.psib007
               LET gs_keys[9] = g_psib_d[g_detail_idx].psib008
               LET gs_keys_bak[9] = g_psib_d_t.psib008
               LET gs_keys[10] = g_psib_d[g_detail_idx].psib013
               LET gs_keys_bak[10] = g_psib_d_t.psib013
               CALL apst350_update_b('psib_t',gs_keys,gs_keys_bak,"'1'")
               END CASE
 
               #將遮罩欄位進行遮蔽
               CALL apst350_psib_t_mask_restore('restore_mask_n')
               
               #判斷key是否有改變
               INITIALIZE gs_keys TO NULL
               IF NOT(g_psib_d[g_detail_idx].psib006 = g_psib_d_t.psib006 
                  AND g_psib_d[g_detail_idx].psib007 = g_psib_d_t.psib007 
                  AND g_psib_d[g_detail_idx].psib008 = g_psib_d_t.psib008 
                  AND g_psib_d[g_detail_idx].psib013 = g_psib_d_t.psib013 
 
                  ) THEN
                  LET gs_keys[01] = g_psia_m.psiasite
                  LET gs_keys[gs_keys.getLength()+1] = g_psia_m.psia001
                  LET gs_keys[gs_keys.getLength()+1] = g_psia_m.psia002
                  LET gs_keys[gs_keys.getLength()+1] = g_psia_m.psia003
                  LET gs_keys[gs_keys.getLength()+1] = g_psia_m.psia004
                  LET gs_keys[gs_keys.getLength()+1] = g_psia_m.psia005
 
                  LET gs_keys[gs_keys.getLength()+1] = g_psib_d_t.psib006
                  LET gs_keys[gs_keys.getLength()+1] = g_psib_d_t.psib007
                  LET gs_keys[gs_keys.getLength()+1] = g_psib_d_t.psib008
                  LET gs_keys[gs_keys.getLength()+1] = g_psib_d_t.psib013
 
                  CALL apst350_key_update_b(gs_keys,'psib_t')
               END IF
               
               #修改歷程記錄(單身修改)
               LET g_log1 = util.JSON.stringify(g_psia_m),util.JSON.stringify(g_psib_d_t)
               LET g_log2 = util.JSON.stringify(g_psia_m),util.JSON.stringify(g_psib_d[l_ac])
               IF NOT cl_log_modified_record_d(g_log1,g_log2) THEN 
                  CALL s_transaction_end('N','0')
               END IF
               
               #add-point:單身修改後 name="input.body.a_update"
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            #add-point:單身after_row name="input.body.after_row"
            {<point name="input.body.after_row"/>}
            #end add-point
            CALL apst350_unlock_b("psib_t","'1'")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock
            #add-point:單身after_row2 name="input.body.after_row2"
            {<point name="input.body.after_row2"/>}
            #end add-point
              
         AFTER INPUT
            #add-point:input段after input  name="input.body.after_input"
