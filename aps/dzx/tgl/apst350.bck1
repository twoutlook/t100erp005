       g_psia_m.psiacrtdp,g_psia_m.psiacrtdt,g_psia_m.psiamodid,g_psia_m.psiamoddt,g_psia_m.psiacnfid, 
       g_psia_m.psiacnfdt,g_psia_m.psia001_desc,g_psia_m.psia003_desc,g_psia_m.psia004_desc,g_psia_m.psiaownid_desc, 
       g_psia_m.psiaowndp_desc,g_psia_m.psiacrtid_desc,g_psia_m.psiacrtdp_desc,g_psia_m.psiamodid_desc, 
       g_psia_m.psiacnfid_desc
   
