   {<point name="main.define"/>}
   #end add-point   
   
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("aps","")
 
   #add-point:作業初始化 name="main.init"
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define name="main.define_sql"
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT psia001,'',psia002,psia003,'',psia004,'',psia005,psiasite,psiastus,psiaownid, 
       '',psiaowndp,'',psiacrtid,'',psiacrtdp,'',psiacrtdt,psiamodid,'',psiamoddt,psiacnfid,'',psiacnfdt", 
        
                      " FROM psia_t",
                      " WHERE psiaent= ? AND psiasite=? AND psia001=? AND psia002=? AND psia003=? AND  
                          psia004=? AND psia005=? FOR UPDATE"
   #add-point:SQL_define name="main.after_define_sql"
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE apst350_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.psia001,t0.psia002,t0.psia003,t0.psia004,t0.psia005,t0.psiasite, 
       t0.psiastus,t0.psiaownid,t0.psiaowndp,t0.psiacrtid,t0.psiacrtdp,t0.psiacrtdt,t0.psiamodid,t0.psiamoddt, 
       t0.psiacnfid,t0.psiacnfdt,t1.psjal003 ,t2.pmaal004 ,t3.ooag011 ,t4.ooag011 ,t5.ooefl003 ,t6.ooag011 , 
       t7.ooefl003 ,t8.ooag011 ,t9.ooag011",
               " FROM psia_t t0",
                              " LEFT JOIN psjal_t t1 ON t1.psjalent="||g_enterprise||" AND t1.psjal001=t0.psia001 AND t1.psjal002='"||g_dlang||"' ",
               " LEFT JOIN pmaal_t t2 ON t2.pmaalent="||g_enterprise||" AND t2.pmaal001=t0.psia003 AND t2.pmaal002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent="||g_enterprise||" AND t3.ooag001=t0.psia004  ",
               " LEFT JOIN ooag_t t4 ON t4.ooagent="||g_enterprise||" AND t4.ooag001=t0.psiaownid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent="||g_enterprise||" AND t5.ooefl001=t0.psiaowndp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent="||g_enterprise||" AND t6.ooag001=t0.psiacrtid  ",
               " LEFT JOIN ooefl_t t7 ON t7.ooeflent="||g_enterprise||" AND t7.ooefl001=t0.psiacrtdp AND t7.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t8 ON t8.ooagent="||g_enterprise||" AND t8.ooag001=t0.psiamodid  ",
               " LEFT JOIN ooag_t t9 ON t9.ooagent="||g_enterprise||" AND t9.ooag001=t0.psiacnfid  ",
 
               " WHERE t0.psiaent = " ||g_enterprise|| " AND t0.psiasite = ? AND t0.psia001 = ? AND t0.psia002 = ? AND t0.psia003 = ? AND t0.psia004 = ? AND t0.psia005 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define name="main.after_refresh_sql"
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE apst350_master_referesh FROM g_sql
 
    
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call name="main.servicecall"
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_apst350 WITH FORM cl_ap_formpath("aps",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL apst350_init()   
 
      #進入選單 Menu (="N")
