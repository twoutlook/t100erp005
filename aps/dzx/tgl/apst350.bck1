   #顯示最新的資料
   EXECUTE apst350_master_referesh USING g_psia_m.psiasite,g_psia_m.psia001,g_psia_m.psia002,g_psia_m.psia003, 
       g_psia_m.psia004,g_psia_m.psia005 INTO g_psia_m.psia001,g_psia_m.psia002,g_psia_m.psia003,g_psia_m.psia004, 
