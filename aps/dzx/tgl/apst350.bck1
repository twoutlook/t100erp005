   #end add-point    
   
   #add-point:Function前置處理  name="modify.pre_function"
   {<point name="modify.pre_function"/>}
   #end add-point
   
   #保存單頭舊值
   LET g_psia_m_t.* = g_psia_m.*
   LET g_psia_m_o.* = g_psia_m.*
   
   IF g_psia_m.psiasite IS NULL
   OR g_psia_m.psia001 IS NULL
   OR g_psia_m.psia002 IS NULL
   OR g_psia_m.psia003 IS NULL
   OR g_psia_m.psia004 IS NULL
   OR g_psia_m.psia005 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_psiasite_t = g_psia_m.psiasite
   LET g_psia001_t = g_psia_m.psia001
   LET g_psia002_t = g_psia_m.psia002
   LET g_psia003_t = g_psia_m.psia003
