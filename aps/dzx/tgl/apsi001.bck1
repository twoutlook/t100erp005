   {<point name="delete.define"/>} 
   #end add-point     
   
   #add-point:Function前置處理  name="delete.pre_function"
   {<point name="delete.pre_function"/>}
   #end add-point
   
   IF g_psba_m.psba001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
   
   LET g_master_multi_table_t.psbal001 = g_psba_m.psba001
LET g_master_multi_table_t.psbal003 = g_psba_m.psbal003
LET g_master_multi_table_t.psbal004 = g_psba_m.psbal004
 
   
   CALL s_transaction_begin()
 
   OPEN apsi001_cl USING g_enterprise,g_psba_m.psba001
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN apsi001_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CLOSE apsi001_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE apsi001_master_referesh USING g_psba_m.psba001 INTO g_psba_m.psba001,g_psba_m.psbastus,g_psba_m.psba002, 
       g_psba_m.psba003,g_psba_m.psba004,g_psba_m.psba005,g_psba_m.psba007,g_psba_m.psba008,g_psba_m.psba009, 
       g_psba_m.psba010,g_psba_m.psba011,g_psba_m.psba012,g_psba_m.psba006,g_psba_m.psba013,g_psba_m.psba014, 
       g_psba_m.psba015,g_psba_m.psba017,g_psba_m.psba018,g_psba_m.psba019,g_psba_m.psba016,g_psba_m.psba020, 
       g_psba_m.psba021,g_psba_m.psba022,g_psba_m.psba023,g_psba_m.psba024,g_psba_m.psba025,g_psba_m.psba026, 
       g_psba_m.psba027,g_psba_m.psba028,g_psba_m.psba029,g_psba_m.psba030,g_psba_m.psbaownid,g_psba_m.psbaowndp, 
       g_psba_m.psbacrtid,g_psba_m.psbacrtdp,g_psba_m.psbacrtdt,g_psba_m.psbamodid,g_psba_m.psbamoddt, 
       g_psba_m.psba007_desc,g_psba_m.psbaownid_desc,g_psba_m.psbaowndp_desc,g_psba_m.psbacrtid_desc, 
       g_psba_m.psbacrtdp_desc,g_psba_m.psbamodid_desc
   
   
   #檢查是否允許此動作
   IF NOT apsi001_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_psba_m_mask_o.* =  g_psba_m.*
   CALL apsi001_psba_t_mask()
   LET g_psba_m_mask_n.* =  g_psba_m.*
   
   CALL apsi001_show()
   
   #add-point:delete段before ask name="delete.before_ask"
   {<point name="delete.before_ask"/>}
   #end add-point 
 
   IF cl_ask_del_master() THEN              #確認一下
   
      #add-point:單頭刪除前 name="delete.head.b_delete"
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point   
      
      #應用 a47 樣板自動產生(Version:4)
      #刪除相關文件
      CALL apsi001_set_pk_array()
      #add-point:相關文件刪除前 name="delete.befroe.related_document_remove"
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
  
  
      #資料備份
      LET g_psba001_t = g_psba_m.psba001
 
 
      DELETE FROM psba_t
       WHERE psbaent = g_enterprise AND psba001 = g_psba_m.psba001
 
       
      #add-point:單頭刪除中 name="delete.head.m_delete"
      {<point name="delete.head.m_delete"/>}
      #end add-point
       
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = g_psba_m.psba001,":",SQLERRMESSAGE  
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = FALSE 
         CALL s_transaction_end('N','0')
         CALL cl_err()
         RETURN
      END IF
      
      #add-point:單頭刪除後 name="delete.head.a_delete"
      {<point name="delete.head.a_delete"/>}
      #end add-point
  
      #add-point:單身刪除前 name="delete.body.b_delete"
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM psbe_t
       WHERE psbeent = g_enterprise AND psbe001 = g_psba_m.psba001
 
 
      #add-point:單身刪除中 name="delete.body.m_delete"
      {<point name="delete.body.m_delete"/>}
      #end add-point
         
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "psbe_t:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = FALSE 
         CALL s_transaction_end('N','0')
         CALL cl_err()
         RETURN
      END IF    
 
      #add-point:單身刪除後 name="delete.body.a_delete"
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
            
                                                               
 
 
 
      
      #修改歷程記錄(刪除)
      IF NOT cl_log_modified_record('','') THEN 
         CLOSE apsi001_cl
         CALL s_transaction_end('N','0')
         RETURN
      END IF
             
      CLEAR FORM
      CALL g_psbe_d.clear() 
 
     
      CALL apsi001_ui_browser_refresh()  
      #CALL apsi001_ui_headershow()  
      #CALL apsi001_ui_detailshow()
 
      #add-point:多語言刪除 name="delete.lang.before_delete"
      {<point name="delete.lang.before_delete"/>}
      #end add-point
      
      #單頭多語言刪除
      INITIALIZE l_var_keys_bak TO NULL 
   INITIALIZE l_field_keys   TO NULL 
   LET l_var_keys_bak[01] = g_enterprise
   LET l_field_keys[01] = 'psbalent'
   LET l_var_keys_bak[02] = g_master_multi_table_t.psbal001
   LET l_field_keys[02] = 'psbal001'
   CALL cl_multitable_delete(l_field_keys,l_var_keys_bak,'psbal_t')
 
      
      #單身多語言刪除
      
 
   
      #add-point:多語言刪除 name="delete.lang.delete"
      {<point name="delete.lang.delete"/>}
      #end add-point
      
      IF g_browser_cnt > 0 THEN 
         #CALL apsi001_browser_fill("")
         CALL apsi001_fetch('P')
         DISPLAY g_browser_cnt TO FORMONLY.h_count   #總筆數的顯示
         DISPLAY g_browser_cnt TO FORMONLY.b_count   #總筆數的顯示
      ELSE
         CLEAR FORM
      END IF
      
      CALL s_transaction_end('Y','0')
   ELSE
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE apsi001_cl
 
   #功能已完成,通報訊息中心
   CALL apsi001_msgcentre_notify('delete')
    
