               #將遮罩欄位還原
               CALL apsi001_psbe_t_mask_restore('restore_mask_o')
      
               UPDATE psbe_t SET (psbe001,psbe002,psbe003) = (g_psba_m.psba001,g_psbe_d[l_ac].psbe002, 
                   g_psbe_d[l_ac].psbe003)
                WHERE psbeent = g_enterprise AND psbe001 = g_psba_m.psba001 
 
                  AND psbe002 = g_psbe_d_t.psbe002 #項次   
 
                  
               #add-point:單身修改中 name="input.body.m_update"
               {<point name="input.body.m_update"/>}
