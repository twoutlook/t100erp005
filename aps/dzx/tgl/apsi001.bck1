               LET gs_keys[gs_keys.getLength()+1] = g_psbe_d_t.psbe002
 
            
               #刪除同層單身
               IF NOT apsi001_delete_b('psbe_t',gs_keys,"'1'") THEN
                  CALL s_transaction_end('N','0')
