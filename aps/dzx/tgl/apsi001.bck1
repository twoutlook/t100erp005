                  LET gs_keys[gs_keys.getLength()+1] = g_psbe_d_t.psbe002
 
                  CALL apsi001_key_update_b(gs_keys,'psbe_t')
               END IF
               
               #修改歷程記錄(單身修改)
               LET g_log1 = util.JSON.stringify(g_psba_m),util.JSON.stringify(g_psbe_d_t)
               LET g_log2 = util.JSON.stringify(g_psba_m),util.JSON.stringify(g_psbe_d[l_ac])
               IF NOT cl_log_modified_record_d(g_log1,g_log2) THEN 
