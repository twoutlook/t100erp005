         ON ACTION controlp INFIELD psbe003
            #add-point:ON ACTION controlp INFIELD psbe003 name="input.c.page1.psbe003"
            {<point name="input.c.page1.psbe003" />}
            #END add-point
 
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
