            #add-point:BEFORE FIELD psbe003 name="input.b.page1.psbe003"
            {<point name="input.b.page1.psbe003" />}
            #END add-point
 
 
         #應用 a02 樣板自動產生(Version:2)
         AFTER FIELD psbe003
            
            #add-point:AFTER FIELD psbe003 name="input.a.page1.psbe003"
            {<point name="input.a.page1.psbe003" />}
            #END add-point
            
 
 
         #應用 a04 樣板自動產生(Version:3)
         ON CHANGE psbe003
            #add-point:ON CHANGE psbe003 name="input.g.page1.psbe003"
            {<point name="input.g.page1.psbe003" />}
            #END add-point 
 
 
 
                  #Ctrlp:input.c.page1.psbe002
         #應用 a03 樣板自動產生(Version:3)
         ON ACTION controlp INFIELD psbe002
            #add-point:ON ACTION controlp INFIELD psbe002 name="input.c.page1.psbe002"
            {<point name="input.c.page1.psbe002" />}
            #END add-point
 
 
         #Ctrlp:input.c.page1.psbe003
