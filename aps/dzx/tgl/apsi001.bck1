                     LET g_errparam.extend = "psbe_t:",SQLERRMESSAGE 
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL s_transaction_end('N','0')
                     CALL cl_err()                   
                     
                  OTHERWISE
                     #資料多語言用-增/改
                     
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_psba_m.psba001
               LET gs_keys_bak[1] = g_psba001_t
               LET gs_keys[2] = g_psbe_d[g_detail_idx].psbe002
               LET gs_keys_bak[2] = g_psbe_d_t.psbe002
