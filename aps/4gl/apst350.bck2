################################################################################
PRIVATE FUNCTION apst350_upd_psib008(p_psib006,p_psib007,p_psib012,p_psib011,p_psib008,p_psib013)
  DEFINE p_psib006   LIKE psib_t.psib006
  DEFINE p_psib007   LIKE psib_t.psib007
  DEFINE p_psib012   LIKE psib_t.psib012
  DEFINE p_psib011   LIKE psib_t.psib011 #160819-00015#3-add
  DEFINE p_psib013   LIKE psib_t.psib013  
  DEFINE p_psib008   LIKE psib_t.psib008
  DEFINE r_success   LIKE type_t.chr1


  LET r_success = TRUE
  IF cl_null(p_psib006) OR p_psib007 IS NULL OR cl_null(p_psib011) OR cl_null(p_psib008) THEN
     LET r_success = FALSE
     RETURN r_success     
  END IF
  
  #160819-00015#3-s-mod 
  #UPDATE psib_t 
  #   SET (psib006,psib007,psib012,psib011) 
  #    = (p_psib006,p_psib007,p_psib012,p_psib011)
  # WHERE psibent = g_enterprise 
  #   AND psibsite = g_psia_m.psiasite
  #   AND psib001 = g_psia_m.psia001
  #   AND psib002 = g_psia_m.psia002
  #   AND psib003 = g_psia_m.psia003
  #   AND psib004 = g_psia_m.psia004
  #   AND psib005 = g_psia_m.psia005
  #   AND psib006 = g_psib_d_t.psib006 
  #   AND psib007 = g_psib_d_t.psib007
  #   AND psib008 = p_psib008
  UPDATE psib_t 
     SET (psib006,psib007,psib012,psib011,psib013) 
      = (p_psib006,p_psib007,p_psib012,p_psib011,p_psib013)
   WHERE psibent = g_enterprise 
     AND psibsite = g_psia_m.psiasite
     AND psib001 = g_psia_m.psia001
     AND psib002 = g_psia_m.psia002
     AND psib003 = g_psia_m.psia003
     AND psib004 = g_psia_m.psia004
     AND psib005 = g_psia_m.psia005
     AND psib006 = g_psib_d_t.psib006 
     AND psib007 = g_psib_d_t.psib007
     AND psib008 = p_psib008
     AND psib013 = g_psib_d_t.psib013
  #160819-00015#3-e-mod
  IF SQLCA.sqlcode THEN
     INITIALIZE g_errparam TO NULL
     LET g_errparam.extend = "UPDATE psib_t"
     LET g_errparam.code   = SQLCA.sqlcode
     LET g_errparam.popup  = FALSE
     CALL cl_err()
     LET r_success = FALSE
     RETURN r_success           
  END IF     
  RETURN r_success 
END FUNCTION
################################################################################
# Descriptions...: 取得截止日期
# Memo...........:
# Usage..........: CALL apst350_get_edate(p_bdate,p_psib008)
#                  RETURNING r_edate
# Input parameter: p_bdate        起始日期
#                : p_psib008      期數
# Return code....: r_edate        截止日期
# Date & Author..: 2014/07/30 By  polly
# Modify.........:
################################################################################
PRIVATE FUNCTION apst350_get_edate(p_bdate,p_psib008)
DEFINE p_bdate           LIKE type_t.dat
DEFINE p_psib008         LIKE psib_t.psib008
DEFINE l_psjb003         LIKE psjb_t.psjb003
DEFINE r_edate           LIKE type_t.dat
DEFINE l_oogc008         LIKE oogc_t.oogc008
DEFINE l_day             LIKE type_t.num5

   SELECT psjb003 INTO l_psjb003
     FROM psjb_t
    WHERE psjbent = g_enterprise
      AND psjbsite = g_site
      AND psjb001 = g_psia_m.psia001
      AND psjb002 = p_psib008

   CASE l_psjb003
      WHEN '1'   #週
           #取得起始日期的週別
           LET l_oogc008 = NULL
           SELECT oogc008 INTO l_oogc008 FROM oogc_t
            WHERE oogcent = g_enterprise
              AND oogc001 = g_ooef008
              AND oogc002 = g_ooef009
              AND oogc003 = p_bdate
           #取得該週的最後一天
           SELECT MAX(oogc003) INTO r_edate
             FROM oogc_t
            WHERE oogcent = g_enterprise
              AND oogc001 = g_ooef008
              AND oogc002 = g_ooef009
              AND oogc008 = l_oogc008
      WHEN '2'   #旬
           #取出日
           LET l_day = DAY(p_bdate)
           #依起始的日決定截止日期
           CASE
              WHEN l_day >= 1 AND l_day <= 10
                   LET r_edate = MDY(MONTH(p_bdate),10,YEAR(p_bdate))
              WHEN l_day >=11 AND l_day <= 20
                   LET r_edate = MDY(MONTH(p_bdate),20,YEAR(p_bdate))
              WHEN l_day >=21
                   LET r_edate = MDY(MONTH(p_bdate)+1,1,YEAR(p_bdate)) - 1
           END CASE
      WHEN '3'   #月
           LET r_edate = MDY(MONTH(p_bdate)+1,1,YEAR(p_bdate)) - 1
   END CASE

   RETURN r_edate
   
END FUNCTION
