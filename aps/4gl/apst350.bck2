################################################################################
# Descriptions...: 依期別數量insert到psib_t
# Memo...........:
# Usage..........: CALL apst350_ins_psib008(p_psib011,p_psib008)
#                  RETURNING r_success
# Input parameter: p_psib011      數量
#                : p_psib008      期別
# Return code....: r_success
# Date & Author..: 2014/07/30 By  POLLY
# Modify.........:
################################################################################
PRIVATE FUNCTION apst350_ins_psib008(p_psib011,p_psib008)
  DEFINE p_psib011   LIKE psib_t.psib011
  DEFINE p_psib008   LIKE psib_t.psib008
  DEFINE r_success   LIKE type_t.chr1
  DEFINE l_cnt       LIKE type_t.num5
  DEFINE l_bdate     LIKE type_t.dat
  DEFINE l_edate     LIKE type_t.dat


  LET r_success = TRUE
  IF cl_null(p_psib011) OR cl_null(p_psib008) THEN
     LET r_success = FALSE
     RETURN r_success     
  END IF    
    
  LET l_cnt = 0
  SELECT COUNT(*) INTO l_cnt
    FROM psib_t
   WHERE psibent = g_enterprise
     AND psibsite = g_site
     AND psib001 = g_psia_m.psia001
     AND psib002 = g_psia_m.psia002
     AND psib003 = g_psia_m.psia003
     AND psib004 = g_psia_m.psia004
     AND psib005 = g_psia_m.psia005
     AND psib006 = g_psib_d[l_ac].psib006
     AND psib007 = g_psib_d[l_ac].psib007
     AND psib008 = p_psib008
     
  IF l_cnt = 0 THEN
       
     #抓取起始日期(抓取上一期的截止日期 +1天)
     IF p_psib008 > 1 THEN
        SELECT psib010 INTO l_bdate
          FROM psib_t
         WHERE psibent = g_enterprise
           AND psibsite = g_site
           AND psib001 = g_psia_m.psia001
           AND psib002 = g_psia_m.psia002
           AND psib003 = g_psia_m.psia003
           AND psib004 = g_psia_m.psia004
           AND psib005 = g_psia_m.psia005
           AND psib006 = g_psib_d[l_ac].psib006
           AND psib007 = g_psib_d[l_ac].psib007
           AND psib008 = p_psib008 - 1            
        LET l_bdate = l_bdate + 1  
     END IF 
     #抓取截止日期
     CALL apst350_get_edate(l_bdate,p_psib008) RETURNING l_edate
     
     #160819-00015#3-s-mod 多psib013
     #INSERT INTO psib_t
     #            (psibent,psibsite,psib001,psib002,psib003,
     #             psib004, psib005,psib006,psib007,psib008,
     #             psib009,psib010,psib011,psib012)
     #       VALUES(g_enterprise,g_site,g_psia_m.psia001,g_psia_m.psia002,g_psia_m.psia003,
     #              g_psia_m.psia004,g_psia_m.psia005,g_psib_d[l_ac].psib006,g_psib_d[l_ac].psib007,p_psib008,
     #              l_bdate,l_edate,p_psib011,g_psib_d[l_ac].psib012)
     INSERT INTO psib_t
                 (psibent,psibsite,psib001,psib002,psib003,
                  psib004, psib005,psib006,psib007,psib008,
                  psib013,psib009,psib010,psib011,psib012)
            VALUES(g_enterprise,g_site,g_psia_m.psia001,g_psia_m.psia002,g_psia_m.psia003,
                   g_psia_m.psia004,g_psia_m.psia005,g_psib_d[l_ac].psib006,g_psib_d[l_ac].psib007,p_psib008,
                   g_psib_d[l_ac].psib013,l_bdate,l_edate,p_psib011,g_psib_d[l_ac].psib012)
     #160819-00015#3-e-mod
     IF SQLCA.sqlcode THEN
        INITIALIZE g_errparam TO NULL
        LET g_errparam.extend = "INSERT psib_t"
        LET g_errparam.code   = SQLCA.sqlcode
        LET g_errparam.popup  = FALSE
        CALL cl_err()
        LET r_success = FALSE
        RETURN r_success           
     END IF 
  ELSE
     INITIALIZE g_errparam TO NULL 
     LET g_errparam.extend = 'INSERT' 
     LET g_errparam.code   = "std-00006" 
     LET g_errparam.popup  = FALSE 
     CALL cl_err()     
     LET r_success = FALSE
     RETURN r_success   
  END IF
  RETURN r_success 
END FUNCTION
