         WHERE psibent = g_enterprise AND psibsite = ps_keys_bak[1] AND psib001 = ps_keys_bak[2] AND psib002 = ps_keys_bak[3] AND psib003 = ps_keys_bak[4] AND psib004 = ps_keys_bak[5] AND psib005 = ps_keys_bak[6] AND psib006 = ps_keys_bak[7] AND psib007 = ps_keys_bak[8] AND psib008 = ps_keys_bak[9] AND psib013 = ps_keys_bak[10]
      #add-point:update_b段修改中 name="update_b.m_update"
      
      #end add-point   
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            INITIALIZE g_errparam TO NULL 
