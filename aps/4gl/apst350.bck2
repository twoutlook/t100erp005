################################################################################
# Descriptions...: 取得最新版本
# Memo...........:
# Usage..........: CALL apst350_get_psia005(p_cmd)
# Date & Author..: 2014/07/25 By Polly
# Modify.........:
################################################################################
PRIVATE FUNCTION apst350_get_psia005(p_cmd)
DEFINE   p_cmd    LIKE type_t.chr1
DEFINE   l_cnt    LIKE type_t.num5  

   IF NOT cl_null(g_psia_m.psia001) AND NOT cl_null(g_psia_m.psia002) AND NOT cl_null(g_psia_m.psia003) AND
      g_psia_m.psia004 IS NOT NULL THEN
      #是否有未確認的前版本
      SELECT COUNT(*) INTO l_cnt FROM psia_t
       WHERE psiaent = g_enterprise
         AND psiasite = g_site
         AND psia001 = g_psia_m.psia001
         AND psia002 = g_psia_m.psia002
         AND psia003 = g_psia_m.psia003
         AND psia004 = g_psia_m.psia004
         AND psiastus = 'N'
      IF cl_null(l_cnt) THEN LET l_cnt = 0 END IF
      IF (p_cmd = 'a' AND l_cnt > 0) THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'axm-00259'
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()
         RETURN FALSE
      END IF      
      SELECT MAX(psia005)+1 INTO g_psia_m.psia005 FROM psia_t
       WHERE psiaent = g_enterprise
         AND psiasite = g_site
         AND psia001 = g_psia_m.psia001
         AND psia002 = g_psia_m.psia002
         AND psia003 = g_psia_m.psia003
         AND psia004 = g_psia_m.psia004
      IF cl_null(g_psia_m.psia005) THEN
         LET g_psia_m.psia005 = 1
      END IF   
      DISPLAY BY NAME g_psia_m.psia005        
   END IF 
   RETURN TRUE 
END FUNCTION
