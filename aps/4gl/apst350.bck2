################################################################################
# Descriptions...: 檢查起始日是否跟預測編號的起始日一致
# Memo...........:
# Usage..........: CALL apst350_psia002_chk()
#                  RETURNING TRUE/FALSE
# Date & Author..: 2014/07/25 By Polly
# Modify.........:
################################################################################
PRIVATE FUNCTION apst350_psia002_chk()
   DEFINE  l_psja003             LIKE psja_t.psja003
   DEFINE  l_psja009             LIKE psja_t.psja009  #160819-00015#2-add
   DEFINE  l_dd                  LIKE psja_t.psja003
   DEFINE  l_msg                 STRING
   DEFINE  l_monday              LIKE psia_t.psia002  #周一的日期,160819-00015#2-add
   DEFINE  r_success             LIKE type_t.num5     #160819-00015#2-add
   
   LET l_msg  = ''
   LET r_success = TRUE  #160819-00015#2-add
   IF NOT cl_null(g_psia_m.psia002) AND NOT cl_null(g_psia_m.psia001) THEN 
      #160819-00015#2-s-mod
      #SELECT psja003 INTO l_psja003 FROM psja_t
      # WHERE psjaent = g_enterprise
      #   AND psjasite = g_site
      #   AND psja001 = g_psia_m.psia001 
      ##取得起始日    
      #LET l_dd = DAY(g_psia_m.psia002)     
      #IF l_psja003 <> l_dd THEN     
      #   CALL cl_getmsg('axm-00247',g_lang) RETURNING l_msg
      #   LET l_msg = l_msg,l_psja003     
      #   INITIALIZE g_errparam TO NULL
      #   LET g_errparam.code = 'axm-00169'
      #   LET g_errparam.extend = l_msg
      #   LET g_errparam.popup = TRUE
      #   CALL cl_err()
      #   RETURN FALSE
      #END IF 
      SELECT psja003,psja009 INTO l_psja003,l_psja009 FROM psja_t
       WHERE psjaent = g_enterprise
         AND psjasite = g_site
         AND psja001 = g_psia_m.psia001 
      CASE l_psja009
        WHEN '1'  #指定日
           LET l_dd = DAY(g_psia_m.psia002)     
           IF l_psja003 <> l_dd THEN     
              CALL cl_getmsg('axm-00247',g_lang) RETURNING l_msg
              LET l_msg = l_msg,l_psja003  
              LET r_success = FALSE              
           END IF
        WHEN '2'  #指定週間
           CALL s_date_get_monday(g_psia_m.psia002) RETURNING l_monday  #取得傳入日期當周的星期一日期
            CASE l_psja003
               WHEN '0' #週日
                  LET l_monday = l_monday - 1
               WHEN '2' #周二
                  LET l_monday = l_monday + 1
               WHEN '3' #周三
                  LET l_monday = l_monday + 2
               WHEN '4' #周四
                  LET l_monday = l_monday + 3
               WHEN '5' #周五
                  LET l_monday = l_monday + 4
               WHEN '6' #周六
                  LET l_monday = l_monday + 5
            END CASE      
            IF l_monday <> g_psia_m.psia002 THEN
               CALL cl_getmsg('axm-00414',g_dlang) RETURNING l_msg  #該預測代號的預測周間是：
               LET l_msg = l_msg CLIPPED,l_psja003
               LET r_success = FALSE
            END IF
      END CASE
      #160819-00015#2-e-mod
   END IF
   
   #RETURN TRUE  #160819-00015#2-mark
   
   #160819-00015#2-s-add
   IF NOT r_success THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'axm-00169'
      LET g_errparam.extend = l_msg
      LET g_errparam.popup = TRUE
      CALL cl_err()
   END IF
   
   RETURN r_success
   #160819-00015#2-e-add
END FUNCTION
