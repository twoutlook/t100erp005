{<section id="apst350.action_chk" >}
#+ 修改/刪除前行為檢查(是否可允許此動作), 若有其他行為須管控也可透過此段落
PRIVATE FUNCTION apst350_action_chk()
   #add-point:action_chk段define(客製用) name="action_chk.define_customerization"
   
   #end add-point
   #add-point:action_chk段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="action_chk.define"
   
   #end add-point
   
   #add-point:action_chk段action_chk name="action_chk.action_chk"
   #160818-00017#32 add-S
   SELECT psiastus  INTO g_psia_m.psiastus
     FROM psia_t
    WHERE psiaent = g_enterprise
      AND psiasite = g_psia_m.psiasite
      AND psia001 = g_psia_m.psia001
      AND psia002 = g_psia_m.psia002
      AND psia003 = g_psia_m.psia003
      AND psia004 = g_psia_m.psia004
      AND psia005 = g_psia_m.psia005

   IF(g_action_choice="modify" OR g_action_choice="delete" OR g_action_choice="modify_detail")  THEN
     LET g_errno = NULL
     CASE g_psia_m.psiastus
        WHEN 'W'
           LET g_errno = 'sub-00180'
        WHEN 'X'
           LET g_errno = 'sub-00229'
        WHEN 'Y'
           LET g_errno = 'sub-00178'
        WHEN 'S'
           LET g_errno = 'sub-00230'
     END CASE

     IF NOT cl_null(g_errno) THEN
        INITIALIZE g_errparam TO NULL
        LET g_errparam.code = g_errno
        LET g_errparam.extend = g_psia_m.psia001
        LET g_errparam.popup = TRUE
        CALL cl_err()
        LET g_errno = NULL
        CALL apst350_set_act_visible()
        CALL apst350_set_act_no_visible()
        CALL apst350_show()
        RETURN FALSE
     END IF
   END IF
   #160818-00017#32 add-E
   #end add-point
      
   RETURN TRUE
   
END FUNCTION
 
