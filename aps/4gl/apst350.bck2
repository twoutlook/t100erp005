#+ 單身自動複製
PRIVATE FUNCTION apst350_detail_reproduce()
   #add-point:delete段define(客製用) name="detail_reproduce.define_customerization"
   
   #end add-point    
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE psib_t.* #此變數樣板目前無使用
 
 
   #add-point:delete段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="detail_reproduce.define"
   DEFINE l_psja002_o   LIKE psja_t.psja002
   DEFINE l_psja002_n   LIKE psja_t.psja002
   DEFINE l_bdate       LIKE type_t.dat
   DEFINE l_edate       LIKE type_t.dat   
   DEFINE i             LIKE type_t.num5
   DEFINE l_psib006     LIKE psib_t.psib006
   DEFINE l_psib007     LIKE psib_t.psib007
   DEFINE l_psib008     LIKE psib_t.psib008
   DEFINE l_psib012     LIKE psib_t.psib012
   DEFINE l_psib013     LIKE psib_t.psib013  #160819-00015#3-add
   DEFINE l_sql         STRING   
   #end add-point    
   
   #add-point:Function前置處理  name="detail_reproduce.pre_function"
   
   #end add-point
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE apst350_detail
   
   #add-point:單身複製前1 name="detail_reproduce.body.table1.b_insert"
 
   #end add-point
   
   #CREATE TEMP TABLE
   SELECT * FROM psib_t
    WHERE psibent = g_enterprise AND psibsite = g_psiasite_t
     AND psib001 = g_psia001_t
     AND psib002 = g_psia002_t
     AND psib003 = g_psia003_t
     AND psib004 = g_psia004_t
     AND psib005 = g_psia005_t
 
    INTO TEMP apst350_detail
 
   #將key修正為調整後   
   UPDATE apst350_detail 
      #更新key欄位
      SET psibsite = g_psia_m.psiasite
          , psib001 = g_psia_m.psia001
          , psib002 = g_psia_m.psia002
          , psib003 = g_psia_m.psia003
          , psib004 = g_psia_m.psia004
          , psib005 = g_psia_m.psia005
 
      #更新共用欄位
      
 
   #add-point:單身修改前 name="detail_reproduce.body.table1.b_update"
   
   #end add-point                                       
  
   #將資料塞回原table   
   INSERT INTO psib_t SELECT * FROM apst350_detail
   
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "reproduce:",SQLERRMESSAGE 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #add-point:單身複製中1 name="detail_reproduce.body.table1.m_insert"
   SELECT psja002 INTO l_psja002_o FROM psja_t       #原本的預測期數
    WHERE psjaent = g_enterprise
      AND psja001 = g_psia001_t

   SELECT psja002 INTO l_psja002_n FROM psja_t       #複製後的預測期數
    WHERE psjaent = g_enterprise
      AND psja001 = g_psia_m.psia001      
      

   #調整起始日期/截止日期
   IF l_psja002_o >= l_psja002_n THEN
       LET l_psib008 = l_psja002_n
   ELSE
      LET l_psib008 = l_psja002_o
   END IF
   LET l_bdate = g_psia_m.psia002      
   FOR i = 1 TO l_psib008
       #抓取截止日期
       CALL apst350_get_edate(l_bdate,i) RETURNING l_edate
       UPDATE psib_t
          SET psib009 = l_bdate,
              psib010 = l_edate
        WHERE psibent = g_enterprise
          AND psib001 = g_psia_m.psia001
          AND psib002 = g_psia_m.psia002
          AND psib003 = g_psia_m.psia003
          AND psib004 = g_psia_m.psia004
          AND psib005 = g_psia_m.psia005 
          AND psib008 = i
       IF SQLCA.sqlcode THEN
          INITIALIZE g_errparam TO NULL
          LET g_errparam.code = SQLCA.sqlcode
          LET g_errparam.extend = "reproduce"
          LET g_errparam.popup = TRUE
          CALL cl_err()
          RETURN
       END IF
       #160819-00015#2-s-add  將庫存、請購、採購、收購 = 0
       UPDATE psic_t SET psic009 = 0,psic010 = 0,psic011 = 0,psic012 =0
        WHERE psicent = g_enterprise
          AND psic001 = g_psia_m.psia001
          AND psic002 = g_psia_m.psia002
          AND psic003 = g_psia_m.psia003
          AND psic004 = g_psia_m.psia004
          AND psic005 = g_psia_m.psia005
       IF SQLCA.sqlcode THEN
          INITIALIZE g_errparam TO NULL
          LET g_errparam.code = SQLCA.sqlcode
          LET g_errparam.extend = "reproduce"
          LET g_errparam.popup = TRUE
          CALL cl_err()
          RETURN
       END IF          
       #160819-00015#2-e-add
       LET l_bdate = l_edate + 1   
   END FOR

   IF l_psja002_o > l_psja002_n THEN                #需將多的期數數量刪除
      DELETE FROM psib_t 
       WHERE psibent = g_enterprise
         AND psib001 = g_psia_m.psia001
         AND psib002 = g_psia_m.psia002
         AND psib003 = g_psia_m.psia003
         AND psib004 = g_psia_m.psia004
         AND psib005 = g_psia_m.psia005
         AND psib008 > l_psja002_n    
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "reproduce"
         LET g_errparam.popup = TRUE
         CALL cl_err()
         RETURN
      END IF   
   END IF
  
   IF l_psja002_o < l_psja002_n  THEN                           #需將少的期數數量INSERT
      #160819-00015#3-s-mod
      #LET l_sql = " SELECT DISTINCT psib006,psib007,psib012 FROM psib_t ",  
      #            "  WHERE psibent = '",g_enterprise,"'",
      #            "    AND psibsite  = '",g_psia_m.psiasite,"'",  
      #            "    AND psib001 = '",g_psia_m.psia001,"'",   
      #            "    AND psib002 = '",g_psia_m.psia002,"'",
      #            "    AND psib003 = '",g_psia_m.psia003,"'",
      #            "    AND psib004 = '",g_psia_m.psia004,"'",
      #            "    AND psib005 = '",g_psia_m.psia005,"'" 
      LET l_sql = " SELECT DISTINCT psib006,psib007,psib012,psib013 FROM psib_t ",  
                  "  WHERE psibent = '",g_enterprise,"'",
                  "    AND psibsite  = '",g_psia_m.psiasite,"'",  
                  "    AND psib001 = '",g_psia_m.psia001,"'",   
                  "    AND psib002 = '",g_psia_m.psia002,"'",
                  "    AND psib003 = '",g_psia_m.psia003,"'",
                  "    AND psib004 = '",g_psia_m.psia004,"'",
                  "    AND psib005 = '",g_psia_m.psia005,"'" 
      #160819-00015#3-e-mod
      PREPARE apst350_pb_02 FROM l_sql
      DECLARE b_fill_cs_02 CURSOR FOR apst350_pb_02        
     
      FOREACH b_fill_cs_02 INTO l_psib006,l_psib007,l_psib012,l_psib013  #160819-00015#3-add-'l_psib013'
         FOR i = l_psja002_o + 1 TO l_psja002_n
             #抓取起始日期(抓取上一期的截止日期 +1天)
             SELECT psib010 INTO l_bdate
               FROM psib_t
              WHERE psibent = g_enterprise
                AND psibsite = g_psia_m.psiasite
                AND psib001 = g_psia_m.psia001
                AND psib002 = g_psia_m.psia002
                AND psib003 = g_psia_m.psia003
                AND psib004 = g_psia_m.psia004
                AND psib005 = g_psia_m.psia005
                AND psib006 = l_psib006
                AND psib013 = l_pisb013  #160819-00015#3-add
                AND psib007 = l_psib007
                AND psib008 = i - 1             #上一期     
             LET l_bdate = l_bdate + 1          
             #抓取截止日期        
             CALL apst350_get_edate(l_bdate,i) RETURNING l_edate        
              
             #160819-00015#3-s-mod
             #INSERT INTO psib_t
             #           (psibent,psibsite,psib001,psib002,psib003,
             #            psib004, psib005,psib006,psib007,psib008,
             #            psib009,psib010,psib011,psib012)
             #     VALUES(g_enterprise,g_psia_m.psiasite,g_psia_m.psia001,g_psia_m.psia002,g_psia_m.psia003,
             #            g_psia_m.psia004,g_psia_m.psia005,l_psib006,l_psib007,i,
             #            l_bdate,l_edate,0,l_psib012)
             INSERT INTO psib_t
                        (psibent,psibsite,psib001,psib002,psib003,
                         psib004, psib005,psib006,psib007,psib008,
                         psib013,psib009,psib010,psib011,psib012)
                  VALUES(g_enterprise,g_psia_m.psiasite,g_psia_m.psia001,g_psia_m.psia002,g_psia_m.psia003,
                         g_psia_m.psia004,g_psia_m.psia005,l_psib006,l_psib007,i,
                         l_pisb013,l_bdate,l_edate,0,l_psib012)
             #160819-00015#3-e-mod
             IF SQLCA.sqlcode THEN
                INITIALIZE g_errparam TO NULL
                LET g_errparam.code = SQLCA.sqlcode
                LET g_errparam.extend = "reproduce"
                LET g_errparam.popup = TRUE
                CALL cl_err()
                RETURN
             END IF                         
         END FOR         
      END FOREACH                    
                      
   END IF    
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE apst350_detail
   
   #add-point:單身複製後1 name="detail_reproduce.body.table1.a_insert"
   
   #end add-point
 
 
   
 
   
   #多語言複製段落
   
   
   CALL s_transaction_end('Y','0')
   
   #已新增完, 調整資料內容(修改時使用)
   LET g_psiasite_t = g_psia_m.psiasite
   LET g_psia001_t = g_psia_m.psia001
   LET g_psia002_t = g_psia_m.psia002
   LET g_psia003_t = g_psia_m.psia003
   LET g_psia004_t = g_psia_m.psia004
   LET g_psia005_t = g_psia_m.psia005
 
   
END FUNCTION
 
