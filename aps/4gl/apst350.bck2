{<section id="apst350.fetch" >}
#+ 指定PK後抓取單頭其他資料
PRIVATE FUNCTION apst350_fetch(p_flag)
   #add-point:fetch段define(客製用) name="fetch.define_customerization"
   
   #end add-point    
   DEFINE p_flag     LIKE type_t.chr1
   DEFINE ls_msg     STRING
   #add-point:fetch段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="fetch.define"
   
   #end add-point    
   
   #add-point:Function前置處理  name="fetch.pre_function"
   
   #end add-point
   
   IF g_browser_cnt = 0 THEN
      RETURN
   END IF
 
   #清空第二階單身
 
   
   CALL cl_ap_performance_next_start()
   CASE p_flag
      WHEN 'F' 
         LET g_current_idx = 1
      WHEN 'L'  
         LET g_current_idx = g_browser.getLength()              
      WHEN 'P'
         IF g_current_idx > 1 THEN               
            LET g_current_idx = g_current_idx - 1
         END IF 
      WHEN 'N'
         IF g_current_idx < g_header_cnt THEN
            LET g_current_idx =  g_current_idx + 1
         END IF        
      WHEN '/'
         IF (NOT g_no_ask) THEN    
            CALL cl_set_act_visible("accept,cancel", TRUE)    
            CALL cl_getmsg('fetch',g_lang) RETURNING ls_msg
            LET INT_FLAG = 0
 
            PROMPT ls_msg CLIPPED,':' FOR g_jump
               #交談指令共用ACTION
               &include "common_action.4gl" 
            END PROMPT
 
            CALL cl_set_act_visible("accept,cancel", FALSE)    
            IF INT_FLAG THEN
                LET INT_FLAG = 0
                EXIT CASE  
            END IF           
         END IF
         
         IF g_jump > 0 AND g_jump <= g_browser.getLength() THEN
             LET g_current_idx = g_jump
         END IF
         LET g_no_ask = FALSE  
   END CASE 
 
   CALL g_curr_diag.setCurrentRow("s_browse", g_current_idx) #設定browse 索引
   
   LET g_current_row = g_current_idx
   LET g_detail_cnt = g_header_cnt                  
   
   #單身總筆數顯示
   IF g_detail_cnt > 0 THEN
      #若單身有資料時, idx至少為1
      IF g_detail_idx <= 0 THEN
         LET g_detail_idx = 1
      END IF
      DISPLAY g_detail_idx TO FORMONLY.idx  
   ELSE
      LET g_detail_idx = 0
      DISPLAY '' TO FORMONLY.idx    
   END IF
   
   #瀏覽頁筆數顯示
   LET g_browser_idx = g_pagestart+g_current_idx-1
   DISPLAY g_browser_idx TO FORMONLY.b_index   #當下筆數
   DISPLAY g_browser_idx TO FORMONLY.h_index   #當下筆數
   
   CALL cl_navigator_setting( g_current_idx, g_browser_cnt )
 
   #代表沒有資料
   IF g_current_idx = 0 OR g_browser.getLength() = 0 THEN
      RETURN
   END IF
   
   #避免超出browser資料筆數上限
   IF g_current_idx > g_browser.getLength() THEN
      LET g_browser_idx = g_browser.getLength()
      LET g_current_idx = g_browser.getLength()
   END IF
   
   LET g_psia_m.psiasite = g_browser[g_current_idx].b_psiasite
   LET g_psia_m.psia001 = g_browser[g_current_idx].b_psia001
   LET g_psia_m.psia002 = g_browser[g_current_idx].b_psia002
   LET g_psia_m.psia003 = g_browser[g_current_idx].b_psia003
   LET g_psia_m.psia004 = g_browser[g_current_idx].b_psia004
   LET g_psia_m.psia005 = g_browser[g_current_idx].b_psia005
 
   
   #重讀DB,因TEMP有不被更新特性
   EXECUTE apst350_master_referesh USING g_psia_m.psiasite,g_psia_m.psia001,g_psia_m.psia002,g_psia_m.psia003, 
       g_psia_m.psia004,g_psia_m.psia005 INTO g_psia_m.psia001,g_psia_m.psia002,g_psia_m.psia003,g_psia_m.psia004, 
       g_psia_m.psia005,g_psia_m.psiasite,g_psia_m.psiastus,g_psia_m.psiaownid,g_psia_m.psiaowndp,g_psia_m.psiacrtid, 
       g_psia_m.psiacrtdp,g_psia_m.psiacrtdt,g_psia_m.psiamodid,g_psia_m.psiamoddt,g_psia_m.psiacnfid, 
       g_psia_m.psiacnfdt,g_psia_m.psia001_desc,g_psia_m.psia003_desc,g_psia_m.psia004_desc,g_psia_m.psiaownid_desc, 
       g_psia_m.psiaowndp_desc,g_psia_m.psiacrtid_desc,g_psia_m.psiacrtdp_desc,g_psia_m.psiamodid_desc, 
       g_psia_m.psiacnfid_desc
   
   #遮罩相關處理
   LET g_psia_m_mask_o.* =  g_psia_m.*
   CALL apst350_psia_t_mask()
   LET g_psia_m_mask_n.* =  g_psia_m.*
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL apst350_set_act_visible()   
   CALL apst350_set_act_no_visible()
   
   #add-point:fetch段action控制 name="fetch.action_control"
   CALL apst350_psib011_hide()      #數量欄位顯示/隱藏
   #end add-point  
   
   
   
   #add-point:fetch結束前 name="fetch.after"
   
   #end add-point
   
   #保存單頭舊值
   LET g_psia_m_t.* = g_psia_m.*
   LET g_psia_m_o.* = g_psia_m.*
   
   LET g_data_owner = g_psia_m.psiaownid      
   LET g_data_dept  = g_psia_m.psiaowndp
   
   #重新顯示   
   CALL apst350_show()
 
   
 
END FUNCTION
 
