{<section id="apst350.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION apst350_b_fill()
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   
   #end add-point     
   DEFINE p_wc2      STRING
   DEFINE li_idx     LIKE type_t.num10
   #add-point:b_fill段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   DEFINE l_seq      LIKE type_t.num5
   DEFINE l_psib006  LIKE psib_t.psib006
   DEFINE l_psib007  LIKE psib_t.psib007
   DEFINE l_psib008  LIKE psib_t.psib008
   DEFINE l_psib012  LIKE psib_t.psib012
   #end add-point     
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   #清空第一階單身
   CALL g_psib_d.clear()
 
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   LET l_psib006 = ''
   LET l_psib007 = ''
   LET l_psib012 = ''
   #end add-point
   
   #判斷是否填充
   IF apst350_fill_chk(1) THEN
      #切換上下筆時不重組SQL
      IF (g_action_choice = "query" OR cl_null(g_action_choice))
      #add-point:b_fill段long_sql_if name="b_fill.long_sql_if"
      
      #end add-point
      THEN
         LET g_sql = "SELECT  DISTINCT psib006,psib007,psib012,psib013,psib009,psib010,psib008,psib011 , 
             t1.imaal003 ,t2.imaal004 ,t3.oocal003 FROM psib_t",   
                     " INNER JOIN psia_t ON psiaent = " ||g_enterprise|| " AND psiasite = psibsite ",
                     " AND psia001 = psib001 ",
                     " AND psia002 = psib002 ",
                     " AND psia003 = psib003 ",
                     " AND psia004 = psib004 ",
                     " AND psia005 = psib005 ",
 
                     #"",
                     
                     "",
                     #下層單身所需的join條件
 
                                    " LEFT JOIN imaal_t t1 ON t1.imaalent="||g_enterprise||" AND t1.imaal001=psib006 AND t1.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t2 ON t2.imaalent="||g_enterprise||" AND t2.imaal001=psib006 AND t2.imaal002='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t3 ON t3.oocalent="||g_enterprise||" AND t3.oocal001=psib012 AND t3.oocal002='"||g_dlang||"' ",
 
                     " WHERE psibent=? AND psibsite=? AND psib001=? AND psib002=? AND psib003=? AND psib004=? AND psib005=?"
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before name="b_fill.body.fill_sql"
         
         #end add-point
         IF NOT cl_null(g_wc2_table1) THEN
            LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY psib_t.psib006,psib_t.psib007,psib_t.psib008,psib_t.psib013"
         
         #add-point:單身填充控制 name="b_fill.sql"
         
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE apst350_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR apst350_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_psia_m.psiasite,g_psia_m.psia001,g_psia_m.psia002,g_psia_m.psia003,g_psia_m.psia004,g_psia_m.psia005
                                               
      FOREACH b_fill_cs INTO g_psib_d[l_ac].psib006,g_psib_d[l_ac].psib007,g_psib_d[l_ac].psib012,g_psib_d[l_ac].psib013, 
          g_psib_d[l_ac].psib009,g_psib_d[l_ac].psib010,g_psib_d[l_ac].psib008,g_psib_d[l_ac].psib011, 
          g_psib_d[l_ac].psib006_desc,g_psib_d[l_ac].psib006_desc_desc,g_psib_d[l_ac].psib012_desc
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充 name="b_fill.fill"
         IF l_ac = 1 THEN
            LET l_psib006 = g_psib_d[l_ac].psib006                     #料件
            LET l_psib007 = g_psib_d[l_ac].psib007                     #產品特徵
            LET l_psib012 = g_psib_d[l_ac].psib012                     #單位
            LET g_psib_d[l_ac].l_psib011_01 = g_psib_d[l_ac].psib011   #數量  
            #160819-00015#3-s-add 取得 庫存量、請購量、採購未收量、採購在驗量、未展開累積預計採購數量 
            IF cl_null(g_psib_d[l_ac].psib013) THEN LET g_psib_d[l_ac].psib013 = 'N' END IF
            CALL apst350_get_psic(g_psia_m.psia001,g_psia_m.psia002,g_psia_m.psia003,g_psia_m.psia004,g_psia_m.psia005,
                                  g_psib_d[l_ac].psib006,g_psib_d[l_ac].psib007,g_psib_d[l_ac].psib013)
               RETURNING g_psib_d[l_ac].psic009,g_psib_d[l_ac].psic010,g_psib_d[l_ac].psic011,g_psib_d[l_ac].psic012,
                         g_psib_d[l_ac].psic013,g_psib_d[l_ac].l_psib011_63
            #160819-00015#3-e-add
         END IF
         LET g_psib_d[l_ac].seq = l_ac
         LET l_psib008 = g_psib_d[l_ac].psib008                        #期別
         #另起一行
         IF g_psib_d[l_ac].psib006 <> l_psib006 OR g_psib_d[l_ac].psib007 <> l_psib007 OR 
            g_psib_d[l_ac].psib012 <> l_psib012 THEN
            LET l_psib006 = g_psib_d[l_ac].psib006                     #料件
            LET l_psib007 = g_psib_d[l_ac].psib007                     #產品特徵
            LET l_psib012 = g_psib_d[l_ac].psib012                     #單位
            LET g_psib_d[l_ac].l_psib011_01 = g_psib_d[l_ac].psib011   #數量               
            #160819-00015#3-s-add 取得 庫存量、請購量、採購未收量、採購在驗量、未展開累積預計採購數量
            IF cl_null(g_psib_d[l_ac].psib013) THEN LET g_psib_d[l_ac].psib013 = 'N' END IF
            CALL apst350_get_psic(g_psia_m.psia001,g_psia_m.psia002,g_psia_m.psia003,g_psia_m.psia004,g_psia_m.psia005,
                                  g_psib_d[l_ac].psib006,g_psib_d[l_ac].psib007,g_psib_d[l_ac].psib013)
               RETURNING g_psib_d[l_ac].psic009,g_psib_d[l_ac].psic010,g_psib_d[l_ac].psic011,g_psib_d[l_ac].psic012,
                         g_psib_d[l_ac].psic013,g_psib_d[l_ac].l_psib011_63
            #160819-00015#3-e-add
         ELSE
            IF l_ac > 1 THEN
               LET l_ac = l_ac - 1
            END IF   
            IF l_psib008 > 1 THEN
               CALL apst350_b_desc(l_psib008,l_ac)
            END IF   
         END IF           
         
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF
            EXIT FOREACH
         END IF
         
         LET l_ac = l_ac + 1
      END FOREACH
      LET g_error_show = 0
   
   END IF
    
 
   
   #add-point:browser_fill段其他table處理 name="browser_fill.other_fill"
   
   #end add-point
   
   CALL g_psib_d.deleteElement(g_psib_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE apst350_pb
 
   
   LET li_idx = l_ac
   
   #遮罩相關處理
   FOR l_ac = 1 TO g_psib_d.getLength()
      LET g_psib_d_mask_o[l_ac].* =  g_psib_d[l_ac].*
      CALL apst350_psib_t_mask()
      LET g_psib_d_mask_n[l_ac].* =  g_psib_d[l_ac].*
   END FOR
   
 
   
   LET l_ac = li_idx
   
   CALL cl_ap_performance_next_end()
 
END FUNCTION
 
