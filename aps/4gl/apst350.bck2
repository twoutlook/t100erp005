PRIVATE FUNCTION apst350_conf_chk()
DEFINE  l_gzze001        LIKE gzze_t.gzze001
DEFINE  r_success        LIKE type_t.num5
DEFINE  l_cnt            LIKE type_t.num5

   LET r_success = TRUE
   IF cl_null(g_psia_m.psiasite) OR cl_null(g_psia_m.psia001) OR  cl_null(g_psia_m.psia002)  OR 
      g_psia_m.psia003 IS NULL OR g_psia_m.psia004 IS NULL OR cl_null(g_psia_m.psia005) THEN 
      LET r_success = FALSE
      LET l_gzze001 = 'axm-00202'
      RETURN r_success,l_gzze001
   END IF
   IF g_psia_m.psiastus <> 'N' THEN
      LET r_success = FALSE
      LET l_gzze001 = 'afa-00024'
      RETURN r_success,l_gzze001  
   END IF
   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt FROM psib_t
    WHERE psibent = g_enterprise
      AND psibsite = g_site
      AND psib001 = g_psia_m.psia001
      AND psib002 = g_psia_m.psia002
      AND psib003 = g_psia_m.psia003
      AND psib004 = g_psia_m.psia004
      AND psib005 = g_psia_m.psia005

   IF cl_null(l_cnt) THEN LET l_cnt = 0 END IF
   IF l_cnt = 0 THEN 
      LET r_success = FALSE
      LET l_gzze001 = 'axm-00261'
      RETURN r_success,l_gzze001        
   END IF   
   RETURN TRUE,''
END FUNCTION

