                  SELECT oobb005,oobb007 INTO g_master.sfaa057,g_oobb007
                    FROM oobb_t
                   WHERE oobbent = g_enterprise
                     AND oobb001 = g_ooef004
                     AND oobb002 = g_master.sfaadocno
                     AND oobb004 = 'sfaa057'
                  IF cl_null(g_master.sfaa057) THEN
                     LET g_master.sfaa057 = '1'
                  END IF
                  IF cl_null(g_oobb007) THEN
                     LET g_oobb007 = 'Y'
                  END IF
