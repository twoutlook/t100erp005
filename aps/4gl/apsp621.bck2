                       RETURNING l_success
                  IF NOT l_success THEN
                     LET g_master.sfaadocno = ''
                     DISPLAY BY NAME g_master.sfaadocno
                     DISPLAY '' TO sfaadocno_desc
                     NEXT FIELD CURRENT
                  END IF
                  
                  #顯示單別說明
                  CALL s_aooi200_get_slip_desc(g_master.sfaadocno)
                       RETURNING l_sfaadocno_desc
                  DISPLAY l_sfaadocno_desc TO sfaadocno_desc
                
                  #預設委外類型、可否修改
