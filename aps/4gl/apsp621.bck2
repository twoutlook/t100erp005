{<section id="apsp621.process" >}
#+ 資料處理   (r類使用g_master為主處理/p類使用l_param為主)
PRIVATE FUNCTION apsp621_process(ls_js)
 
   #add-point:process段define (客製用)
   
   #end add-point
   DEFINE ls_js         STRING
   DEFINE lc_param      type_parameter
   DEFINE li_stus       LIKE type_t.num5
   DEFINE li_count      LIKE type_t.num10  #progressbar計量
   DEFINE ls_sql        STRING             #主SQL
   DEFINE li_p01_status LIKE type_t.num5
   #add-point:process段define 
   #stellar：
   #特別注意：
   #1. type_parameter的record一定要和apsp620、s_apsp620一樣
   #2. 資料抓取的record一定要和apsp620、s_apsp620一樣
   DEFINE l_psos        RECORD
         sel             LIKE type_t.chr1,       #選擇
         psos054         LIKE psos_t.psos054,    #料號
         psos054_desc    LIKE type_t.chr80,      #品名
         psos054_desc_1  LIKE type_t.chr80,      #規格
         psos055         LIKE psos_t.psos055,    #產品特徵 
         #160512-00016#9 20160523 add by ming -----(S) 
         psos063         LIKE psos_t.psos063,    #保稅否  
         #160512-00016#9 20160523 add by ming -----(E) 
         imaf013         LIKE imaf_t.imaf013,    #補給策略
         imaa009         LIKE imaa_t.imaa009,    #產品分類
         imaa009_desc    LIKE type_t.chr80,      #說明
         imae011         LIKE imae_t.imae011,    #生管分類
         imae011_desc    LIKE type_t.chr80,      #說明
         psos036         LIKE psos_t.psos036,    #生產數量 
         qty             LIKE psos_t.psos036,    #本次生產數量 
         imae016         LIKE imae_t.imae016,    #單位
         imae016_desc    LIKE type_t.chr80,      #說明
         psos012         LIKE psos_t.psos012,    #預計開工日
         psos011         LIKE psos_t.psos011,    #預計完工日
         psos021         LIKE psos_t.psos021,    #需求單號
         imae012         LIKE imae_t.imae012,    #計劃員
         imae012_desc    LIKE type_t.chr80,      #全名 
         psos062         LIKE psos_t.psos062,    #已轉數量 
         psos057         LIKE psos_t.psos057,    #已轉單號
         psos058         LIKE psos_t.psos058,    #產生工單單號
         psos004         LIKE psos_t.psos004,    #APS虛擬單號
         imaa006         LIKE imaa_t.imaa006     #基礎單位
                         END RECORD
DEFINE l_sel_cnt         LIKE type_t.num5
DEFINE l_tot_success     LIKE type_t.num5
DEFINE l_success         LIKE type_t.num5
DEFINE l_str             STRING
DEFINE la_param          RECORD
          prog           STRING,
          param          DYNAMIC ARRAY OF STRING
                         END RECORD
DEFINE l_sfaadocno       LIKE sfaa_t.sfaadocno
DEFINE l_msg             STRING
DEFINE ls_js1            STRING
   #end add-point
 
  #INITIALIZE lc_param TO NULL           #p類不可以清空
   CALL util.JSON.parse(ls_js,lc_param)  #r類作業被t類呼叫時使用, p類主要解開參數處
   LET li_p01_status = 1
 
  #IF lc_param.wc IS NOT NULL THEN
  #   LET g_bgjob = "T"       #特殊情況,此為t類作業鬆耦合串入報表主程式使用
  #END IF
 
   #add-point:process段前處理
   IF cl_null(lc_param.wc) THEN
      LET lc_param.wc = " 1=1"
   END IF
   
   SELECT MAX(psos002) INTO g_psos002
     FROM psos_t
    WHERE psosent = g_enterprise
      AND psossite= g_site
      AND psos001 = lc_param.psos001
      AND psos009 = '1'
      
   #160512-00016#9 20160523 modify by ming -----(S) 
   #LET ls_sql = " SELECT 'Y',psos054,imaal003,imaal004,psos055,imaf013,imaa009,'',imae011,'',psos045, ",
   LET ls_sql = " SELECT 'Y',psos054,imaal003,imaal004,psos055,COALESCE(psos063,'N'),imaf013,imaa009,'',imae011,'',psos045, ",
   #160512-00016#9 20160523 modify by ming -----(E) 
                "        '',imae016,'',psos012,psos011,psos021,imae012,'',COALESCE(psos062,0),psos057,psos058,psos004, ",
                "        imaa006 ",
                "   FROM psos_t ",
                "   LEFT OUTER JOIN imaal_t ON imaalent = psosent AND imaal001 = psos054 AND imaal002 = '",g_dlang,"'",
                "   LEFT OUTER JOIN imaa_t ON imaaent = psosent AND imaa001 = psos054 ",
                "   LEFT OUTER JOIN imaf_t ON imafent = psosent AND imafsite= psossite AND imaf001 = psos054 ",
                "   LEFT OUTER JOIN imae_t ON imaeent = psosent AND imaesite= psossite AND imae001 = psos054 ",
                "   LEFT OUTER JOIN psoq_t ON psoqent = psosent AND psoqsite = psossite AND psoq001 = psos001 AND psoq002 = psos002 AND psoq004 = psos021 ",
                "  WHERE psosent = ",g_enterprise,
                "    AND psossite='",g_site,"'",
                "    AND psos001 ='",lc_param.psos001,"'",
                "    AND psos002 ='",g_psos002,"'",
                "    AND psos009 = 1 ",  #建議單據(is_new)
                "    AND ",lc_param.wc CLIPPED
                
   #已轉單據是否顯示
   IF lc_param.psos057 = 'N' THEN
      LET g_sql = g_sql CLIPPED," AND (COALESCE(psos062,0) < psos045 )"
   END IF
   
   #訂單延遲，是否不產生工單
   IF lc_param.l_delay = 'Y' THEN
      LET g_sql = g_sql CLIPPED," AND psoq024 < (psoq007+",lc_param.l_delay_days,") "
   END IF
   LET g_sql = g_sql CLIPPED," ORDER BY psos054,psos055 "
 
   DECLARE apsp621_process_cs CURSOR FROM ls_sql
   
   #end add-point
 
   #預先計算progressbar迴圈次數
   IF g_bgjob <> "Y" THEN
      #add-point:process段count_progress
      LET ls_sql = " SELECT COUNT(*) FROM (",ls_sql,")"
      
      PREPARE apsp621_process_cnt_cs FROM ls_sql
      EXECUTE apsp621_process_cnt_cs INTO li_count
      
      CALL cl_progress_bar_no_window(li_count)
      #end add-point
   END IF
 
   #主SQL及相關FOREACH前置處理
#  DECLARE apsp621_process_cs CURSOR FROM ls_sql
#  FOREACH apsp621_process_cs INTO
   #add-point:process段process
   LET l_sel_cnt = 0
   LET l_tot_success = TRUE
   LET l_success = TRUE
   LET l_str = ''
   CALL s_transaction_begin()
   CALL cl_err_collect_init()
   
   FOREACH apsp621_process_cs INTO 
      l_psos.sel,l_psos.psos054,l_psos.psos054_desc,l_psos.psos054_desc_1,
      #160512-00016#9 20160523 modify by ming -----(S) 
      #l_psos.psos055,l_psos.imaf013,l_psos.imaa009,l_psos.imaa009_desc, 
      l_psos.psos055,l_psos.psos063,
      l_psos.imaf013,l_psos.imaa009,l_psos.imaa009_desc,
      #160512-00016#9 20160523 modify by ming -----(E) 
      l_psos.imae011,l_psos.imae011_desc,l_psos.psos036,l_psos.qty,
      l_psos.imae016,
      l_psos.imae016_desc,l_psos.psos012,l_psos.psos011,l_psos.psos021,
      l_psos.imae012,l_psos.imae012_desc,l_psos.psos062,l_psos.psos057,
      l_psos.psos058,
      l_psos.psos004,
      l_psos.imaa006
      
      IF g_bgjob <> "Y" THEN
         CALL cl_progress_no_window_ing(l_msg)
      END IF
      
      IF NOT l_success THEN
         LET l_tot_success = FALSE
      END IF
      LET l_success = TRUE
      
      #累計選擇'Y'的筆數
      LET l_sel_cnt = l_sel_cnt + 1
      
      #數量由基礎單位轉換為生產單位的數量
      CALL s_aooi250_convert_qty(l_psos.psos054,l_psos.imaa006,
                                 l_psos.imae016,l_psos.psos036)
             RETURNING l_success,l_psos.psos036 
      IF NOT l_success THEN
         CONTINUE FOREACH
      END IF
             
      #預設 本次生產數量 
      #建議量-已轉數量的剩餘數量 
      LET l_psos.qty = l_psos.psos036 - l_psos.psos062
      
      #呼叫應用元件產生工單，並更新psos_t
      LET ls_js1 = util.JSON.stringify(l_psos)
      #傳入參數：APS版本、工單單別、委外、部門/廠商、單據是否確認、psos_t資料
      CALL s_apsp620_process(g_psos002,ls_js,ls_js1)
           RETURNING l_success,l_sfaadocno
      IF NOT l_success THEN
         CONTINUE FOREACH
      END IF
      
      IF NOT cl_null(l_sfaadocno) THEN
         IF cl_null(l_str) THEN
            LET l_str = " (sfaadocno = '", l_sfaadocno,"') "
         ELSE
            LET l_str = l_str," OR (sfaadocno = '", l_sfaadocno,"') "
         END IF
      END IF
   END FOREACH
   
   CALL cl_err_collect_show()
   LET l_success = l_tot_success
   
   IF cl_null(l_sel_cnt) OR l_sel_cnt = 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'sub-00491'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()

      CALL s_transaction_end('N','0')
      RETURN
   END IF
   #end add-point
#  END FOREACH
 
   IF g_bgjob = "N" THEN
      #前景作業完成處理
      #add-point:process段foreground完成處理
      IF l_success THEN
         CALL s_transaction_end('Y','0')
         IF NOT cl_null(l_str) THEN
            LET la_param.prog = 'asft300'
            LET la_param.param[1] = ''
            LET la_param.param[2] = ''
            LET la_param.param[3] = l_str
            LET ls_js = util.JSON.stringify(la_param )
            CALL cl_cmdrun_wait(ls_js)
         END IF
      ELSE
         CALL s_transaction_end('N','0')
      END IF
      #end add-point
      CALL cl_ask_confirm3("std-00012","")
   ELSE
      #背景作業完成處理
      #add-point:process段background完成處理
      IF l_success THEN
         CALL s_transaction_end('Y','0')
      ELSE
         CALL s_transaction_end('N','0')
      END IF
      #end add-point
      CALL cl_schedule_exec_call(li_p01_status)
   END IF
 
   #呼叫訊息中心傳遞本關完成訊息
   CALL apsp621_msgcentre_notify()
 
END FUNCTION
 
