                  LET g_master.sfaa017 = ''
                  DISPLAY BY NAME g_master.sfaa017
                  DISPLAY '' TO sfaa017_desc
               END IF
               LET l_sfaa057_o = g_master.sfaa057
               
            AFTER FIELD sfaa017
