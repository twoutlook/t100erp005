  <section id="abci100.reproduce" ver="2" status="" src="s" readonly="">
    <![CDATA[#+ 資料複製
PRIVATE FUNCTION abci100_reproduce()
   DEFINE l_newno     LIKE bcpa_t.bcpasite 
   DEFINE l_oldno     LIKE bcpa_t.bcpasite 
   DEFINE l_newno02     LIKE bcpa_t.bcpa001 
   DEFINE l_oldno02     LIKE bcpa_t.bcpa001 
 
   DEFINE l_master    RECORD LIKE bcpa_t.*
   DEFINE l_detail    RECORD LIKE bcpb_t.*
 
 
   DEFINE l_cnt       LIKE type_t.num10
   #add-point:reproduce段define
   {<point name="reproduce.define" edit="s"/>}
   #end add-point   
   #add-point:reproduce段define(客製用)
   {<point name="reproduce.define_customerization" edit="c"/>}
   #end add-point   
   
   #切換畫面
   
   LET g_master_insert = FALSE
   
   IF g_bcpa_m.bcpasite IS NULL
   OR g_bcpa_m.bcpa001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
    
   LET g_bcpasite_t = g_bcpa_m.bcpasite
   LET g_bcpa001_t = g_bcpa_m.bcpa001
 
    
   LET g_bcpa_m.bcpasite = ""
   LET g_bcpa_m.bcpa001 = ""
 
 
   CALL cl_set_head_visible("","YES")
 
   #公用欄位給予預設值
   #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_bcpa_m.bcpaownid = g_user
      LET g_bcpa_m.bcpaowndp = g_dept
      LET g_bcpa_m.bcpacrtid = g_user
      LET g_bcpa_m.bcpacrtdp = g_dept 
      LET g_bcpa_m.bcpacrtdt = cl_get_current()
      LET g_bcpa_m.bcpamodid = g_user
      LET g_bcpa_m.bcpamoddt = cl_get_current()
      LET g_bcpa_m.bcpastus = 'Y'
 
 
   
   CALL s_transaction_begin()
   
   #add-point:複製輸入前
   {<point name="reproduce.head.b_input"/>}
   #end add-point
   
   #顯示狀態(stus)圖片
         #應用 a21 樣板自動產生(Version:2)
	  #根據當下狀態碼顯示圖片
      CASE g_bcpa_m.bcpastus 
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
         
      END CASE
 
 
   
   #清空key欄位的desc
      LET g_bcpa_m.bcpasite_desc = ''
   DISPLAY BY NAME g_bcpa_m.bcpasite_desc
   LET g_bcpa_m.bcpa001_desc = ''
   DISPLAY BY NAME g_bcpa_m.bcpa001_desc
 
   
   CALL abci100_input("r")
   
   IF INT_FLAG AND NOT g_master_insert THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = '' 
      LET g_errparam.code   = 9001 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      CALL s_transaction_end('N','0')
      LET INT_FLAG = 0
      DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
      DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
      LET INT_FLAG = 0
      INITIALIZE g_bcpa_m.* TO NULL
      INITIALIZE g_bcpb_d TO NULL
 
      #add-point:複製取消後
      {<point name="reproduce.cancel" />}
      #end add-point
      CALL abci100_show()
      RETURN
   END IF
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL abci100_set_act_visible()   
   CALL abci100_set_act_no_visible()
   
   #將新增的資料併入搜尋條件中
   LET g_bcpasite_t = g_bcpa_m.bcpasite
   LET g_bcpa001_t = g_bcpa_m.bcpa001
 
   
   #組合新增資料的條件
   LET g_add_browse = " bcpaent = '" ||g_enterprise|| "' AND",
                      " bcpasite = '", g_bcpa_m.bcpasite, "' "
                      ," AND bcpa001 = '", g_bcpa_m.bcpa001, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL abci100_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   #add-point:完成複製段落後
   {<point name="reproduce.after_reproduce" />}
   #end add-point
   
   CALL abci100_idx_chk()
   
   #功能已完成,通報訊息中心
   CALL abci100_msgcentre_notify('reproduce')
 
END FUNCTION
]]>
  </section>
