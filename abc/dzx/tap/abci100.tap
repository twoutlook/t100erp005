  <section id="abci100.state_change" ver="2" status="" src="s" readonly="">
    <![CDATA[   #應用 a09 樣板自動產生(Version:12)
#+ 確認碼變更 
PRIVATE FUNCTION abci100_statechange()
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define
   {<point name="statechange.define" edit="s"/>}
   #end add-point  
   #add-point:statechange段define(客製用)
   {<point name="statechange.define_customerization" edit="c"/>}
   #end add-point  
   
   #add-point:statechange段開始前
   {<point name="statechange.before"/>}
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_bcpa_m.bcpasite IS NULL
      OR g_bcpa_m.bcpa001 IS NULL
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN abci100_cl USING g_enterprise,g_bcpa_m.bcpasite,g_bcpa_m.bcpa001
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN abci100_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE abci100_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE abci100_master_referesh USING g_bcpa_m.bcpasite,g_bcpa_m.bcpa001 INTO g_bcpa_m.bcpasite,g_bcpa_m.bcpa001, 
       g_bcpa_m.bcpastus,g_bcpa_m.bcpaownid,g_bcpa_m.bcpaowndp,g_bcpa_m.bcpacrtid,g_bcpa_m.bcpacrtdp, 
       g_bcpa_m.bcpacrtdt,g_bcpa_m.bcpamodid,g_bcpa_m.bcpamoddt,g_bcpa_m.bcpasite_desc,g_bcpa_m.bcpa001_desc, 
       g_bcpa_m.bcpaownid_desc,g_bcpa_m.bcpaowndp_desc,g_bcpa_m.bcpacrtid_desc,g_bcpa_m.bcpacrtdp_desc, 
       g_bcpa_m.bcpamodid_desc
 
   #檢查是否允許此動作
   IF NOT abci100_action_chk() THEN
      CLOSE abci100_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_bcpa_m.bcpasite,g_bcpa_m.bcpasite_desc,g_bcpa_m.bcpa001,g_bcpa_m.bcpa001_desc,g_bcpa_m.bcpastus, 
       g_bcpa_m.bcpaownid,g_bcpa_m.bcpaownid_desc,g_bcpa_m.bcpaowndp,g_bcpa_m.bcpaowndp_desc,g_bcpa_m.bcpacrtid, 
       g_bcpa_m.bcpacrtid_desc,g_bcpa_m.bcpacrtdp,g_bcpa_m.bcpacrtdp_desc,g_bcpa_m.bcpacrtdt,g_bcpa_m.bcpamodid, 
       g_bcpa_m.bcpamodid_desc,g_bcpa_m.bcpamoddt
 
   CASE g_bcpa_m.bcpastus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
      
   END CASE
 
   #add-point:資料刷新後
   {<point name="statechange.after_refresh"/>}
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_bcpa_m.bcpastus
            
            WHEN "N"
               HIDE OPTION "inactive"
            WHEN "Y"
               HIDE OPTION "active"
         END CASE
     
      #add-point:menu前
      {<point name="statechange.before_menu"/>}
      #end add-point
      
      
	  
      ON ACTION inactive
         IF cl_auth_chk_act("inactive") THEN
            LET lc_state = "N"
            #add-point:action控制
            {<point name="statechange.inactive"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION active
         IF cl_auth_chk_act("active") THEN
            LET lc_state = "Y"
            #add-point:action控制
            {<point name="statechange.active"/>}
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制
      {<point name="statechange.more_control"/>}
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      ) OR 
      g_bcpa_m.bcpastus = lc_state OR cl_null(lc_state) THEN
      CLOSE abci100_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前
   {<point name="statechange.b_update"/>}
   #end add-point
   
   LET g_bcpa_m.bcpamodid = g_user
   LET g_bcpa_m.bcpamoddt = cl_get_current()
   LET g_bcpa_m.bcpastus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE bcpa_t 
      SET (bcpastus,bcpamodid,bcpamoddt) 
        = (g_bcpa_m.bcpastus,g_bcpa_m.bcpamodid,g_bcpa_m.bcpamoddt)     
    WHERE bcpaent = g_enterprise AND bcpasite = g_bcpa_m.bcpasite
      AND bcpa001 = g_bcpa_m.bcpa001
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE abci100_master_referesh USING g_bcpa_m.bcpasite,g_bcpa_m.bcpa001 INTO g_bcpa_m.bcpasite, 
          g_bcpa_m.bcpa001,g_bcpa_m.bcpastus,g_bcpa_m.bcpaownid,g_bcpa_m.bcpaowndp,g_bcpa_m.bcpacrtid, 
          g_bcpa_m.bcpacrtdp,g_bcpa_m.bcpacrtdt,g_bcpa_m.bcpamodid,g_bcpa_m.bcpamoddt,g_bcpa_m.bcpasite_desc, 
          g_bcpa_m.bcpa001_desc,g_bcpa_m.bcpaownid_desc,g_bcpa_m.bcpaowndp_desc,g_bcpa_m.bcpacrtid_desc, 
          g_bcpa_m.bcpacrtdp_desc,g_bcpa_m.bcpamodid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_bcpa_m.bcpasite,g_bcpa_m.bcpasite_desc,g_bcpa_m.bcpa001,g_bcpa_m.bcpa001_desc, 
          g_bcpa_m.bcpastus,g_bcpa_m.bcpaownid,g_bcpa_m.bcpaownid_desc,g_bcpa_m.bcpaowndp,g_bcpa_m.bcpaowndp_desc, 
          g_bcpa_m.bcpacrtid,g_bcpa_m.bcpacrtid_desc,g_bcpa_m.bcpacrtdp,g_bcpa_m.bcpacrtdp_desc,g_bcpa_m.bcpacrtdt, 
          g_bcpa_m.bcpamodid,g_bcpa_m.bcpamodid_desc,g_bcpa_m.bcpamoddt
   END IF
 
   #add-point:stus修改後
   {<point name="statechange.a_update"/>}
   #end add-point
 
   #add-point:statechange段結束前
   {<point name="statechange.after"/>}
   #end add-point  
 
   CLOSE abci100_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL abci100_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION
 
 
]]>
  </section>
