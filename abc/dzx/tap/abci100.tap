               IF SQLCA.sqlcode THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "g_bcpa_m" 
                  LET g_errparam.code   = SQLCA.sqlcode 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  CALL s_transaction_end('N','0')
                  NEXT FIELD CURRENT
               END IF
               
               #add-point:單頭新增中
               {<point name="input.head.m_insert"/>}
               #end add-point
               
               
               
               
               #add-point:單頭新增後
               {<point name="input.head.a_insert"/>}
               #end add-point
               CALL s_transaction_end('Y','0') 
               
               IF l_cmd_t = 'r' AND p_cmd = 'a' THEN
                  CALL abci100_detail_reproduce()
                  #因應特定程式需求, 重新刷新單身資料
                  CALL abci100_b_fill()
                  CALL abci100_b_fill2('0')
               END IF
               
               #add-point:單頭新增後
               {<point name="input.head.a_insert2"/>}
               #end add-point
               
               LET g_master_insert = TRUE
               
               LET p_cmd = 'u'
            ELSE
               CALL s_transaction_begin()
            
               #add-point:單頭修改前
               {<point name="input.head.b_update" mark="Y"/>}
               #end add-point
               
               #將遮罩欄位還原
               CALL abci100_bcpa_t_mask_restore('restore_mask_o')
               
               UPDATE bcpa_t SET (bcpasite,bcpa001,bcpastus,bcpaownid,bcpaowndp,bcpacrtid,bcpacrtdp, 
                   bcpacrtdt,bcpamodid,bcpamoddt) = (g_bcpa_m.bcpasite,g_bcpa_m.bcpa001,g_bcpa_m.bcpastus, 
                   g_bcpa_m.bcpaownid,g_bcpa_m.bcpaowndp,g_bcpa_m.bcpacrtid,g_bcpa_m.bcpacrtdp,g_bcpa_m.bcpacrtdt, 
                   g_bcpa_m.bcpamodid,g_bcpa_m.bcpamoddt)
                WHERE bcpaent = g_enterprise AND bcpasite = g_bcpasite_t
                  AND bcpa001 = g_bcpa001_t
 
               IF SQLCA.sqlcode THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "bcpa_t" 
                  LET g_errparam.code   = SQLCA.sqlcode 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  CALL s_transaction_end('N','0')
                  NEXT FIELD CURRENT
               END IF
               
               #add-point:單頭修改中
               {<point name="input.head.m_update"/>}
               #end add-point
               
               
               
               
               #將遮罩欄位進行遮蔽
               CALL abci100_bcpa_t_mask_restore('restore_mask_n')
               
               #修改歷程記錄(單頭修改)
               LET g_log1 = util.JSON.stringify(g_bcpa_m_t)
               LET g_log2 = util.JSON.stringify(g_bcpa_m)
               IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                  CALL s_transaction_end('N','0')
               ELSE
                  CALL s_transaction_end('Y','0')
               END IF
               
               #add-point:單頭修改後
               {<point name="input.head.a_update"/>}
               #end add-point
            END IF
            
            LET g_bcpasite_t = g_bcpa_m.bcpasite
            LET g_bcpa001_t = g_bcpa_m.bcpa001
 
            
      END INPUT
   
]]>
