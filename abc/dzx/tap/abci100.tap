  <section id="abci100.modify" ver="2" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION abci100_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   DEFINE l_wc2_table1          STRING
 
 
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point    
   #add-point:modify段define(客製用)
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point    
   
   #保存單頭舊值
   LET g_bcpa_m_t.* = g_bcpa_m.*
   LET g_bcpa_m_o.* = g_bcpa_m.*
   
   IF g_bcpa_m.bcpasite IS NULL
   OR g_bcpa_m.bcpa001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_bcpasite_t = g_bcpa_m.bcpasite
   LET g_bcpa001_t = g_bcpa_m.bcpa001
 
   CALL s_transaction_begin()
   
   OPEN abci100_cl USING g_enterprise,g_bcpa_m.bcpasite,g_bcpa_m.bcpa001
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN abci100_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE abci100_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE abci100_master_referesh USING g_bcpa_m.bcpasite,g_bcpa_m.bcpa001 INTO g_bcpa_m.bcpasite,g_bcpa_m.bcpa001, 
       g_bcpa_m.bcpastus,g_bcpa_m.bcpaownid,g_bcpa_m.bcpaowndp,g_bcpa_m.bcpacrtid,g_bcpa_m.bcpacrtdp, 
       g_bcpa_m.bcpacrtdt,g_bcpa_m.bcpamodid,g_bcpa_m.bcpamoddt,g_bcpa_m.bcpasite_desc,g_bcpa_m.bcpa001_desc, 
       g_bcpa_m.bcpaownid_desc,g_bcpa_m.bcpaowndp_desc,g_bcpa_m.bcpacrtid_desc,g_bcpa_m.bcpacrtdp_desc, 
       g_bcpa_m.bcpamodid_desc
   
   #檢查是否允許此動作
   IF NOT abci100_action_chk() THEN
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_bcpa_m_mask_o.* =  g_bcpa_m.*
   CALL abci100_bcpa_t_mask()
   LET g_bcpa_m_mask_n.* =  g_bcpa_m.*
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
 
   
   CALL abci100_show()
   #add-point:modify段show之後
   {<point name="modify.after_show"/>}
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
 
 
    
   WHILE TRUE
      LET g_bcpasite_t = g_bcpa_m.bcpasite
      LET g_bcpa001_t = g_bcpa_m.bcpa001
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_bcpa_m.bcpamodid = g_user 
LET g_bcpa_m.bcpamoddt = cl_get_current()
LET g_bcpa_m.bcpamodid_desc = cl_get_username(g_bcpa_m.bcpamodid)
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      CALL abci100_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE bcpa_t SET (bcpamodid,bcpamoddt) = (g_bcpa_m.bcpamodid,g_bcpa_m.bcpamoddt)
          WHERE bcpaent = g_enterprise AND bcpasite = g_bcpasite_t
            AND bcpa001 = g_bcpa001_t
 
      END IF
    
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_bcpa_m.* = g_bcpa_m_t.*
         CALL abci100_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_bcpa_m.bcpasite != g_bcpasite_t 
      OR g_bcpa_m.bcpa001 != g_bcpa001_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE bcpb_t SET bcpbsite = g_bcpa_m.bcpasite
                                       ,bcpb001 = g_bcpa_m.bcpa001
 
          WHERE bcpbent = g_enterprise AND bcpbsite = g_bcpasite_t
            AND bcpb001 = g_bcpa001_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "bcpb_t" 
               LET g_errparam.code   = "std-00009" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "bcpb_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL abci100_set_act_visible()   
   CALL abci100_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " bcpaent = '" ||g_enterprise|| "' AND",
                      " bcpasite = '", g_bcpa_m.bcpasite, "' "
                      ," AND bcpa001 = '", g_bcpa_m.bcpa001, "' "
 
   #填到對應位置
   CALL abci100_browser_fill("")
 
   CLOSE abci100_cl
   
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL abci100_msgcentre_notify('modify')
 
END FUNCTION   
]]>
  </section>
