  <point name="function.abci100_bcpb002_change" order="1" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 更改順序
# Memo...........:
# Usage..........: CALL abci100_bcpb002_change(p_cmd)
#                  RETURNING r_success
# Input parameter: p_cmd          更改方式(1=提升順序 2=降低順序)
# Return code....: r_sccess       成功否
# Date & Author..: 2015/05/29 By  shiun
# Modify.........:
################################################################################
PRIVATE FUNCTION abci100_bcpb002_change(p_cmd)
DEFINE p_cmd      LIKE type_t.chr1
DEFINE r_success  LIKE type_t.num5
DEFINE l_ac_count LIKE type_t.num5
DEFINE l_bcpb     RECORD LIKE bcpb_t.* 
   
   LET r_success = TRUE
   CALL s_transaction_begin()
   IF p_cmd = '1' THEN
      LET l_ac_count = l_ac - 1
   ELSE
      LET l_ac_count = l_ac + 1
   END IF
   LET l_bcpb.bcpb003 = g_bcpb_d[l_ac].bcpb003
   LET l_bcpb.bcpb004 = g_bcpb_d[l_ac].bcpb004
   LET l_bcpb.bcpb005 = g_bcpb_d[l_ac].bcpb005
   LET l_bcpb.bcpb006 = g_bcpb_d[l_ac].bcpb006
   UPDATE bcpb_t SET (bcpb003,bcpb004,bcpb005,bcpb006) 
                   = (g_bcpb_d[l_ac_count].bcpb003,g_bcpb_d[l_ac_count].bcpb004,
                      g_bcpb_d[l_ac_count].bcpb005,g_bcpb_d[l_ac_count].bcpb006)
    WHERE bcpbent = g_enterprise
      AND bcpbsite = g_bcpa_m.bcpasite
      AND bcpb001 = g_bcpa_m.bcpa001
      AND bcpb002 = g_bcpb_d[l_ac].bcpb002
      
    IF SQLCA.sqlcode THEN
       CALL s_transaction_end('N','0')
       LET r_success = FALSE
       RETURN r_success
    END IF
    
    UPDATE bcpb_t SET (bcpb003,bcpb004,bcpb005,bcpb006) 
                    = (l_bcpb.bcpb003,l_bcpb.bcpb004,
                       l_bcpb.bcpb005,l_bcpb.bcpb006)
     WHERE bcpbent = g_enterprise
       AND bcpbsite = g_bcpa_m.bcpasite
       AND bcpb001 = g_bcpa_m.bcpa001
       AND bcpb002 = g_bcpb_d[l_ac_count].bcpb002
       
    IF SQLCA.sqlcode THEN
       CALL s_transaction_end('N','0')
       LET r_success = FALSE
       RETURN r_success
    END IF
    CALL s_transaction_end('Y','0')
    RETURN r_success
END FUNCTION]]>
  </point>
  <point name="function.abci100_bcpb003_chk" order="2" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION abci100_bcpb003_chk()
DEFINE l_count    LIKE type_t.num5
DEFINE r_success  LIKE type_t.num5

   LET l_count = 0
   LET r_success = FALSE
   
   SELECT COUNT(*) INTO l_count
     FROM bcpb_t
    WHERE bcpbent = g_enterprise
      AND bcpbsite = g_bcpa_m.bcpasite
      AND bcpb001 = g_bcpa_m.bcpa001
      AND bcpb003 = g_bcpb_d[l_ac].bcpb003
      
   IF l_count = 0 THEN
      LET r_success = TRUE
   END IF
   
   RETURN r_success
END FUNCTION]]>
