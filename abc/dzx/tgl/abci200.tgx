{<section id="abci200.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION abci200_b_fill(p_wc2)              #BODY FILL UP
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   {<point name="b_fill.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE p_wc2            STRING
   DEFINE ls_owndept_list  STRING  #(ver:35) add
   DEFINE ls_ownuser_list  STRING  #(ver:35) add
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   {<point name="b_fill.define"/>}
   #end add-point
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   {<point name="b_fill.pre_function"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前 name="b_fill.sql_before"
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  DISTINCT t0.bcahsite,t0.bcah008,t0.bcah009,t0.bcahdocno,t0.bcahseq,t0.bcah001,t0.bcah002,t0.bcah003,t0.bcah004,t0.bcah005,t0.bcah006,t0.bcah007,t0.bcah010,t0.bcah011,t0.bcah012,t0.bcah013,t0.bcah014,t0.bcah016,t0.bcah017,t0.bcah018,t0.bcah019,t0.bcah020,t0.bcah021,t0.bcah022,t0.bcah023,t0.bcah024,t0.bcah025,t0.bcah026,t0.bcah027,t0.bcah028,t0.bcah029,t0.bcah030,t0.bcah031,t0.bcah032,t0.bcah033,t0.bcah034,t0.bcah035,t0.bcahstus,t0.bcah015,t0.bcahsite,t0.bcahdocno,t0.bcahseq,t0.bcah001,t0.bcahownid,t0.bcahowndp,t0.bcahcrtid,t0.bcahcrtdp,t0.bcahcrtdt,t0.bcahmodid,t0.bcahmoddt,t0.bcahcnfid,t0.bcahcnfdt ,t1.imaal003 ,t2.inayl003 ,t3.inab003 ,t4.oocal003 ,t5.oocal003 ,t6.ooag011 ,t7.ooag011 ,t8.ooag011 ,t9.ooag011 ,t10.ooag011 ,t11.ooefl003 ,t12.ooag011 ,t13.ooefl003 ,t14.ooag011 ,t15.ooag011 FROM bcah_t t0",
               "",
                              " LEFT JOIN imaal_t t1 ON t1.imaalent="||g_enterprise||" AND t1.imaal001=t0.bcah002 AND t1.imaal002='"||g_dlang||"' ",
               " LEFT JOIN inayl_t t2 ON t2.inaylent="||g_enterprise||" AND t2.inayl001=t0.bcah005 AND t2.inayl002='"||g_dlang||"' ",
               " LEFT JOIN inab_t t3 ON t3.inabent="||g_enterprise||" AND t3.inabsite=t0.bcahsite AND t3.inab001=t0.bcah005 AND t3.inab002=t0.bcah006  ",
               " LEFT JOIN oocal_t t4 ON t4.oocalent="||g_enterprise||" AND t4.oocal001=t0.bcah010 AND t4.oocal002='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t5 ON t5.oocalent="||g_enterprise||" AND t5.oocal001=t0.bcah012 AND t5.oocal002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent="||g_enterprise||" AND t6.ooag001=t0.bcah018  ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent="||g_enterprise||" AND t7.ooag001=t0.bcah022  ",
               " LEFT JOIN ooag_t t8 ON t8.ooagent="||g_enterprise||" AND t8.ooag001=t0.bcah028  ",
               " LEFT JOIN ooag_t t9 ON t9.ooagent="||g_enterprise||" AND t9.ooag001=t0.bcah032  ",
               " LEFT JOIN ooag_t t10 ON t10.ooagent="||g_enterprise||" AND t10.ooag001=t0.bcahownid  ",
               " LEFT JOIN ooefl_t t11 ON t11.ooeflent="||g_enterprise||" AND t11.ooefl001=t0.bcahowndp AND t11.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t12 ON t12.ooagent="||g_enterprise||" AND t12.ooag001=t0.bcahcrtid  ",
               " LEFT JOIN ooefl_t t13 ON t13.ooeflent="||g_enterprise||" AND t13.ooefl001=t0.bcahcrtdp AND t13.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t14 ON t14.ooagent="||g_enterprise||" AND t14.ooag001=t0.bcahmodid  ",
               " LEFT JOIN ooag_t t15 ON t15.ooagent="||g_enterprise||" AND t15.ooag001=t0.bcahcnfid  ",

               " WHERE t0.bcahent= ?  AND  1=1 AND (", p_wc2, ") "
 
   #(ver:35) ---add start---
      #應用 a68 樣板自動產生(Version:1)
   #若是修改，須視權限加上條件
   IF g_action_choice = "modify" OR g_action_choice = "modify_detail" THEN
      LET ls_owndept_list = NULL
      LET ls_ownuser_list = NULL

      #若有設定部門權限
      CALL cl_get_owndept_list("bcah_t","modify") RETURNING ls_owndept_list
      IF NOT cl_null(ls_owndept_list) THEN
         LET g_sql = g_sql, " AND bcahowndp IN (",ls_owndept_list,")"
      END IF

      #若有設定個人權限
      CALL cl_get_ownuser_list("bcah_t","modify") RETURNING ls_ownuser_list
      IF NOT cl_null(ls_ownuser_list) THEN
         LET g_sql = g_sql," AND bcahownid IN (",ls_ownuser_list,")"
      END IF
   END IF



   #(ver:35) --- add end ---
 
   #add-point:b_fill段sql wc name="b_fill.sql_wc"
   {<point name="b_fill.sql_wc"/>}
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("bcah_t"),
                      " ORDER BY t0.bcahsite,t0.bcahdocno,t0.bcahseq,t0.bcah001"
   
   #add-point:b_fill段sql之後 name="b_fill.sql_after"
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"bcah_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE abci200_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR abci200_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_bcah_d.clear()
   CALL g_bcah2_d.clear()   

 
   LET g_cnt = l_ac
