               LET gs_keys_bak[4] = g_bcah_d_t.bcah001
               CALL abci200_update_b('bcah_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
                     #修改歷程記錄(修改)
