PRIVATE FUNCTION abcr002_bcaa002_ref()
   
   INITIALIZE g_master.bcaa002_desc,g_master.bcaa002_desc_1,g_master.bcaa002_desc_2 TO NULL
   CALL s_desc_get_item_desc(g_master.b_bcaa002) RETURNING g_master.bcaa002_desc,g_master.bcaa002_desc_1
   SELECT imaf053 INTO g_master.bcaa002_desc_2
     FROM imaf_t
    WHERE imafent = g_enterprise
      AND imafsite = g_site
      AND imaf001 = g_master.b_bcaa002
   DISPLAY BY NAME g_master.bcaa002_desc,g_master.bcaa002_desc_1,g_master.bcaa002_desc_2
END FUNCTION

