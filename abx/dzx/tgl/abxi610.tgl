   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("abx","")
 
   #add-point:作業初始化 name="main.init"
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define name="main.define_sql"
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT bxfbsite,bxfb001,'','',bxfb002,bxfb003,bxfb004,bxfb005,bxfb006,bxfb007, 
       bxfbstus,bxfbownid,'',bxfbowndp,'',bxfbcrtid,'',bxfbcrtdp,'',bxfbcrtdt,bxfbmodid,'',bxfbmoddt, 
       bxfb011,bxfb015,bxfb016,bxfb017,bxfb020,bxfb021,bxfb022,bxfb012,bxfb013,bxfb014,bxfb018,bxfb019", 
        
                      " FROM bxfb_t",
                      " WHERE bxfbent= ? AND bxfbsite=? AND bxfb001=? AND bxfb002=? AND bxfb003=? AND  
                          bxfb004=? AND bxfb005=? AND bxfb006=? AND bxfb007=? FOR UPDATE"
   #add-point:SQL_define name="main.after_define_sql"
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE abxi610_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.bxfbsite,t0.bxfb001,t0.bxfb002,t0.bxfb003,t0.bxfb004,t0.bxfb005, 
       t0.bxfb006,t0.bxfb007,t0.bxfbstus,t0.bxfbownid,t0.bxfbowndp,t0.bxfbcrtid,t0.bxfbcrtdp,t0.bxfbcrtdt, 
       t0.bxfbmodid,t0.bxfbmoddt,t0.bxfb011,t0.bxfb015,t0.bxfb016,t0.bxfb017,t0.bxfb020,t0.bxfb021,t0.bxfb022, 
       t0.bxfb012,t0.bxfb013,t0.bxfb014,t0.bxfb018,t0.bxfb019,t1.imaal003 ,t2.ooag011 ,t3.ooefl003 , 
       t4.ooag011 ,t5.ooefl003 ,t6.ooag011",
               " FROM bxfb_t t0",
                              " LEFT JOIN imaal_t t1 ON t1.imaalent="||g_enterprise||" AND t1.imaal001=t0.bxfb001 AND t1.imaal002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t2 ON t2.ooagent="||g_enterprise||" AND t2.ooag001=t0.bxfbownid  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent="||g_enterprise||" AND t3.ooefl001=t0.bxfbowndp AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t4 ON t4.ooagent="||g_enterprise||" AND t4.ooag001=t0.bxfbcrtid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent="||g_enterprise||" AND t5.ooefl001=t0.bxfbcrtdp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent="||g_enterprise||" AND t6.ooag001=t0.bxfbmodid  ",
 
               " WHERE t0.bxfbent = " ||g_enterprise|| " AND t0.bxfbsite = ? AND t0.bxfb001 = ? AND t0.bxfb002 = ? AND t0.bxfb003 = ? AND t0.bxfb004 = ? AND t0.bxfb005 = ? AND t0.bxfb006 = ? AND t0.bxfb007 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define name="main.after_refresh_sql"
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE abxi610_master_referesh FROM g_sql
 
    
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call name="main.servicecall"
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_abxi610 WITH FORM cl_ap_formpath("abx",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL abxi610_init()   
 
      #進入選單 Menu (="N")
      CALL abxi610_ui_dialog() 
      
      #add-point:畫面關閉前 name="main.before_close"
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_abxi610
      
   END IF 
   
   CLOSE abxi610_cl
   
   
 
   #add-point:作業離開前 name="main.exit"
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
END MAIN
 
 
 

