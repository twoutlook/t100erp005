   {<point name="show.define"/>}
   #end add-point  
   
   #add-point:show段Function前置處理  name="show.before"
   {<point name="show.before"/>}
   #end add-point
   
   
   
   #帶出公用欄位reference值
   #應用 a12 樣板自動產生(Version:4)
 
 
 
    
