{<section id="abxi610.modify" >}
#+ 資料修改
PRIVATE FUNCTION abxi610_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   {<point name="modify.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   {<point name="modify.define"/>}
   #end add-point
   
   #add-point:Function前置處理 name="modify.pre_function"
   {<point name="modify.pre_function"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_bxfb_m.bxfbsite IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_bxfbsite_t = g_bxfb_m.bxfbsite
   LET g_bxfb001_t = g_bxfb_m.bxfb001
   LET g_bxfb002_t = g_bxfb_m.bxfb002
   LET g_bxfb003_t = g_bxfb_m.bxfb003
   LET g_bxfb004_t = g_bxfb_m.bxfb004
   LET g_bxfb005_t = g_bxfb_m.bxfb005
   LET g_bxfb006_t = g_bxfb_m.bxfb006
   LET g_bxfb007_t = g_bxfb_m.bxfb007
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN abxi610_cl USING g_enterprise,g_bxfb_m.bxfbsite,g_bxfb_m.bxfb001,g_bxfb_m.bxfb002,g_bxfb_m.bxfb003,g_bxfb_m.bxfb004,g_bxfb_m.bxfb005,g_bxfb_m.bxfb006,g_bxfb_m.bxfb007
   IF SQLCA.SQLCODE THEN    #(ver:49)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN abxi610_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE
      LET g_errparam.popup = TRUE 
      CLOSE abxi610_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE abxi610_master_referesh USING g_bxfb_m.bxfbsite,g_bxfb_m.bxfb001,g_bxfb_m.bxfb002,g_bxfb_m.bxfb003, 
       g_bxfb_m.bxfb004,g_bxfb_m.bxfb005,g_bxfb_m.bxfb006,g_bxfb_m.bxfb007 INTO g_bxfb_m.bxfbsite,g_bxfb_m.bxfb001, 
       g_bxfb_m.bxfb002,g_bxfb_m.bxfb003,g_bxfb_m.bxfb004,g_bxfb_m.bxfb005,g_bxfb_m.bxfb006,g_bxfb_m.bxfb007, 
       g_bxfb_m.bxfbstus,g_bxfb_m.bxfbownid,g_bxfb_m.bxfbowndp,g_bxfb_m.bxfbcrtid,g_bxfb_m.bxfbcrtdp, 
       g_bxfb_m.bxfbcrtdt,g_bxfb_m.bxfbmodid,g_bxfb_m.bxfbmoddt,g_bxfb_m.bxfb011,g_bxfb_m.bxfb015,g_bxfb_m.bxfb016, 
       g_bxfb_m.bxfb017,g_bxfb_m.bxfb020,g_bxfb_m.bxfb021,g_bxfb_m.bxfb022,g_bxfb_m.bxfb012,g_bxfb_m.bxfb013, 
       g_bxfb_m.bxfb014,g_bxfb_m.bxfb018,g_bxfb_m.bxfb019,g_bxfb_m.bxfb001_desc,g_bxfb_m.bxfbownid_desc, 
       g_bxfb_m.bxfbowndp_desc,g_bxfb_m.bxfbcrtid_desc,g_bxfb_m.bxfbcrtdp_desc,g_bxfb_m.bxfbmodid_desc 

 
   #檢查是否允許此動作
   IF NOT abxi610_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_bxfb_m_mask_o.* =  g_bxfb_m.*
   CALL abxi610_bxfb_t_mask()
   LET g_bxfb_m_mask_n.* =  g_bxfb_m.*
   
   
 
   #顯示資料
   CALL abxi610_show()
   
   WHILE TRUE
      LET g_bxfb_m.bxfbsite = g_bxfbsite_t
      LET g_bxfb_m.bxfb001 = g_bxfb001_t
      LET g_bxfb_m.bxfb002 = g_bxfb002_t
      LET g_bxfb_m.bxfb003 = g_bxfb003_t
      LET g_bxfb_m.bxfb004 = g_bxfb004_t
      LET g_bxfb_m.bxfb005 = g_bxfb005_t
      LET g_bxfb_m.bxfb006 = g_bxfb006_t
      LET g_bxfb_m.bxfb007 = g_bxfb007_t
 
      
      #寫入修改者/修改日期資訊
      LET g_bxfb_m.bxfbmodid = g_user 
LET g_bxfb_m.bxfbmoddt = cl_get_current()
LET g_bxfb_m.bxfbmodid_desc = cl_get_username(g_bxfb_m.bxfbmodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
      #end add-point
 
      #資料輸入
      CALL abxi610_input("u")     
 
      #add-point:modify段修改後 name="modify.after_input"
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         LET g_bxfb_m.* = g_bxfb_m_t.*
         CALL abxi610_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE bxfb_t SET (bxfbmodid,bxfbmoddt) = (g_bxfb_m.bxfbmodid,g_bxfb_m.bxfbmoddt)
       WHERE bxfbent = g_enterprise AND bxfbsite = g_bxfbsite_t
         AND bxfb001 = g_bxfb001_t
         AND bxfb002 = g_bxfb002_t
         AND bxfb003 = g_bxfb003_t
         AND bxfb004 = g_bxfb004_t
         AND bxfb005 = g_bxfb005_t
         AND bxfb006 = g_bxfb006_t
         AND bxfb007 = g_bxfb007_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL abxi610_set_act_visible()
   CALL abxi610_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " bxfbent = " ||g_enterprise|| " AND",
                      " bxfbsite = '", g_bxfb_m.bxfbsite, "' "
                      ," AND bxfb001 = '", g_bxfb_m.bxfb001, "' "
                      ," AND bxfb002 = '", g_bxfb_m.bxfb002, "' "
                      ," AND bxfb003 = '", g_bxfb_m.bxfb003, "' "
                      ," AND bxfb004 = '", g_bxfb_m.bxfb004, "' "
                      ," AND bxfb005 = '", g_bxfb_m.bxfb005, "' "
                      ," AND bxfb006 = '", g_bxfb_m.bxfb006, "' "
                      ," AND bxfb007 = '", g_bxfb_m.bxfb007, "' "
 
   #填到對應位置
   CALL abxi610_browser_fill(g_wc,"")
 
   CLOSE abxi610_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL abxi610_msgcentre_notify('modify')
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   

