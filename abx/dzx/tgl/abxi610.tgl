       g_bxfb_m.bxfbcrtdt,g_bxfb_m.bxfbmodid,g_bxfb_m.bxfbmoddt,g_bxfb_m.bxfb011,g_bxfb_m.bxfb015,g_bxfb_m.bxfb016, 
       g_bxfb_m.bxfb017,g_bxfb_m.bxfb020,g_bxfb_m.bxfb021,g_bxfb_m.bxfb022,g_bxfb_m.bxfb012,g_bxfb_m.bxfb013, 
       g_bxfb_m.bxfb014,g_bxfb_m.bxfb018,g_bxfb_m.bxfb019,g_bxfb_m.bxfb001_desc,g_bxfb_m.bxfbownid_desc, 
       g_bxfb_m.bxfbowndp_desc,g_bxfb_m.bxfbcrtid_desc,g_bxfb_m.bxfbcrtdp_desc,g_bxfb_m.bxfbmodid_desc 

   
 
   #檢查是否允許此動作
   IF NOT abxi610_action_chk() THEN
      CLOSE abxi610_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_bxfb_m.bxfbsite,g_bxfb_m.bxfb001,g_bxfb_m.bxfb001_desc,g_bxfb_m.bxfb001_desc_1, 
       g_bxfb_m.bxfb002,g_bxfb_m.bxfb003,g_bxfb_m.bxfb004,g_bxfb_m.bxfb005,g_bxfb_m.bxfb006,g_bxfb_m.bxfb007, 
       g_bxfb_m.bxfbstus,g_bxfb_m.bxfbownid,g_bxfb_m.bxfbownid_desc,g_bxfb_m.bxfbowndp,g_bxfb_m.bxfbowndp_desc, 
       g_bxfb_m.bxfbcrtid,g_bxfb_m.bxfbcrtid_desc,g_bxfb_m.bxfbcrtdp,g_bxfb_m.bxfbcrtdp_desc,g_bxfb_m.bxfbcrtdt, 
       g_bxfb_m.bxfbmodid,g_bxfb_m.bxfbmodid_desc,g_bxfb_m.bxfbmoddt,g_bxfb_m.bxfb011,g_bxfb_m.bxfb015, 
       g_bxfb_m.bxfb016,g_bxfb_m.bxfb017,g_bxfb_m.bxfb020,g_bxfb_m.bxfb021,g_bxfb_m.bxfb022,g_bxfb_m.bxfb012, 
       g_bxfb_m.bxfb013,g_bxfb_m.bxfb014,g_bxfb_m.bxfb018,g_bxfb_m.bxfb019
 
   CASE g_bxfb_m.bxfbstus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   {<point name="statechange.after_refresh"/>}
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_bxfb_m.bxfbstus
            
            WHEN "N"
               HIDE OPTION "inactive"
            WHEN "Y"
               HIDE OPTION "active"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      {<point name="statechange.before_menu"/>}
      #end add-point
      
      
	  
      ON ACTION inactive
         IF cl_auth_chk_act("inactive") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.inactive"
            {<point name="statechange.inactive"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION active
         IF cl_auth_chk_act("active") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.active"
            {<point name="statechange.active"/>}
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      {<point name="statechange.more_control"/>}
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      ) OR 
      g_bxfb_m.bxfbstus = lc_state OR cl_null(lc_state) THEN
      CLOSE abxi610_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   {<point name="statechange.b_update"/>}
   #end add-point
   
   LET g_bxfb_m.bxfbmodid = g_user
   LET g_bxfb_m.bxfbmoddt = cl_get_current()
   LET g_bxfb_m.bxfbstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE bxfb_t 
      SET (bxfbstus,bxfbmodid,bxfbmoddt) 
        = (g_bxfb_m.bxfbstus,g_bxfb_m.bxfbmodid,g_bxfb_m.bxfbmoddt)     
    WHERE bxfbent = g_enterprise AND bxfbsite = g_bxfb_m.bxfbsite
      AND bxfb001 = g_bxfb_m.bxfb001      AND bxfb002 = g_bxfb_m.bxfb002      AND bxfb003 = g_bxfb_m.bxfb003      AND bxfb004 = g_bxfb_m.bxfb004      AND bxfb005 = g_bxfb_m.bxfb005      AND bxfb006 = g_bxfb_m.bxfb006      AND bxfb007 = g_bxfb_m.bxfb007
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE abxi610_master_referesh USING g_bxfb_m.bxfbsite,g_bxfb_m.bxfb001,g_bxfb_m.bxfb002,g_bxfb_m.bxfb003, 
          g_bxfb_m.bxfb004,g_bxfb_m.bxfb005,g_bxfb_m.bxfb006,g_bxfb_m.bxfb007 INTO g_bxfb_m.bxfbsite, 
          g_bxfb_m.bxfb001,g_bxfb_m.bxfb002,g_bxfb_m.bxfb003,g_bxfb_m.bxfb004,g_bxfb_m.bxfb005,g_bxfb_m.bxfb006, 
          g_bxfb_m.bxfb007,g_bxfb_m.bxfbstus,g_bxfb_m.bxfbownid,g_bxfb_m.bxfbowndp,g_bxfb_m.bxfbcrtid, 
          g_bxfb_m.bxfbcrtdp,g_bxfb_m.bxfbcrtdt,g_bxfb_m.bxfbmodid,g_bxfb_m.bxfbmoddt,g_bxfb_m.bxfb011, 
          g_bxfb_m.bxfb015,g_bxfb_m.bxfb016,g_bxfb_m.bxfb017,g_bxfb_m.bxfb020,g_bxfb_m.bxfb021,g_bxfb_m.bxfb022, 
          g_bxfb_m.bxfb012,g_bxfb_m.bxfb013,g_bxfb_m.bxfb014,g_bxfb_m.bxfb018,g_bxfb_m.bxfb019,g_bxfb_m.bxfb001_desc, 
          g_bxfb_m.bxfbownid_desc,g_bxfb_m.bxfbowndp_desc,g_bxfb_m.bxfbcrtid_desc,g_bxfb_m.bxfbcrtdp_desc, 
          g_bxfb_m.bxfbmodid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_bxfb_m.bxfbsite,g_bxfb_m.bxfb001,g_bxfb_m.bxfb001_desc,g_bxfb_m.bxfb001_desc_1, 
          g_bxfb_m.bxfb002,g_bxfb_m.bxfb003,g_bxfb_m.bxfb004,g_bxfb_m.bxfb005,g_bxfb_m.bxfb006,g_bxfb_m.bxfb007, 
          g_bxfb_m.bxfbstus,g_bxfb_m.bxfbownid,g_bxfb_m.bxfbownid_desc,g_bxfb_m.bxfbowndp,g_bxfb_m.bxfbowndp_desc, 
          g_bxfb_m.bxfbcrtid,g_bxfb_m.bxfbcrtid_desc,g_bxfb_m.bxfbcrtdp,g_bxfb_m.bxfbcrtdp_desc,g_bxfb_m.bxfbcrtdt, 
          g_bxfb_m.bxfbmodid,g_bxfb_m.bxfbmodid_desc,g_bxfb_m.bxfbmoddt,g_bxfb_m.bxfb011,g_bxfb_m.bxfb015, 
          g_bxfb_m.bxfb016,g_bxfb_m.bxfb017,g_bxfb_m.bxfb020,g_bxfb_m.bxfb021,g_bxfb_m.bxfb022,g_bxfb_m.bxfb012, 
          g_bxfb_m.bxfb013,g_bxfb_m.bxfb014,g_bxfb_m.bxfb018,g_bxfb_m.bxfb019
   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
   {<point name="statechange.a_update"/>}
   #end add-point
 
   #add-point:statechange段結束前 name="statechange.after"
   {<point name="statechange.after"/>}
   #end add-point  
 
   CLOSE abxi610_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL abxi610_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION
 
 
 

