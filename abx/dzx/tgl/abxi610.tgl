   #讀入ref值(單頭)
   #add-point:show段reference name="show.head.reference"
   {<point name="show.head.reference"/>}
   #end add-point
 
   #將資料輸出到畫面上
   DISPLAY BY NAME g_bxfb_m.bxfbsite,g_bxfb_m.bxfb001,g_bxfb_m.bxfb001_desc,g_bxfb_m.bxfb001_desc_1, 
       g_bxfb_m.bxfb002,g_bxfb_m.bxfb003,g_bxfb_m.bxfb004,g_bxfb_m.bxfb005,g_bxfb_m.bxfb006,g_bxfb_m.bxfb007, 
       g_bxfb_m.bxfbstus,g_bxfb_m.bxfbownid,g_bxfb_m.bxfbownid_desc,g_bxfb_m.bxfbowndp,g_bxfb_m.bxfbowndp_desc, 
       g_bxfb_m.bxfbcrtid,g_bxfb_m.bxfbcrtid_desc,g_bxfb_m.bxfbcrtdp,g_bxfb_m.bxfbcrtdp_desc,g_bxfb_m.bxfbcrtdt, 
       g_bxfb_m.bxfbmodid,g_bxfb_m.bxfbmodid_desc,g_bxfb_m.bxfbmoddt,g_bxfb_m.bxfb011,g_bxfb_m.bxfb015, 
       g_bxfb_m.bxfb016,g_bxfb_m.bxfb017,g_bxfb_m.bxfb020,g_bxfb_m.bxfb021,g_bxfb_m.bxfb022,g_bxfb_m.bxfb012, 
       g_bxfb_m.bxfb013,g_bxfb_m.bxfb014,g_bxfb_m.bxfb018,g_bxfb_m.bxfb019
   
