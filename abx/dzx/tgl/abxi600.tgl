{<section id="abxi600.state_change" >}
   #應用 a09 樣板自動產生(Version:17)
#+ 確認碼變更 
PRIVATE FUNCTION abxi600_statechange()
   #add-point:statechange段define(客製用) name="statechange.define_customerization"
   {<point name="statechange.define_customerization" edit="c" mark="Y"/>}
   #end add-point  
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="statechange.define"
   {<point name="statechange.define"/>}
   #end add-point  
   
   #add-point:Function前置處理 name="statechange.before"
   {<point name="statechange.before"/>}
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_bxfa_m.bxfasite IS NULL
      OR g_bxfa_m.bxfa001 IS NULL      OR g_bxfa_m.bxfa002 IS NULL      OR g_bxfa_m.bxfa003 IS NULL      OR g_bxfa_m.bxfa004 IS NULL
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN abxi600_cl USING g_enterprise,g_bxfa_m.bxfasite,g_bxfa_m.bxfa001,g_bxfa_m.bxfa002,g_bxfa_m.bxfa003,g_bxfa_m.bxfa004
   IF STATUS THEN
      CLOSE abxi600_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN abxi600_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE abxi600_master_referesh USING g_bxfa_m.bxfasite,g_bxfa_m.bxfa001,g_bxfa_m.bxfa002,g_bxfa_m.bxfa003, 
       g_bxfa_m.bxfa004 INTO g_bxfa_m.bxfa001,g_bxfa_m.bxfasite,g_bxfa_m.bxfa002,g_bxfa_m.bxfa003,g_bxfa_m.bxfa004, 
       g_bxfa_m.bxfastus,g_bxfa_m.bxfa005,g_bxfa_m.bxfa006,g_bxfa_m.bxfa007,g_bxfa_m.bxfa008,g_bxfa_m.bxfa009, 
       g_bxfa_m.bxfa010,g_bxfa_m.bxfa011,g_bxfa_m.bxfa012,g_bxfa_m.bxfa021,g_bxfa_m.bxfa022,g_bxfa_m.bxfa023, 
       g_bxfa_m.bxfa024,g_bxfa_m.bxfa025,g_bxfa_m.bxfa026,g_bxfa_m.bxfa027,g_bxfa_m.bxfa028,g_bxfa_m.bxfa029, 
       g_bxfa_m.bxfa030,g_bxfa_m.bxfa031,g_bxfa_m.bxfaownid,g_bxfa_m.bxfaowndp,g_bxfa_m.bxfacrtid,g_bxfa_m.bxfacrtdp, 
       g_bxfa_m.bxfacrtdt,g_bxfa_m.bxfamodid,g_bxfa_m.bxfamoddt,g_bxfa_m.bxfa001_desc,g_bxfa_m.bxfa002_desc, 
       g_bxfa_m.bxfaownid_desc,g_bxfa_m.bxfaowndp_desc,g_bxfa_m.bxfacrtid_desc,g_bxfa_m.bxfacrtdp_desc, 
       g_bxfa_m.bxfamodid_desc
   
 
   #檢查是否允許此動作
   IF NOT abxi600_action_chk() THEN
      CLOSE abxi600_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_bxfa_m.bxfa001,g_bxfa_m.bxfa001_desc,g_bxfa_m.bxfa001_desc_1,g_bxfa_m.bxfasite, 
       g_bxfa_m.bxfa002,g_bxfa_m.bxfa002_desc,g_bxfa_m.bxfa003,g_bxfa_m.bxfa004,g_bxfa_m.bxfastus,g_bxfa_m.bxfa005, 
       g_bxfa_m.bxfa006,g_bxfa_m.bxfa007,g_bxfa_m.bxfa008,g_bxfa_m.bxfa009,g_bxfa_m.bxfa010,g_bxfa_m.bxfa011, 
       g_bxfa_m.bxfa012,g_bxfa_m.bxfa021,g_bxfa_m.bxfa022,g_bxfa_m.bxfa023,g_bxfa_m.bxfa024,g_bxfa_m.bxfa025, 
       g_bxfa_m.bxfa026,g_bxfa_m.bxfa027,g_bxfa_m.bxfa028,g_bxfa_m.bxfa029,g_bxfa_m.bxfa030,g_bxfa_m.bxfa031, 
       g_bxfa_m.bxfaownid,g_bxfa_m.bxfaownid_desc,g_bxfa_m.bxfaowndp,g_bxfa_m.bxfaowndp_desc,g_bxfa_m.bxfacrtid, 
       g_bxfa_m.bxfacrtid_desc,g_bxfa_m.bxfacrtdp,g_bxfa_m.bxfacrtdp_desc,g_bxfa_m.bxfacrtdt,g_bxfa_m.bxfamodid, 
       g_bxfa_m.bxfamodid_desc,g_bxfa_m.bxfamoddt
 
   CASE g_bxfa_m.bxfastus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   {<point name="statechange.after_refresh"/>}
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_bxfa_m.bxfastus
            
            WHEN "N"
               HIDE OPTION "inactive"
            WHEN "Y"
               HIDE OPTION "active"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      {<point name="statechange.before_menu"/>}
      #end add-point
      
      
	  
      ON ACTION inactive
         IF cl_auth_chk_act("inactive") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.inactive"
            {<point name="statechange.inactive"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION active
         IF cl_auth_chk_act("active") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.active"
            {<point name="statechange.active"/>}
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      {<point name="statechange.more_control"/>}
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      ) OR 
      g_bxfa_m.bxfastus = lc_state OR cl_null(lc_state) THEN
      CLOSE abxi600_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   {<point name="statechange.b_update"/>}
   #end add-point
   
   LET g_bxfa_m.bxfamodid = g_user
   LET g_bxfa_m.bxfamoddt = cl_get_current()
   LET g_bxfa_m.bxfastus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE bxfa_t 
      SET (bxfastus,bxfamodid,bxfamoddt) 
        = (g_bxfa_m.bxfastus,g_bxfa_m.bxfamodid,g_bxfa_m.bxfamoddt)     
    WHERE bxfaent = g_enterprise AND bxfasite = g_bxfa_m.bxfasite
      AND bxfa001 = g_bxfa_m.bxfa001      AND bxfa002 = g_bxfa_m.bxfa002      AND bxfa003 = g_bxfa_m.bxfa003      AND bxfa004 = g_bxfa_m.bxfa004
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE abxi600_master_referesh USING g_bxfa_m.bxfasite,g_bxfa_m.bxfa001,g_bxfa_m.bxfa002,g_bxfa_m.bxfa003, 
          g_bxfa_m.bxfa004 INTO g_bxfa_m.bxfa001,g_bxfa_m.bxfasite,g_bxfa_m.bxfa002,g_bxfa_m.bxfa003, 
          g_bxfa_m.bxfa004,g_bxfa_m.bxfastus,g_bxfa_m.bxfa005,g_bxfa_m.bxfa006,g_bxfa_m.bxfa007,g_bxfa_m.bxfa008, 
          g_bxfa_m.bxfa009,g_bxfa_m.bxfa010,g_bxfa_m.bxfa011,g_bxfa_m.bxfa012,g_bxfa_m.bxfa021,g_bxfa_m.bxfa022, 
          g_bxfa_m.bxfa023,g_bxfa_m.bxfa024,g_bxfa_m.bxfa025,g_bxfa_m.bxfa026,g_bxfa_m.bxfa027,g_bxfa_m.bxfa028, 
          g_bxfa_m.bxfa029,g_bxfa_m.bxfa030,g_bxfa_m.bxfa031,g_bxfa_m.bxfaownid,g_bxfa_m.bxfaowndp,g_bxfa_m.bxfacrtid, 
          g_bxfa_m.bxfacrtdp,g_bxfa_m.bxfacrtdt,g_bxfa_m.bxfamodid,g_bxfa_m.bxfamoddt,g_bxfa_m.bxfa001_desc, 
          g_bxfa_m.bxfa002_desc,g_bxfa_m.bxfaownid_desc,g_bxfa_m.bxfaowndp_desc,g_bxfa_m.bxfacrtid_desc, 
          g_bxfa_m.bxfacrtdp_desc,g_bxfa_m.bxfamodid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_bxfa_m.bxfa001,g_bxfa_m.bxfa001_desc,g_bxfa_m.bxfa001_desc_1,g_bxfa_m.bxfasite, 
          g_bxfa_m.bxfa002,g_bxfa_m.bxfa002_desc,g_bxfa_m.bxfa003,g_bxfa_m.bxfa004,g_bxfa_m.bxfastus, 
          g_bxfa_m.bxfa005,g_bxfa_m.bxfa006,g_bxfa_m.bxfa007,g_bxfa_m.bxfa008,g_bxfa_m.bxfa009,g_bxfa_m.bxfa010, 
          g_bxfa_m.bxfa011,g_bxfa_m.bxfa012,g_bxfa_m.bxfa021,g_bxfa_m.bxfa022,g_bxfa_m.bxfa023,g_bxfa_m.bxfa024, 
          g_bxfa_m.bxfa025,g_bxfa_m.bxfa026,g_bxfa_m.bxfa027,g_bxfa_m.bxfa028,g_bxfa_m.bxfa029,g_bxfa_m.bxfa030, 
          g_bxfa_m.bxfa031,g_bxfa_m.bxfaownid,g_bxfa_m.bxfaownid_desc,g_bxfa_m.bxfaowndp,g_bxfa_m.bxfaowndp_desc, 
          g_bxfa_m.bxfacrtid,g_bxfa_m.bxfacrtid_desc,g_bxfa_m.bxfacrtdp,g_bxfa_m.bxfacrtdp_desc,g_bxfa_m.bxfacrtdt, 
          g_bxfa_m.bxfamodid,g_bxfa_m.bxfamodid_desc,g_bxfa_m.bxfamoddt
   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
   {<point name="statechange.a_update"/>}
   #end add-point
 
   #add-point:statechange段結束前 name="statechange.after"
   {<point name="statechange.after"/>}
   #end add-point  
 
   CLOSE abxi600_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL abxi600_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION
 
 
 

