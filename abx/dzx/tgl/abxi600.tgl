       bxfamodid,'',bxfamoddt", 
                      " FROM bxfa_t",
                      " WHERE bxfaent= ? AND bxfasite=? AND bxfa001=? AND bxfa002=? AND bxfa003=? AND  
                          bxfa004=? FOR UPDATE"
   #add-point:SQL_define name="main.after_define_sql"
   {<point name="main.after_define_sql"/>}
   #end add-point
