PRIVATE FUNCTION abxi600_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   {<point name="modify.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   {<point name="modify.define"/>}
   #end add-point
   
   #add-point:Function前置處理 name="modify.pre_function"
   {<point name="modify.pre_function"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_bxfa_m.bxfasite IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_bxfasite_t = g_bxfa_m.bxfasite
   LET g_bxfa001_t = g_bxfa_m.bxfa001
   LET g_bxfa002_t = g_bxfa_m.bxfa002
   LET g_bxfa003_t = g_bxfa_m.bxfa003
   LET g_bxfa004_t = g_bxfa_m.bxfa004
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN abxi600_cl USING g_enterprise,g_bxfa_m.bxfasite,g_bxfa_m.bxfa001,g_bxfa_m.bxfa002,g_bxfa_m.bxfa003,g_bxfa_m.bxfa004
   IF SQLCA.SQLCODE THEN    #(ver:49)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN abxi600_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE
      LET g_errparam.popup = TRUE 
      CLOSE abxi600_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE abxi600_master_referesh USING g_bxfa_m.bxfasite,g_bxfa_m.bxfa001,g_bxfa_m.bxfa002,g_bxfa_m.bxfa003, 
       g_bxfa_m.bxfa004 INTO g_bxfa_m.bxfa001,g_bxfa_m.bxfasite,g_bxfa_m.bxfa002,g_bxfa_m.bxfa003,g_bxfa_m.bxfa004, 
       g_bxfa_m.bxfastus,g_bxfa_m.bxfa005,g_bxfa_m.bxfa006,g_bxfa_m.bxfa007,g_bxfa_m.bxfa008,g_bxfa_m.bxfa009, 
       g_bxfa_m.bxfa010,g_bxfa_m.bxfa011,g_bxfa_m.bxfa012,g_bxfa_m.bxfa021,g_bxfa_m.bxfa022,g_bxfa_m.bxfa023, 
       g_bxfa_m.bxfa024,g_bxfa_m.bxfa025,g_bxfa_m.bxfa026,g_bxfa_m.bxfa027,g_bxfa_m.bxfa028,g_bxfa_m.bxfa029, 
       g_bxfa_m.bxfa030,g_bxfa_m.bxfa031,g_bxfa_m.bxfaownid,g_bxfa_m.bxfaowndp,g_bxfa_m.bxfacrtid,g_bxfa_m.bxfacrtdp, 
       g_bxfa_m.bxfacrtdt,g_bxfa_m.bxfamodid,g_bxfa_m.bxfamoddt,g_bxfa_m.bxfa001_desc,g_bxfa_m.bxfa002_desc, 
       g_bxfa_m.bxfaownid_desc,g_bxfa_m.bxfaowndp_desc,g_bxfa_m.bxfacrtid_desc,g_bxfa_m.bxfacrtdp_desc, 
       g_bxfa_m.bxfamodid_desc
 
   #檢查是否允許此動作
   IF NOT abxi600_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_bxfa_m_mask_o.* =  g_bxfa_m.*
   CALL abxi600_bxfa_t_mask()
   LET g_bxfa_m_mask_n.* =  g_bxfa_m.*
   
   
 
   #顯示資料
   CALL abxi600_show()
   
   WHILE TRUE
      LET g_bxfa_m.bxfasite = g_bxfasite_t
      LET g_bxfa_m.bxfa001 = g_bxfa001_t
      LET g_bxfa_m.bxfa002 = g_bxfa002_t
      LET g_bxfa_m.bxfa003 = g_bxfa003_t
      LET g_bxfa_m.bxfa004 = g_bxfa004_t
 
      
      #寫入修改者/修改日期資訊
      LET g_bxfa_m.bxfamodid = g_user 
LET g_bxfa_m.bxfamoddt = cl_get_current()
LET g_bxfa_m.bxfamodid_desc = cl_get_username(g_bxfa_m.bxfamodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
      #end add-point
 
      #資料輸入
      CALL abxi600_input("u")     
 
      #add-point:modify段修改後 name="modify.after_input"
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         LET g_bxfa_m.* = g_bxfa_m_t.*
         CALL abxi600_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE bxfa_t SET (bxfamodid,bxfamoddt) = (g_bxfa_m.bxfamodid,g_bxfa_m.bxfamoddt)
       WHERE bxfaent = g_enterprise AND bxfasite = g_bxfasite_t
         AND bxfa001 = g_bxfa001_t
         AND bxfa002 = g_bxfa002_t
         AND bxfa003 = g_bxfa003_t
         AND bxfa004 = g_bxfa004_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL abxi600_set_act_visible()
   CALL abxi600_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " bxfaent = " ||g_enterprise|| " AND",
                      " bxfasite = '", g_bxfa_m.bxfasite, "' "
                      ," AND bxfa001 = '", g_bxfa_m.bxfa001, "' "
                      ," AND bxfa002 = '", g_bxfa_m.bxfa002, "' "
                      ," AND bxfa003 = '", g_bxfa_m.bxfa003, "' "
                      ," AND bxfa004 = '", g_bxfa_m.bxfa004, "' "
 
   #填到對應位置
   CALL abxi600_browser_fill(g_wc,"")
 
   CLOSE abxi600_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL abxi600_msgcentre_notify('modify')
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   

