   DECLARE abxi600_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.bxfa001,t0.bxfasite,t0.bxfa002,t0.bxfa003,t0.bxfa004,t0.bxfastus, 
       t0.bxfa005,t0.bxfa006,t0.bxfa007,t0.bxfa008,t0.bxfa009,t0.bxfa010,t0.bxfa011,t0.bxfa012,t0.bxfa021, 
       t0.bxfa022,t0.bxfa023,t0.bxfa024,t0.bxfa025,t0.bxfa026,t0.bxfa027,t0.bxfa028,t0.bxfa029,t0.bxfa030, 
