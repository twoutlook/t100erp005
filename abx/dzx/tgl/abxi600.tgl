               " LEFT JOIN ooag_t t7 ON t7.ooagent="||g_enterprise||" AND t7.ooag001=t0.bxfamodid  ",
 
               " WHERE t0.bxfaent = " ||g_enterprise|| " AND t0.bxfasite = ? AND t0.bxfa001 = ? AND t0.bxfa002 = ? AND t0.bxfa003 = ? AND t0.bxfa004 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
