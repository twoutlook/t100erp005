{<section id="aprt562.modify" >}
#+ 資料修改
PRIVATE FUNCTION aprt562_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   {<point name="modify.define_customerization" edit="c" mark="Y"/>}
   #end add-point    
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   DEFINE l_wc2_table1          STRING
 
 
   #add-point:modify段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   {<point name="modify.define"/>} 
   #end add-point    
   
   #add-point:Function前置處理  name="modify.pre_function"
   {<point name="modify.pre_function"/>}
   #end add-point
   
   #保存單頭舊值
   LET g_prci_m_t.* = g_prci_m.*
   LET g_prci_m_o.* = g_prci_m.*
   
   IF g_prci_m.prcidocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code = "std-00003" 
      LET g_errparam.popup = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_prcidocno_t = g_prci_m.prcidocno
 
   CALL s_transaction_begin()
   
   OPEN aprt562_cl USING g_enterprise,g_prci_m.prcidocno
   IF SQLCA.SQLCODE THEN   #(ver:78)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aprt562_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE   #(ver:78)
      LET g_errparam.popup = TRUE 
      CLOSE aprt562_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aprt562_master_referesh USING g_prci_m.prcidocno INTO g_prci_m.prcisite,g_prci_m.prcidocdt, 
       g_prci_m.prcidocno,g_prci_m.prci001,g_prci_m.prci002,g_prci_m.prci005,g_prci_m.prci003,g_prci_m.prci004, 
       g_prci_m.prciunit,g_prci_m.prcistus,g_prci_m.prciownid,g_prci_m.prciowndp,g_prci_m.prcicrtid, 
       g_prci_m.prcicrtdp,g_prci_m.prcicrtdt,g_prci_m.prcimodid,g_prci_m.prcimoddt,g_prci_m.prcicnfid, 
       g_prci_m.prcicnfdt,g_prci_m.prcisite_desc,g_prci_m.prci001_desc,g_prci_m.prci002_desc,g_prci_m.prci003_desc, 
       g_prci_m.prci004_desc,g_prci_m.prciownid_desc,g_prci_m.prciowndp_desc,g_prci_m.prcicrtid_desc, 
       g_prci_m.prcicrtdp_desc,g_prci_m.prcimodid_desc,g_prci_m.prcicnfid_desc
   
   #檢查是否允許此動作
   IF NOT aprt562_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_prci_m_mask_o.* =  g_prci_m.*
   CALL aprt562_prci_t_mask()
   LET g_prci_m_mask_n.* =  g_prci_m.*
   
   
   
   #add-point:modify段show之前 name="modify.before_show"
   {<point name="modify.before_show"/>}
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
 
   
   CALL aprt562_show()
   #add-point:modify段show之後 name="modify.after_show"
   {<point name="modify.after_show"/>}
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
 
 
    
   WHILE TRUE
      LET g_prcidocno_t = g_prci_m.prcidocno
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_prci_m.prcimodid = g_user 
LET g_prci_m.prcimoddt = cl_get_current()
LET g_prci_m.prcimodid_desc = cl_get_username(g_prci_m.prcimodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      LET g_master_commit = "N"
      CALL aprt562_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後 name="modify.after_input"
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE prci_t SET (prcimodid,prcimoddt) = (g_prci_m.prcimodid,g_prci_m.prcimoddt)
          WHERE prcient = g_enterprise AND prcidocno = g_prcidocno_t
 
      END IF
    
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         #若單頭無commit則還原
         IF g_master_commit = "N" THEN
            LET g_prci_m.* = g_prci_m_t.*
            CALL aprt562_show()
         END IF
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code = 9001 
         LET g_errparam.popup = FALSE 
         CALL cl_err()
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_prci_m.prcidocno != g_prci_m_t.prcidocno
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前 name="modify.body.b_fk_update"
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE prcj_t SET prcjdocno = g_prci_m.prcidocno
 
          WHERE prcjent = g_enterprise AND prcjdocno = g_prci_m_t.prcidocno
 
            
         #add-point:單身fk修改中 name="modify.body.m_fk_update"
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            #   INITIALIZE g_errparam TO NULL 
            #   LET g_errparam.extend = "prcj_t" 
            #   LET g_errparam.code = "std-00009" 
            #   LET g_errparam.popup = TRUE 
            #   CALL cl_err()
            #   CALL s_transaction_end('N','0')
            #   CONTINUE WHILE
            WHEN SQLCA.SQLCODE #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "prcj_t:",SQLERRMESSAGE 
               LET g_errparam.code = SQLCA.SQLCODE 
               LET g_errparam.popup = TRUE 
               CALL s_transaction_end('N','0')
               CALL cl_err()
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後 name="modify.body.a_fk_update"
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL aprt562_set_act_visible()   
   CALL aprt562_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " prcient = " ||g_enterprise|| " AND",
                      " prcidocno = '", g_prci_m.prcidocno, "' "
 
   #填到對應位置
   CALL aprt562_browser_fill("")
 
   CLOSE aprt562_cl
   
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aprt562_msgcentre_notify('modify')
 
END FUNCTION 
