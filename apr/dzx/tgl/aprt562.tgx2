   LET g_forupd_sql = " SELECT prcisite,'',prcidocdt,prcidocno,'',prci001,'',prci002,'',prci005,prci003, 
       '',prci004,'',prciunit,prcistus,prciownid,'',prciowndp,'',prcicrtid,'',prcicrtdp,'',prcicrtdt, 
