   DECLARE aprt211_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.prdasite,t0.prdadocdt,t0.prdadocno,t0.prda000,t0.prda001,t0.prda002, 
       t0.prdaacti,t0.prda006,t0.prda007,t0.prda027,t0.prdastus,t0.prda010,t0.prda011,t0.prda012,t0.prda013, 
       t0.prda038,t0.prda024,t0.prda025,t0.prda026,t0.prda098,t0.prda037,t0.prda034,t0.prda042,t0.prda004, 
       t0.prda005,t0.prda003,t0.prda033,t0.prda008,t0.prda009,t0.prdaunit,t0.prdacrtid,t0.prdacrtdp, 
