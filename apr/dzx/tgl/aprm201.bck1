                   prcfowndp,prcfcrtid,prcfcrtdp,prcfcrtdt,prcfmodid,prcfmoddt,prcfcnfid,prcfcnfdt) = (g_prcf_m.prcfunit, 
                   g_prcf_m.prcfdocdt,g_prcf_m.prcf001,g_prcf_m.prcf002,g_prcf_m.prcf003,g_prcf_m.prcf004, 
                   g_prcf_m.prcf098,g_prcf_m.prcfstus,g_prcf_m.prcf005,g_prcf_m.prcf006,g_prcf_m.prcf011, 
                   g_prcf_m.prcf012,g_prcf_m.prcf007,g_prcf_m.prcf008,g_prcf_m.prcfsite,g_prcf_m.prcf009, 
                   g_prcf_m.prcf010,g_prcf_m.prcf013,g_prcf_m.prcf014,g_prcf_m.prcf015,g_prcf_m.prcf019, 
                   g_prcf_m.prcf016,g_prcf_m.prcf017,g_prcf_m.prcf020,g_prcf_m.prcf018,g_prcf_m.prcf021, 
                   g_prcf_m.prcf024,g_prcf_m.prcf027,g_prcf_m.prcf030,g_prcf_m.prcf033,g_prcf_m.prcf036, 
                   g_prcf_m.prcf038,g_prcf_m.prcf042,g_prcf_m.prcf045,g_prcf_m.prcf022,g_prcf_m.prcf025, 
                   g_prcf_m.prcf028,g_prcf_m.prcf031,g_prcf_m.prcf034,g_prcf_m.prcf037,g_prcf_m.prcf039, 
