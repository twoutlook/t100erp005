       g_prcf_m.prcf024,g_prcf_m.prcf027,g_prcf_m.prcf030,g_prcf_m.prcf033,g_prcf_m.prcf036,g_prcf_m.prcf038, 
       g_prcf_m.prcf042,g_prcf_m.prcf045,g_prcf_m.prcf022,g_prcf_m.prcf025,g_prcf_m.prcf028,g_prcf_m.prcf031, 
       g_prcf_m.prcf034,g_prcf_m.prcf037,g_prcf_m.prcf039,g_prcf_m.prcf043,g_prcf_m.prcf023,g_prcf_m.prcf026, 
       g_prcf_m.prcf029,g_prcf_m.prcf032,g_prcf_m.prcf035,g_prcf_m.prcf040,g_prcf_m.prcf041,g_prcf_m.prcf044, 
       g_prcf_m.prcfownid,g_prcf_m.prcfowndp,g_prcf_m.prcfcrtid,g_prcf_m.prcfcrtdp,g_prcf_m.prcfcrtdt, 
       g_prcf_m.prcfmodid,g_prcf_m.prcfmoddt,g_prcf_m.prcfcnfid,g_prcf_m.prcfcnfdt,g_prcf_m.prcfunit_desc, 
       g_prcf_m.prcf002_desc,g_prcf_m.prcf003_desc,g_prcf_m.prcf004_desc,g_prcf_m.prcf007_desc,g_prcf_m.prcf008_desc, 
       g_prcf_m.prcfownid_desc,g_prcf_m.prcfowndp_desc,g_prcf_m.prcfcrtid_desc,g_prcf_m.prcfcrtdp_desc, 
       g_prcf_m.prcfmodid_desc,g_prcf_m.prcfcnfid_desc
   
 
