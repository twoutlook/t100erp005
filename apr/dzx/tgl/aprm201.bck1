   {<point name="filter.define_customerization" edit="c" mark="Y"/>}
   #end add-point   
   #add-point:filter段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="filter.define"
   {<point name="filter.define"/>}
   #end add-point   
   
   #add-point:Function前置處理  name="filter.pre_function"
   {<point name="filter.pre_function"/>}
   #end add-point
   
   #切換畫面
   IF NOT g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",1)
      CALL gfrm_curr.setElementHidden("worksheet",0)
      LET g_main_hidden = 1
   END IF   
 
   LET INT_FLAG = 0
 
   LET g_qryparam.state = 'c'
 
   LET g_wc_filter_t = g_wc_filter.trim()
   LET g_wc_t = g_wc
 
   LET g_wc = cl_replace_str(g_wc, g_wc_filter_t, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON prcf001,prcf002,prcf003,prcf004,prcf005,prcf006,prcf007,prcf008,prcf009, 
          prcf010,prcfdocdt,prcfunit
                          FROM s_browse[1].b_prcf001,s_browse[1].b_prcf002,s_browse[1].b_prcf003,s_browse[1].b_prcf004, 
                              s_browse[1].b_prcf005,s_browse[1].b_prcf006,s_browse[1].b_prcf007,s_browse[1].b_prcf008, 
                              s_browse[1].b_prcf009,s_browse[1].b_prcf010,s_browse[1].b_prcfdocdt,s_browse[1].b_prcfunit 

 
         BEFORE CONSTRUCT
               DISPLAY aprm201_filter_parser('prcf001') TO s_browse[1].b_prcf001
            DISPLAY aprm201_filter_parser('prcf002') TO s_browse[1].b_prcf002
            DISPLAY aprm201_filter_parser('prcf003') TO s_browse[1].b_prcf003
            DISPLAY aprm201_filter_parser('prcf004') TO s_browse[1].b_prcf004
            DISPLAY aprm201_filter_parser('prcf005') TO s_browse[1].b_prcf005
            DISPLAY aprm201_filter_parser('prcf006') TO s_browse[1].b_prcf006
            DISPLAY aprm201_filter_parser('prcf007') TO s_browse[1].b_prcf007
            DISPLAY aprm201_filter_parser('prcf008') TO s_browse[1].b_prcf008
            DISPLAY aprm201_filter_parser('prcf009') TO s_browse[1].b_prcf009
            DISPLAY aprm201_filter_parser('prcf010') TO s_browse[1].b_prcf010
            DISPLAY aprm201_filter_parser('prcfdocdt') TO s_browse[1].b_prcfdocdt
            DISPLAY aprm201_filter_parser('prcfunit') TO s_browse[1].b_prcfunit
      
