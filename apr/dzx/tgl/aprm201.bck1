         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE prcf_t SET (prcfmodid,prcfmoddt) = (g_prcf_m.prcfmodid,g_prcf_m.prcfmoddt)
       WHERE prcfent = g_enterprise AND prcf001 = g_prcf001_t
 
 
      EXIT WHILE
      
   END WHILE
 
