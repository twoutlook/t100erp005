               UPDATE prcf_t SET (prcfunit,prcfdocdt,prcf001,prcf002,prcf003,prcf004,prcf098,prcfstus, 
                   prcf005,prcf006,prcf011,prcf012,prcf007,prcf008,prcfsite,prcf009,prcf010,prcf013, 
                   prcf014,prcf015,prcf019,prcf016,prcf017,prcf020,prcf018,prcf021,prcf024,prcf027,prcf030, 
                   prcf033,prcf036,prcf038,prcf042,prcf045,prcf022,prcf025,prcf028,prcf031,prcf034,prcf037, 
