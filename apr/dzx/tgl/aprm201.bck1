       g_prcf_m.prcfcrtid_desc,g_prcf_m.prcfcrtdp,g_prcf_m.prcfcrtdp_desc,g_prcf_m.prcfcrtdt,g_prcf_m.prcfmodid, 
       g_prcf_m.prcfmodid_desc,g_prcf_m.prcfmoddt,g_prcf_m.prcfcnfid,g_prcf_m.prcfcnfid_desc,g_prcf_m.prcfcnfdt 

 
   CASE g_prcf_m.prcfstus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
      WHEN "C"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/closed.png")
      WHEN "X"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   {<point name="statechange.after_refresh"/>}
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_prcf_m.prcfstus
            
            WHEN "N"
               HIDE OPTION "unconfirmed"
            WHEN "Y"
               HIDE OPTION "confirmed"
            WHEN "C"
               HIDE OPTION "closed"
            WHEN "X"
               HIDE OPTION "invalid"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      {<point name="statechange.before_menu"/>}
      #end add-point
      
      
	  
      ON ACTION unconfirmed
         IF cl_auth_chk_act("unconfirmed") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.unconfirmed"
            {<point name="statechange.unconfirmed"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION confirmed
         IF cl_auth_chk_act("confirmed") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.confirmed"
            {<point name="statechange.confirmed"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION closed
         IF cl_auth_chk_act("closed") THEN
            LET lc_state = "C"
            #add-point:action控制 name="statechange.closed"
            {<point name="statechange.closed"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION invalid
         IF cl_auth_chk_act("invalid") THEN
            LET lc_state = "X"
            #add-point:action控制 name="statechange.invalid"
            {<point name="statechange.invalid"/>}
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      {<point name="statechange.more_control"/>}
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      AND lc_state <> "C"
      AND lc_state <> "X"
      ) OR 
      g_prcf_m.prcfstus = lc_state OR cl_null(lc_state) THEN
      CLOSE aprm201_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   {<point name="statechange.b_update"/>}
   #end add-point
   
   LET g_prcf_m.prcfmodid = g_user
   LET g_prcf_m.prcfmoddt = cl_get_current()
   LET g_prcf_m.prcfstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE prcf_t 
      SET (prcfstus,prcfmodid,prcfmoddt) 
        = (g_prcf_m.prcfstus,g_prcf_m.prcfmodid,g_prcf_m.prcfmoddt)     
    WHERE prcfent = g_enterprise AND prcf001 = g_prcf_m.prcf001
 
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "C"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/closed.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE aprm201_master_referesh USING g_prcf_m.prcf001 INTO g_prcf_m.prcfunit,g_prcf_m.prcfdocdt, 
          g_prcf_m.prcf001,g_prcf_m.prcf002,g_prcf_m.prcf003,g_prcf_m.prcf004,g_prcf_m.prcf098,g_prcf_m.prcfstus, 
          g_prcf_m.prcf005,g_prcf_m.prcf006,g_prcf_m.prcf011,g_prcf_m.prcf012,g_prcf_m.prcf007,g_prcf_m.prcf008, 
          g_prcf_m.prcfsite,g_prcf_m.prcf009,g_prcf_m.prcf010,g_prcf_m.prcf013,g_prcf_m.prcf014,g_prcf_m.prcf015, 
          g_prcf_m.prcf019,g_prcf_m.prcf016,g_prcf_m.prcf017,g_prcf_m.prcf020,g_prcf_m.prcf018,g_prcf_m.prcf021, 
          g_prcf_m.prcf024,g_prcf_m.prcf027,g_prcf_m.prcf030,g_prcf_m.prcf033,g_prcf_m.prcf036,g_prcf_m.prcf038, 
          g_prcf_m.prcf042,g_prcf_m.prcf045,g_prcf_m.prcf022,g_prcf_m.prcf025,g_prcf_m.prcf028,g_prcf_m.prcf031, 
          g_prcf_m.prcf034,g_prcf_m.prcf037,g_prcf_m.prcf039,g_prcf_m.prcf043,g_prcf_m.prcf023,g_prcf_m.prcf026, 
          g_prcf_m.prcf029,g_prcf_m.prcf032,g_prcf_m.prcf035,g_prcf_m.prcf040,g_prcf_m.prcf041,g_prcf_m.prcf044, 
          g_prcf_m.prcfownid,g_prcf_m.prcfowndp,g_prcf_m.prcfcrtid,g_prcf_m.prcfcrtdp,g_prcf_m.prcfcrtdt, 
          g_prcf_m.prcfmodid,g_prcf_m.prcfmoddt,g_prcf_m.prcfcnfid,g_prcf_m.prcfcnfdt,g_prcf_m.prcfunit_desc, 
          g_prcf_m.prcf002_desc,g_prcf_m.prcf003_desc,g_prcf_m.prcf004_desc,g_prcf_m.prcf007_desc,g_prcf_m.prcf008_desc, 
          g_prcf_m.prcfownid_desc,g_prcf_m.prcfowndp_desc,g_prcf_m.prcfcrtid_desc,g_prcf_m.prcfcrtdp_desc, 
          g_prcf_m.prcfmodid_desc,g_prcf_m.prcfcnfid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_prcf_m.prcfunit,g_prcf_m.prcfunit_desc,g_prcf_m.prcfdocdt,g_prcf_m.prcf001,g_prcf_m.prcfl003, 
          g_prcf_m.prcfl004,g_prcf_m.prcf002,g_prcf_m.prcf002_desc,g_prcf_m.prcf003,g_prcf_m.prcf003_desc, 
          g_prcf_m.prcf004,g_prcf_m.prcf004_desc,g_prcf_m.prcf098,g_prcf_m.prcfstus,g_prcf_m.prcf005, 
          g_prcf_m.prcf006,g_prcf_m.prcf011,g_prcf_m.prcf012,g_prcf_m.prcf007,g_prcf_m.prcf007_desc, 
          g_prcf_m.prcf008,g_prcf_m.prcf008_desc,g_prcf_m.prcfsite,g_prcf_m.prcf009,g_prcf_m.prcf010, 
          g_prcf_m.prcf013,g_prcf_m.prcf014,g_prcf_m.prcf015,g_prcf_m.prcf019,g_prcf_m.prcf016,g_prcf_m.prcf017, 
          g_prcf_m.prcf020,g_prcf_m.prcf018,g_prcf_m.prcf021,g_prcf_m.prcf024,g_prcf_m.prcf027,g_prcf_m.prcf030, 
          g_prcf_m.prcf033,g_prcf_m.prcf036,g_prcf_m.prcf038,g_prcf_m.prcf042,g_prcf_m.prcf045,g_prcf_m.prcf022, 
          g_prcf_m.prcf025,g_prcf_m.prcf028,g_prcf_m.prcf031,g_prcf_m.prcf034,g_prcf_m.prcf037,g_prcf_m.prcf039, 
          g_prcf_m.prcf043,g_prcf_m.prcf023,g_prcf_m.prcf026,g_prcf_m.prcf029,g_prcf_m.prcf032,g_prcf_m.prcf035, 
          g_prcf_m.prcf040,g_prcf_m.prcf041,g_prcf_m.prcf044,g_prcf_m.prcfownid,g_prcf_m.prcfownid_desc, 
          g_prcf_m.prcfowndp,g_prcf_m.prcfowndp_desc,g_prcf_m.prcfcrtid,g_prcf_m.prcfcrtid_desc,g_prcf_m.prcfcrtdp, 
          g_prcf_m.prcfcrtdp_desc,g_prcf_m.prcfcrtdt,g_prcf_m.prcfmodid,g_prcf_m.prcfmodid_desc,g_prcf_m.prcfmoddt, 
          g_prcf_m.prcfcnfid,g_prcf_m.prcfcnfid_desc,g_prcf_m.prcfcnfdt
   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
   {<point name="statechange.a_update"/>}
   #end add-point
 
   #add-point:statechange段結束前 name="statechange.after"
   {<point name="statechange.after"/>}
   #end add-point  
 
   CLOSE aprm201_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aprm201_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION
 
 
 

