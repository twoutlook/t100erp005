   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_prcf_m.prcfunit,g_prcf_m.prcfunit_desc,g_prcf_m.prcfdocdt,g_prcf_m.prcf001,g_prcf_m.prcfl003, 
