  <point name="function.aprq212_01_set_comp_visible" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 畫面UI的呈現方式
# Memo...........:
# Usage..........: CALL aprq212_01_set_comp_visible()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/03/20 By xumm
# Modify.........:
################################################################################
PRIVATE FUNCTION aprq212_01_set_comp_visible()
DEFINE l_n1      LIKE type_t.num5
DEFINE l_n2      LIKE type_t.num5
DEFINE l_n3      LIKE type_t.num5

   LET l_n1 = 0
   SELECT COUNT(*) INTO l_n1
     FROM prdm_t
    WHERE prdment = g_enterprise
      AND prdm001 = g_prdw001
      AND prdm003  = '1'
      AND prdmstus = 'Y'
      
   LET l_n2 = 0
   SELECT COUNT(*) INTO l_n2
     FROM prdm_t
    WHERE prdment = g_enterprise
      AND prdm001 = g_prdw001
      AND prdm003  = '2'
      AND prdmstus = 'Y'

   LET l_n3 = 0
   SELECT COUNT(*) INTO l_n3
     FROM prdm_t
    WHERE prdment = g_enterprise
      AND prdm001 = g_prdw001
      AND prdm003  = '3'
      AND prdmstus = 'Y'
      
   CALL cl_set_comp_visible('bpage03',TRUE)
   CALL cl_set_comp_visible('prdu004',TRUE)
   CALL cl_set_comp_visible('prdu003',TRUE)
   CALL cl_set_comp_visible('prdu004_desc',TRUE)
   CALL cl_set_comp_visible('prdu004_desc1',TRUE)
   CALL cl_set_comp_visible('prdu006',TRUE)
   CALL cl_set_comp_visible('prdu00601',TRUE)
   CALL cl_set_comp_visible('prdu00602',TRUE)
   CALL cl_set_comp_visible('prdu00603',TRUE)
   IF g_prdw_m.prdw019 = '3' THEN
      CALL cl_set_comp_visible('prdu00601',FALSE)
      CALL cl_set_comp_visible('prdu00602',FALSE)
      CALL cl_set_comp_visible('prdu00603',FALSE)
   ELSE
      IF l_n1 = 0 THEN
         CALL cl_set_comp_visible('prdu00601',FALSE)
      END IF
      IF l_n2 = 0 THEN
         CALL cl_set_comp_visible('prdu00602',FALSE)
      END IF
      IF l_n3 = 0 THEN
         CALL cl_set_comp_visible('prdu00603',FALSE)
      END IF
      CALL cl_set_comp_visible('bpage03',FALSE)
      CALL cl_set_comp_visible('prdu004',FALSE)
      CALL cl_set_comp_visible('prdu004_desc',FALSE)
      CALL cl_set_comp_visible('prdu004_desc1',FALSE)
      CALL cl_set_comp_visible('prdu006',FALSE)
   END IF
   IF g_type1 <> 'Y' THEN
      CALL cl_set_comp_visible('prdu003',FALSE)
   END IF 
END FUNCTION]]>
  </point>
