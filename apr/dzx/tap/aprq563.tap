  <section id="aprq563.b_fill" ver="3" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aprq563_b_fill()
   {<Local define>}
   DEFINE ls_wc           STRING
   DEFINE l_pid           LIKE type_t.chr50
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_detail_idx  = 1
   LET g_detail_idx2 = 1
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
 
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
 
   CALL g_prcg_d.clear()
 
   #add-point:陣列清空
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   
 
   LET g_cnt = l_ac
   LET l_ac = 1
   ERROR "Searching!"
 
   # b_fill段sql組成及FOREACH撰寫
   #+ 此段落由子樣板qs04產生
   #+ b_fill段資料取得(包含sql組成及FOREACH段撰寫)
   LET g_sql = "SELECT  UNIQUE prcgsite,'',prcg001,'',prcg002,'','','','','','','','','','','','','', 
       '','','','','' FROM prcg_t",
 
 
               "",
               " WHERE prcgent= ? AND 1=1 AND ", ls_wc
 
   LET g_sql = g_sql, " ORDER BY prcg_t.prcgdocno"
 
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   PREPARE aprq563_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aprq563_pb
 
   FOREACH b_fill_curs INTO g_prcg_d[l_ac].prcgsite,g_prcg_d[l_ac].prcgsite_desc,g_prcg_d[l_ac].prcg001, 
       g_prcg_d[l_ac].prcg001_desc,g_prcg_d[l_ac].prcg002,g_prcg_d[l_ac].prcg002_desc,g_prcg_d[l_ac].prch002, 
       g_prcg_d[l_ac].prch002_desc,g_prcg_d[l_ac].prch003,g_prcg_d[l_ac].prch003_desc,g_prcg_d[l_ac].prch004, 
       g_prcg_d[l_ac].prch004_desc,g_prcg_d[l_ac].prch005,g_prcg_d[l_ac].prch005_desc,g_prcg_d[l_ac].prch006, 
       g_prcg_d[l_ac].prch006_desc,g_prcg_d[l_ac].prch007,g_prcg_d[l_ac].prch007_desc,g_prcg_d[l_ac].prch008, 
       g_prcg_d[l_ac].prch009,g_prcg_d[l_ac].prch010,g_prcg_d[l_ac].prch011,g_prcg_d[l_ac].prch012
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
 
      LET g_prcg_d[l_ac].sel = "N"
 
      
 
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL aprq563_detail_show("'1'")
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            CALL cl_err( "", 9035, 1 )
         END IF
         EXIT FOREACH
      END IF
 
   END FOREACH
 
 
 
 
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   CALL g_prcg_d.deleteElement(g_prcg_d.getLength())
 
   #add-point:陣列長度調整
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
 
   LET g_error_show = 0
 
   LET g_detail_cnt = l_ac - 1
   DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
 
   #+ 此段落由子樣板qs06產生
   #+ b_fill段CURSOR釋放
   CLOSE b_fill_curs
   FREE aprq563_pb
 
 
 
 
   LET l_ac = 1
   CALL aprq563_b_fill2()
 
      CALL aprq563_filter_show('prcgsite','b_prcgsite')
   CALL aprq563_filter_show('prcg001','b_prcg001')
   CALL aprq563_filter_show('prcg002','b_prcg002')
   CALL aprq563_filter_show('prch002','b_prch002')
   CALL aprq563_filter_show('prch003','b_prch003')
   CALL aprq563_filter_show('prch004','b_prch004')
   CALL aprq563_filter_show('prch005','b_prch005')
   CALL aprq563_filter_show('prch006','b_prch006')
   CALL aprq563_filter_show('prch007','b_prch007')
   CALL aprq563_filter_show('prch008','b_prch008')
