  <point name="function.aprq563_query" cite_std="N" status="" ver="1" src="s" new="Y" order="1" mark_hard="N">
<![CDATA[
#查詢
PRIVATE FUNCTION aprq563_query()
   DEFINE ls_wc      LIKE type_t.chr500
   DEFINE ls_return  STRING
   DEFINE ls_result  STRING 
   #add-point:query段define
   DEFINE l_sql      STRING 
   #end add-point 
   DELETE FROM aprq563_tmp
   LET l_sql = " INSERT INTO aprq563_tmp(ooed004) ",
               " SELECT ooed004  FROM ooed_t WHERE  ooed001='2' ",
               "   AND TO_CHAR(ooed006,'YYYY/MM/DD')<='",g_today,"' AND (TO_CHAR(ooed007,'YYYY/MM/DD')>='",g_today,"' OR ooed007 IS null) ",
               "   AND ooed004 IN ((select DISTINCT ooed004 FROM ooed_t START WITH ooed005='",g_site,"' AND ooed001='2' ",
               "   AND TO_CHAR(ooed006,'YYYY/MM/DD')<='",g_today,"' AND (TO_CHAR(ooed007,'YYYY/MM/DD')>='",g_today,"' OR ooed007 IS null) ",
               "  CONNECT BY  NOCYCLE PRIOR ooed004=ooed005 AND ooed001='2' ",
               "   AND TO_CHAR(ooed006,'YYYY/MM/DD')<='",g_today,"' AND (TO_CHAR(ooed007,'YYYY/MM/DD')>='",g_today,"' OR ooed007 IS null))",
               "          UNION ",
               "         (SELECT ooed004 FROM ooed_t WHERE ooed004='",g_site,"'))"
   PREPARE l_sql_ooed004_pre FROM l_sql
   EXECUTE l_sql_ooed004_pre
   IF sqlca.sqlcode THEN
      CALL cl_err('',sqlca.sqlcode,1)
      RETURN
   END IF
   
   LET INT_FLAG = 0
   
   CLEAR FORM
   CALL g_prcg_d.clear()
   
   LET g_qryparam.state = "c"
   
   #wc備份
   LET ls_wc = g_wc2
   CALL cl_set_act_visible("accept,cancel", TRUE)
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      CONSTRUCT g_wc2 ON prcgsite,prcgdocno,prcgdocdt,prcg001,prcg002,prch004,prch005,prch006,prch007
         FROM prcgsite,prcgdocno,prcgdocdt,prcg001,prcg002,prch004,prch005,prch006,prch007

         ON ACTION controlp INFIELD prcgsite
            #add-point:ON ACTION controlp INFIELD prcgsite
            #此段落由子樣板a08產生
            #開窗c段
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c'
            LET g_qryparam.reqry = FALSE
            LET g_qryparam.arg1 = g_site
            LET g_qryparam.arg2 = '2'
            CALL q_ooed004_3()                          #呼叫開窗
            DISPLAY g_qryparam.return1 TO prcgsite  #顯示到畫面上
            NEXT FIELD prcgsite                     #返回原欄位
            
 
         ON ACTION controlp INFIELD prcgdocno
            #add-point:ON ACTION controlp INFIELD prcgdocno
            #此段落由子樣板a08產生
            #開窗c段
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c'
            LET g_qryparam.reqry = FALSE
            LET g_qryparam.where = "prcgsite IN ( SELECT ooed004 FROM aprq563_tmp )"
            CALL q_prcgdocno()                           #呼叫開窗
            DISPLAY g_qryparam.return1 TO prcgdocno  #顯示到畫面上
            NEXT FIELD prcgdocno                     #返回原欄位
 
         ON ACTION controlp INFIELD prcg001
            #add-point:ON ACTION controlp INFIELD prcg001
            #此段落由子樣板a08產生
            #開窗c段
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c'
            LET g_qryparam.reqry = FALSE
            LET g_qryparam.arg1 = '2'
            CALL q_prcf001()                           #呼叫開窗
            DISPLAY g_qryparam.return1 TO prcg001  #顯示到畫面上
            NEXT FIELD prcg001                     #返回原欄位
         ON ACTION controlp INFIELD prcg002
            #add-point:ON ACTION controlp INFIELD prcg002
            #此段落由子樣板a08產生
            #開窗c段
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c'
            LET g_qryparam.reqry = FALSE
            LET g_qryparam.arg1 = '2'
            CALL q_prcd001_1()                           #呼叫開窗
            DISPLAY g_qryparam.return1 TO prcg002  #顯示到畫面上
            NEXT FIELD prcg002                     #返回原欄位    
 
         ON ACTION controlp INFIELD prch004
            #add-point:ON ACTION controlp INFIELD prch004
            #此段落由子樣板a08產生
            #開窗c段
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c'
            LET g_qryparam.reqry = FALSE
            CALL q_dbbc001()                           #呼叫開窗
            DISPLAY g_qryparam.return1 TO prch004  #顯示到畫面上
            NEXT FIELD prch004                     #返回原欄位
         ON ACTION controlp INFIELD prch005
            #add-point:ON ACTION controlp INFIELD prch005
            #此段落由子樣板a08產生
            #開窗c段
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c'
            LET g_qryparam.reqry = FALSE
            LET g_qryparam.arg1 = '1'
            CALL q_oojd001()                           #呼叫開窗
            DISPLAY g_qryparam.return1 TO prch005  #顯示到畫面上
            NEXT FIELD prch005                     #返回原欄位
         ON ACTION controlp INFIELD prch006
            #add-point:ON ACTION controlp INFIELD prch006
            #此段落由子樣板a08產生
            #開窗c段
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c'
            LET g_qryparam.reqry = FALSE
            LET g_qryparam.arg1 = 'G'
            CALL q_ooef001_3()                           #呼叫開窗
            DISPLAY g_qryparam.return1 TO prch006  #顯示到畫面上
            NEXT FIELD prch006                     #返回原欄位
         ON ACTION controlp INFIELD prch007
            #add-point:ON ACTION controlp INFIELD prch007
            #此段落由子樣板a08產生
            #開窗c段
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c'
            LET g_qryparam.reqry = FALSE
            CALL q_dbba001()                           #呼叫開窗
            DISPLAY g_qryparam.return1 TO prch007  #顯示到畫面上
            NEXT FIELD prch007            
 
      
         BEFORE CONSTRUCT
            #add-point:cs段more_construct
            
            #end add-point 
      
      END CONSTRUCT
  
      #add-point:query段more_construct

      #end add-point 
  
      BEFORE DIALOG 
         CALL cl_qbe_init()
         #add-point:query段before_dialog

         #end add-point 
      
      ON ACTION qbe_select
         LET ls_wc = ""
         CALL cl_qbe_list("c") RETURNING ls_wc
      
      ON ACTION qbe_save
         CALL cl_qbe_save()
      
      ON ACTION accept
         ACCEPT DIALOG
         
      ON ACTION cancel
         LET INT_FLAG = 1
         EXIT DIALOG
      
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE DIALOG 
   END DIALOG
 
   #add-point:query段after_construct

   #end add-point
 
   IF INT_FLAG THEN
      LET INT_FLAG = 0
      #還原
      LET g_wc2 = ls_wc
   END IF
    
   LET g_error_show = 1
   CALL aprq563_b_fill2()
   
   IF g_detail_cnt = 0 AND NOT INT_FLAG THEN
      CALL cl_err("",-100,1)
   END IF
   CALL cl_set_act_visible("accept,cancel", FALSE)
   LET INT_FLAG = FALSE
END FUNCTION]]>
</point>
