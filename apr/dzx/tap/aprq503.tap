  <point name="function.aprq503_idx_chk" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
#chk
PRIVATE FUNCTION aprq503_idx_chk()
   
   IF l_current_page = 1 THEN
      LET g_detail_idx = g_curr_diag.getCurrentRow("s_detail1")
      IF g_detail_idx > g_prfm_d.getLength() THEN
         LET g_detail_idx = g_prfm_d.getLength()
      END IF
      IF g_detail_idx = 0 AND g_prfm_d.getLength() <> 0 THEN
         LET g_detail_idx = 1
      END IF
      DISPLAY g_detail_idx TO FORMONLY.idx
      DISPLAY g_prfm_d.getLength() TO FORMONLY.cnt
   END IF
END FUNCTION]]>
