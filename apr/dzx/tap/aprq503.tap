  <section id="aprq503.modify" ver="6" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aprq503_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_prfm_m.prfm001 IS NULL
   OR g_prfm_m.prfm002 IS NULL
   OR g_prfm_m.prfm004 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE aprq503_master_referesh USING g_prfm_m.prfm001,g_prfm_m.prfm002,g_prfm_m.prfm004 INTO g_prfm_m.prfm001, 
       g_prfm_m.prfm002,g_prfm_m.prfm004
 
   ERROR ""
  
   LET g_prfm001_t = g_prfm_m.prfm001
   LET g_prfm002_t = g_prfm_m.prfm002
   LET g_prfm004_t = g_prfm_m.prfm004
 
   CALL s_transaction_begin()
   
   OPEN aprq503_cl USING g_enterprise,g_prfm_m.prfm001
                                                       ,g_prfm_m.prfm002
                                                       ,g_prfm_m.prfm004
 
   IF STATUS THEN
      CALL cl_err("OPEN aprq503_cl:", STATUS, 1)
      CLOSE aprq503_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aprq503_cl INTO g_prfm_m.prfm001,g_prfm_m.prfm002,g_prfm_m.prfm002_desc,g_prfm_m.prfm004
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_prfm_m.prfm001,SQLCA.sqlcode,0)
      CLOSE aprq503_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL aprq503_show()
   WHILE TRUE
      LET g_prfm001_t = g_prfm_m.prfm001
      LET g_prfm002_t = g_prfm_m.prfm002
      LET g_prfm004_t = g_prfm_m.prfm004
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL aprq503_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_prfm_m.* = g_prfm_m_t.*
         CALL aprq503_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_prfm_m.prfm001 != g_prfm001_t 
      OR g_prfm_m.prfm002 != g_prfm002_t 
      OR g_prfm_m.prfm004 != g_prfm004_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE prfm_t SET prfm001 = g_prfm_m.prfm001
                                       , prfm002 = g_prfm_m.prfm002
                                       , prfm004 = g_prfm_m.prfm004
 
          WHERE prfment = g_enterprise AND prfm001 = g_prfm001_t
            AND prfm002 = g_prfm002_t
            AND prfm004 = g_prfm004_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("prfm_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("prfm_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_prfm_m.prfm001,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE aprq503_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_prfm_m.prfm001,'U')
 
   CALL aprq503_b_fill("1=1")
   
END FUNCTION   
]]>
  </section>
