  <section id="aprq503.b_fill" ver="6" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aprq503_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_prfm_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
      LET g_sql = "SELECT  UNIQUE prfm003,'','',prfm005,'',prfm006,'',prfm007,prfm008,prfm009,'',prfm010, 
          prfm011,prfm012,prfm013,prfm014,prfm015,prfm016,prfmunit,prfm019 FROM prfm_t",   
                  "",
                  
                  " WHERE prfment= ? AND prfm001=? AND prfm002=? AND prfm004=?"  
                  
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF aprq503_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY prfm_t.prfm003,prfm_t.prfm005,prfm_t.prfm006,prfm_t.prfm007,prfm_t.prfm008,prfm_t.prfm009"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      PREPARE aprq503_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR aprq503_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_prfm_m.prfm001
                                               ,g_prfm_m.prfm002
                                               ,g_prfm_m.prfm004
 
                                               
      FOREACH b_fill_cs INTO g_prfm_d[l_ac].prfm003,g_prfm_d[l_ac].imaal003,g_prfm_d[l_ac].imaal004, 
          g_prfm_d[l_ac].prfm005,g_prfm_d[l_ac].prfm005_desc,g_prfm_d[l_ac].prfm006,g_prfm_d[l_ac].prfm006_desc, 
          g_prfm_d[l_ac].prfm007,g_prfm_d[l_ac].prfm008,g_prfm_d[l_ac].prfm009,g_prfm_d[l_ac].prfm009_desc, 
          g_prfm_d[l_ac].prfm010,g_prfm_d[l_ac].prfm011,g_prfm_d[l_ac].prfm012,g_prfm_d[l_ac].prfm013, 
          g_prfm_d[l_ac].prfm014,g_prfm_d[l_ac].prfm015,g_prfm_d[l_ac].prfm016,g_prfm_d[l_ac].prfmunit, 
          g_prfm_d[l_ac].prfm019 
                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_prfm_d.deleteElement(g_prfm_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE aprq503_pb   
   
END FUNCTION
]]>
