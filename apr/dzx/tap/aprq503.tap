  <section id="aprq503.delete" ver="6" status="" src="s">
    <![CDATA[#+ 資料刪除
PRIVATE FUNCTION aprq503_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_prfm_m.prfm001 IS NULL
   OR g_prfm_m.prfm002 IS NULL
   OR g_prfm_m.prfm004 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE aprq503_master_referesh USING g_prfm_m.prfm001,g_prfm_m.prfm002,g_prfm_m.prfm004 INTO g_prfm_m.prfm001, 
       g_prfm_m.prfm002,g_prfm_m.prfm004
   
   CALL aprq503_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN aprq503_cl USING g_enterprise,g_prfm_m.prfm001
                                                       ,g_prfm_m.prfm002
                                                       ,g_prfm_m.prfm004
 
   IF STATUS THEN
      CALL cl_err("OPEN aprq503_cl:", STATUS, 1)
      CLOSE aprq503_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aprq503_cl INTO g_prfm_m.prfm001,g_prfm_m.prfm002,g_prfm_m.prfm002_desc,g_prfm_m.prfm004
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_prfm_m.prfm001,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL aprq503_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM prfm_t WHERE prfment = g_enterprise AND prfm001 = g_prfm_m.prfm001
                                                               AND prfm002 = g_prfm_m.prfm002
                                                               AND prfm004 = g_prfm_m.prfm004
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("prfm_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_prfm_d.clear() 
 
     
      CALL aprq503_ui_browser_refresh()  
      CALL aprq503_ui_headershow()  
      CALL aprq503_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL aprq503_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL aprq503_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE aprq503_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_prfm_m.prfm001,'D')
    
END FUNCTION
]]>
