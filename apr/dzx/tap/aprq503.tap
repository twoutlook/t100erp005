  <section id="aprq503.input.body" ver="6" status="" src="s">
    <![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_prfm_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂單身ACTION
         
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_prfm_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL aprq503_b_fill(g_wc2) 
            IF g_rec_b != 0 THEN
               CALL fgl_set_arr_curr(l_ac)
            END IF
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN aprq503_cl USING g_enterprise,
                                               g_prfm_m.prfm001
                                               ,g_prfm_m.prfm002
                                               ,g_prfm_m.prfm004
 
                                               
               IF STATUS THEN
                  CALL cl_err("OPEN aprq503_cl:", STATUS, 1)
                  CLOSE aprq503_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_prfm_d[l_ac].prfm003 IS NOT NULL
               AND g_prfm_d[l_ac].prfm005 IS NOT NULL
               AND g_prfm_d[l_ac].prfm006 IS NOT NULL
               AND g_prfm_d[l_ac].prfm007 IS NOT NULL
               AND g_prfm_d[l_ac].prfm008 IS NOT NULL
               AND g_prfm_d[l_ac].prfm009 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_prfm_d_t.* = g_prfm_d[l_ac].*  #BACKUP
               CALL aprq503_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL aprq503_set_no_entry_b(l_cmd)
               OPEN aprq503_bcl USING g_enterprise,g_prfm_m.prfm001,
                                                g_prfm_m.prfm002,
                                                g_prfm_m.prfm004,
 
                                                g_prfm_d_t.prfm003
                                                ,g_prfm_d_t.prfm005
                                                ,g_prfm_d_t.prfm006
                                                ,g_prfm_d_t.prfm007
                                                ,g_prfm_d_t.prfm008
                                                ,g_prfm_d_t.prfm009
 
               IF STATUS THEN
                  CALL cl_err("OPEN aprq503_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aprq503_bcl INTO g_prfm_d[l_ac].prfm003,g_prfm_d[l_ac].imaal003,g_prfm_d[l_ac].imaal004, 
                      g_prfm_d[l_ac].prfm005,g_prfm_d[l_ac].prfm005_desc,g_prfm_d[l_ac].prfm006,g_prfm_d[l_ac].prfm006_desc, 
                      g_prfm_d[l_ac].prfm007,g_prfm_d[l_ac].prfm008,g_prfm_d[l_ac].prfm009,g_prfm_d[l_ac].prfm009_desc, 
                      g_prfm_d[l_ac].prfm010,g_prfm_d[l_ac].prfm011,g_prfm_d[l_ac].prfm012,g_prfm_d[l_ac].prfm013, 
                      g_prfm_d[l_ac].prfm014,g_prfm_d[l_ac].prfm015,g_prfm_d[l_ac].prfm016,g_prfm_d[l_ac].prfmunit, 
                      g_prfm_d[l_ac].prfm019
                  IF SQLCA.sqlcode THEN
                      CALL cl_err(g_prfm_d_t.prfm003,SQLCA.sqlcode,1)
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL aprq503_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
        
         BEFORE INSERT
            
            INITIALIZE g_prfm_d_t.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_prfm_d[l_ac].* TO NULL
            
            #公用欄位預設值
              
            
            #一般欄位預設值
            
            
            
            LET g_prfm_d_t.* = g_prfm_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL aprq503_set_entry_b(l_cmd)
            #add-point:set_entry_b後
            {<point name="input.body.before_insert.set_entry_b"/>}
            #end add-point
            CALL aprq503_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_prfm_d[li_reproduce_target].* = g_prfm_d[li_reproduce].*
 
