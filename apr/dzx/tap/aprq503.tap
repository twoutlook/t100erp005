       prfm011,prfm012,prfm013,prfm014,prfm015,prfm016,prfmunit,prfm019 FROM prfm_t WHERE prfment=?  
       AND prfm001=? AND prfm002=? AND prfm004=? AND prfm003=? AND prfm005=? AND prfm006=? AND prfm007=?  
