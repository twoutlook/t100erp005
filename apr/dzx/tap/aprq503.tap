   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE aprq503_detail AS ",
                "SELECT * FROM prfm_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO aprq503_detail SELECT * FROM prfm_t 
                                         WHERE prfment = g_enterprise AND prfm001 = g_prfm001_t
                                         AND prfm002 = g_prfm002_t
                                         AND prfm004 = g_prfm004_t
 
   
   #將key修正為調整後   
   UPDATE aprq503_detail 
      #更新key欄位
      SET prfm001 = g_prfm_m.prfm001
          , prfm002 = g_prfm_m.prfm002
          , prfm004 = g_prfm_m.prfm004
 
      #更新共用欄位
      
                                       
  
   #將資料塞回原table   
   INSERT INTO prfm_t SELECT * FROM aprq503_detail
   
   IF SQLCA.sqlcode THEN
      CALL cl_err("reproduce",SQLCA.sqlcode,1)
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE aprq503_detail
   
   #add-point:單身複製後1
   {<point name="detail_reproduce.body.table1.a_insert"/>}
   #end add-point
 
 
   
 
   
   #多語言複製段落
   
   
   CALL s_transaction_end('Y','0')
   
   #已新增完, 調整資料內容(修改時使用)
   LET g_prfm001_t = g_prfm_m.prfm001
   LET g_prfm002_t = g_prfm_m.prfm002
   LET g_prfm004_t = g_prfm_m.prfm004
 
   
