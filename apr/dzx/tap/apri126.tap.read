  <section id="apri126.state_change" ver="8" status="" src="s" readonly="">
    <![CDATA[   #應用 a09 樣板自動產生(Version:12)
#+ 確認碼變更 
PRIVATE FUNCTION apri126_statechange()
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define
   {<point name="statechange.define" edit="s"/>}
   #end add-point  
   #add-point:statechange段define(客製用)
   {<point name="statechange.define_customerization" edit="c"/>}
   #end add-point  
   
   #add-point:statechange段開始前
   {<point name="statechange.before"/>}
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_prbl_m.prbl001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN apri126_cl USING g_enterprise,g_prbl_m.prbl001
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN apri126_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE apri126_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE apri126_master_referesh USING g_prbl_m.prbl001 INTO g_prbl_m.prbl001,g_prbl_m.prbl002,g_prbl_m.prblunit, 
       g_prbl_m.prbl003,g_prbl_m.prbl004,g_prbl_m.prbl005,g_prbl_m.prbl006,g_prbl_m.prblstus,g_prbl_m.prblownid, 
       g_prbl_m.prblowndp,g_prbl_m.prblcrtid,g_prbl_m.prblcrtdp,g_prbl_m.prblcrtdt,g_prbl_m.prblmodid, 
       g_prbl_m.prblmoddt,g_prbl_m.prblownid_desc,g_prbl_m.prblowndp_desc,g_prbl_m.prblcrtid_desc,g_prbl_m.prblcrtdp_desc, 
       g_prbl_m.prblmodid_desc
 
   #檢查是否允許此動作
   IF NOT apri126_action_chk() THEN
      CLOSE apri126_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_prbl_m.prbl001,g_prbl_m.prbl002,g_prbl_m.prblunit,g_prbl_m.prbl003,g_prbl_m.prbl004, 
       g_prbl_m.prbl005,g_prbl_m.prbl006,g_prbl_m.prblstus,g_prbl_m.prblownid,g_prbl_m.prblownid_desc, 
       g_prbl_m.prblowndp,g_prbl_m.prblowndp_desc,g_prbl_m.prblcrtid,g_prbl_m.prblcrtid_desc,g_prbl_m.prblcrtdp, 
       g_prbl_m.prblcrtdp_desc,g_prbl_m.prblcrtdt,g_prbl_m.prblmodid,g_prbl_m.prblmodid_desc,g_prbl_m.prblmoddt 

 
   CASE g_prbl_m.prblstus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/open.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/valid.png")
      WHEN "X"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/void.png")
      
   END CASE
 
   #add-point:資料刷新後
   {<point name="statechange.after_refresh"/>}
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_prbl_m.prblstus
            
            WHEN "N"
               HIDE OPTION "open"
            WHEN "Y"
               HIDE OPTION "valid"
            WHEN "X"
               HIDE OPTION "void"
         END CASE
     
      #add-point:menu前
      {<point name="statechange.before_menu"/>}
      #end add-point
      
      
	  
      ON ACTION open
         IF cl_auth_chk_act("open") THEN
            LET lc_state = "N"
            #add-point:action控制
            {<point name="statechange.open"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION valid
         IF cl_auth_chk_act("valid") THEN
            LET lc_state = "Y"
            #add-point:action控制
            {<point name="statechange.valid"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION void
         IF cl_auth_chk_act("void") THEN
            LET lc_state = "X"
            #add-point:action控制
            {<point name="statechange.void"/>}
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制
      {<point name="statechange.more_control"/>}
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      AND lc_state <> "X"
      ) OR 
      g_prbl_m.prblstus = lc_state OR cl_null(lc_state) THEN
      CLOSE apri126_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前
   {<point name="statechange.b_update"/>}
   #end add-point
   
   LET g_prbl_m.prblmodid = g_user
   LET g_prbl_m.prblmoddt = cl_get_current()
   LET g_prbl_m.prblstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE prbl_t 
      SET (prblstus,prblmodid,prblmoddt) 
        = (g_prbl_m.prblstus,g_prbl_m.prblmodid,g_prbl_m.prblmoddt)     
    WHERE prblent = g_enterprise AND prbl001 = g_prbl_m.prbl001
 
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/open.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/valid.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/void.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE apri126_master_referesh USING g_prbl_m.prbl001 INTO g_prbl_m.prbl001,g_prbl_m.prbl002, 
          g_prbl_m.prblunit,g_prbl_m.prbl003,g_prbl_m.prbl004,g_prbl_m.prbl005,g_prbl_m.prbl006,g_prbl_m.prblstus, 
          g_prbl_m.prblownid,g_prbl_m.prblowndp,g_prbl_m.prblcrtid,g_prbl_m.prblcrtdp,g_prbl_m.prblcrtdt, 
          g_prbl_m.prblmodid,g_prbl_m.prblmoddt,g_prbl_m.prblownid_desc,g_prbl_m.prblowndp_desc,g_prbl_m.prblcrtid_desc, 
          g_prbl_m.prblcrtdp_desc,g_prbl_m.prblmodid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_prbl_m.prbl001,g_prbl_m.prbl002,g_prbl_m.prblunit,g_prbl_m.prbl003,g_prbl_m.prbl004, 
          g_prbl_m.prbl005,g_prbl_m.prbl006,g_prbl_m.prblstus,g_prbl_m.prblownid,g_prbl_m.prblownid_desc, 
          g_prbl_m.prblowndp,g_prbl_m.prblowndp_desc,g_prbl_m.prblcrtid,g_prbl_m.prblcrtid_desc,g_prbl_m.prblcrtdp, 
          g_prbl_m.prblcrtdp_desc,g_prbl_m.prblcrtdt,g_prbl_m.prblmodid,g_prbl_m.prblmodid_desc,g_prbl_m.prblmoddt 

   END IF
 
   #add-point:stus修改後
   {<point name="statechange.a_update"/>}
   #end add-point
 
   #add-point:statechange段結束前
   {<point name="statechange.after"/>}
   #end add-point  
 
   CLOSE apri126_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL apri126_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION
 
 
]]>
  </section>
