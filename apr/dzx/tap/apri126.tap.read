  <section id="apri126.input" ver="7" status="" src="s" readonly="">
    <![CDATA[#+ 資料輸入
PRIVATE FUNCTION apri126_input(p_cmd)
   DEFINE  p_cmd                 LIKE type_t.chr1
   DEFINE  l_cmd_t               LIKE type_t.chr1
   DEFINE  l_cmd                 LIKE type_t.chr1
   DEFINE  l_n                   LIKE type_t.num10                #檢查重複用  
   DEFINE  l_cnt                 LIKE type_t.num10                #檢查重複用  
   DEFINE  l_lock_sw             LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert        LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete        LIKE type_t.num5                #可刪除否  
   DEFINE  l_count               LIKE type_t.num10
   DEFINE  l_i                   LIKE type_t.num10
   DEFINE  l_insert              BOOLEAN
   DEFINE  ls_return             STRING
   DEFINE  l_var_keys            DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys          DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                DYNAMIC ARRAY OF STRING
   DEFINE  l_fields              DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak        DYNAMIC ARRAY OF STRING
   DEFINE  lb_reproduce          BOOLEAN
   DEFINE  li_reproduce          LIKE type_t.num10
   DEFINE  li_reproduce_target   LIKE type_t.num10
   DEFINE  ls_keys               DYNAMIC ARRAY OF VARCHAR(500)
   #add-point:input段define
   {<point name="input.define" edit="s"/>}
   #end add-point  
   #add-point:input段define(客製用)
   {<point name="input.define_customerization" edit="c"/>}
   #end add-point  
   
   #先做狀態判定
   IF p_cmd = 'r' THEN
      LET l_cmd_t = 'r'
      LET p_cmd   = 'a'
   ELSE
      LET l_cmd_t = p_cmd
   END IF   
   
   #將資料輸出到畫面上
   DISPLAY BY NAME g_prbl_m.prbl001,g_prbl_m.prbl002,g_prbl_m.prblunit,g_prbl_m.prbl003,g_prbl_m.prbl004, 
       g_prbl_m.prbl005,g_prbl_m.prbl006,g_prbl_m.prblstus,g_prbl_m.prblownid,g_prbl_m.prblownid_desc, 
       g_prbl_m.prblowndp,g_prbl_m.prblowndp_desc,g_prbl_m.prblcrtid,g_prbl_m.prblcrtid_desc,g_prbl_m.prblcrtdp, 
       g_prbl_m.prblcrtdp_desc,g_prbl_m.prblcrtdt,g_prbl_m.prblmodid,g_prbl_m.prblmodid_desc,g_prbl_m.prblmoddt 

   
   #切換畫面
 
   CALL cl_set_head_visible("","YES")  
 
   LET l_insert = FALSE
   LET g_action_choice = ""
 
   #add-point:input段define_sql
   {<point name="input.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT prbmstus,prbm002,prbm003,prbm004,prbm005,prbm006,prbm007,prbm008,prbm009, 
       prbm010,prbmunit FROM prbm_t WHERE prbment=? AND prbm001=? AND prbm002=? FOR UPDATE"
   #add-point:input段define_sql
   {<point name="input.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE apri126_bcl CURSOR FROM g_forupd_sql
   
 
   
 
 
   #add-point:input段define_sql
   {<point name="input.other_sql"/>}
   #end add-point 
 
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   LET g_qryparam.state = 'i'
   
   #控制key欄位可否輸入
   CALL apri126_set_entry(p_cmd)
   #add-point:set_entry後
   {<point name="input.after_set_entry"/>}
   #end add-point
   CALL apri126_set_no_entry(p_cmd)
 
   DISPLAY BY NAME g_prbl_m.prbl001,g_prbl_m.prbl002,g_prbl_m.prblunit,g_prbl_m.prbl003,g_prbl_m.prbl004, 
       g_prbl_m.prbl005,g_prbl_m.prbl006,g_prbl_m.prblstus
   
   LET lb_reproduce = FALSE
   
   #關閉被遮罩相關欄位輸入, 無法確定USER是否會需要輸入此欄位
   #因此先行關閉, 若有需要可於下方add-point中自行開啟
   CALL cl_mask_set_no_entry()
   
   #add-point:資料輸入前
   {<point name="input.before_input"/>}
   #end add-point
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
]]>
  </section>
  <section id="apri126.input.body" ver="20" status="" src="s" readonly="">
    <![CDATA[   
      #Page1 預設值產生於此處
      INPUT ARRAY g_prbm_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,WITHOUT DEFAULTS, #MAXCOUNT = g_max_rec,
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            #add-point:資料輸入前
            {<point name="input.body.before_input2"/>}
            #end add-point
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_prbm_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL apri126_b_fill()
            #如果一直都在單身1則控制筆數位置
            IF g_loc = 'm' AND g_rec_b != 0 THEN
               CALL FGL_SET_ARR_CURR(g_detail_idx)
            END IF
            LET g_loc = 'm'
            LET g_rec_b = g_prbm_d.getLength()
            #add-point:資料輸入前
            {<point name="input.d.before_input"/>}
            #end add-point
         
         BEFORE ROW
            #add-point:modify段before row2
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET l_insert = FALSE
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET g_detail_idx = l_ac
            LET g_current_page = 1
            
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
         
            CALL s_transaction_begin()
            OPEN apri126_cl USING g_enterprise,g_prbl_m.prbl001
            IF STATUS THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "OPEN apri126_cl:" 
               LET g_errparam.code   = STATUS 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CLOSE apri126_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            
            LET g_rec_b = g_prbm_d.getLength()
            
            IF g_rec_b >= l_ac 
               AND g_prbm_d[l_ac].prbm002 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_prbm_d_t.* = g_prbm_d[l_ac].*  #BACKUP
               LET g_prbm_d_o.* = g_prbm_d[l_ac].*  #BACKUP
               CALL apri126_set_entry_b(l_cmd)
               #add-point:modify段after_set_entry_b
               {<point name="input.body.after_set_entry_b"/>}
               #end add-point  
               CALL apri126_set_no_entry_b(l_cmd)
               IF NOT apri126_lock_b("prbm_t","'1'") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH apri126_bcl INTO g_prbm_d[l_ac].prbmstus,g_prbm_d[l_ac].prbm002,g_prbm_d[l_ac].prbm003, 
                      g_prbm_d[l_ac].prbm004,g_prbm_d[l_ac].prbm005,g_prbm_d[l_ac].prbm006,g_prbm_d[l_ac].prbm007, 
                      g_prbm_d[l_ac].prbm008,g_prbm_d[l_ac].prbm009,g_prbm_d[l_ac].prbm010,g_prbm_d[l_ac].prbmunit 

                  IF SQLCA.sqlcode THEN
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = g_prbm_d_t.prbm002 
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     LET l_lock_sw = "Y"
                  END IF
                  
                  #遮罩相關處理
                  LET g_prbm_d_mask_o[l_ac].* =  g_prbm_d[l_ac].*
                  CALL apri126_prbm_t_mask()
                  LET g_prbm_d_mask_n[l_ac].* =  g_prbm_d[l_ac].*
                  
                  LET g_bfill = "N"
                  CALL apri126_show()
                  LET g_bfill = "Y"
                  
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
        
         BEFORE INSERT
            
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_prbm_d[l_ac].* TO NULL 
            INITIALIZE g_prbm_d_t.* TO NULL 
            INITIALIZE g_prbm_d_o.* TO NULL 
            #公用欄位給值(單身)
            #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_prbm_d[l_ac].prbmstus = 'N'
 
 
            #自定義預設值
                  LET g_prbm_d[l_ac].prbmstus = "Y"
      LET g_prbm_d[l_ac].prbm005 = "1"
      LET g_prbm_d[l_ac].prbm006 = "1"
      LET g_prbm_d[l_ac].prbm007 = "prbh_t"
 
            #add-point:modify段before備份
            {<point name="input.body.insert.before_bak"/>}
            #end add-point
            LET g_prbm_d_t.* = g_prbm_d[l_ac].*     #新輸入資料
            LET g_prbm_d_o.* = g_prbm_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL apri126_set_entry_b(l_cmd)
            #add-point:modify段after_set_entry_b
            {<point name="input.body.insert.after_set_entry_b"/>}
            #end add-point
            CALL apri126_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_prbm_d[li_reproduce_target].* = g_prbm_d[li_reproduce].*
 
               LET g_prbm_d[li_reproduce_target].prbm002 = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            #add-point:單身新增
            {<point name="input.body.b_a_insert"/>}
            #end add-point
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM prbm_t 
             WHERE prbment = g_enterprise AND prbm001 = g_prbl_m.prbl001
 
               AND prbm002 = g_prbm_d[l_ac].prbm002
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前
               {<point name="input.body.b_insert"/>}
               #end add-point
            
               #同步新增到同層的table
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_prbl_m.prbl001
               LET gs_keys[2] = g_prbm_d[g_detail_idx].prbm002
               CALL apri126_insert_b('prbm_t',gs_keys,"'1'")
                           
               #add-point:單身新增後
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE    
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = 'INSERT' 
               LET g_errparam.code   = "std-00006" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               INITIALIZE g_prbm_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "prbm_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL apri126_b_fill()
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert2"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               #ERROR 'INSERT O.K'
               LET g_rec_b = g_rec_b + 1
            END IF
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' THEN
               LET l_cmd='d'
               #add-point:單身刪除後(=d)
               {<point name="input.body.after_delete_d"/>}
               #end add-point
            ELSE
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "" 
                  LET g_errparam.code   = -263 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  CANCEL DELETE
               END IF
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete" mark="Y"/>}
               #end add-point 
               
               #取得該筆資料key值
               INITIALIZE gs_keys TO NULL
               LET gs_keys[01] = g_prbl_m.prbl001
 
               LET gs_keys[gs_keys.getLength()+1] = g_prbm_d_t.prbm002
 
            
               #刪除同層單身
               IF NOT apri126_delete_b('prbm_t',gs_keys,"'1'") THEN
                  CALL s_transaction_end('N','0')
                  CLOSE apri126_bcl
                  CANCEL DELETE
               END IF
    
               #刪除下層單身
               IF NOT apri126_key_delete_b(gs_keys,'prbm_t') THEN
                  CALL s_transaction_end('N','0')
                  CLOSE apri126_bcl
                  CANCEL DELETE
               END IF
               
               #刪除多語言
               
               
               #add-point:單身刪除中
               {<point name="input.body.m_delete"/>}
               #end add-point 
               
               CALL s_transaction_end('Y','0')
               CLOSE apri126_bcl
            
               LET g_rec_b = g_rec_b-1
               #add-point:單身刪除後
               {<point name="input.body.a_delete"/>}
               #end add-point
               LET l_count = g_prbm_d.getLength()
               
               #add-point:單身刪除後(<>d)
               {<point name="input.body.after_delete"/>}
               #end add-point
            END IF
 
         AFTER DELETE
            #如果是最後一筆
            IF l_ac = (g_prbm_d.getLength() + 1) THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
            END IF
 
                  #應用 a01 樣板自動產生(Version:1)
         BEFORE FIELD prbmstus
            #add-point:BEFORE FIELD prbmstus
            {<point name="input.b.page1.prbmstus" />}
            #END add-point
 
         #應用 a02 樣板自動產生(Version:1)
         AFTER FIELD prbmstus
            
            #add-point:AFTER FIELD prbmstus
            {<point name="input.a.page1.prbmstus" />}
            #END add-point
            
 
         #應用 a04 樣板自動產生(Version:2)
         ON CHANGE prbmstus
            #add-point:ON CHANGE prbmstus
            {<point name="input.g.page1.prbmstus" />}
            #END add-point 
 
         #應用 a02 樣板自動產生(Version:1)
         AFTER FIELD prbm002
            #應用 a15 樣板自動產生(Version:2)
            #確認欄位值在特定區間內
            IF NOT cl_ap_chk_range(g_prbm_d[l_ac].prbm002,"0.000","0","","","azz-00079",1) THEN
               NEXT FIELD prbm002
            END IF 
 
 
            #add-point:AFTER FIELD prbm002
            {<point name="input.a.page1.prbm002" />}
            #END add-point
            
 
         #應用 a01 樣板自動產生(Version:1)
         BEFORE FIELD prbm002
            #add-point:BEFORE FIELD prbm002
            {<point name="input.b.page1.prbm002" />}
            #END add-point
 
         #應用 a04 樣板自動產生(Version:2)
         ON CHANGE prbm002
            #add-point:ON CHANGE prbm002
            {<point name="input.g.page1.prbm002" />}
            #END add-point 
 
