  <section id="apri126.modify" ver="16" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION apri126_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   DEFINE l_wc2_table1          STRING
 
 
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point    
   #add-point:modify段define(客製用)
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point    
   
   #保存單頭舊值
   LET g_prbl_m_t.* = g_prbl_m.*
   LET g_prbl_m_o.* = g_prbl_m.*
   
   IF g_prbl_m.prbl001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_prbl001_t = g_prbl_m.prbl001
 
   CALL s_transaction_begin()
   
   OPEN apri126_cl USING g_enterprise,g_prbl_m.prbl001
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN apri126_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE apri126_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE apri126_master_referesh USING g_prbl_m.prbl001 INTO g_prbl_m.prbl001,g_prbl_m.prbl002,g_prbl_m.prblunit, 
       g_prbl_m.prbl003,g_prbl_m.prbl004,g_prbl_m.prbl005,g_prbl_m.prbl006,g_prbl_m.prblstus,g_prbl_m.prblownid, 
       g_prbl_m.prblowndp,g_prbl_m.prblcrtid,g_prbl_m.prblcrtdp,g_prbl_m.prblcrtdt,g_prbl_m.prblmodid, 
       g_prbl_m.prblmoddt,g_prbl_m.prblownid_desc,g_prbl_m.prblowndp_desc,g_prbl_m.prblcrtid_desc,g_prbl_m.prblcrtdp_desc, 
       g_prbl_m.prblmodid_desc
   
   #檢查是否允許此動作
   IF NOT apri126_action_chk() THEN
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_prbl_m_mask_o.* =  g_prbl_m.*
   CALL apri126_prbl_t_mask()
   LET g_prbl_m_mask_n.* =  g_prbl_m.*
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
 
   
   CALL apri126_show()
   #add-point:modify段show之後
   {<point name="modify.after_show"/>}
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
 
 
    
   WHILE TRUE
      LET g_prbl001_t = g_prbl_m.prbl001
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_prbl_m.prblmodid = g_user 
LET g_prbl_m.prblmoddt = cl_get_current()
LET g_prbl_m.prblmodid_desc = cl_get_username(g_prbl_m.prblmodid)
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      CALL apri126_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE prbl_t SET (prblmodid,prblmoddt) = (g_prbl_m.prblmodid,g_prbl_m.prblmoddt)
          WHERE prblent = g_enterprise AND prbl001 = g_prbl001_t
 
      END IF
    
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_prbl_m.* = g_prbl_m_t.*
         CALL apri126_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_prbl_m.prbl001 != g_prbl001_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE prbm_t SET prbm001 = g_prbl_m.prbl001
 
          WHERE prbment = g_enterprise AND prbm001 = g_prbl001_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "prbm_t" 
               LET g_errparam.code   = "std-00009" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "prbm_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL apri126_set_act_visible()   
   CALL apri126_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " prblent = '" ||g_enterprise|| "' AND",
                      " prbl001 = '", g_prbl_m.prbl001, "' "
 
   #填到對應位置
   CALL apri126_browser_fill("")
 
   CLOSE apri126_cl
   
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL apri126_msgcentre_notify('modify')
 
END FUNCTION   
]]>
  </section>
