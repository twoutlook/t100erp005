  <section id="apri126.insert" ver="16" status="" src="s" readonly="">
    <![CDATA[#+ 資料新增
PRIVATE FUNCTION apri126_insert()
   #add-point:insert段define
   {<point name="insert.define" edit="s"/>}
   #end add-point    
   #add-point:insert段define(客製用)
   {<point name="insert.define_customerization" edit="c"/>}
   #end add-point    
   
   #清畫面欄位內容
   CLEAR FORM                    
   CALL g_prbm_d.clear()   
 
 
   INITIALIZE g_prbl_m.* LIKE prbl_t.*             #DEFAULT 設定
   
   LET g_prbl001_t = NULL
 
   
   LET g_master_insert = FALSE
   
   #add-point:insert段before
   {<point name="insert.before"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   WHILE TRUE
      #公用欄位給值(單頭)
      #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_prbl_m.prblownid = g_user
      LET g_prbl_m.prblowndp = g_dept
      LET g_prbl_m.prblcrtid = g_user
      LET g_prbl_m.prblcrtdp = g_dept 
      LET g_prbl_m.prblcrtdt = cl_get_current()
      LET g_prbl_m.prblmodid = g_user
      LET g_prbl_m.prblmoddt = cl_get_current()
      LET g_prbl_m.prblstus = 'N'
 
 
 
      #append欄位給值
      
     
      #一般欄位給值
            LET g_prbl_m.prbl003 = "1"
      LET g_prbl_m.prbl005 = "1"
      LET g_prbl_m.prbl006 = "1"
 
  
      #add-point:單頭預設值
      {<point name="insert.default"/>}
      #end add-point 
      
      #保存單頭舊值(用於資料輸入錯誤還原預設值時使用)
      LET g_prbl_m_t.* = g_prbl_m.*
      LET g_prbl_m_o.* = g_prbl_m.*
      
      #顯示狀態(stus)圖片
            #應用 a21 樣板自動產生(Version:2)
	  #根據當下狀態碼顯示圖片
      CASE g_prbl_m.prblstus 
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/open.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/valid.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/void.png")
         
      END CASE
 
 
    
      CALL apri126_input("a")
      
      #add-point:單頭輸入後
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
      END IF
      
      IF NOT g_master_insert THEN
         DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
         DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
         INITIALIZE g_prbl_m.* TO NULL
         INITIALIZE g_prbm_d TO NULL
 
         #add-point:取消新增後
         {<point name="insert.cancel"/>}
         #end add-point 
         CALL apri126_show()
         RETURN
      END IF
      
      LET INT_FLAG = 0
      #CALL g_prbm_d.clear()
 
 
      LET g_rec_b = 0
      CALL s_transaction_end('Y','0')
      EXIT WHILE
        
   END WHILE
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL apri126_set_act_visible()   
   CALL apri126_set_act_no_visible()
   
   #將新增的資料併入搜尋條件中
   LET g_prbl001_t = g_prbl_m.prbl001
 
   
   #組合新增資料的條件
   LET g_add_browse = " prblent = '" ||g_enterprise|| "' AND",
                      " prbl001 = '", g_prbl_m.prbl001, "' "
 
                      
   #add-point:組合新增資料的條件後
   {<point name="insert.after.add_browse"/>}
   #end add-point
      
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL apri126_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   CLOSE apri126_cl
   
   CALL apri126_idx_chk()
   
   #撈取異動後的資料(主要是帶出reference)
   EXECUTE apri126_master_referesh USING g_prbl_m.prbl001 INTO g_prbl_m.prbl001,g_prbl_m.prbl002,g_prbl_m.prblunit, 
       g_prbl_m.prbl003,g_prbl_m.prbl004,g_prbl_m.prbl005,g_prbl_m.prbl006,g_prbl_m.prblstus,g_prbl_m.prblownid, 
       g_prbl_m.prblowndp,g_prbl_m.prblcrtid,g_prbl_m.prblcrtdp,g_prbl_m.prblcrtdt,g_prbl_m.prblmodid, 
       g_prbl_m.prblmoddt,g_prbl_m.prblownid_desc,g_prbl_m.prblowndp_desc,g_prbl_m.prblcrtid_desc,g_prbl_m.prblcrtdp_desc, 
       g_prbl_m.prblmodid_desc
   
   #遮罩相關處理
   LET g_prbl_m_mask_o.* =  g_prbl_m.*
   CALL apri126_prbl_t_mask()
   LET g_prbl_m_mask_n.* =  g_prbl_m.*
   
   #將資料顯示到畫面上
   DISPLAY BY NAME g_prbl_m.prbl001,g_prbl_m.prbl002,g_prbl_m.prblunit,g_prbl_m.prbl003,g_prbl_m.prbl004, 
       g_prbl_m.prbl005,g_prbl_m.prbl006,g_prbl_m.prblstus,g_prbl_m.prblownid,g_prbl_m.prblownid_desc, 
       g_prbl_m.prblowndp,g_prbl_m.prblowndp_desc,g_prbl_m.prblcrtid,g_prbl_m.prblcrtid_desc,g_prbl_m.prblcrtdp, 
       g_prbl_m.prblcrtdp_desc,g_prbl_m.prblcrtdt,g_prbl_m.prblmodid,g_prbl_m.prblmodid_desc,g_prbl_m.prblmoddt 

   
   #add-point:新增結束後
   {<point name="insert.after"/>}
   #end add-point 
   
   #功能已完成,通報訊息中心
   CALL apri126_msgcentre_notify('insert')
   
END FUNCTION
]]>
  </section>
