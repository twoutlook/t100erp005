  <point name="function.apri126_display_prbm008" order="2" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 動態顯示來源字段
# Memo...........:
# Usage..........: CALL apri126_display_prbm008()
# Date & Author..: 2014/03/25 By dongsz
# Modify.........:
################################################################################
PRIVATE FUNCTION apri126_display_prbm008()
DEFINE l_dzeb002      LIKE dzeb_t.dzeb002
DEFINE l_dzeb003      LIKE dzeb_t.dzeb003
DEFINE l_cnt          LIKE type_t.num5
DEFINE l_sql          STRING
DEFINE cb004          ui.ComboBox

   LET cb004 = ui.ComboBox.forName("prbm008")
   CALL cb004.clear()
   
   LET l_sql = " SELECT DISTINCT dzeb002,dzeb003 FROM dzeb_t ",
               "  WHERE dzeb001 = 'prbh_t' "
   PREPARE sel_dzeb_pre FROM l_sql
   DECLARE sel_dzeb_cs  CURSOR FOR sel_dzeb_pre
   LET l_cnt = 1
   FOREACH sel_dzeb_cs  INTO l_dzeb002,l_dzeb003
      LET l_dzeb003 = l_dzeb002,':',l_dzeb003
      IF cl_null(l_dzeb003) THEN
         CALL cb004.addItem(l_dzeb002,l_dzeb002)
      ELSE
         CALL cb004.addItem(l_dzeb002,l_dzeb003)
      END IF
      LET l_cnt = l_cnt+1
   END FOREACH
   
END FUNCTION]]>
  </point>
  <point name="function.apri126_aooi390_default" order="3" ver="2" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 自動編碼並預帶連動欄位值
# Memo...........:
# Usage..........: CALL apri126_aooi390_default()
#                  
# Input parameter: 
#                : 
# Return code....: 
#                : 
# Date & Author..: 150310 By earl
# Modify.........:
################################################################################
PRIVATE FUNCTION apri126_aooi390_default()
   DEFINE l_sql                STRING
   DEFINE l_i                  LIKE type_t.num5
   DEFINE l_success            LIKE type_t.num5
   DEFINE l_oofg_return    DYNAMIC ARRAY OF RECORD
                   oofg019     LIKE oofg_t.oofg019,   #field
                   oofg020     LIKE oofg_t.oofg020    #value
                           END RECORD

   CALL s_aooi390_auto_no('19') RETURNING l_success,g_prbl_m.prbl001,l_oofg_return
   DISPLAY BY NAME g_prbl_m.prbl001
   
   IF NOT l_success THEN
      RETURN
   END IF

   CALL s_transaction_begin()   #因為s_aooi390_auto_no中有commit
   
   DELETE FROM apri126_prbl_tmp
   
   INSERT INTO apri126_prbl_tmp (prblent,prbl001,prbl002,prblunit,prbl003,prbl004,prbl005,prbl006, 
                                 prblstus,prblownid,prblowndp,prblcrtid,prblcrtdp,prblcrtdt,prblmodid,prblmoddt)
   VALUES (g_enterprise,g_prbl_m.prbl001,g_prbl_m.prbl002,g_prbl_m.prblunit,g_prbl_m.prbl003, 
           g_prbl_m.prbl004,g_prbl_m.prbl005,g_prbl_m.prbl006,g_prbl_m.prblstus,g_prbl_m.prblownid, 
           g_prbl_m.prblowndp,g_prbl_m.prblcrtid,g_prbl_m.prblcrtdp,g_prbl_m.prblcrtdt,g_prbl_m.prblmodid, 
           g_prbl_m.prblmoddt) 
           
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "apri126_prbl_tmp" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()

      RETURN
   END IF

   FOR l_i = 1 TO l_oofg_return.getLength()
      #無設定
      IF cl_null(l_oofg_return[l_i].oofg019) THEN
         CONTINUE FOR
      END IF
      
      LET l_sql = "UPDATE apri126_prbl_tmp",
                  "   SET ",l_oofg_return[l_i].oofg019," = '",l_oofg_return[l_i].oofg020,"'"
      PREPARE apri126_aooi390_pre02 FROM l_sql
      EXECUTE apri126_aooi390_pre02
      FREE apri126_aooi390_pre02

   END FOR

   LET l_sql = " SELECT UNIQUE t0.prbl001,t0.prbl002,t0.prblunit,
                               t0.prbl003,t0.prbl004,t0.prbl005,
                               t0.prbl006,t0.prblstus,t0.prblownid,t0.prblowndp,t0.prblcrtid,t0.prblcrtdp,t0.prblcrtdt,t0.prblmodid,t0.prblmoddt, 
                               t1.ooag011 ,t2.ooefl003 ,t3.ooag011 ,t4.ooefl003 ,t5.ooag011",
               " FROM apri126_prbl_tmp t0",
               " LEFT JOIN ooag_t t1 ON t1.ooagent='"||g_enterprise||"' AND t1.ooag001=t0.prblownid  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent='"||g_enterprise||"' AND t2.ooefl001=t0.prblowndp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent='"||g_enterprise||"' AND t3.ooag001=t0.prblcrtid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=t0.prblcrtdp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent='"||g_enterprise||"' AND t5.ooag001=t0.prblmodid  "
   PREPARE apri126_aooi390_pre03 FROM l_sql

   EXECUTE apri126_aooi390_pre03 INTO g_prbl_m.prbl001,g_prbl_m.prbl002,g_prbl_m.prblunit, 
       g_prbl_m.prbl003,g_prbl_m.prbl004,g_prbl_m.prbl005,g_prbl_m.prbl006,g_prbl_m.prblstus,g_prbl_m.prblownid, 
       g_prbl_m.prblowndp,g_prbl_m.prblcrtid,g_prbl_m.prblcrtdp,g_prbl_m.prblcrtdt,g_prbl_m.prblmodid, 
       g_prbl_m.prblmoddt,g_prbl_m.prblownid_desc,g_prbl_m.prblowndp_desc,g_prbl_m.prblcrtid_desc,g_prbl_m.prblcrtdp_desc, 
       g_prbl_m.prblmodid_desc

   FREE apri126_aooi390_pre03
   
   CALL apri126_show()

END FUNCTION]]>
  </point>
