   LET g_sql = " SELECT UNIQUE t0.prcfunit,t0.prcfdocdt,t0.prcf001,t0.prcf002,t0.prcf003,t0.prcf004, 
       t0.prcf098,t0.prcfstus,t0.prcf005,t0.prcf006,t0.prcf011,t0.prcf012,t0.prcf007,t0.prcf008,t0.prcfsite, 
       t0.prcf009,t0.prcf010,t0.prcf013,t0.prcf014,t0.prcf015,t0.prcf019,t0.prcf016,t0.prcf017,t0.prcf020, 
       t0.prcf018,t0.prcf021,t0.prcf024,t0.prcf027,t0.prcf030,t0.prcf033,t0.prcf036,t0.prcf038,t0.prcf042, 
       t0.prcf045,t0.prcf022,t0.prcf025,t0.prcf028,t0.prcf031,t0.prcf034,t0.prcf037,t0.prcf039,t0.prcf043, 
       t0.prcf023,t0.prcf026,t0.prcf029,t0.prcf032,t0.prcf035,t0.prcf040,t0.prcf041,t0.prcf044,t0.prcfownid, 
       t0.prcfowndp,t0.prcfcrtid,t0.prcfcrtdp,t0.prcfcrtdt,t0.prcfmodid,t0.prcfmoddt,t0.prcfcnfid,t0.prcfcnfdt, 
       t1.ooefl003 ,t2.prcdl003 ,t3.ooag011 ,t4.ooefl003 ,t5.oocql004 ,t6.oocql004 ,t7.ooag011 ,t8.ooefl003 , 
       t9.ooag011 ,t10.ooefl003 ,t11.ooag011 ,t12.ooag011",
               " FROM prcf_t t0",
                              " LEFT JOIN ooefl_t t1 ON t1.ooeflent='"||g_enterprise||"' AND t1.ooefl001=t0.prcfunit AND t1.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN prcdl_t t2 ON t2.prcdlent='"||g_enterprise||"' AND t2.prcdl001=t0.prcf002 AND t2.prcdl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent='"||g_enterprise||"' AND t3.ooag001=t0.prcf003  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=t0.prcf004 AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t5 ON t5.oocqlent='"||g_enterprise||"' AND t5.oocql001='2100' AND t5.oocql002=t0.prcf007 AND t5.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t6 ON t6.oocqlent='"||g_enterprise||"' AND t6.oocql001='2101' AND t6.oocql002=t0.prcf008 AND t6.oocql003='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent='"||g_enterprise||"' AND t7.ooag001=t0.prcfownid  ",
               " LEFT JOIN ooefl_t t8 ON t8.ooeflent='"||g_enterprise||"' AND t8.ooefl001=t0.prcfowndp AND t8.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t9 ON t9.ooagent='"||g_enterprise||"' AND t9.ooag001=t0.prcfcrtid  ",
               " LEFT JOIN ooefl_t t10 ON t10.ooeflent='"||g_enterprise||"' AND t10.ooefl001=t0.prcfcrtdp AND t10.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t11 ON t11.ooagent='"||g_enterprise||"' AND t11.ooag001=t0.prcfmodid  ",
               " LEFT JOIN ooag_t t12 ON t12.ooagent='"||g_enterprise||"' AND t12.ooag001=t0.prcfcnfid  ",
 
               " WHERE t0.prcfent = '" ||g_enterprise|| "' AND t0.prcf001 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE aprm201_master_referesh FROM g_sql
 
    
 
   
   IF g_bgjob = "Y" THEN
