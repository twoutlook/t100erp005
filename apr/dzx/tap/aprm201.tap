   IF g_prcf_m.prcf001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN aprm201_cl USING g_enterprise,g_prcf_m.prcf001
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aprm201_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE aprm201_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE aprm201_master_referesh USING g_prcf_m.prcf001 INTO g_prcf_m.prcfunit,g_prcf_m.prcfdocdt, 
       g_prcf_m.prcf001,g_prcf_m.prcf002,g_prcf_m.prcf003,g_prcf_m.prcf004,g_prcf_m.prcf098,g_prcf_m.prcfstus, 
