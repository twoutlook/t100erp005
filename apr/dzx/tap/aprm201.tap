  <section id="aprm201.modify" ver="18" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aprm201_modify()
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point
   #add-point:modify段define(客製用)
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_prcf_m.prcf001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_prcf001_t = g_prcf_m.prcf001
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN aprm201_cl USING g_enterprise,g_prcf_m.prcf001
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aprm201_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE aprm201_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aprm201_master_referesh USING g_prcf_m.prcf001 INTO g_prcf_m.prcfunit,g_prcf_m.prcfdocdt, 
       g_prcf_m.prcf001,g_prcf_m.prcf002,g_prcf_m.prcf003,g_prcf_m.prcf004,g_prcf_m.prcf098,g_prcf_m.prcfstus, 
       g_prcf_m.prcf005,g_prcf_m.prcf006,g_prcf_m.prcf011,g_prcf_m.prcf012,g_prcf_m.prcf007,g_prcf_m.prcf008, 
       g_prcf_m.prcfsite,g_prcf_m.prcf009,g_prcf_m.prcf010,g_prcf_m.prcf013,g_prcf_m.prcf014,g_prcf_m.prcf015, 
       g_prcf_m.prcf019,g_prcf_m.prcf016,g_prcf_m.prcf017,g_prcf_m.prcf020,g_prcf_m.prcf018,g_prcf_m.prcf021, 
       g_prcf_m.prcf024,g_prcf_m.prcf027,g_prcf_m.prcf030,g_prcf_m.prcf033,g_prcf_m.prcf036,g_prcf_m.prcf038, 
       g_prcf_m.prcf042,g_prcf_m.prcf045,g_prcf_m.prcf022,g_prcf_m.prcf025,g_prcf_m.prcf028,g_prcf_m.prcf031, 
       g_prcf_m.prcf034,g_prcf_m.prcf037,g_prcf_m.prcf039,g_prcf_m.prcf043,g_prcf_m.prcf023,g_prcf_m.prcf026, 
       g_prcf_m.prcf029,g_prcf_m.prcf032,g_prcf_m.prcf035,g_prcf_m.prcf040,g_prcf_m.prcf041,g_prcf_m.prcf044, 
       g_prcf_m.prcfownid,g_prcf_m.prcfowndp,g_prcf_m.prcfcrtid,g_prcf_m.prcfcrtdp,g_prcf_m.prcfcrtdt, 
       g_prcf_m.prcfmodid,g_prcf_m.prcfmoddt,g_prcf_m.prcfcnfid,g_prcf_m.prcfcnfdt,g_prcf_m.prcfunit_desc, 
       g_prcf_m.prcf002_desc,g_prcf_m.prcf003_desc,g_prcf_m.prcf004_desc,g_prcf_m.prcf007_desc,g_prcf_m.prcf008_desc, 
       g_prcf_m.prcfownid_desc,g_prcf_m.prcfowndp_desc,g_prcf_m.prcfcrtid_desc,g_prcf_m.prcfcrtdp_desc, 
       g_prcf_m.prcfmodid_desc,g_prcf_m.prcfcnfid_desc
 
   #檢查是否允許此動作
   IF NOT aprm201_action_chk() THEN
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_prcf_m_mask_o.* =  g_prcf_m.*
   CALL aprm201_prcf_t_mask()
   LET g_prcf_m_mask_n.* =  g_prcf_m.*
   
   
 
   #顯示資料
   CALL aprm201_show()
   
   WHILE TRUE
      LET g_prcf_m.prcf001 = g_prcf001_t
 
      
      #寫入修改者/修改日期資訊
      LET g_prcf_m.prcfmodid = g_user 
LET g_prcf_m.prcfmoddt = cl_get_current()
LET g_prcf_m.prcfmodid_desc = cl_get_username(g_prcf_m.prcfmodid)
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
 
      #資料輸入
      CALL aprm201_input("u")     
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_prcf_m.* = g_prcf_m_t.*
         CALL aprm201_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE prcf_t SET (prcfmodid,prcfmoddt) = (g_prcf_m.prcfmodid,g_prcf_m.prcfmoddt)
       WHERE prcfent = g_enterprise AND prcf001 = g_prcf001_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL aprm201_set_act_visible()
   CALL aprm201_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " prcfent = '" ||g_enterprise|| "' AND",
                      " prcf001 = '", g_prcf_m.prcf001, "' "
 
   #填到對應位置
   CALL aprm201_browser_fill(g_wc,"")
 
   CLOSE aprm201_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aprm201_msgcentre_notify('modify')
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
]]>
