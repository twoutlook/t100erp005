################################################################################
# Descriptions...: 活動計劃帶值
# Memo...........:
# Usage..........: CALL aprm201_prcf002_def()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/03/25 By xumm
# Modify.........:
################################################################################
PRIVATE FUNCTION aprm201_prcf002_def()
DEFINE l_prcd      RECORD LIKE prcd_t.*

   INITIALIZE l_prcd.* TO NULL
   SELECT * INTO l_prcd.*
     FROM prcd_t
    WHERE prcdent = g_enterprise
      AND prcd001= g_prcf_m.prcf002
   LET g_prcf_m.prcf007 = l_prcd.prcd002
   LET g_prcf_m.prcf008 = l_prcd.prcd003
   LET g_prcf_m.prcf009 = l_prcd.prcd004
   LET g_prcf_m.prcf010 = l_prcd.prcd005
   DISPLAY BY NAME g_prcf_m.prcf007,g_prcf_m.prcf008,g_prcf_m.prcf009,g_prcf_m.prcf010
END FUNCTION]]>
