    LET g_prcf_m.prcf003_desc = '', g_rtn_fields[1] , ''
    DISPLAY BY NAME g_prcf_m.prcf003_desc

    INITIALIZE g_ref_fields TO NULL
    LET g_ref_fields[1] = g_prcf_m.prcf004
    CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
    LET g_prcf_m.prcf004_desc = '', g_rtn_fields[1] , ''
    DISPLAY BY NAME g_prcf_m.prcf004_desc

    INITIALIZE g_ref_fields TO NULL
    LET g_ref_fields[1] = g_prcf_m.prcf007
