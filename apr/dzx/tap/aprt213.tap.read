  <point name="browser_fill.other_fill" order="" ver="7" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   CALL g_prde8_d.clear()
   
   IF g_prda_m.prda019 = '2' THEN
      LET g_sql = "SELECT  UNIQUE prdjacti,prdj002,prdj003,'',prdj005,'',prdjsite,prdjunit,prdj001 FROM prdj_t",   
                  " INNER JOIN prda_t ON prdadocno = prdjdocno ",
                  " WHERE prdaent = ? AND prdadocno = ? "
   ELSE
      LET g_sql = "SELECT  UNIQUE prdjacti,prdj002,prdj003,prdj004,prdj005,prdj006,prdjsite,prdjunit,prdj001 FROM prdj_t",   
               " INNER JOIN prda_t ON prdadocno = prdjdocno ",
               " WHERE prdaent = ? AND prdadocno = ? "
   END IF                     
                
    LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
 
    IF NOT cl_null(g_wc2_table7) THEN
       LET g_sql = g_sql CLIPPED," AND ",g_wc2_table7 CLIPPED
    END IF
    
    #子單身的WC
    
    IF g_prda_m.prda019 = '2' THEN
       LET g_sql = g_sql, " ORDER BY prdj_t.prdj002,prdj_t.prdj003"
    ELSE
       LET g_sql = g_sql, " ORDER BY prdj_t.prdj002,prdj_t.prdj003,prdj_t.prdj004"
    END IF

    
    LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
    PREPARE aprt213_pb71 FROM g_sql
    DECLARE b_fill_cs71 CURSOR FOR aprt213_pb71

    
    LET l_ac = 1
    
    OPEN b_fill_cs71 USING g_enterprise,g_prda_m.prdadocno
                                             
    FOREACH b_fill_cs71 INTO g_prde8_d[l_ac].prdjacti,g_prde8_d[l_ac].prdj002,g_prde8_d[l_ac].prdj003, 
        g_prde8_d[l_ac].prdj004,g_prde8_d[l_ac].prdj005,g_prde8_d[l_ac].prdj006,g_prde8_d[l_ac].prdjsite, 
        g_prde8_d[l_ac].prdjunit,g_prde8_d[l_ac].prdj001
       IF SQLCA.sqlcode THEN
          INITIALIZE g_errparam TO NULL 
          LET g_errparam.extend = "FOREACH:" 
          LET g_errparam.code   = SQLCA.sqlcode 
          LET g_errparam.popup  = TRUE 
          CALL cl_err()
          EXIT FOREACH
       END IF
            
       CALL aprt213_prdj_desc()     
       
       LET l_ac = l_ac + 1
       IF l_ac > g_max_rec THEN
          INITIALIZE g_errparam TO NULL 
          LET g_errparam.extend = l_ac
          LET g_errparam.code   = 9035 
          LET g_errparam.popup  = TRUE 
          CALL cl_err()
          EXIT FOREACH
       END IF
       
    END FOREACH
