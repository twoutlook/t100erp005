               #add-point:modify段after_set_entry_b
               {<point name="input.body.after_set_entry_b"/>}
               #end add-point  
               CALL aprt213_set_no_entry_b(l_cmd)
               IF NOT aprt213_lock_b("prde_t","'1'") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aprt213_bcl INTO g_prde_d[l_ac].prde002,g_prde_d[l_ac].prde001,g_prde_d[l_ac].prdeacti, 
                      g_prde_d[l_ac].prde003,g_prde_d[l_ac].prde004,g_prde_d[l_ac].prdesite,g_prde_d[l_ac].prdeunit 

