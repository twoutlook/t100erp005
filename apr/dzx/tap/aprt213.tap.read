   DEFINE l_detail3    RECORD LIKE prdf_t.*
 
   DEFINE l_detail4    RECORD LIKE prdd_t.*
 
   DEFINE l_detail5    RECORD LIKE prdh_t.*
 
   DEFINE l_detail6    RECORD LIKE prdb_t.*
 
   DEFINE l_detail7    RECORD LIKE prdj_t.*
 
   DEFINE l_detail8    RECORD LIKE prdk_t.*
 
 
 
   DEFINE l_cnt       LIKE type_t.num10
   #add-point:reproduce段define
   {<point name="reproduce.define" edit="s"/>}
   #end add-point   
   #add-point:reproduce段define(客製用)
