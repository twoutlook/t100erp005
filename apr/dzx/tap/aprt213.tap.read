# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/04/09 By xumm
# Modify.........:
################################################################################
PRIVATE FUNCTION aprt213_detail_init()
DEFINE l_prdl               RECORD LIKE prdl_t.*

   INITIALIZE l_prdl.* TO NULL
   SELECT * INTO l_prdl.*
     FROM prdl_t
    WHERE prdlent = g_enterprise
      AND prdl001 = g_prda_m.prda001 
   #INS促銷規則申請生效組織資料檔
   INSERT INTO prde_t(prdedocno,prdeent,prdeunit,prdesite,prde001,prde002,prde003,prde004,prdeacti)
   SELECT g_prda_m.prdadocno,prdpent,prdpunit,prdpsite,prdp001,prdp002,prdp003,prdp004,prdpstus
     FROM prdp_t
    WHERE prdpent = g_enterprise
      AND prdp001 = g_prda_m.prda001
   
   #INS促銷規則申請商品範圍資料檔
   IF g_prda_m.prda010 = 'N' THEN
      INSERT INTO prdg_t(prdgdocno,prdgent,prdgunit,prdgsite,prdg001,prdg002,prdg003,prdg004,prdg005,prdg006,prdg007,prdgacti,prdg008,prdg009,prdg010,prdg011)
      SELECT g_prda_m.prdadocno,prdrent,prdrunit,prdrsite,prdr001,prdr002,prdr003,prdr004,prdr005,prdr006,prdr007,prdrstus,prdr008,prdr009,prdr010,prdr011
        FROM prdr_t
       WHERE prdrent = g_enterprise
         AND prdr001 = g_prda_m.prda001
   END IF
   
   #INS促銷規則申請款別限定資料檔
   IF g_prda_m.prda011 = 'Y' THEN
      INSERT INTO prdf_t(prdfdocno,prdfent,prdfunit,prdfsite,prdf001,prdf002,prdf003,prdf004,prdfacti)
      SELECT g_prda_m.prdadocno,prdqent,prdqunit,prdqsite,prdq001,prdq002,prdq003,prdq004,prdqstus
        FROM prdq_t
       WHERE prdqent = g_enterprise
         AND prdq001 = g_prda_m.prda001
   END IF
   
   #INS促銷規則申請時間資料檔
   IF g_prda_m.prda013 = 'Y' THEN
      DELETE FROM prdd_t WHERE prddent = g_enterprise AND prdddocno = g_prda_m.prdadocno
      INSERT INTO prdd_t(prdddocno,prddent,prddunit,prddsite,prdd001,prdd002,prdd003,prdd004,prdd005,prdd006,prdd007,prdd008,prddacti)
      SELECT g_prda_m.prdadocno,prdoent,prdounit,prdosite,prdo001,prdo002,prdo003,prdo004,prdo005,prdo006,prdo007,prdo008,prdostus
        FROM prdo_t
       WHERE prdoent = g_enterprise
         AND prdo001 = g_prda_m.prda001
   END IF
   
   #INS促銷規則申請對象主檔
   #INS促銷規則申請對象明細檔
   
   INSERT INTO prdb_t(prdbdocno,prdbent,prdbunit,prdbsite,prdb001,prdb002,prdb003,prdb004,prdb005,prdbacti)
   SELECT g_prda_m.prdadocno,prdment,prdmunit,prdmsite,prdm001,prdm002,prdm003,prdm004,prdm005,prdmstus
     FROM prdm_t
    WHERE prdment = g_enterprise
      AND prdm001 = g_prda_m.prda001
   
   IF g_prda_m.prda026 = '4' THEN   
      INSERT INTO prdc_t(prdcdocno,prdcent,prdcunit,prdcsite,prdc001,prdc002,prdc003,prdc004,prdcacti)
      SELECT g_prda_m.prdadocno,prdnent,prdnunit,prdnsite,prdn001,prdn002,prdn003,prdn004,prdnstus
        FROM prdn_t
       WHERE prdnent = g_enterprise
         AND prdn001 = g_prda_m.prda001   
   END IF
  
   IF l_prdl.prdl026='4' THEN
      DELETE FROM prdb_t WHERE prdbent = g_enterprise AND prdbdocno = g_prda_m.prdadocno AND (prdb003 = '2' OR prdb003 = '3')      
   END IF
   
   IF g_prda_m.prda026 = '1' THEN
      DELETE FROM prdb_t WHERE prdbent = g_enterprise AND prdbdocno = g_prda_m.prdadocno AND (prdb003 = '2' OR prdb003 = '3')
      DELETE FROM prdc_t WHERE prdcent = g_enterprise AND prdcdocno = g_prda_m.prdadocno 
   END IF
   IF g_prda_m.prda026 = '2' THEN
      DELETE FROM prdb_t WHERE prdbent = g_enterprise AND prdbdocno = g_prda_m.prdadocno AND (prdb003 = '1' OR prdb003 = '3')     
      DELETE FROM prdc_t WHERE prdcent = g_enterprise AND prdcdocno = g_prda_m.prdadocno 
   END IF
   IF g_prda_m.prda026 = '3' THEN               
      DELETE FROM prdc_t WHERE prdcent = g_enterprise AND prdcdocno = g_prda_m.prdadocno 
   END IF   
   
   #INS促銷規則申請組合條件資料檔
   INSERT INTO prdh_t(prdhdocno,prdhent,prdhunit,prdhsite,prdh001,prdh002,prdh003,prdh005,prdh006,prdh007,prdh008,prdhacti)
   SELECT g_prda_m.prdadocno,prdsent,prdsunit,prdssite,prds001,prds002,prds003,prds005,prds006,prds007,prds008,prdsstus
     FROM prds_t
    WHERE prdsent = g_enterprise
      AND prds001 = g_prda_m.prda001
      
   #INS促銷規則申請換贈設定資料檔
   #INS促銷規則申請換贈商品資料檔
   IF g_prda_m.prda016 = 'Y'  AND g_prda_m.prda026=l_prdl.prdl026 THEN
      UPDATE prda_t SET prda017 = l_prdl.prdl017,
                        prda018 = l_prdl.prdl018,
                        prda019 = l_prdl.prdl019,
                        prda020 = l_prdl.prdl020,
                        prda021 = l_prdl.prdl021,
                        prda022 = l_prdl.prdl022,
                        prda023 = l_prdl.prdl023
       WHERE prdadocno = g_prda_m.prdadocno
         AND prdaent = g_enterprise
      INSERT INTO prdj_t(prdjdocno,prdjent,prdjunit,prdjsite,prdj001,prdj002,prdj003,prdj004,prdj005,prdj006,prdjacti)
      SELECT g_prda_m.prdadocno,prduent,prduunit,prdusite,prdu001,prdu002,prdu003,prdu004,prdu005,prdu006,prdustus
        FROM prdu_t
       WHERE prduent = g_enterprise
         AND prdu001 = g_prda_m.prda001
      INSERT INTO prdk_t(prdkdocno,prdkent,prdkunit,prdksite,prdk001,prdk002,prdk003,prdk004,prdk005,prdk006,prdk007,prdk008,prdk009,prdk010,prdk011,prdk012,prdkacti)
      SELECT g_prda_m.prdadocno,prdvent,prdvunit,prdvsite,prdv001,prdv002,prdv003,prdv004,prdv005,prdv006,prdv007,prdv008,prdv009,prdv010,prdv011,prdv012,prdvstus
        FROM prdv_t
       WHERE prdvent = g_enterprise
         AND prdv001 = g_prda_m.prda001  
   END IF         
END FUNCTION]]>
