                  CALL aprt213_prde_t_mask()
                  LET g_prde_d_mask_n[l_ac].* =  g_prde_d[l_ac].*
                  
                  LET g_bfill = "N"
                  CALL aprt213_show()
                  LET g_bfill = "Y"
                  
