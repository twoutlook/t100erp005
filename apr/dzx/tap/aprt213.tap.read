      PREPARE l_sql_mmap003 FROM l_sql
      DECLARE l_sql_mmap003_cs CURSOR FOR l_sql_mmap003
      FOREACH l_sql_mmap003_cs INTO l_mmap003,l_mmap005
         INSERT INTO aprt213_tmp VALUES (l_mmap003)
         IF SQLCA.sqlcode THEN
            LET g_errno = SQLCA.sqlcode
            RETURN
         END IF
         IF l_mmap005='Y' THEN
            EXECUTE sel_ooed_pre1 USING l_mmap003
            IF SQLCA.sqlcode THEN
               LET g_errno = SQLCA.sqlcode
               RETURN
            END IF
         END IF
      END FOREACH
      SELECT gcafstus INTO l_gcafstus
