   #end add-point   
   
   #add-point:browser_fill段動作開始前
   {<point name="browser_fill.before_browser_fill"/>}
   #end add-point
   
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   LET l_wc  = g_wc.trim() 
   LET l_wc2 = g_wc2.trim()
 
   #add-point:browser_fill,foreach前
   {<point name="browser_fill.before_foreach"/>}
   #end add-point
   
   IF g_wc2 <> " 1=1" THEN
      #單身有輸入搜尋條件                      
      LET l_sub_sql = " SELECT UNIQUE prdadocno ",
                      " FROM prda_t ",
                      " ",
                      " LEFT JOIN prde_t ON prdeent = prdaent AND prdadocno = prdedocno ", "  ",
                      #add-point:browser_fill段sql(prde_t1)
                      {<point name="browser_fill.cnt.join.}"/>}
                      #end add-point
                      " LEFT JOIN prdg_t ON prdgent = prdaent AND prdadocno = prdgdocno", "  ",
                      #add-point:browser_fill段sql(prdg_t1)
                      {<point name="browser_fill.cnt.join.prdg_t1"/>}
                      #end add-point
 
                      " LEFT JOIN prdf_t ON prdfent = prdaent AND prdadocno = prdfdocno", "  ",
                      #add-point:browser_fill段sql(prdf_t1)
                      {<point name="browser_fill.cnt.join.prdf_t1"/>}
                      #end add-point
 
                      " LEFT JOIN prdd_t ON prddent = prdaent AND prdadocno = prdddocno", "  ",
                      #add-point:browser_fill段sql(prdd_t1)
                      {<point name="browser_fill.cnt.join.prdd_t1"/>}
                      #end add-point
 
                      " LEFT JOIN prdh_t ON prdhent = prdaent AND prdadocno = prdhdocno", "  ",
                      #add-point:browser_fill段sql(prdh_t1)
                      {<point name="browser_fill.cnt.join.prdh_t1"/>}
                      #end add-point
 
                      " LEFT JOIN prdb_t ON prdbent = prdaent AND prdadocno = prdbdocno", "  ",
                      #add-point:browser_fill段sql(prdb_t1)
                      {<point name="browser_fill.cnt.join.prdb_t1"/>}
                      #end add-point
 
                      " LEFT JOIN prdj_t ON prdjent = prdaent AND prdadocno = prdjdocno", "  ",
                      #add-point:browser_fill段sql(prdj_t1)
                      {<point name="browser_fill.cnt.join.prdj_t1"/>}
                      #end add-point
 
                      " LEFT JOIN prdk_t ON prdkent = prdaent AND prdadocno = prdkdocno", "  ",
                      #add-point:browser_fill段sql(prdk_t1)
                      {<point name="browser_fill.cnt.join.prdk_t1"/>}
                      #end add-point
 
 
 
                      " LEFT JOIN prdal_t ON prdalent = '"||g_enterprise||"' AND prdadocno = prdaldocno AND prdal001 = '",g_dlang,"' ", 
                      " ", 
                      " WHERE prdaent = '" ||g_enterprise|| "' AND prdeent = '" ||g_enterprise|| "' AND ",l_wc, " AND ", l_wc2, cl_sql_add_filter("prda_t")
   ELSE
      #單身未輸入搜尋條件
      LET l_sub_sql = " SELECT UNIQUE prdadocno ",
                      " FROM prda_t ", 
                      "  ",
                      "  LEFT JOIN prdal_t ON prdalent = '"||g_enterprise||"' AND prdadocno = prdaldocno AND prdal001 = '",g_dlang,"' ",
                      " WHERE prdaent = '" ||g_enterprise|| "' AND ",l_wc CLIPPED, cl_sql_add_filter("prda_t")
   END IF
   
   #add-point:browser_fill,cnt wc
   {<point name="browser_fill.cnt_sqlwc"/>}
