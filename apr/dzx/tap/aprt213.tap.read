    <![CDATA[
################################################################################
# Descriptions...: 批量新增换赠资料
# Memo...........:
# Usage..........: CALL s_aprt213_ins_prdj()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2015/04/29 By huangrh
# Modify.........:
################################################################################
PRIVATE FUNCTION aprt213_ins_prdj()
DEFINE l_sql             STRING
DEFINE l_prdj            RECORD LIKE prdj_t.*
DEFINE l_prdj002         LIKE prdj_t.prdj002
DEFINE l_prdj003         LIKE prdj_t.prdj003

   IF g_prda_m.prda019 = '2' THEN
      LET l_prdj002 = ''
      LET l_prdj003 = ''
      LET l_sql = "SELECT DISTINCT prdj002,prdj003 ",
                  "  FROM prdj_t",
                  " WHERE prdjent = '",g_enterprise,"'",
                  "   AND prdjdocno = '",g_prda_m.prdadocno,"'",
                  "   AND prdj003 = '",g_prde6_d[l_ac].prdb004,"'"
      PREPARE aprt213_sel_prdj_pre FROM l_sql
      DECLARE aprt213_sel_prdj_cur CURSOR FOR aprt213_sel_prdj_pre
      FOREACH aprt213_sel_prdj_cur INTO l_prdj002,l_prdj003
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "FOREACH:"
            LET g_errparam.popup = TRUE
            CALL cl_err()

            RETURN FALSE
         END IF
         INITIALIZE l_prdj.* TO NULL
         LET l_prdj.prdj002 = l_prdj002
         LET l_prdj.prdj003 = g_prde6_d[l_ac].prdb004
         SELECT prda001 INTO l_prdj.prdj001
           FROM prda_t
          WHERE prdaent = g_enterprise
            AND prdadocno = g_prda_m.prdadocno 
         SELECT MIN(prdj004) INTO l_prdj.prdj004
           FROM prdj_t
          WHERE prdjent = g_enterprise
            AND prdjdocno = g_prda_m.prdadocno
            AND prdj002 = l_prdj.prdj002
            AND prdj003 = l_prdj003
            AND prdj004 IN (SELECT prdb002 
                            FROM prdb_t
                           WHERE prdbent = g_enterprise
                             AND prdbdocno = g_prda_m.prdadocno
                             AND prdb003 = g_prde6_d[l_ac].prdb003
                             AND prdb004 = l_prdj003)
         IF NOT cl_null(l_prdj.prdj004) THEN 
            SELECT prdj005,prdj006 
              INTO l_prdj.prdj005,l_prdj.prdj006
              FROM prdj_t
             WHERE prdjent = g_enterprise
               AND prdjdocno = g_prda_m.prdadocno
               AND prdj002 = l_prdj.prdj002
               AND prdj003 = l_prdj003
               AND prdj004 = l_prdj.prdj004
         END IF      
         IF cl_null(l_prdj.prdj005) THEN
            SELECT MIN(prdj005) INTO l_prdj.prdj005
              FROM prdj_t
             WHERE prdjent = g_enterprise
               AND prdjdocno = g_prda_m.prdadocno
               AND prdj002 = l_prdj.prdj002
               AND prdj003 = l_prdj003
            IF cl_null(l_prdj.prdj005) THEN
               LET l_prdj.prdj005 = 0 
            END IF            
         END IF      
         IF cl_null(l_prdj.prdj006) THEN
            LET l_prdj.prdj006 = 0                       
         END IF               
         LET l_prdj.prdj004 = g_prde6_d[l_ac].prdb002
         LET l_prdj.prdjent = g_enterprise
         LET l_prdj.prdjunit = g_site
         LET l_prdj.prdjsite = g_site
         LET l_prdj.prdjdocno = g_prda_m.prdadocno
         LET l_prdj.prdjacti = 'Y'
         INSERT INTO prdj_t VALUES(l_prdj.*)
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "ins_prdj"
            LET g_errparam.popup = TRUE
            CALL cl_err()

            RETURN FALSE
         END IF
      END FOREACH
   END IF
   RETURN TRUE
END FUNCTION]]>
