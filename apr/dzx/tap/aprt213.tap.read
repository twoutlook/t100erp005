                              s_browse[1].b_prdadocdt,s_browse[1].b_prda000,s_browse[1].b_prda001,s_browse[1].b_prda002, 
                              s_browse[1].b_prda006,s_browse[1].b_prda007,s_browse[1].b_prda027
 
         BEFORE CONSTRUCT
               DISPLAY aprt213_filter_parser('prdasite') TO s_browse[1].b_prdasite
            DISPLAY aprt213_filter_parser('prdaunit') TO s_browse[1].b_prdaunit
            DISPLAY aprt213_filter_parser('prdadocno') TO s_browse[1].b_prdadocno
            DISPLAY aprt213_filter_parser('prdadocdt') TO s_browse[1].b_prdadocdt
            DISPLAY aprt213_filter_parser('prda000') TO s_browse[1].b_prda000
            DISPLAY aprt213_filter_parser('prda001') TO s_browse[1].b_prda001
            DISPLAY aprt213_filter_parser('prda002') TO s_browse[1].b_prda002
            DISPLAY aprt213_filter_parser('prda006') TO s_browse[1].b_prda006
            DISPLAY aprt213_filter_parser('prda007') TO s_browse[1].b_prda007
