   LET g_forupd_sql = " SELECT prdasite,'',prdadocdt,prdadocno,prda000,prda001,prda002,prdaacti,'',prdaunit, 
       '',prda006,'',prda007,'',prda027,prdastus,prda010,prda011,prda012,prda013,prda024,prda025,prda026, 
