                   prdacrtdp,prdacrtdt,prdaownid,prdaowndp,prdamodid,prdamoddt,prdacnfid,prdacnfdt,prda017, 
                   prda019,prda020,prda022,prda018,prda021,prda023) = (g_prda_m.prdasite,g_prda_m.prdadocdt, 
                   g_prda_m.prdadocno,g_prda_m.prda000,g_prda_m.prda001,g_prda_m.prda002,g_prda_m.prdaacti, 
                   g_prda_m.prdaunit,g_prda_m.prda006,g_prda_m.prda007,g_prda_m.prda027,g_prda_m.prdastus, 
                   g_prda_m.prda010,g_prda_m.prda011,g_prda_m.prda012,g_prda_m.prda013,g_prda_m.prda024, 
                   g_prda_m.prda025,g_prda_m.prda026,g_prda_m.prda098,g_prda_m.prda015,g_prda_m.prda016, 
                   g_prda_m.prda004,g_prda_m.prda005,g_prda_m.prda003,g_prda_m.prda033,g_prda_m.prda008, 
                   g_prda_m.prda009,g_prda_m.prdacrtid,g_prda_m.prdacrtdp,g_prda_m.prdacrtdt,g_prda_m.prdaownid, 
                   g_prda_m.prdaowndp,g_prda_m.prdamodid,g_prda_m.prdamoddt,g_prda_m.prdacnfid,g_prda_m.prdacnfdt, 
                   g_prda_m.prda017,g_prda_m.prda019,g_prda_m.prda020,g_prda_m.prda022,g_prda_m.prda018, 
                   g_prda_m.prda021,g_prda_m.prda023)
                WHERE prdaent = g_enterprise AND prdadocno = g_prdadocno_t
 
               IF SQLCA.sqlcode THEN
                  INITIALIZE g_errparam TO NULL 
