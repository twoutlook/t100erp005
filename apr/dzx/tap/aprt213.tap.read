  <section id="aprt213.reproduce" ver="31" status="" src="s" readonly="">
    <![CDATA[#+ 資料複製
PRIVATE FUNCTION aprt213_reproduce()
   DEFINE l_newno     LIKE prda_t.prdadocno 
   DEFINE l_oldno     LIKE prda_t.prdadocno 
 
   DEFINE l_master    RECORD LIKE prda_t.*
   DEFINE l_detail    RECORD LIKE prde_t.*
