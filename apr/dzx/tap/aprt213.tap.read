  <point name="function.aprt213_prda006_def" order="5" ver="6" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 促銷方案帶值
# Memo...........:
# Usage..........: CALL aprt213_prda006_def()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/03/14 By xumm
# Modify.........:
################################################################################
PRIVATE FUNCTION aprt213_prda006_def()
DEFINE l_sql             STRING
DEFINE l_prde002         LIKE prde_t.prde002
DEFINE l_prce            RECORD LIKE prce_t.*

   SELECT prcf002,prcf007,prcf008,prcf009,prcf010
     INTO g_prda_m.prda007,g_prda_m.prda008,g_prda_m.prda009,g_prda_m.l_prdd003_1,g_prda_m.l_prdd004_1
     FROM prcf_t
    WHERE prcfent = g_enterprise
      AND prcf001 = g_prda_m.prda006
   DISPLAY BY NAME g_prda_m.prda007,g_prda_m.prda008,g_prda_m.prda009,g_prda_m.l_prdd003_1,g_prda_m.l_prdd004_1
   IF NOT cl_null(g_prda_m.prda007) THEN
      DELETE FROM prde_t WHERE prdeent = g_enterprise AND prdedocno = g_prda_m.prdadocno
      LET l_prde002 = 1
      LET l_sql = " SELECT * FROM prce_t",
                  "  WHERE prceent = '",g_enterprise,"'",
                  "    AND prce001 = '",g_prda_m.prda007,"'"
      PREPARE aprt213_sel_prce_pre FROM l_sql
      DECLARE aprt213_sel_prce_cur CURSOR FOR aprt213_sel_prce_pre
      FOREACH aprt213_sel_prce_cur INTO l_prce.*
         INSERT INTO prde_t(prdeent,prdeunit,prdesite,prdedocno,prde001,prde002,prde003,prde004,prdeacti)
                     VALUES(g_enterprise,g_site,g_site,g_prda_m.prdadocno,g_prda_m.prda001,l_prde002,l_prce.prce002,l_prce.prce003,l_prce.prceacti)
         LET l_prde002 = l_prde002+1            
      END FOREACH
      CALL aprt213_b_fill() 
   END IF
END FUNCTION]]>
  </point>
