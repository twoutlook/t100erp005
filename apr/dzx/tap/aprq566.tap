  <section id="aprq566.b_fill" ver="3" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aprq566_b_fill()
   {<Local define>}
   DEFINE ls_wc           STRING
   DEFINE l_pid           LIKE type_t.chr50
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_detail_idx  = 1
   LET g_detail_idx2 = 1
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
 
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
 
   CALL g_prck_d.clear()
 
   #add-point:陣列清空
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   
 
   LET g_cnt = l_ac
   LET l_ac = 1
   ERROR "Searching!"
 
   # b_fill段sql組成及FOREACH撰寫
   #+ 此段落由子樣板qs04產生
   #+ b_fill段資料取得(包含sql組成及FOREACH段撰寫)
   LET g_sql = "SELECT  UNIQUE prcksite,'',prck001,'',prck002,'','','','','','','','','','','','','', 
       '','','','','','','','','' FROM prck_t",
 
 
               "",
               " WHERE prckent= ? AND 1=1 AND ", ls_wc
 
   LET g_sql = g_sql, " ORDER BY prck_t.prckdocno"
 
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   PREPARE aprq566_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aprq566_pb
 
   FOREACH b_fill_curs INTO g_prck_d[l_ac].prcksite,g_prck_d[l_ac].prcksite_desc,g_prck_d[l_ac].prck001, 
       g_prck_d[l_ac].prck001_desc,g_prck_d[l_ac].prck002,g_prck_d[l_ac].prck002_desc,g_prck_d[l_ac].prcl002, 
       g_prck_d[l_ac].prcl002_desc,g_prck_d[l_ac].prcl003,g_prck_d[l_ac].prcl003_desc,g_prck_d[l_ac].prcl004, 
       g_prck_d[l_ac].prcl004_desc,g_prck_d[l_ac].prcl005,g_prck_d[l_ac].prcl005_desc,g_prck_d[l_ac].prcl006, 
       g_prck_d[l_ac].prcl006_desc,g_prck_d[l_ac].prcl007,g_prck_d[l_ac].prcl007_desc,g_prck_d[l_ac].prcl008, 
       g_prck_d[l_ac].prcl008_desc,g_prck_d[l_ac].prcl009,g_prck_d[l_ac].prcl009_desc,g_prck_d[l_ac].prcl010, 
       g_prck_d[l_ac].prcl011,g_prck_d[l_ac].prcl012,g_prck_d[l_ac].prcl013,g_prck_d[l_ac].prcl014
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
 
      LET g_prck_d[l_ac].sel = "N"
 
      
 
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL aprq566_detail_show("'1'")
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            CALL cl_err( "", 9035, 1 )
         END IF
         EXIT FOREACH
      END IF
 
   END FOREACH
 
 
 
 
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   CALL g_prck_d.deleteElement(g_prck_d.getLength())
 
   #add-point:陣列長度調整
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
 
   LET g_error_show = 0
 
   LET g_detail_cnt = l_ac - 1
   DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
 
   #+ 此段落由子樣板qs06產生
   #+ b_fill段CURSOR釋放
   CLOSE b_fill_curs
   FREE aprq566_pb
 
 
 
 
   LET l_ac = 1
   CALL aprq566_b_fill2()
 
      CALL aprq566_filter_show('prcksite','b_prcksite')
   CALL aprq566_filter_show('prck001','b_prck001')
   CALL aprq566_filter_show('prck002','b_prck002')
   CALL aprq566_filter_show('prcl002','b_prcl002')
   CALL aprq566_filter_show('prcl003','b_prcl003')
   CALL aprq566_filter_show('prcl004','b_prcl004')
   CALL aprq566_filter_show('prcl005','b_prcl005')
   CALL aprq566_filter_show('prcl006','b_prcl006')
   CALL aprq566_filter_show('prcl007','b_prcl007')
   CALL aprq566_filter_show('prcl008','b_prcl008')
   CALL aprq566_filter_show('prcl009','b_prcl009')
   CALL aprq566_filter_show('prcl010','b_prcl010')
   CALL aprq566_filter_show('prcl011','b_prcl011')
   CALL aprq566_filter_show('prcl012','b_prcl012')
   CALL aprq566_filter_show('prcl013','b_prcl013')
   CALL aprq566_filter_show('prcl014','b_prcl014')
 
 
END FUNCTION
]]>
