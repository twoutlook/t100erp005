   CALL gfrm_curr.setFieldHidden("formonly.sel", TRUE)
   CALL gfrm_curr.setFieldHidden("formonly.b_statepic", TRUE)
 
   
 
   LET g_wc = cl_replace_str(g_wc, g_wc_filter, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   #+ 此段落由子樣板qs08產生
   #+ filter段DIALOG段的組成
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON prcksite,prck001,prck002,prcl002,prcl003,prcl004,prcl005,prcl006,prcl007, 
          prcl008,prcl009,prcl010,prcl011,prcl012,prcl013,prcl014
                          FROM s_detail1[1].b_prcksite,s_detail1[1].b_prck001,s_detail1[1].b_prck002, 
                              s_detail1[1].b_prcl002,s_detail1[1].b_prcl003,s_detail1[1].b_prcl004,s_detail1[1].b_prcl005, 
                              s_detail1[1].b_prcl006,s_detail1[1].b_prcl007,s_detail1[1].b_prcl008,s_detail1[1].b_prcl009, 
                              s_detail1[1].b_prcl010,s_detail1[1].b_prcl011,s_detail1[1].b_prcl012,s_detail1[1].b_prcl013, 
                              s_detail1[1].b_prcl014
 
         BEFORE CONSTRUCT
            CALL cl_qbe_init()
                     DISPLAY aprq566_filter_parser('prcksite') TO s_detail1[1].b_prcksite
            DISPLAY aprq566_filter_parser('prck001') TO s_detail1[1].b_prck001
            DISPLAY aprq566_filter_parser('prck002') TO s_detail1[1].b_prck002
            DISPLAY aprq566_filter_parser('prcl002') TO s_detail1[1].b_prcl002
            DISPLAY aprq566_filter_parser('prcl003') TO s_detail1[1].b_prcl003
            DISPLAY aprq566_filter_parser('prcl004') TO s_detail1[1].b_prcl004
            DISPLAY aprq566_filter_parser('prcl005') TO s_detail1[1].b_prcl005
            DISPLAY aprq566_filter_parser('prcl006') TO s_detail1[1].b_prcl006
            DISPLAY aprq566_filter_parser('prcl007') TO s_detail1[1].b_prcl007
            DISPLAY aprq566_filter_parser('prcl008') TO s_detail1[1].b_prcl008
            DISPLAY aprq566_filter_parser('prcl009') TO s_detail1[1].b_prcl009
            DISPLAY aprq566_filter_parser('prcl010') TO s_detail1[1].b_prcl010
            DISPLAY aprq566_filter_parser('prcl011') TO s_detail1[1].b_prcl011
            DISPLAY aprq566_filter_parser('prcl012') TO s_detail1[1].b_prcl012
            DISPLAY aprq566_filter_parser('prcl013') TO s_detail1[1].b_prcl013
            DISPLAY aprq566_filter_parser('prcl014') TO s_detail1[1].b_prcl014
 
 
      END CONSTRUCT
 
      #add-point:filter段add_cs
      {<point name="filter.add_cs"/>}
      #end add-point
 
      BEFORE DIALOG
         #add-point:filter段b_dialog
         {<point name="filter.b_dialog"/>}
         #end add-point
 
      ON ACTION accept
         ACCEPT DIALOG
 
      ON ACTION cancel
         LET INT_FLAG = 1
         EXIT DIALOG
 
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE DIALOG
 
   END DIALOG
 
 
 
   
 
   IF NOT INT_FLAG THEN
      LET g_wc_filter = g_wc_filter, " "
   ELSE
      LET g_wc_filter = g_wc_filter_t
   END IF
 
      CALL aprq566_filter_show('prcksite','b_prcksite')
   CALL aprq566_filter_show('prck001','b_prck001')
   CALL aprq566_filter_show('prck002','b_prck002')
   CALL aprq566_filter_show('prcl002','b_prcl002')
