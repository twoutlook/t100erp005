  <section id="aprm502.modify" ver="1" status="" src="s">
<![CDATA[#+ 資料修改
PRIVATE FUNCTION aprm502_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_prfg_m.prfg001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
    SELECT UNIQUE prfg001,prfg002,prfgunit,prfgstus,prfgownid,prfgowndp,prfgcrtid,prfgcrtdp,prfgcrtdt, 
        prfgmodid,prfgmoddt
 INTO g_prfg_m.prfg001,g_prfg_m.prfg002,g_prfg_m.prfgunit,g_prfg_m.prfgstus,g_prfg_m.prfgownid,g_prfg_m.prfgowndp, 
     g_prfg_m.prfgcrtid,g_prfg_m.prfgcrtdp,g_prfg_m.prfgcrtdt,g_prfg_m.prfgmodid,g_prfg_m.prfgmoddt
 FROM prfg_t
 WHERE prfgent = g_enterprise AND prfg001 = g_prfg_m.prfg001
 
   ERROR ""
  
   LET g_prfg001_t = g_prfg_m.prfg001
 
   CALL s_transaction_begin()
   
   OPEN aprm502_cl USING g_enterprise,g_prfg_m.prfg001
 
   IF STATUS THEN
      CALL cl_err("OPEN aprm502_cl:", STATUS, 1)
      CLOSE aprm502_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aprm502_cl INTO g_prfg_m.prfg001,g_prfg_m.prfg002,g_prfg_m.prfgunit,g_prfg_m.prfgl003,g_prfg_m.prfgl004, 
       g_prfg_m.prfgstus,g_prfg_m.prfgownid,g_prfg_m.prfgownid_desc,g_prfg_m.prfgowndp,g_prfg_m.prfgowndp_desc, 
       g_prfg_m.prfgcrtid,g_prfg_m.prfgcrtid_desc,g_prfg_m.prfgcrtdp,g_prfg_m.prfgcrtdp_desc,g_prfg_m.prfgcrtdt, 
       g_prfg_m.prfgmodid,g_prfg_m.prfgmodid_desc,g_prfg_m.prfgmoddt
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_prfg_m.prfg001,SQLCA.sqlcode,0)
      CLOSE aprm502_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   CALL aprm502_show()
   WHILE TRUE
      LET g_prfg001_t = g_prfg_m.prfg001
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_prfg_m.prfgmodid = g_user 
LET g_prfg_m.prfgmoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL aprm502_input("u")     #欄位更改
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_prfg_m.* = g_prfg_m_t.*
         CALL aprm502_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_prfg_m.prfg001 != g_prfg001_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE prfh_t SET prfh001 = g_prfg_m.prfg001
 
          WHERE prfhent = g_enterprise AND prfh001 = g_prfg001_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("prfh_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("prfh_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
         #更新單身key值
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update2" mark="Y"/>}
         #end add-point
         UPDATE pmaa_t
            SET  = g_prfg_m.prfg001
 
          WHERE pmaaent = g_enterprise AND
                 = g_prfg001_t
 
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update2"/>}
         #end add-point
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("pmaa_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("pmaa_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update2"/>}
         #end add-point
 
 
         
         #UPDATE 多語言table key值
         
         
 
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_prfg_m.prfg001,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE aprm502_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_prfg_m.prfg001,'U')
 
END FUNCTION   
]]>
</section>
