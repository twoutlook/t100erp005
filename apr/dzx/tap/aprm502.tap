   DEFINE  l_cnt                 LIKE type_t.num5                #檢查重複用  
   DEFINE  l_lock_sw             LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert        LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete        LIKE type_t.num5                #可刪除否  
   DEFINE  l_count               LIKE type_t.num5
   DEFINE  l_i                   LIKE type_t.num5
   DEFINE  l_insert              BOOLEAN
   DEFINE  ls_return             STRING
   DEFINE  l_var_keys            DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys          DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                DYNAMIC ARRAY OF STRING
   DEFINE  l_fields              DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak        DYNAMIC ARRAY OF STRING
   DEFINE  lb_reproduce          BOOLEAN
   DEFINE  li_reproduce          LIKE type_t.num5
   DEFINE  li_reproduce_target   LIKE type_t.num5
   #add-point:input段define
   {<point name="input.define"/>}
   #end add-point  
 
   #先做狀態判定
   IF p_cmd = 'r' THEN
      LET l_cmd_t = 'r'
      LET p_cmd   = 'a'
   ELSE
      LET l_cmd_t = p_cmd
   END IF   
   
   #切換畫面
 
   CALL cl_set_head_visible("","YES")  
 
   LET l_insert = FALSE
   LET g_action_choice = ""
 
   #add-point:input段define_sql
   {<point name="input.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT prfhstus,prfh002,prfh003,prfh004,'',prfhunit FROM prfh_t WHERE prfhent=?  
       AND prfh001=? AND prfh002=? AND prfh003=? AND prfh004=? FOR UPDATE"
   #add-point:input段define_sql
   {<point name="input.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   DECLARE aprm502_bcl CURSOR FROM g_forupd_sql
   
 
   
   #add-point:input段define_sql
   {<point name="input.define_sql2" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT '','','','' FROM pmaa_t WHERE pmaaent=? AND pmaa001=? FOR UPDATE"
   #add-point:input段define_sql
   {<point name="input.after_define_sql2"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   DECLARE aprm502_bcl2 CURSOR FROM g_forupd_sql
 
 
 
   #add-point:input段define_sql
   {<point name="input.other_sql"/>}
   #end add-point 
 
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   LET g_qryparam.state = 'i'
   
   #控制key欄位可否輸入
   CALL aprm502_set_entry(p_cmd)
   #add-point:set_entry後
   {<point name="input.after_set_entry"/>}
   #end add-point
   CALL aprm502_set_no_entry(p_cmd)
 
   DISPLAY BY NAME g_prfg_m.prfg001,g_prfg_m.prfg002,g_prfg_m.prfgunit,g_prfg_m.prfgl003,g_prfg_m.prfgl004, 
       g_prfg_m.prfgstus
   
   LET lb_reproduce = FALSE
   
   #add-point:資料輸入前
   {<point name="input.before_input"/>}
   #end add-point
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
   
]]>
</section>
  <section id="aprm502.input.body" ver="1" status="" src="s">
<![CDATA[   
      #Page1 預設值產生於此處
      INPUT ARRAY g_prfh_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_prfh_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL aprm502_b_fill()
            LET g_rec_b = g_prfh_d.getLength()
            #add-point:資料輸入前
            {<point name="input.d.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET g_detail_idx = l_ac
                           CALL aprm502_b_fill2('2')
 
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
         
            CALL s_transaction_begin()
            OPEN aprm502_cl USING g_enterprise,g_prfg_m.prfg001
 
            IF STATUS THEN
               CALL cl_err("OPEN aprm502_cl:", STATUS, 1)
               CLOSE aprm502_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            
            LET g_rec_b = g_prfh_d.getLength()
            
            IF g_rec_b >= l_ac 
               AND g_prfh_d[l_ac].prfh002 IS NOT NULL
               AND g_prfh_d[l_ac].prfh003 IS NOT NULL
 
               AND g_prfh_d[l_ac].prfh004 IS NOT NULL
 
 
            THEN
               LET l_cmd='u'
               LET g_prfh_d_t.* = g_prfh_d[l_ac].*  #BACKUP
               CALL aprm502_set_entry_b(l_cmd)
               #add-point:modify段after_set_entry_b
               {<point name="input.body.after_set_entry_b"/>}
               #end add-point  
               CALL aprm502_set_no_entry_b(l_cmd)
               IF NOT aprm502_lock_b("prfh_t","'1'") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aprm502_bcl INTO g_prfh_d[l_ac].prfhstus,g_prfh_d[l_ac].prfh002,g_prfh_d[l_ac].prfh003, 
                      g_prfh_d[l_ac].prfh004,g_prfh_d[l_ac].prfh004_desc,g_prfh_d[l_ac].prfhunit
                  IF SQLCA.sqlcode THEN
                     CALL cl_err(g_prfh_d_t.prfh002,SQLCA.sqlcode,1)
                     LET l_lock_sw = "Y"
                  END IF
                  
                  LET g_bfill = "N"
                  CALL aprm502_show()
                  LET g_bfill = "Y"
                  
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
        
         BEFORE INSERT
            
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_prfh_d[l_ac].* TO NULL 
                  LET g_prfh_d[l_ac].prfhstus = "Y"
 
 
            LET g_prfh_d_t.* = g_prfh_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL aprm502_set_entry_b(l_cmd)
            #add-point:modify段after_set_entry_b
            {<point name="input.body.insert.after_set_entry_b"/>}
            #end add-point
            CALL aprm502_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_prfh_d[li_reproduce_target].* = g_prfh_d[li_reproduce].*
 
               LET g_prfh_d[li_reproduce_target].prfh002 = NULL
               LET g_prfh_d[li_reproduce_target].prfh003 = NULL
 
               LET g_prfh_d[li_reproduce_target].prfh004 = NULL
 
 
            END IF
            #公用欄位給值(單身)
            #此段落由子樣板a14產生    
      #LET g_prfh2_d[l_ac].prfdownid = g_user
      #LET g_prfh2_d[l_ac].prfdowndp = g_dept
      #LET g_prfh2_d[l_ac].prfdcrtid = g_user
      #LET g_prfh2_d[l_ac].prfdcrtdp = g_dept 
      #LET g_prfh2_d[l_ac].prfdcrtdt = cl_get_current()
      #LET g_prfh2_d[l_ac].prfdmodid = ""
      #LET g_prfh2_d[l_ac].prfdmoddt = ""
      #LET g_prfh_d[l_ac].prfhstus = ""
 
 
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            #add-point:單身新增
            {<point name="input.body.b_a_insert"/>}
            #end add-point
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM prfh_t 
             WHERE prfhent = g_enterprise AND prfh001 = g_prfg_m.prfg001
 
               AND prfh002 = g_prfh_d[l_ac].prfh002
               AND prfh003 = g_prfh_d[l_ac].prfh003
 
               AND prfh004 = g_prfh_d[l_ac].prfh004
 
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前
               {<point name="input.body.b_insert"/>}
               #end add-point
            
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_prfg_m.prfg001
               LET gs_keys[2] = g_prfh_d[g_detail_idx].prfh002
               LET gs_keys[3] = g_prfh_d[g_detail_idx].prfh003
               LET gs_keys[4] = g_prfh_d[g_detail_idx].prfh004
               CALL aprm502_insert_b('prfh_t',gs_keys,"'1'")
                           
               #add-point:單身新增後
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_prfh_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("prfh_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL aprm502_b_fill()
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert2"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR 'INSERT O.K'
               LET g_rec_b = g_rec_b + 1
            END IF
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_prfh_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_prfh_d.deleteElement(l_ac)
               NEXT FIELD prfh002
            END IF
         
            IF g_prfh_d[l_ac].prfh002 IS NOT NULL
               AND g_prfh_d_t.prfh003 IS NOT NULL
 
               AND g_prfh_d_t.prfh004 IS NOT NULL
 
 
               THEN 
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete" mark="Y"/>}
               #end add-point 
               
               DELETE FROM prfh_t
                WHERE prfhent = g_enterprise AND prfh001 = g_prfg_m.prfg001 AND
 
                      prfh002 = g_prfh_d_t.prfh002
                  AND prfh003 = g_prfh_d_t.prfh003
 
                  AND prfh004 = g_prfh_d_t.prfh004
 
 
                  
               #add-point:單身刪除中
               {<point name="input.body.m_delete"/>}
               #end add-point 
               
               IF SQLCA.sqlcode THEN
                  CALL cl_err("prfh_t",SQLCA.sqlcode,1)
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               ELSE
                  LET g_rec_b = g_rec_b-1
                  
                  #add-point:單身刪除後
                  {<point name="input.body.a_delete"/>}
                  #end add-point
                  CALL s_transaction_end('Y','0')
               END IF 
               CLOSE aprm502_bcl
               LET l_count = g_prfh_d.getLength()
            END IF 
            
                           INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_prfg_m.prfg001
               LET gs_keys[2] = g_prfh_d[g_detail_idx].prfh002
               LET gs_keys[3] = g_prfh_d[g_detail_idx].prfh003
               LET gs_keys[4] = g_prfh_d[g_detail_idx].prfh004
 
              
         AFTER DELETE 
            #add-point:單身刪除後2
            {<point name="input.body.after_delete"/>}
            #end add-point
                           CALL aprm502_delete_b('prfh_t',gs_keys,"'1'")
 
         #---------------------<  Detail: page1  >---------------------
         #----<<prfhstus>>----
         #此段落由子樣板a01產生
         BEFORE FIELD prfhstus
            #add-point:BEFORE FIELD prfhstus
            {<point name="input.b.page1.prfhstus" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD prfhstus
            
            #add-point:AFTER FIELD prfhstus
            {<point name="input.a.page1.prfhstus" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE prfhstus
            #add-point:ON CHANGE prfhstus
            {<point name="input.g.page1.prfhstus" />}
            #END add-point
 
         #----<<prfh002>>----
         #此段落由子樣板a02產生
         AFTER FIELD prfh002
            #此段落由子樣板a15產生
            IF NOT cl_ap_chk_Range(g_prfh_d[l_ac].prfh002,"0.000","1","","","azz-00079",1) THEN
               NEXT FIELD prfh002
            END IF
 
 
            #add-point:AFTER FIELD prfh002
            {<point name="input.a.page1.prfh002" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD prfh002
            #add-point:BEFORE FIELD prfh002
            {<point name="input.b.page1.prfh002" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE prfh002
            #add-point:ON CHANGE prfh002
            {<point name="input.g.page1.prfh002" />}
            #END add-point
 
         #----<<prfh003>>----
         #此段落由子樣板a01產生
         BEFORE FIELD prfh003
            #add-point:BEFORE FIELD prfh003
            {<point name="input.b.page1.prfh003" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD prfh003
            
            #add-point:AFTER FIELD prfh003
            {<point name="input.a.page1.prfh003" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE prfh003
            #add-point:ON CHANGE prfh003
            {<point name="input.g.page1.prfh003" />}
            #END add-point
 
         #----<<prfh004>>----
         #此段落由子樣板a01產生
         BEFORE FIELD prfh004
            #add-point:BEFORE FIELD prfh004
            {<point name="input.b.page1.prfh004" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD prfh004
            
            #add-point:AFTER FIELD prfh004
            {<point name="input.a.page1.prfh004" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE prfh004
            #add-point:ON CHANGE prfh004
            {<point name="input.g.page1.prfh004" />}
            #END add-point
 
         #----<<prfhunit>>----
         #此段落由子樣板a01產生
         BEFORE FIELD prfhunit
            #add-point:BEFORE FIELD prfhunit
            {<point name="input.b.page1.prfhunit" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD prfhunit
            
            #add-point:AFTER FIELD prfhunit
            {<point name="input.a.page1.prfhunit" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE prfhunit
            #add-point:ON CHANGE prfhunit
            {<point name="input.g.page1.prfhunit" />}
            #END add-point
 
 
         #---------------------<  Detail: page1  >---------------------
         #----<<prfhstus>>----
         #Ctrlp:input.c.page1.prfhstus
         ON ACTION controlp INFIELD prfhstus
            #add-point:ON ACTION controlp INFIELD prfhstus
            {<point name="input.c.page1.prfhstus" />}
            #END add-point
 
         #----<<prfh002>>----
         #Ctrlp:input.c.page1.prfh002
         ON ACTION controlp INFIELD prfh002
            #add-point:ON ACTION controlp INFIELD prfh002
            {<point name="input.c.page1.prfh002" />}
            #END add-point
 
         #----<<prfh003>>----
         #Ctrlp:input.c.page1.prfh003
         ON ACTION controlp INFIELD prfh003
            #add-point:ON ACTION controlp INFIELD prfh003
            {<point name="input.c.page1.prfh003" />}
            #END add-point
 
         #----<<prfh004>>----
         #Ctrlp:input.c.page1.prfh004
         ON ACTION controlp INFIELD prfh004
            #add-point:ON ACTION controlp INFIELD prfh004
            {<point name="input.c.page1.prfh004" />}
            #END add-point
 
         #----<<prfhunit>>----
         #Ctrlp:input.c.page1.prfhunit
         ON ACTION controlp INFIELD prfhunit
            #add-point:ON ACTION controlp INFIELD prfhunit
            {<point name="input.c.page1.prfhunit" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_prfh_d[l_ac].* = g_prfh_d_t.*
               CLOSE aprm502_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_prfh_d[l_ac].prfh002,-263,1)
               LET g_prfh_d[l_ac].* = g_prfh_d_t.*
            ELSE
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               #寫入修改者/修改日期資訊(單身)
               #LET g_prfh2_d[l_ac].prfdmodid = g_user 
#LET g_prfh2_d[l_ac].prfdmoddt = cl_get_current()
 
      
               UPDATE prfh_t SET (prfh001,prfhstus,prfh002,prfh003,prfh004,prfhunit) = (g_prfg_m.prfg001, 
                   g_prfh_d[l_ac].prfhstus,g_prfh_d[l_ac].prfh002,g_prfh_d[l_ac].prfh003,g_prfh_d[l_ac].prfh004, 
                   g_prfh_d[l_ac].prfhunit)
                WHERE prfhent = g_enterprise AND prfh001 = g_prfg_m.prfg001 
 
                  AND prfh002 = g_prfh_d_t.prfh002 #項次   
                  AND prfh003 = g_prfh_d_t.prfh003  
 
                  AND prfh004 = g_prfh_d_t.prfh004  
 
 
                  
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("prfh_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                     LET g_prfh_d[l_ac].* = g_prfh_d_t.*
                  WHEN SQLCA.sqlcode #其他錯誤
                     CALL cl_err("prfh_t",SQLCA.sqlcode,1)
                     LET g_prfh_d[l_ac].* = g_prfh_d_t.*                     
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_prfg_m.prfg001
               LET gs_keys_bak[1] = g_prfg001_t
               LET gs_keys[2] = g_prfh_d[g_detail_idx].prfh002
               LET gs_keys_bak[2] = g_prfh_d_t.prfh002
               LET gs_keys[3] = g_prfh_d[g_detail_idx].prfh003
               LET gs_keys_bak[3] = g_prfh_d_t.prfh003
               LET gs_keys[4] = g_prfh_d[g_detail_idx].prfh004
               LET gs_keys_bak[4] = g_prfh_d_t.prfh004
               CALL aprm502_update_b('prfh_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
               END CASE
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            #add-point:單身after_row
            {<point name="input.body.after_row"/>}
            #end add-point
            CALL aprm502_unlock_b("prfh_t","'1'")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock
            
              
         AFTER INPUT
            #add-point:input段after input 
            {<point name="input.body.after_input"/>}
            #end add-point 
 
         ON ACTION controlo    
            CALL FGL_SET_ARR_CURR(g_prfh_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_prfh_d.getLength()+1
            
         #ON ACTION cancel
         #   LET INT_FLAG = 1
         #   LET g_detail_idx = 1
         #   EXIT DIALOG 
 
      END INPUT
      
 
      
 
      
 
      
      DISPLAY ARRAY g_prfh2_d TO s_detail2.* ATTRIBUTES(COUNT=g_rec_b)  
    
         BEFORE ROW 
            CALL aprm502_idx_chk()
            LET l_ac = DIALOG.getCurrentRow("s_detail2")
            LET g_detail_idx2 = l_ac
            
            #add-point:page2, before row動作
            {<point name="input.body2.before_row"/>}
            #end add-point
            
         BEFORE DISPLAY
            CALL FGL_SET_ARR_CURR(g_detail_idx)
            LET l_ac = DIALOG.getCurrentRow("s_detail2")
            CALL aprm502_idx_chk()
            LET g_current_page = 2
      
         #自訂ACTION(detail_show,page_2)
         
      
         #add-point:page2自定義行為
         {<point name="input.body2.action"/>}
         #end add-point
      
      END DISPLAY
 
 
      
]]>
</section>
  <section id="aprm502.input.head" ver="1" status="" src="s">
<![CDATA[      #單頭段
      INPUT BY NAME g_prfg_m.prfg001,g_prfg_m.prfg002,g_prfg_m.prfgunit,g_prfg_m.prfgl003,g_prfg_m.prfgl004, 
