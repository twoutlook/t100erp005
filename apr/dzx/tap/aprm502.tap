  <section id="aprm502.delete" ver="1" status="" src="s">
<![CDATA[#+ 資料刪除
PRIVATE FUNCTION aprm502_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_prfg_m.prfg001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
    SELECT UNIQUE prfg001,prfg002,prfgunit,prfgstus,prfgownid,prfgowndp,prfgcrtid,prfgcrtdp,prfgcrtdt, 
        prfgmodid,prfgmoddt
 INTO g_prfg_m.prfg001,g_prfg_m.prfg002,g_prfg_m.prfgunit,g_prfg_m.prfgstus,g_prfg_m.prfgownid,g_prfg_m.prfgowndp, 
     g_prfg_m.prfgcrtid,g_prfg_m.prfgcrtdp,g_prfg_m.prfgcrtdt,g_prfg_m.prfgmodid,g_prfg_m.prfgmoddt
 FROM prfg_t
 WHERE prfgent = g_enterprise AND prfg001 = g_prfg_m.prfg001
   
   LET g_master_multi_table_t.prfgl001 = g_prfg_m.prfg001
LET g_master_multi_table_t.prfgl003 = g_prfg_m.prfgl003
LET g_master_multi_table_t.prfgl004 = g_prfg_m.prfgl004
 
 
   CALL aprm502_show()
   
   CALL s_transaction_begin()
 
   OPEN aprm502_cl USING g_enterprise,g_prfg_m.prfg001
 
   IF STATUS THEN
      CALL cl_err("OPEN aprm502_cl:", STATUS, 1)
      CLOSE aprm502_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   FETCH aprm502_cl INTO g_prfg_m.prfg001,g_prfg_m.prfg002,g_prfg_m.prfgunit,g_prfg_m.prfgl003,g_prfg_m.prfgl004, 
       g_prfg_m.prfgstus,g_prfg_m.prfgownid,g_prfg_m.prfgownid_desc,g_prfg_m.prfgowndp,g_prfg_m.prfgowndp_desc, 
       g_prfg_m.prfgcrtid,g_prfg_m.prfgcrtid_desc,g_prfg_m.prfgcrtdp,g_prfg_m.prfgcrtdp_desc,g_prfg_m.prfgcrtdt, 
       g_prfg_m.prfgmodid,g_prfg_m.prfgmodid_desc,g_prfg_m.prfgmoddt              #鎖住將被更改或取消的資料 

   IF SQLCA.sqlcode THEN
      CALL cl_err(g_prfg_m.prfg001,SQLCA.sqlcode,0)          #資料被他人LOCK
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:delete段before ask
   {<point name="delete.before_ask"/>}
   #end add-point 
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
   
      #add-point:單頭刪除前
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point   
      
      INITIALIZE g_doc.* TO NULL         
      LET g_doc.column1 = "prfg001"       
      LET g_doc.value1 = g_prfg_m.prfg001     
      CALL cl_doc_remove()   
  
      #資料備份
      LET g_prfg001_t = g_prfg_m.prfg001
 
 
      DELETE FROM prfg_t
       WHERE prfgent = g_enterprise AND prfg001 = g_prfg_m.prfg001
 
       
      #add-point:單頭刪除中
      {<point name="delete.head.m_delete"/>}
      #end add-point
       
      IF SQLCA.sqlcode THEN
         CALL cl_err(g_prfg_m.prfg001,SQLCA.sqlcode,0) 
         CALL s_transaction_end('N','0')
         RETURN
      END IF
      
      #add-point:單頭刪除後
      {<point name="delete.head.a_delete"/>}
      #end add-point
  
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM prfh_t
       WHERE prfhent = g_enterprise AND prfh001 = g_prfg_m.prfg001
 
 
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
         
      IF SQLCA.sqlcode THEN
         CALL cl_err("prfh_t",SQLCA.sqlcode,0) 
         CALL s_transaction_end('N','0')
         RETURN
      END IF    
 
      #add-point:單身刪除後
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
            
                                                               
 
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete2" mark="Y"/>}
      #end add-point
      DELETE FROM pmaa_t
       WHERE pmaaent = g_enterprise AND
              = g_prfg_m.prfg001
      #add-point:單身刪除中
      {<point name="delete.body.m_delete2"/>}
      #end add-point
      IF SQLCA.sqlcode THEN
         CALL cl_err("pmaa_t",SQLCA.sqlcode,0) 
         CALL s_transaction_end('N','0')
         RETURN
      END IF      
 
      #add-point:單身刪除後
      {<point name="delete.body.a_delete2"/>}
      #end add-point
 
 
                                                               
      CLEAR FORM
      CALL g_prfh_d.clear() 
      CALL g_prfh2_d.clear()       
 
 
     
      CALL aprm502_ui_browser_refresh()  
      CALL aprm502_ui_headershow()  
      CALL aprm502_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL aprm502_fetch('P')
         DISPLAY g_browser_cnt TO FORMONLY.h_count   #總筆數的顯示
      ELSE
         LET g_wc = " 1=1"
         CALL aprm502_browser_fill("F")
      END IF
      
      #單頭多語言刪除
      INITIALIZE l_var_keys_bak TO NULL 
   INITIALIZE l_field_keys   TO NULL 
