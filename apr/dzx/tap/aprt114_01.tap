    END IF

    #插入单身数据
    FOR l_i = 1 TO g_prbg_d.getLength()
        IF cl_null(g_prbg_d[l_i].prbg002) THEN
           EXIT FOR
        END IF
        IF g_flagprog='aprt114' OR g_flagprog='aprt115' OR g_flagprog='aprt116' OR g_flagprog='aprt117' THEN
