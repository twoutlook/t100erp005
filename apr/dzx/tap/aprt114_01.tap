                              WHERE prbgent = g_enterprise AND prbgdocno = g_docno AND prbg002 = p_prbg002)
         IF l_cnt > 0 THEN
            RETURN FALSE
         END IF
      END IF
      IF g_prbg001 = '1' THEN
