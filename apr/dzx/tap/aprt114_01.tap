################################################################################
PRIVATE FUNCTION aprt114_01_pinsert()
DEFINE l_count     LIKE type_t.num5
DEFINE l_i         LIKE type_t.num5
DEFINE l_prbg      RECORD LIKE prbg_t.*
DEFINE l_prbg_t    RECORD LIKE prbg_t.*
