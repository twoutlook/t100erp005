      {<point name="delete.lang.delete"/>}
      #end add-point
   
   END IF
 
   CALL s_transaction_end('Y','0')
   CLOSE aprt111_cl
 
   #功能已完成,通報訊息中心
   CALL aprt111_msgcentre_notify('delete')
    
