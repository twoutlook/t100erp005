  <section id="aprt501.input" ver="6" status="" src="s">
<![CDATA[#+ 資料輸入
PRIVATE FUNCTION aprt501_input(p_cmd)
   DEFINE  p_cmd                 LIKE type_t.chr1
   DEFINE  l_cmd_t               LIKE type_t.chr1
   DEFINE  l_cmd                 LIKE type_t.chr1
   DEFINE  l_n                   LIKE type_t.num5                #檢查重複用  
   DEFINE  l_cnt                 LIKE type_t.num5                #檢查重複用  
   DEFINE  l_lock_sw             LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert        LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete        LIKE type_t.num5                #可刪除否  
   DEFINE  l_count               LIKE type_t.num5
   DEFINE  l_i                   LIKE type_t.num5
   DEFINE  l_insert              BOOLEAN
   DEFINE  ls_return             STRING
   DEFINE  l_var_keys            DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys          DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                DYNAMIC ARRAY OF STRING
   DEFINE  l_fields              DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak        DYNAMIC ARRAY OF STRING
   DEFINE  lb_reproduce          BOOLEAN
   DEFINE  li_reproduce          LIKE type_t.num5
   DEFINE  li_reproduce_target   LIKE type_t.num5
   #add-point:input段define
   {<point name="input.define"/>}
   #end add-point  
 
   #先做狀態判定
   IF p_cmd = 'r' THEN
      LET l_cmd_t = 'r'
      LET p_cmd   = 'a'
   ELSE
      LET l_cmd_t = p_cmd
   END IF   
   
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF
 
   CALL cl_set_head_visible("","YES")  
 
   LET l_insert = FALSE
   LET g_action_choice = ""
 
   #add-point:input段define_sql
   {<point name="input.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT prfbacti,prfb001,prfb002,prfb003,'',prfbsite,prfbunit FROM prfb_t WHERE  
       prfbent=? AND prfbdocno=? AND prfb001=? AND prfb002=? AND prfb003=? FOR UPDATE"
   #add-point:input段define_sql
   {<point name="input.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   DECLARE aprt501_bcl CURSOR FROM g_forupd_sql
   
 
   
 
 
   #add-point:input段define_sql
   {<point name="input.other_sql"/>}
   #end add-point 
 
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   LET g_qryparam.state = 'i'
   
   #控制key欄位可否輸入
   CALL aprt501_set_entry(p_cmd)
   #add-point:set_entry後
   {<point name="input.after_set_entry"/>}
   #end add-point
   CALL aprt501_set_no_entry(p_cmd)
 
   DISPLAY BY NAME g_prfa_m.prfaunit,g_prfa_m.prfadocdt,g_prfa_m.prfadocno,g_prfa_m.prfa001,g_prfa_m.prfa002, 
       g_prfa_m.prfa003,g_prfa_m.prfaacti,g_prfa_m.prfal002,g_prfa_m.prfal003,g_prfa_m.prfa004,g_prfa_m.prfa005, 
       g_prfa_m.prfasite,g_prfa_m.prfastus
   
   LET lb_reproduce = FALSE
   
   #add-point:資料輸入前
   {<point name="input.before_input"/>}
   #end add-point
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
   
]]>
</section>
  <section id="aprt501.input.body" ver="8" status="" src="s">
<![CDATA[   
      #Page1 預設值產生於此處
      INPUT ARRAY g_prfb_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_prfb_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL aprt501_b_fill()
            LET g_rec_b = g_prfb_d.getLength()
            #add-point:資料輸入前
            {<point name="input.d.before_input"/>}
            #end add-point
         
         BEFORE ROW
            #add-point:modify段before row2
            {<point name="input.body.before_row2"/>}
            #end add-point              
            LET l_insert = FALSE
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET g_detail_idx = l_ac
            
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
         
            CALL s_transaction_begin()
            OPEN aprt501_cl USING g_enterprise,g_prfa_m.prfadocno
 
            IF STATUS THEN
               CALL cl_err("OPEN aprt501_cl:", STATUS, 1)
               CLOSE aprt501_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            
            LET g_rec_b = g_prfb_d.getLength()
            
            IF g_rec_b >= l_ac 
               AND g_prfb_d[l_ac].prfb001 IS NOT NULL
               AND g_prfb_d[l_ac].prfb002 IS NOT NULL
               AND g_prfb_d[l_ac].prfb003 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_prfb_d_t.* = g_prfb_d[l_ac].*  #BACKUP
               CALL aprt501_set_entry_b(l_cmd)
               #add-point:modify段after_set_entry_b
               {<point name="input.body.after_set_entry_b"/>}
               #end add-point  
               CALL aprt501_set_no_entry_b(l_cmd)
               IF NOT aprt501_lock_b("prfb_t","'1'") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aprt501_bcl INTO g_prfb_d[l_ac].prfbacti,g_prfb_d[l_ac].prfb001,g_prfb_d[l_ac].prfb002, 
                      g_prfb_d[l_ac].prfb003,g_prfb_d[l_ac].prfb003_desc,g_prfb_d[l_ac].prfbsite,g_prfb_d[l_ac].prfbunit 

                  IF SQLCA.sqlcode THEN
                     CALL cl_err(g_prfb_d_t.prfb001,SQLCA.sqlcode,1)
                     LET l_lock_sw = "Y"
                  END IF
                  
                  LET g_bfill = "N"
                  CALL aprt501_show()
                  LET g_bfill = "Y"
                  
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
        
         BEFORE INSERT
            
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_prfb_d[l_ac].* TO NULL 
                  LET g_prfb_d[l_ac].prfbacti = "Y"
 
 
            LET g_prfb_d_t.* = g_prfb_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL aprt501_set_entry_b(l_cmd)
            #add-point:modify段after_set_entry_b
            {<point name="input.body.insert.after_set_entry_b"/>}
            #end add-point
            CALL aprt501_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_prfb_d[li_reproduce_target].* = g_prfb_d[li_reproduce].*
 
               LET g_prfb_d[li_reproduce_target].prfb001 = NULL
               LET g_prfb_d[li_reproduce_target].prfb002 = NULL
               LET g_prfb_d[li_reproduce_target].prfb003 = NULL
 
            END IF
            #公用欄位給值(單身)
            
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            #add-point:單身新增
            {<point name="input.body.b_a_insert"/>}
            #end add-point
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM prfb_t 
             WHERE prfbent = g_enterprise AND prfbdocno = g_prfa_m.prfadocno
 
               AND prfb001 = g_prfb_d[l_ac].prfb001
               AND prfb002 = g_prfb_d[l_ac].prfb002
               AND prfb003 = g_prfb_d[l_ac].prfb003
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前
               {<point name="input.body.b_insert"/>}
               #end add-point
            
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_prfa_m.prfadocno
               LET gs_keys[2] = g_prfb_d[g_detail_idx].prfb001
               LET gs_keys[3] = g_prfb_d[g_detail_idx].prfb002
               LET gs_keys[4] = g_prfb_d[g_detail_idx].prfb003
               CALL aprt501_insert_b('prfb_t',gs_keys,"'1'")
                           
               #add-point:單身新增後
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_prfb_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("prfb_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL aprt501_b_fill()
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert2"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR 'INSERT O.K'
               LET g_rec_b = g_rec_b + 1
            END IF
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_prfb_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_prfb_d.deleteElement(l_ac)
               NEXT FIELD prfb001
            END IF
         
            IF g_prfb_d[l_ac].prfb001 IS NOT NULL
               AND g_prfb_d_t.prfb002 IS NOT NULL
               AND g_prfb_d_t.prfb003 IS NOT NULL
 
               THEN 
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete" mark="Y"/>}
               #end add-point 
               
               DELETE FROM prfb_t
                WHERE prfbent = g_enterprise AND prfbdocno = g_prfa_m.prfadocno AND
 
                      prfb001 = g_prfb_d_t.prfb001
                  AND prfb002 = g_prfb_d_t.prfb002
                  AND prfb003 = g_prfb_d_t.prfb003
 
                  
               #add-point:單身刪除中
               {<point name="input.body.m_delete"/>}
               #end add-point 
               
               IF SQLCA.sqlcode THEN
                  CALL cl_err("prfb_t",SQLCA.sqlcode,1)
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               ELSE
                  LET g_rec_b = g_rec_b-1
                  
                  #add-point:單身刪除後
                  {<point name="input.body.a_delete"/>}
                  #end add-point
                  CALL s_transaction_end('Y','0')
               END IF 
               CLOSE aprt501_bcl
               LET l_count = g_prfb_d.getLength()
            END IF 
            
                           INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_prfa_m.prfadocno
               LET gs_keys[2] = g_prfb_d[g_detail_idx].prfb001
               LET gs_keys[3] = g_prfb_d[g_detail_idx].prfb002
               LET gs_keys[4] = g_prfb_d[g_detail_idx].prfb003
 
              
         AFTER DELETE 
            #add-point:單身刪除後2
            {<point name="input.body.after_delete"/>}
            #end add-point
                           CALL aprt501_delete_b('prfb_t',gs_keys,"'1'")
 
         #---------------------<  Detail: page1  >---------------------
         #----<<prfbacti>>----
         #此段落由子樣板a01產生
         BEFORE FIELD prfbacti
            #add-point:BEFORE FIELD prfbacti
            {<point name="input.b.page1.prfbacti" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD prfbacti
            
            #add-point:AFTER FIELD prfbacti
            {<point name="input.a.page1.prfbacti" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE prfbacti
            #add-point:ON CHANGE prfbacti
            {<point name="input.g.page1.prfbacti" />}
            #END add-point
 
         #----<<prfb001>>----
         #此段落由子樣板a02產生
         AFTER FIELD prfb001
            #此段落由子樣板a15產生
            IF NOT cl_ap_chk_Range(g_prfb_d[l_ac].prfb001,"0.000","1","","","azz-00079",1) THEN
               NEXT FIELD prfb001
            END IF
 
 
            #add-point:AFTER FIELD prfb001
            {<point name="input.a.page1.prfb001" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD prfb001
            #add-point:BEFORE FIELD prfb001
            {<point name="input.b.page1.prfb001" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE prfb001
            #add-point:ON CHANGE prfb001
            {<point name="input.g.page1.prfb001" />}
            #END add-point
 
         #----<<prfb002>>----
         #此段落由子樣板a01產生
         BEFORE FIELD prfb002
            #add-point:BEFORE FIELD prfb002
            {<point name="input.b.page1.prfb002" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD prfb002
            
            #add-point:AFTER FIELD prfb002
            {<point name="input.a.page1.prfb002" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE prfb002
            #add-point:ON CHANGE prfb002
            {<point name="input.g.page1.prfb002" />}
            #END add-point
 
         #----<<prfb003>>----
         #此段落由子樣板a01產生
         BEFORE FIELD prfb003
            #add-point:BEFORE FIELD prfb003
            {<point name="input.b.page1.prfb003" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD prfb003
            
            #add-point:AFTER FIELD prfb003
            {<point name="input.a.page1.prfb003" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE prfb003
            #add-point:ON CHANGE prfb003
            {<point name="input.g.page1.prfb003" />}
            #END add-point
 
         #----<<prfb003_desc>>----
         #----<<prfbsite>>----
         #此段落由子樣板a01產生
         BEFORE FIELD prfbsite
            #add-point:BEFORE FIELD prfbsite
            {<point name="input.b.page1.prfbsite" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD prfbsite
            
            #add-point:AFTER FIELD prfbsite
            {<point name="input.a.page1.prfbsite" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE prfbsite
            #add-point:ON CHANGE prfbsite
            {<point name="input.g.page1.prfbsite" />}
            #END add-point
 
         #----<<prfbunit>>----
         #此段落由子樣板a01產生
         BEFORE FIELD prfbunit
            #add-point:BEFORE FIELD prfbunit
            {<point name="input.b.page1.prfbunit" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD prfbunit
            
            #add-point:AFTER FIELD prfbunit
            {<point name="input.a.page1.prfbunit" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE prfbunit
            #add-point:ON CHANGE prfbunit
            {<point name="input.g.page1.prfbunit" />}
            #END add-point
 
 
         #---------------------<  Detail: page1  >---------------------
         #----<<prfbacti>>----
         #Ctrlp:input.c.page1.prfbacti
         ON ACTION controlp INFIELD prfbacti
            #add-point:ON ACTION controlp INFIELD prfbacti
            {<point name="input.c.page1.prfbacti" />}
            #END add-point
 
         #----<<prfb001>>----
         #Ctrlp:input.c.page1.prfb001
         ON ACTION controlp INFIELD prfb001
            #add-point:ON ACTION controlp INFIELD prfb001
            {<point name="input.c.page1.prfb001" />}
            #END add-point
 
         #----<<prfb002>>----
         #Ctrlp:input.c.page1.prfb002
         ON ACTION controlp INFIELD prfb002
            #add-point:ON ACTION controlp INFIELD prfb002
            {<point name="input.c.page1.prfb002" />}
            #END add-point
 
         #----<<prfb003>>----
         #Ctrlp:input.c.page1.prfb003
         ON ACTION controlp INFIELD prfb003
            #add-point:ON ACTION controlp INFIELD prfb003
            {<point name="input.c.page1.prfb003" />}
            #END add-point
 
         #----<<prfb003_desc>>----
         #----<<prfbsite>>----
         #Ctrlp:input.c.page1.prfbsite
         ON ACTION controlp INFIELD prfbsite
            #add-point:ON ACTION controlp INFIELD prfbsite
            {<point name="input.c.page1.prfbsite" />}
            #END add-point
 
         #----<<prfbunit>>----
         #Ctrlp:input.c.page1.prfbunit
         ON ACTION controlp INFIELD prfbunit
            #add-point:ON ACTION controlp INFIELD prfbunit
            {<point name="input.c.page1.prfbunit" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_prfb_d[l_ac].* = g_prfb_d_t.*
               CLOSE aprt501_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_prfb_d[l_ac].prfb001,-263,1)
               LET g_prfb_d[l_ac].* = g_prfb_d_t.*
            ELSE
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               #寫入修改者/修改日期資訊(單身)
               
      
               UPDATE prfb_t SET (prfbdocno,prfbacti,prfb001,prfb002,prfb003,prfbsite,prfbunit) = (g_prfa_m.prfadocno, 
                   g_prfb_d[l_ac].prfbacti,g_prfb_d[l_ac].prfb001,g_prfb_d[l_ac].prfb002,g_prfb_d[l_ac].prfb003, 
                   g_prfb_d[l_ac].prfbsite,g_prfb_d[l_ac].prfbunit)
                WHERE prfbent = g_enterprise AND prfbdocno = g_prfa_m.prfadocno 
 
                  AND prfb001 = g_prfb_d_t.prfb001 #項次   
                  AND prfb002 = g_prfb_d_t.prfb002  
                  AND prfb003 = g_prfb_d_t.prfb003  
 
                  
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
