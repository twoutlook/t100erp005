  <section id="aprt501.delete" ver="11" status="" src="s">
<![CDATA[#+ 資料刪除
PRIVATE FUNCTION aprt501_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_prfa_m.prfadocno IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE aprt501_master_referesh USING g_prfa_m.prfadocno INTO g_prfa_m.prfaunit,g_prfa_m.prfadocdt, 
       g_prfa_m.prfadocno,g_prfa_m.prfa001,g_prfa_m.prfa002,g_prfa_m.prfa003,g_prfa_m.prfaacti,g_prfa_m.prfa004, 
       g_prfa_m.prfa005,g_prfa_m.prfasite,g_prfa_m.prfastus,g_prfa_m.prfaownid,g_prfa_m.prfaowndp,g_prfa_m.prfacrtid, 
       g_prfa_m.prfacrtdp,g_prfa_m.prfacrtdt,g_prfa_m.prfamodid,g_prfa_m.prfamoddt,g_prfa_m.prfacnfid, 
       g_prfa_m.prfacnfdt
   
   LET g_master_multi_table_t.prfaldocno = g_prfa_m.prfadocno
LET g_master_multi_table_t.prfal002 = g_prfa_m.prfal002
LET g_master_multi_table_t.prfal003 = g_prfa_m.prfal003
 
 
   CALL aprt501_show()
   
   CALL s_transaction_begin()
 
   OPEN aprt501_cl USING g_enterprise,g_prfa_m.prfadocno
 
   IF STATUS THEN
      CALL cl_err("OPEN aprt501_cl:", STATUS, 1)
      CLOSE aprt501_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   FETCH aprt501_cl INTO g_prfa_m.prfaunit,g_prfa_m.prfaunit_desc,g_prfa_m.prfadocdt,g_prfa_m.prfadocno, 
       g_prfa_m.prfa001,g_prfa_m.prfa002,g_prfa_m.prfa003,g_prfa_m.prfaacti,g_prfa_m.prfal002,g_prfa_m.prfal003, 
       g_prfa_m.prfa004,g_prfa_m.prfa004_desc,g_prfa_m.prfa005,g_prfa_m.prfa005_desc,g_prfa_m.prfasite, 
       g_prfa_m.prfastus,g_prfa_m.prfaownid,g_prfa_m.prfaownid_desc,g_prfa_m.prfaowndp,g_prfa_m.prfaowndp_desc, 
       g_prfa_m.prfacrtid,g_prfa_m.prfacrtid_desc,g_prfa_m.prfacrtdp,g_prfa_m.prfacrtdp_desc,g_prfa_m.prfacrtdt, 
       g_prfa_m.prfamodid,g_prfa_m.prfamodid_desc,g_prfa_m.prfamoddt,g_prfa_m.prfacnfid,g_prfa_m.prfacnfid_desc, 
       g_prfa_m.prfacnfdt              #鎖住將被更改或取消的資料
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_prfa_m.prfadocno,SQLCA.sqlcode,0)          #資料被他人LOCK
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:delete段before ask
   {<point name="delete.before_ask"/>}
   #end add-point 
 
   IF cl_ask_del_master() THEN              #確認一下
   
      #add-point:單頭刪除前
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point   
      
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL aprt501_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
  
      #資料備份
      LET g_prfadocno_t = g_prfa_m.prfadocno
 
 
      DELETE FROM prfa_t
       WHERE prfaent = g_enterprise AND prfadocno = g_prfa_m.prfadocno
 
       
      #add-point:單頭刪除中
      {<point name="delete.head.m_delete"/>}
      #end add-point
       
      IF SQLCA.sqlcode THEN
         CALL cl_err(g_prfa_m.prfadocno,SQLCA.sqlcode,0) 
         CALL s_transaction_end('N','0')
         RETURN
      END IF
      
      #add-point:單頭刪除後
      {<point name="delete.head.a_delete"/>}
      #end add-point
  
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM prfb_t
       WHERE prfbent = g_enterprise AND prfbdocno = g_prfa_m.prfadocno
 
 
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
         
      IF SQLCA.sqlcode THEN
         CALL cl_err("prfb_t",SQLCA.sqlcode,0) 
         CALL s_transaction_end('N','0')
         RETURN
      END IF    
 
      #add-point:單身刪除後
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
                     CALL aprt501_pmaa_t('d')      
                                                               
 
 
 
                                                               
      CLEAR FORM
      CALL g_prfb_d.clear() 
 
     
      #CALL aprt501_ui_browser_refresh()  
      CALL aprt501_ui_headershow()  
      CALL aprt501_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL aprt501_browser_fill("")
         CALL aprt501_fetch('P')
         DISPLAY g_browser_cnt TO FORMONLY.h_count   #總筆數的顯示
      ELSE
         LET g_wc = " 1=1"
         CALL aprt501_browser_fill("F")
      END IF
      
      #單頭多語言刪除
      INITIALIZE l_var_keys_bak TO NULL 
   INITIALIZE l_field_keys   TO NULL 
   LET l_var_keys_bak[01] = g_master_multi_table_t.prfaldocno
   LET l_field_keys[01] = 'prfaldocno'
   LET l_var_keys_bak[02] = g_dlang
   LET l_field_keys[02] = 'prfal001'
   CALL cl_multitable_delete(l_field_keys,l_var_keys_bak,'prfal_t')
 
      
      #單身多語言刪除
      
 
 
  
 
   END IF
 
   CALL s_transaction_end('Y','0')
   
   CLOSE aprt501_cl
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_prfa_m.prfadocno,'D')
    
END FUNCTION
]]>
