  <section id="aprt501.b_fill" ver="6" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aprt501_b_fill()
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_prfb_d.clear()    #g_prfb_d 單頭及單身 
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF aprt501_fill_chk(1) THEN
   
      LET g_sql = "SELECT  UNIQUE prfbacti,prfb001,prfb002,prfb003,'',prfbsite,prfbunit FROM prfb_t", 
             
                  " INNER JOIN prfa_t ON prfadocno = prfbdocno ",
 
                  #"",
                  
                  "",
                  " WHERE prfbent=? AND prfbdocno=?"
      #add-point:b_fill段sql_before
      {<point name="b_fill.body.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table1) THEN
         LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
      END IF
      
      #子單身的WC
      
      
      LET g_sql = g_sql, " ORDER BY prfb_t.prfb001,prfb_t.prfb002,prfb_t.prfb003"
      
      #add-point:單身填充控制
      {<point name="b_fill.sql"/>}
      #end add-point
      
      PREPARE aprt501_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR aprt501_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_prfa_m.prfadocno
 
                                               
      FOREACH b_fill_cs INTO g_prfb_d[l_ac].prfbacti,g_prfb_d[l_ac].prfb001,g_prfb_d[l_ac].prfb002,g_prfb_d[l_ac].prfb003, 
          g_prfb_d[l_ac].prfb003_desc,g_prfb_d[l_ac].prfbsite,g_prfb_d[l_ac].prfbunit
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               CALL cl_err( '', 9035, 1)
            END IF
            EXIT FOREACH
         END IF
         
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
 
   
   #add-point:browser_fill段其他table處理
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_prfb_d.deleteElement(g_prfb_d.getLength())
 
   
 
