  <section id="aprt501.modify" ver="10" status="" src="s">
<![CDATA[#+ 資料修改
PRIVATE FUNCTION aprt501_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_prfa_m.prfadocno IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE aprt501_master_referesh USING g_prfa_m.prfadocno INTO g_prfa_m.prfaunit,g_prfa_m.prfadocdt, 
       g_prfa_m.prfadocno,g_prfa_m.prfa001,g_prfa_m.prfa002,g_prfa_m.prfa003,g_prfa_m.prfaacti,g_prfa_m.prfa004, 
       g_prfa_m.prfa005,g_prfa_m.prfasite,g_prfa_m.prfastus,g_prfa_m.prfaownid,g_prfa_m.prfaowndp,g_prfa_m.prfacrtid, 
       g_prfa_m.prfacrtdp,g_prfa_m.prfacrtdt,g_prfa_m.prfamodid,g_prfa_m.prfamoddt,g_prfa_m.prfacnfid, 
       g_prfa_m.prfacnfdt
 
   ERROR ""
  
   LET g_prfadocno_t = g_prfa_m.prfadocno
 
   CALL s_transaction_begin()
   
   OPEN aprt501_cl USING g_enterprise,g_prfa_m.prfadocno
 
   IF STATUS THEN
      CALL cl_err("OPEN aprt501_cl:", STATUS, 1)
      CLOSE aprt501_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aprt501_cl INTO g_prfa_m.prfaunit,g_prfa_m.prfaunit_desc,g_prfa_m.prfadocdt,g_prfa_m.prfadocno, 
       g_prfa_m.prfa001,g_prfa_m.prfa002,g_prfa_m.prfa003,g_prfa_m.prfaacti,g_prfa_m.prfal002,g_prfa_m.prfal003, 
       g_prfa_m.prfa004,g_prfa_m.prfa004_desc,g_prfa_m.prfa005,g_prfa_m.prfa005_desc,g_prfa_m.prfasite, 
       g_prfa_m.prfastus,g_prfa_m.prfaownid,g_prfa_m.prfaownid_desc,g_prfa_m.prfaowndp,g_prfa_m.prfaowndp_desc, 
       g_prfa_m.prfacrtid,g_prfa_m.prfacrtid_desc,g_prfa_m.prfacrtdp,g_prfa_m.prfacrtdp_desc,g_prfa_m.prfacrtdt, 
       g_prfa_m.prfamodid,g_prfa_m.prfamodid_desc,g_prfa_m.prfamoddt,g_prfa_m.prfacnfid,g_prfa_m.prfacnfid_desc, 
       g_prfa_m.prfacnfdt
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_prfa_m.prfadocno,SQLCA.sqlcode,0)
      CLOSE aprt501_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
                  CALL aprt501_pmaa_t('s')
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   CALL aprt501_show()
   WHILE TRUE
      LET g_prfadocno_t = g_prfa_m.prfadocno
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_prfa_m.prfamodid = g_user 
LET g_prfa_m.prfamoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL aprt501_input("u")     #欄位更改
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_prfa_m.* = g_prfa_m_t.*
         CALL aprt501_show()
         CALL cl_err('',9001,0)
         CALL s_transaction_end('N','0')
         EXIT WHILE
      END IF
     
      #若有modid跟moddt則進行update
      UPDATE prfa_t SET (prfamodid,prfamoddt) = (g_prfa_m.prfamodid,g_prfa_m.prfamoddt)
       WHERE prfaent = g_enterprise AND prfadocno = g_prfadocno_t
 
 
      #若單頭key欄位有變更
      IF g_prfa_m.prfadocno != g_prfadocno_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE prfb_t SET prfbdocno = g_prfa_m.prfadocno
 
          WHERE prfbent = g_enterprise AND prfbdocno = g_prfadocno_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("prfb_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("prfb_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_prfa_m.prfadocno,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE aprt501_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_prfa_m.prfadocno,'U')
 
END FUNCTION   
]]>
