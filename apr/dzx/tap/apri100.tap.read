  <point name="function.apri100_create_tree" order="2" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#create tree
PRIVATE FUNCTION apri100_create_tree()
   DEFINE ps_wc              STRING
   DEFINE ps_page_action     STRING
   DEFINE ls_sql             STRING
   DEFINE li_idx             LIKE type_t.num5 
   define l_n2               like type_t.num5
   CLEAR FORM
   INITIALIZE g_prba_m.* TO NULL
   CALL g_browser.clear()
   
   LET ls_sql = " SELECT UNIQUE prba001 ",
                " FROM prba_t ",
                "  ",
                "  ",
                " WHERE prbaent = '" ||g_enterprise|| "' AND ", g_wc
                
   PREPARE browse_pre1 FROM ls_sql
   DECLARE browse_cur1 CURSOR FOR browse_pre1
   
   LET li_idx = 1 
   FOREACH browse_cur1 INTO g_browser[li_idx].b_prba001
      
      LET g_browser[li_idx].b_pid     = 0
      LET g_browser[li_idx].b_id      = 0, ".", li_idx USING "<<<"
      LET g_browser[li_idx].b_exp     = FALSE
      LET g_browser[li_idx].b_expcode = 1
      LET g_browser[li_idx].b_hasC    = TRUE
      LET g_browser[li_idx].b_show    = g_browser[li_idx].b_prba001
      LET g_browser[li_idx].b_isexp   = FALSE
      CALL apri100_desc_show(li_idx)
      LET li_idx = li_idx + 1
      
      IF li_idx > g_max_rec AND g_error_show = 1 THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code =  9035
         LET g_errparam.extend =  ''
         LET g_errparam.popup = TRUE
         CALL cl_err()

         EXIT FOREACH
      END IF
      
   END FOREACH
   LET g_error_show = 0
   
   CALL g_browser.deleteElement(g_browser.getLength())
   
   LET g_browser_cnt = g_browser.getLength()

   #瀏覽頁筆數顯示
   DISPLAY g_browser_cnt TO FORMONLY.b_count        #總筆數
   DISPLAY g_browser_cnt TO FORMONLY.h_count        #總筆數
   
   #CALL apri100_fetch("") 
   
   FREE browse_pre
   FOR l_n2 = 1 TO g_browser.getLength()
       IF g_browser[l_n2].b_isExp = FALSE THEN
          CALL apri100_browser_expand(l_n2)
          let g_browser[l_n2].b_isExp = TRUE
      END IF
  END FOR
END FUNCTION]]>
  </point>
  <point name="function.apri100_chk_prba001" order="3" ver="2" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#chk prba001
PRIVATE FUNCTION apri100_chk_prba001()
   DEFINE   l_cnt       LIKE type_t.num5
   DEFINE   l_cnt1      LIKE type_t.num5 

   LET g_errno = NULL
   LET l_cnt = 0
   LET l_cnt1 = 0
   SELECT COUNT(*) INTO l_cnt  FROM ooef_t 
    WHERE ooef001 = g_prba_m.prba001 AND ooefent = g_enterprise AND ooef304='Y'
      
   IF l_cnt <= 0 THEN
      LET g_errno = "aim-00060"
   ELSE
      SELECT COUNT(*) INTO l_cnt1  FROM ooef_t
       WHERE ooef001 = g_prba_m.prba001 AND ooefent = g_enterprise AND ooef304='Y' AND ooefstus='Y'
      IF l_cnt1 <= 0 THEN
         LET g_errno = "amm-00007"
      END IF         
   END IF
END FUNCTION]]>
  </point>
  <point name="function.apri100_aooi390_default" order="4" ver="7" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 自動編碼並預帶連動欄位值
# Memo...........:
# Usage..........: CALL apri100_aooi390_default()
#                  
# Input parameter: 
#                : 
# Return code....: 
#                : 
# Date & Author..: 150310 By earl
# Modify.........:
################################################################################
PRIVATE FUNCTION apri100_aooi390_default()
   DEFINE l_sql                STRING
   DEFINE l_str                STRING
   DEFINE l_i                  LIKE type_t.num5
   DEFINE l_success            LIKE type_t.num5
   DEFINE l_oofg_return    DYNAMIC ARRAY OF RECORD
                   oofg019     LIKE oofg_t.oofg019,   #field
                   oofg020     LIKE oofg_t.oofg020    #value
                           END RECORD

   #CALL s_aooi390_auto_no('16') RETURNING l_success,g_prba_m.prba002,l_oofg_return   #mark--2015/05/08 By shiun
   CALL s_aooi390_gen('16') RETURNING l_success,g_prba_m.prba002,l_oofg_return   #add--2015/05/08 By shiun
   DISPLAY BY NAME g_prba_m.prba002
   
   IF NOT l_success THEN
      RETURN
   END IF
   
#   CALL s_transaction_begin()   #因為s_aooi390_auto_no中有commit
   
   DELETE FROM apri100_prba_tmp
   
   INSERT INTO apri100_prba_tmp (prbaent,prba001,prba002,prba003,prba004,prba005,prba006,prba007, 
                                 prbastus,prbaownid,prbaowndp,prbacrtid,prbacrtdp,prbacrtdt,prbamodid,prbamoddt) 
   VALUES (g_enterprise,g_prba_m.prba001,g_prba_m.prba002,g_prba_m.prba003,g_prba_m.prba004, 
       g_prba_m.prba005,g_prba_m.prba006,g_prba_m.prba007,g_prba_m.prbastus,g_prba_m.prbaownid, 
       g_prba_m.prbaowndp,g_prba_m.prbacrtid,g_prba_m.prbacrtdp,g_prba_m.prbacrtdt,g_prba_m.prbamodid, 
       g_prba_m.prbamoddt)
       
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "apri100_prba_tmp" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()

      RETURN
   END IF
   
   FOR l_i = 1 TO l_oofg_return.getLength()
      #無設定
      IF cl_null(l_oofg_return[l_i].oofg019) THEN
         CONTINUE FOR
      END IF
      
      LET l_sql = "UPDATE apri100_prba_tmp",
                  "   SET ",l_oofg_return[l_i].oofg019," = '",l_oofg_return[l_i].oofg020,"'"
      PREPARE apri100_aooi390_pre02 FROM l_sql
      EXECUTE apri100_aooi390_pre02
      FREE apri100_aooi390_pre02

   END FOR
   
   LET l_sql = " SELECT UNIQUE t0.prba001,t0.prba002,t0.prba003,t0.prba004,t0.prba005,t0.prba006,t0.prba007, 
                               t0.prbastus,t0.prbaownid,t0.prbaowndp,t0.prbacrtid,t0.prbacrtdp,t0.prbacrtdt,t0.prbamodid,t0.prbamoddt, 
                               t1.ooefl003 ,t2.ooag011 ,t3.ooefl003 ,t4.ooag011 ,t5.ooefl003 ,t6.ooag011",
               " FROM apri100_prba_tmp t0",
               " LEFT JOIN ooefl_t t1 ON t1.ooeflent='"||g_enterprise||"' AND t1.ooefl001=t0.prba001 AND t1.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t2 ON t2.ooagent='"||g_enterprise||"' AND t2.ooag001=t0.prbaownid  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent='"||g_enterprise||"' AND t3.ooefl001=t0.prbaowndp AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t4 ON t4.ooagent='"||g_enterprise||"' AND t4.ooag001=t0.prbacrtid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=t0.prbacrtdp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent='"||g_enterprise||"' AND t6.ooag001=t0.prbamodid  "
   PREPARE apri100_aooi390_pre03 FROM l_sql
               
   EXECUTE apri100_aooi390_pre03 INTO g_prba_m.prba001,g_prba_m.prba002, 
       g_prba_m.prba003,g_prba_m.prba004,g_prba_m.prba005,g_prba_m.prba006,g_prba_m.prba007,g_prba_m.prbastus, 
       g_prba_m.prbaownid,g_prba_m.prbaowndp,g_prba_m.prbacrtid,g_prba_m.prbacrtdp,g_prba_m.prbacrtdt, 
       g_prba_m.prbamodid,g_prba_m.prbamoddt,g_prba_m.prba001_desc,g_prba_m.prbaownid_desc,g_prba_m.prbaowndp_desc, 
       g_prba_m.prbacrtid_desc,g_prba_m.prbacrtdp_desc,g_prba_m.prbamodid_desc
   
   FREE apri100_aooi390_pre03
   
   CALL apri100_show()
END FUNCTION]]>
  </point>
