            {<point name="input.c.prbastus" />}
            #END add-point
 
 #欄位開窗
 
         AFTER INPUT
            #若點選cancel則離開dialog
            IF INT_FLAG THEN
               EXIT DIALOG
            END IF
            
            #錯誤訊息統整顯示
            #CALL cl_err_collect_show()
            #CALL cl_showmsg()
  
            IF p_cmd <> "u" THEN
               #當p_cmd不為u代表為新增/複製
               LET l_count = 1  
 
               #確定新增的資料不存在(不重複)
               SELECT COUNT(*) INTO l_count FROM prba_t
                WHERE prbaent = g_enterprise AND prba001 = g_prba_m.prba001
                  AND prba002 = g_prba_m.prba002
 
               IF l_count = 0 THEN
               
                  #add-point:單頭新增前
                  {<point name="input.head.b_insert" mark="Y"/>}
                  #end add-point
               
                  #將新增的單頭資料寫入資料庫
                  INSERT INTO prba_t (prbaent,prba001,prba002,prba003,prba004,prba005,prba006,prba007, 
                      prbastus,prbaownid,prbaowndp,prbacrtid,prbacrtdp,prbacrtdt,prbamodid,prbamoddt) 

                  VALUES (g_enterprise,g_prba_m.prba001,g_prba_m.prba002,g_prba_m.prba003,g_prba_m.prba004, 
                      g_prba_m.prba005,g_prba_m.prba006,g_prba_m.prba007,g_prba_m.prbastus,g_prba_m.prbaownid, 
                      g_prba_m.prbaowndp,g_prba_m.prbacrtid,g_prba_m.prbacrtdp,g_prba_m.prbacrtdt,g_prba_m.prbamodid, 
                      g_prba_m.prbamoddt) 
                  
                  #add-point:單頭新增中
                  {<point name="input.head.m_insert"/>}
                  #end add-point
                  
                  #若寫入錯誤則提示錯誤訊息並返回輸入頁面
                  IF SQLCA.sqlcode THEN
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "prba_t" 
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     NEXT FIELD CURRENT
                  END IF
                  
                  
                  
                  #資料多語言用-增/改
                  
                  
                  #add-point:單頭新增後
                  {<point name="input.head.a_insert"/>}
                  #end add-point
                  
                  CALL s_transaction_end('Y','0')
               ELSE
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend =  "g_prba_m.prba001" 
                  LET g_errparam.code   = "std-00006" 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  CALL s_transaction_end('N','0')
               END IF 
            ELSE
               #add-point:單頭修改前
               {<point name="input.head.b_update" mark="Y"/>}
               #end add-point
               
               #將遮罩欄位還原
               CALL apri100_prba_t_mask_restore('restore_mask_o')
               
               UPDATE prba_t SET (prba001,prba002,prba003,prba004,prba005,prba006,prba007,prbastus,prbaownid, 
                   prbaowndp,prbacrtid,prbacrtdp,prbacrtdt,prbamodid,prbamoddt) = (g_prba_m.prba001, 
                   g_prba_m.prba002,g_prba_m.prba003,g_prba_m.prba004,g_prba_m.prba005,g_prba_m.prba006, 
                   g_prba_m.prba007,g_prba_m.prbastus,g_prba_m.prbaownid,g_prba_m.prbaowndp,g_prba_m.prbacrtid, 
                   g_prba_m.prbacrtdp,g_prba_m.prbacrtdt,g_prba_m.prbamodid,g_prba_m.prbamoddt)
                WHERE prbaent = g_enterprise AND prba001 = g_prba001_t #
                  AND prba002 = g_prba002_t
 
               #add-point:單頭修改中
               {<point name="input.head.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "prba_t" 
                     LET g_errparam.code   = "std-00009" 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     CALL s_transaction_end('N','0')
                     NEXT FIELD CURRENT
                  WHEN SQLCA.sqlcode #其他錯誤
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "prba_t" 
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     CALL s_transaction_end('N','0')
                     NEXT FIELD CURRENT
                  OTHERWISE
                     
