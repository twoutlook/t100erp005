  <section id="apri100.modify" ver="15" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION apri100_modify()
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point
   #add-point:modify段define(客製用)
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_prba_m.prba001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_prba001_t = g_prba_m.prba001
   LET g_prba002_t = g_prba_m.prba002
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN apri100_cl USING g_enterprise,g_prba_m.prba001,g_prba_m.prba002
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN apri100_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE apri100_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE apri100_master_referesh USING g_prba_m.prba001,g_prba_m.prba002 INTO g_prba_m.prba001,g_prba_m.prba002, 
       g_prba_m.prba003,g_prba_m.prba004,g_prba_m.prba005,g_prba_m.prba006,g_prba_m.prba007,g_prba_m.prbastus, 
       g_prba_m.prbaownid,g_prba_m.prbaowndp,g_prba_m.prbacrtid,g_prba_m.prbacrtdp,g_prba_m.prbacrtdt, 
       g_prba_m.prbamodid,g_prba_m.prbamoddt,g_prba_m.prba001_desc,g_prba_m.prbaownid_desc,g_prba_m.prbaowndp_desc, 
       g_prba_m.prbacrtid_desc,g_prba_m.prbacrtdp_desc,g_prba_m.prbamodid_desc
 
   #檢查是否允許此動作
   IF NOT apri100_action_chk() THEN
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_prba_m_mask_o.* =  g_prba_m.*
   CALL apri100_prba_t_mask()
   LET g_prba_m_mask_n.* =  g_prba_m.*
   
   
 
   #顯示資料
   CALL apri100_show()
   
   WHILE TRUE
      LET g_prba_m.prba001 = g_prba001_t
      LET g_prba_m.prba002 = g_prba002_t
 
      
      #寫入修改者/修改日期資訊
      LET g_prba_m.prbamodid = g_user 
LET g_prba_m.prbamoddt = cl_get_current()
LET g_prba_m.prbamodid_desc = cl_get_username(g_prba_m.prbamodid)
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
 
      #資料輸入
      CALL apri100_input("u")     
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_prba_m.* = g_prba_m_t.*
         CALL apri100_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE prba_t SET (prbamodid,prbamoddt) = (g_prba_m.prbamodid,g_prba_m.prbamoddt)
       WHERE prbaent = g_enterprise AND prba001 = g_prba001_t
         AND prba002 = g_prba002_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL apri100_set_act_visible()
   CALL apri100_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " prbaent = '" ||g_enterprise|| "' AND",
                      " prba001 = '", g_prba_m.prba001, "' "
                      ," AND prba002 = '", g_prba_m.prba002, "' "
 
   #填到對應位置
   CALL apri100_browser_fill(g_wc,"")
 
   CLOSE apri100_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL apri100_msgcentre_notify('modify')
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
]]>
