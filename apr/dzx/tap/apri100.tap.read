  <section id="apri100.delete" ver="16" status="" src="s" readonly="">
    <![CDATA[#+ 資料刪除 
PRIVATE FUNCTION apri100_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define" edit="s"/>}
   #end add-point  
   #add-point:delete段define(客製用)
   {<point name="delete.define_customerization" edit="c"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_prba_m.prba001 IS NULL
   OR g_prba_m.prba002 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
    
   LET g_prba001_t = g_prba_m.prba001
   LET g_prba002_t = g_prba_m.prba002
 
   
   
 
   OPEN apri100_cl USING g_enterprise,g_prba_m.prba001,g_prba_m.prba002
 
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN apri100_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE apri100_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE apri100_master_referesh USING g_prba_m.prba001,g_prba_m.prba002 INTO g_prba_m.prba001,g_prba_m.prba002, 
       g_prba_m.prba003,g_prba_m.prba004,g_prba_m.prba005,g_prba_m.prba006,g_prba_m.prba007,g_prba_m.prbastus, 
       g_prba_m.prbaownid,g_prba_m.prbaowndp,g_prba_m.prbacrtid,g_prba_m.prbacrtdp,g_prba_m.prbacrtdt, 
       g_prba_m.prbamodid,g_prba_m.prbamoddt,g_prba_m.prba001_desc,g_prba_m.prbaownid_desc,g_prba_m.prbaowndp_desc, 
       g_prba_m.prbacrtid_desc,g_prba_m.prbacrtdp_desc,g_prba_m.prbamodid_desc
   
   #檢查是否允許此動作
   IF NOT apri100_action_chk() THEN
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_prba_m_mask_o.* =  g_prba_m.*
   CALL apri100_prba_t_mask()
   LET g_prba_m_mask_n.* =  g_prba_m.*
   
   #將最新資料顯示到畫面上
   CALL apri100_show()
   
   IF cl_ask_delete() THEN
 
      #add-point:單頭刪除前
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point
 
      #應用 a47 樣板自動產生(Version:2)
      #刪除相關文件
      CALL apri100_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
 
      DELETE FROM prba_t 
       WHERE prbaent = g_enterprise AND prba001 = g_prba_m.prba001 
         AND prba002 = g_prba_m.prba002 
 
 
      #add-point:單頭刪除中
      {<point name="delete.head.m_delete"/>}
      #end add-point
         
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "prba_t" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
      END IF
  
      
      
      #add-point:單頭刪除後
      {<point name="delete.head.a_delete"/>}
      #end add-point
      
       
 
      #修改歷程記錄(刪除)
      IF NOT cl_log_modified_record('','') THEN 
         CLOSE apri100_cl
         CALL s_transaction_end('N','0')
         RETURN
      END IF
      
      CLEAR FORM
      CALL apri100_ui_browser_refresh()
      
      #確保畫面上保有資料
      IF g_browser_cnt > 0 THEN
         CALL apri100_fetch("P")
      ELSE
         CLEAR FORM
      END IF
      
   END IF
 
   CLOSE apri100_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL apri100_msgcentre_notify('delete')
 
END FUNCTION
]]>
