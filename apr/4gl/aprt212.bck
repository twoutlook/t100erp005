PRIVATE FUNCTION aprt212_prdc003_display()
DEFINE l_oocq002      LIKE oocq_t.oocq002
DEFINE l_oocql004     LIKE oocql_t.oocql004
DEFINE l_cnt          LIKE type_t.num5
DEFINE l_sql          STRING
DEFINE cb004          ui.ComboBox
   
   LET cb004 = ui.ComboBox.forName('prdc003')
   CALL cb004.clear()
   
   LET l_cnt = 0
   LET l_sql = " SELECT DISTINCT oocq002,oocql004 ",
               "   FROM oocq_t LEFT JOIN oocql_t ON oocqent = oocqlent AND oocq001 = oocql001 AND oocq002 = oocql002 AND oocql003 = '",g_dlang,"' ",
               "  WHERE oocqent = ",g_enterprise," ",
               "    AND oocq001 = '2049' ",
               "    AND oocqstus = 'Y' ",
               "  ORDER BY oocq002 "
   PREPARE sel_oocq_pre FROM l_sql
   DECLARE sel_oocq_cs  CURSOR FOR sel_oocq_pre
   LET l_cnt = 1
   FOREACH sel_oocq_cs  INTO l_oocq002,l_oocql004
      LET l_oocql004 = l_oocq002,':',l_oocql004
      IF cl_null(l_oocql004) THEN
         CALL cb004.addItem(l_oocq002,l_oocq002)
      ELSE
         CALL cb004.addItem(l_oocq002,l_oocql004)
      END IF
      LET l_cnt = l_cnt+1
   END FOREACH
END FUNCTION

################################################################################
# Descriptions...: 單身填充
# Memo...........:
# Usage..........: CALL aprt212_b7_fill()
# Input parameter: 
# Return code....: 
# Date & Author..: 20150420 By huangrh
# Modify.........:
################################################################################
PRIVATE FUNCTION aprt212_b7_fill()
DEFINE l_i          LIKE type_t.num5
DEFINE l_ac_t       LIKE type_t.num5
    
   CALL g_prde7_d.clear() 
   IF g_detail_idx <= 0 THEN
      RETURN
   END IF
   IF g_prde6_d.getLength() = 0 THEN
      RETURN
   END IF   
   
   IF g_prde6_d[1].prdb002 IS NULL THEN
      RETURN
   END IF    
   
    IF cl_null(g_wc2_table9) THEN
       LET g_wc2_table9=" 1=1"
    END IF
         
     LET g_sql = "SELECT  UNIQUE prdcacti,prdc003,prdc004,prdc001,prdcsite,prdcunit  FROM prdc_t", 
                 " WHERE prdcent=? AND prdcdocno=? AND prdc002=?"
     
     IF NOT cl_null(g_wc2_table9) THEN
        LET g_sql = g_sql CLIPPED," AND ",g_wc2_table9 CLIPPED
     END IF
     
     LET g_sql = g_sql, " ORDER BY  prdc_t.prdc003,prdc_t.prdc004" 
                        
     
     LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
     PREPARE aprt212_pb9 FROM g_sql
     DECLARE b_fill_curs9 CURSOR FOR aprt212_pb9
     
     OPEN b_fill_curs9 USING g_enterprise,g_prda_m.prdadocno,g_prde6_d[g_detail_idx].prdb002

     LET l_i = 1
     FOREACH b_fill_curs9 INTO g_prde7_d[l_i].prdcacti,g_prde7_d[l_i].prdc003,g_prde7_d[l_i].prdc004, 
         g_prde7_d[l_i].prdc001,g_prde7_d[l_i].prdcsite,g_prde7_d[l_i].prdcunit 
        IF SQLCA.sqlcode THEN
           INITIALIZE g_errparam TO NULL 
           LET g_errparam.extend = "FOREACH:" 
           LET g_errparam.code   = SQLCA.sqlcode 
           LET g_errparam.popup  = TRUE 
           CALL cl_err()
           EXIT FOREACH
        END IF
        

              
        IF l_i > g_max_rec THEN
           INITIALIZE g_errparam TO NULL 
           LET g_errparam.extend = l_i
           LET g_errparam.code   = 9035 
           LET g_errparam.popup  = TRUE 
           CALL cl_err()
           EXIT FOREACH
        END IF
        
        LET l_i = l_i + 1
        
     END FOREACH
     CALL g_prde7_d.deleteElement(g_prde7_d.getLength())
     FREE aprt212_pb9
     
     LET l_ac_t=l_ac
     FOR l_ac = 1 TO g_prde7_d.getLength()     
        CALL aprt212_prdc_desc()      
     END FOR  
     LET l_ac=l_ac_t     
   
END FUNCTION

