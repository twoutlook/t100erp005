PRIVATE FUNCTION aprt212_b9_fill()
DEFINE l_i          LIKE type_t.num5
DEFINE l_ac_t       LIKE type_t.num5
    
   CALL g_prde9_d.clear() 
#   IF g_detail_idx <= 0 THEN
#      RETURN
#   END IF
#   IF g_prde8_d.getLength() = 0 THEN
#      RETURN
#   END IF    
    
    IF cl_null(g_wc2_table8) THEN
       LET g_wc2_table8=" 1=1"
    END IF


    LET g_sql = "SELECT  UNIQUE prdkacti,prdk002,prdk003,prdk004,prdk005,prdk006,prdk007,prdk008, 
        prdk009,prdk010,prdk011,prdk012,prdk001,prdksite,prdkunit ,t4.oocal003 FROM prdk_t",    

                " INNER JOIN prda_t ON prdadocno = prdkdocno ",
                " LEFT JOIN oocal_t t4 ON t4.oocalent='"||g_enterprise||"' AND t4.oocal001=prdk007 AND t4.oocal002='"||g_dlang||"' ",
 
                " WHERE prdkent=? AND prdkdocno=?"   
    LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料

    IF NOT cl_null(g_wc2_table8) THEN
       LET g_sql = g_sql CLIPPED," AND ",g_wc2_table8 CLIPPED
    END IF
    
    #子單身的WC
    
    
    LET g_sql = g_sql, " ORDER BY prdk_t.prdk002,prdk_t.prdk003,prdk_t.prdk004,prdk_t.prdk005"
    
    
    LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
    PREPARE aprt212_pb81 FROM g_sql
    DECLARE b_fill_cs81 CURSOR FOR aprt212_pb81

    LET l_ac_t=l_ac
    LET l_ac = 1
    
    OPEN b_fill_cs81 USING g_enterprise,g_prda_m.prdadocno
                                             
    FOREACH b_fill_cs81 INTO g_prde9_d[l_ac].prdkacti,g_prde9_d[l_ac].prdk002,g_prde9_d[l_ac].prdk003, 
        g_prde9_d[l_ac].prdk004,g_prde9_d[l_ac].prdk005,g_prde9_d[l_ac].prdk006,g_prde9_d[l_ac].prdk007, 
        g_prde9_d[l_ac].prdk008,g_prde9_d[l_ac].prdk009,g_prde9_d[l_ac].prdk010,g_prde9_d[l_ac].prdk011, 
        g_prde9_d[l_ac].prdk012,g_prde9_d[l_ac].prdk001,g_prde9_d[l_ac].prdksite,g_prde9_d[l_ac].prdkunit, 
        g_prde9_d[l_ac].prdk007_desc
       IF SQLCA.sqlcode THEN
          INITIALIZE g_errparam TO NULL 
          LET g_errparam.extend = "FOREACH:" 
          LET g_errparam.code   = SQLCA.sqlcode 
          LET g_errparam.popup  = TRUE 
          CALL cl_err()
          EXIT FOREACH
       END IF
      

       CALL aprt212_prdk_desc()

    
       LET l_ac = l_ac + 1
       IF l_ac > g_max_rec THEN
          INITIALIZE g_errparam TO NULL 
          LET g_errparam.extend = l_ac
          LET g_errparam.code   = 9035 
          LET g_errparam.popup  = TRUE 
          CALL cl_err()
          EXIT FOREACH
       END IF
       
    END FOREACH

    CALL g_prde9_d.deleteElement(g_prde9_d.getLength())
    FREE aprt212_pb81

    LET l_ac=l_ac_t   
    
END FUNCTION

