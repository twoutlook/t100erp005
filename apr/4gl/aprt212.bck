   ELSE
      CALL cl_set_comp_visible("page_2", TRUE)
   END IF
   CALL aprt212_set_comp_att_text()
   #end add-point  
   
   
   
   #add-point:fetch結束前 name="fetch.after"
   
   #end add-point
   
   #保存單頭舊值
   LET g_prda_m_t.* = g_prda_m.*
