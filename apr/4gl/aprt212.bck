PRIVATE FUNCTION aprt212_set_no_entry_b(p_cmd)
   #add-point:set_no_entry_b段define(客製用) name="set_no_entry_b.define_customerization"
   
   #end add-point    
   DEFINE p_cmd   LIKE type_t.chr1   
   #add-point:set_no_entry_b段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="set_no_entry_b.define"
   DEFINE l_ooia002       LIKE ooia_t.ooia002
   DEFINE l_n1            LIKE type_t.num5
   DEFINE l_n2            LIKE type_t.num5
   DEFINE l_n3            LIKE type_t.num5   
   #end add-point    
   
   #add-point:Function前置處理  name="set_no_entry_b.pre_function"
   
   #end add-point
   
   IF p_cmd = 'u' AND g_chkey = 'N' THEN
      CALL cl_set_comp_entry("",FALSE)
      #add-point:set_no_entry_b段欄位控制 name="set_no_entry_b.field_control"
      
      #end add-point 
   END IF 
   
   #add-point:set_no_entry_b段 name="set_no_entry_b.set_no_entry_b"
   LET l_ooia002 = ''
   IF NOT cl_null(g_prde3_d[l_ac].prdf003) THEN
      SELECT ooia002 INTO l_ooia002
        FROM ooia_t
       WHERE ooiaent = g_enterprise
         AND ooia001 = g_prde3_d[l_ac].prdf003
   END IF
   IF l_ooia002 <> '40' AND l_ooia002 <> '60' THEN
      CALL cl_set_comp_entry("prdf004",FALSE)
      LET g_prde3_d[l_ac].prdf004 = ""
      LET g_prde3_d[l_ac].prdf004_desc = ""
   END IF    
   IF g_prde5_d[l_ac].prdh004 = '2' THEN
      CALL cl_set_comp_entry("prdh005",FALSE)
      LET g_prde5_d[l_ac].prdh005 = 0
   END IF
  #IF g_prde2_d[l_ac].prdg003 <> '4' THEN     #160819-00054#17 161013 by lori mark
   IF g_prde2_d[l_ac].prdg003 <> '15' THEN    #160819-00054#17 161013 by lori add
      CALL cl_set_comp_entry("prdg005",FALSE)
      LET g_prde2_d[l_ac].prdg005 = ""
      CALL cl_set_comp_entry("prdg006",FALSE)
      LET g_prde2_d[l_ac].prdg006 = ""
      CALL cl_set_comp_entry("prdg006_desc",FALSE)
      LET g_prde2_d[l_ac].prdg006_desc = ""
   END IF
   IF p_cmd = 'u' AND g_prda_m.prda000 = 'U' THEN
      CALL cl_set_comp_entry("prde003",FALSE)
      CALL cl_set_comp_entry("prde004",FALSE)
   END IF
   
     
   IF g_prde9_d[l_ac].prdk004 <> 'M' AND g_prde9_d[l_ac].prdk004 <> 'P'  THEN
      CALL cl_set_comp_entry("prdk009,prdk010,prdk011,prdk012",FAlSE)
      LET g_prde9_d[l_ac].prdk009 = ''
      LET g_prde9_d[l_ac].prdk010 = ''
      LET g_prde9_d[l_ac].prdk011 = ''
      LET g_prde9_d[l_ac].prdk012 = ''
   END IF
   IF g_prde9_d[l_ac].prdk004 <> '4' THEN
      CALL cl_set_comp_entry("prdk006",FAlSE)
      LET g_prde9_d[l_ac].prdk006 = ''
      LET g_prde9_d[l_ac].prdk007 = ''
      LET g_prde9_d[l_ac].prdk007_desc = ''
      CALL aprt212_prdk_desc()
   END IF
   IF g_prde9_d[l_ac].prdk010 <> '1' THEN
      CALL cl_set_comp_entry("prdk011",FALSE)
      LET g_prde9_d[l_ac].prdk011 = ''
   END IF
   IF g_prde9_d[l_ac].prdk010 <> '2' AND g_prde9_d[l_ac].prdk010 <> '3' THEN
      CALL cl_set_comp_entry("prdk012",FALSE)
      LET g_prde9_d[l_ac].prdk012 = ''
   END IF
   LET l_n1 = 0
   SELECT COUNT(*) INTO l_n1
     FROM prdb_t
    WHERE prdbent = g_enterprise
      AND prdbdocno = g_prda_m.prdadocno
      AND prdb003 = '1'
      AND prdb004 = g_prde8_d[l_ac].prdj003
      
   LET l_n2 = 0
   SELECT COUNT(*) INTO l_n2
     FROM prdb_t
    WHERE prdbent = g_enterprise
      AND prdbdocno = g_prda_m.prdadocno
      AND prdb003  = '2'
      AND prdb004 = g_prde8_d[l_ac].prdj003

   LET l_n3 = 0
   SELECT COUNT(*) INTO l_n3
     FROM prdb_t
    WHERE prdbent = g_enterprise
      AND prdbdocno = g_prda_m.prdadocno
      AND prdb003  = '3'
      AND prdb004 = g_prde8_d[l_ac].prdj003
      
   IF g_prda_m.prda019 = '3' THEN
      CALL aprt212_cl_set_comp_entry("prdj00601",FALSE)
      LET g_prde8_d[l_ac].prdj00601 = ''
      CALL aprt212_cl_set_comp_entry("prdj00602",FALSE)
      LET g_prde8_d[l_ac].prdj00602 = ''
      CALL aprt212_cl_set_comp_entry("prdj00603",FALSE)
      LET g_prde8_d[l_ac].prdj00603 = ''
   ELSE
      IF l_n1 = 0 THEN
         CALL aprt212_cl_set_comp_entry('prdj00601',FALSE)
         LET g_prde8_d[l_ac].prdj00601 = ''
      END IF
      IF l_n2 = 0 THEN
         CALL aprt212_cl_set_comp_entry('prdj00602',FALSE)
         LET g_prde8_d[l_ac].prdj00602 = ''
      END IF
      IF l_n3 = 0 THEN
         CALL aprt212_cl_set_comp_entry('prdj00603',FALSE)
         LET g_prde8_d[l_ac].prdj00603 = ''
      END IF
   END IF
   ##160819-00054#43 by sakura mark(S)
   #160819-00054#32 by sakura add(S)
   #IF g_prde9_d[l_ac].prdk004 <> 'M' THEN
   #   CALL cl_set_comp_entry("prdk013",FALSE)
   #END IF
   #160819-00054#32 by sakura add(E)
   #160819-00054#43 by sakura mark(E)
   ##160819-00054#43 by sakura add(S)
   IF g_prde9_d[l_ac].prdk004 NOT MATCHES "[MP]" THEN
      CALL cl_set_comp_entry("prdk013",FALSE)
   END IF
   #160819-00054#43 by sakura add(E)
   #end add-point     
