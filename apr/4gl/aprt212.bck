        INITIALIZE g_errparam TO NULL
        LET g_errparam.code = g_errno
        LET g_errparam.extend = g_prda_m.prdadocno
        LET g_errparam.popup = TRUE
        CALL cl_err()
        LET g_errno = NULL
        CALL aprt212_set_act_visible()
        CALL aprt212_set_act_no_visible()
        CALL aprt212_show()
        RETURN FALSE
     END IF
   END IF
