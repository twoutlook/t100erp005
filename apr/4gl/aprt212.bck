PRIVATE FUNCTION aprt212_master_def()
   DELETE FROM prdb_t WHERE prdbent = g_enterprise AND prdbdocno = g_prda_m.prdadocno
   IF g_prda_m.prda026 = '1'  OR g_prda_m.prda026 = '3'  THEN
      #散客
      IF NOT cl_null(g_prda_m.prdb005_1) THEN
         INSERT INTO prdb_t(prdbent,prdbunit,prdbsite,prdbdocno,prdb001,prdb002,prdb003,prdb004,prdb005,prdbacti,prdb014) #add prdb014 by geza 20160328 #160328-00014#1
                     VALUES(g_enterprise,g_site,g_site,g_prda_m.prdadocno,g_prda_m.prda001,1,'1',0,g_prda_m.prdb005_1,'Y',g_prda_m.prdb014_1)    #add prdb014 by geza 20160328 #160328-00014#1     
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = 'ins prdb'
            LET g_errparam.popup = TRUE
            CALL cl_err()

            RETURN FALSE
         END IF
      END IF
   END IF
   IF g_prda_m.prda026 = '2'  OR g_prda_m.prda026 = '3'  THEN
      #會員
      IF NOT cl_null(g_prda_m.prdb005_2) THEN
         INSERT INTO prdb_t(prdbent,prdbunit,prdbsite,prdbdocno,prdb001,prdb002,prdb003,prdb004,prdb005,prdbacti,prdb014) #add prdb014 by geza 20160328 #160328-00014#1
                     VALUES(g_enterprise,g_site,g_site,g_prda_m.prdadocno,g_prda_m.prda001,2,'2',0,g_prda_m.prdb005_2,'Y',g_prda_m.prdb014_2) #add prdb014 by geza 20160328 #160328-00014#1        
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = 'ins prdb'
            LET g_errparam.popup = TRUE
            CALL cl_err()

            RETURN FALSE
         END IF
      END IF
   END IF
   DELETE FROM prdd_t WHERE prddent = g_enterprise AND prdddocno = g_prda_m.prdadocno
   #日期
   IF g_prda_m.prda013 = 'N' THEN
      INSERT INTO prdd_t(prddent,prddunit,prddsite,prdddocno,prdd001,prdd002,prdd003,prdd004,prdd005,prdd006,prdd007,prdd008,prddacti)
                  VALUES(g_enterprise,g_site,g_site,g_prda_m.prdadocno,g_prda_m.prda001,1,g_prda_m.l_prdd003_1,g_prda_m.l_prdd004_1,g_prda_m.prdd005_1,g_prda_m.prdd006_1,g_prda_m.prdd007_1,g_prda_m.prdd008_1,'Y')
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = 'ins prdd'
         LET g_errparam.popup = TRUE
         CALL cl_err()

         RETURN FALSE
      END IF
   END IF
   RETURN TRUE
END FUNCTION
