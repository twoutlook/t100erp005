################################################################################
PRIVATE FUNCTION aprt212_chk_prdf004()
DEFINE l_ooia002       LIKE ooia_t.ooia002
DEFINE l_gcafstus      LIKE gcaf_t.gcafstus
DEFINE l_mmanstus      LIKE mman_t.mmanstus
DEFINE l_n             LIKE type_t.num5
DEFINE l_mmap003       LIKE mmap_t.mmap003
DEFINE l_mmap005       LIKE mmap_t.mmap005
DEFINE l_sql           STRING

   
   DELETE FROM aprt212_tmp
   LET l_sql=" MERGE INTO aprt212_tmp ",
             " USING (SELECT ooed004 FROM ooed_t ",
             "         START WITH ooed005= ? AND ooed001 = '8' AND ooedent = '",g_enterprise,"' ",
             "           AND TO_CHAR(ooed006,'YYYY/MM/DD')<='",g_today,"' ",
             "           AND (TO_CHAR(ooed007,'YYYY/MM/DD')>='",g_today,"' OR ooed007 IS NULL)",
             "       CONNECT BY  NOCYCLE PRIOR ooed004=ooed005 ",
             "           AND ooed001='8' AND ooedent = '",g_enterprise,"'",
             "           AND TO_CHAR(ooed006,'YYYY/MM/DD')<='",g_today,"' ",
             "           AND (TO_CHAR(ooed007,'YYYY/MM/DD')>='",g_today,"' OR ooed007 IS NULL))",
             "     ON (mmap003 = ooed004)",
             "   WHEN NOT MATCHED THEN ",
             " INSERT (mmap003)",
             " VALUES (ooed004)"
   PREPARE sel_ooed_pre1 FROM l_sql
   
   LET g_errno = ""
   SELECT ooia002 INTO l_ooia002
     FROM ooia_t
    WHERE ooiaent = g_enterprise
      AND ooia001 = g_prde3_d[l_ac].prdf003
   IF l_ooia002 = '40' THEN #券
       LET l_sql = "SELECT DISTINCT mmap003,mmap005 FROM mmap_t ",
                  " WHERE mmap001 =  'agcm300'",
                  "   AND mmap002='",g_prde3_d[l_ac].prdf004,"' ",
                  "   AND mmapstus='Y' ",
                  "   AND mmapent= '",g_enterprise,"'"
      PREPARE l_sql_mmap003 FROM l_sql
      DECLARE l_sql_mmap003_cs CURSOR FOR l_sql_mmap003
      FOREACH l_sql_mmap003_cs INTO l_mmap003,l_mmap005
         INSERT INTO aprt212_tmp VALUES (l_mmap003)
         IF SQLCA.sqlcode THEN
            LET g_errno = SQLCA.sqlcode
            RETURN
         END IF
         IF l_mmap005='Y' THEN
            EXECUTE sel_ooed_pre1 USING l_mmap003
            IF SQLCA.sqlcode THEN
               LET g_errno = SQLCA.sqlcode
               RETURN
            END IF
         END IF
      END FOREACH
      SELECT gcafstus INTO l_gcafstus
        FROM gcaf_t
       WHERE gcafent = g_enterprise
         AND gcaf001 = g_prde3_d[l_ac].prdf004
      CASE 
         WHEN SQLCA.sqlcode = 100
              LET g_errno = 'apr-00085'
              RETURN
         WHEN l_gcafstus = 'X'
              LET g_errno = 'sub-01307'   #apr-00086  #160318-00005#40  by 07900--mod
              LET g_errparam.exeprog = 'agcm300'      #160318-00005#40  By 07900--add
              RETURN
         WHEN l_gcafstus = 'N'
              LET g_errno = 'sub-01302'  #apr-00087   #160318-00005#40  By 07900 --mod
              LET g_errparam.exeprog = 'agcm300'      #160318-00005#40  By 07900--add
              RETURN
      END CASE 
      LET l_n = 0
      SELECT COUNT(*) INTO l_n
        FROM gcaf_t  
       WHERE gcafent = g_enterprise
         AND gcaf001 = g_prde3_d[l_ac].prdf004
         AND gcaf031 = g_prde3_d[l_ac].prdf003 
      IF l_n = 0 THEN
         LET g_errno = 'apr-00302'
         RETURN
      END IF   
      LET l_n = 0
      SELECT COUNT(*) INTO l_n
        FROM aprt212_tmp
       WHERE mmap003 = g_site
       IF l_n = 0 THEN
          LET g_errno = 'apr-00092'
          RETURN
       END IF
   END IF
   IF l_ooia002 = '60' THEN #卡
      LET l_sql = "SELECT DISTINCT mmap003,mmap005 FROM mmap_t ",
                  " WHERE mmap001 =  'ammm320'",
                  "   AND mmap002='",g_prde3_d[l_ac].prdf004,"' ",
                  "   AND mmapstus='Y' ",
                  "   AND mmapent= '",g_enterprise,"'"
      PREPARE l_sql_mmap003_1 FROM l_sql
      DECLARE l_sql_mmap003_cs1 CURSOR FOR l_sql_mmap003_1
      FOREACH l_sql_mmap003_cs1 INTO l_mmap003,l_mmap005
         INSERT INTO aprt212_tmp VALUES (l_mmap003)
         IF SQLCA.sqlcode THEN
            LET g_errno = SQLCA.sqlcode
            RETURN
         END IF
         IF l_mmap005='Y' THEN
            EXECUTE sel_ooed_pre1 USING l_mmap003
            IF SQLCA.sqlcode THEN
               LET g_errno = SQLCA.sqlcode
               RETURN
            END IF
         END IF
      END FOREACH
      SELECT mmanstus INTO l_mmanstus
        FROM mman_t
       WHERE mmanent = g_enterprise
         AND mman001 = g_prde3_d[l_ac].prdf004
      CASE 
         WHEN SQLCA.sqlcode = 100
              LET g_errno = 'apr-00088'
              RETURN
         WHEN l_mmanstus = 'X'
              LET g_errno = 'sub-01307' #apr-00089   #160318-00005#40  By 07900 --mod
               LET g_errparam.exeprog = 'ammm320'    #160318-00005#40  By 07900--add
              RETURN
         WHEN l_mmanstus = 'N'
              LET g_errno = 'sub-01302' #apr-00090   #160318-00005#40  By 07900 --mod
               LET g_errparam.exeprog = 'ammm320'    #160318-00005#40  By 07900--add
              RETURN
      END CASE 
      LET l_n = 0
      SELECT COUNT(*) INTO l_n
        FROM mman_t  
       WHERE mmanent = g_enterprise
         AND mman001 = g_prde3_d[l_ac].prdf004
         AND mman058 = g_prde3_d[l_ac].prdf003 
      IF l_n = 0 THEN
         LET g_errno = 'apr-00303'
         RETURN
      END IF 
      LET l_n = 0
      SELECT COUNT(*) INTO l_n
        FROM aprt212_tmp
       WHERE mmap003 = g_site
       IF l_n = 0 THEN
          LET g_errno = 'apr-00092'
          RETURN
       END IF
   END IF
END FUNCTION
