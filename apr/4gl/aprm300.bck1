   LET g_action_choice = ""
    
   INITIALIZE g_wc TO NULL
   INITIALIZE g_wc2 TO NULL
   
   INITIALIZE g_wc2_table1 TO NULL
 
    
   LET g_qryparam.state = 'c'
   
   #add-point:cs段開始前 name="cs.before_construct"
   
   #end add-point 
   
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
      
      #單頭
      CONSTRUCT BY NAME g_wc ON prcfunit,prcfdocdt,prcf001,prcfl003,prcfl004,prcf003,prcf004,prcf098, 
          prcfstus,prcf046,prcf002,prcf007,prcf008,prcf051,prcf005,prcf011,prcf012,prcf055,prcf056,prcf047, 
          prcf048,prcf049,prcf050,prcf006,prcf009,prcf053,prcf010,prcf054,prcf052,prcfsite,prcf013,prcf014, 
          prcf015,prcf019,prcf017,prcf016,prcf058,prcf018,prcf057,prcf020,prcf021,prcf024,prcf027,prcf030, 
          prcf033,prcf036,prcf038,prcf042,prcf045,prcf022,prcf025,prcf028,prcf031,prcf034,prcf037,prcf039, 
          prcf043,prcf023,prcf026,prcf029,prcf032,prcf035,prcf040,prcf041,prcf044,prcfownid,prcfowndp, 
          prcfcrtid,prcfcrtdp,prcfcrtdt,prcfmodid,prcfmoddt,prcfcnfid,prcfcnfdt
 
         BEFORE CONSTRUCT
            #add-point:cs段before_construct name="cs.head.before_construct"
            
            #end add-point 
            
         #公用欄位開窗相關處理
         #應用 a11 樣板自動產生(Version:3)
         #共用欄位查詢處理  
         ##----<<prcfcrtdt>>----
         AFTER FIELD prcfcrtdt
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
