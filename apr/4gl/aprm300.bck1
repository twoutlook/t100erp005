# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/03/25 By xumm
# Modify.........:
################################################################################
PRIVATE FUNCTION aprm300_prcf002_def()
#DEFINE l_prcd      RECORD LIKE prcd_t.* #161111-00028#2--mark
#161111-00028#2----add---begin-----------
DEFINE l_prcd RECORD  #促銷活動計劃主檔
       prcdent LIKE prcd_t.prcdent, #企業編號
       prcdunit LIKE prcd_t.prcdunit, #應用組織
       prcdsite LIKE prcd_t.prcdsite, #NO USE
       prcdstus LIKE prcd_t.prcdstus, #有效否
       prcd001 LIKE prcd_t.prcd001, #活動計劃
       prcd002 LIKE prcd_t.prcd002, #檔期類型
       prcd003 LIKE prcd_t.prcd003, #活動等級
       prcd004 LIKE prcd_t.prcd004, #開始日期
       prcd005 LIKE prcd_t.prcd005, #截止日期
       prcd006 LIKE prcd_t.prcd006, #制定促銷方案否
       prcd098 LIKE prcd_t.prcd098, #應用業態
       prcdownid LIKE prcd_t.prcdownid, #資料所有者
       prcdowndp LIKE prcd_t.prcdowndp, #資料所屬部門
       prcdcrtid LIKE prcd_t.prcdcrtid, #資料建立者
       prcdcrtdp LIKE prcd_t.prcdcrtdp, #資料建立部門
       prcdcrtdt LIKE prcd_t.prcdcrtdt, #資料創建日
       prcdmodid LIKE prcd_t.prcdmodid, #資料修改者
       prcdmoddt LIKE prcd_t.prcdmoddt, #最近修改日
       prcd007 LIKE prcd_t.prcd007      #年度
END RECORD
#161111-00028#2----add---end-------------

   INITIALIZE l_prcd.* TO NULL
 #  SELECT * INTO l_prcd.*    #161111-00028#2--mark
    #161111-00028#2--add--begin--------------
    SELECT  prcdent,prcdunit,prcdsite,prcdstus,prcd001,prcd002,prcd003,prcd004,prcd005,prcd006,
            prcd098,prcdownid,prcdowndp,prcdcrtid,prcdcrtdp,prcdcrtdt,prcdmodid,prcdmoddt,prcd007
            INTO l_prcd.* 
    #161111-00028#2--add--end----------------
     FROM prcd_t
    WHERE prcdent = g_enterprise
      AND prcd001= g_prcf_m.prcf002
   LET g_prcf_m.prcf007 = l_prcd.prcd002
   LET g_prcf_m.prcf008 = l_prcd.prcd003
   LET g_prcf_m.prcf009 = l_prcd.prcd004
   LET g_prcf_m.prcf010 = l_prcd.prcd005
   IF cl_null(g_prcf_m.prcf046) THEN
      LET g_prcf_m.prcf046 = l_prcd.prcd007
   END IF
   DISPLAY BY NAME g_prcf_m.prcf007,g_prcf_m.prcf008,g_prcf_m.prcf009,g_prcf_m.prcf010,g_prcf_m.prcf046
END FUNCTION

