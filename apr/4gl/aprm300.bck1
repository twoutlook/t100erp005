   #add-point:set_no_entry段欄位控制後 name="set_no_entry.after_control"
   IF NOT s_aooi500_comp_entry(g_prog,'prcfunit') OR g_site_flag THEN
      CALL cl_set_comp_entry("prcfunit",FALSE)
   END IF
   IF g_prcf_m.prcf051 = '1' OR g_prcf_m.prcf051 = '2' THEN
