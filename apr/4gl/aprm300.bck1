      LET g_prcf_m.prcf056_desc = ''
      DISPLAY BY NAME g_prcf_m.prcf056_desc
      CALL cl_set_comp_required("prcf055,prcf056",FALSE)
      CALL cl_set_comp_entry("prcf055,prcf056",FALSE)
   END IF
   
   IF p_cmd = 'u' AND g_prcf_m.prcfstus='Y' THEN
      CALL cl_set_comp_entry("prcfl003,prcfl004,prcf003,prcf004,prcf046,prcf002,prcf007,prcf008,prcf051",FALSE)
      CALL cl_set_comp_entry("prcf055,prcf056,prcf047,prcf048,prcf049,prcf050,prcf009,prcf053,prcf010,prcf054",FALSE)
      CALL cl_set_comp_entry("prcf052,prcf013,prcf014,prcf015,prcf019,prcf016,prcf017,prcf018,prcf057,prcf058",FALSE)
   END IF
   #160104-00012#1----add---begin--by huangrh--20160106--- 
   IF g_prcf_m.prcf051<>'4' THEN
      CALL cl_set_comp_entry("prcf005",FALSE)          
