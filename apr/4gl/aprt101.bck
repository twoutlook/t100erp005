      LET g_prbb_m.prbbmodid = g_user
      LET g_prbb_m.prbbmoddt = cl_get_current()
      LET g_prbb_m.prbbstus = 'N'
 
 
 
 
      #append欄位給值
      
     
      #一般欄位給值
            LET g_prbb_m.prbb001 = "2"
      LET g_prbb_m.prbbstus = "N"
 
  
      #add-point:單頭預設值 name="insert.default"
      LET g_prbb_m.prbbunit =g_site
      LET g_site_flag = FALSE
      CALL s_aooi500_default(g_prog,'prbbsite',g_prbb_m.prbbsite)
         RETURNING l_insert,g_prbb_m.prbbsite
      IF NOT l_insert THEN
         RETURN
      END IF
#      LET g_prbb_m.prbbsite = g_prbb_m.prbbunit
      CALL aprt101_prbbsite_ref()
      LET g_prbb_m.prbbdocdt = g_today
      LET g_prbb_m.prbb006 = g_user
      CALL aprt101_prbb006_ref()
      SELECT ooag003 INTO g_prbb_m.prbb007
        FROM ooag_t
       WHERE ooagent= g_enterprise
         AND ooag001= g_prbb_m.prbb006 
      CALL aprt101_prbb007_ref()     
  
      #dongsz--add--str---
      #預設單據的單別
      LET r_success = ''
      LET r_doctype = ''
      CALL s_arti200_get_def_doc_type(g_prbb_m.prbbunit,g_prog,'1')
           RETURNING r_success,r_doctype
      LET g_prbb_m.prbbdocno = r_doctype
      #dongsz--add--end---

      LET g_prbb_m_t.* = g_prbb_m.*
      #end add-point 
      
      #保存單頭舊值(用於資料輸入錯誤還原預設值時使用)
      LET g_prbb_m_t.* = g_prbb_m.*
      LET g_prbb_m_o.* = g_prbb_m.*
      
      #顯示狀態(stus)圖片
            #應用 a21 樣板自動產生(Version:3)
	  #根據當下狀態碼顯示圖片
      CASE g_prbb_m.prbbstus 
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "A"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/approved.png")
         WHEN "D"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/withdraw.png")
         WHEN "R"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/rejection.png")
         WHEN "W"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/signing.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
 
 
 
    
      CALL aprt101_input("a")
      
      #add-point:單頭輸入後 name="insert.after_insert"
                                                            
