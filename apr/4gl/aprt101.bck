#chk prbbdocno
PRIVATE FUNCTION aprt101_chk_prbbdocno()
   DEFINE   l_cnt       LIKE type_t.num5
   DEFINE   l_cnt1      LIKE type_t.num5
   define   p_prbbdocno like prbb_t.prbbdocno   
   LET l_cnt=0
   LET l_cnt1=0
   LET g_errno = null
   SELECT COUNT(*) INTO l_cnt  FROM ooba_t lEFT JOIN oobl_t ON ooba001=oobl001 AND ooba002=oobl002 AND oobaent=ooblent  #160906-00061#1 dongsz add ent
    WHERE ooba001 = g_ooef004 AND ooba002=g_prbb_m.prbbdocno
      AND oobaent = g_enterprise            #160906-00061#1 dongsz add
      AND oobl003 = 'aprt101'
   IF l_cnt <= 0 THEN
      LET g_errno = "aim-00056"
   ELSE
