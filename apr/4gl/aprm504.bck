#               DISPLAY g_rec_b TO formonly.cnt
#               DISPLAY g_detail_idx TO formonly.idx
#         END DISPLAY
         
         DISPLAY ARRAY g_prfm2_d TO s_detail2.* ATTRIBUTES (count=g_rec_b)
            BEFORE DISPLAY
               LET l_current_page = 2
               CALL aprm504_idx_chk()
            BEFORE ROW
