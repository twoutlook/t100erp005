{<section id="aprm504.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION aprm504_b_fill(p_wc2)
   #add-point:b_fill段define name="b_fill.define_customerization"
   
   #end add-point
   DEFINE p_wc2      STRING
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   DEFINE l_ooef019  LIKE ooef_t.ooef019
   #end add-point     
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   #先清空單身變數內容
   CALL g_prfm_d.clear()
 
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   CALL aprm504_idx_chk()
   LET l_ac = g_detail_idx
   #end add-point
   
   LET g_sql = "SELECT  DISTINCT prfmstus,prfm003,prfm005,prfm006,prfm007,prfm008,prfm009,prfm010,prfm011, 
       prfm012,prfm013,prfm014,prfm015,prfm016,prfm017,prfm018,prfmunit,prfm019,t1.ooail003 ,t2.oocal003 FROM prfm_t", 
          
               "",
               
                              " LEFT JOIN ooail_t t1 ON t1.ooailent="||g_enterprise||" AND t1.ooail001=prfm005 AND t1.ooail002='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t2 ON t2.oocalent="||g_enterprise||" AND t2.oocal001=prfm009 AND t2.oocal002='"||g_dlang||"' ",
 
               " WHERE prfment= ? AND prfmsite=? AND prfm001=? AND prfm002=? AND prfm004=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("prfm_t")
   END IF
   
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   IF cl_null(l_ac) OR l_ac=0 THEN
      LET l_ac=1
   END IF
   IF NOT cl_null(g_prfm2_d[l_ac].prfm003) THEN
      LET g_sql = g_sql CLIPPED clipped," AND prfm003='",g_prfm2_d[l_ac].prfm003,"' "
   END IF
   IF NOT cl_null(g_prfm2_d[l_ac].prfm020) THEN
      LET g_sql = g_sql CLIPPED clipped," AND prfm020='",g_prfm2_d[l_ac].prfm020,"' "
   END IF
   IF aprm504_fill_chk(1) then
      LET g_sql = g_sql, " ORDER BY prfm_t.prfm007 DESC "
	  
	  
      PREPARE aprm504_pb2 FROM g_sql
      DECLARE b_fill_cs2 CURSOR FOR aprm504_pb2
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs2 USING g_enterprise,g_prfm_m.prfmsite,g_prfm_m.prfm001,g_prfm_m.prfm002,g_prfm_m.prfm004
 
 
                                               
      FOREACH b_fill_cs2 INTO g_prfm_d[l_ac].prfmstus,g_prfm_d[l_ac].prfm003,g_prfm_d[l_ac].prfm005,g_prfm_d[l_ac].prfm006, 
          g_prfm_d[l_ac].prfm007,g_prfm_d[l_ac].prfm008,g_prfm_d[l_ac].prfm009,g_prfm_d[l_ac].prfm010, 
          g_prfm_d[l_ac].prfm011,g_prfm_d[l_ac].prfm012,g_prfm_d[l_ac].prfm013,g_prfm_d[l_ac].prfm014, 
          g_prfm_d[l_ac].prfm015,g_prfm_d[l_ac].prfm016,g_prfm_d[l_ac].prfm017,g_prfm_d[l_ac].prfm018, 
          g_prfm_d[l_ac].prfmunit,g_prfm_d[l_ac].prfm019,g_prfm_d[l_ac].prfm005_desc,g_prfm_d[l_ac].prfm009_desc
 
                             
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "FOREACH:"
            LET g_errparam.popup = TRUE
            CALL cl_err()

            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充

         #end add-point
        
         #add-point:單身資料抓取
#         INITIALIZE g_ref_fields TO NULL
#         LET g_ref_fields[1] = g_prfm_d[l_ac].prfm005
#         CALL ap_ref_array2(g_ref_fields,"SELECT ooail003 FROM ooail_t WHERE ooailent='"||g_enterprise||"' AND ooail001=? AND ooail002='"||g_dlang||"'","") RETURNING g_rtn_fields
#         LET g_prfm_d[l_ac].prfm005_desc = '', g_rtn_fields[1] , ''
#         DISPLAY BY NAME g_prfm_d[l_ac].prfm005_desc
        
         SELECT ooef019 INTO l_ooef019 FROM ooef_t
          WHERE ooefent = g_enterprise
            AND ooef001 = g_prfm_m.prfmsite
         INITIALIZE g_ref_fields TO NULL
         LET g_ref_fields[1] = l_ooef019
         LET g_ref_fields[2] = g_prfm_d[l_ac].prfm006
         CALL ap_ref_array2(g_ref_fields,"SELECT oodbl004 FROM oodbl_t WHERE oodblent='"||g_enterprise||"' AND oodbl001=? AND oodbl002=? AND oodbl003='"||g_dlang||"'","") RETURNING g_rtn_fields
         LET g_prfm_d[l_ac].prfm006_desc = '', g_rtn_fields[1] , ''
         DISPLAY BY NAME g_prfm_d[l_ac].prfm006_desc
#
#         INITIALIZE g_ref_fields TO NULL
