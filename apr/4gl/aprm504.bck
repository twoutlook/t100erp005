#fill prfm_t
PRIVATE FUNCTION aprm504_prfm_fill(p_wc3)
   DEFINE l_sql      STRING
   DEFINE p_wc3      STRING
   
   LET g_detail_idx =1
   CALL g_prfm2_d.clear()
   LET l_sql = " SELECT DISTINCT prfm020,prfm003,'','','','' ",
               "   FROM prfm_t ",
               "  WHERE prfment=",g_enterprise," AND prfmsite='",g_prfm_m.prfmsite,"' ",
               "    AND prfm002='",g_prfm_m.prfm002,"' AND prfm004='",g_prfm_m.prfm004,"' "

   IF NOT cl_null(p_wc3) THEN
      LET l_sql=l_sql," AND ",p_wc3
   END IF 
   LET l_sql=l_sql,"   ORDER BY prfm003 "   
   PREPARE l_sql_pre2 FROM l_sql
   DECLARE l_sql_cs2 CURSOR FOR l_sql_pre2
   LET g_cnt = l_ac
   LET l_ac = 1
   FOREACH l_sql_cs2 INTO g_prfm2_d[l_ac].prfm020,g_prfm2_d[l_ac].prfm003,g_prfm2_d[l_ac].imaal003,g_prfm2_d[l_ac].imaal004,g_prfm2_d[l_ac].imaa009,g_prfm2_d[l_ac].imaa009_desc
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "FOREACH:"
            LET g_errparam.popup = TRUE
            CALL cl_err()

            EXIT FOREACH
         END IF
         SELECT imaal003,imaal004 INTO g_prfm2_d[l_ac].imaal003,g_prfm2_d[l_ac].imaal004
           FROM imaal_t
          WHERE imaal001 = g_prfm2_d[l_ac].prfm003 AND imaalent = g_enterprise
            AND imaal002 = g_dlang

         SELECT imaa009 INTO g_prfm2_d[l_ac].imaa009
           FROM imaa_t
          WHERE imaaent = g_enterprise AND imaa001 = g_prfm2_d[l_ac].prfm003
          
         SELECT rtaxl003 INTO g_prfm2_d[l_ac].imaa009_desc
           FROM rtaxl_t
          WHERE rtaxlent = g_enterprise AND rtaxl001 = g_prfm2_d[l_ac].imaa009
            AND rtaxl002 = g_dlang          
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec AND g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code =  9035
            LET g_errparam.extend =  ''
            LET g_errparam.popup = TRUE
            CALL cl_err()

            EXIT FOREACH
         END IF
         
   END FOREACH
   LET g_error_show = 0
   
   CALL g_prfm2_d.deleteElement(g_prfm2_d.getLength())
 
   LET g_rec_b = l_ac-1
   DISPLAY g_rec_b TO formonly.cnt
   DISPLAY g_detail_idx TO formonly.idx
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE l_sql_cs2 
END FUNCTION

