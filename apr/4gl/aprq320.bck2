       g_stfa_d[l_ac].rtaxl003,g_stfa_d[l_ac].stfc004,g_stfa_d[l_ac].l_rtaxl003,g_stfa_d[l_ac].stfa010, 
       g_stfa_d[l_ac].pmaal004,g_stfa_d[l_ac].stfa005,g_stfa_d[l_ac].stfal003,g_stfa_d[l_ac].stfc002, 
       g_stfa_d[l_ac].stfc003,g_stfa_d[l_ac].l_stfc024,g_stfa_d[l_ac].stfa001,g_stfa_d[l_ac].stfa004, 
       g_stfa_d[l_ac].stfa019,g_stfa_d[l_ac].stfa020,g_stfa_d[l_ac].stfc018,g_stfa_d[l_ac].preh003,g_stfa_d[l_ac].preh004, 
       g_stfa_d[l_ac].l_kind
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #LET g_stfa_d[l_ac].statepic = cl_get_actipic(g_stfa_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充 name="b_fill.fill"

