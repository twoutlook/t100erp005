     IF  g_stfa_d[l_ac].l_stfc024 ='2'  THEN 
         IF cl_null(g_stfa_d[l_ac].preh003) or cl_null(g_stfa_d[l_ac].preh004) THEN 
           let g_stfa_d[l_ac].l_kind = '0'
