      stfaent  LIKE stfa_t.stfaent,
      stfasite LIKE stfa_t.stfasite, 
      ooefl003 LIKE ooefl_t.ooefl003, 
      stfa051 LIKE stfa_t.stfa051, 
      rtaxl003 LIKE rtaxl_t.rtaxl003, 
