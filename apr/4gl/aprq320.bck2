                     "   FROM aprq320_temp  ",
                     "  WHERE stfaent = ? ",
                     " ORDER BY stfa051,stfa005,stfc002 "
   #end add-point
 
   LET g_sql = "SELECT COUNT(1) FROM (",ls_sql_rank,")"
 
