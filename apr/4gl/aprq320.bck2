                     " LEFT JOIN rtaxl_t t1 ON t1.rtaxlent=stfaent AND t1.rtaxl001=stfa051 AND t1.rtaxl002='",g_dlang,"'" , 
                     " LEFT JOIN rtaxl_t t2 ON t2.rtaxlent=stfcent AND t2.rtaxl001=stfc004 AND t2.rtaxl002='",g_dlang,"'" , 
                     " LEFT join pmaal_t ON pmaalent=stfaent AND pmaal001=stfa010 AND pmaal002='",g_dlang,"'" , 
                     " LEFT join stfal_t ON stfalent=stfaent AND stfal001=stfa001 AND stfal002='",g_dlang,"'" , 
                     " LEFT join gzcbl_t ON gzcbl001='6781' AND gzcbl002=stfc024 AND gzcbl003='",g_dlang,"'" , 
                    " WHERE stfa004!='7' and stfastus!='X' and stfaent= ",g_enterprise," AND stfc024='2' AND 1=1 ",
                    " AND stfa004 IN ('1','2','3','4','5','6')", #added by lanjj 2015-12-23 合同状态限制为小于6 #lanjj add 6 on 2016-09-13
                    #") WHERE  ", ls_wc #20160307 mark by beckxie
                    "AND ",ls_wc,") WHERE 1=1 "   #20160307 add by beckxie
   DISPLAY "ls_sql_rank:",ls_sql_rank                    
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("stfa_t")

   LET l_sql = "INSERT INTO aprq320_temp ",ls_sql_rank
   TRY
      EXECUTE IMMEDIATE l_sql
   CATCH
      IF STATUS THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "ins aprq320_temp:" 
         LET g_errparam.code   = STATUS
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         RETURN
      END IF
   END TRY
   
   LET l_sql = "INSERT INTO aprq320_temp2 ",
               "   SELECT  preient,prei003,prei004,preg013,preg014,preh003,preh004  ",
               "     FROM prei_t ",
               "     LEFT join preg_t ON preient=pregent AND prei001=preg001 ",
               "     LEFT join preh_t ON prehent=preient AND preh001=prei001 ",
               "    WHERE pregstus != 'X' ",
               "      AND EXISTS(SELECT 1 FROM aprq320_temp WHERE stfaent = preient AND stfc002 = prei003 AND stfa005 = prei004) "
   TRY
      EXECUTE IMMEDIATE l_sql
   CATCH
      IF STATUS THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "ins aprq320_temp2:" 
         LET g_errparam.code   = STATUS
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         RETURN
      END IF
