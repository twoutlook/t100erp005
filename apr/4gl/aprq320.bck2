         LET g_errparam.code   = STATUS
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         RETURN
      END IF
   END TRY
   
   LET l_sql = " MERGE INTO aprq320_temp a",
               " USING aprq320_temp3 b",
