                  LET l_lock_sw='Y'
               ELSE
                  FETCH aprt212_01_bcl INTO g_prdj_d[l_ac].prdj002,g_prdj_d[l_ac].prdj003,g_prdj_d[l_ac].prdj004, 
                      g_prdj_d[l_ac].prdj004_desc,g_prdj_d[l_ac].prdj004_desc1,g_prdj_d[l_ac].prdj005, 
