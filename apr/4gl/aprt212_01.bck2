            LET g_current_row = g_current_idx
            LET g_curr_diag = ui.DIALOG.getCurrent()
            CALL aprt212_01_idx_chk()
            
         ON ACTION jump
            CALL aprt212_01_fetch('/')
            LET g_current_row = g_current_idx
            LET g_curr_diag = ui.DIALOG.getCurrent()
            CALL aprt212_01_idx_chk()
            
         ON ACTION next
            CALL aprt212_01_fetch('N')
            LET g_current_row = g_current_idx
            LET g_curr_diag = ui.DIALOG.getCurrent()
            CALL aprt212_01_idx_chk()
            
         ON ACTION last
            CALL aprt212_01_fetch('L')
            LET g_current_row = g_current_idx
            LET g_curr_diag = ui.DIALOG.getCurrent()
            CALL aprt212_01_idx_chk()
            
