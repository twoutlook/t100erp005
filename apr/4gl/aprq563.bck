DEFINE l_sql      STRING    
   
   DELETE FROM aprq563_tmp
   LET l_sql = " INSERT INTO aprq563_tmp(ooed004) ",
               #" SELECT ooed004  FROM ooed_t WHERE  ooed001='8' ",#160905-00007#12 mark
