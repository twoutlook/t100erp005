#               "  CONNECT BY  NOCYCLE PRIOR ooed004=ooed005 AND ooed001='8' ",
#               "   AND TO_CHAR(ooed006,'YYYY/MM/DD')<='",g_today,"' AND (TO_CHAR(ooed007,'YYYY/MM/DD')>='",g_today,"' OR ooed007 IS null))",
#               "          UNION ",
#               "         (SELECT ooed004 FROM ooed_t WHERE ooed004='",g_site,"'))"
#   PREPARE l_sql_ooed004_pre1 FROM l_sql
#   EXECUTE l_sql_ooed004_pre1
#   IF sqlca.sqlcode THEN
#      INITIALIZE g_errparam TO NULL
#      LET g_errparam.code = sqlca.sqlcode
#      LET g_errparam.extend = ''
#      LET g_errparam.popup = TRUE
#      CALL cl_err()
#
#      RETURN
#   END IF
   LET l_where = s_aooi500_sql_where(g_prog,'prcgsite')
   IF cl_null(g_wc) THEN
      LET g_wc = l_where
   ELSE
      LET g_wc = g_wc CLIPPED," AND ",l_where
   END IF
   #end add-point
 
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
 
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
 
   CALL g_prcg_d.clear()
 
   #add-point:陣列清空 name="b_fill.array_clear"
   
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1
 
   # b_fill段sql組成及FOREACH撰寫
   #應用 qs04 樣板自動產生(Version:9)
   #+ b_fill段資料取得(包含sql組成及FOREACH段撰寫)
   LET ls_sql_rank = "SELECT  UNIQUE '',prcgsite,'',prcg001,'',prcg002,'','','','','','','','','','', 
       '','','','','','','',''  ,DENSE_RANK() OVER( ORDER BY prcg_t.prcgdocno) AS RANK FROM prcg_t", 
 
 
 
                     "",
                     " WHERE prcgent= ? AND 1=1 AND ", ls_wc
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("prcg_t"),
                     " ORDER BY prcg_t.prcgdocno"
 
   #add-point:b_fill段rank_sql_after name="b_fill.rank_sql_after"
   #160307-00023#2 160314 By pomelo add(S)
   LET ls_sql_rank = "SELECT 'N', prcgsite, (SELECT ooefl003",
                     "                         FROM ooefl_t",
                     "                        WHERE ooeflent = prcgent",
                     "                          AND ooefl001 = prcgsite",
                     "                          AND ooefl002 = '",g_dlang,"') prcgsite_desc,",
                     "        prcg001, (SELECT prcfl003",
                     "                    FROM prcfl_t",
                     "                   WHERE prcflent = prcgent",
                     "                     AND prcfl001 = prcg001",
                     "                     AND prcfl002 = '",g_dlang,"') prcg001_desc,",
                     "        prcg002, (SELECT prcdl003",
                     "                    FROM prcdl_t",
                     "                   WHERE prcdlent = prcgent",
                     "                     AND prcdl001 = prcg002",
                     "                     AND prcdl002 = '",g_dlang,"') prcg002_desc,",
                     "        prch002, (SELECT pmaal004",
                     "                    FROM pmaal_t",
                     "                   WHERE pmaalent = prchent",
                     "                     AND pmaal001 = prch002",
                     "                     AND pmaal002 = '",g_dlang,"') prch002_desc,",
                     "        prch003, (SELECT pmaal004",
                     "                    FROM pmaal_t",
                     "                   WHERE pmaalent = prchent",
                     "                     AND pmaal001 = prch003",
                     "                     AND pmaal002 = '",g_dlang,"') prch003_desc,",
                     "        prch004, (SELECT oojdl003",
                     "                    FROM oojdl_t",
                     "                   WHERE oojdlent = prchent",
                     "                     AND oojdl001 = prch004",
                     "                     AND oojdl002 = '",g_dlang,"') prch004_desc,",
                     "       prch005, (SELECT oojdl003",
                     "                   FROM oojdl_t",
                     "                  WHERE oojdlent = prchent",
                     "                    AND oojdl001 = prch005",
                     "                    AND oojdl002 = '",g_dlang,"') prch005_desc,",
                     "       prch006, (SELECT ooefl003",
                     "                   FROM ooefl_t",
                     "                  WHERE ooeflent = prchent",
                     "                    AND ooefl001 = prch006",
                     "                    AND ooefl002 = '",g_dlang,"') prch006_desc,",
                     "       prch007, (SELECT dbbal003",
                     "                   FROM dbbal_t",
                     "                  WHERE dbbalent = prchent",
                     "                    AND dbbal001 = prch007",
                     "                    AND dbbal002 = '",g_dlang,"') prch007_desc,",
                     "       prch008, prch009, prch010, prch011, prch012, ",
                     "       DENSE_RANK() OVER( ORDER BY prcg_t.prcgdocno) AS RANK",
                     "  FROM prcg_t LEFT JOIN prch_t ON prchent=prcgent AND prchdocno=prcgdocno ",
                     "              LEFT JOIN prcf_t ON prcfent=prcgent AND prcf001=prcg001 AND prcf098='2' ",
                     "              LEFT JOIN prcd_t ON prcdent=prcgent AND prcd001=prcg002 ",
                     " WHERE prcgent = ?",
                     "   AND prcgstus='Y' ",
                     "   AND ",ls_wc               
    
   LET ls_sql_rank = ls_sql_rank," ORDER BY prcg_t.prcgsite,prcg_t.prcg001,prcg_t.prcg002,prch_t.prchdocno,prch_t.prchseq "
   #160307-00023#2 160314 By pomelo add(E)
   #end add-point
 
   LET g_sql = "SELECT COUNT(1) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql  #總筆數
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   #add-point:b_fill段rank_sql_after_count name="b_fill.rank_sql_after_count"
   
   #end add-point
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT '',prcgsite,'',prcg001,'',prcg002,'','','','','','','','','','','','','','','', 
       '','',''",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   #160307-00023#2 160314 By pomelo add(S)
   LET g_sql = "SELECT 'N', prcgsite, prcgsite_desc, prcg001, prcg001_desc,",
               "            prcg002,  prcg002_desc,  prch002, prch002_desc,",
               "            prch003,  prch003_desc,  prch004, prch004_desc,",
               "            prch005,  prch005_desc,  prch006, prch006_desc,",
               "            prch007,  prch007_desc,  prch008, prch009,",
               "            prch010,  prch011,       prch012 ",
               "  FROM (",ls_sql_rank,")",
               " WHERE RANK >= ",g_pagestart,
               "   AND RANK < ",g_pagestart + g_num_in_page
   #160307-00023#2 160314 By pomelo add(E)
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE aprq563_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aprq563_pb
 
   OPEN b_fill_curs USING g_enterprise
 
   FOREACH b_fill_curs INTO g_prcg_d[l_ac].sel,g_prcg_d[l_ac].prcgsite,g_prcg_d[l_ac].prcgsite_desc, 
       g_prcg_d[l_ac].prcg001,g_prcg_d[l_ac].prcg001_desc,g_prcg_d[l_ac].prcg002,g_prcg_d[l_ac].prcg002_desc, 
       g_prcg_d[l_ac].prch002,g_prcg_d[l_ac].prch002_desc,g_prcg_d[l_ac].prch003,g_prcg_d[l_ac].prch003_desc, 
       g_prcg_d[l_ac].prch004,g_prcg_d[l_ac].prch004_desc,g_prcg_d[l_ac].prch005,g_prcg_d[l_ac].prch005_desc, 
       g_prcg_d[l_ac].prch006,g_prcg_d[l_ac].prch006_desc,g_prcg_d[l_ac].prch007,g_prcg_d[l_ac].prch007_desc, 
       g_prcg_d[l_ac].prch008,g_prcg_d[l_ac].prch009,g_prcg_d[l_ac].prch010,g_prcg_d[l_ac].prch011,g_prcg_d[l_ac].prch012 
 
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
 
      
 
      #add-point:b_fill段資料填充 name="b_fill.fill"
      
      #end add-point
 
      CALL aprq563_detail_show("'1'")
 
      CALL aprq563_prcg_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
 
   END FOREACH
 
 
 
 
 
   #應用 qs05 樣板自動產生(Version:3)
   #+ b_fill段其他table資料取得(包含sql組成及資料填充)
 
 
 
 
 
 
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   
   #end add-point
 
   CALL g_prcg_d.deleteElement(g_prcg_d.getLength())
 
   #add-point:陣列長度調整 name="b_fill.array_deleteElement"
   
   #end add-point
 
   LET g_error_show = 0
 
   LET g_detail_cnt = g_prcg_d.getLength()
   LET l_ac = g_cnt
   LET g_cnt = 0
 
   #應用 qs06 樣板自動產生(Version:3)
   #+ b_fill段CURSOR釋放
   CLOSE b_fill_curs
   FREE aprq563_pb
 
 
 
 
 
 
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL aprq563_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL aprq563_detail_action_trans()
 
   LET l_ac = 1
   IF g_prcg_d.getLength() > 0 THEN
      CALL aprq563_b_fill2()
   END IF
 
      CALL aprq563_filter_show('prcgsite','b_prcgsite')
   CALL aprq563_filter_show('prcg001','b_prcg001')
   CALL aprq563_filter_show('prcg002','b_prcg002')
   CALL aprq563_filter_show('prch002','b_prch002')
   CALL aprq563_filter_show('prch003','b_prch003')
   CALL aprq563_filter_show('prch004','b_prch004')
   CALL aprq563_filter_show('prch005','b_prch005')
   CALL aprq563_filter_show('prch006','b_prch006')
   CALL aprq563_filter_show('prch007','b_prch007')
   CALL aprq563_filter_show('prch008','b_prch008')
   CALL aprq563_filter_show('prch009','b_prch009')
   CALL aprq563_filter_show('prch010','b_prch010')
   CALL aprq563_filter_show('prch011','b_prch011')
   CALL aprq563_filter_show('prch012','b_prch012')
 
 
END FUNCTION
 
