  <section id="aeci005.modify" ver="4" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aeci005_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_ecab_m.ecab001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE aeci005_master_referesh USING g_ecab_m.ecab001 INTO g_ecab_m.ecab001,g_ecab_m.ecab001_desc 

 
   ERROR ""
  
   LET g_ecab001_t = g_ecab_m.ecab001
 
   CALL s_transaction_begin()
   
   OPEN aeci005_cl USING g_enterprise,g_ecab_m.ecab001
 
   IF STATUS THEN
      CALL cl_err("OPEN aeci005_cl:", STATUS, 1)
      CLOSE aeci005_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aeci005_cl INTO g_ecab_m.ecab001,g_ecab_m.ecab001_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_ecab_m.ecab001,SQLCA.sqlcode,0)
      CLOSE aeci005_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL aeci005_show()
   WHILE TRUE
      LET g_ecab001_t = g_ecab_m.ecab001
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL aeci005_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_ecab_m.* = g_ecab_m_t.*
         CALL aeci005_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_ecab_m.ecab001 != g_ecab001_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE ecab_t SET ecab001 = g_ecab_m.ecab001
 
          WHERE ecabent = g_enterprise AND ecab001 = g_ecab001_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("ecab_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("ecab_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_ecab_m.ecab001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE aeci005_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_ecab_m.ecab001,'U')
 
   CALL aeci005_b_fill("1=1")
   
END FUNCTION   
]]>
  </section>
