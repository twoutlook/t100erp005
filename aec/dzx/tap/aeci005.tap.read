      #end add-point
      
 
      
      CLEAR FORM
      CALL g_ecab_d.clear() 
      CALL g_ecab2_d.clear()       
 
     
      CALL aeci005_ui_browser_refresh()  
      CALL aeci005_ui_headershow()  
