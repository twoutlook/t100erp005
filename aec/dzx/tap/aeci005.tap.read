      #LET g_ecab2_d[l_ac].ecabcrtid_desc = cl_get_username(g_ecab2_d[l_ac].ecabcrtid)
      #LET g_ecab2_d[l_ac].ecabcrtdp_desc = cl_get_deptname(g_ecab2_d[l_ac].ecabcrtdp)
      #LET g_ecab2_d[l_ac].ecabmodid_desc = cl_get_username(g_ecab2_d[l_ac].ecabmodid)
      
 
 
 
        
