      #end add-point
      
      DELETE FROM ecab_t WHERE ecabent = g_enterprise AND ecab001 = g_ecab_m.ecab001
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("ecab_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
