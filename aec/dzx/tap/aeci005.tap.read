          g_ecab_d[l_ac].ecab004,g_ecab_d[l_ac].ecab005,g_ecab_d[l_ac].ecab006,g_ecab_d[l_ac].ecab007, 
          g_ecab_d[l_ac].ecab008,g_ecab2_d[l_ac].ecabseq,g_ecab2_d[l_ac].ecabmodid,g_ecab2_d[l_ac].ecabmoddt, 
          g_ecab2_d[l_ac].ecabownid,g_ecab2_d[l_ac].ecabowndp,g_ecab2_d[l_ac].ecabcrtid,g_ecab2_d[l_ac].ecabcrtdp, 
          g_ecab2_d[l_ac].ecabcrtdt,g_ecab_d[l_ac].ecab003_desc,g_ecab2_d[l_ac].ecabmodid_desc,g_ecab2_d[l_ac].ecabownid_desc, 
          g_ecab2_d[l_ac].ecabowndp_desc,g_ecab2_d[l_ac].ecabcrtid_desc,g_ecab2_d[l_ac].ecabcrtdp_desc 

                             
         IF SQLCA.sqlcode THEN
