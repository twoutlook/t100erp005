      #add-point:多語言刪除
      {<point name="delete.lang.delete"/>}
      #end add-point
   
   END IF
 
   CALL s_transaction_end('Y','0')
   
   CLOSE aect801_cl
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_ecca_m.eccasite,'D')
    
END FUNCTION
