  <section id="aeci001.modify" ver="4" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aeci001_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point
   
   IF g_ecaa_m.ecaa001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF 
   
   EXECUTE aeci001_master_referesh USING g_ecaa_m.ecaa001 INTO g_ecaa_m.ecaa001,g_ecaa_m.ecaa002,g_ecaa_m.ecaa003, 
       g_ecaa_m.ecaa004,g_ecaa_m.ecaa005,g_ecaa_m.ecaa006,g_ecaa_m.ecaa007,g_ecaa_m.ecaa008,g_ecaa_m.ecaa009, 
       g_ecaa_m.ecaa010,g_ecaa_m.ecaa011,g_ecaa_m.ecaastus,g_ecaa_m.ecaaownid,g_ecaa_m.ecaaowndp,g_ecaa_m.ecaacrtid, 
       g_ecaa_m.ecaacrtdp,g_ecaa_m.ecaacrtdt,g_ecaa_m.ecaamodid,g_ecaa_m.ecaamoddt,g_ecaa_m.ecaa003_desc, 
       g_ecaa_m.ecaa005_desc,g_ecaa_m.ecaaownid_desc,g_ecaa_m.ecaaowndp_desc,g_ecaa_m.ecaacrtid_desc, 
       g_ecaa_m.ecaacrtdp_desc,g_ecaa_m.ecaamodid_desc
 
   ERROR ""
  
   LET g_ecaa001_t = g_ecaa_m.ecaa001
 
   
   CALL s_transaction_begin()
   
   OPEN aeci001_cl USING g_enterprise, g_site,g_ecaa_m.ecaa001
   IF STATUS THEN
      CALL cl_err("OPEN aeci001_cl:", STATUS, 1)
      CLOSE aeci001_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aeci001_master_referesh USING g_ecaa_m.ecaa001 INTO g_ecaa_m.ecaa001,g_ecaa_m.ecaa002,g_ecaa_m.ecaa003, 
       g_ecaa_m.ecaa004,g_ecaa_m.ecaa005,g_ecaa_m.ecaa006,g_ecaa_m.ecaa007,g_ecaa_m.ecaa008,g_ecaa_m.ecaa009, 
       g_ecaa_m.ecaa010,g_ecaa_m.ecaa011,g_ecaa_m.ecaastus,g_ecaa_m.ecaaownid,g_ecaa_m.ecaaowndp,g_ecaa_m.ecaacrtid, 
       g_ecaa_m.ecaacrtdp,g_ecaa_m.ecaacrtdt,g_ecaa_m.ecaamodid,g_ecaa_m.ecaamoddt,g_ecaa_m.ecaa003_desc, 
       g_ecaa_m.ecaa005_desc,g_ecaa_m.ecaaownid_desc,g_ecaa_m.ecaaowndp_desc,g_ecaa_m.ecaacrtid_desc, 
       g_ecaa_m.ecaacrtdp_desc,g_ecaa_m.ecaamodid_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err("ecaa_t",SQLCA.sqlcode,0)
      CLOSE aeci001_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
 
   CALL aeci001_show()
   
   WHILE TRUE
      LET g_ecaa_m.ecaa001 = g_ecaa001_t
 
      
      #寫入修改者/修改日期資訊
      LET g_ecaa_m.ecaamodid = g_user 
LET g_ecaa_m.ecaamoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
 
      CALL aeci001_input("u")     #欄位更改
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_ecaa_m.* = g_ecaa_m_t.*
         CALL aeci001_show()
         CALL cl_err("",9001,0)
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE ecaa_t SET (ecaamodid,ecaamoddt) = (g_ecaa_m.ecaamodid,g_ecaa_m.ecaamoddt)
       WHERE ecaaent = g_enterprise AND ecaasite = g_site AND ecaa001 = g_ecaa001_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_ecaa_m.ecaa001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE aeci001_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_ecaa_m.ecaa001,"U")
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
