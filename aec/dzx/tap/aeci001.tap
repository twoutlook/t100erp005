  <section id="aeci001.delete" ver="7" status="" src="s">
    <![CDATA[#+ 資料刪除 
PRIVATE FUNCTION aeci001_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point  
   
   IF g_ecaa_m.ecaa001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   CALL aeci001_show()
   
   CALL s_transaction_begin()
    
   LET g_ecaa001_t = g_ecaa_m.ecaa001
 
   
   
 
   OPEN aeci001_cl USING g_enterprise, g_site,
                           g_ecaa_m.ecaa001
 
   IF STATUS THEN
      CALL cl_err("OPEN aeci001_cl:", STATUS, 0)
      CLOSE aeci001_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aeci001_master_referesh USING g_ecaa_m.ecaa001 INTO g_ecaa_m.ecaa001,g_ecaa_m.ecaa002,g_ecaa_m.ecaa003, 
       g_ecaa_m.ecaa004,g_ecaa_m.ecaa005,g_ecaa_m.ecaa006,g_ecaa_m.ecaa007,g_ecaa_m.ecaa008,g_ecaa_m.ecaa009, 
       g_ecaa_m.ecaa010,g_ecaa_m.ecaa011,g_ecaa_m.ecaastus,g_ecaa_m.ecaaownid,g_ecaa_m.ecaaowndp,g_ecaa_m.ecaacrtid, 
       g_ecaa_m.ecaacrtdp,g_ecaa_m.ecaacrtdt,g_ecaa_m.ecaamodid,g_ecaa_m.ecaamoddt,g_ecaa_m.ecaa003_desc, 
       g_ecaa_m.ecaa005_desc,g_ecaa_m.ecaaownid_desc,g_ecaa_m.ecaaowndp_desc,g_ecaa_m.ecaacrtid_desc, 
       g_ecaa_m.ecaacrtdp_desc,g_ecaa_m.ecaamodid_desc
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_ecaa_m.ecaa001,SQLCA.sqlcode,0)
      RETURN
   END IF
   
   IF cl_ask_delete() THEN
 
      #add-point:單頭刪除前
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point
 
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL aeci001_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
      DELETE FROM ecaa_t 
       WHERE ecaaent = g_enterprise AND ecaasite = g_site AND ecaa001 = g_ecaa_m.ecaa001 
 
 
      #add-point:單頭刪除中
      {<point name="delete.head.m_delete"/>}
      #end add-point
         
      IF SQLCA.sqlcode THEN
         CALL cl_err("ecaa_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
  
      
      
      #add-point:單頭刪除後
      {<point name="delete.head.a_delete"/>}
      #end add-point
      
          
      CLEAR FORM
      CALL aeci001_ui_browser_refresh()
      IF g_browser_cnt > 0 THEN
         CALL aeci001_browser_fill(g_wc,"")
         CALL aeci001_fetch("P")
      ELSE
         CALL aeci001_browser_fill(" 1=1 ","F")
      END IF
      
   END IF
 
   CLOSE aeci001_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_ecaa_m.ecaa001,"D")
 
END FUNCTION
]]>
