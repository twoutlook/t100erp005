  <section id="aeci001.filter" ver="5" status="" src="s">
    <![CDATA[#+ 此段落由子樣板a50產生
#+ filter過濾功能
PRIVATE FUNCTION aeci001_filter()
   #add-point:filter段define
   {<point name="filter.define"/>}
   #end add-point   
 
   #切換畫面
   IF NOT g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",1)
      CALL gfrm_curr.setElementHidden("worksheet",0)
      LET g_main_hidden = 1
   END IF   
 
   LET INT_FLAG = 0
 
   LET g_qryparam.state = 'c'
 
   LET g_wc_filter_t = g_wc_filter.trim()
   LET g_wc_t = g_wc
 
   LET g_wc = cl_replace_str(g_wc, g_wc_filter_t, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON ecaa001,ecaa002,ecaa003,ecaa004,ecaa005,ecaa006,ecaa007,ecaa008,ecaa009, 
          ecaa010,ecaa011
                          FROM s_browse[1].b_ecaa001,s_browse[1].b_ecaa002,s_browse[1].b_ecaa003,s_browse[1].b_ecaa004, 
                              s_browse[1].b_ecaa005,s_browse[1].b_ecaa006,s_browse[1].b_ecaa007,s_browse[1].b_ecaa008, 
                              s_browse[1].b_ecaa009,s_browse[1].b_ecaa010,s_browse[1].b_ecaa011
 
         BEFORE CONSTRUCT
               DISPLAY aeci001_filter_parser('ecaa001') TO s_browse[1].b_ecaa001
            DISPLAY aeci001_filter_parser('ecaa002') TO s_browse[1].b_ecaa002
            DISPLAY aeci001_filter_parser('ecaa003') TO s_browse[1].b_ecaa003
            DISPLAY aeci001_filter_parser('ecaa004') TO s_browse[1].b_ecaa004
            DISPLAY aeci001_filter_parser('ecaa005') TO s_browse[1].b_ecaa005
            DISPLAY aeci001_filter_parser('ecaa006') TO s_browse[1].b_ecaa006
            DISPLAY aeci001_filter_parser('ecaa007') TO s_browse[1].b_ecaa007
            DISPLAY aeci001_filter_parser('ecaa008') TO s_browse[1].b_ecaa008
            DISPLAY aeci001_filter_parser('ecaa009') TO s_browse[1].b_ecaa009
            DISPLAY aeci001_filter_parser('ecaa010') TO s_browse[1].b_ecaa010
            DISPLAY aeci001_filter_parser('ecaa011') TO s_browse[1].b_ecaa011
      
         #add-point:filter段cs_ctrl
         {<point name="filter.cs_ctrl"/>}
         #end add-point
      
      END CONSTRUCT
 
      #add-point:filter段add_cs
      {<point name="filter.add_cs"/>}
      #end add-point
 
      BEFORE DIALOG
         #add-point:filter段b_dialog
         {<point name="filter.b_dialog"/>}
         #end add-point  
      
      ON ACTION accept
         ACCEPT DIALOG
 
      ON ACTION cancel
         LET INT_FLAG = 1
         EXIT DIALOG 
 
      #交談指令共用ACTION
      &include "common_action.4gl" 
         CONTINUE DIALOG
   
   END DIALOG
 
   IF NOT INT_FLAG THEN
      LET g_wc_filter = "   AND   ", g_wc_filter, "   "
      LET g_wc = g_wc , g_wc_filter
   ELSE
      LET g_wc_filter = g_wc_filter_t
      LET g_wc = g_wc_t
   END IF
 
      CALL aeci001_filter_show('ecaa001')
   CALL aeci001_filter_show('ecaa002')
   CALL aeci001_filter_show('ecaa003')
   CALL aeci001_filter_show('ecaa004')
