#+ 資料輸入
PUBLIC FUNCTION aecm200_02(--)
   #add-point:main段變數傳入
   {<point name="main.get_var"/>}
   #end add-point
   )
   #add-point:main段define
   {<point name="main.define"/>}
   #end add-point   
 
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" mark="Y"/>}
   #end add-point
   LET g_forupd_sql = "SELECT ecbb001,ecbb002,'',ecbb003,ecbb004,'',ecbb005 FROM ecbb_t WHERE ecbbent=  
       ? AND ecbbsite= ? AND ecbb001=? AND ecbb002=? AND ecbb003=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)   #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE aecm200_02_cl CURSOR FROM g_forupd_sql     # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE ecbb001,ecbb002,ecbb003,ecbb004,ecbb005",
               " FROM ecbb_t",
               " WHERE ecbbent = '" ||g_enterprise|| "' AND ecbbsite = '" ||g_site|| "' AND ecbb001 = ? AND ecbb002 = ? AND ecbb003 = ?"
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE aecm200_02_master_referesh FROM g_sql
   
 
 
   
   #畫面開啟 (identifier)
   OPEN WINDOW w_aecm200_02 WITH FORM cl_ap_formpath("aec","aecm200_02")
   
   #瀏覽頁簽資料初始化
   CALL cl_ui_init()
   
   #程式初始化
   CALL aecm200_02_init()   
 
   #進入選單 Menu (="N")
   CALL aecm200_02_ui_dialog() 
 
   #畫面關閉
   CLOSE WINDOW w_aecm200_02
 
   CLOSE aecm200_02_cl
   
   
 
   #add-point:離開前
   {<point name="main.exit" />}
   #end add-point
 
END FUNCTION
 
 
]]>
  </section>
  <section id="aecm200_02.modify" ver="8" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aecm200_02_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_ecbb_m.ecbb001 IS NULL
   OR g_ecbb_m.ecbb002 IS NULL
   OR g_ecbb_m.ecbb003 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   ERROR ""
  
   LET g_ecbb001_t = g_ecbb_m.ecbb001
   LET g_ecbb002_t = g_ecbb_m.ecbb002
   LET g_ecbb003_t = g_ecbb_m.ecbb003
 
   CALL s_transaction_begin()
   
   OPEN aecm200_02_cl USING g_enterprise, g_site,g_ecbb_m.ecbb001,g_ecbb_m.ecbb002,g_ecbb_m.ecbb003
   IF STATUS THEN
      CALL cl_err("OPEN aecm200_02_cl:", STATUS, 1)
      CLOSE aecm200_02_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aecm200_02_master_referesh USING g_ecbb_m.ecbb001,g_ecbb_m.ecbb002,g_ecbb_m.ecbb003 INTO g_ecbb_m.ecbb001, 
       g_ecbb_m.ecbb002,g_ecbb_m.ecbb003,g_ecbb_m.ecbb004,g_ecbb_m.ecbb005,g_ecbb_m.ecbb004_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_ecbb_m.ecbb001,SQLCA.sqlcode,0)
      CLOSE aecm200_02_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   CALL aecm200_02_show()
   WHILE TRUE
      LET g_ecbb001_t = g_ecbb_m.ecbb001
      LET g_ecbb002_t = g_ecbb_m.ecbb002
      LET g_ecbb003_t = g_ecbb_m.ecbb003
 
      
      #寫入修改者/修改日期資訊(單頭)
      
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      CALL aecm200_02_input("u")
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_ecbb_m.* = g_ecbb_m_t.*
         CALL aecm200_02_show()
         CALL cl_err('',9001,0)
         CALL s_transaction_end('N','0')
         EXIT WHILE
      END IF 
      
      #若有modid跟moddt則進行update
 
                  
      #若單頭key欄位有變更
      IF g_ecbb_m.ecbb001 != g_ecbb001_t 
      OR g_ecbb_m.ecbb002 != g_ecbb002_t 
      OR g_ecbb_m.ecbb003 != g_ecbb003_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE ecbf_t SET ecbf001 = g_ecbb_m.ecbb001
                                       ,ecbf002 = g_ecbb_m.ecbb002
                                       ,ecbf003 = g_ecbb_m.ecbb003
 
          WHERE ecbfent = g_enterprise AND ecbfsite = g_site AND ecbf001 = g_ecbb001_t
            AND ecbf002 = g_ecbb002_t
            AND ecbf003 = g_ecbb003_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("ecbf_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("ecbf_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_ecbb_m.ecbb001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE aecm200_02_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_ecbb_m.ecbb001,'U')
 
END FUNCTION   
]]>
  </section>
