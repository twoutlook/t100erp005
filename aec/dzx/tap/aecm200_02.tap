  <section id="aecm200_02.b_fill" ver="8" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aecm200_02_b_fill()
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_ecbf_d.clear()    #g_ecbf_d 單頭及單身 
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF aecm200_02_fill_chk(1) THEN
   
      LET g_sql = "SELECT  UNIQUE ecbfseq,ecbf004,ecbf005,ecbf006,ecbf007,ecbf008,ecbf009,ecbf010 ,t1.oocql004 FROM ecbf_t", 
             
                  " INNER JOIN ecbb_t ON ecbb001 = ecbf001 ",
                  " AND ecbb002 = ecbf002 ",
                  " AND ecbb003 = ecbf003 ",
 
                  #"",
                  
                  "",
                                 " LEFT JOIN oocql_t t1 ON t1.oocqlent='"||g_enterprise||"' AND t1.oocql002=ecbf005 AND t1.oocql003='"||g_dlang||"' ",
 
                  " WHERE"
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      #add-point:b_fill段sql_before
      {<point name="b_fill.body.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table1) THEN
         LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
      END IF
      
      #子單身的WC
      
      
      LET g_sql = g_sql, " ORDER BY ecbf_t.ecbfseq"
      
      #add-point:單身填充控制
      {<point name="b_fill.sql"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE aecm200_02_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR aecm200_02_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise, g_site,g_ecbb_m.ecbb001,g_ecbb_m.ecbb002,g_ecbb_m.ecbb003
                                               
      FOREACH b_fill_cs INTO g_ecbf_d[l_ac].ecbfseq,g_ecbf_d[l_ac].ecbf004,g_ecbf_d[l_ac].ecbf005,g_ecbf_d[l_ac].ecbf006, 
          g_ecbf_d[l_ac].ecbf007,g_ecbf_d[l_ac].ecbf008,g_ecbf_d[l_ac].ecbf009,g_ecbf_d[l_ac].ecbf010, 
          g_ecbf_d[l_ac].ecbf005_desc
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               CALL cl_err( '', 9035, 1)
            END IF
            EXIT FOREACH
         END IF
         
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
 
   
   #add-point:browser_fill段其他table處理
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_ecbf_d.deleteElement(g_ecbf_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE aecm200_02_pb
 
   
END FUNCTION
]]>
  </section>
