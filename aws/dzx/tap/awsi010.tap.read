  <section id="awsi010.modify" ver="6" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION awsi010_modify()
   DEFINE  l_cmd                  LIKE type_t.chr1
   DEFINE  l_ac_t                 LIKE type_t.num5                #未取消的ARRAY CNT 
   DEFINE  l_n                    LIKE type_t.num5                #檢查重複用  
   DEFINE  l_cnt                  LIKE type_t.num5                #檢查重複用  
   DEFINE  l_lock_sw              LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert         LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete         LIKE type_t.num5                #可刪除否  
   DEFINE  l_count                LIKE type_t.num5
   DEFINE  l_i                    LIKE type_t.num5
   DEFINE  ls_return              STRING
   DEFINE  l_var_keys             DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys           DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                 DYNAMIC ARRAY OF STRING
   DEFINE  l_fields               DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak         DYNAMIC ARRAY OF STRING
   DEFINE  li_reproduce           LIKE type_t.num5
   DEFINE  li_reproduce_target    LIKE type_t.num5
   DEFINE  lb_reproduce           BOOLEAN
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point 
   
   LET g_action_choice = ""
   
   LET g_qryparam.state = "i"
 
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   
   #add-point:modify開始前
   {<point name="modify.define_sql" mark="Y"/>}
   #end add-point
   
   LET INT_FLAG = FALSE
   LET lb_reproduce = FALSE
 
   #add-point:modify段修改前
   {<point name="modify.before_input"/>}
   #end add-point
 
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #Page1 預設值產生於此處
      INPUT ARRAY g_wsae_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_detail_cnt,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_wsae_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL awsi010_b_fill(g_wc2)
            LET g_detail_cnt = g_wsae_d.getLength()
         
         BEFORE ROW
            #add-point:modify段before row
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_cmd = ''
            LET l_ac = g_detail_idx
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            DISPLAY g_wsae_d.getLength() TO FORMONLY.cnt
         
            CALL s_transaction_begin()
            LET g_detail_cnt = g_wsae_d.getLength()
            
            IF g_detail_cnt >= l_ac 
               AND g_wsae_d[l_ac].wsae001 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_wsae_d_t.* = g_wsae_d[l_ac].*  #BACKUP
               IF NOT awsi010_lock_b("wsae_t") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH awsi010_bcl INTO g_wsae_d[l_ac].wsaestus,g_wsae_d[l_ac].wsae002,g_wsae_d[l_ac].wsae003, 
                      g_wsae_d[l_ac].wsae001,g_wsae_d[l_ac].wsaeownid,g_wsae_d[l_ac].wsaeowndp,g_wsae_d[l_ac].wsaecrtid, 
                      g_wsae_d[l_ac].wsaecrtdp,g_wsae_d[l_ac].wsaecrtdt,g_wsae_d[l_ac].wsaemodid,g_wsae_d[l_ac].wsaemoddt 

                  IF SQLCA.sqlcode THEN
                     CALL cl_err(g_wsae_d_t.wsae001,SQLCA.sqlcode,1)
                     LET l_lock_sw = "Y"
                  END IF
                  
                  CALL awsi010_detail_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
        
         BEFORE INSERT
            
            CALL s_transaction_begin()
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_wsae_d_t.* TO NULL
            INITIALIZE g_wsae_d[l_ac].* TO NULL 
            #公用欄位給值(單身)
            #此段落由子樣板a14產生    
      LET g_wsae_d[l_ac].wsaeownid = g_user
      LET g_wsae_d[l_ac].wsaeowndp = g_dept
      LET g_wsae_d[l_ac].wsaecrtid = g_user
      LET g_wsae_d[l_ac].wsaecrtdp = g_dept 
      LET g_wsae_d[l_ac].wsaecrtdt = cl_get_current()
      LET g_wsae_d[l_ac].wsaemodid = ""
      LET g_wsae_d[l_ac].wsaemoddt = ""
      LET g_wsae_d[l_ac].wsaestus = ""
 
 
            
            LET g_wsae_d_t.* = g_wsae_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL awsi010_set_entry_b("a")
            CALL awsi010_set_no_entry_b("a")
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_wsae_d[li_reproduce_target].* = g_wsae_d[li_reproduce].*
 
               LET g_wsae_d[g_wsae_d.getLength()].wsae001 = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM wsae_t 
             WHERE wsaeent = g_enterprise AND wsae001 = g_wsae_d[l_ac].wsae001
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前
               {<point name="input.body.b_insert"/>}
               #end add-point
            
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_wsae_d[g_detail_idx].wsae001
               CALL awsi010_insert_b('wsae_t',gs_keys,"'1'")
                           
               #add-point:單身新增後
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_wsae_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("wsae_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL awsi010_b_fill(g_wc2)
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert2"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR 'INSERT O.K'
               LET g_detail_cnt = g_detail_cnt + 1
               
               LET g_wc2 = g_wc2, " OR (wsae001 = '", g_wsae_d[l_ac].wsae001, "' "
 
                                  ,")"
            END IF                
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_wsae_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_wsae_d.deleteElement(l_ac)
               NEXT FIELD wsae001
            END IF
            IF g_wsae_d[l_ac].wsae001 IS NOT NULL
 
               THEN     
            
               #add-point:單身刪除ask前
               {<point name="input.body.b_delete_ask"/>}
               #end add-point   
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete" mark="Y"/>}
               #end add-point   
               
               DELETE FROM wsae_t
                WHERE wsaeent = g_enterprise AND 
                      wsae001 = g_wsae_d_t.wsae001
 
                      
               #add-point:單身刪除中
               {<point name="input.body.m_delete"/>}
               #end add-point  
                      
               IF SQLCA.sqlcode THEN
                  CALL cl_err("wsae_t",SQLCA.sqlcode,1)
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               ELSE
                  LET g_detail_cnt = g_detail_cnt-1
                  
                  #add-point:單身刪除後
                  {<point name="input.body.a_delete"/>}
                  #end add-point
                  CALL s_transaction_end('Y','0')
               END IF 
               CLOSE awsi010_bcl
               LET l_count = g_wsae_d.getLength()
            END IF 
                           INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_wsae_d[g_detail_idx].wsae001
 
              
         AFTER DELETE 
            #add-point:單身刪除後2
            {<point name="input.body.after_delete"/>}
            #end add-point
                           CALL awsi010_delete_b('wsae_t',gs_keys,"'1'")
 
                  #此段落由子樣板a01產生
         BEFORE FIELD wsaestus
            #add-point:BEFORE FIELD wsaestus
            {<point name="input.b.page1.wsaestus" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD wsaestus
            
            #add-point:AFTER FIELD wsaestus
            {<point name="input.a.page1.wsaestus" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE wsaestus
            #add-point:ON CHANGE wsaestus
            {<point name="input.g.page1.wsaestus" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD wsae002
            #add-point:BEFORE FIELD wsae002
            {<point name="input.b.page1.wsae002" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD wsae002
            
            #add-point:AFTER FIELD wsae002
            {<point name="input.a.page1.wsae002" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE wsae002
            #add-point:ON CHANGE wsae002
            {<point name="input.g.page1.wsae002" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD wsae003
            #add-point:BEFORE FIELD wsae003
            {<point name="input.b.page1.wsae003" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD wsae003
            
            #add-point:AFTER FIELD wsae003
            {<point name="input.a.page1.wsae003" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE wsae003
            #add-point:ON CHANGE wsae003
            {<point name="input.g.page1.wsae003" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD wsae001
            
            #add-point:AFTER FIELD wsae001
            {<point name="input.a.page1.wsae001" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD wsae001
            #add-point:BEFORE FIELD wsae001
            {<point name="input.b.page1.wsae001" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE wsae001
            #add-point:ON CHANGE wsae001
            {<point name="input.g.page1.wsae001" />}
            #END add-point
 
 
                  #Ctrlp:input.c.page1.wsaestus
         ON ACTION controlp INFIELD wsaestus
            #add-point:ON ACTION controlp INFIELD wsaestus
            {<point name="input.c.page1.wsaestus" />}
            #END add-point
 
         #Ctrlp:input.c.page1.wsae002
         ON ACTION controlp INFIELD wsae002
            #add-point:ON ACTION controlp INFIELD wsae002
            {<point name="input.c.page1.wsae002" />}
            #END add-point
 
         #Ctrlp:input.c.page1.wsae003
         ON ACTION controlp INFIELD wsae003
            #add-point:ON ACTION controlp INFIELD wsae003
            {<point name="input.c.page1.wsae003" />}
            #END add-point
 
         #Ctrlp:input.c.page1.wsae001
         ON ACTION controlp INFIELD wsae001
            #add-point:ON ACTION controlp INFIELD wsae001
            {<point name="input.c.page1.wsae001" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_wsae_d[l_ac].* = g_wsae_d_t.*
               CLOSE awsi010_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_wsae_d[l_ac].wsae001,-263,1)
               LET g_wsae_d[l_ac].* = g_wsae_d_t.*
            ELSE
               
               #寫入修改者/修改日期資訊(單身)
               LET g_wsae_d[l_ac].wsaemodid = g_user 
LET g_wsae_d[l_ac].wsaemoddt = cl_get_current()
 
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               UPDATE wsae_t SET (wsaestus,wsae002,wsae003,wsae001,wsaeownid,wsaeowndp,wsaecrtid,wsaecrtdp, 
                   wsaecrtdt,wsaemodid,wsaemoddt) = (g_wsae_d[l_ac].wsaestus,g_wsae_d[l_ac].wsae002, 
                   g_wsae_d[l_ac].wsae003,g_wsae_d[l_ac].wsae001,g_wsae_d[l_ac].wsaeownid,g_wsae_d[l_ac].wsaeowndp, 
                   g_wsae_d[l_ac].wsaecrtid,g_wsae_d[l_ac].wsaecrtdp,g_wsae_d[l_ac].wsaecrtdt,g_wsae_d[l_ac].wsaemodid, 
                   g_wsae_d[l_ac].wsaemoddt)
                WHERE wsaeent = g_enterprise AND
                  wsae001 = g_wsae_d_t.wsae001 #項次   
 
                  
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
                  
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("wsae_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                    WHEN SQLCA.sqlcode #其他錯誤
                     CALL cl_err("wsae_t",SQLCA.sqlcode,1)  
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_wsae_d[g_detail_idx].wsae001
               LET gs_keys_bak[1] = g_wsae_d_t.wsae001
               CALL awsi010_update_b('wsae_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
                     LET g_log1 = util.JSON.stringify(g_wsae_d_t)
                     LET g_log2 = util.JSON.stringify(g_wsae_d[l_ac])
                     IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                        CALL s_transaction_end('N','0')
                     END IF
               END CASE
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            CALL awsi010_unlock_b("wsae_t")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock
            
             #add-point:單身after row
            {<point name="input.body.a_row"/>}
            #end add-point
            
         AFTER INPUT
            #add-point:單身input後
            {<point name="input.body.a_input"/>}
            #end add-point
            
         ON ACTION controlo   
            CALL FGL_SET_ARR_CURR(g_wsae_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_wsae_d.getLength()+1
            
      END INPUT
      
 
      
 
      
      #add-point:before_more_input
      {<point name="input.more_input"/>}
      #end add-point
      
      BEFORE DIALOG 
         IF g_temp_idx > 0 THEN
            LET l_ac = g_temp_idx
            CALL DIALOG.setCurrentRow("s_detail1",l_ac)
            LET g_temp_idx = 1
         END IF
         LET g_curr_diag = ui.DIALOG.getCurrent()
         #add-point:before_dialog
         {<point name="input.before_dialog"/>}
         #end add-point
         CASE g_aw
            WHEN "s_detail1"
               NEXT FIELD wsaestus
 
         END CASE
   
      ON ACTION accept
         ACCEPT DIALOG
      
      ON ACTION cancel
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION controlr
         CALL cl_show_req_fields()
 
      ON ACTION controlf
         CALL cl_set_focus_form(ui.Interface.getRootNode()) 
              RETURNING g_fld_name,g_frm_name 
         CALL cl_fldhelp(g_frm_name,g_fld_name,g_lang) 
           
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE DIALOG
   
   END DIALOG 
   
   #add-point:modify段修改後
   {<point name="modify.after_input"/>}
   #end add-point
 
   CLOSE awsi010_bcl
   CALL s_transaction_end('Y','0')
   
END FUNCTION
]]>
