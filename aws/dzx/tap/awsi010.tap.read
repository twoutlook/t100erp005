  <section id="awsi010.b_fill" ver="5" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION awsi010_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2    STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE wsaestus,wsae002,wsae003,wsae001,wsaeownid,wsaeowndp,wsaecrtid,wsaecrtdp, 
       wsaecrtdt,wsaemodid,wsaemoddt ,t1.ooefl003 ,t2.oofa011 ,t3.ooefl003 ,t4.oofa011 ,t5.ooefl003 , 
       t6.oofa011 FROM wsae_t",
               "",
                              " LEFT JOIN ooefl_t t1 ON t1.ooeflent='"||g_enterprise||"' AND t1.ooefl001=wsae001 AND t1.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t2 ON t2.oofaent='"||g_enterprise||"' AND t2.oofa002='2' AND t2.oofa003=wsaeownid  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent='"||g_enterprise||"' AND t3.ooefl001=wsaeowndp AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t4 ON t4.oofaent='"||g_enterprise||"' AND t4.oofa002='2' AND t4.oofa003=wsaecrtid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=wsaecrtdp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t6 ON t6.oofaent='"||g_enterprise||"' AND t6.oofa002='2' AND t6.oofa003=wsaemodid  ",
 
               " WHERE wsaeent= ? AND 1=1 AND ", p_wc2 
    
   LET g_sql = g_sql, cl_sql_add_filter("wsae_t"),
                      " ORDER BY wsae_t.wsae001"
   
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE awsi010_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR awsi010_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_wsae_d.clear()
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_wsae_d[l_ac].wsaestus,g_wsae_d[l_ac].wsae002,g_wsae_d[l_ac].wsae003,g_wsae_d[l_ac].wsae001, 
       g_wsae_d[l_ac].wsaeownid,g_wsae_d[l_ac].wsaeowndp,g_wsae_d[l_ac].wsaecrtid,g_wsae_d[l_ac].wsaecrtdp, 
       g_wsae_d[l_ac].wsaecrtdt,g_wsae_d[l_ac].wsaemodid,g_wsae_d[l_ac].wsaemoddt,g_wsae_d[l_ac].wsae001_desc, 
       g_wsae_d[l_ac].wsaeownid_desc,g_wsae_d[l_ac].wsaeowndp_desc,g_wsae_d[l_ac].wsaecrtid_desc,g_wsae_d[l_ac].wsaecrtdp_desc, 
       g_wsae_d[l_ac].wsaemodid_desc
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL awsi010_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec THEN
      IF g_error_show = 1 THEN
         CALL cl_err("wsae_t",9035,1)
      END IF
   END IF 
   LET g_error_show = 0
   
 
   
   CALL g_wsae_d.deleteElement(g_wsae_d.getLength())   
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_wsae_d.getLength()
 
   END FOR
   
   IF g_cnt > g_wsae_d.getLength() THEN
      LET l_ac = g_wsae_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_wsae_d.getLength()
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE awsi010_pb
   
END FUNCTION
]]>
  </section>
  <section id="awsi010.default_search" ver="4" status="" src="s">
    <![CDATA[#+ 外部參數搜尋
PRIVATE FUNCTION awsi010_default_search()
   DEFINE li_idx  LIKE type_t.num5
   DEFINE li_cnt  LIKE type_t.num5
