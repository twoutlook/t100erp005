  <section id="awsp900_03.delete" ver="1" status="" src="s">
<![CDATA[#+ 資料刪除 
PRIVATE FUNCTION awsp900_03_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point  
   
   IF g_wsaa_m.wsaa001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   CALL awsp900_03_show()
   
   CALL s_transaction_begin()
    
   LET g_wsaa001_t = g_wsaa_m.wsaa001
 
   
   
 
   OPEN awsp900_03_cl USING 
                           g_wsaa_m.wsaa001
 
   IF STATUS THEN
      CALL cl_err("OPEN awsp900_03_cl:", STATUS, 0)
      CLOSE awsp900_03_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE awsp900_03_master_referesh USING g_wsaa_m.wsaa001 INTO g_wsaa_m.wsaa001
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_wsaa_m.wsaa001,SQLCA.sqlcode,0)
      RETURN
   END IF
   
   IF cl_ask_delete() THEN
 
      #add-point:單頭刪除前
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point
 
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL awsp900_03_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
      DELETE FROM wsaa_t 
       WHERE  wsaa001 = g_wsaa_m.wsaa001 
 
 
      #add-point:單頭刪除中
      {<point name="delete.head.m_delete"/>}
      #end add-point
         
      IF SQLCA.sqlcode THEN
         CALL cl_err("wsaa_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
  
      
      
      #add-point:單頭刪除後
      {<point name="delete.head.a_delete"/>}
      #end add-point
      
          
      CLEAR FORM
      CALL awsp900_03_ui_browser_refresh()
      IF g_browser_cnt > 0 THEN
         CALL awsp900_03_browser_fill(g_wc,"")
         CALL awsp900_03_fetch("P")
      ELSE
         CALL awsp900_03_browser_fill(" 1=1 ","F")
      END IF
      
   END IF
 
   CLOSE awsp900_03_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_wsaa_m.wsaa001,"D")
 
END FUNCTION
]]>
</section>
