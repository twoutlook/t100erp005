                  ELSE
                     CALL awsp900_03_fetch("F")
                  END IF
               END IF
               #重新搜尋會將notice區塊清空,此處不可用EXIT DIALOG, SUBDIALOG重讀會導致部分變數消失
               CALL cl_notice()


         #+ 此段落由子樣板a43產生
