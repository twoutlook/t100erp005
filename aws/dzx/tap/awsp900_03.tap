  <section id="awsp900_03.modify" ver="1" status="" src="s">
<![CDATA[#+ 資料修改
PRIVATE FUNCTION awsp900_03_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point
   
   IF g_wsaa_m.wsaa001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF 
 
   ERROR ""
  
   LET g_wsaa001_t = g_wsaa_m.wsaa001
 
   
   CALL s_transaction_begin()
   
   OPEN awsp900_03_cl USING g_wsaa_m.wsaa001
   IF STATUS THEN
      CALL cl_err("OPEN awsp900_03_cl:", STATUS, 1)
      CLOSE awsp900_03_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE awsp900_03_master_referesh USING g_wsaa_m.wsaa001 INTO g_wsaa_m.wsaa001
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err("wsaa_t",SQLCA.sqlcode,0)
      CLOSE awsp900_03_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
 
   CALL awsp900_03_show()
   
   WHILE TRUE
      LET g_wsaa_m.wsaa001 = g_wsaa001_t
 
      
      #寫入修改者/修改日期資訊
      
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
 
      CALL awsp900_03_input("u")     #欄位更改
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_wsaa_m.* = g_wsaa_m_t.*
         CALL awsp900_03_show()
         CALL cl_err("",9001,0)
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
 
 
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_wsaa_m.wsaa001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE awsp900_03_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_wsaa_m.wsaa001,"U")
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
]]>
</section>
