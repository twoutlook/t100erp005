         ON ACTION controlp INFIELD wsaa002
            #add-point:ON ACTION controlp INFIELD wsaa002
            {<point name="input.c.page1.wsaa002" />}
            LET g_qryparam.reqry = FALSE
            CALL awsi001_01()
            LET g_wsaa_d[l_ac].wsaa002 = g_qryparam.return1
            LET g_wsaa_d[l_ac].wsaa002_desc = g_qryparam.return2 

            DISPLAY BY NAME g_wsaa_d[l_ac].wsaa002, g_wsaa_d[l_ac].wsaa002_desc
            LET g_wsaa_d[l_ac].wsaa002_img_template = "/u1/t10dap/efgp/", g_wsaa_d[l_ac].wsaa002 CLIPPED, ".png"   #FlowTemplateLvl3.png"   
            DISPLAY BY NAME g_wsaa_d[l_ac].wsaa002_img_template
            
            LET g_wsaa002_img = g_wsaa_d[l_ac].wsaa002_img_template
            DISPLAY g_wsaa002_img TO wsaa002_img
            #END add-point

         #----<<wsaa003>>----
         #Ctrlp:input.c.page1.wsaa003
#         ON ACTION controlp INFIELD wsaa003
            #add-point:ON ACTION controlp INFIELD wsaa003
            {<point name="input.c.page1.wsaa003" />}
            #END add-point

         #----<<wsaaownid>>----
         #----<<wsaaowndp>>----
         #----<<wsaacrtid>>----
         #----<<wsaacrtdp>>----
         #----<<wsaacrtdt>>----
         #----<<wsaamodid>>----
         #----<<wsaamoddt>>----


         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_wsaa_d[l_ac].* = g_wsaa_d_t.*
               CLOSE awsi002_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG
            END IF

            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_wsaa_d[l_ac].wsaa001,-263,1)
               LET g_wsaa_d[l_ac].* = g_wsaa_d_t.*
            ELSE

               #寫入修改者/修改日期資訊(單身)
               LET g_wsaa_d[l_ac].wsaamodid = g_user
               LET g_wsaa_d[l_ac].wsaamoddt = cl_get_current()


               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point

               UPDATE wsaa_t SET (wsaa001,wsaa002,wsaa003,wsaaownid,wsaaowndp,wsaacrtid,wsaacrtdp,wsaacrtdt,wsaamodid,wsaamoddt) = (g_wsaa_d[l_ac].wsaa001,g_wsaa_d[l_ac].wsaa002,g_wsaa_d[l_ac].wsaa003,g_wsaa_d[l_ac].wsaaownid,g_wsaa_d[l_ac].wsaaowndp,g_wsaa_d[l_ac].wsaacrtid,g_wsaa_d[l_ac].wsaacrtdp,g_wsaa_d[l_ac].wsaacrtdt,g_wsaa_d[l_ac].wsaamodid,g_wsaa_d[l_ac].wsaamoddt)
                 WHERE wsaa001 = g_wsaa_d_t.wsaa001 #項次


               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point

               IF SQLCA.sqlcode THEN
                  CALL cl_err("wsaa_t",SQLCA.sqlcode,1)
                  LET g_wsaa_d[l_ac].* = g_wsaa_d_t.*
               ELSE
                  INITIALIZE gs_keys TO NULL
                  LET gs_keys[1] = g_wsaa_d[g_detail_idx].wsaa001
                  LET gs_keys_bak[1] = g_wsaa_d_t.wsaa001
                  CALL awsi002_update_b('wsaa_t',gs_keys,gs_keys_bak,"'1'")
                  #資料多語言用-增/改

               END IF

               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point

            END IF

         AFTER ROW
            CALL awsi002_unlock_b("wsaa_t")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock


         AFTER INPUT
            #add-point:單身input後
            {<point name="input.body.a_input"/>}
            #end add-point

      END INPUT





      BEFORE DIALOG
         IF g_temp_idx > 0 THEN
            LET l_ac = g_temp_idx
            CALL DIALOG.setCurrentRow("s_detail1",l_ac)
            LET g_temp_idx = 1
         END IF
         LET g_curr_diag = ui.DIALOG.getCurrent()
         #add-point:before_dialog
         {<point name="input.before_dialog"/>}
         #end add-point

      ON ACTION accept
         ACCEPT DIALOG

      ON ACTION cancel
         LET INT_FLAG = TRUE
         EXIT DIALOG

      ON ACTION controlr
         CALL cl_show_req_fields()

      ON ACTION controlf
         CALL cl_set_focus_form(ui.Interface.getRootNode())
              RETURNING g_fld_name,g_frm_name
         CALL cl_fldhelp(g_frm_name,g_fld_name,g_lang)

      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE DIALOG

   END DIALOG

   #add-point:modify段修改後
   {<point name="modify.after_input"/>}
   #end add-point

   CLOSE awsi002_bcl
   CALL s_transaction_end('Y','0')

END FUNCTION]]>
</point>
  <point name="function.awsi002_b_fill" cite_std="N" status="u" ver="1" src="s" new="Y" order="7" cite_ver="" mark_hard="N">
<![CDATA[
PRIVATE FUNCTION awsi002_b_fill(p_wc2)
              #BODY FILL UP
   {<Local define>}
   DEFINE p_wc2           STRING
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point

   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF

   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point

   LET g_sql = "SELECT UNIQUE wsaa003, '', wsaa001, '', wsaa002, '', '', ",
               "              wsaaownid, '', wsaaowndp, '', wsaacrtid, '', wsaacrtdp, '', wsaacrtdt, wsaamodid, '', wsaamoddt ",
               "  FROM wsaa_t",
               " WHERE 1=1 AND ", p_wc2

   LET g_sql = g_sql, " ORDER BY wsaa_t.wsaa003, wsaa_t.wsaa001"

   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point

   PREPARE awsi002_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR awsi002_pb



   CALL g_wsaa_d.clear()


   LET g_cnt = l_ac
   LET l_ac = 1
   ERROR "Searching!"

   FOREACH b_fill_curs INTO g_wsaa_d[l_ac].wsaa003, g_wsaa_d[l_ac].wsaa003_desc, g_wsaa_d[l_ac].wsaa001, g_wsaa_d[l_ac].wsaa001_desc, g_wsaa_d[l_ac].wsaa002, g_wsaa_d[l_ac].wsaa002_desc, g_wsaa_d[l_ac].wsaa002_img_template,
                            g_wsaa_d[l_ac].wsaaownid, g_wsaa_d[l_ac].wsaaownid_desc, g_wsaa_d[l_ac].wsaaowndp, g_wsaa_d[l_ac].wsaaowndp_desc, g_wsaa_d[l_ac].wsaacrtid, g_wsaa_d[l_ac].wsaacrtid_desc, g_wsaa_d[l_ac].wsaacrtdp, g_wsaa_d[l_ac].wsaacrtdp_desc, g_wsaa_d[l_ac].wsaacrtdt, g_wsaa_d[l_ac].wsaamodid, g_wsaa_d[l_ac].wsaamodid_desc, g_wsaa_d[l_ac].wsaamoddt
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF

      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point

      CALL awsi002_detail_show()

      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF

   END FOREACH

   IF l_ac > g_max_rec AND g_error_show = 1 THEN
      CALL cl_err("wsaa_t",9035,1)
   END IF
   LET g_error_show = 0




   CALL g_wsaa_d.deleteElement(g_wsaa_d.getLength())


   #將key欄位填到每個page
   FOR l_ac = 1 TO g_wsaa_d.getLength()

   END FOR

   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point

   LET g_detail_cnt = l_ac - 1
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   LET l_ac = g_cnt
   LET g_cnt = 0

   CLOSE b_fill_curs
   FREE awsi002_pb

END FUNCTION]]>
</point>
