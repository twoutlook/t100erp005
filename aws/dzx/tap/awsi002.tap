                  DISPLAY BY NAME g_wsaa_d[l_ac].wsaa002_img_template
                  
                  LET g_wsaa002_img = g_wsaa_d[l_ac].wsaa002_img_template
                  DISPLAY g_wsaa002_img TO wsaa002_img
               END IF
            END IF
            #END add-point


         #此段落由子樣板a04產生
         ON CHANGE wsaa002
            #add-point:ON CHANGE wsaa002
            {<point name="input.g.page1.wsaa002" />}
            #END add-point

         #----<<wsaa003>>----
         #此段落由子樣板a02產生
         AFTER FIELD wsaa003

            #add-point:AFTER FIELD wsaa003

