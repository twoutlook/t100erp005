<![CDATA[
PRIVATE FUNCTION awsi002_modify()
   {<Local define>}
   DEFINE  l_cmd           LIKE type_t.chr1
   DEFINE  l_ac_t          LIKE type_t.num5                #未取消的ARRAY CNT
   DEFINE  l_n             LIKE type_t.num5                #檢查重複用
   DEFINE  l_cnt           LIKE type_t.num5                #檢查重複用
   DEFINE  l_lock_sw       LIKE type_t.chr1                #單身鎖住否
   DEFINE  l_allow_insert  LIKE type_t.num5                #可新增否
   DEFINE  l_allow_delete  LIKE type_t.num5                #可刪除否
   DEFINE  l_count         LIKE type_t.num5
   DEFINE  l_i             LIKE type_t.num5
   DEFINE  ls_return       STRING
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   {</Local define>}
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point

   LET g_action_choice = ""

   LET g_qryparam.state = "i"

   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")

   #add-point:modify段define_sql
   {<point name="modify.define_sql" mark="Y"/>}
   #end add-point
   LET g_forupd_sql = "SELECT wsaa003, '', wsaa001, '', wsaa002, '', '', ",
                      "       wsaaownid, '', wsaaowndp, '', wsaacrtid, '', wsaacrtdp, '', wsaacrtdt, wsaamodid, '', wsaamoddt ",
                      "  FROM wsaa_t",
                      "  WHERE wsaa001=? FOR UPDATE"
                      
   #add-point:modify段define_sql
   {<point name="modify.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   DECLARE awsi002_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR



   LET INT_FLAG = FALSE

   #add-point:modify段修改前
   {<point name="modify.before_input"/>}
   #end add-point

   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)

      #Page1 預設值產生於此處
      INPUT ARRAY g_wsaa_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_detail_cnt,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS,
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)

         #自訂ACTION(detail_input,page_1)


         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN
              CALL FGL_SET_ARR_CURR(g_wsaa_d.getLength()+1)
              LET g_insert = 'N'
           END IF

            CALL awsi002_b_fill(g_wc2)
            LET g_detail_cnt = g_wsaa_d.getLength()

         BEFORE ROW
            #add-point:modify段before row
            {<point name="input.body.before_row2"/>}
            LET g_wsaa002_img = ""
            #end add-point
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx

            CALL s_transaction_begin()
            LET g_detail_cnt = g_wsaa_d.getLength()

            IF g_detail_cnt >= l_ac AND NOT cl_null(g_wsaa_d[l_ac].wsaa001) THEN
               LET l_cmd='u'
               LET g_wsaa_d_t.* = g_wsaa_d[l_ac].*  #BACKUP
               IF NOT awsi002_lock_b("wsaa_t") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH awsi002_bcl INTO g_wsaa_d[l_ac].wsaa003, g_wsaa_d[l_ac].wsaa003_desc, g_wsaa_d[l_ac].wsaa001, g_wsaa_d[l_ac].wsaa001_desc, g_wsaa_d[l_ac].wsaa002, g_wsaa_d[l_ac].wsaa002_desc, g_wsaa_d[l_ac].wsaa002_img_template,
                                         g_wsaa_d[l_ac].wsaaownid, g_wsaa_d[l_ac].wsaaownid_desc, g_wsaa_d[l_ac].wsaaowndp, g_wsaa_d[l_ac].wsaaowndp_desc, g_wsaa_d[l_ac].wsaacrtid, g_wsaa_d[l_ac].wsaacrtid_desc, g_wsaa_d[l_ac].wsaacrtdp, g_wsaa_d[l_ac].wsaacrtdp_desc, g_wsaa_d[l_ac].wsaacrtdt, g_wsaa_d[l_ac].wsaamodid, g_wsaa_d[l_ac].wsaamodid_desc, g_wsaa_d[l_ac].wsaamoddt
              
                  IF SQLCA.sqlcode THEN
                     CALL cl_err(g_wsaa_d_t.wsaa001,SQLCA.sqlcode,1)
                     LET l_lock_sw = "Y"
                  END IF

                  CALL awsi002_detail_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point
            #其他table資料備份(確定是否更改用)

            #其他table進行lock


         BEFORE INSERT

            CALL s_transaction_begin()
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_wsaa_d_t.* TO NULL
            INITIALIZE g_wsaa_d[l_ac].* TO NULL

            LET g_wsaa_d_t.* = g_wsaa_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL awsi002_set_entry_b("a")
            CALL awsi002_set_no_entry_b("a")
            
            #公用欄位給值(單身)
            #此段落由子樣板a14產生
            LET g_wsaa_d[l_ac].wsaaownid = g_user
            LET g_wsaa_d[l_ac].wsaaowndp = g_dept
            LET g_wsaa_d[l_ac].wsaacrtid = g_user
            LET g_wsaa_d[l_ac].wsaacrtdp = g_dept
            LET g_wsaa_d[l_ac].wsaacrtdt = cl_get_current()
            #LET g_wsaa_d[l_ac].wsaamodid = g_user
            #LET g_wsaa_d[l_ac].wsaamoddt = cl_get_current()
            ##LET g_wsaa_d[l_ac].wsaastus = "Y"



            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point

         AFTER INSERT
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF

            LET l_count = 1
            SELECT COUNT(*) INTO l_count FROM wsaa_t
              WHERE  wsaa001 = g_wsaa_d[l_ac].wsaa001


            #資料未重複, 插入新增資料
            IF l_count = 0 THEN
               #add-point:單身新增前
               {<point name="input.body.b_insert"/>}
               #end add-point

               INITIALIZE gs_keys TO NULL
               LET gs_keys[1] = g_wsaa_d[g_detail_idx].wsaa001
               CALL awsi002_insert_b('wsaa_t',gs_keys,"'1'")

               #add-point:單身新增後
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_wsaa_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF

            IF SQLCA.SQLcode  THEN
               CALL cl_err("wsaa_t",SQLCA.sqlcode,1)
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL awsi002_b_fill(g_wc2)
               #資料多語言用-增/改

               #add-point:input段-after_insert
               {<point name="input.body.a_insert2"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR 'INSERT O.K'
               LET g_detail_cnt = g_detail_cnt + 1
            END IF

         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_wsaa_d.deleteElement(l_ac)
               NEXT FIELD wsaa001
            END IF
            
            IF NOT cl_null(g_wsaa_d[l_ac].wsaa001) THEN
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF

               #add-point:單身刪除前
               {<point name="input.body.b_delete" mark="Y"/>}
               #end add-point

               DELETE FROM wsaa_t
                 WHERE wsaa001 = g_wsaa_d_t.wsaa001


               #add-point:單身刪除中
               {<point name="input.body.m_delete"/>}
               #end add-point

               IF SQLCA.sqlcode THEN
                  CALL cl_err("wsaa_t",SQLCA.sqlcode,1)
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE
               ELSE
                  LET g_detail_cnt = g_detail_cnt-1

                  #add-point:單身刪除後
                  {<point name="input.body.a_delete"/>}
                  #end add-point
                  CALL s_transaction_end('Y','0')
               END IF
               CLOSE awsi002_bcl
               LET l_count = g_wsaa_d.getLength()
            END IF
            
            INITIALIZE gs_keys TO NULL
            LET gs_keys[1] = g_wsaa_d[g_detail_idx].wsaa001


         AFTER DELETE
            #add-point:單身刪除後2
            {<point name="input.body.after_delete"/>}
            #end add-point
            CALL awsi002_delete_b('wsaa_t',gs_keys,"'1'")

         #---------------------<  Detail: page1  >---------------------
         #----<<wsaa001>>----
         #此段落由子樣板a02產生
         AFTER FIELD wsaa001

            #add-point:AFTER FIELD wsaa001

            #INITIALIZE g_ref_fields TO NULL
            #LET g_ref_fields[1] = g_wsaa_d[l_ac].wsaa001
            #CALL ap_ref_array2(g_ref_fields,"SELECT gzcbl004 FROM gzcbl_t WHERE gzcbl001='24' AND gzcbl002=? AND gzcbl003='"||g_lang||"'","") RETURNING g_rtn_fields
            #LET g_wsaa_d[l_ac].wsaa001_desc = '', g_rtn_fields[1] , ''
            #DISPLAY BY NAME g_wsaa_d[l_ac].wsaa001_desc
            CALL awsi002_wsaa001_desc()

            #此段落由子樣板a05產生
            IF  NOT cl_null(g_wsaa_d[g_detail_idx].wsaa001) THEN
               IF l_cmd = 'a' OR ( l_cmd = 'u' AND (g_wsaa_d[g_detail_idx].wsaa001 != g_wsaa_d_t.wsaa001)) THEN
                  IF NOT ap_chk_notDup("","SELECT COUNT(*) FROM wsaa_t WHERE "||"wsaa001 = '"||g_wsaa_d[g_detail_idx].wsaa001 ||"'",'std-00004',0) THEN
                     NEXT FIELD CURRENT
                  END IF
                  
                  #取得模組代碼
                  SELECT gzza003, gzzal003 INTO g_wsaa_d[l_ac].wsaa003, g_wsaa_d[l_ac].wsaa003_desc
                    FROM gzza_t LEFT JOIN gzzal_t ON gzza001 = gzzal001 AND gzzal002 = g_lang
                    WHERE gzza001 = g_wsaa_d[g_detail_idx].wsaa001
                  
                  DISPLAY BY NAME g_wsaa_d[l_ac].wsaa003, g_wsaa_d[l_ac].wsaa003_desc
               END IF
            END IF

          {#ADP版次:1#}
            #END add-point


         #此段落由子樣板a01產生
         BEFORE FIELD wsaa001
            #add-point:BEFORE FIELD wsaa001
            {<point name="input.b.page1.wsaa001" />}
            #END add-point

         #此段落由子樣板a04產生
         ON CHANGE wsaa001
            #add-point:ON CHANGE wsaa001
            {<point name="input.g.page1.wsaa001" />}
            #END add-point

         #----<<wsaa002>>----
         #此段落由子樣板a01產生
         BEFORE FIELD wsaa002
            #add-point:BEFORE FIELD wsaa002
            {<point name="input.b.page1.wsaa002" />}
            #END add-point

         #此段落由子樣板a02產生
         AFTER FIELD wsaa002

            #add-point:AFTER FIELD wsaa002
            {<point name="input.a.page1.wsaa002" />}
            IF  NOT cl_null(g_wsaa_d[g_detail_idx].wsaa002) THEN
               IF l_cmd = 'a' OR ( l_cmd = 'u' AND (g_wsaa_d[g_detail_idx].wsaa002 != g_wsaa_d_t.wsaa002)) THEN
