<![CDATA[{<Module define>}

#單身 type 宣告
PRIVATE TYPE type_g_wsaa_d RECORD
       wsaa003                LIKE wsaa_t.wsaa003, 
       wsaa003_desc           LIKE gzzol_t.gzzol003,
       wsaa001                LIKE wsaa_t.wsaa001, 
       wsaa001_desc           LIKE gzcbl_t.gzcbl004,
