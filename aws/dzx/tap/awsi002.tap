         ON CHANGE wsaa003
            #add-point:ON CHANGE wsaa003
            {<point name="input.g.page1.wsaa003" />}
            #END add-point

         #----<<wsaaownid>>----
         #----<<wsaaowndp>>----
         #----<<wsaacrtid>>----
         #----<<wsaacrtdp>>----
         #----<<wsaacrtdt>>----
         #----<<wsaamodid>>----
         #----<<wsaamoddt>>----

         #---------------------<  Detail: page1  >---------------------
         #----<<wsaa001>>----
         #Ctrlp:input.c.page1.wsaa001
         ON ACTION controlp INFIELD wsaa001
            #add-point:ON ACTION controlp INFIELD wsaa001
            {<point name="input.c.page1.wsaa001" />}
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'i'
            LET g_qryparam.reqry = FALSE

            LET g_qryparam.default1 = g_wsaa_d[l_ac].wsaa001   #給予default值

            #給予arg
            LET g_qryparam.arg1 = "24" #

            CALL q_gzcb001()                                   #呼叫開窗

            LET g_wsaa_d[l_ac].wsaa001 = g_qryparam.return1    #將開窗取得的值回傳到變數
