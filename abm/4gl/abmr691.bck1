         END IF
      END IF
   END FOR
   LET l_chk = g_argv[01]
   LET g_master.wc = " bmaaent = '",g_enterprise,"' AND ( ",g_master.wc CLIPPED,") "   #151007 polly add
   CALL abmr691_x01(g_master.wc,g_master.l_select1,g_master.l_select2,g_master.l_pr,l_chk,' ')
