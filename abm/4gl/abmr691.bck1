#+ 資料處理   (r類使用g_master為主處理/p類使用l_param為主)
PRIVATE FUNCTION abmr691_process(ls_js)
   DEFINE ls_js         STRING
   DEFINE lc_param      type_parameter
   DEFINE li_stus       LIKE type_t.num5
   DEFINE li_count      LIKE type_t.num10  #progressbar計量
   DEFINE ls_sql        STRING             #主SQL
   DEFINE li_r01_status LIKE type_t.num5
   DEFINE l_token       base.StringTokenizer      #cmdrun使用
   DEFINE ls_next       STRING                    #cmdrun使用
   DEFINE l_cnt         LIKE type_t.num5          #cmdrun使用   
   DEFINE l_arg         DYNAMIC ARRAY OF STRING   ##cmdrun使用的陣列 
   #add-point:process段define name="process.define"
   DEFINE l_ac       LIKE type_t.num10   #161108-00012#1 num5==》num10
   DEFINE l_chk      LIKE type_t.chr10
   #end add-point
   #add-point:process段define (客製用) name="process.define_customerization"
   
   #end add-point
 
   INITIALIZE lc_param TO NULL
   CALL util.JSON.parse(ls_js,lc_param)  #r類作業被t類呼叫時使用, p類主要解開參數處
   LET li_r01_status = 1
 
  #IF lc_param.wc IS NOT NULL THEN
  #   LET g_bgjob = "T"       #特殊情況,此為t類作業鬆耦合串入報表主程式使用
  #END IF
 
   LET g_rep_wcchp = cl_wcchp(g_master.wc,"")  #取得列印條件
  
   #add-point:process段前處理 name="process.pre_process"
   FOR l_ac = 1 TO g_bmaa_d.getLength()
      IF cl_null(g_bmaa_d[l_ac].bmaa002) THEN
         LET g_bmaa_d[l_ac].bmaa002 = ' '
      END IF
      IF NOT cl_null(g_bmaa_d[l_ac].bmaa001) THEN
         LET l_wc[l_ac].wc = " bmaa001 = '",g_bmaa_d[l_ac].bmaa001 CLIPPED,"' AND bmaa002 = '",g_bmaa_d[l_ac].bmaa002 ,"' "
#         IF NOT cl_null(g_bmaa_d[l_ac].bmfa012) THEN
#            LET l_wc[l_ac].wc = l_wc[l_ac].wc ," AND bmfa012 = '",g_bmaa_d[l_ac].bmfa012 CLIPPED,"' " 
#         END IF
         IF NOT cl_null(g_bmaa_d[l_ac].bmfa005) THEN
            LET l_wc[l_ac].wc = l_wc[l_ac].wc," AND (to_char(bmba005,'yyyy-mm-dd hh24:mi:ss') <= '",g_bmaa_d[l_ac].bmfa005 CLIPPED,"' AND (to_char(bmba006,'yyyy-mm-dd hh24:mi:ss') >  '",g_bmaa_d[l_ac].bmfa005 CLIPPED,"' OR to_char(bmba006,'yyyy-mm-dd hh24:mi:ss') IS NULL)) "
         END IF
      END IF   
   END FOR
   IF NOT cl_null(g_master.wc) THEN
      LET g_master.wc = NULL
   END IF
   FOR l_count = 1 TO l_wc.getLength()
      IF NOT cl_null(l_wc[l_count].wc) THEN
         IF l_count = 1 THEN
