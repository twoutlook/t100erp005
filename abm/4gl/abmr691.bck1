            END IF       
            IF cl_null(g_bmaa_d[g_detail_idx].bmaa001) AND cl_null(g_bmaa_d[g_detail_idx].bmaa002) AND cl_null(g_bmaa_d[g_detail_idx].bmfa012) THEN
