PUBLIC FUNCTION abmr691_create_tmp()
   DROP TABLE abmr691_bmaa_tmp
   
   SELECT bmaa001,imaal003,imaal004,bmaa002,bmfa012,bmfa015 
     FROM bmaa_t LEFT OUTER JOIN imaal_t ON imaal001 = bmaa001 AND imaalent = bmaaent AND imaal002 = g_dlang
                 LEFT OUTER JOIN bmfa_t ON bmfadocno = bmaadocno AND bmfaent = bmaaent AND bmfasite = bmaasite
    WHERE 1=2 INTO TEMP abmr691_bmaa_tmp
    
END FUNCTION

