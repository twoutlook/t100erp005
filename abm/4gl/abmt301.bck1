      CALL abmt301_bmfp_t_mask()
      LET g_bmfp_d_mask_n[l_ac].* =  g_bmfp_d[l_ac].*
   END FOR
   
   LET g_bmfp2_d_mask_o.* =  g_bmfp2_d.*
