          g_bmfp2_d[l_ac].bmfb016,g_bmfp2_d[l_ac].bmfb017,g_bmfp2_d[l_ac].bmfb018,g_bmfp2_d[l_ac].bmfb019, 
          g_bmfp2_d[l_ac].bmfb020,g_bmfp2_d[l_ac].bmfb022,g_bmfp2_d[l_ac].bmfb023,g_bmfp2_d[l_ac].bmfb005_desc, 
          g_bmfp2_d[l_ac].bmfb006_desc,g_bmfp2_d[l_ac].bmfb008_desc,g_bmfp2_d[l_ac].bmfb009_desc,g_bmfp2_d[l_ac].bmfb015_desc, 
          g_bmfp2_d[l_ac].bmfb023_desc   #(ver:78)
         IF SQLCA.SQLCODE THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code = SQLCA.SQLCODE 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充 name="b_fill2.fill"
         CALL abmt301_bmfb_desc()
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code = 9035 
            LET g_errparam.popup = TRUE 
