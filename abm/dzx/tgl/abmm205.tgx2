       g_bmga_m.bmgaownid_desc,g_bmga_m.bmgaowndp_desc,g_bmga_m.bmgacrtid_desc,g_bmga_m.bmgacrtdp_desc, 
       g_bmga_m.bmgamodid_desc
   
   
   #檢查是否允許此動作
   IF NOT abmm205_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
