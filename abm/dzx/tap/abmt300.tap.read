         {<point name="b_fill.sql4"/>}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE abmt300_pb4 FROM g_sql
         DECLARE b_fill_cs4 CURSOR FOR abmt300_pb4
      END IF
    
      LET l_ac = 1
      
