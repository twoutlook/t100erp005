   LET g_sql = " SELECT DISTINCT t0.bmfastus,t0.bmfadocno,t0.bmfadocdt,t0.bmfa003,t0.bmfa004,t0.bmfa005, 
       t0.bmfa006,t0.bmfa007,t0.bmfa008 ",
