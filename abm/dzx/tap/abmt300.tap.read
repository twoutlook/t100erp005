          g_bmfb4_d[l_ac].bmff005,g_bmfb4_d[l_ac].bmff006,g_bmfb4_d[l_ac].bmff007,g_bmfb4_d[l_ac].bmff008, 
          g_bmfb4_d[l_ac].bmff004_desc
         IF SQLCA.sqlcode THEN
