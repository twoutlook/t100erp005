   {<point name="input.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT bmfb002,bmfb003,bmfb004,bmfb005,bmfb006,bmfb007,bmfb008,bmfb009,bmfb010, 
       bmfb011,bmfb012,bmfb013,bmfb014,bmfb015,bmfb016,bmfb017,bmfb018,bmfb019,bmfb020,bmfb021 FROM  
       bmfb_t WHERE bmfbent=? AND bmfbsite=? AND bmfbdocno=? AND bmfb002=? FOR UPDATE"
