  <point name="rep.sub04.after" cite_std="N" status="" ver="1" src="s" new="N" order="" mark_hard="N">
<![CDATA[           LET l_sql =
             "SELECT b.oocql004,c.oocql004,a.bmic003,a.bmic004,a.bmic005",
