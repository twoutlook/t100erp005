  <section id="abmr400_g01.rep_data" ver="1" status="" src="s">
<![CDATA[PRIVATE FUNCTION abmr400_g01_rep_data()
DEFINE HANDLER         om.SaxDocumentHandler
DEFINE l_i             INTEGER
 
    #判斷是否有報表資料，若回彈出訊息視窗
    IF sr.getLength() = 0 THEN
       CALL cl_err_msg(NULL , "adz-00285", '', 1)
       RETURN 
    END IF
    #CALL cl_gr_init_apr()                          #t100先不用
    WHILE TRUE
        CALL cl_gr_init_pageheader()               
        LET handler = cl_gr_handler()
        IF handler IS NOT NULL THEN
            START REPORT abmr400_g01_rep TO XML HANDLER handler
            FOR l_i = 1 TO sr.getLength()
                OUTPUT TO REPORT abmr400_g01_rep(sr[l_i].*)
            END FOR
            FINISH REPORT abmr400_g01_rep
        END IF
        IF INT_FLAG = TRUE THEN
            LET INT_FLAG = FALSE
            EXIT WHILE
        END IF
    END WHILE
END FUNCTION
]]>
</section>
  <section id="abmr400_g01.sel_prep" ver="13" status="" src="s">
<![CDATA[#+ 選單功能實際執行處
PRIVATE FUNCTION abmr400_g01_sel_prep()
   #add-point:sel_prep段define
   {<point name="sel_prep.define"/>}
   #end add-point
 
   #add-point:sel_prep before
   {<point name="sel_prep.before" mark="Y"/>}
   #end add-point
   
   #add-point:sel_prep g_select
   {<point name="sel_prep.g_select" mark="Y"/>}
   #end add-point
   LET g_select = " SELECT bmia001,bmia002,bmia003,bmia004,bmia005,bmia006,bmia007,bmia008,bmia009,bmia010, 
       bmia011,bmia012,bmia013,bmia014,bmia015,bmia016,bmia017,bmia018,bmia019,bmia020,bmiadocdt,bmiadocno, 
       bmiaent,bmiasite,bmiastus,bmib001,bmib002,bmib003,bmib004,bmib005,bmib006,bmib007,bmib008,bmib009, 
       bmibseq,bmibseq1,bmibsite,pmaal_t.pmaal003,t3.oofa011,x.t7_oofa011,oofa_t.oofa011,ooefl_t.ooefl003, 
       x.t4_ooefl003,x.t6_ooefl003,bmhel_t.bmhel003,oocql_t.oocql004,t1.oocql004,t2.oocql004,x.t5_oocql004, 
       oobxl_t.oobxl003,trim(bmia007)||'.'||trim(pmaal_t.pmaal003),trim(bmia002)||'.'||trim(t3.oofa011), 
       trim(bmia003)||'.'||trim(ooefl_t.ooefl003),trim(bmiadocno)||'.'||trim(oobxl_t.oobxl003),trim(bmib008)||'.'||trim(x.t6_ooefl003), 
       trim(bmib007)||'.'||trim(x.t7_oofa011),trim(bmib003)||'.'||trim(x.t4_ooefl003),'','','','',trim(bmibseq)||'.'||trim(bmibseq1), 
       ''"
 
   #add-point:sel_prep g_from
   {<point name="sel_prep.g_from" mark="Y"/>}
   #end add-point
    LET g_from = " FROM bmia_t LEFT OUTER JOIN oofa_t ON oofa_t.oofa001 = bmia_t.bmia011 AND oofa_t.oofaent = bmia_t.bmiaent             LEFT OUTER JOIN oobxl_t ON oobxl_t.oobxl001 = bmia_t.bmiadocno AND oobxl_t.oobxlent = bmia_t.bmiaent AND oobxl_t.oobxl002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oocql_t ON oocql_t.oocql001 = '210' AND oocql_t.oocql002 = bmia_t.bmia014 AND oocql_t.oocqlent = bmia_t.bmiaent AND oocql_t.oocql003 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oocql_t t1 ON t1.oocql001 = '1116' AND t1.oocql002 = bmia_t.bmia013 AND t1.oocqlent = bmia_t.bmiaent AND t1.oocql003 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oocql_t t2 ON t2.oocql001 = '221' AND t2.oocql002 = bmia_t.bmia008 AND t2.oocqlent = bmia_t.bmiaent AND t2.oocql003 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN pmaal_t ON pmaal_t.pmaal001 = bmia_t.bmia007 AND pmaal_t.pmaalent = bmia_t.bmiaent AND pmaal_t.pmaal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oofa_t t3 ON t3.oofa002 = '2' AND t3.oofa003 = bmia_t.bmia002 AND t3.oofaent = bmia_t.bmiaent             LEFT OUTER JOIN ooefl_t ON ooefl_t.ooefl001 = bmia_t.bmia003 AND ooefl_t.ooeflent = bmia_t.bmiaent AND ooefl_t.ooefl002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN bmhel_t ON bmhel_t.bmhel001 = bmia_t.bmia005 AND bmhel_t.bmhelent = bmia_t.bmiaent AND bmhel_t.bmhel002 = '" , 
        g_dlang,"'" ," LEFT OUTER JOIN ( SELECT bmib_t.*,t7.oofa011 t7_oofa011,t4.ooefl003 t4_ooefl003, 
        t6.ooefl003 t6_ooefl003,t5.oocql004 t5_oocql004 FROM bmib_t LEFT OUTER JOIN ooefl_t t4 ON t4.ooefl001 = bmib_t.bmib003 AND t4.ooeflent = bmib_t.bmibent AND t4.ooefl002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oocql_t t5 ON t5.oocql001 = '1115' AND t5.oocql002 = bmib_t.bmib001 AND t5.oocqlent = bmib_t.bmibent AND t5.oocql003 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN ooefl_t t6 ON t6.ooefl001 = bmib_t.bmib008 AND t6.ooeflent = bmib_t.bmibent AND t6.ooefl002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oofa_t t7 ON t7.oofa002 = '2' AND t7.oofa003 = bmib_t.bmib007  
        AND t7.oofaent = bmib_t.bmibent ) x  ON bmia_t.bmiaent = x.bmibent AND bmia_t.bmiadocno = x.bmibdocno" 

 
   #add-point:sel_prep g_where
   {<point name="sel_prep.g_where" mark="Y"/>}
   #end add-point
    LET g_where = " WHERE " ,tm.wc CLIPPED
 
   #add-point:sel_prep g_order
   {<point name="sel_prep.g_order" mark="Y"/>}
   #end add-point
    LET g_order = " ORDER BY bmiadocno,bmibseq,bmibseq1"
 
   #add-point:sel_prep.sql.before
   {<point name="sel_prep.sql.before mark="Y""/>}
   #end add-point:sel_prep.sql.before
 
   LET g_sql = g_select CLIPPED ," ",g_from CLIPPED ," ",g_where CLIPPED ," ",g_order CLIPPED 
 
   #add-point:sel_prep.sql.after
   {<point name="sel_prep.sql.after mark="Y""/>}
   #end add-point
   PREPARE abmr400_g01_prepare FROM g_sql
   IF STATUS THEN
      CALL cl_err('prepare:',STATUS,1)
      CALL cl_ap_exitprogram("0")
   END IF
   DECLARE abmr400_g01_curs CURSOR FOR abmr400_g01_prepare
 
END FUNCTION
]]>
