   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE abmm204_detail AS ",
                "SELECT * FROM bmba_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
