  <point name="function.abmi500_bmjb002_ref" order="2" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION abmi500_bmjb002_ref()
DEFINE l_cnt      LIKE type_t.num5
 
   LET l_cnt = 0 
   SELECT COUNT(*) INTO l_cnt 
     FROM bmjb_t
    WHERE bmjbent = g_enterprise
      AND bmjb001 = g_bmja_m.bmja001 
      AND bmjbseq = g_bmjb_d[l_ac].bmjbseq
   IF l_cnt > 0 THEN 
      SELECT DISTINCT bmjb002 INTO g_bmjb_d[l_ac].bmjb002
        FROM bmjb_t
       WHERE bmjbent = g_enterprise
         AND bmjb001 = g_bmja_m.bmja001 
         AND bmjbseq = g_bmjb_d[l_ac].bmjbseq
      CALL abmi500_oocql004_desc('1117',g_bmjb_d[l_ac].bmjb002) RETURNING g_bmjb_d[l_ac].bmjb002_desc
      DISPLAY BY NAME g_bmjb_d[l_ac].bmjb002_desc
      LET g_bmjbseq_e = TRUE  
   ELSE
      LET g_bmjbseq_e = FALSE    
   END IF
END FUNCTION]]>
