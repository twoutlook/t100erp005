  <section id="abmi500.input.body" ver="3" status="" src="s">
    <![CDATA[   
      #Page1 預設值產生於此處
      INPUT ARRAY g_bmjb_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_bmjb_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL abmi500_b_fill()
            LET g_rec_b = g_bmjb_d.getLength()
            #add-point:資料輸入前
            {<point name="input.d.before_input"/>}
            #end add-point
         
         BEFORE ROW
            #add-point:modify段before row2
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET l_insert = FALSE
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET g_detail_idx = l_ac
            
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
         
            CALL s_transaction_begin()
            OPEN abmi500_cl USING g_enterprise,g_bmja_m.bmja001
            IF STATUS THEN
               CALL cl_err("OPEN abmi500_cl:", STATUS, 1)
               CLOSE abmi500_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            
            LET g_rec_b = g_bmjb_d.getLength()
            
            IF g_rec_b >= l_ac 
               AND g_bmjb_d[l_ac].bmjbseq IS NOT NULL
               AND g_bmjb_d[l_ac].bmjbseq1 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_bmjb_d_t.* = g_bmjb_d[l_ac].*  #BACKUP
               LET g_bmjb_d_o.* = g_bmjb_d[l_ac].*  #BACKUP
               CALL abmi500_set_entry_b(l_cmd)
               #add-point:modify段after_set_entry_b
               {<point name="input.body.after_set_entry_b"/>}
               #end add-point  
               CALL abmi500_set_no_entry_b(l_cmd)
               IF NOT abmi500_lock_b("bmjb_t","'1'") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH abmi500_bcl INTO g_bmjb_d[l_ac].bmjbseq,g_bmjb_d[l_ac].bmjb002,g_bmjb_d[l_ac].bmjbseq1, 
                      g_bmjb_d[l_ac].bmjb003,g_bmjb_d[l_ac].bmjb004,g_bmjb_d[l_ac].bmjbsite
                  IF SQLCA.sqlcode THEN
                     CALL cl_err(g_bmjb_d_t.bmjbseq,SQLCA.sqlcode,1)
                     LET l_lock_sw = "Y"
                  END IF
                  
                  LET g_bfill = "N"
                  CALL abmi500_show()
                  LET g_bfill = "Y"
                  
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
        
         BEFORE INSERT
            
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_bmjb_d[l_ac].* TO NULL 
			INITIALIZE g_bmjb_d_t.* TO NULL 
            INITIALIZE g_bmjb_d_o.* TO NULL 
                  LET g_bmjb_d[l_ac].bmjb004 = "1"
 
            LET g_bmjb_d_t.* = g_bmjb_d[l_ac].*     #新輸入資料
            LET g_bmjb_d_o.* = g_bmjb_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL abmi500_set_entry_b(l_cmd)
            #add-point:modify段after_set_entry_b
            {<point name="input.body.insert.after_set_entry_b"/>}
            #end add-point
            CALL abmi500_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_bmjb_d[li_reproduce_target].* = g_bmjb_d[li_reproduce].*
 
               LET g_bmjb_d[li_reproduce_target].bmjbseq = NULL
               LET g_bmjb_d[li_reproduce_target].bmjbseq1 = NULL
 
            END IF
            #公用欄位給值(單身)
            
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
