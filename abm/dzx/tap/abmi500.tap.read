  <point name="function.abmi500_oocql004_desc" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION abmi500_oocql004_desc(p_oocql001,p_oocql002)
DEFINE p_oocql001  LIKE oocql_t.oocql001,
       p_oocql002  LIKE oocql_t.oocql002,
       r_oocql004  LIKE oocql_t.oocql004

   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = p_oocql001
   LET g_ref_fields[2] = p_oocql002
   CALL ap_ref_array2(g_ref_fields,"SELECT oocql004 FROM oocql_t WHERE oocqlent='"||g_enterprise||"' AND oocql001=? AND oocql002=? AND oocql003='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET r_oocql004 = g_rtn_fields[1] 
   RETURN r_oocql004
END FUNCTION]]>
