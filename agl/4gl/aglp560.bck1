################################################################################
PRIVATE FUNCTION aglp560_diff_p(p_glaa001_p)
   DEFINE p_glaa001_p    LIKE glaa_t.glaa001      #目的币别 
   DEFINE l_sum1         LIKE glaq_t.glaq003      #借方金额合计
   DEFINE l_sum2         LIKE glaq_t.glaq004      #贷方金额合计
