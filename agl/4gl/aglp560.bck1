     INTO l_glaa015,l_glaa016,l_glaa018,l_glaa019,l_glaa020,l_glaa022 
     FROM glaa_t 
    WHERE glaaent = g_enterprise
      AND glaald = g_master.glapld_p
   #汇兑收益
