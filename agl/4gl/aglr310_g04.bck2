################################################################################
# Descriptions...: 抓取明细科目的一级科目
# Memo...........:
# Usage..........: CALL aglr310_g04_first_account(p_ac)
#                  RETURNING r_str
# Input parameter: p_ac           数组项次
# Return code....: r_str          一级科目
# Date & Author..: 2016/05/11 By 02599
# Modify.........:
################################################################################
PRIVATE FUNCTION aglr310_g04_first_account(p_ac)
   DEFINE p_ac            LIKE type_t.num10
   DEFINE r_str           STRING
   DEFINE l_glaq002       LIKE glaq_t.glaq002 #下级科目
   DEFINE l_account       LIKE glac_t.glac002 #上级科目
   DEFINE l_i             LIKE type_t.num5    #防止死循环
   DEFINE l_glaq018_desc  LIKE ooefl_t.ooefl003  #部门
   DEFINE l_glacl004      LIKE glacl_t.glacl004  #科目名称
   DEFINE l_desc          STRING
   
   LET l_i = 1
   LET l_account='' #上级科目
   LET l_glaq002=sr[p_ac].glaq002 #下级科目
   WHILE l_i < 100
      SELECT glac004 INTO l_account FROM glac_t  #所属据统制科目
       WHERE glacent = g_enterprise
         AND glac001 = (SELECT glaa004 FROM glaa_t WHERE glaaent=g_enterprise AND glaald=sr[p_ac].glapld)
         AND glac002 = l_glaq002
      
      IF l_account = l_glaq002 OR cl_null(l_account) THEN 
         #最上层科目
         EXIT WHILE
      ELSE
         LET l_glaq002=l_account
         LET l_i = l_i + 1
      END IF
   END WHILE
   
   #抓取科目名称
   SELECT glacl004 INTO l_glacl004 FROM glacl_t
    WHERE glaclent = g_enterprise
      AND glacl001 = (SELECT glaa004 FROM glaa_t WHERE glaaent=g_enterprise AND glaald=sr[p_ac].glapld)
      AND glacl002 = l_account
      AND glacl003 = g_dlang
   LET r_str = l_glacl004
   
   CALL s_desc_get_department_desc(sr[p_ac].glaq018) RETURNING l_glaq018_desc  #客户名称
   IF NOT cl_null(l_glaq018_desc) THEN
      LET r_str = r_str CLIPPED,l_glaq018_desc
   END IF
   
   RETURN r_str
END FUNCTION

################################################################################
# Descriptions...: 更新列印次数
# Memo...........:
# Usage..........: CALL aglr310_g04_update_glap012(p_glapld,p_glapdocno)
# Input parameter: p_glapld       账套
#                : p_glapdocno    单号
# Date & Author..: 2016/05/11 By 02599
# Modify.........:
################################################################################
PRIVATE FUNCTION aglr310_g04_update_glap012(p_glapld,p_glapdocno)
   DEFINE p_glapld       LIKE glap_t.glapld
   DEFINE p_glapdocno    LIKE glap_t.glapdocno
   DEFINE l_cnt           LIKE type_t.num10
   
   UPDATE glap_t SET glap012=glap012 + 1
    WHERE glapent=g_enterprise
      AND glapld=p_glapld
      AND glapdocno=p_glapdocno
END FUNCTION

 
