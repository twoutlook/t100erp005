               LET g_gldk_m.gldk007 = g_argv[7]
               LET g_gldk_m.gldk008 = g_argv[8]
               LET g_gldj009        = g_argv[9]
               DISPLAY BY NAME g_gldk_m.gldk001,g_gldk_m.gldk002,g_gldk_m.gldk003,
