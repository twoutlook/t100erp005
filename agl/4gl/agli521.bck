{<section id="agli521.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION agli521_b_fill(p_wc2)
   #add-point:b_fill段define name="b_fill.define_customerization"
   
   #end add-point
   DEFINE p_wc2      STRING
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   
   #end add-point     
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   #先清空單身變數內容
   CALL g_gldk_d.clear()
   CALL g_gldk2_d.clear()
 
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   
   #end add-point
   
   LET g_sql = "SELECT  DISTINCT gldk009,gldk009,gldkownid,gldkowndp,gldkcrtid,gldkcrtdp,gldkcrtdt,gldkmodid, 
       gldkmoddt,t1.ooag011 ,t2.ooefl003 ,t3.ooag011 ,t4.ooefl003 ,t5.ooag011 FROM gldk_t",   
               "",
               
                              " LEFT JOIN ooag_t t1 ON t1.ooagent="||g_enterprise||" AND t1.ooag001=gldkownid  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent="||g_enterprise||" AND t2.ooefl001=gldkowndp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent="||g_enterprise||" AND t3.ooag001=gldkcrtid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent="||g_enterprise||" AND t4.ooefl001=gldkcrtdp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=gldkmodid  ",
 
               " WHERE gldkent= ? AND gldk001=? AND gldk002=? AND gldk003=? AND gldk004=? AND gldk005=? AND gldk006=? AND gldk007=? AND gldk008=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("gldk_t")
   END IF
   
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF agli521_fill_chk(1) THEN
      IF (g_action_choice = "query" OR cl_null(g_action_choice))
         #add-point:b_fill段long_sql_if name="b_fill.long_sql_if"
         
         #end add-point
      THEN
         LET g_sql = g_sql, " ORDER BY gldk_t.gldk009"
         #add-point:b_fill段fill_before name="b_fill.fill_before"
         
         #end add-point
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE agli521_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR agli521_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_gldk_m.gldk001,g_gldk_m.gldk002,g_gldk_m.gldk003,g_gldk_m.gldk004,g_gldk_m.gldk005,g_gldk_m.gldk006,g_gldk_m.gldk007,g_gldk_m.gldk008
                                               
      FOREACH b_fill_cs INTO g_gldk_d[l_ac].gldk009,g_gldk2_d[l_ac].gldk009,g_gldk2_d[l_ac].gldkownid, 
          g_gldk2_d[l_ac].gldkowndp,g_gldk2_d[l_ac].gldkcrtid,g_gldk2_d[l_ac].gldkcrtdp,g_gldk2_d[l_ac].gldkcrtdt, 
          g_gldk2_d[l_ac].gldkmodid,g_gldk2_d[l_ac].gldkmoddt,g_gldk2_d[l_ac].gldkownid_desc,g_gldk2_d[l_ac].gldkowndp_desc, 
          g_gldk2_d[l_ac].gldkcrtid_desc,g_gldk2_d[l_ac].gldkcrtdp_desc,g_gldk2_d[l_ac].gldkmodid_desc 
 
                             
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充 name="b_fill.fill"
         LET g_gldk_d[l_ac].gldk009_desc =  s_desc_get_account_desc(g_gldk_m.gldk002,g_gldk_d[l_ac].gldk009)
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #應用 a12 樣板自動產生(Version:4)
 
 
 
 
        
         #add-point:單身資料抓取 name="bfill.foreach"
         
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF 
            EXIT FOREACH
         END IF
      
         LET l_ac = l_ac + 1
         
      END FOREACH
 
            CALL g_gldk_d.deleteElement(g_gldk_d.getLength())
      CALL g_gldk2_d.deleteElement(g_gldk2_d.getLength())
 
      
   END IF
   
   #add-point:b_fill段more name="b_fill.more"
   
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_gldk_d.getLength()
      LET g_gldk_d_mask_o[l_ac].* =  g_gldk_d[l_ac].*
      CALL agli521_gldk_t_mask()
      LET g_gldk_d_mask_n[l_ac].* =  g_gldk_d[l_ac].*
   END FOR
   
   LET g_gldk2_d_mask_o.* =  g_gldk2_d.*
   FOR l_ac = 1 TO g_gldk2_d.getLength()
      LET g_gldk2_d_mask_o[l_ac].* =  g_gldk2_d[l_ac].*
      CALL agli521_gldk_t_mask()
      LET g_gldk2_d_mask_n[l_ac].* =  g_gldk2_d[l_ac].*
   END FOR
 
 
   FREE agli521_pb   
   
END FUNCTION
 
