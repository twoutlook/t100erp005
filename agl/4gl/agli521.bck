# Descriptions...: 取得核算項沖銷方式(來源)
# Memo...........:
# Usage..........: CALL agli521_get_gldj013()
# Date & Author..: 2015/03/31 By Hans
# Modify.........:
################################################################################
PRIVATE FUNCTION agli521_get_gldj013()
  
   CALL s_ld_sel_glaa(g_gldk_m.gldk002,'glaa134') RETURNING g_sub_success,g_gldk002_glaa134
   SELECT gldj009 INTO g_gldj009
     FROM gldj_t
    WHERE gldj001 = g_gldk_m.gldk001 
      AND gldj002 = g_gldk_m.gldk002
      AND gldj003 = g_gldk_m.gldk003
      AND gldj004 = g_gldk_m.gldk004
      AND gldj005 = g_gldk_m.gldk005
      AND gldj006 = g_gldk_m.gldk006
      AND gldj007 = g_gldk_m.gldk007
      AND gldj010 = g_gldk_m.gldk008
      AND gldjent = g_enterprise    #160905-00007#3 add
      CALL cl_set_act_visible("agli524",TRUE)   
      IF g_gldj009 <> 2 AND NOT cl_null(g_gldk002_glaa134) THEN         
         CALL cl_set_act_visible("agli524",FALSE)
      END IF      




END FUNCTION

################################################################################
# Descriptions...: 刪除agli524
# Memo...........:
# Usage..........: CALL agli521_del_agli524(p_type)
# Date & Author..: 2015/04/07 By Hans
# Modify.........:
################################################################################
PRIVATE FUNCTION agli521_del_agli524(p_type)
DEFINE p_type  LIKE type_t.num5

   #p_type= 1 刪除單頭 
   #p_type= 2 刪除單身
   
    #del_algi524    
    IF p_type = 1 THEN  #刪除項次
       DELETE FROM gldw_t
       WHERE gldwent = g_enterprise     AND gldw001 = g_gldk_m.gldk001 
         AND gldw002 = g_gldk_m.gldk002 AND gldw003 = g_gldk_m.gldk003 
         AND gldw004 = g_gldk_m.gldk004 AND gldw005 = g_gldk_m.gldk005
         AND gldw006 = g_gldk_m.gldk006 AND gldw007 = g_gldk_m.gldk007
         AND gldw008 = '1'              AND gldw009 = g_gldk_m.gldk008
         AND NOT EXISTS (SELECT 1 
                          FROM gldk_t 
                         WHERE gldkent = gldwent AND gldk001 = gldw001 AND gldk002 = gldw002
                           AND gldk003 = gldw003 AND gldk004 = gldw004 AND gldk005 = gldw005
                           AND gldk006 = gldw006 AND gldk007 = gldw007 AND gldk008 = gldw009 
                           AND gldk009 = gldw010)
     ELSE
       DELETE FROM gldw_t
       WHERE gldwent = g_enterprise     AND gldw001 = g_gldk_m.gldk001 
         AND gldw002 = g_gldk_m.gldk002 AND gldw003 = g_gldk_m.gldk003 
         AND gldw004 = g_gldk_m.gldk004 AND gldw005 = g_gldk_m.gldk005
         AND gldw006 = g_gldk_m.gldk006 AND gldw007 = g_gldk_m.gldk007
         AND gldw008 = '1'              AND gldw009 = g_gldk_m.gldk008
         AND gldw010 = g_gldk_d[l_ac].gldk009
         AND NOT EXISTS (SELECT 1 
                          FROM gldk_t 
                         WHERE gldkent = gldwent AND gldk001 = gldw001 AND gldk002 = gldw002
                           AND gldk003 = gldw003 AND gldk004 = gldw004 AND gldk005 = gldw005
                           AND gldk006 = gldw006 AND gldk007 = gldw007 AND gldk008 = gldw009 
                           AND gldk009 = gldw010)
     
     END IF
              
END FUNCTION

 
