   #傳票憑證單頭
   CALL s_aooi200_fin_gen_docno(g_master.glalld,g_glaa024,g_glaa003,g_glal.glal006,g_master.glapdocdt,'aglt310') RETURNING l_success,g_glap.glapdocno
   IF l_success = FALSE THEN
