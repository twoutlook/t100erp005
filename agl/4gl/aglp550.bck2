PRIVATE FUNCTION aglp550_glap_insert()
DEFINE l_success           LIKE type_t.num5
   
   #若有勾选 '同日已产生传票者重新产生',则产生前先删除原同日分摊传票后再产生
   IF g_master.regenerate = 'Y' THEN 
