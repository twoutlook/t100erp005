################################################################################
# Descriptions...: 產生沖銷傳票
# Memo...........:
# Usage..........: CALL aglt523_gen_aglt524(p_slip,p_date)
#                  RETURNING r_success,r_gldv008
# Input parameter: 1.p_slip         傳票單別
#                : 2.p_date         傳票日期
# Return code....: 1.r_success      處理狀態
#                : 2.r_gldv008      沖銷傳票單號
# Date & Author..: 2015/12/07 By Lori
# Modify.........:
################################################################################
PRIVATE FUNCTION aglt523_gen_aglt524(p_slip,p_date)
   DEFINE p_slip         LIKE gldv_t.gldv008
   DEFINE p_date         LIKE type_t.dat
   DEFINE r_success      LIKE type_t.num5
   DEFINE r_gldv008      LIKE gldv_t.gldv008
   DEFINE l_success      LIKE type_t.num5
   DEFINE l_slip_start   LIKE gldv_t.gldv008
   DEFINE l_slip_end     LIKE gldv_t.gldv008
   
   LET r_success = TRUE
   LET r_gldv008 = ''
   
   LET l_slip_start = ''
   LET l_slip_end = ''       
   
   LET l_success = ''
   CALL s_merge_cre_gl_tmp_table() RETURNING l_success
   
   CALL s_transaction_begin()
   CALL cl_err_collect_init() 
   
   CALL s_merge_gen_gl(g_gldv_m.gldvld,g_gldv_m.gldv001,g_gldv_m.gldv003,g_gldv_m.gldv004,p_slip,p_date,'1','','aglt523')
        RETURNING l_success,l_slip_start,l_slip_end
   IF l_success THEN
      CALL s_transaction_end('Y','0')
      
      LET r_gldv008 = l_slip_start
      
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'adz-00217' #執行成功
      LET g_errparam.popup = TRUE
      CALL cl_err()
   ELSE
      IF cl_null(l_slip_start) THEN 
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = ''
         LET g_errparam.code   = 'axc-00530' #無資料產生
         LET g_errparam.popup = TRUE
         CALL cl_err()
      ELSE
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = ''
         LET g_errparam.code   = 'axr-00120' #傳票拋轉失敗
         LET g_errparam.popup = TRUE
         CALL cl_err()
      END IF
      
      CALL s_transaction_end('N','0')
      LET r_success = FALSE
   END IF 
   
   CALL cl_err_collect_show()
   
   LET l_success = ''
   CALL s_merge_drop_gl_tmp_table() RETURNING l_success  
   
   RETURN r_success,r_gldv008
END FUNCTION

################################################################################
# Descriptions...: 檢查單頭資料
# Memo...........:
# Usage..........: CALL aglt523_gldv_master_chk()
#                  RETURNING r_success
# Input parameter: 無
# Return code....: r_success      檢查結果
# Date & Author..: 2015/12/7 By Lori
# Modify.........:
################################################################################
PRIVATE FUNCTION aglt523_gldv_master_chk()
   DEFINE r_success     LIKE type_t.num5
   DEFINE l_cnt         LIKE type_t.num5
   DEFINE l_gldvstus    LIKE gldv_t.gldvstus
   DEFINE l_gldv008     LIKE gldv_t.gldv008
   
   LET r_success = TRUE
   LET l_cnt = 0
   LET l_gldvstus = ''
   LET l_gldv008 = ''

   IF NOT cl_null(g_gldv_m.gldvld) AND NOT cl_null(g_gldv_m.gldv001) AND NOT cl_null(g_gldv_m.gldv002) 
     AND NOT cl_null(g_gldv_m.gldv003) AND NOT cl_null(g_gldv_m.gldv004) THEN 
     SELECT COUNT(gldv009),gldvstus,l_gldv008 
       INTO l_cnt,l_gldvstus
       FROM gldv_t 
      WHERE gldvent = g_enterprise
        AND gldvld = g_gldv_m.gldvld
        AND gldv001 = g_gldv_m.gldv001
        AND gldv002 = g_gldv_m.gldv002
        AND gldv003 = g_gldv_m.gldv003
        AND gldv004 = g_gldv_m.gldv004
      GROUP BY gldvstus,l_gldv008
      
     IF l_cnt > 0 THEN
        LET r_success = FALSE
     ELSE
        IF l_gldvstus = 'Y' THEN
           IF NOT cl_null(l_gldv008) THEN
              #合併帳別(%1)/上層公司編號(%2)/年度(%3)/期別(%4)存在已確認且有沖銷傳票的資料，不可重複新增！
              INITIALIZE g_errparam TO NULL
              LET g_errparam.code   = "agl-00400"
              LET g_errparam.replace[1] = g_gldv_m.gldv001
              LET g_errparam.replace[2] = g_gldv_m.gldv002
              LET g_errparam.replace[3] = g_gldv_m.gldv003
              LET g_errparam.replace[4] = g_gldv_m.gldv004
              LET g_errparam.popup  = FALSE
              CALL cl_err()
              LET r_success = FALSE
           ELSE
              #合併帳別(%1)/上層公司編號(%2)/年度(%3)/期別(%4)存在已確認資料，不可重複新增！
              INITIALIZE g_errparam TO NULL
              LET g_errparam.code   = "agl-00399"
              LET g_errparam.replace[1] = g_gldv_m.gldv001
              LET g_errparam.replace[2] = g_gldv_m.gldv002
              LET g_errparam.replace[3] = g_gldv_m.gldv003
              LET g_errparam.replace[4] = g_gldv_m.gldv004
              LET g_errparam.popup  = FALSE
              CALL cl_err() 
              
              LET r_success = FALSE                
           END IF       
        END IF
     END IF 
   END IF
   
  
   RETURN r_success
END FUNCTION

