################################################################################
# Descriptions...: 狀態處理
# Memo...........:
# Usage..........: CALL aglt523_statechange()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2015/12/04 By Lori
# Modify.........:
################################################################################
PRIVATE FUNCTION aglt523_statechange()
   DEFINE lc_state       LIKE type_t.chr5
   DEFINE l_date         DATETIME YEAR TO SECOND
   DEFINE l_gldvcnfid    LIKE gldv_t.gldvcnfid
   DEFINE l_gldvcnfdt    DATETIME YEAR TO SECOND
   DEFINE ls_js          STRING
   DEFINE lc_param       g_gldv_param
   
   IF cl_null(g_gldv_m.gldvld) OR cl_null(g_gldv_m.gldv001) OR cl_null(g_gldv_m.gldv002) 
      OR cl_null(g_gldv_m.gldv003) OR cl_null(g_gldv_m.gldv004) THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code   = "agl-00398"
      LET g_errparam.popup  = FALSE
      CALL cl_err()
      RETURN
   END IF
   
   INITIALIZE lc_param.* TO NULL
   LET lc_param.gldvld  = g_gldv_m.gldvld 
   LET lc_param.gldv001 = g_gldv_m.gldv001
   LET lc_param.gldv002 = g_gldv_m.gldv002
   LET lc_param.gldv003 = g_gldv_m.gldv003
   LET lc_param.gldv004 = g_gldv_m.gldv004
   LET ls_js = util.JSON.stringify(lc_param)
   
   CALL s_transaction_begin()

   OPEN aglt523_cl USING g_enterprise,g_gldv_m.gldvld,g_gldv_m.gldv001,g_gldv_m.gldv002,g_gldv_m.gldv003,
                         g_gldv_m.gldv004
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aglt523_cl:" 
      LET g_errparam.code   =  STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE aglt523_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   #160809-00048#1--add--str--
   #顯示最新的資料
   EXECUTE aglt523_master_referesh USING g_gldv_m.gldvld,g_gldv_m.gldv001,g_gldv_m.gldv002,g_gldv_m.gldv003,g_gldv_m.gldv004 
   INTO g_gldv_m.gldvld,g_gldv_m.gldv001,g_gldv_m.gldv002,g_gldv_m.gldv003,g_gldv_m.gldv004, 
        g_gldv_m.gldv008,g_gldv_m.gldvstus,g_gldv_m.gldvld_desc,g_gldv_m.gldv001_desc,g_gldv_m.gldv002_desc 
   
   DISPLAY BY NAME g_gldv_m.gldvld,g_gldv_m.gldv001,g_gldv_m.gldv002,g_gldv_m.gldv003,g_gldv_m.gldv004, 
        g_gldv_m.gldv008,g_gldv_m.gldvstus,g_gldv_m.gldvld_desc,g_gldv_m.gldv001_desc,g_gldv_m.gldv002_desc 
   #160809-00048#1--add--end    
   #標準檢查段落, 因本程式暫無需求先標註
   #檢查是否允許此動作
   #IF NOT aglt523_action_chk() THEN
   #   CLOSE aglt523_cl
   #   CALL s_transaction_end('N','0')
   #   RETURN
   #END IF   
   
  #CALL aglt523_stus_img(g_gldv_m.l_gldvstus) #160315 mark By Reanna
   CALL aglt523_stus_img(g_gldv_m.gldvstus)   #160315 add By Reanna
   
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         LET l_gldvcnfid = ''
         LET l_gldvcnfdt = ''
         
        #CASE g_gldv_m.l_gldvstus  #160315 mark By Reanna
         CASE g_gldv_m.gldvstus    #160315 add By Reanna
            WHEN "N"
               HIDE OPTION "unconfirmed"
            WHEN "Y"
               HIDE OPTION "confirmed"
            WHEN "X"
               HIDE OPTION "invalid"
         END CASE
      
         CALL aglt523_set_act_visible()
         CALL aglt523_set_act_no_visible() 

      ON ACTION unconfirmed
         IF cl_auth_chk_act("unconfirmed") THEN
            LET lc_state = "N"
            CALL cl_err_collect_init()
            
            IF NOT s_aglt523_unconf_chk(ls_js) THEN
               CALL cl_err_collect_show()
               CLOSE aglt523_cl
               CALL s_transaction_end('N','0')
               RETURN
            ELSE
               IF NOT s_aglt523_unconf_upd(ls_js) THEN  
                  CALL cl_err_collect_show()
                  CLOSE aglt523_cl
                  CALL s_transaction_end('N','0')
                  RETURN                
               END IF                
            END IF
            
            CALL cl_err_collect_show()
         END IF
         EXIT MENU
      
      ON ACTION confirmed
         IF cl_auth_chk_act("confirmed") THEN
            LET lc_state = "Y"
            CALL cl_err_collect_init()
            
            IF NOT s_aglt523_conf_chk(ls_js) THEN
               CALL cl_err_collect_show()
               CLOSE aglt523_cl
               CALL s_transaction_end('N','0')
               RETURN
            ELSE
               IF NOT s_aglt523_conf_upd(ls_js) THEN  
                  CALL cl_err_collect_show()
                  CLOSE aglt523_cl
                  CALL s_transaction_end('N','0')
                  RETURN                
               END IF                
            END IF
            
            CALL cl_err_collect_show()            
         END IF   
         EXIT MENU     
       
      ON ACTION invalid
         IF cl_auth_chk_act("invalid") THEN
            LET lc_state = "X"  
            CALL cl_err_collect_init()
            
            IF NOT s_aglt523_invalid_chk(ls_js) THEN
               CALL cl_err_collect_show()
               CLOSE aglt523_cl
               CALL s_transaction_end('N','0')
               RETURN
            ELSE
               IF NOT s_aglt523_invalid_upd(ls_js) THEN  
                  CALL cl_err_collect_show()
                  CLOSE aglt523_cl
                  CALL s_transaction_end('N','0')
                  RETURN                
               END IF                
            END IF
            
            CALL cl_err_collect_show()            
         END IF
         EXIT MENU   
         
   END MENU

   #確認被選取的狀態碼是否在清單中
   IF (lc_state <> "N" AND lc_state <> "Y" AND lc_state <> "A" AND lc_state <> "D"
      AND lc_state <> "R" AND lc_state <> "W" AND lc_state <> "X" ) 
      #OR g_gldv_m.l_gldvstus = lc_state OR cl_null(lc_state) THEN #160315 mark By Reanna
       OR g_gldv_m.gldvstus = lc_state OR cl_null(lc_state) THEN   #160315 add By Reanna   
      CLOSE aglt523_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF

   CALL aglt523_stus_img(lc_state)
    
   EXECUTE aglt523_master_referesh USING g_gldv_m.gldvld,g_gldv_m.gldv001,g_gldv_m.gldv002,g_gldv_m.gldv003, 
                                         g_gldv_m.gldv004 
                                    INTO g_gldv_m.gldvld,g_gldv_m.gldv001,g_gldv_m.gldv002,g_gldv_m.gldv003,g_gldv_m.gldv004, 
                                         g_gldv_m.gldvld_desc,g_gldv_m.gldv001_desc,g_gldv_m.gldv002_desc
   
   DISPLAY BY NAME g_gldv_m.gldvld,g_gldv_m.gldv001,g_gldv_m.gldv002,g_gldv_m.gldv003,g_gldv_m.gldv004,           
                   g_gldv_m.gldvld_desc,g_gldv_m.gldv001_desc,g_gldv_m.gldv002_desc
   
   CALL aglt523_show()
   CALL aglt523_b_fill(g_wc2)  

   CLOSE aglt523_cl
   CALL s_transaction_end('Y','0')

   #功能已完成,通報訊息中心
   CALL aglt523_msgcentre_notify(lc_state)

END FUNCTION

