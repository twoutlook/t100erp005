   INITIALIZE g_glaa.* TO NULL
   LET l_success = ''
   
   CALL s_ld_sel_glaa(g_gldv_m.gldvld,'glaa001|glaa003|glaa004|glaa015|glaa016|glaa019|glaa020') 
      RETURNING l_success,
                g_glaa.glaa001,g_glaa.glaa003,g_glaa.glaa004,g_glaa.glaa015,g_glaa.glaa016,
