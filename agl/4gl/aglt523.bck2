   CALL g_gldv2_d.clear()
 
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   
   #end add-point
   
   LET g_sql = "SELECT  DISTINCT gldv009,gldv005,gldv006,gldv007,gldv010,gldv011,gldv012,gldv013,gldv014, 
       gldv015,gldv016,gldv017,gldv018,gldv019,gldv020,gldv021,gldv027,gldv028,gldv029,gldv022,gldv023, 
       gldv024,gldv025,gldv026,gldv030,gldv031,gldv032,gldv009,gldvownid,gldvowndp,gldvcrtid,gldvcrtdp, 
       gldvcrtdt,gldvmodid,gldvmoddt,gldvcnfid,gldvcnfdt,t1.gldal003 ,t2.gldal003 ,t3.ooag011 ,t4.ooefl003 , 
       t5.ooag011 ,t6.ooefl003 ,t7.ooag011 ,t8.ooag011 FROM gldv_t",   
               "",
               
                              " LEFT JOIN gldal_t t1 ON t1.gldalent="||g_enterprise||" AND t1.gldal001=gldv012 AND t1.gldal002='"||g_dlang||"' ",
               " LEFT JOIN gldal_t t2 ON t2.gldalent="||g_enterprise||" AND t2.gldal001=gldv014 AND t2.gldal002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent="||g_enterprise||" AND t3.ooag001=gldvownid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent="||g_enterprise||" AND t4.ooefl001=gldvowndp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=gldvcrtid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent="||g_enterprise||" AND t6.ooefl001=gldvcrtdp AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent="||g_enterprise||" AND t7.ooag001=gldvmodid  ",
               " LEFT JOIN ooag_t t8 ON t8.ooagent="||g_enterprise||" AND t8.ooag001=gldvcnfid  ",
 
               " WHERE gldvent= ? AND gldvld=? AND gldv001=? AND gldv002=? AND gldv003=? AND gldv004=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("gldv_t")
   END IF
   
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   
