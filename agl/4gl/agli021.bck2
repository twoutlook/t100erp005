                #150814-00006#1--add--end
           FROM glac_t
          WHERE glacent = g_enterprise AND glac001 = p_glac001
            AND glac002 = p_glac002
      #glac003 不為獨立科目時，將選擇資料的所有內容全部帶給當前筆資料
      IF l_glac003 != '3' THEN
         
