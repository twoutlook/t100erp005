   #end add-point  
   
   
   
   #保存單頭舊值
   LET g_glad_m_t.* = g_glad_m.*
   LET g_glad_m_o.* = g_glad_m.*
   
   LET g_data_owner = g_glad_m.gladownid      
   
   #重新顯示
   CALL agli031_show()
   
 
