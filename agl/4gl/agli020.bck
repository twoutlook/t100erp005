# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL s_aooi150_ins (传入参数)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION agli020_dis(p_glac001,p_glac002)
   DEFINE p_glac001      LIKE glac_t.glac001
   DEFINE p_glac002      LIKE glac_t.glac002
   DEFINE l_glac004      LIKE glac_t.glac004

   SELECT glac004 INTO l_glac004 FROM glac_t
    WHERE glacent = g_enterprise AND glac001 = p_glac001 AND glac002 = p_glac002
   IF l_glac004 <> p_glac002 THEN
      CALL agli020_dis(p_glac001,l_glac004)
   END IF

   #獲取需要展開的節點坐標
   FOR l_ac = 1 TO g_browser.getLength()
      IF g_browser[l_ac].b_glac001 = p_glac001 AND 
         g_browser[l_ac].b_glac002 = p_glac002 THEN 
         LET g_current_idx = l_ac
         LET g_browser[l_ac].b_exp = TRUE
         EXIT FOR
      END IF     
   END FOR
   IF g_row_index > 0 THEN
      CALL agli020_browser_expand(g_current_idx)
      LET g_browser[g_current_idx].b_isExp = 1
   END IF

END FUNCTION

################################################################################
# Descriptions...: 复制科目参照表
# Memo...........: #160517-00001#2 add
# Usage..........: CALL agli020_copy(p_glac001)
#                  RETURNING 回传参数
# Input parameter: p_glac001      科目参照表号
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 2016/06/13 By 02599
# Modify.........:
################################################################################
PRIVATE FUNCTION agli020_copy(p_glac001)
   DEFINE p_glac001       LIKE glac_t.glac001
   DEFINE sr              RECORD
          glac001         LIKE glac_t.glac001,
          glac001_desc    LIKE ooall_t.ooall004
          END RECORD
   DEFINE l_cnt           LIKE type_t.num5
   
   OPEN WINDOW w_agli020_s01 WITH FORM cl_ap_formpath("agl",'agli020_s01')

   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
     
      INPUT BY NAME sr.glac001,sr.glac001_desc ATTRIBUTE(WITHOUT DEFAULTS)
 
         BEFORE INPUT
         
         AFTER FIELD glac001
            IF NOT cl_null(sr.glac001) THEN 
               #160823-00029#1--add--str--
               IF sr.glac001 = p_glac001 THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "" 
                  LET g_errparam.code   = "agl-00484" 
                  LET g_errparam.popup  = FALSE 
                  CALL cl_err()
                  NEXT FIELD CURRENT
               END IF
               #160823-00029#1--add--end
               #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
               INITIALIZE g_chkparam.* TO NULL
               #設定g_chkparam.*的參數
               LET g_chkparam.arg1 = sr.glac001
               #呼叫檢查存在並帶值的library
               IF NOT cl_chk_exist("v_ooal002_7") THEN
                  #檢查失敗時後續處理
                  LET sr.glac001 = ''
                  LET sr.glac001_desc = ''
                  DISPLAY BY NAME sr.glac001_desc
                  NEXT FIELD CURRENT
               END IF
            END IF
            INITIALIZE g_ref_fields TO NULL
            LET g_ref_fields[1] = sr.glac001
            CALL ap_ref_array2(g_ref_fields,"SELECT ooall004 FROM ooall_t WHERE ooallent='"||g_enterprise||"' AND ooall001='0' AND ooall002=? AND ooall003='"||g_dlang||"'","") RETURNING g_rtn_fields
            LET sr.glac001_desc = '('|| g_rtn_fields[1] || ')'
            DISPLAY BY NAME sr.glac001_desc
         
      ON ACTION controlp INFIELD glac001        
         #開窗i段
 			   INITIALIZE g_qryparam.* TO NULL
         LET g_qryparam.state = 'i'
 			   LET g_qryparam.reqry = FALSE
         LET g_qryparam.default1 = sr.glac001    #給予default值
         CALL q_ooal002_1()                              #呼叫開窗
         LET sr.glac001 = g_qryparam.return1           #將開窗取得的值回傳到變數
         DISPLAY sr.glac001 TO glac001               #顯示到畫面上
         NEXT FIELD glac001                            #返回原欄位
 
      END INPUT
 
      ON ACTION accept
         ACCEPT DIALOG
 
      ON ACTION cancel
         LET INT_FLAG = TRUE
         EXIT DIALOG
 
      ON ACTION close
         LET INT_FLAG = TRUE
         EXIT DIALOG
 
      ON ACTION exit
         LET INT_FLAG = TRUE
         EXIT DIALOG
 
   END DIALOG
   CLOSE WINDOW w_agli020_s01
   IF INT_FLAG = 0 THEN
      LET l_cnt=0
      SELECT COUNT(*) INTO l_cnt FROM glac_t
       WHERE glacent=g_enterprise AND glac001=sr.glac001
      IF cl_null(l_cnt) THEN LET l_cnt=0 END IF
      IF l_cnt > 0 THEN
         IF NOT cl_ask_confirm("agl-00459") THEN
            RETURN
         ELSE
            #判斷是否存在glad_t資料，如果存在，詢問是否刪除
            LET l_cnt=0
            SELECT COUNT(*) INTO l_cnt FROM glad_t
            WHERE gladent=g_enterprise
              AND gladld IN (SELECT glaald FROM glaa_t WHERE glaaent=g_enterprise AND glaa004=sr.glac001)
            IF cl_null(l_cnt) THEN LET l_cnt=0 END IF
            IF l_cnt > 0 THEN
               IF NOT cl_ask_confirm("agl-00281") THEN
                  RETURN
               ELSE
                  #刪除對應帳套科目glad_t資料
                  DELETE FROM glad_t 
                   WHERE gladent=g_enterprise
                     AND gladld=(SELECT glaald FROM glaa_t WHERE glaaent=g_enterprise AND glaa004=sr.glac001)
               END IF
            END IF
            #刪除已存在的科目資料
            DELETE FROM glac_t WHERE glacent=g_enterprise AND glac001=sr.glac001
            #刪除多語言檔
            DELETE FROM glacl_t WHERE glaclent=g_enterprise AND glacl001=sr.glac001
            #刪除提速檔
            DELETE FROM glacs_t WHERE glacsent=g_enterprise AND glacs001=sr.glac001
         END IF
      END IF
      #复制参照表号下的科目资料
      CALL s_transaction_begin()
      DROP TABLE agli020_detail
      #CREATE TEMP TABLE
      SELECT * FROM glac_t
       WHERE glacent = g_enterprise AND glac001 = p_glac001      
        INTO TEMP agli020_detail
 
      #將key修正為調整後   
      UPDATE agli020_detail 
         #更新key欄位
         SET glac001 = sr.glac001
  
      #將資料塞回原table   
      INSERT INTO glac_t SELECT * FROM agli020_detail
      
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "reproduce:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         RETURN
      END IF
      
      #刪除TEMP TABLE
      DROP TABLE agli020_detail
      
      #多語言複製段落
      SELECT * FROM glacl_t
       WHERE glaclent = g_enterprise AND glacl001 = p_glac001      
        INTO TEMP agli020_detail
        
      #將key修正為調整後   
      UPDATE agli020_detail 
         #更新key欄位
         SET glacl001 = sr.glac001
  
      #將資料塞回原table   
      INSERT INTO glacl_t SELECT * FROM agli020_detail
      
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "reproduce:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         RETURN
      END IF
      
      #刪除TEMP TABLE
      DROP TABLE agli020_detail
      #复制提速档
      SELECT * FROM glacs_t
       WHERE glacsent = g_enterprise AND glacs001 = p_glac001      
        INTO TEMP agli020_detail
        
      #將key修正為調整後   
      UPDATE agli020_detail 
         #更新key欄位
         SET glacs001 = sr.glac001
  
      #將資料塞回原table   
      INSERT INTO glacs_t SELECT * FROM agli020_detail
      
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "reproduce:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         RETURN
      END IF
      
      #刪除TEMP TABLE
      DROP TABLE agli020_detail
      
      CALL s_transaction_end('Y','0')
   ELSE
      LET INT_FLAG = FALSE
   END IF

   

END FUNCTION

 
