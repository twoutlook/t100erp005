# Descriptions...: 整批產生帳別科目管理資料
# Memo...........:
# Usage..........: CALL agli020_gen_glad()
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION agli020_gen_glad()
   DEFINE l_ask            LIKE type_t.num5  #TRUE 重新產生，FALSE不更新
   DEFINE l_cnt            LIKE type_t.num5
   DEFINE l_gladld         LIKE glad_t.gladld
   DEFINE l_glac002        LIKE glac_t.glac002
   DEFINE l_glac017        LIKE glac_t.glac017
   DEFINE l_glac018        LIKE glac_t.glac018
   DEFINE l_glac019        LIKE glac_t.glac019
   DEFINE l_glac020        LIKE glac_t.glac020
   DEFINE l_glac021        LIKE glac_t.glac021
   DEFINE l_glac022        LIKE glac_t.glac022
   DEFINE l_glac023        LIKE glac_t.glac023
   DEFINE l_glac025        LIKE glac_t.glac025
   DEFINE l_glac026        LIKE glac_t.glac026
   DEFINE l_glac027        LIKE glac_t.glac027
   DEFINE l_glac028        LIKE glac_t.glac028
   DEFINE l_glac029        LIKE glac_t.glac029
   DEFINE l_glac030        LIKE glac_t.glac030
   DEFINE l_glac031        LIKE glac_t.glac031
   DEFINE l_gladcrtdt      LIKE glad_t.gladcrtdt
   
   IF cl_null(g_glac_m.glac001) THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "-400" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
 
      RETURN
   END IF
   LET l_ask=TRUE
   #(1)判斷使用此科目參照表號的帳別是否已存在對應的glad_t帳別科目資料，如果存在詢問是否覆蓋重寫，TRUE重寫FALSE放棄
   SELECT COUNT(*) INTO l_cnt FROM glac_t,glad_t,glaa_t
    WHERE gladent=glaaent AND gladld=glaald 
      AND glacent=gladent AND glac001=glaa004
      AND glac002=glad001 AND glac001=g_glac_m.glac001
      AND glacent=g_enterprise
   
   IF l_cnt>0 THEN
      IF cl_ask_confirm("agl-00279") THEN
         LET l_ask=TRUE
      ELSE
         LET l_ask=FALSE
      END IF
   ELSE
      #(2)判斷是否存在使用該科目參照表號的帳別資料，如果存在將科目寫到帳別科目資料glad_t中
      SELECT COUNT(*) INTO l_cnt FROM glac_t,glaa_t
       WHERE glacent=glaaent AND glac001=glaa004
         AND glac001=g_glac_m.glac001
         AND glacent=g_enterprise
      IF l_cnt >0 THEN
         LET l_ask=TRUE
      ELSE         
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = "agl-00285" 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         RETURN
      END IF
   END IF
   LET g_sql="SELECT glac002,glac017,glac018,glac019,glac020,glac021,glac022,glac023,",
             "       glac025,glac026,glac027,glac028,glac029,glac030,glac031 ",
             "  FROM glac_t ",
             " WHERE glacent=",g_enterprise," AND glac001='",g_glac_m.glac001,"'",
             "   AND glacstus='Y' "
   PREPARE agli020_gen_pr1 FROM g_sql
   DECLARE agli020_gen_cs1 CURSOR FOR agli020_gen_pr1
   
   LET g_sql="SELECT DISTINCT glaald FROM glaa_t",
             " WHERE glaaent=",g_enterprise," AND glaa004='",g_glac_m.glac001,"'",
             " ORDER BY glaald"
   PREPARE agli020_gen_pr2 FROM g_sql
   DECLARE agli020_gen_cs2 CURSOR FOR agli020_gen_pr2
   
   CALL cl_err_collect_init()
   LET g_success='Y'
   CALL s_transaction_begin()
   LET l_gladcrtdt = cl_get_current()
   
   FOREACH agli020_gen_cs1 INTO l_glac002,l_glac017,l_glac018,l_glac019,l_glac020,l_glac021,l_glac022,l_glac023,
                                l_glac025,l_glac026,l_glac027,l_glac028,l_glac029,l_glac030,l_glac031
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "FOREACH agli020_gen_cs1"
         LET g_errparam.popup = TRUE
         CALL cl_err()
         EXIT FOREACH
      END IF
      #將所有使用此科目參照表號的帳套抓出，以及產生對應的glad_t資料
      FOREACH agli020_gen_cs2 INTO l_gladld
         IF SQLCA.sqlcode THEN
#            CALL cl_errmsg('FOREACH agli020_gen_cs2','','',SQLCA.SQLCODE,1) 
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.SQLCODE
            LET g_errparam.extend = 'FOREACH agli020_gen_cs2'
            LET g_errparam.popup = TRUE
            CALL cl_err()
            LET g_success = 'N' 
            EXIT FOREACH
         END IF
         SELECT COUNT(*) INTO l_cnt FROM glad_t
         WHERE gladent=g_enterprise AND gladld=l_gladld AND glad001=l_glac002
         IF l_cnt>0 THEN
            IF l_ask=TRUE THEN
               UPDATE glad_t SET glad007=l_glac017,
                                 glad008=l_glac018, 
                                 glad009=l_glac019,
                                 glad010=l_glac020, 
                                 glad011=l_glac021,
                                 glad012=l_glac022,
                                 glad013=l_glac023,
                                 glad015=l_glac025,
                                 glad016=l_glac026,
                                 glad027=l_glac027,
                                 glad031=l_glac028,
                                 glad032=l_glac029,
                                 glad033=l_glac030,
                                 glad034=l_glac031,
                                 gladmodid=g_user,
                                 gladmoddt=l_gladcrtdt
               WHERE gladent=g_enterprise AND gladld=l_gladld AND glad001=l_glac002
               IF SQLCA.sqlcode THEN
#                  CALL cl_errmsg('upd glad_t','','',SQLCA.SQLCODE,1)
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = SQLCA.SQLCODE
                  LET g_errparam.extend = 'upd glad_t'
                  LET g_errparam.popup = TRUE
                  CALL cl_err()
                  LET g_success = 'N'
               END IF
            END IF
         ELSE
            #新增glad_t
            INSERT INTO glad_t(gladent,gladld,glad001,glad002,glad003,glad005,
                               glad007,glad008,glad009,glad010,glad011,glad012,
                               glad013,glad015,glad016,glad027,glad031,glad032,glad033,
                               glad017,glad018,glad019,glad020,glad021,
                               glad022,glad023,glad024,glad025,glad026,glad030,glad034,
                               gladstus,gladownid,gladowndp,gladcrtid,gladcrtdt,gladcrtdp)
                         VALUES(g_enterprise,l_gladld,l_glac002,'N','N','N',
                                l_glac017,l_glac018,l_glac019,l_glac020,l_glac021,l_glac022,
                                l_glac023,l_glac025,l_glac026,l_glac027,l_glac028,l_glac029,l_glac030,
                                'N','N','N','N','N',
                                'N','N','N','N','N','N',l_glac031,
                                'Y',g_user,g_dept,g_user,l_gladcrtdt,g_dept)
            IF SQLCA.sqlcode THEN
#               CALL cl_errmsg('ins glad_t','','',SQLCA.SQLCODE,1) 
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = SQLCA.SQLCODE
               LET g_errparam.extend = 'ins glad_t'
               LET g_errparam.popup = TRUE
               CALL cl_err()
               LET g_success = 'N' 
            END IF
         END IF
      END FOREACH
   END FOREACH  
   IF g_success='N' THEN
      CALL cl_err_collect_show()
      CALL s_transaction_end('N','0')
   ELSE
      CALL s_transaction_end('Y','0')
   END IF 
END FUNCTION

