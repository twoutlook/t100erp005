PRIVATE FUNCTION agli020_open_agli030()
   DEFINE l_bookno LIKE glaa_t.glaald
   DEFINE l_legal  LIKE ooef_t.ooef017
   LET l_bookno = ''
   LET l_legal = g_legal
   IF cl_null(g_legal) THEN
      SELECT ooef017 INTO l_legal FROM ooef_t
       WHERE ooefent = g_enterprise AND ooef001 = g_site
   END IF 
   SELECT glaald INTO l_bookno FROM glaa_t
    WHERE glaaent = g_enterprise AND glaacomp =l_legal
      AND glaa014 = 'Y'
   IF NOT cl_null(l_bookno) THEN
      CALL cl_cmdrun("agli030 "||l_bookno)
   ELSE
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'agl-00014'
      LET g_errparam.extend = l_legal
      LET g_errparam.popup = FALSE
      CALL cl_err()

   END IF 
END FUNCTION

