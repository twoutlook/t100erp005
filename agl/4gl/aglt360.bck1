PRIVATE FUNCTION aglt360_glalld_chk(p_glalld)
   DEFINE p_glalld       LIKE glal_t.glalld
   DEFINE l_n            LIKE type_t.num5
   
   #判斷資料是否存在
      
   SELECT count(*) INTO l_n
     FROM glaa_t
    WHERE glaaent = g_enterprise
      AND glaald = p_glalld
      
   IF l_n = 0 THEN 
      LET g_glal_m.glalld_desc = ''
      LET g_glal_m.glalcomp = ''
      LET g_glal_m.glalcomp_desc = ''
      LET g_glal_m.glaa001 = ''
