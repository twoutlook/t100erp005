      DISPLAY g_glal_m.glalcomp TO glaacomp
      DISPLAY g_glal_m.glalcomp_desc TO glalcomp_desc
      DISPLAY g_glal_m.glaa001 TO glaa001
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'ais-00043'
      LET g_errparam.extend = p_glalld
      LET g_errparam.popup = TRUE
      CALL cl_err()

      RETURN FALSE
   END IF 
   
   #判斷資料是否存在
   SELECT count(*) INTO l_n
     FROM glaa_t
    WHERE glaaent = g_enterprise
      AND glaald = p_glalld
      AND glaastus = 'Y'
      
   IF l_n = 0 THEN 
