# 根據起始日期和截止日期計算期數
PRIVATE FUNCTION aglt360_periods_get()
   DEFINE l_n1           LIKE type_t.num5
   DEFINE l_n2           LIKE type_t.num5
   DEFINE l_year_str     LIKE type_t.num5
   DEFINE l_year_end     LIKE type_t.num5
   DEFINE l_glav004_max  LIKE glav_t.glav004
   DEFINE l_glav004_min  LIKE glav_t.glav004
   
   LET l_year_str = YEAR(g_glal_m.glal004)
   LET l_year_end = YEAR(g_glal_m.glal005)
   IF l_year_str = l_year_end THEN 
      LET l_n1 = 0
      SELECT count(distinct glav006) INTO l_n1
        FROM glav_t
       WHERE glavent = g_enterprise
         AND glav001 = g_glaa003
         AND glav002 = l_year_str
         AND glav004 BETWEEN g_glal_m.glal004 AND g_glal_m.glal005
       LET g_glal_m.periods = l_n1  
       DISPLAY g_glal_m.periods TO periods
   END IF
   
   IF l_year_str <> l_year_end THEN   
      LET l_n1 = 0
      SELECT MAX(glav004) INTO l_glav004_max
        FROM glav_t
       WHERE glavent = g_enterprise
         AND glav001 = g_glaa003
         AND glav002 = l_year_str 
      
      SELECT count(distinct glav006) INTO l_n1
        FROM glav_t
       WHERE glavent = g_enterprise
         AND glav001 = g_glaa003
         AND glav002 = l_year_str
         AND glav004 BETWEEN g_glal_m.glal004 AND l_glav004_max
         
      LET l_n2 = 0
      SELECT MIN(glav004) INTO l_glav004_min
        FROM glav_t
       WHERE glavent = g_enterprise
         AND glav001 = g_glaa003
         AND glav002 = l_year_end  
      
      SELECT count(distinct glav006) INTO l_n2
        FROM glav_t
       WHERE glavent = g_enterprise
         AND glav001 = g_glaa003
         AND glav002 = l_year_end
         AND glav004 BETWEEN l_glav004_min AND g_glal_m.glal005  
         
      LET g_glal_m.periods = l_n1 + l_n2 + (l_year_end - l_year_str - 1) * 12
      DISPLAY g_glal_m.periods TO periods
   END IF
END FUNCTION

 
