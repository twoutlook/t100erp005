   #add-point:modify段define name="modify.define_customerization"
   
   #end add-point    
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   DEFINE l_pass           LIKE type_t.num5  #160811-00039#5 add
   #end add-point  
   
   #add-point:Function前置處理  name="modify.pre_function"
   #160811-00039#5--add--str--
   IF g_glad_m.gladld IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
   CALL s_ld_chk_authorization(g_user,g_glad_m.gladld) RETURNING l_pass
   IF l_pass = FALSE THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'agl-00165'
      LET g_errparam.extend = g_glad_m.gladld
      LET g_errparam.popup = TRUE
      CALL cl_err()
   
      RETURN
   END IF
   #160811-00039#5--add--end
   #end add-point
   
   IF g_glad_m.gladld IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL  
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
   LET g_gladld_t = g_glad_m.gladld
 
   CALL s_transaction_begin()
   
   OPEN agli032_cl USING g_enterprise,g_glad_m.gladld
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN agli032_cl:" 
      LET g_errparam.code   =  STATUS 
      LET g_errparam.popup  = TRUE 
      CLOSE agli032_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE agli032_master_referesh USING g_glad_m.gladld INTO g_glad_m.gladld,g_glad_m.gladld_desc
   
   #遮罩相關處理
   LET g_glad_m_mask_o.* =  g_glad_m.*
   CALL agli032_glad_t_mask()
   LET g_glad_m_mask_n.* =  g_glad_m.*
   
   CALL s_transaction_end('Y','0')
 
   CALL agli032_show()
   WHILE TRUE
      LET g_gladld_t = g_glad_m.gladld
 
 
      #add-point:modify段修改前 name="modify.before_input"
      
      #end add-point
      
      CALL agli032_input("u")     #欄位更改
      
      #add-point:modify段修改後 name="modify.after_input"
      
      #end add-point
      
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         LET g_glad_m.* = g_glad_m_t.*
         CALL agli032_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更(更新單身table的key欄位值)
      IF g_glad_m.gladld != g_gladld_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前 name="modify.b_key_update"
         
         #end add-point
         
         #add-point:單頭(偽)修改中 name="modify.m_key_update"
         
         #end add-point
         
 
         
         #add-point:單頭(偽)修改後 name="modify.a_key_update"
         
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("modify,modify_detail,delete,reproduce", TRUE)
   CALL agli032_set_act_visible()
   CALL agli032_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " gladent = " ||g_enterprise|| " AND",
                      " gladld = '", g_glad_m.gladld, "' "
 
   #填到對應位置
   CALL agli032_browser_fill("")
 
   CALL agli032_idx_chk()
 
   CLOSE agli032_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL agli032_msgcentre_notify('modify')
   
END FUNCTION   
 
