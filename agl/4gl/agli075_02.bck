                  LET l_field_keys[01] = 'glfflent'
                  LET l_var_keys_bak[01] = g_enterprise
                  LET l_field_keys[02] = 'glffl001'
                  LET l_var_keys_bak[02] = g_detail_multi_table_t.glffl001
                  LET l_field_keys[03] = 'glffl002'
                  LET l_var_keys_bak[03] = g_detail_multi_table_t.glffl002
                  CALL cl_multitable_delete(l_field_keys,l_var_keys_bak,'glffl_t')
 
 
 
            #add-point:單身同步刪除前(同層table) name="delete.body.a_delete"
            
            #end add-point
            LET g_detail_idx = li_idx
                           INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_glff_d_t.glff001
               LET gs_keys[2] = g_glff_d_t.glff002
 
            #add-point:單身同步刪除中(同層table) name="delete.body.a_delete2"
            
