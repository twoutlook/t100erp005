   CALL cl_mask_set_no_entry()
 
   #add-point:modify段修改前 name="modify.before_input"
   
   #end add-point
 
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #Page1 預設值產生於此處
      INPUT ARRAY g_glff_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_detail_cnt,WITHOUT DEFAULTS, #MAXCOUNT = g_max_rec,
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
 
         #應用 a43 樣板自動產生(Version:4)
         ON ACTION update_item
            LET g_action_choice="update_item"
            IF cl_auth_chk_act("update_item") THEN
               
               #add-point:ON ACTION update_item name="input.detail_input.page1.update_item"
               CALL n_glffl(g_glff001,g_glff_d[l_ac].glff002)
               CALL agli075_02_get_glffl004(g_glff001,g_glff_d[l_ac].glff002) RETURNING g_glff_d[l_ac].glffl004
               DISPLAY BY NAME g_glff_d[l_ac].glffl004
               #END add-point
            END IF
 
 
 
 
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_glff_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL agli075_02_b_fill(g_wc2)
            LET g_detail_cnt = g_glff_d.getLength()
         
         BEFORE ROW
            #add-point:modify段before row name="input.body.before_row2"
            
            #end add-point  
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_cmd = ''
            LET l_ac_t = l_ac 
            LET l_ac = g_detail_idx
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            DISPLAY g_glff_d.getLength() TO FORMONLY.cnt
         
            CALL s_transaction_begin()
            LET g_detail_cnt = g_glff_d.getLength()
            
            IF g_detail_cnt >= l_ac 
               AND g_glff_d[l_ac].glff001 IS NOT NULL
               AND g_glff_d[l_ac].glff002 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_glff_d_t.* = g_glff_d[l_ac].*  #BACKUP
               LET g_glff_d_o.* = g_glff_d[l_ac].*  #BACKUP
               IF NOT agli075_02_lock_b("glff_t") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH agli075_02_bcl INTO g_glff_d[l_ac].glff001,g_glff_d[l_ac].glff002,g_glff_d[l_ac].glff003, 
                      g_glff_d[l_ac].glff004,g_glff_d[l_ac].glffstus,g_glff2_d[l_ac].glff001,g_glff2_d[l_ac].glff002, 
                      g_glff2_d[l_ac].glffownid,g_glff2_d[l_ac].glffowndp,g_glff2_d[l_ac].glffcrtid, 
                      g_glff2_d[l_ac].glffcrtdp,g_glff2_d[l_ac].glffcrtdt,g_glff2_d[l_ac].glffmodid, 
                      g_glff2_d[l_ac].glffmoddt
                  IF SQLCA.sqlcode THEN
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = g_glff_d_t.glff001,":",SQLERRMESSAGE  
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     LET l_lock_sw = "Y"
                  END IF
                  
                  #遮罩相關處理
                  LET g_glff_d_mask_o[l_ac].* =  g_glff_d[l_ac].*
                  CALL agli075_02_glff_t_mask()
                  LET g_glff_d_mask_n[l_ac].* =  g_glff_d[l_ac].*
                  
                  CALL agli075_02_detail_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            CALL agli075_02_set_entry_b(l_cmd)
            CALL agli075_02_set_no_entry_b(l_cmd)
            #add-point:modify段before row name="input.body.before_row"
            #当来源是4.自行输入时，说明栏位开启录入
            IF g_glff_d[l_ac].glff004 = '4' THEN
               CALL cl_set_comp_entry('glffl004',TRUE)
            ELSE
               CALL cl_set_comp_entry('glffl004',FALSE)
            END IF
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
LET g_detail_multi_table_t.glffl001 = g_glff_d[l_ac].glff001
LET g_detail_multi_table_t.glffl002 = g_glff_d[l_ac].glff002
LET g_detail_multi_table_t.glffl003 = g_dlang
LET g_detail_multi_table_t.glffl004 = g_glff_d[l_ac].glffl004
 
 
            #其他table進行lock
            
            INITIALIZE l_var_keys TO NULL 
            INITIALIZE l_field_keys TO NULL 
            LET l_field_keys[01] = 'glfflent'
            LET l_var_keys[01] = g_enterprise
            LET l_field_keys[02] = 'glffl001'
            LET l_var_keys[02] = g_glff_d[l_ac].glff001
            LET l_field_keys[03] = 'glffl002'
            LET l_var_keys[03] = g_glff_d[l_ac].glff002
