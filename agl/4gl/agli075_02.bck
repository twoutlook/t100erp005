            IF s_transaction_chk("N",0) THEN
               CALL s_transaction_begin()
            END IF
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_glff_d_t.* TO NULL
            INITIALIZE g_glff_d_o.* TO NULL
            INITIALIZE g_glff_d[l_ac].* TO NULL 
            #公用欄位給值(單身)
            #應用 a14 樣板自動產生(Version:5)    
      #公用欄位新增給值  
      LET g_glff2_d[l_ac].glffownid = g_user
      LET g_glff2_d[l_ac].glffowndp = g_dept
      LET g_glff2_d[l_ac].glffcrtid = g_user
      LET g_glff2_d[l_ac].glffcrtdp = g_dept 
      LET g_glff2_d[l_ac].glffcrtdt = cl_get_current()
      LET g_glff2_d[l_ac].glffmodid = g_user
      LET g_glff2_d[l_ac].glffmoddt = cl_get_current()
      LET g_glff_d[l_ac].glffstus = ''
 
 
 
            #自定義預設值(單身2)
                  LET g_glff_d[l_ac].glff002 = "0"
      LET g_glff_d[l_ac].glff003 = "1"
      LET g_glff_d[l_ac].glff004 = "1"
      LET g_glff_d[l_ac].glffstus = "Y"
 
            #add-point:modify段before備份 name="input.body.before_bak"
            LET g_glff_d[l_ac].glff001=g_glff001
            SELECT MAX(glff002) INTO g_glff_d[l_ac].glff002 FROM glff_t
             WHERE glffent=g_enterprise AND glff001=g_glff001
