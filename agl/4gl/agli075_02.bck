{<section id="agli075_02.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION agli075_02_b_fill(p_wc2)              #BODY FILL UP
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   
   #end add-point
   DEFINE p_wc2    STRING
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   
   #end add-point
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前 name="b_fill.sql_before"
   LET p_wc2 = p_wc2," AND glff001='",g_glff001,"'"
   #end add-point
 
   LET g_sql = "SELECT  DISTINCT t0.glff001,t0.glff002,t0.glff003,t0.glff004,t0.glffstus,t0.glff001, 
       t0.glff002,t0.glffownid,t0.glffowndp,t0.glffcrtid,t0.glffcrtdp,t0.glffcrtdt,t0.glffmodid,t0.glffmoddt , 
       t1.ooag011 ,t2.ooefl003 ,t3.ooag011 ,t4.ooefl003 ,t5.ooag011 FROM glff_t t0",
               " LEFT JOIN glffl_t ON glfflent = "||g_enterprise||" AND glff001 = glffl001 AND glff002 = glffl002 AND glffl003 = '",g_dlang,"'",
                              " LEFT JOIN ooag_t t1 ON t1.ooagent="||g_enterprise||" AND t1.ooag001=t0.glffownid  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent="||g_enterprise||" AND t2.ooefl001=t0.glffowndp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent="||g_enterprise||" AND t3.ooag001=t0.glffcrtid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent="||g_enterprise||" AND t4.ooefl001=t0.glffcrtdp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=t0.glffmodid  ",
 
               " WHERE t0.glffent= ?  AND  1=1 AND (", p_wc2, ") " 
   #add-point:b_fill段sql wc name="b_fill.sql_wc"
   
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("glff_t"),
                      " ORDER BY t0.glff001,t0.glff002"
   
   #add-point:b_fill段sql之後 name="b_fill.sql_after"
   
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"glff_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE agli075_02_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR agli075_02_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_glff_d.clear()
   CALL g_glff2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_glff_d[l_ac].glff001,g_glff_d[l_ac].glff002,g_glff_d[l_ac].glff003,g_glff_d[l_ac].glff004, 
       g_glff_d[l_ac].glffstus,g_glff2_d[l_ac].glff001,g_glff2_d[l_ac].glff002,g_glff2_d[l_ac].glffownid, 
       g_glff2_d[l_ac].glffowndp,g_glff2_d[l_ac].glffcrtid,g_glff2_d[l_ac].glffcrtdp,g_glff2_d[l_ac].glffcrtdt, 
       g_glff2_d[l_ac].glffmodid,g_glff2_d[l_ac].glffmoddt,g_glff2_d[l_ac].glffownid_desc,g_glff2_d[l_ac].glffowndp_desc, 
       g_glff2_d[l_ac].glffcrtid_desc,g_glff2_d[l_ac].glffcrtdp_desc,g_glff2_d[l_ac].glffmodid_desc
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充 name="b_fill.fill"
      
      #end add-point
      
      CALL agli075_02_detail_show()      
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
 
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   LET g_error_show = 0
   
 
   
   CALL g_glff_d.deleteElement(g_glff_d.getLength())   
   CALL g_glff2_d.deleteElement(g_glff2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_glff_d.getLength()
      LET g_glff2_d[l_ac].glff001 = g_glff_d[l_ac].glff001 
      LET g_glff2_d[l_ac].glff002 = g_glff_d[l_ac].glff002 
 
      #add-point:b_fill段key值相關欄位 name="b_fill.keys.fill"
      
      #end add-point
   END FOR
   
   IF g_cnt > g_glff_d.getLength() THEN
      LET l_ac = g_glff_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_glff_d.getLength()
      LET g_glff_d_mask_o[l_ac].* =  g_glff_d[l_ac].*
      CALL agli075_02_glff_t_mask()
      LET g_glff_d_mask_n[l_ac].* =  g_glff_d[l_ac].*
   END FOR
   
   LET g_glff2_d_mask_o.* =  g_glff2_d.*
   FOR l_ac = 1 TO g_glff2_d.getLength()
      LET g_glff2_d_mask_o[l_ac].* =  g_glff2_d[l_ac].*
      CALL agli075_02_glff_t_mask()
      LET g_glff2_d_mask_n[l_ac].* =  g_glff2_d[l_ac].*
   END FOR
 
   
   LET l_ac = g_cnt
 
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_glff_d.getLength()
   DISPLAY g_detail_idx TO FORMONLY.idx
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE agli075_02_pb
   
END FUNCTION
 
