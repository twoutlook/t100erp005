PRIVATE FUNCTION agli075_02_get_glffl004(p_glff001,p_glff002)
   DEFINE p_glff001       LIKE glff_t.glff001
   DEFINE p_glff002       LIKE glff_t.glff002
   DEFINE r_glffl004      LIKE glffl_t.glffl004
   
   LET r_glffl004=''
   SELECT glffl004 INTO r_glffl004 FROM glffl_t
    WHERE glfflent=g_enterprise AND glffl001=p_glff001 
      AND glffl002=p_glff002 AND glffl003=g_dlang
   RETURN r_glffl004
END FUNCTION

 
