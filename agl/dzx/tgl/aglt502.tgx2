       '',gldpcnfdt,gldppstid,'',gldppstdt,'','','','','','','','','','','','','','','','','','','', 
       '','','','','','','',''", 
                      " FROM gldp_t",
                      " WHERE gldpent= ? AND gldpdocno=? FOR UPDATE"
   #add-point:SQL_define name="main.after_define_sql"
   {<point name="main.after_define_sql"/>}
   #end add-point
