   LET g_forupd_sql = " SELECT gldp005,gldp006,gldp007,gldpld,'',gldp001,'',gldp002,'',gldpdocno,gldpdocdt, 
       gldp003,gldp004,gldp008,gldp011,gldp014,gldp016,gldp009,gldp010,gldp012,gldp013,gldp015,gldpstus, 
