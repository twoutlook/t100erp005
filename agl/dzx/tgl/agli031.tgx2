   EXECUTE agli031_master_referesh USING g_glad_m.gladld,g_glad_m.glad001 INTO g_glad_m.gladld,g_glad_m.glad001, 
       g_glad_m.gladstus,g_glad_m.glad030,g_glad_m.glad002,g_glad_m.glad003,g_glad_m.glad004,g_glad_m.glad005, 
