      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
 
 
      DELETE FROM glad_t 
       WHERE gladent = g_enterprise AND gladld = g_glad_m.gladld 
         AND glad001 = g_glad_m.glad001 
 
 
      #add-point:單頭刪除中 name="delete.head.m_delete"
