PRIVATE FUNCTION agli562_modify()
   #add-point:modify段define name="modify.define_customerization"
   {<point name="modify.define_customerization" edit="c" mark="Y"/>}
   #end add-point    
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   {<point name="modify.define"/>}
   #end add-point  
   
   #add-point:Function前置處理  name="modify.pre_function"
   {<point name="modify.pre_function"/>}
   #end add-point
   
   IF g_glek_m.glekld IS NULL
   OR g_glek_m.glek001 IS NULL
   OR g_glek_m.glek002 IS NULL
   OR g_glek_m.glek003 IS NULL
   OR g_glek_m.glek004 IS NULL
   OR g_glek_m.glek005 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL  
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
   LET g_glekld_t = g_glek_m.glekld
   LET g_glek001_t = g_glek_m.glek001
   LET g_glek002_t = g_glek_m.glek002
   LET g_glek003_t = g_glek_m.glek003
   LET g_glek004_t = g_glek_m.glek004
   LET g_glek005_t = g_glek_m.glek005
 
   CALL s_transaction_begin()
   
   OPEN agli562_cl USING g_enterprise,g_glek_m.glekld,g_glek_m.glek001,g_glek_m.glek002,g_glek_m.glek003,g_glek_m.glek004,g_glek_m.glek005
   IF SQLCA.SQLCODE THEN   #(ver:49)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN agli562_cl:" 
      LET g_errparam.code   = SQLCA.SQLCODE   #(ver:49)
      LET g_errparam.popup  = TRUE 
      CLOSE agli562_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE agli562_master_referesh USING g_glek_m.glekld,g_glek_m.glek001,g_glek_m.glek002,g_glek_m.glek003, 
       g_glek_m.glek004,g_glek_m.glek005 INTO g_glek_m.glekld,g_glek_m.glek001,g_glek_m.glek002,g_glek_m.glek003, 
       g_glek_m.glek004,g_glek_m.glek005,g_glek_m.glek007,g_glek_m.glek010,g_glek_m.glek013
   
   #遮罩相關處理
   LET g_glek_m_mask_o.* =  g_glek_m.*
   CALL agli562_glek_t_mask()
   LET g_glek_m_mask_n.* =  g_glek_m.*
   
   CALL s_transaction_end('Y','0')
 
   CALL agli562_show()
   WHILE TRUE
      LET g_glekld_t = g_glek_m.glekld
      LET g_glek001_t = g_glek_m.glek001
      LET g_glek002_t = g_glek_m.glek002
      LET g_glek003_t = g_glek_m.glek003
      LET g_glek004_t = g_glek_m.glek004
      LET g_glek005_t = g_glek_m.glek005
 
 
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL agli562_input("u")     #欄位更改
      
      #add-point:modify段修改後 name="modify.after_input"
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         LET g_glek_m.* = g_glek_m_t.*
         CALL agli562_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更(更新單身table的key欄位值)
      IF g_glek_m.glekld != g_glekld_t 
      OR g_glek_m.glek001 != g_glek001_t 
      OR g_glek_m.glek002 != g_glek002_t 
      OR g_glek_m.glek003 != g_glek003_t 
      OR g_glek_m.glek004 != g_glek004_t 
      OR g_glek_m.glek005 != g_glek005_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前 name="modify.b_key_update"
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #add-point:單頭(偽)修改中 name="modify.m_key_update"
         {<point name="modify.m_key_update"/>}
         #end add-point
         
 
         
         #add-point:單頭(偽)修改後 name="modify.a_key_update"
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("modify,modify_detail,delete,reproduce", TRUE)
   CALL agli562_set_act_visible()
   CALL agli562_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " glekent = " ||g_enterprise|| " AND",
                      " glekld = '", g_glek_m.glekld, "' "
                      ," AND glek001 = '", g_glek_m.glek001, "' "
                      ," AND glek002 = '", g_glek_m.glek002, "' "
                      ," AND glek003 = '", g_glek_m.glek003, "' "
                      ," AND glek004 = '", g_glek_m.glek004, "' "
                      ," AND glek005 = '", g_glek_m.glek005, "' "
 
   #填到對應位置
   CALL agli562_browser_fill("")
 
   CALL agli562_idx_chk()
 
   CLOSE agli562_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL agli562_msgcentre_notify('modify')
   
END FUNCTION   

