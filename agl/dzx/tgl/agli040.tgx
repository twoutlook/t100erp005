{<section id="agli040.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION agli040_b_fill(p_wc2)              #BODY FILL UP
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   {<point name="b_fill.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE p_wc2            STRING
   DEFINE ls_owndept_list  STRING  #(ver:35) add
   DEFINE ls_ownuser_list  STRING  #(ver:35) add
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   {<point name="b_fill.define"/>}
   #end add-point
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   {<point name="b_fill.pre_function"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前 name="b_fill.sql_before"
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  DISTINCT t0.glcgstus,t0.glcg001,t0.glcg002,t0.glcg003,t0.glcg012,t0.glcg004,t0.glcg005,t0.glcg006,t0.glcg007,t0.glcg008,t0.glcg009,t0.glcg010,t0.glcg011,t0.glcg001,t0.glcg002,t0.glcgownid,t0.glcgowndp,t0.glcgcrtid,t0.glcgcrtdp,t0.glcgcrtdt,t0.glcgmodid,t0.glcgmoddt ,t1.ooag011 ,t2.ooefl003 ,t3.ooag011 ,t4.ooefl003 ,t5.ooag011 FROM glcg_t t0",
               "",
                              " LEFT JOIN ooag_t t1 ON t1.ooagent="||g_enterprise||" AND t1.ooag001=t0.glcgownid  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent="||g_enterprise||" AND t2.ooefl001=t0.glcgowndp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent="||g_enterprise||" AND t3.ooag001=t0.glcgcrtid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent="||g_enterprise||" AND t4.ooefl001=t0.glcgcrtdp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=t0.glcgmodid  ",

               " WHERE t0.glcgent= ?  AND  1=1 AND (", p_wc2, ") "
 
   #(ver:35) ---add start---
      #應用 a68 樣板自動產生(Version:1)
   #若是修改，須視權限加上條件
   IF g_action_choice = "modify" OR g_action_choice = "modify_detail" THEN
      LET ls_owndept_list = NULL
      LET ls_ownuser_list = NULL

      #若有設定部門權限
      CALL cl_get_owndept_list("glcg_t","modify") RETURNING ls_owndept_list
      IF NOT cl_null(ls_owndept_list) THEN
         LET g_sql = g_sql, " AND glcgowndp IN (",ls_owndept_list,")"
      END IF

      #若有設定個人權限
      CALL cl_get_ownuser_list("glcg_t","modify") RETURNING ls_ownuser_list
      IF NOT cl_null(ls_ownuser_list) THEN
         LET g_sql = g_sql," AND glcgownid IN (",ls_ownuser_list,")"
      END IF
   END IF



   #(ver:35) --- add end ---
 
   #add-point:b_fill段sql wc name="b_fill.sql_wc"
   {<point name="b_fill.sql_wc"/>}
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("glcg_t"),
                      " ORDER BY t0.glcg001,t0.glcg002"
   
   #add-point:b_fill段sql之後 name="b_fill.sql_after"
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"glcg_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE agli040_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR agli040_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_glcg_d.clear()
   CALL g_glcg2_d.clear()   

 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_glcg_d[l_ac].glcgstus,g_glcg_d[l_ac].glcg001,g_glcg_d[l_ac].glcg002,g_glcg_d[l_ac].glcg003,g_glcg_d[l_ac].glcg012,g_glcg_d[l_ac].glcg004,g_glcg_d[l_ac].glcg005,g_glcg_d[l_ac].glcg006,g_glcg_d[l_ac].glcg007,g_glcg_d[l_ac].glcg008,g_glcg_d[l_ac].glcg009,g_glcg_d[l_ac].glcg010,g_glcg_d[l_ac].glcg011,g_glcg2_d[l_ac].glcg001,g_glcg2_d[l_ac].glcg002,g_glcg2_d[l_ac].glcgownid,g_glcg2_d[l_ac].glcgowndp,g_glcg2_d[l_ac].glcgcrtid,g_glcg2_d[l_ac].glcgcrtdp,g_glcg2_d[l_ac].glcgcrtdt,g_glcg2_d[l_ac].glcgmodid,g_glcg2_d[l_ac].glcgmoddt,g_glcg2_d[l_ac].glcgownid_desc,g_glcg2_d[l_ac].glcgowndp_desc,g_glcg2_d[l_ac].glcgcrtid_desc,g_glcg2_d[l_ac].glcgcrtdp_desc,g_glcg2_d[l_ac].glcgmodid_desc
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH b_fill_curs:",SQLERRMESSAGE 
         LET g_errparam.code = SQLCA.SQLCODE
         LET g_errparam.popup = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充 name="b_fill.fill"
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL agli040_detail_show()      
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
 
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   LET g_error_show = 0
   

   
   CALL g_glcg_d.deleteElement(g_glcg_d.getLength())   
   CALL g_glcg2_d.deleteElement(g_glcg2_d.getLength())

   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_glcg_d.getLength()
      LET g_glcg2_d[l_ac].glcg001 = g_glcg_d[l_ac].glcg001 
      LET g_glcg2_d[l_ac].glcg002 = g_glcg_d[l_ac].glcg002 

      #add-point:b_fill段key值相關欄位 name="b_fill.keys.fill"
      {<point name="b_fill.keys.fill"/>}
      #end add-point
   END FOR
   
   IF g_cnt > g_glcg_d.getLength() THEN
      LET l_ac = g_glcg_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_glcg_d.getLength()
      LET g_glcg_d_mask_o[l_ac].* =  g_glcg_d[l_ac].*
      CALL agli040_glcg_t_mask()
      LET g_glcg_d_mask_n[l_ac].* =  g_glcg_d[l_ac].*
   END FOR
   
   LET g_glcg2_d_mask_o.* =  g_glcg2_d.*
   FOR l_ac = 1 TO g_glcg2_d.getLength()
      LET g_glcg2_d_mask_o[l_ac].* =  g_glcg2_d[l_ac].*
      CALL agli040_glcg_t_mask()
      LET g_glcg2_d_mask_n[l_ac].* =  g_glcg2_d[l_ac].*
   END FOR

   
   LET l_ac = g_cnt
 
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   {<point name="b_fill.others.fill"/>}
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_glcg_d.getLength()
   DISPLAY g_detail_idx TO FORMONLY.idx
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE agli040_pb
   
END FUNCTION
