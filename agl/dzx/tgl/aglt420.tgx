   #end add-point
   LET g_forupd_sql = " SELECT glceld,'',glcecomp,'','','','',glce001,glce002,glce003,'',glce004,glce006,'',glce005,glcedocno,glcestus,glceownid,'',glceowndp,'',glcecrtid,'',glcecrtdp,'',glcecrtdt,glcemodid,'',glcemoddt,glcecnfid,'',glcecnfdt,'','','','','','','','','','','','','','','','','','','','','','','','','','','','','','','','','','','','',''", 
                      " FROM glce_t",
                      " WHERE glceent= ? AND glceld=? AND glce001=? AND glce002=? FOR UPDATE"
