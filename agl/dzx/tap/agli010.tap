  <section id="agli010.tmp_tbl_chk" ver="5" status="" src="s" readonly="">
    <![CDATA[#+ TEMP TABLE CHK
PRIVATE FUNCTION agli010_tmp_tbl_chk(ps_id,pi_code)
   DEFINE ps_id       STRING
   DEFINE pi_code     LIKE type_t.num10
   DEFINE ps_type     STRING
   DEFINE ls_id       LIKE type_t.chr500
   DEFINE ls_search   LIKE type_t.chr500
   DEFINE ls_type     LIKE type_t.chr500
   DEFINE li_cnt      LIKE type_t.num10
   DEFINE li_code     LIKE type_t.num10   
   #add-point:tmp_tbl_chk段define
   {<point name="tmp_tbl_chk.define" edit="s"/>}
   #end add-point
   #add-point:tmp_tbl_chk段define
   {<point name="tmp_tbl_chk.define_customerization" edit="c"/>}
   #end add-point
   
   LET ls_id = ps_id
  
   IF cl_null(ls_id) THEN
      RETURN TRUE
   END IF
   
   LET g_sql = " SELECT COUNT(*) FROM agli010_tmp ", 
               " WHERE glaald = ? "
 
   PREPARE agli010_get_cnt FROM g_sql
   EXECUTE agli010_get_cnt USING ls_id 
                                     INTO li_cnt
   FREE agli010_get_cnt
 
   IF li_cnt = 0 OR SQLCA.sqlcode THEN
      RETURN TRUE
   ELSE
      #資料已存在, 確定是否需要更新展開值
      LET g_sql = " SELECT exp_code FROM agli010_tmp ",
                  " WHERE glaald = ? " 
 
      PREPARE agli010_chk_exp FROM g_sql
      EXECUTE agli010_chk_exp USING ls_id 
                                        INTO li_code
      FREE agli010_chk_exp
      
      #若新展開值>原展開值則做更新
      IF pi_code > li_code THEN
         LET g_sql = " UPDATE agli010_tmp SET (exp_code) = ('",pi_code,"') ",
                     " WHERE glaald = ? "
         PREPARE agli010_upd_exp FROM g_sql
         EXECUTE agli010_upd_exp USING ls_id 
         FREE agli010_upd_exp
      END IF
      
      RETURN FALSE
   END IF
 
END FUNCTION
]]>
  </section>
  <section id="agli010.ui_dialog" ver="31" status="" src="s" readonly="">
    <![CDATA[#+ 選單功能實際執行處
PRIVATE FUNCTION agli010_ui_dialog()
   DEFINE li_exit      LIKE type_t.num5    #判別是否為離開作業
   DEFINE la_param  RECORD
          prog       STRING,
          actionid   STRING,
          background LIKE type_t.chr1,
          param      DYNAMIC ARRAY OF STRING
                    END RECORD
   #add-point:ui_dialog段define
   {<point name="ui_dialog.define" edit="s"/>}
   #end add-point
   #add-point:ui_dialog段define
   {<point name="ui_dialog.define_customerization" edit="c"/>}
   #end add-point
   
   LET li_exit = FALSE
   LET gwin_curr = ui.Window.getCurrent()
   LET gfrm_curr = gwin_curr.getForm() 
 
   #add-point:ui_dialog段before dialog 
   {<point name="ui_dialog.before_dialog"/>}
   #end add-point
   
   WHILE li_exit = FALSE
 
      IF g_action_choice = "logistics" THEN
         #清除畫面及相關資料
         CLEAR FORM
         CALL g_browser.clear()       
         INITIALIZE g_glaa_m.* TO NULL
