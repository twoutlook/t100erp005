               LET g_current_idx = g_jump
            END IF
            LET g_no_ask = FALSE  
         END IF           
      END IF
   END IF    
   
   #若無資料則離開
   IF g_current_idx = 0 THEN
      RETURN
   END IF
 
   LET g_glaa_m.glaald = g_browser[g_current_idx].b_glaald
   LET g_glaa_m.glaacomp = g_browser[g_current_idx].b_glaacomp
    
