  <section id="agli191.modify" ver="3" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION agli191_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_glab_m.glabld IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE agli191_master_referesh USING g_glab_m.glabld INTO g_glab_m.glabld,g_glab_m.glabld_desc
 
   ERROR ""
  
   LET g_glabld_t = g_glab_m.glabld
 
   CALL s_transaction_begin()
   
   OPEN agli191_cl USING g_enterprise,g_glab_m.glabld
 
   IF STATUS THEN
      CALL cl_err("OPEN agli191_cl:", STATUS, 1)
      CLOSE agli191_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH agli191_cl INTO g_glab_m.glabld,g_glab_m.glabld_desc,g_glab_m.glaastus,g_glab_m.glaacomp,g_glab_m.glaacomp_desc, 
       g_glab_m.glaa004,g_glab_m.glaa004_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_glab_m.glabld,SQLCA.sqlcode,0)
      CLOSE agli191_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL agli191_show()
   WHILE TRUE
      LET g_glabld_t = g_glab_m.glabld
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL agli191_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_glab_m.* = g_glab_m_t.*
         CALL agli191_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_glab_m.glabld != g_glabld_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE glab_t SET glabld = g_glab_m.glabld
 
          WHERE glabent = g_enterprise AND glabld = g_glabld_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("glab_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("glab_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_glab_m.glabld,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE agli191_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_glab_m.glabld,'U')
 
   CALL agli191_b_fill("1=1")
   
END FUNCTION   
]]>
  </section>
