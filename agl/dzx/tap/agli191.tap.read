   #end add-point     
   
   IF g_glab_m.glabld IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE agli191_master_referesh USING g_glab_m.glabld INTO g_glab_m.glabld,g_glab_m.glabld_desc
   
   CALL agli191_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN agli191_cl USING g_enterprise,g_glab_m.glabld
 
   IF STATUS THEN
      CALL cl_err("OPEN agli191_cl:", STATUS, 1)
      CLOSE agli191_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH agli191_cl INTO g_glab_m.glabld,g_glab_m.glabld_desc,g_glab_m.glaastus,g_glab_m.glaacomp,g_glab_m.glaacomp_desc, 
       g_glab_m.glaa004,g_glab_m.glaa004_desc
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_glab_m.glabld,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL agli191_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM glab_t WHERE glabent = g_enterprise AND glabld = g_glab_m.glabld
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("glab_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_glab_d.clear() 
 
     
      CALL agli191_ui_browser_refresh()  
      CALL agli191_ui_headershow()  
      CALL agli191_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL agli191_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL agli191_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE agli191_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_glab_m.glabld,'D')
    
END FUNCTION
