  <point name="function.agli191_insert_glab" order="4" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#+ 單身插入資料
PRIVATE FUNCTION agli191_insert_glab()
DEFINE   l_glab001      LIKE glab_t.glab001
DEFINE   l_glab002      LIKE glab_t.glab002
DEFINE   l_glab003      LIKE glab_t.glab003
DEFINE   l_glaa004      LIKE glaa_t.glaa004

   CALL s_transaction_begin()
   LET g_sql = "SELECT ooia001 FROM ooia_t",
               " WHERE ooiaent = '",g_enterprise,"'",
               "   AND ooia002 = '30'",
               "   AND ooiastus = 'Y'"
   PREPARE ooia001_pre FROM g_sql
   DECLARE ooia001_cur CURSOR FOR ooia001_pre
   FOREACH ooia001_cur INTO l_glab003
      LET l_glab001 = '42'
      LET l_glab002 = '30'
      SELECT glaa004 INTO l_glaa004
        FROM glaa_t
       WHERE glaald = g_glab_m.glabld
      INSERT INTO glab_t(glabent,glabld,glab001,glab002,glab003,glab004)
                  VALUES (g_enterprise,g_glab_m.glabld,l_glab001,l_glab002,l_glab003,l_glaa004)
      IF SQLCA.SQLcode  THEN
         LET g_success = 'N'
      ELSE
         LET g_success = 'Y'
      END IF
   END FOREACH 
   IF g_success = 'N' THEN
      CALL cl_err("glab_t",SQLCA.sqlcode,1)  
      CALL s_transaction_end('N','0') 
   ELSE
      CALL s_transaction_end('Y','0')
       ERROR "INSERT O.K"
   END IF
END FUNCTION]]>
