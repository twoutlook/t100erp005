  <point name="function.agli060_glak004_desc" cite_std="N" status="" ver="1" src="s" new="Y" order="1" mark_hard="N">
<![CDATA[#+ 核算項值名稱顯示
PRIVATE FUNCTION agli060_glak004_desc()
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_glak_d[l_ac].glak004
   CASE g_glak_m.glak001
      WHEN 1                       #營運據點
         CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields                      
      WHEN 2                       #部門管理         
         CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
      WHEN 3                       #利潤成本管理
         CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
      WHEN 4                       #區域管理
         CALL ap_ref_array2(g_ref_fields,"SELECT oocql004 FROM oocql_t WHERE oocqlent='"||g_enterprise||"' AND oocql001='287' AND oocql002=? AND oocql003='"||g_dlang||"'","") RETURNING g_rtn_fields                         
      WHEN 5                       #交易客商
         CALL ap_ref_array2(g_ref_fields,"SELECT pmaal004 FROM pmaal_t WHERE pmaalent='"||g_enterprise||"' AND pmaal001=? AND pmaal002='"||g_dlang||"'","") RETURNING g_rtn_fields                        
      WHEN 6                       #帳款客商
         CALL ap_ref_array2(g_ref_fields,"SELECT pmaal004 FROM pmaal_t WHERE pmaalent='"||g_enterprise||"' AND pmaal001=? AND pmaal002='"||g_dlang||"'","") RETURNING g_rtn_fields
      WHEN 7                       #客群管理
         CALL ap_ref_array2(g_ref_fields,"SELECT oocql004 FROM oocql_t WHERE oocqlent='"||g_enterprise||"' AND oocql001='281' AND oocql002=? AND oocql003='"||g_dlang||"'","") RETURNING g_rtn_fields               
      WHEN 8                       #產品類別管理
         CALL ap_ref_array2(g_ref_fields,"SELECT rtaxl003 FROM rtaxl_t WHERE rtaxlent='"||g_enterprise||"' AND rtaxl001=? AND rtaxl002='"||g_dlang||"'","") RETURNING g_rtn_fields                        
      WHEN 9                       #人員管理
         CALL ap_ref_array2(g_ref_fields,"SELECT oofa011 FROM oofa_t,ooag_t WHERE oofa001 = ooag002 AND ooagent='"||g_enterprise||"' AND ooag001=?","") RETURNING g_rtn_fields                        
      WHEN 10                      #預算管理
         CALL ap_ref_array2(g_ref_fields,"SELECT bgaal003 FROM bgaal_t WHERE bgaalent='"||g_enterprise||"' AND bgaal001=? AND bgaal002='"||g_dlang||"'","") RETURNING g_rtn_fields                         
      WHEN 11                       #專案管理

      WHEN 12                      #WBS
   END CASE   
   LET g_glak_d[l_ac].glak004_desc = '', g_rtn_fields[1] , ''
   DISPLAY BY NAME g_glak_d[l_ac].glak004_desc       
END FUNCTION]]>
</point>
  <point name="function.agli060_glak004_chk" cite_std="N" status="" ver="1" src="s" new="Y" order="2" mark_hard="N">
<![CDATA[#+ 核算項值存在有效性檢查
PRIVATE FUNCTION agli060_glak004_chk()
   IF NOT cl_null(g_glak_d[l_ac].glak004) THEN 
      CASE g_glak_m.glak001
         WHEN 1                       #營運據點
            IF NOT ap_chk_isExist(g_glak_d[l_ac].glak004,"SELECT COUNT(*) FROM ooef_t WHERE ooefent='"||g_enterprise||"' AND ooef001=? ",'aoo-00088',1) THEN RETURN FALSE END IF
            IF NOT ap_chk_isExist(g_glak_d[l_ac].glak004,"SELECT COUNT(*) FROM ooef_t WHERE ooefent='"||g_enterprise||"' AND ooef001=? AND ooefstus = 'Y' ",'aoo-00089',1) THEN RETURN FALSE END IF      
         WHEN 2                       #部門管理
            IF NOT ap_chk_isExist(g_glak_d[l_ac].glak004,"SELECT COUNT(*) FROM ooea_t WHERE ooeaent='"||g_enterprise||"' AND ooea001=? AND ooea016 = 'Y' ",'aim-00058',1) THEN RETURN FALSE END IF
            IF NOT ap_chk_isExist(g_glak_d[l_ac].glak004,"SELECT COUNT(*) FROM ooea_t WHERE ooeaent='"||g_enterprise||"' AND ooea001=? AND ooea016 = 'Y' AND ooeastus = 'Y' ",'aim-00059',1) THEN RETURN FALSE END IF
         WHEN 3                       #利潤成本管理 
            IF NOT ap_chk_isExist(g_glak_d[l_ac].glak004,"SELECT COUNT(*) FROM ooea_t WHERE ooeaent='"||g_enterprise||"' AND ooea001=? AND ooea005 = 'Y' ",'agl-00097',1) THEN RETURN FALSE END IF
            IF NOT ap_chk_isExist(g_glak_d[l_ac].glak004,"SELECT COUNT(*) FROM ooea_t WHERE ooeaent='"||g_enterprise||"' AND ooea001=? AND ooea005 = 'Y' AND ooeastus = 'Y' ",'agl-00097',1) THEN RETURN FALSE END IF
         WHEN 4                       #區域管理
            IF NOT ap_chk_isExist(g_glak_d[l_ac].glak004,"SELECT COUNT(*) FROM oocq_t WHERE oocqent='"||g_enterprise||"' AND oocq001= '287' AND oocq002=? ",'apm-00092',1) THEN RETURN FALSE END IF                         
            IF NOT ap_chk_isExist(g_glak_d[l_ac].glak004,"SELECT COUNT(*) FROM oocq_t WHERE oocqent='"||g_enterprise||"' AND oocq001= '287' AND oocq002=? AND oocqstus= 'Y' ",'apm-00093',1) THEN RETURN FALSE END IF
         WHEN 5                       #交易客商
            IF NOT ap_chk_isExist(g_glak_d[l_ac].glak004,"SELECT COUNT(*) FROM pmaa_t WHERE pmaaent='"||g_enterprise||"' AND pmaa001=? ",'apm-00028',1) THEN RETURN FALSE END IF   
            IF NOT ap_chk_isExist(g_glak_d[l_ac].glak004,"SELECT COUNT(*) FROM pmaa_t WHERE pmaaent='"||g_enterprise||"' AND pmaa001=? AND pmaastus = 'Y'",'apm-00029',1) THEN RETURN FALSE END IF             
         WHEN 6                       #帳款客商            
            IF NOT ap_chk_isExist(g_glak_d[l_ac].glak004,"SELECT COUNT(*) FROM pmaa_t WHERE pmaaent='"||g_enterprise||"' AND pmaa001=? ",'apm-00028',1) THEN RETURN FALSE END IF   
            IF NOT ap_chk_isExist(g_glak_d[l_ac].glak004,"SELECT COUNT(*) FROM pmaa_t WHERE pmaaent='"||g_enterprise||"' AND pmaa001=? AND pmaastus = 'Y'",'apm-00029',1) THEN RETURN FALSE END IF  
         WHEN 7                       #客群管理
            IF NOT ap_chk_isExist(g_glak_d[l_ac].glak004,"SELECT COUNT(*) FROM oocq_t WHERE oocqent='"||g_enterprise||"' AND oocq001= '281' AND oocq002=? ",'axm-00003',1) THEN RETURN FALSE END IF                         
            IF NOT ap_chk_isExist(g_glak_d[l_ac].glak004,"SELECT COUNT(*) FROM oocq_t WHERE oocqent='"||g_enterprise||"' AND oocq001= '281' AND oocq002=? AND oocqstus= 'Y' ",'axm-00004',1) THEN RETURN FALSE END IF                       
         WHEN 8                       #產品類別管理
            IF NOT ap_chk_isExist(g_glak_d[l_ac].glak004,"SELECT COUNT(*) FROM rtax_t WHERE rtaxent='"||g_enterprise||"' AND rtax001=? ",'aoo-00137',1) THEN RETURN FALSE END IF                       
            IF NOT ap_chk_isExist(g_glak_d[l_ac].glak004,"SELECT COUNT(*) FROM rtax_t WHERE rtaxent='"||g_enterprise||"' AND rtax001=?  AND rtaxstus = 'Y' ",'aoo-00138',1) THEN RETURN FALSE END IF 
         WHEN 9                       #人員管理
            IF NOT ap_chk_isExist(g_glak_d[l_ac].glak004,"SELECT COUNT(*) FROM ooag_t WHERE ooagent='"||g_enterprise||"' AND ooag001=? ",'aoo-00074',1) THEN RETURN FALSE END IF 
            IF NOT ap_chk_isExist(g_glak_d[l_ac].glak004,"SELECT COUNT(*) FROM ooag_t WHERE ooagent='"||g_enterprise||"' AND ooag001=? AND ooagstus = 'Y' ",'aoo-00071',1) THEN RETURN FALSE END IF             
         WHEN 10                      #預算管理
            IF NOT ap_chk_isExist(g_glak_d[l_ac].glak004,"SELECT COUNT(*) FROM bgaa_t WHERE bgaaent='"||g_enterprise||"' AND bgaa001=? ",'abg-00007',1) THEN RETURN FALSE END IF 
            IF NOT ap_chk_isExist(g_glak_d[l_ac].glak004,"SELECT COUNT(*) FROM bgaa_t WHERE bgaaent='"||g_enterprise||"' AND bgaa001=? AND bgaastus = 'Y' ",'abg-00008',1) THEN RETURN FALSE END IF                          
         WHEN 11                      #專案管理
            RETURN TRUE
         WHEN 12                      #WBS
            RETURN TRUE 
      END CASE       
   END IF
   RETURN TRUE
END FUNCTION]]>
</point>
