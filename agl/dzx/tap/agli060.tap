  <section id="agli060.delete" ver="6" status="" src="s">
<![CDATA[#+ 資料刪除
PRIVATE FUNCTION agli060_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_glak_m.glakld IS NULL
   OR g_glak_m.glak001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE agli060_master_referesh USING g_glak_m.glakld,g_glak_m.glak001 INTO g_glak_m.glakld,g_glak_m.glak001, 
       g_glak_m.glak002,g_glak_m.glakld_desc
   
   CALL agli060_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN agli060_cl USING g_enterprise,g_glak_m.glakld
                                                       ,g_glak_m.glak001
 
   IF STATUS THEN
      CALL cl_err("OPEN agli060_cl:", STATUS, 1)
      CLOSE agli060_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH agli060_cl INTO g_glak_m.glakld,g_glak_m.glakld_desc,g_glak_m.glaacomp,g_glak_m.glak001,g_glak_m.glak002 

   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_glak_m.glakld,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL agli060_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM glak_t WHERE glakent = g_enterprise AND glakld = g_glak_m.glakld
                                                               AND glak001 = g_glak_m.glak001
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("glak_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_glak_d.clear() 
      CALL g_glak2_d.clear()       
 
     
      CALL agli060_ui_browser_refresh()  
      CALL agli060_ui_headershow()  
      CALL agli060_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL agli060_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL agli060_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE agli060_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_glak_m.glakld,'D')
    
END FUNCTION
]]>
</section>
