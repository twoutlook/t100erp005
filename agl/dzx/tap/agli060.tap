            DISPLAY BY NAME g_glak_d[l_ac].glak003_desc

            #此段落由子樣板a05產生
            IF  NOT cl_null(g_glak_m.glakld) AND NOT cl_null(g_glak_m.glak001) AND NOT cl_null(g_glak_d[l_ac].glak003) AND NOT cl_null(g_glak_d[l_ac].glak004) THEN 
               IF l_cmd = 'a' OR ( l_cmd = 'u' AND ( l_cmd = 'u' AND (g_glak_m.glakld != g_glakld_t OR g_glak_m.glak001 != g_glak001_t OR g_glak_d[l_ac].glak003 != g_glak_d_t.glak003 OR g_glak_d[l_ac].glak004 != g_glak_d_t.glak004))) THEN 
                  IF NOT ap_chk_notDup(g_glak_d[l_ac].glak003,"SELECT COUNT(*) FROM glak_t WHERE "||"glakent = '" ||g_enterprise|| "' AND "||"glakld = '"||g_glak_m.glakld ||"' AND "|| "glak001 = '"||g_glak_m.glak001 ||"' AND "|| "glak003 = '"||g_glak_d[l_ac].glak003 ||"' AND "|| "glak004 = '"||g_glak_d[l_ac].glak004 ||"'",'std-00004',0) THEN 
                     LET g_glak_d[l_ac].glak003 = g_glak_d_t.glak003 
                     LET g_glak_d[l_ac].glak003_desc = ''
                     NEXT FIELD CURRENT
                  END IF
               END IF
            END IF
            
            IF NOT cl_null(g_glak_d[l_ac].glak003) THEN 
               IF NOT ap_chk_isExist(g_glak_d[l_ac].glak003,"SELECT COUNT(*) FROM glad_t WHERE gladld = '"||g_glak_m.glakld||"' AND "||"gladent = '" ||g_enterprise|| "' AND "||"glad001 = ? ",'ais-00045',1) THEN 
                  LET g_glak_d[l_ac].glak003 = g_glak_d_t.glak003 
                  NEXT FIELD CURRENT
               END IF 
               IF NOT ap_chk_isExist(g_glak_d[l_ac].glak003,"SELECT COUNT(*) FROM glad_t WHERE gladld = '"||g_glak_m.glakld||"' AND "||"gladent = '" ||g_enterprise|| "' AND "||"glad001 = ? AND gladstus = 'Y' ",'ais-00046',1) THEN 
                  LET g_glak_d[l_ac].glak003 = g_glak_d_t.glak003 
