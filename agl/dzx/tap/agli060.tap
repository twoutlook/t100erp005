  <section id="agli060.reproduce" ver="5" status="" src="s">
<![CDATA[#+ 資料複製
PRIVATE FUNCTION agli060_reproduce()
   DEFINE l_newno     LIKE glak_t.glakld 
   DEFINE l_oldno     LIKE glak_t.glakld 
   DEFINE l_newno02     LIKE glak_t.glak001 
   DEFINE l_oldno02     LIKE glak_t.glak001 
 
   DEFINE l_master    RECORD LIKE glak_t.*
   DEFINE l_detail    RECORD LIKE glak_t.*
 
   DEFINE l_cnt       LIKE type_t.num5
   #add-point:reproduce段define
   {<point name="reproduce.define"/>}
   #end add-point
 
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF     
 
   IF g_glak_m.glakld IS NULL
      OR g_glak_m.glak001 IS NULL
 
      THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   LET g_glakld_t = g_glak_m.glakld
   LET g_glak001_t = g_glak_m.glak001
 
   
   LET g_glak_m.glakld = ""
   LET g_glak_m.glak001 = ""
 
    
   CALL agli060_set_entry('a')
   CALL agli060_set_no_entry('a')
   
   CALL cl_set_head_visible("","YES")
   
   CALL s_transaction_begin()
   
   #add-point:複製輸入前
   {<point name="reproduce.head.b_input"/>}
   #end add-point
   
