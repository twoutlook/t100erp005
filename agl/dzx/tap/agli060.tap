  <section id="agli060.b_fill" ver="7" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION agli060_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_glak_d.clear()
   CALL g_glak2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE glakstus,glak003,glak004,glak003,glak004,glakmodid,glakmoddt,glakownid, 
       glakowndp,glakcrtid,glakcrtdp,glakcrtdt,t1.oofa011 ,t2.oofa011 ,t3.ooefl003 ,t4.oofa011 ,t5.ooefl003 FROM glak_t", 
          
               "",
               
                              " LEFT JOIN oofa_t t1 ON t1.oofaent='"||g_enterprise||"' AND t1.oofa002='2' AND t1.oofa003=glakmodid  ",
               " LEFT JOIN oofa_t t2 ON t2.oofaent='"||g_enterprise||"' AND t2.oofa002='2' AND t2.oofa003=glakownid  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent='"||g_enterprise||"' AND t3.ooefl001=glakowndp AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t4 ON t4.oofaent='"||g_enterprise||"' AND t4.oofa002='2' AND t4.oofa003=glakcrtid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=glakcrtdp AND t5.ooefl002='"||g_dlang||"' ",
 
               " WHERE glakent= ? AND glakld=? AND glak001=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("glak_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF agli060_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY glak_t.glak003,glak_t.glak004"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE agli060_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR agli060_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_glak_m.glakld,g_glak_m.glak001
                                               
      FOREACH b_fill_cs INTO g_glak_d[l_ac].glakstus,g_glak_d[l_ac].glak003,g_glak_d[l_ac].glak004,g_glak2_d[l_ac].glak003, 
          g_glak2_d[l_ac].glak004,g_glak2_d[l_ac].glakmodid,g_glak2_d[l_ac].glakmoddt,g_glak2_d[l_ac].glakownid, 
          g_glak2_d[l_ac].glakowndp,g_glak2_d[l_ac].glakcrtid,g_glak2_d[l_ac].glakcrtdp,g_glak2_d[l_ac].glakcrtdt, 
          g_glak2_d[l_ac].glakmodid_desc,g_glak2_d[l_ac].glakownid_desc,g_glak2_d[l_ac].glakowndp_desc, 
          g_glak2_d[l_ac].glakcrtid_desc,g_glak2_d[l_ac].glakcrtdp_desc
                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_glak2_d[l_ac].glakownid_desc = cl_get_username(g_glak2_d[l_ac].glakownid)
      #LET g_glak2_d[l_ac].glakowndp_desc = cl_get_deptname(g_glak2_d[l_ac].glakowndp)
      #LET g_glak2_d[l_ac].glakcrtid_desc = cl_get_username(g_glak2_d[l_ac].glakcrtid)
      #LET g_glak2_d[l_ac].glakcrtdp_desc = cl_get_deptname(g_glak2_d[l_ac].glakcrtdp)
      #LET g_glak2_d[l_ac].glakmodid_desc = cl_get_username(g_glak2_d[l_ac].glakmodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_glak_d.deleteElement(g_glak_d.getLength())
      CALL g_glak2_d.deleteElement(g_glak2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE agli060_pb   
   
END FUNCTION
]]>
</section>
