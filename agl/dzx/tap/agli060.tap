   DEFINE  l_allow_insert        LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete        LIKE type_t.num5                #可刪除否  
   DEFINE  l_count               LIKE type_t.num5
   DEFINE  l_i                   LIKE type_t.num5
   DEFINE  l_insert              BOOLEAN
   DEFINE  ls_return             STRING
   DEFINE  l_var_keys            DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys          DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                DYNAMIC ARRAY OF STRING
   DEFINE  l_fields              DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak        DYNAMIC ARRAY OF STRING
   DEFINE  lb_reproduce          BOOLEAN
   DEFINE  li_reproduce          LIKE type_t.num5
   DEFINE  li_reproduce_target   LIKE type_t.num5
   #add-point:input段define
   {<point name="input.define"/>}
   #end add-point    
   
   #先做狀態判定
   IF p_cmd = 'r' THEN
      LET l_cmd_t = 'r'
      LET p_cmd   = 'a'
   ELSE
      LET l_cmd_t = p_cmd
   END IF   
   
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF  
 
   CALL cl_set_head_visible("","YES")  
 
   LET l_insert = FALSE
   LET g_action_choice = ""
 
   #add-point:input段define_sql
   {<point name="input.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT glakstus,glak003,glak004,glak003,glak004,glakmodid,glakmoddt,glakownid, 
       glakowndp,glakcrtid,glakcrtdp,glakcrtdt FROM glak_t WHERE glakent=? AND glakld=? AND glak001=?  
       AND glak003=? AND glak004=? FOR UPDATE"
   #add-point:input段define_sql
   {<point name="input.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE agli060_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR
 
 
   
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   LET g_qryparam.state = 'i'
   
   #控制key欄位可否輸入
   CALL agli060_set_entry(p_cmd)
   #add-point:set_entry後
   {<point name="input.after_set_entry"/>}
   #end add-point
   CALL agli060_set_no_entry(p_cmd)
   #add-point:set_no_entry後
   {<point name="input.after_set_no_entry"/>}
   #end add-point
 
   DISPLAY BY NAME g_glak_m.glakld,g_glak_m.glaacomp,g_glak_m.glak001,g_glak_m.glak002
   
   LET lb_reproduce = FALSE
   
   #add-point:進入修改段前
   {<point name="input.before_input"/>}
   #end add-point
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
]]>
</section>
  <section id="agli060.input.body" ver="7" status="" src="s">
<![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_glak_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂單身ACTION
         
 
         #+ 此段落由子樣板a43產生
         ON ACTION agli060_01
            LET g_action_choice="agli060_01"
            IF cl_auth_chk_act("agli060_01") THEN
               
               #add-point:ON ACTION agli060_01
               {<point name="input.detail_input.page1.agli060_01" />}
               #END add-point
            END IF
 
 
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_glak_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL agli060_b_fill(g_wc2) 
            IF g_rec_b != 0 THEN
               CALL fgl_set_arr_curr(l_ac)
            END IF
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN agli060_cl USING g_enterprise,
                                               g_glak_m.glakld
                                               ,g_glak_m.glak001
 
                                               
               IF STATUS THEN
                  CALL cl_err("OPEN agli060_cl:", STATUS, 1)
                  CLOSE agli060_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_glak_d[l_ac].glak003 IS NOT NULL
               AND g_glak_d[l_ac].glak004 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_glak_d_t.* = g_glak_d[l_ac].*  #BACKUP
               CALL agli060_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL agli060_set_no_entry_b(l_cmd)
               OPEN agli060_bcl USING g_enterprise,g_glak_m.glakld,
                                                g_glak_m.glak001,
 
                                                g_glak_d_t.glak003
                                                ,g_glak_d_t.glak004
 
               IF STATUS THEN
                  CALL cl_err("OPEN agli060_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH agli060_bcl INTO g_glak_d[l_ac].glakstus,g_glak_d[l_ac].glak003,g_glak_d[l_ac].glak004 

                  IF SQLCA.sqlcode THEN
                      CALL cl_err(g_glak_d_t.glak003,SQLCA.sqlcode,1)
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL agli060_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
        
         BEFORE INSERT
            
            INITIALIZE g_glak_d_t.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_glak_d[l_ac].* TO NULL
            
            #公用欄位預設值
            #此段落由子樣板a14產生    
      LET g_glak2_d[l_ac].glakownid = g_user
      LET g_glak2_d[l_ac].glakowndp = g_dept
      LET g_glak2_d[l_ac].glakcrtid = g_user
      LET g_glak2_d[l_ac].glakcrtdp = g_dept 
      LET g_glak2_d[l_ac].glakcrtdt = cl_get_current()
      LET g_glak2_d[l_ac].glakmodid = ""
      LET g_glak2_d[l_ac].glakmoddt = ""
      LET g_glak_d[l_ac].glakstus = ""
 
  
            
            #一般欄位預設值
                  LET g_glak_d[l_ac].glakstus = "Y"
 
            
            
            LET g_glak_d_t.* = g_glak_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL agli060_set_entry_b(l_cmd)
            #add-point:set_entry_b後
            {<point name="input.body.before_insert.set_entry_b"/>}
            #end add-point
            CALL agli060_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_glak_d[li_reproduce_target].* = g_glak_d[li_reproduce].*
               LET g_glak2_d[li_reproduce_target].* = g_glak2_d[li_reproduce].*
 
               LET g_glak_d[g_glak_d.getLength()].glak003 = NULL
               LET g_glak_d[g_glak_d.getLength()].glak004 = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
 
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM glak_t 
             WHERE glakent = g_enterprise AND glakld = g_glak_m.glakld
               AND glak001 = g_glak_m.glak001
 
               AND glak003 = g_glak_d[l_ac].glak003
               AND glak004 = g_glak_d[l_ac].glak004
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               CALL s_transaction_begin()
               #add-point:單身新增前
               {<point name="input.body.b_insert" mark="Y"/>}
               #end add-point
               INSERT INTO glak_t
                           (glakent,
                            glakld,glak001,glak002,
                            glak003,glak004
                            ,glakstus,glakmodid,glakmoddt,glakownid,glakowndp,glakcrtid,glakcrtdp,glakcrtdt) 
                     VALUES(g_enterprise,
                            g_glak_m.glakld,g_glak_m.glak001,g_glak_m.glak002,
                            g_glak_d[l_ac].glak003,g_glak_d[l_ac].glak004
                            ,g_glak_d[l_ac].glakstus,g_glak2_d[l_ac].glakmodid,g_glak2_d[l_ac].glakmoddt, 
                                g_glak2_d[l_ac].glakownid,g_glak2_d[l_ac].glakowndp,g_glak2_d[l_ac].glakcrtid, 
                                g_glak2_d[l_ac].glakcrtdp,g_glak2_d[l_ac].glakcrtdt)
               #add-point:單身新增中
               {<point name="input.body.m_insert"/>}
               #end add-point
               LET p_cmd = 'u'
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_glak_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("glak_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR "INSERT O.K"
               LET g_rec_b=g_rec_b+1
               DISPLAY g_rec_b TO FORMONLY.cnt
            END IF
            
            #add-point:單身新增後
            {<point name="input.body.after_insert"/>}
            #end add-point
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_glak_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_glak_d.deleteElement(l_ac)
               NEXT FIELD glak003
            END IF
            IF g_glak_d_t.glak003 IS NOT NULL
               AND g_glak_d_t.glak004 IS NOT NULL
 
               THEN
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete"/>}
               #end add-point
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               IF agli060_before_delete() THEN 
                  CALL s_transaction_end('Y','0')
               ELSE 
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               END IF 
               CLOSE agli060_bcl
               LET l_count = g_glak_d.getLength()
            END IF 
            
            #add-point:單身刪除後
            {<point name="input.body.a_delete"/>}
            #end add-point
              
         AFTER DELETE 
            #add-point:單身AFTER DELETE 
            {<point name="input.body.after_delete"/>}
            #end add-point
 
                  #此段落由子樣板a01產生
         BEFORE FIELD glakstus
            #add-point:BEFORE FIELD glakstus
            {<point name="input.b.page1.glakstus" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD glakstus
            
            #add-point:AFTER FIELD glakstus
            {<point name="input.a.page1.glakstus" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE glakstus
            #add-point:ON CHANGE glakstus
            {<point name="input.g.page1.glakstus" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD glak003
            
            #add-point:AFTER FIELD glak003
            {<point name="input.a.page1.glak003" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD glak003
            #add-point:BEFORE FIELD glak003
            {<point name="input.b.page1.glak003" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE glak003
            #add-point:ON CHANGE glak003
            {<point name="input.g.page1.glak003" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD glak004
            
            #add-point:AFTER FIELD glak004
            {<point name="input.a.page1.glak004" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD glak004
            #add-point:BEFORE FIELD glak004
            {<point name="input.b.page1.glak004" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE glak004
            #add-point:ON CHANGE glak004
            {<point name="input.g.page1.glak004" />}
            #END add-point
 
 
                  #Ctrlp:input.c.page1.glakstus
         ON ACTION controlp INFIELD glakstus
            #add-point:ON ACTION controlp INFIELD glakstus
            {<point name="input.c.page1.glakstus" />}
            #END add-point
 
         #Ctrlp:input.c.page1.glak003
         ON ACTION controlp INFIELD glak003
            #add-point:ON ACTION controlp INFIELD glak003
            {<point name="input.c.page1.glak003" />}
            #END add-point
 
         #Ctrlp:input.c.page1.glak004
         ON ACTION controlp INFIELD glak004
            #add-point:ON ACTION controlp INFIELD glak004
            {<point name="input.c.page1.glak004" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_glak_d[l_ac].* = g_glak_d_t.*
               CLOSE agli060_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_glak_d[l_ac].glak003,-263,1)
               LET g_glak_d[l_ac].* = g_glak_d_t.*
            ELSE
               #寫入修改者/修改日期資訊
               LET g_glak2_d[l_ac].glakmodid = g_user 
LET g_glak2_d[l_ac].glakmoddt = cl_get_current()
 
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
         
               UPDATE glak_t SET (glakld,glak001,glakstus,glak003,glak004,glakmodid,glakmoddt,glakownid, 
                   glakowndp,glakcrtid,glakcrtdp,glakcrtdt) = (g_glak_m.glakld,g_glak_m.glak001,g_glak_d[l_ac].glakstus, 
                   g_glak_d[l_ac].glak003,g_glak_d[l_ac].glak004,g_glak2_d[l_ac].glakmodid,g_glak2_d[l_ac].glakmoddt, 
                   g_glak2_d[l_ac].glakownid,g_glak2_d[l_ac].glakowndp,g_glak2_d[l_ac].glakcrtid,g_glak2_d[l_ac].glakcrtdp, 
                   g_glak2_d[l_ac].glakcrtdt)
                WHERE glakent = g_enterprise AND glakld = g_glak_m.glakld 
                 AND glak001 = g_glak_m.glak001 
 
                 AND glak003 = g_glak_d_t.glak003 #項次   
                 AND glak004 = g_glak_d_t.glak004  
 
                 
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
               
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("glak_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                  #WHEN SQLCA.sqlcode #其他錯誤
                  #   CALL cl_err("glak_t",SQLCA.sqlcode,1)  
                  #   CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_glak_m.glakld
               LET gs_keys_bak[1] = g_glakld_t
               LET gs_keys[2] = g_glak_m.glak001
               LET gs_keys_bak[2] = g_glak001_t
               LET gs_keys[3] = g_glak_d[g_detail_idx].glak003
               LET gs_keys_bak[3] = g_glak_d_t.glak003
               LET gs_keys[4] = g_glak_d[g_detail_idx].glak004
               LET gs_keys_bak[4] = g_glak_d_t.glak004
               CALL agli060_update_b('glak_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
                     LET g_log1 = util.JSON.stringify(g_glak_m),util.JSON.stringify(g_glak_d_t)
                     LET g_log2 = util.JSON.stringify(g_glak_m),util.JSON.stringify(g_glak_d[l_ac])
                     IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                        CALL s_transaction_end('N','0')
                     END IF
               END CASE
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
            END IF
 
         AFTER INPUT
            #若單身還沒有輸入資料, 強制切換至單身
            IF g_glak_d.getLength() = 0 THEN
               NEXT FIELD glak003
            END IF
            #add-point:input段after input 
            {<point name="input.body.after_input"/>}
            #end add-point    
            
         ON ACTION controlo   
            CALL FGL_SET_ARR_CURR(g_glak_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_glak_d.getLength()+1
            
      END INPUT
 
 
      
      DISPLAY ARRAY g_glak2_d TO s_detail2.* ATTRIBUTES(COUNT=g_rec_b)  
      
         BEFORE ROW
            CALL agli060_b_fill(g_wc2) 
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail2")
            LET l_ac = g_detail_idx
            DISPLAY g_detail_idx TO FORMONLY.idx
            CALL agli060_ui_detailshow()
        
         BEFORE DISPLAY 
            CALL FGL_SET_ARR_CURR(g_detail_idx)      
         
         #add-point:page2自定義行為
         {<point name="input.body2.action"/>}
         #end add-point
         
      END DISPLAY
 
      
 
      
      #add-point:input段more_input
      {<point name="input.more_inputarray"/>}
      #end add-point    
      
      BEFORE DIALOG
         #add-point:input段before_dialog
         {<point name="input.before_dialog"/>}
         #end add-point 
         #新增時強制從單頭開始填
         IF p_cmd = 'a' THEN
            NEXT FIELD glakld
        ELSE
            CASE g_aw
               WHEN "s_detail1"
                  NEXT FIELD glakstus
               WHEN "s_detail2"
                  NEXT FIELD glak003_2
 
            END CASE
         END IF
   
      ON ACTION controlf
         CALL cl_set_focus_form(ui.Interface.getRootNode()) RETURNING g_fld_name,g_frm_name
         CALL cl_fldhelp(g_frm_name,g_fld_name,g_lang)
 
      ON ACTION controlr
         CALL cl_show_req_fields()
 
      ON ACTION controls
         IF g_header_hidden THEN
            CALL gfrm_curr.setElementHidden("vb_master",0)
            CALL gfrm_curr.setElementImage("controls","small/arr-u.png")
            LET g_header_hidden = 0     #visible
         ELSE
            CALL gfrm_curr.setElementHidden("vb_master",1)
            CALL gfrm_curr.setElementImage("controls","small/arr-d.png")
            LET g_header_hidden = 1     #hidden     
         END IF
 
      ON ACTION accept
         ACCEPT DIALOG
        
      ON ACTION cancel      #在dialog button (放棄)
         LET g_action_choice=""
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION close       #在dialog 右上角 (X)
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION exit        #toolbar 離開
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      #交談指令共用ACTION
      &include "common_action.4gl" 
         CONTINUE DIALOG 
   END DIALOG
   
   #add-point:input段after_input
   {<point name="input.after_input"/>}
   #end add-point  
   
END FUNCTION
]]>
</section>
  <section id="agli060.input.head" ver="4" status="" src="s">
<![CDATA[   
      #單頭段
      INPUT BY NAME g_glak_m.glakld,g_glak_m.glaacomp,g_glak_m.glak001,g_glak_m.glak002 
         ATTRIBUTE(WITHOUT DEFAULTS)
         
         #自訂單頭ACTION
         
         
         BEFORE INPUT
            IF s_transaction_chk("N",0) THEN
               CALL s_transaction_begin()
            END IF
            
            IF l_cmd_t = 'r' THEN
               
            END IF
            #add-point:單頭input前
            {<point name="input.head.b_input"/>}
            #end add-point
          
                  #此段落由子樣板a02產生
         AFTER FIELD glakld
            
            #add-point:AFTER FIELD glakld
            {<point name="input.a.glakld" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD glakld
            #add-point:BEFORE FIELD glakld
