               VALUES (g_enterprise,g_glal_m.glalld,g_glal_m.glalcomp,g_glal_m.glal001,g_glal_m.glaldocno, 
                   g_glal_m.glaldocdt,g_glal_m.glal002,g_glal_m.glal003,g_glal_m.glal004,g_glal_m.glal005, 
