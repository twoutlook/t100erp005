   IF NOT aglt380_action_chk() THEN
      CLOSE aglt380_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_glal_m.glalld,g_glal_m.glalld_desc,g_glal_m.glalcomp,g_glal_m.glalcomp_desc,g_glal_m.glaa001, 
       g_glal_m.glal001,g_glal_m.glaldocno,g_glal_m.glaldocdt,g_glal_m.glal002,g_glal_m.glal002_desc, 
       g_glal_m.glal003,g_glal_m.glal004,g_glal_m.glal005,g_glal_m.periods,g_glal_m.glal006,g_glal_m.glal007, 
       g_glal_m.glalstus,g_glal_m.glalownid,g_glal_m.glalownid_desc,g_glal_m.glalowndp,g_glal_m.glalowndp_desc, 
       g_glal_m.glalcrtid,g_glal_m.glalcrtid_desc,g_glal_m.glalcrtdp,g_glal_m.glalcrtdp_desc,g_glal_m.glalcrtdt, 
       g_glal_m.glalmodid,g_glal_m.glalmodid_desc,g_glal_m.glalmoddt,g_glal_m.glalcnfid,g_glal_m.glalcnfid_desc, 
       g_glal_m.glalcnfdt,g_glal_m.glalpstid,g_glal_m.glalpstid_desc,g_glal_m.glalpstdt,g_glal_m.glam007, 
       g_glal_m.glam007_desc,g_glal_m.glam008,g_glal_m.glam008_desc,g_glal_m.glam009,g_glal_m.glam009_desc, 
       g_glal_m.glam010,g_glal_m.glam010_desc,g_glal_m.glam011,g_glal_m.glam011_desc,g_glal_m.glam012, 
       g_glal_m.glam012_desc,g_glal_m.glam013,g_glal_m.glam013_desc,g_glal_m.glam014,g_glal_m.glam014_desc, 
       g_glal_m.glam051,g_glal_m.glam052,g_glal_m.glam052_desc,g_glal_m.glam053,g_glal_m.glam053_desc, 
       g_glal_m.glam015,g_glal_m.glam015_desc,g_glal_m.glam017,g_glal_m.glam017_desc,g_glal_m.glam018, 
       g_glal_m.glam018_desc,g_glal_m.glam019,g_glal_m.glam019_desc,g_glal_m.glam022,g_glal_m.glam022_desc, 
       g_glal_m.glam025,g_glal_m.glam025_desc,g_glal_m.glam054,g_glal_m.glam027,g_glal_m.glam027_desc, 
       g_glal_m.glam020,g_glal_m.glam020_desc,g_glal_m.glam023,g_glal_m.glam023_desc,g_glal_m.glam026, 
