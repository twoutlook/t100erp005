       g_glal_m.glam039,g_glal_m.glam039_desc,g_glal_m.glam032,g_glal_m.glam032_desc,g_glal_m.glam035, 
       g_glal_m.glam035_desc,g_glal_m.glam038,g_glal_m.glam038_desc,g_glal_m.glam058,g_glal_m.glam058_desc, 
       g_glal_m.glam041,g_glal_m.glam041_desc,g_glal_m.glam033,g_glal_m.glam033_desc,g_glal_m.glam036, 
       g_glal_m.glam036_desc,g_glal_m.glam059,g_glal_m.glam059_desc,g_glal_m.glam042,g_glal_m.glam042_desc 

 
   CASE g_glal_m.glalstus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
      WHEN "Y"
