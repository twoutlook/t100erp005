   LET g_wc = cl_replace_str(g_wc, g_wc_filter_t, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON glalld,glaldocno,glal002,glal003,glal004,glal005
                          FROM s_browse[1].b_glalld,s_browse[1].b_glaldocno,s_browse[1].b_glal002,s_browse[1].b_glal003, 
                              s_browse[1].b_glal004,s_browse[1].b_glal005
 
         BEFORE CONSTRUCT
               DISPLAY aglt380_filter_parser('glalld') TO s_browse[1].b_glalld
            DISPLAY aglt380_filter_parser('glaldocno') TO s_browse[1].b_glaldocno
            DISPLAY aglt380_filter_parser('glal002') TO s_browse[1].b_glal002
            DISPLAY aglt380_filter_parser('glal003') TO s_browse[1].b_glal003
            DISPLAY aglt380_filter_parser('glal004') TO s_browse[1].b_glal004
