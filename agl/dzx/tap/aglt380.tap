  <point name="function.aglt380_glalld_chk" order="1" ver="7" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[# glalld帳別欄位檢查
PRIVATE FUNCTION aglt380_glalld_chk(p_glalld)
   DEFINE p_glalld       LIKE glal_t.glalld
   DEFINE l_n            LIKE type_t.num5
   
   #判斷資料是否存在
   LET l_n = 0  
      
   SELECT count(*) INTO l_n
     FROM glaa_t
    WHERE glaaent = g_enterprise
      AND glaald = p_glalld
      
   IF l_n = 0 THEN 
      LET g_glal_m.glalld_desc = ''
      LET g_glal_m.glalcomp = ''
      LET g_glal_m.glalcomp_desc = ''
      LET g_glal_m.glaa001 = ''
      DISPLAY g_glal_m.glalld_desc TO glalld_desc
      DISPLAY g_glal_m.glalcomp TO glaacomp
      DISPLAY g_glal_m.glalcomp_desc TO glaacomp_desc
      DISPLAY g_glal_m.glaa001 TO glaa001
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'ais-00043'
      LET g_errparam.extend = p_glalld
      LET g_errparam.popup = TRUE
      CALL cl_err()

      RETURN FALSE
   END IF 
   
   #判斷資料是否存在
   LET l_n = 0  
   SELECT count(*) INTO l_n
     FROM glaa_t
    WHERE glaaent = g_enterprise
      AND glaald = p_glalld
      AND glaastus = 'Y'
      
   IF l_n = 0 THEN 
      DISPLAY g_glal_m.glalld_desc TO glalld_desc
      DISPLAY g_glal_m.glalcomp TO glaacomp
      DISPLAY g_glal_m.glalcomp_desc TO glaacomp_desc
      DISPLAY g_glal_m.glaa001 TO glaa001
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'ais-00044'
      LET g_errparam.extend = p_glalld
      LET g_errparam.popup = TRUE
      CALL cl_err()

      RETURN FALSE
   END IF 
   
   #帶值
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = p_glalld
   CALL ap_ref_array2(g_ref_fields,"SELECT glaal002 FROM glaal_t WHERE glaalent='"||g_enterprise||"' AND glaalld=? AND glaal001='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_glal_m.glalld_desc = g_rtn_fields[1] 
   DISPLAY BY NAME g_glal_m.glalld_desc 
   
   SELECT glaacomp,glaa001,glaa003,glaa004,glaa013,glaa024 
     INTO g_glal_m.glalcomp,g_glal_m.glaa001,g_glaa003,g_glaa004,g_glaa013,g_glaa024
     FROM glaa_t
    WHERE glaaent = g_enterprise
      AND glaald = p_glalld 
   DISPLAY g_glal_m.glaa001 TO glaa001
   DISPLAY g_glal_m.glalcomp TO glaacomp
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_glal_m.glalcomp
   CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_glal_m.glalcomp_desc = g_rtn_fields[1]
   DISPLAY BY NAME g_glal_m.glalcomp_desc
   
   RETURN TRUE 
END FUNCTION]]>
  </point>
  <point name="function.aglt380_glaldocno_chk" order="2" ver="6" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[# glaldocno分攤單別欄位檢查
PRIVATE FUNCTION aglt380_glaldocno_chk(p_glaldocno)
   DEFINE p_glaldocno     LIKE glal_t.glaldocno 
   DEFINE l_n             LIKE type_t.num5 
      
   #判斷資料是否存在   
   LET l_n = 0   
