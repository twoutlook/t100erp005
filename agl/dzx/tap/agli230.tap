   DEFINE l_detail    RECORD LIKE glbg_t.*
 
 
   {</Local define>}
   #add-point:delete段define
   {<point name="detail_reproduce.define"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE agli230_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE agli230_detail AS ",
                "SELECT * FROM glbg_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO agli230_detail SELECT * FROM glbg_t 
                                         WHERE glbgent = g_enterprise AND glbgld = g_glbgld_t
                                         AND glbg001 = g_glbg001_t
 
                                         AND glbg002 = g_glbg002_t
 
                                         AND glbg003 = g_glbg003_t
 
 
   
   #將key修正為調整後   
   UPDATE agli230_detail 
      #更新key欄位
      SET glbgld = g_glbg_m.glbgld
          , glbg001 = g_glbg_m.glbg001
 
          , glbg002 = g_glbg_m.glbg002
 
          , glbg003 = g_glbg_m.glbg003
 
 
      #更新共用欄位
      #此段落由子樣板a13產生
       , glbgownid = g_user
       , glbgowndp = g_dept
       , glbgcrtid = g_user
       , glbgcrtdp = g_dept 
       , glbgcrtdt = ld_date
       , glbgmodid = "" 
       , glbgmoddt = "" 
      #, glbgstus = "Y"
 
 
                                       
  
   #將資料塞回原table   
   INSERT INTO glbg_t SELECT * FROM agli230_detail
   
   IF SQLCA.sqlcode THEN
      CALL cl_err("reproduce",SQLCA.sqlcode,1)
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE agli230_detail
   
   #add-point:單身複製後1
   {<point name="detail_reproduce.body.table1.a_insert"/>}
   #end add-point
 
 
   
 
   
   #多語言複製段落
   
   
   CALL s_transaction_end('Y','0')
   
   #已新增完, 調整資料內容(修改時使用)
   LET g_glbgld_t = g_glbg_m.glbgld
   LET g_glbg001_t = g_glbg_m.glbg001
 
   LET g_glbg002_t = g_glbg_m.glbg002
 
   LET g_glbg003_t = g_glbg_m.glbg003
 
 
   
   DROP TABLE agli230_detail
   
END FUNCTION
