  <section id="agli230.delete" ver="1" status="" src="s">
    <![CDATA[#+ 資料刪除
PRIVATE FUNCTION agli230_delete()
   {<Local define>}
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   {</Local define>}
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_glbg_m.glbgld IS NULL
   OR g_glbg_m.glbg001 IS NULL
 
   OR g_glbg_m.glbg002 IS NULL
 
   OR g_glbg_m.glbg003 IS NULL
 
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
    SELECT UNIQUE glbgld,glbg002,glbg003,glbg001
 INTO g_glbg_m.glbgld,g_glbg_m.glbg002,g_glbg_m.glbg003,g_glbg_m.glbg001
 FROM glbg_t
 WHERE glbgent = g_enterprise AND glbgld = g_glbg_m.glbgld AND glbg001 = g_glbg_m.glbg001 AND glbg002 = g_glbg_m.glbg002 AND glbg003 = g_glbg_m.glbg003
   
   CALL agli230_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN agli230_cl USING g_enterprise,g_glbg_m.glbgld
                                                       ,g_glbg_m.glbg001
 
                                                       ,g_glbg_m.glbg002
 
                                                       ,g_glbg_m.glbg003
 
 
   IF STATUS THEN
      CALL cl_err("OPEN agli230_cl:", STATUS, 1)
      CLOSE agli230_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH agli230_cl INTO g_glbg_m.glbgld,g_glbg_m.glbgld_desc,g_glbg_m.glaacomp,g_glbg_m.glaacomp_desc, 
       g_glbg_m.glbg002,g_glbg_m.glbg003,g_glbg_m.glbg001,g_glbg_m.glaa001,g_glbg_m.glaa016,g_glbg_m.glaa020 

   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_glbg_m.glbgld,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      INITIALIZE g_doc.* TO NULL         
      LET g_doc.column1 = "glbgld"       
      LET g_doc.value1 = g_glbg_m.glbgld     
      CALL cl_del_doc()    
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM glbg_t WHERE glbgent = g_enterprise AND glbgld = g_glbg_m.glbgld
                                                               AND glbg001 = g_glbg_m.glbg001
 
                                                               AND glbg002 = g_glbg_m.glbg002
 
                                                               AND glbg003 = g_glbg_m.glbg003
 
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("glbg_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_glbg_d.clear() 
      CALL g_glbg2_d.clear()       
 
 
     
      CALL agli230_ui_browser_refresh()  
      CALL agli230_ui_headershow()  
      CALL agli230_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL agli230_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL agli230_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE agli230_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_glbg_m.glbgld,'D')
    
END FUNCTION
]]>
