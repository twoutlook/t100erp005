  <section id="agli230.modify" ver="1" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION agli230_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_glbg_m.glbgld IS NULL
   OR g_glbg_m.glbg001 IS NULL
 
   OR g_glbg_m.glbg002 IS NULL
 
   OR g_glbg_m.glbg003 IS NULL
 
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
    SELECT UNIQUE glbgld,glbg002,glbg003,glbg001
 INTO g_glbg_m.glbgld,g_glbg_m.glbg002,g_glbg_m.glbg003,g_glbg_m.glbg001
 FROM glbg_t
 WHERE glbgent = g_enterprise AND glbgld = g_glbg_m.glbgld AND glbg001 = g_glbg_m.glbg001 AND glbg002 = g_glbg_m.glbg002 AND glbg003 = g_glbg_m.glbg003
 
   ERROR ""
  
   LET g_glbgld_t = g_glbg_m.glbgld
   LET g_glbg001_t = g_glbg_m.glbg001
 
   LET g_glbg002_t = g_glbg_m.glbg002
 
   LET g_glbg003_t = g_glbg_m.glbg003
 
 
   CALL s_transaction_begin()
   
   OPEN agli230_cl USING g_enterprise,g_glbg_m.glbgld
                                                       ,g_glbg_m.glbg001
 
                                                       ,g_glbg_m.glbg002
 
                                                       ,g_glbg_m.glbg003
 
 
   IF STATUS THEN
      CALL cl_err("OPEN agli230_cl:", STATUS, 1)
      CLOSE agli230_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH agli230_cl INTO g_glbg_m.glbgld,g_glbg_m.glbgld_desc,g_glbg_m.glaacomp,g_glbg_m.glaacomp_desc, 
       g_glbg_m.glbg002,g_glbg_m.glbg003,g_glbg_m.glbg001,g_glbg_m.glaa001,g_glbg_m.glaa016,g_glbg_m.glaa020 

 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_glbg_m.glbgld,SQLCA.sqlcode,0)
      CLOSE agli230_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL agli230_show()
   WHILE TRUE
      LET g_glbgld_t = g_glbg_m.glbgld
      LET g_glbg001_t = g_glbg_m.glbg001
 
      LET g_glbg002_t = g_glbg_m.glbg002
 
      LET g_glbg003_t = g_glbg_m.glbg003
 
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL agli230_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_glbg_m.* = g_glbg_m_t.*
         CALL agli230_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_glbg_m.glbgld != g_glbgld_t 
      OR g_glbg_m.glbg001 != g_glbg001_t 
 
      OR g_glbg_m.glbg002 != g_glbg002_t 
 
      OR g_glbg_m.glbg003 != g_glbg003_t 
 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE glbg_t SET glbgld = g_glbg_m.glbgld
                                       , glbg001 = g_glbg_m.glbg001
 
                                       , glbg002 = g_glbg_m.glbg002
 
                                       , glbg003 = g_glbg_m.glbg003
 
 
          WHERE glbgent = g_enterprise AND glbgld = g_glbgld_t
            AND glbg001 = g_glbg001_t
 
            AND glbg002 = g_glbg002_t
 
            AND glbg003 = g_glbg003_t
 
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("glbg_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("glbg_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_glbg_m.glbgld,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE agli230_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_glbg_m.glbgld,'U')
 
   CALL agli230_b_fill("1=1")
   
END FUNCTION   
]]>
  </section>
