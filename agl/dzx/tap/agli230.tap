   FOR l_i =1 TO g_browser.getLength()
      IF g_browser[l_i].b_glbgld = g_glbg_m.glbgld 
         AND g_browser[l_i].b_glbg001 = g_glbg_m.glbg001 
 
         AND g_browser[l_i].b_glbg002 = g_glbg_m.glbg002 
 
