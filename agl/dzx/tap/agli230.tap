      #LET g_glbg2_d[l_ac].glbgcrtid_desc = cl_get_username(g_glbg2_d[l_ac].glbgcrtid)
      #LET g_glbg2_d[l_ac].glbgcrtdp_desc = cl_get_deptname(g_glbg2_d[l_ac].glbgcrtdp)
