                             ,g_glbg_d[l_ac].glbgstus,g_glbg_d[l_ac].glbg004,g_glbg_d[l_ac].glbg005, 
                                 g_glbg_d[l_ac].glbg006,g_glbg_d[l_ac].glbg007,g_glbg_d[l_ac].glbg008, 
                                 g_glbg_d[l_ac].glbg009,g_glbg_d[l_ac].glbg010,g_glbg_d[l_ac].glbg011, 
                                 g_glbg2_d[l_ac].glbg004,g_glbg2_d[l_ac].glbgmodid,g_glbg2_d[l_ac].glbgmodid_desc, 
                                 g_glbg2_d[l_ac].glbgmoddt,g_glbg2_d[l_ac].glbgownid,g_glbg2_d[l_ac].glbgownid_desc, 
                                 g_glbg2_d[l_ac].glbgowndp,g_glbg2_d[l_ac].glbgowndp_desc,g_glbg2_d[l_ac].glbgcrtid, 
                                 g_glbg2_d[l_ac].glbgcrtid_desc,g_glbg2_d[l_ac].glbgcrtdp,g_glbg2_d[l_ac].glbgcrtdp_desc, 
