  <section id="agli220.main" ver="7" status="" src="s">
<![CDATA[#+ 此段落由子樣板a26產生
#OPTIONS SHORT CIRCUIT
#+ 作業開始 
MAIN
   #add-point:main段define
   {<point name="main.define"/>}
   #end add-point   
 
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("agl","")
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT glbfld,'','','',glbf001,glbf002,glbf003,'',glbf004,'','','',''", 
                      " FROM glbf_t",
                      " WHERE glbfent= ? AND glbfld=? AND glbf001=? AND glbf002=? AND glbf003=? AND  
                          glbf004=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE agli220_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE glbfld,glbf001,glbf002,glbf003,glbf004,t1.glaal002 ,t3.glbdl003",
               " FROM glbf_t",
                              " LEFT JOIN glaal_t t1 ON t1.glaalent='"||g_enterprise||"' AND t1.glaalld=glbfld AND t1.glaal001='"||g_dlang||"' ",
               " LEFT JOIN glbdl_t t3 ON t3.glbdlent='"||g_enterprise||"' AND t3.glbdl001=glbf003 AND t3.glbdl002='"||g_dlang||"' ",
 
               " WHERE glbfent = '" ||g_enterprise|| "' AND glbfld = ? AND glbf001 = ? AND glbf002 = ? AND glbf003 = ? AND glbf004 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE agli220_master_referesh FROM g_sql
 
 
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_agli220 WITH FORM cl_ap_formpath("agl",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL agli220_init()   
 
      #進入選單 Menu (="N")
      CALL agli220_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_agli220
      
   END IF 
   
   CLOSE agli220_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
   
END MAIN
 
 
]]>
</section>
  <section id="agli220.modify" ver="5" status="" src="s">
<![CDATA[#+ 資料修改
PRIVATE FUNCTION agli220_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_glbf_m.glbfld IS NULL
   OR g_glbf_m.glbf001 IS NULL
   OR g_glbf_m.glbf002 IS NULL
   OR g_glbf_m.glbf003 IS NULL
   OR g_glbf_m.glbf004 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE agli220_master_referesh USING g_glbf_m.glbfld,g_glbf_m.glbf001,g_glbf_m.glbf002,g_glbf_m.glbf003, 
       g_glbf_m.glbf004 INTO g_glbf_m.glbfld,g_glbf_m.glbf001,g_glbf_m.glbf002,g_glbf_m.glbf003,g_glbf_m.glbf004, 
       g_glbf_m.glbfld_desc,g_glbf_m.glbf003_desc
 
   ERROR ""
  
   LET g_glbfld_t = g_glbf_m.glbfld
   LET g_glbf001_t = g_glbf_m.glbf001
   LET g_glbf002_t = g_glbf_m.glbf002
   LET g_glbf003_t = g_glbf_m.glbf003
   LET g_glbf004_t = g_glbf_m.glbf004
 
   CALL s_transaction_begin()
   
   OPEN agli220_cl USING g_enterprise,g_glbf_m.glbfld
                                                       ,g_glbf_m.glbf001
                                                       ,g_glbf_m.glbf002
                                                       ,g_glbf_m.glbf003
                                                       ,g_glbf_m.glbf004
 
   IF STATUS THEN
      CALL cl_err("OPEN agli220_cl:", STATUS, 1)
      CLOSE agli220_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH agli220_cl INTO g_glbf_m.glbfld,g_glbf_m.glbfld_desc,g_glbf_m.glaacomp,g_glbf_m.glaacomp_desc, 
       g_glbf_m.glbf001,g_glbf_m.glbf002,g_glbf_m.glbf003,g_glbf_m.glbf003_desc,g_glbf_m.glbf004,g_glbf_m.glbf004_desc, 
       g_glbf_m.glaa001,g_glbf_m.glaa016,g_glbf_m.glaa020
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_glbf_m.glbfld,SQLCA.sqlcode,0)
      CLOSE agli220_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL agli220_show()
   WHILE TRUE
      LET g_glbfld_t = g_glbf_m.glbfld
      LET g_glbf001_t = g_glbf_m.glbf001
      LET g_glbf002_t = g_glbf_m.glbf002
      LET g_glbf003_t = g_glbf_m.glbf003
      LET g_glbf004_t = g_glbf_m.glbf004
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL agli220_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_glbf_m.* = g_glbf_m_t.*
         CALL agli220_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_glbf_m.glbfld != g_glbfld_t 
      OR g_glbf_m.glbf001 != g_glbf001_t 
      OR g_glbf_m.glbf002 != g_glbf002_t 
      OR g_glbf_m.glbf003 != g_glbf003_t 
      OR g_glbf_m.glbf004 != g_glbf004_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE glbf_t SET glbfld = g_glbf_m.glbfld
                                       , glbf001 = g_glbf_m.glbf001
                                       , glbf002 = g_glbf_m.glbf002
                                       , glbf003 = g_glbf_m.glbf003
                                       , glbf004 = g_glbf_m.glbf004
 
          WHERE glbfent = g_enterprise AND glbfld = g_glbfld_t
            AND glbf001 = g_glbf001_t
            AND glbf002 = g_glbf002_t
            AND glbf003 = g_glbf003_t
            AND glbf004 = g_glbf004_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("glbf_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("glbf_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_glbf_m.glbfld,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE agli220_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_glbf_m.glbfld,'U')
 
   CALL agli220_b_fill("1=1")
   
END FUNCTION   
]]>
</section>
