  <section id="agli220.delete" ver="7" status="" src="s">
<![CDATA[#+ 資料刪除
PRIVATE FUNCTION agli220_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_glbf_m.glbfld IS NULL
   OR g_glbf_m.glbf001 IS NULL
   OR g_glbf_m.glbf002 IS NULL
   OR g_glbf_m.glbf003 IS NULL
   OR g_glbf_m.glbf004 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE agli220_master_referesh USING g_glbf_m.glbfld,g_glbf_m.glbf001,g_glbf_m.glbf002,g_glbf_m.glbf003, 
       g_glbf_m.glbf004 INTO g_glbf_m.glbfld,g_glbf_m.glbf001,g_glbf_m.glbf002,g_glbf_m.glbf003,g_glbf_m.glbf004, 
       g_glbf_m.glbfld_desc,g_glbf_m.glbf003_desc
   
   CALL agli220_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN agli220_cl USING g_enterprise,g_glbf_m.glbfld
                                                       ,g_glbf_m.glbf001
                                                       ,g_glbf_m.glbf002
                                                       ,g_glbf_m.glbf003
                                                       ,g_glbf_m.glbf004
 
   IF STATUS THEN
      CALL cl_err("OPEN agli220_cl:", STATUS, 1)
      CLOSE agli220_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH agli220_cl INTO g_glbf_m.glbfld,g_glbf_m.glbfld_desc,g_glbf_m.glaacomp,g_glbf_m.glaacomp_desc, 
       g_glbf_m.glbf001,g_glbf_m.glbf002,g_glbf_m.glbf003,g_glbf_m.glbf003_desc,g_glbf_m.glbf004,g_glbf_m.glbf004_desc, 
       g_glbf_m.glaa001,g_glbf_m.glaa016,g_glbf_m.glaa020
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_glbf_m.glbfld,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL agli220_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM glbf_t WHERE glbfent = g_enterprise AND glbfld = g_glbf_m.glbfld
                                                               AND glbf001 = g_glbf_m.glbf001
                                                               AND glbf002 = g_glbf_m.glbf002
                                                               AND glbf003 = g_glbf_m.glbf003
                                                               AND glbf004 = g_glbf_m.glbf004
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("glbf_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_glbf_d.clear() 
      CALL g_glbf2_d.clear()       
 
     
      CALL agli220_ui_browser_refresh()  
      CALL agli220_ui_headershow()  
      CALL agli220_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL agli220_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL agli220_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE agli220_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_glbf_m.glbfld,'D')
    
END FUNCTION
]]>
</section>
