            #add-point:AFTER FIELD glbf003
            {<point name="construct.a.glbf003" />}
            #END add-point
            
 
         #Ctrlp:construct.c.glbf004
         ON ACTION controlp INFIELD glbf004
            #add-point:ON ACTION controlp INFIELD glbf004
            {<point name="construct.c.glbf004" />}
            #END add-point
 
         #此段落由子樣板a01產生
