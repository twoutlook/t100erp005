  <section id="agli220.b_fill" ver="6" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION agli220_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_glbf_d.clear()
   CALL g_glbf2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE glbfstus,glbf005,glbf006,glbf007,glbf008,glbf009,glbf010,glbf011,glbf005, 
       glbfmodid,glbfmoddt,glbfownid,glbfowndp,glbfcrtid,glbfcrtdp,glbfcrtdt,t1.oofa011 ,t2.oofa011 , 
       t3.ooefl003 ,t4.oofa011 ,t5.ooefl003 FROM glbf_t",   
               "",
               
                              " LEFT JOIN oofa_t t1 ON t1.oofaent='"||g_enterprise||"' AND t1.oofa002='2' AND t1.oofa003=glbfmodid  ",
               " LEFT JOIN oofa_t t2 ON t2.oofaent='"||g_enterprise||"' AND t2.oofa002='2' AND t2.oofa003=glbfownid  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent='"||g_enterprise||"' AND t3.ooefl001=glbfowndp AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t4 ON t4.oofaent='"||g_enterprise||"' AND t4.oofa002='2' AND t4.oofa003=glbfcrtid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=glbfcrtdp AND t5.ooefl002='"||g_dlang||"' ",
 
               " WHERE glbfent= ? AND glbfld=? AND glbf001=? AND glbf002=? AND glbf003=? AND glbf004=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("glbf_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF agli220_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY glbf_t.glbf005"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE agli220_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR agli220_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_glbf_m.glbfld,g_glbf_m.glbf001,g_glbf_m.glbf002,g_glbf_m.glbf003,g_glbf_m.glbf004
                                               
      FOREACH b_fill_cs INTO g_glbf_d[l_ac].glbfstus,g_glbf_d[l_ac].glbf005,g_glbf_d[l_ac].glbf006,g_glbf_d[l_ac].glbf007, 
          g_glbf_d[l_ac].glbf008,g_glbf_d[l_ac].glbf009,g_glbf_d[l_ac].glbf010,g_glbf_d[l_ac].glbf011, 
          g_glbf2_d[l_ac].glbf005,g_glbf2_d[l_ac].glbfmodid,g_glbf2_d[l_ac].glbfmoddt,g_glbf2_d[l_ac].glbfownid, 
          g_glbf2_d[l_ac].glbfowndp,g_glbf2_d[l_ac].glbfcrtid,g_glbf2_d[l_ac].glbfcrtdp,g_glbf2_d[l_ac].glbfcrtdt, 
          g_glbf2_d[l_ac].glbfmodid_desc,g_glbf2_d[l_ac].glbfownid_desc,g_glbf2_d[l_ac].glbfowndp_desc, 
          g_glbf2_d[l_ac].glbfcrtid_desc,g_glbf2_d[l_ac].glbfcrtdp_desc
                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_glbf2_d[l_ac].glbfownid_desc = cl_get_username(g_glbf2_d[l_ac].glbfownid)
      #LET g_glbf2_d[l_ac].glbfowndp_desc = cl_get_deptname(g_glbf2_d[l_ac].glbfowndp)
      #LET g_glbf2_d[l_ac].glbfcrtid_desc = cl_get_username(g_glbf2_d[l_ac].glbfcrtid)
      #LET g_glbf2_d[l_ac].glbfcrtdp_desc = cl_get_deptname(g_glbf2_d[l_ac].glbfcrtdp)
      #LET g_glbf2_d[l_ac].glbfmodid_desc = cl_get_username(g_glbf2_d[l_ac].glbfmodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_glbf_d.deleteElement(g_glbf_d.getLength())
      CALL g_glbf2_d.deleteElement(g_glbf2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE agli220_pb   
   
END FUNCTION
]]>
</section>
