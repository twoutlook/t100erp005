         BEFORE FIELD glbf001
            #add-point:BEFORE FIELD glbf001
            {<point name="construct.b.glbf001" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD glbf001
            
            #add-point:AFTER FIELD glbf001
            {<point name="construct.a.glbf001" />}
            #END add-point
            
 
         #Ctrlp:construct.c.glbf001
         ON ACTION controlp INFIELD glbf001
            #add-point:ON ACTION controlp INFIELD glbf001
            {<point name="construct.c.glbf001" />}
            #END add-point
 
         #此段落由子樣板a01產生
