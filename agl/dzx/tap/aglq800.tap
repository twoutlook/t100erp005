  <section id="aglq800.b_fill" ver="10" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aglq800_b_fill()
   {<Local define>}
   DEFINE ls_wc           STRING
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   LET g_sql = "SELECT  UNIQUE glaxdocdt,glaxdocno,glaxseq,glax010,glax046,glax045,'',glax003,glax042, 
       glax041,'',glax052,glax054,glax053,'',glax056,glax058,glax057,'',glax008,glax044,glax043,'','', 
       '','',glax001 FROM glax_t",
 
 
               "",
               " WHERE glaxent= ? AND 1=1 AND ", ls_wc
    
   LET g_sql = g_sql, " ORDER BY glax_t.glaxld,glax_t.glaxdocno,glax_t.glaxseq"
  
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
  
   PREPARE aglq800_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aglq800_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_glax_d.clear()
 
   #add-point:陣列清空
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_glax_d[l_ac].glaxdocdt,g_glax_d[l_ac].glaxdocno,g_glax_d[l_ac].glaxseq, 
       g_glax_d[l_ac].glax010,g_glax_d[l_ac].glax046,g_glax_d[l_ac].glax045,g_glax_d[l_ac].amt,g_glax_d[l_ac].glax003, 
       g_glax_d[l_ac].glax042,g_glax_d[l_ac].glax041,g_glax_d[l_ac].amt1,g_glax_d[l_ac].glax052,g_glax_d[l_ac].glax054, 
       g_glax_d[l_ac].glax053,g_glax_d[l_ac].amt2,g_glax_d[l_ac].glax056,g_glax_d[l_ac].glax058,g_glax_d[l_ac].glax057, 
       g_glax_d[l_ac].amt3,g_glax_d[l_ac].glax008,g_glax_d[l_ac].glax044,g_glax_d[l_ac].glax043,g_glax_d[l_ac].num, 
       g_glax_d[l_ac].glapcrtid,g_glax_d[l_ac].glapcrtid_desc,g_glax_d[l_ac].glapstus,g_glax_d[l_ac].glax001 

      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
      
      LET g_glax_d[l_ac].sel = "N"
      #LET g_glax_d[l_ac].statepic = cl_get_actipic(g_glax_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL aglq800_detail_show("'1'")      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            CALL cl_err( "", 9035, 1 )
         END IF
         EXIT FOREACH
      END IF
      
   END FOREACH
   LET g_error_show = 0
   
   CALL g_glax_d.deleteElement(g_glax_d.getLength())   
   #add-point:陣列長度調整
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
    
   LET g_detail_cnt = l_ac - 1 
   DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE aglq800_pb
   
   LET l_ac = 1
   CALL aglq800_fetch()
   
      CALL aglq800_filter_show('glaxdocdt','b_glaxdocdt')
   CALL aglq800_filter_show('glaxdocno','b_glaxdocno')
   CALL aglq800_filter_show('glaxseq','b_glaxseq')
   CALL aglq800_filter_show('glax010','b_glax010')
   CALL aglq800_filter_show('glax046','b_glax046')
   CALL aglq800_filter_show('glax045','b_glax045')
   CALL aglq800_filter_show('glax003','b_glax003')
   CALL aglq800_filter_show('glax042','b_glax042')
   CALL aglq800_filter_show('glax041','b_glax041')
   CALL aglq800_filter_show('glax052','b_glax052')
   CALL aglq800_filter_show('glax054','b_glax054')
   CALL aglq800_filter_show('glax053','b_glax053')
   CALL aglq800_filter_show('glax056','b_glax056')
   CALL aglq800_filter_show('glax058','b_glax058')
   CALL aglq800_filter_show('glax057','b_glax057')
   CALL aglq800_filter_show('glax008','b_glax008')
   CALL aglq800_filter_show('glax044','b_glax044')
   CALL aglq800_filter_show('glax043','b_glax043')
   CALL aglq800_filter_show('glapcrtid','b_glapcrtid')
   CALL aglq800_filter_show('glapstus','b_glapstus')
   CALL aglq800_filter_show('glax001','b_glax001')
 
   
END FUNCTION
]]>
</section>
