   {<point name="default_search.before"/>}
   #end add-point  
   
   IF g_searchtype = 0 OR cl_null(g_searchtype) THEN
      LET g_searchtype = 3
   END IF 
   
   IF NOT cl_null(g_argv[1]) THEN
      LET ls_wc = ls_wc, " glac001 = '", g_argv[1], "' AND "
