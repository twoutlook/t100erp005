  <point name="function.agli020_open_agli030" order="1" ver="1" cite_std="N" cite_ver="" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#+
PRIVATE FUNCTION agli020_open_agli030()
   DEFINE l_bookno LIKE glaa_t.glaald
   DEFINE l_legal  LIKE ooea_t.ooea002
   LET l_bookno = ''
   LET l_legal = g_legal
   IF cl_null(g_legal) THEN
      SELECT ooea002 INTO l_legal FROM ooea_t
       WHERE ooeaent = g_enterprise AND ooea001 = g_site
   END IF 
   SELECT glaald INTO l_bookno FROM glaa_t
    WHERE glaaent = g_enterprise AND glaacomp =l_legal
      AND glaa014 = 'Y'
   IF NOT cl_null(l_bookno) THEN
      CALL cl_cmdrun("agli030 "||l_bookno)
   ELSE
      CALL cl_err(l_legal,'agl-00014',0)
   END IF 
END FUNCTION]]>
