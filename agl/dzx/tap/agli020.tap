          CALL cl_err("",9001,0)
          EXIT WHILE
      END IF
 
      EXIT WHILE
  
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_glac_m.glac002,g_site) THEN 
      CALL s_transaction_end("N","0")
