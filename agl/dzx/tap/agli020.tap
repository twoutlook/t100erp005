  <section id="agli020.delete" ver="1" status="" src="s">
    <![CDATA[#+ 資料刪除
PRIVATE FUNCTION agli020_delete(l_dialog)
   {<Local define>}
   DEFINE l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE l_vars          DYNAMIC ARRAY OF STRING
   DEFINE l_fields        DYNAMIC ARRAY OF STRING
   DEFINE l_var_keys_bak  DYNAMIC ARRAY OF STRING
   DEFINE l_dialog        ui.DIALOG
   DEFINE li_status       LIKE type_t.num5  #SQL實體資料刪除狀態
   DEFINE li_cnt          LIKE type_t.num5  #查看本階是否有兄弟資料
   {</Local define>}
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point
   
   IF g_glac_m.glac001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   #add-point:delete段before_delete
   {<point name="delete.before_delete"/>}
   #end add-point
 
    SELECT UNIQUE glac001,glac002,glacstus,glac003,glac007,glac012,glac017,glac022,glac018,glac023,glac019, 
        glac024,glac020,glac025,glac027,glac026,glac021,glac004,glac006,glac013,glac005,glac008,glac014, 
        glac011,glac010,glac015,glac016,glac009,glacownid,glacowndp,glaccrtid,glaccrtdp,glaccrtdt,glacmodid, 
        glacmoddt
 INTO g_glac_m.glac001,g_glac_m.glac002,g_glac_m.glacstus,g_glac_m.glac003,g_glac_m.glac007,g_glac_m.glac012, 
     g_glac_m.glac017,g_glac_m.glac022,g_glac_m.glac018,g_glac_m.glac023,g_glac_m.glac019,g_glac_m.glac024, 
     g_glac_m.glac020,g_glac_m.glac025,g_glac_m.glac027,g_glac_m.glac026,g_glac_m.glac021,g_glac_m.glac004, 
     g_glac_m.glac006,g_glac_m.glac013,g_glac_m.glac005,g_glac_m.glac008,g_glac_m.glac014,g_glac_m.glac011, 
     g_glac_m.glac010,g_glac_m.glac015,g_glac_m.glac016,g_glac_m.glac009,g_glac_m.glacownid,g_glac_m.glacowndp, 
     g_glac_m.glaccrtid,g_glac_m.glaccrtdp,g_glac_m.glaccrtdt,g_glac_m.glacmodid,g_glac_m.glacmoddt
 FROM glac_t
 WHERE glacent = g_enterprise AND glac001 = g_glac_m.glac001 AND glac002 = g_glac_m.glac002
    
   CALL agli020_show()
   
   #Transaction開始
   CALL s_transaction_begin()
   
   LET g_master_multi_table_t.glacl001 = g_glac_m.glac001
LET g_master_multi_table_t.glacl002 = g_glac_m.glac002
LET g_master_multi_table_t.glacl004 = g_glac_m.glacl004
LET g_master_multi_table_t.glacl005 = g_glac_m.glacl005
 
 
   OPEN agli020_cl USING g_enterprise,g_glac_m.glac001,g_glac_m.glac002
   IF STATUS THEN
      CALL cl_err("OPEN agli020_cl:", STATUS, 1)
      CLOSE agli020_cl
      CALL s_transaction_end("N","0")
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH agli020_cl INTO g_glac_m.glac001,g_glac_m.glac001_desc,g_glac_m.glac002,g_glac_m.glacl004,g_glac_m.glacl005, 
       g_glac_m.glacstus,g_glac_m.glac003,g_glac_m.glac007,g_glac_m.glac012,g_glac_m.glac017,g_glac_m.glac022, 
       g_glac_m.glac018,g_glac_m.glac023,g_glac_m.glac019,g_glac_m.glac024,g_glac_m.glac020,g_glac_m.glac025, 
       g_glac_m.glac027,g_glac_m.glac026,g_glac_m.glac021,g_glac_m.glac004,g_glac_m.glac006,g_glac_m.glac013, 
       g_glac_m.glac005,g_glac_m.glac008,g_glac_m.glac014,g_glac_m.glac011,g_glac_m.fflabel2_desc,g_glac_m.glac010, 
       g_glac_m.fflabel3_desc,g_glac_m.glac015,g_glac_m.fflabel1_desc,g_glac_m.glac016,g_glac_m.glac009, 
       g_glac_m.glacownid,g_glac_m.glacownid_desc,g_glac_m.glacowndp,g_glac_m.glacowndp_desc,g_glac_m.glaccrtid, 
       g_glac_m.glaccrtid_desc,g_glac_m.glaccrtdp,g_glac_m.glaccrtdp_desc,g_glac_m.glaccrtdt,g_glac_m.glacmodid, 
       g_glac_m.glacmodid_desc,g_glac_m.glacmoddt
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_glac_m.glac002,SQLCA.sqlcode,0)
      CALL s_transaction_end("N","0")
      RETURN
   END IF
 
   #先判斷是否有子節點(hasC) 詢問是否砍除全部
   IF g_browser[g_current_idx].b_hasC THEN
      IF cl_ask_delete_all_node() THEN
         LET li_status = agli020_sql_delete(TRUE)
      ELSE
         LET li_status = FALSE
      END IF
   ELSE
      #如果沒有子節點,詢問是否刪除本筆資料
      IF cl_ask_delete() THEN
         LET li_status = agli020_sql_delete(FALSE)
      ELSE
         LET li_status = FALSE
      END IF
   END IF
 
   #檢查實體砍除是否發生意外中止
   IF NOT li_status THEN
      CALL s_transaction_end("N","0")
      CLOSE agli020_cl
      RETURN
   END IF
 
   #刪除節點與資料內容
   CALL l_dialog.deleteNode("s_browse",g_current_idx)  #deleteNode會順便幫忙做deleteElement
 
   #確認這一階還有沒有兄弟 (有:不異動上階屬性/否:上階hasC及exp設定成0)
   SELECT COUNT(*) INTO li_cnt
     FROM gzwe_t
    WHERE gzwe001 = g_gzwe_m.gzwe001
   IF li_cnt = 0 THEN
      LET g_browser[g_current_idx - 1].b_hasC = 0
      LET g_browser[g_current_idx - 1].b_exp = 0
   END IF
 
   #取得CurrentRow後,將CurrentRow放入右側MainLayout
   CALL agli020_desc_show(g_current_idx -1)
 
   #add-point:單頭刪除後
   {<point name="delete.head.a_delete"/>}
   #end add-point
 
   CLOSE agli020_cl
   CALL s_transaction_end("Y","0")
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_glac_m.glac002,"D")
    
END FUNCTION
]]>
