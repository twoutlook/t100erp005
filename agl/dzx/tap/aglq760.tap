  <point name="query.more_construct" cite_std="N" status="" ver="1" src="s" new="N" mark_hard="N">
<![CDATA[      CONSTRUCT g_wc1 ON glapdocdt,glaqdocno
           FROM s_detail1[1].glapdocdt,s_detail1[1].glaqdocno
                      
