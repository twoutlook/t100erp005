  <section id="agli201.construct" ver="4" status="" src="s">
<![CDATA[#+ QBE資料查詢
PRIVATE FUNCTION agli201_construct()
   DEFINE ls_return   STRING
   DEFINE ls_result   STRING 
   DEFINE ls_wc       STRING 
   #add-point:cs段define
   {<point name="cs.define"/>}
   #end add-point    
 
   #清除畫面上相關資料
   CLEAR FORM                 
   INITIALIZE g_glfc_m.* TO NULL
   CALL g_glfc_d.clear()
   CALL g_glfc2_d.clear()
 
   INITIALIZE g_wc TO NULL
   INITIALIZE g_wc2 TO NULL
   LET g_action_choice = ""
    
   LET g_qryparam.state = 'c'
   
   #add-point:cs段construct外
   {<point name="cs.head.before"/>}
   #end add-point 
   
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
      
      #單頭
      CONSTRUCT BY NAME g_wc ON glfc001,glfcl003,glfc002
 
         BEFORE CONSTRUCT
            #add-point:cs段more_construct
            {<point name="cs.head.before_construct"/>}
            #end add-point 
            
                 #Ctrlp:construct.c.glfc001
         ON ACTION controlp INFIELD glfc001
            #add-point:ON ACTION controlp INFIELD glfc001
            {<point name="construct.c.glfc001" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD glfc001
            #add-point:BEFORE FIELD glfc001
            {<point name="construct.b.glfc001" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD glfc001
            
            #add-point:AFTER FIELD glfc001
            {<point name="construct.a.glfc001" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD glfcl003
            #add-point:BEFORE FIELD glfcl003
            {<point name="construct.b.glfcl003" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD glfcl003
            
            #add-point:AFTER FIELD glfcl003
            {<point name="construct.a.glfcl003" />}
            #END add-point
            
 
         #Ctrlp:construct.c.glfcl003
         ON ACTION controlp INFIELD glfcl003
            #add-point:ON ACTION controlp INFIELD glfcl003
            {<point name="construct.c.glfcl003" />}
            #END add-point
 
         #Ctrlp:construct.c.glfc002
         ON ACTION controlp INFIELD glfc002
            #add-point:ON ACTION controlp INFIELD glfc002
            {<point name="construct.c.glfc002" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD glfc002
            #add-point:BEFORE FIELD glfc002
            {<point name="construct.b.glfc002" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD glfc002
            
            #add-point:AFTER FIELD glfc002
            {<point name="construct.a.glfc002" />}
            #END add-point
            
 
 
         
      END CONSTRUCT
 
      CONSTRUCT g_wc2_table1 ON glfcseq,glfc011,glfc003,glfc004,glfc005,glfc006,glfc007,glfc008,glfc012, 
          glfc009,glfc010,glfcownid,glfcowndp,glfccrtid,glfccrtdp,glfccrtdt,glfcmodid,glfcmoddt
           FROM s_detail1[1].glfcseq,s_detail1[1].glfc011,s_detail1[1].glfc003,s_detail1[1].glfc004, 
               s_detail1[1].glfc005,s_detail1[1].glfc006,s_detail1[1].glfc007,s_detail1[1].glfc008,s_detail1[1].glfc012, 
               s_detail1[1].glfc009,s_detail1[1].glfc010,s_detail2[1].glfcownid,s_detail2[1].glfcowndp, 
               s_detail2[1].glfccrtid,s_detail2[1].glfccrtdp,s_detail2[1].glfccrtdt,s_detail2[1].glfcmodid, 
               s_detail2[1].glfcmoddt
                      
         BEFORE CONSTRUCT
            #add-point:cs段more_construct
            {<point name="cs.body.before_construct"/>}
            #end add-point 
            
         #單身公用欄位開窗相關處理
         #此段落由子樣板a11產生
         ##----<<glfccrtdt>>----
         AFTER FIELD glfccrtdt
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
         
         #----<<glfcmoddt>>----
         AFTER FIELD glfcmoddt
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
         
         #----<<glfccnfdt>>----
         
         #----<<glfcpstdt>>----
 
 
           
         #單身一般欄位開窗相關處理
            
       
      END CONSTRUCT
  
 
  
      #add-point:cs段more_construct
      {<point name="cs.more_construct"/>}
      #end add-point
 
      BEFORE DIALOG
         CALL cl_qbe_init()
         #add-point:ui_dialog段b_dialog
         {<point name="cs.before_dialog"/>}
         #end add-point
      
      #查詢方案列表
      ON ACTION qbe_select
         LET ls_wc = ""
         CALL cl_qbe_list("c") RETURNING ls_wc
    
      #條件儲存為方案
      ON ACTION qbe_save
         CALL cl_qbe_save()
 
      ON ACTION accept
         ACCEPT DIALOG
 
      ON ACTION cancel
         LET INT_FLAG = 1
         EXIT DIALOG 
 
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE DIALOG
   END DIALOG
   
   #add-point:cs段after_construct
   {<point name="cs.after_construct"/>}
   #end add-point
   
   #組合g_wc2
   LET g_wc2 = g_wc2_table1
 
 
 
   
   LET g_current_row = 1
 
   IF INT_FLAG THEN
      RETURN
   END IF
   
   LET g_wc_filter = ""
 
END FUNCTION
]]>
