  <section id="agli201.b_fill" ver="4" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION agli201_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_glfc_d.clear()
   CALL g_glfc2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE glfcseq,glfc011,glfc003,glfc004,glfc005,glfc006,glfc007,glfc008,glfc012, 
       glfc009,glfc010,glfcseq,glfcownid,glfcowndp,glfccrtid,glfccrtdp,glfccrtdt,glfcmodid,glfcmoddt, 
       t1.glacl004 ,t2.glacl004 ,t3.oofa011 ,t4.ooefl003 ,t5.oofa011 ,t6.ooefl003 ,t7.oofa011 FROM glfc_t", 
          
               "",
               
                              " LEFT JOIN glacl_t t1 ON t1.glaclent='"||g_enterprise||"' AND t1.glacl001=glfc002 AND t1.glacl002=glfc003 AND t1.glacl003='"||g_dlang||"' ",
               " LEFT JOIN glacl_t t2 ON t2.glaclent='"||g_enterprise||"' AND t2.glacl001=glfc002 AND t2.glacl002=glfc004 AND t2.glacl003='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t3 ON t3.oofaent='"||g_enterprise||"' AND t3.oofa002='2' AND t3.oofa003=glfcownid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=glfcowndp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t5 ON t5.oofaent='"||g_enterprise||"' AND t5.oofa002='2' AND t5.oofa003=glfccrtid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent='"||g_enterprise||"' AND t6.ooefl001=glfccrtdp AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t7 ON t7.oofaent='"||g_enterprise||"' AND t7.oofa002='2' AND t7.oofa003=glfcmodid  ",
 
               " WHERE glfcent= ? AND glfc001=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("glfc_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF agli201_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY glfc_t.glfcseq"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE agli201_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR agli201_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_glfc_m.glfc001
                                               
      FOREACH b_fill_cs INTO g_glfc_d[l_ac].glfcseq,g_glfc_d[l_ac].glfc011,g_glfc_d[l_ac].glfc003,g_glfc_d[l_ac].glfc004, 
          g_glfc_d[l_ac].glfc005,g_glfc_d[l_ac].glfc006,g_glfc_d[l_ac].glfc007,g_glfc_d[l_ac].glfc008, 
          g_glfc_d[l_ac].glfc012,g_glfc_d[l_ac].glfc009,g_glfc_d[l_ac].glfc010,g_glfc2_d[l_ac].glfcseq, 
          g_glfc2_d[l_ac].glfcownid,g_glfc2_d[l_ac].glfcowndp,g_glfc2_d[l_ac].glfccrtid,g_glfc2_d[l_ac].glfccrtdp, 
          g_glfc2_d[l_ac].glfccrtdt,g_glfc2_d[l_ac].glfcmodid,g_glfc2_d[l_ac].glfcmoddt,g_glfc_d[l_ac].glfc003_desc, 
          g_glfc_d[l_ac].glfc004_desc,g_glfc2_d[l_ac].glfcownid_desc,g_glfc2_d[l_ac].glfcowndp_desc, 
          g_glfc2_d[l_ac].glfccrtid_desc,g_glfc2_d[l_ac].glfccrtdp_desc,g_glfc2_d[l_ac].glfcmodid_desc 

                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_glfc2_d[l_ac].glfcownid_desc = cl_get_username(g_glfc2_d[l_ac].glfcownid)
      #LET g_glfc2_d[l_ac].glfcowndp_desc = cl_get_deptname(g_glfc2_d[l_ac].glfcowndp)
      #LET g_glfc2_d[l_ac].glfccrtid_desc = cl_get_username(g_glfc2_d[l_ac].glfccrtid)
      #LET g_glfc2_d[l_ac].glfccrtdp_desc = cl_get_deptname(g_glfc2_d[l_ac].glfccrtdp)
      #LET g_glfc2_d[l_ac].glfcmodid_desc = cl_get_username(g_glfc2_d[l_ac].glfcmodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_glfc_d.deleteElement(g_glfc_d.getLength())
      CALL g_glfc2_d.deleteElement(g_glfc2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE agli201_pb   
   
END FUNCTION
]]>
</section>
