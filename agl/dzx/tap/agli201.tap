  <section id="agli201.detail_reproduce" ver="2" status="" src="s">
<![CDATA[#+ 單身自動複製
PRIVATE FUNCTION agli201_detail_reproduce()
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE glfc_t.*
 
 
   #add-point:delete段define
   {<point name="detail_reproduce.define"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE agli201_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE agli201_detail AS ",
                "SELECT * FROM glfc_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO agli201_detail SELECT * FROM glfc_t 
                                         WHERE glfcent = g_enterprise AND glfc001 = g_glfc001_t
 
   
   #將key修正為調整後   
   UPDATE agli201_detail 
      #更新key欄位
      SET glfc001 = g_glfc_m.glfc001
 
      #更新共用欄位
      #此段落由子樣板a13產生
       , glfcownid = g_user
       , glfcowndp = g_dept
       , glfccrtid = g_user
       , glfccrtdp = g_dept 
       , glfccrtdt = ld_date
       , glfcmodid = "" 
       , glfcmoddt = "" 
 
 
                                       
  
   #將資料塞回原table   
   INSERT INTO glfc_t SELECT * FROM agli201_detail
   
   IF SQLCA.sqlcode THEN
      CALL cl_err("reproduce",SQLCA.sqlcode,1)
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE agli201_detail
   
   #add-point:單身複製後1
   {<point name="detail_reproduce.body.table1.a_insert"/>}
   #end add-point
 
 
   
 
   
   #多語言複製段落
   
   
   CALL s_transaction_end('Y','0')
   
   #已新增完, 調整資料內容(修改時使用)
   LET g_glfc001_t = g_glfc_m.glfc001
 
   
   DROP TABLE agli201_detail
   
END FUNCTION
]]>
