  <point name="function.agli201_glfc003_desc" cite_std="N" status="" ver="1" src="s" new="Y" order="1" mark_hard="N">
<![CDATA[
################################################################################
# Descriptions...: 科目說明
# Memo...........:
# Usage..........: CALL agli201_glfc003_desc(p_glfc003)
#                  RETURNING r_desc
# Input parameter: p_glfc003      科目編號
# Return code....: r_desc         說明
# Date & Author..: 2014/5/12 By wangrr
# Modify.........:
################################################################################
PRIVATE FUNCTION agli201_glfc003_desc(p_glfc003)
   DEFINE p_glfc003    LIKE glfc_t.glfc003
   DEFINE r_desc       LIKE glacl_t.glacl004
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_glfc_m.glfc002
   LET g_ref_fields[2] = p_glfc003
   CALL ap_ref_array2(g_ref_fields,"SELECT glacl004 FROM glacl_t WHERE glaclent='"||g_enterprise||"' AND glacl001=? AND glacl002=? AND glacl003='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET r_desc = '', g_rtn_fields[1] , ''
   RETURN r_desc
END FUNCTION]]>
</point>
  <point name="function.agli201_glfc003_chk" cite_std="N" status="" ver="1" src="s" new="Y" order="2" mark_hard="N">
<![CDATA[
################################################################################
# Descriptions...: p_glfc003
# Memo...........:
# Usage..........: CALL agli201_glfc003_chk(p_glfc003)
# Input parameter: p_glfc003      科目編號
# Date & Author..: 2014/5/12 By wangrr
# Modify.........:
################################################################################
PRIVATE FUNCTION agli201_glfc003_chk(p_glfc003)
   DEFINE p_glfc003    LIKE glfc_t.glfc003
   DEFINE l_glacstus   LIKE glac_t.glacstus
   DEFINE l_glac003    LIKE glac_t.glac003
   
   LET g_errno = ''
   SELECT glacstus,glac003 INTO l_glacstus,l_glac003
     FROM glac_t
    WHERE glacent = g_enterprise
      AND glac001 = g_glfc_m.glfc002  #会计科目参照表号
      AND glac002 = p_glfc003
       
   CASE
      WHEN SQLCA.SQLCODE = 100   LET g_errno = 'agl-00011'
      WHEN l_glacstus = 'N'      LET g_errno = 'agl-00012' 
      WHEN l_glac003 = '1'       LET g_errno = 'agl-00013'  #必须为非统治类科目      
   END CASE   
END FUNCTION]]>
</point>
