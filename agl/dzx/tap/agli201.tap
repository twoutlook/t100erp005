      WHEN '21' #自由核算項九
         CALL q_glar032()
      WHEN '22' #自由核算項十
         CALL q_glar033()
   END CASE
END FUNCTION]]>
</point>
  <point name="function.agli201_get_formula" cite_std="N" status="" ver="1" src="s" new="Y" order="5" mark_hard="N">
<![CDATA[
################################################################################
# Descriptions...: 產生公式樣式
# Memo...........:
# Usage..........: CALL agli201_get_formula()
# Date & Author..: 2014/5/13 By wangrr
# Modify.........:
################################################################################
PRIVATE FUNCTION agli201_get_formula()
   DEFINE l_sql          STRING
   DEFINE l_str          STRING
   DEFINE l_i            LIKE type_t.num5
   DEFINE l_glfc009      LIKE glfc_t.glfc009
   DEFINE l_glfc011      LIKE glfc_t.glfc011
   DEFINE l_glfc012      LIKE glfc_t.glfc012
   
   LET l_sql="SELECT glfc009,glfc011,glfc012 FROM glfc_t ",
             " WHERE glfcent=",g_enterprise,
             "   AND glfc001='",g_glfc_m.glfc001,"'",
             "   AND glfc002='",g_glfc_m.glfc002,"'",
             " ORDER BY glfcseq "
   PREPARE agli201_sel_pr FROM l_sql
   DECLARE agli201_sel_cs CURSOR FOR agli201_sel_pr
   LET l_i=1
   LET l_str=''
   FOREACH agli201_sel_cs INTO l_glfc009,l_glfc011,l_glfc012
      IF SQLCA.SQLCODE THEN
         CALL cl_err('FOREACH agli201_sel_cs',SQLCA.SQLCODE,1)
         EXIT FOREACH
      END IF
      #左括號
      IF l_glfc011='Y' THEN
         LET l_str=l_str,' ( '
      END IF
      #金額
      LET l_str=l_str,"amt",l_i USING '<<'
      #右括號
      IF l_glfc012='Y' THEN
         LET l_str=l_str," ) "
      END IF
      #預算符
      CASE l_glfc009
         WHEN '1' LET l_str=l_str," + "
         WHEN '2' LET l_str=l_str," - "
         WHEN '3' LET l_str=l_str," * "
         WHEN '4' LET l_str=l_str," / "
      END CASE
      LET l_i=l_i+1
   END FOREACH
   LET g_glfc_m.formula=l_str
END FUNCTION]]>
</point>
