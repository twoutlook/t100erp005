################################################################################
PRIVATE FUNCTION agli201_open_glfc006()
   CASE g_glfc_d[l_ac].glfc005
      WHEN '1' #營運據點
         LET g_qryparam.where = " ooed001 = '2' AND ooed004 IN (SELECT DISTINCT glar012 FROM glar_t WHERE glarent=",g_enterprise,")"
