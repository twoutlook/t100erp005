      WHEN '3' #利潤/成本中心
         LET g_qryparam.where = " ooed001 = '6' AND ooed004 IN (SELECT DISTINCT glar014 FROM glar_t WHERE glarent=",g_enterprise,")"
         CALL q_ooed004_1()
      WHEN '4' #區域
         LET g_qryparam.arg1 = '287'
         LET g_qryparam.where = " oocq002 IN (SELECT DISTINCT glar015 FROM glar_t WHERE glarent=",g_enterprise,")"
         CALL q_oocq002()
      WHEN '5' #交易客商
         LET g_qryparam.where = " pmaa001 IN (SELECT DISTINCT glar016 FROM glar_t WHERE glarent=",g_enterprise,")"
         CALL q_pmaa001() 
      WHEN '6' #帳款客商
         LET g_qryparam.where = " pmaa001 IN (SELECT DISTINCT glar017 FROM glar_t WHERE glarent=",g_enterprise,")"
         CALL q_pmaa001()
      WHEN '7' #客群
         LET g_qryparam.arg1 = '281'
         LET g_qryparam.where = " oocq002 IN (SELECT DISTINCT glar018 FROM glar_t WHERE glarent=",g_enterprise,")"
         CALL q_oocq002()
      WHEN '8' #產品類別
         LET g_qryparam.where = " rtax001 IN (SELECT DISTINCT glar019 FROM glar_t WHERE glarent=",g_enterprise,")"
         CALL q_rtax001_1()
      WHEN '9' #人員
         LET g_qryparam.where = " ooag001 IN (SELECT DISTINCT glar020 FROM glar_t WHERE glarent=",g_enterprise,")"
         CALL q_ooag001_2() 
      WHEN '10' #預算編號
         LET g_qryparam.where = " bgaa001 IN (SELECT DISTINCT glar021 FROM glar_t WHERE glarent=",g_enterprise,")"
         CALL q_bgaa001()
      WHEN '11' #專案編號
         CALL q_glar022()
      WHEN '12' #WBS
         CALL q_glar023()
      WHEN '13' #自由核算項一
         CALL q_glar024()
      WHEN '14' #自由核算項二
         CALL q_glar025()
      WHEN '15' #自由核算項三
