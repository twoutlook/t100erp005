  <section id="agli201.delete" ver="4" status="" src="s">
<![CDATA[#+ 資料刪除
PRIVATE FUNCTION agli201_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_glfc_m.glfc001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE agli201_master_referesh USING g_glfc_m.glfc001 INTO g_glfc_m.glfc001,g_glfc_m.glfc002,g_glfc_m.glfc002_desc 

   
   CALL agli201_show()
   
   CALL s_transaction_begin()
   
   LET g_master_multi_table_t.glfcl001 = g_glfc_m.glfc001
LET g_master_multi_table_t.glfcl003 = g_glfc_m.glfcl003
 
 
   OPEN agli201_cl USING g_enterprise,g_glfc_m.glfc001
 
   IF STATUS THEN
      CALL cl_err("OPEN agli201_cl:", STATUS, 1)
      CLOSE agli201_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH agli201_cl INTO g_glfc_m.glfc001,g_glfc_m.formula,g_glfc_m.glfcl003,g_glfc_m.glfc002,g_glfc_m.glfc002_desc 

   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_glfc_m.glfc001,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL agli201_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM glfc_t WHERE glfcent = g_enterprise AND glfc001 = g_glfc_m.glfc001
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("glfc_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      INITIALIZE l_var_keys_bak TO NULL 
   INITIALIZE l_field_keys   TO NULL 
   LET l_var_keys_bak[01] = g_master_multi_table_t.glfcl001
   LET l_field_keys[01] = 'glfcl001'
   LET l_var_keys_bak[02] = g_dlang
   LET l_field_keys[02] = 'glfcl002'
   CALL cl_multitable_delete(l_field_keys,l_var_keys_bak,'glfcl_t')
 
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_glfc_d.clear() 
      CALL g_glfc2_d.clear()       
 
     
      CALL agli201_ui_browser_refresh()  
      CALL agli201_ui_headershow()  
      CALL agli201_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL agli201_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL agli201_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE agli201_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_glfc_m.glfc001,'D')
    
END FUNCTION
]]>
</section>
