  <point name="function.agli041_range_chk" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#+ 起始位數與截止位數的大小控管
PRIVATE FUNCTION agli041_range_chk()
DEFINE l_str  STRING
DEFINE l_cnt  LIKE type_t.num5
   LET l_cnt = 0
   SELECT dzeb008 INTO l_cnt FROM dzeb_t WHERE dzeb001 = g_glae_d[l_ac].glae003 AND dzeb002= g_glae_d[l_ac].glae004
   
   IF NOT cl_null(g_glae_d[l_ac].glae007)  THEN 
      IF g_glae_d[l_ac].glae007 > l_cnt THEN
         CALL cl_err(g_glae_d[l_ac].glae007,'agl-00058',1)
         RETURN FALSE
      END IF
   END IF
   IF NOT cl_null(g_glae_d[l_ac].glae007) AND NOT cl_null(g_glae_d[l_ac].glae008) THEN 
      IF g_glae_d[l_ac].glae008 > l_cnt OR g_glae_d[l_ac].glae008 < g_glae_d[l_ac].glae007  THEN
         CALL cl_err(g_glae_d[l_ac].glae008,'agl-00059',1)
         RETURN FALSE
      END IF
   END IF   
   RETURN TRUE
END FUNCTION]]>
