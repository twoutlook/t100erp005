  <section id="agli041.b_fill" ver="6" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION agli041_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2           STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE glaestus,glae001,'',glae002,glae003,'',glae004,'',glae005,'',glae006, 
       '',glae007,glae008,glae009,'',glae001,glaemodid,'',glaemoddt,glaeownid,'',glaeowndp,'',glaecrtid, 
       '',glaecrtdp,'',glaecrtdt FROM glae_t",
               " LEFT JOIN glael_t ON glae001 = glael001 AND glael002 = '",g_lang,"'",
               " WHERE glaeent= ? AND 1=1 AND ", p_wc2 
    
   LET g_sql = g_sql, #,cl_get_extra_cond('zzuser', 'zzgrup') 
                      " ORDER BY glae_t.glae001"
  
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
  
   PREPARE agli041_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR agli041_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_glae_d.clear()
   CALL g_glae2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_glae_d[l_ac].glaestus,g_glae_d[l_ac].glae001,g_glae_d[l_ac].glael003,g_glae_d[l_ac].glae002, 
       g_glae_d[l_ac].glae003,g_glae_d[l_ac].glae003_desc,g_glae_d[l_ac].glae004,g_glae_d[l_ac].glae004_desc, 
       g_glae_d[l_ac].glae005,g_glae_d[l_ac].glae005_desc,g_glae_d[l_ac].glae006,g_glae_d[l_ac].glae006_desc, 
       g_glae_d[l_ac].glae007,g_glae_d[l_ac].glae008,g_glae_d[l_ac].glae009,g_glae_d[l_ac].glae009_desc, 
       g_glae2_d[l_ac].glae001,g_glae2_d[l_ac].glaemodid,g_glae2_d[l_ac].glaemodid_desc,g_glae2_d[l_ac].glaemoddt, 
       g_glae2_d[l_ac].glaeownid,g_glae2_d[l_ac].glaeownid_desc,g_glae2_d[l_ac].glaeowndp,g_glae2_d[l_ac].glaeowndp_desc, 
       g_glae2_d[l_ac].glaecrtid,g_glae2_d[l_ac].glaecrtid_desc,g_glae2_d[l_ac].glaecrtdp,g_glae2_d[l_ac].glaecrtdp_desc, 
       g_glae2_d[l_ac].glaecrtdt
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL agli041_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec THEN
      IF g_error_show = 1 THEN
         CALL cl_err("glae_t",9035,1)
      END IF
   END IF 
   LET g_error_show = 0
   
 
  
   
   CALL g_glae_d.deleteElement(g_glae_d.getLength())   
   CALL g_glae2_d.deleteElement(g_glae2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_glae_d.getLength()
      LET g_glae2_d[l_ac].glae001 = g_glae_d[l_ac].glae001 
 
   END FOR
   
   IF g_cnt > g_glae_d.getLength() THEN
      LET l_ac = g_glae_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = g_glae_d.getLength() 
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE agli041_pb
   
END FUNCTION
]]>
  </section>
  <section id="agli041.default_search" ver="3" status="" src="s">
    <![CDATA[#+ 外部參數搜尋
PRIVATE FUNCTION agli041_default_search()
   DEFINE li_idx  LIKE type_t.num5
   DEFINE li_cnt  LIKE type_t.num5
   DEFINE ls_wc   STRING
   #add-point:default_search段define
   {<point name="default_search.define"/>}
   #end add-point  
   
   IF NOT cl_null(g_argv[1]) THEN
      LET ls_wc = ls_wc, " glae001 = '", g_argv[1], "' AND "
   END IF
   
 
 
   #add-point:default_search段after sql
   {<point name="default_search.after_sql"/>}
   #end add-point  
   
   IF NOT cl_null(ls_wc) THEN
      LET ls_wc = ls_wc.subString(1,ls_wc.getLength()-5)
      LET g_wc2 = ls_wc
   ELSE
      LET g_wc2 = " 1=1"
   END IF
 
END FUNCTION
]]>
