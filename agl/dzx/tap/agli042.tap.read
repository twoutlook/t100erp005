   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT glaf001,''", 
                      " FROM glaf_t",
                      " WHERE glafent= ? AND glaf001=? FOR UPDATE"
   #add-point:SQL_define
