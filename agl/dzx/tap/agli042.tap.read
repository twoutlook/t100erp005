  <section id="agli042.modify" ver="3" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION agli042_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_glaf_m.glaf001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE agli042_master_referesh USING g_glaf_m.glaf001 INTO g_glaf_m.glaf001,g_glaf_m.glaf001_desc 

 
   ERROR ""
  
   LET g_glaf001_t = g_glaf_m.glaf001
 
   CALL s_transaction_begin()
   
   OPEN agli042_cl USING g_enterprise,g_glaf_m.glaf001
 
   IF STATUS THEN
      CALL cl_err("OPEN agli042_cl:", STATUS, 1)
      CLOSE agli042_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH agli042_cl INTO g_glaf_m.glaf001,g_glaf_m.glaf001_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_glaf_m.glaf001,SQLCA.sqlcode,0)
      CLOSE agli042_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL agli042_show()
   WHILE TRUE
      LET g_glaf001_t = g_glaf_m.glaf001
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL agli042_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_glaf_m.* = g_glaf_m_t.*
         CALL agli042_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_glaf_m.glaf001 != g_glaf001_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE glaf_t SET glaf001 = g_glaf_m.glaf001
 
          WHERE glafent = g_enterprise AND glaf001 = g_glaf001_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("glaf_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("glaf_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_glaf_m.glaf001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE agli042_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_glaf_m.glaf001,'U')
 
   CALL agli042_b_fill("1=1")
   
END FUNCTION   
]]>
  </section>
