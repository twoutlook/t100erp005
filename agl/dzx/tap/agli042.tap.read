       glafcrtdp,glafcrtdt,t1.oofa011 ,t2.oofa011 ,t3.ooefl003 ,t4.oofa011 ,t5.ooefl003 FROM glaf_t", 
          
               " LEFT JOIN glafl_t ON glaf001 = glafl001 AND glaf002 = glafl002 AND glafl003 = '",g_lang,"'",
               
                              " LEFT JOIN oofa_t t1 ON t1.oofaent='"||g_enterprise||"' AND t1.oofa002='2' AND t1.oofa003=glafmodid  ",
               " LEFT JOIN oofa_t t2 ON t2.oofaent='"||g_enterprise||"' AND t2.oofa002='2' AND t2.oofa003=glafownid  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent='"||g_enterprise||"' AND t3.ooefl001=glafowndp AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t4 ON t4.oofaent='"||g_enterprise||"' AND t4.oofa002='2' AND t4.oofa003=glafcrtid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=glafcrtdp AND t5.ooefl002='"||g_dlang||"' ",
 
               " WHERE glafent= ? AND glaf001=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("glaf_t")
