   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE agli042_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE glaf001,t1.glael003",
               " FROM glaf_t",
                              " LEFT JOIN glael_t t1 ON t1.glaelent='"||g_enterprise||"' AND t1.glael001=glaf001 AND t1.glael002='"||g_dlang||"' ",
 
               " WHERE glafent = '" ||g_enterprise|| "' AND glaf001 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE agli042_master_referesh FROM g_sql
 
 
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
