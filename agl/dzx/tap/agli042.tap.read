   IF agli042_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY glaf_t.glaf002"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE agli042_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR agli042_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_glaf_m.glaf001
                                               
      FOREACH b_fill_cs INTO g_glaf_d[l_ac].glafstus,g_glaf_d[l_ac].glaf002,g_glaf2_d[l_ac].glaf002, 
          g_glaf2_d[l_ac].glafmodid,g_glaf2_d[l_ac].glafmoddt,g_glaf2_d[l_ac].glafownid,g_glaf2_d[l_ac].glafowndp, 
          g_glaf2_d[l_ac].glafcrtid,g_glaf2_d[l_ac].glafcrtdp,g_glaf2_d[l_ac].glafcrtdt,g_glaf2_d[l_ac].glafmodid_desc, 
          g_glaf2_d[l_ac].glafownid_desc,g_glaf2_d[l_ac].glafowndp_desc,g_glaf2_d[l_ac].glafcrtid_desc, 
          g_glaf2_d[l_ac].glafcrtdp_desc
                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_glaf2_d[l_ac].glafownid_desc = cl_get_username(g_glaf2_d[l_ac].glafownid)
      #LET g_glaf2_d[l_ac].glafowndp_desc = cl_get_deptname(g_glaf2_d[l_ac].glafowndp)
      #LET g_glaf2_d[l_ac].glafcrtid_desc = cl_get_username(g_glaf2_d[l_ac].glafcrtid)
      #LET g_glaf2_d[l_ac].glafcrtdp_desc = cl_get_deptname(g_glaf2_d[l_ac].glafcrtdp)
      #LET g_glaf2_d[l_ac].glafmodid_desc = cl_get_username(g_glaf2_d[l_ac].glafmodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_glaf_d.deleteElement(g_glaf_d.getLength())
      CALL g_glaf2_d.deleteElement(g_glaf2_d.getLength())
 
