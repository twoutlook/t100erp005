  <section id="agli110.delete" ver="6" status="" src="s">
<![CDATA[#+ 資料刪除
PRIVATE FUNCTION agli110_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_glaw_m.glaw001 IS NULL
   OR g_glaw_m.glaw003 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE agli110_master_referesh USING g_glaw_m.glaw001,g_glaw_m.glaw003 INTO g_glaw_m.glaw001,g_glaw_m.glaw003, 
       g_glaw_m.glaw001_desc,g_glaw_m.glaw003_desc
   
   CALL agli110_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN agli110_cl USING g_enterprise,g_glaw_m.glaw001
                                                       ,g_glaw_m.glaw003
 
   IF STATUS THEN
      CALL cl_err("OPEN agli110_cl:", STATUS, 1)
      CLOSE agli110_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH agli110_cl INTO g_glaw_m.glaw001,g_glaw_m.glaw001_desc,g_glaw_m.glaw003,g_glaw_m.glaw003_desc 

   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_glaw_m.glaw001,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL agli110_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM glaw_t WHERE glawent = g_enterprise AND glaw001 = g_glaw_m.glaw001
                                                               AND glaw003 = g_glaw_m.glaw003
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("glaw_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_glaw_d.clear() 
      CALL g_glaw2_d.clear()       
 
     
      CALL agli110_ui_browser_refresh()  
      CALL agli110_ui_headershow()  
      CALL agli110_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL agli110_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL agli110_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE agli110_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_glaw_m.glaw001,'D')
    
END FUNCTION
]]>
</section>
