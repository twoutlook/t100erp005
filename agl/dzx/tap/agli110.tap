  <section id="agli110.main" ver="5" status="" src="s">
<![CDATA[#+ 此段落由子樣板a26產生
#OPTIONS SHORT CIRCUIT
#+ 作業開始 
MAIN
   #add-point:main段define
   {<point name="main.define"/>}
   #end add-point   
 
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("agl","")
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT glaw001,'',glaw003,''", 
                      " FROM glaw_t",
                      " WHERE glawent= ? AND glaw001=? AND glaw003=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE agli110_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE glaw001,glaw003,t1.glaal002 ,t2.glaal002",
               " FROM glaw_t",
                              " LEFT JOIN glaal_t t1 ON t1.glaalent='"||g_enterprise||"' AND t1.glaalld=glaw001 AND t1.glaal001='"||g_dlang||"' ",
               " LEFT JOIN glaal_t t2 ON t2.glaalent='"||g_enterprise||"' AND t2.glaalld=glaw003 AND t2.glaal001='"||g_dlang||"' ",
 
               " WHERE glawent = '" ||g_enterprise|| "' AND glaw001 = ? AND glaw003 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE agli110_master_referesh FROM g_sql
 
 
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_agli110 WITH FORM cl_ap_formpath("agl",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL agli110_init()   
 
      #進入選單 Menu (="N")
      CALL agli110_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_agli110
      
   END IF 
   
   CLOSE agli110_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
   
END MAIN
 
 
]]>
</section>
  <section id="agli110.modify" ver="4" status="" src="s">
<![CDATA[#+ 資料修改
PRIVATE FUNCTION agli110_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_glaw_m.glaw001 IS NULL
   OR g_glaw_m.glaw003 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE agli110_master_referesh USING g_glaw_m.glaw001,g_glaw_m.glaw003 INTO g_glaw_m.glaw001,g_glaw_m.glaw003, 
       g_glaw_m.glaw001_desc,g_glaw_m.glaw003_desc
 
   ERROR ""
  
   LET g_glaw001_t = g_glaw_m.glaw001
   LET g_glaw003_t = g_glaw_m.glaw003
 
   CALL s_transaction_begin()
   
   OPEN agli110_cl USING g_enterprise,g_glaw_m.glaw001
                                                       ,g_glaw_m.glaw003
 
   IF STATUS THEN
      CALL cl_err("OPEN agli110_cl:", STATUS, 1)
      CLOSE agli110_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH agli110_cl INTO g_glaw_m.glaw001,g_glaw_m.glaw001_desc,g_glaw_m.glaw003,g_glaw_m.glaw003_desc 

 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_glaw_m.glaw001,SQLCA.sqlcode,0)
      CLOSE agli110_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL agli110_show()
   WHILE TRUE
      LET g_glaw001_t = g_glaw_m.glaw001
      LET g_glaw003_t = g_glaw_m.glaw003
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL agli110_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_glaw_m.* = g_glaw_m_t.*
         CALL agli110_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_glaw_m.glaw001 != g_glaw001_t 
      OR g_glaw_m.glaw003 != g_glaw003_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE glaw_t SET glaw001 = g_glaw_m.glaw001
                                       , glaw003 = g_glaw_m.glaw003
 
          WHERE glawent = g_enterprise AND glaw001 = g_glaw001_t
            AND glaw003 = g_glaw003_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("glaw_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("glaw_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_glaw_m.glaw001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE agli110_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_glaw_m.glaw001,'U')
 
   CALL agli110_b_fill("1=1")
   
END FUNCTION   
]]>
</section>
