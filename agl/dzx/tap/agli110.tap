  <section id="agli110.construct" ver="7" status="" src="s">
<![CDATA[#+ QBE資料查詢
PRIVATE FUNCTION agli110_construct()
   DEFINE ls_return   STRING
   DEFINE ls_result   STRING 
   DEFINE ls_wc       STRING 
   #add-point:cs段define
   {<point name="cs.define"/>}
   #end add-point    
 
   #清除畫面上相關資料
   CLEAR FORM                 
   INITIALIZE g_glaw_m.* TO NULL
   CALL g_glaw_d.clear()
   CALL g_glaw2_d.clear()
 
   INITIALIZE g_wc TO NULL
   INITIALIZE g_wc2 TO NULL
   LET g_action_choice = ""
    
   LET g_qryparam.state = 'c'
   
   #add-point:cs段construct外
   {<point name="cs.head.before"/>}
   #end add-point 
   
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
      
      #單頭
      CONSTRUCT BY NAME g_wc ON glaw001,glaw003
 
         BEFORE CONSTRUCT
            #add-point:cs段more_construct
            {<point name="cs.head.before_construct"/>}
            #end add-point 
            
                 #Ctrlp:construct.c.glaw001
         ON ACTION controlp INFIELD glaw001
            #add-point:ON ACTION controlp INFIELD glaw001
            {<point name="construct.c.glaw001" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD glaw001
            #add-point:BEFORE FIELD glaw001
            {<point name="construct.b.glaw001" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD glaw001
            
            #add-point:AFTER FIELD glaw001
            {<point name="construct.a.glaw001" />}
            #END add-point
            
 
         #Ctrlp:construct.c.glaw003
         ON ACTION controlp INFIELD glaw003
            #add-point:ON ACTION controlp INFIELD glaw003
            {<point name="construct.c.glaw003" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD glaw003
            #add-point:BEFORE FIELD glaw003
            {<point name="construct.b.glaw003" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD glaw003
            
            #add-point:AFTER FIELD glaw003
            {<point name="construct.a.glaw003" />}
            #END add-point
            
 
 
         
      END CONSTRUCT
 
      CONSTRUCT g_wc2_table1 ON glawstus,glaw002,glaw002_desc,glaw005,glaw004,glaw006,glaw007,glawownid, 
          glawowndp,glawcrtid,glawcrtdp,glawcrtdt,glawmodid,glawmoddt
           FROM s_detail1[1].glawstus,s_detail1[1].glaw002,s_detail1[1].glaw002_desc,s_detail1[1].glaw005, 
               s_detail1[1].glaw004,s_detail1[1].glaw006,s_detail1[1].glaw007,s_detail2[1].glawownid, 
               s_detail2[1].glawowndp,s_detail2[1].glawcrtid,s_detail2[1].glawcrtdp,s_detail2[1].glawcrtdt, 
               s_detail2[1].glawmodid,s_detail2[1].glawmoddt
                      
         BEFORE CONSTRUCT
            #add-point:cs段more_construct
            {<point name="cs.body.before_construct"/>}
            #end add-point 
            
         #單身公用欄位開窗相關處理
         #此段落由子樣板a11產生
         ##----<<glawcrtdt>>----
         AFTER FIELD glawcrtdt
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
         
         #----<<glawmoddt>>----
         AFTER FIELD glawmoddt
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
         
         #----<<glawcnfdt>>----
         
         #----<<glawpstdt>>----
 
 
           
         #單身一般欄位開窗相關處理
            
       
      END CONSTRUCT
  
 
  
      #add-point:cs段more_construct
      {<point name="cs.more_construct"/>}
      #end add-point
 
      BEFORE DIALOG
         CALL cl_qbe_init()
         #add-point:ui_dialog段b_dialog
         {<point name="cs.before_dialog"/>}
         #end add-point
      
      #查詢方案列表
      ON ACTION qbe_select
         LET ls_wc = ""
         CALL cl_qbe_list("c") RETURNING ls_wc
    
      #條件儲存為方案
      ON ACTION qbe_save
         CALL cl_qbe_save()
 
      ON ACTION accept
         ACCEPT DIALOG
 
      ON ACTION cancel
         LET INT_FLAG = 1
         EXIT DIALOG 
 
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE DIALOG
   END DIALOG
   
   #add-point:cs段after_construct
   {<point name="cs.after_construct"/>}
   #end add-point
   
   #組合g_wc2
   LET g_wc2 = g_wc2_table1
 
 
 
   
   LET g_current_row = 1
 
   IF INT_FLAG THEN
      RETURN
   END IF
   
   LET g_wc_filter = ""
 
END FUNCTION
]]>
</section>
