  <section id="agli110.b_fill" ver="6" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION agli110_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_glaw_d.clear()
   CALL g_glaw2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE glawstus,glaw002,glaw005,glaw004,glaw006,glaw007,glaw002,glaw004,glawownid, 
       glawowndp,glawcrtid,glawcrtdp,glawcrtdt,glawmodid,glawmoddt,t1.oofa011 ,t2.ooefl003 ,t3.oofa011 , 
       t4.ooefl003 ,t5.oofa011 FROM glaw_t",   
               "",
               
                              " LEFT JOIN oofa_t t1 ON t1.oofaent='"||g_enterprise||"' AND t1.oofa002='2' AND t1.oofa003=glawownid  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent='"||g_enterprise||"' AND t2.ooefl001=glawowndp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t3 ON t3.oofaent='"||g_enterprise||"' AND t3.oofa002='2' AND t3.oofa003=glawcrtid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=glawcrtdp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t5 ON t5.oofaent='"||g_enterprise||"' AND t5.oofa002='2' AND t5.oofa003=glawmodid  ",
 
               " WHERE glawent= ? AND glaw001=? AND glaw003=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("glaw_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF agli110_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY glaw_t.glaw002,glaw_t.glaw004"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE agli110_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR agli110_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_glaw_m.glaw001,g_glaw_m.glaw003
                                               
      FOREACH b_fill_cs INTO g_glaw_d[l_ac].glawstus,g_glaw_d[l_ac].glaw002,g_glaw_d[l_ac].glaw005,g_glaw_d[l_ac].glaw004, 
          g_glaw_d[l_ac].glaw006,g_glaw_d[l_ac].glaw007,g_glaw2_d[l_ac].glaw002,g_glaw2_d[l_ac].glaw004, 
          g_glaw2_d[l_ac].glawownid,g_glaw2_d[l_ac].glawowndp,g_glaw2_d[l_ac].glawcrtid,g_glaw2_d[l_ac].glawcrtdp, 
          g_glaw2_d[l_ac].glawcrtdt,g_glaw2_d[l_ac].glawmodid,g_glaw2_d[l_ac].glawmoddt,g_glaw2_d[l_ac].glawownid_desc, 
          g_glaw2_d[l_ac].glawowndp_desc,g_glaw2_d[l_ac].glawcrtid_desc,g_glaw2_d[l_ac].glawcrtdp_desc, 
          g_glaw2_d[l_ac].glawmodid_desc
                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_glaw2_d[l_ac].glawownid_desc = cl_get_username(g_glaw2_d[l_ac].glawownid)
      #LET g_glaw2_d[l_ac].glawowndp_desc = cl_get_deptname(g_glaw2_d[l_ac].glawowndp)
      #LET g_glaw2_d[l_ac].glawcrtid_desc = cl_get_username(g_glaw2_d[l_ac].glawcrtid)
      #LET g_glaw2_d[l_ac].glawcrtdp_desc = cl_get_deptname(g_glaw2_d[l_ac].glawcrtdp)
      #LET g_glaw2_d[l_ac].glawmodid_desc = cl_get_username(g_glaw2_d[l_ac].glawmodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_glaw_d.deleteElement(g_glaw_d.getLength())
      CALL g_glaw2_d.deleteElement(g_glaw2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE agli110_pb   
   
END FUNCTION
]]>
</section>
