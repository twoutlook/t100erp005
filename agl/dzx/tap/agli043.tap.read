  <point name="function.agli043_glag003_desc" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#+ 核算項名稱帶值
PRIVATE FUNCTION agli043_glag003_desc()
DEFINE l_str  STRING
DEFINE l_sql  STRING 
DEFINE l_glae001 LIKE glae_t.glae001

   CASE g_glag_d[l_ac].glag003
      WHEN '01'
         LET l_str = " glad0171" 
      WHEN '02'
         LET l_str = " glad0181 "  
      WHEN '03'
         LET l_str = " glad0191"           
      WHEN '04'
         LET l_str = " glad0201"  
      WHEN '05'
         LET l_str = " glad0211"
      WHEN '06'
         LET l_str = " glad0221 "  
      WHEN '07'
         LET l_str = " glad0231"  
      WHEN '08'
         LET l_str = " glad0241"           
      WHEN '09'
         LET l_str = " glad0251"  
      WHEN '10'
         LET l_str = " glad0261"         
   END CASE 
   LET l_sql = "SELECT ",l_str," FROM glad_t WHERE gladent = '",g_enterprise,"' AND gladld = '",g_glag_m.glagld,"' AND glad001 ='",g_glag_m.glag001,"'"
   PREPARE glag003_desc_pre FROM l_sql
   EXECUTE glag003_desc_pre INTO l_glae001 
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = l_glae001 
   CALL ap_ref_array2(g_ref_fields,"SELECT glael003 FROM glael_t WHERE glaelent='"||g_enterprise||"' AND glael001=? AND glael002='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_glag_d[l_ac].glag003_desc = g_rtn_fields[1]
   DISPLAY BY NAME g_glag_d[l_ac].glag003_desc
   
   
   
END FUNCTION]]>
  </point>
  <point name="function.agli043_glag001_chk" order="2" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#+ 科目存在性檢查
PRIVATE FUNCTION agli043_glag001_chk()
   IF NOT cl_null(g_glag_m.glag001) AND NOT cl_null(g_glag_m.glagld) THEN 
      IF NOT ap_chk_isExist(g_glag_m.glag001,"SELECT COUNT(*) FROM glac_t,glaa_t WHERE glac001 = glaa004  AND glaald = '"||g_glag_m.glagld||"' AND "||"glacent = '" ||g_enterprise|| "' AND "||"glac002 = ? ",'agl-00011',0) THEN 
         LET g_glag_m.glag001 = g_glag_m_t.glag001
         RETURN FALSE
      END IF    
      IF NOT ap_chk_isExist(g_glag_m.glag001,"SELECT COUNT(*) FROM glac_t,glaa_t WHERE glac001 = glaa004  AND glaald = '"||g_glag_m.glagld||"' AND "||"glacent = '" ||g_enterprise|| "' AND "||"glac002 = ? AND glacstus = 'Y' ",'agl-00012',0) THEN 
         LET g_glag_m.glag001 = g_glag_m_t.glag001
         RETURN FALSE
      END IF    
      IF NOT ap_chk_isExist(g_glag_m.glag001,"SELECT COUNT(*) FROM glac_t,glaa_t WHERE glac001 = glaa004  AND glaald = '"||g_glag_m.glagld||"' AND "||"glacent = '" ||g_enterprise|| "' AND "||"glac002 = ? AND glacstus = 'Y' AND glac003 != '1'",'agl-00013',0) THEN 
         LET g_glag_m.glag001 = g_glag_m_t.glag001 
         RETURN FALSE
      END IF 
      IF NOT ap_chk_isExist(g_glag_m.glag001,"SELECT COUNT(*) FROM glac_t,glaa_t WHERE glac001 = glaa004  AND glaald = '"||g_glag_m.glagld||"' AND "||"glacent = '" ||g_enterprise|| "' AND "||"glac002 = ? AND glacstus = 'Y' AND glac003 != '1' AND glac006 ='1' ",'agl-00064',0) THEN 
         LET g_glag_m.glag001 = g_glag_m_t.glag001
         RETURN FALSE
      END IF                
   END IF
   RETURN TRUE            
END FUNCTION]]>
  </point>
