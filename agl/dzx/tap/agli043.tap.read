  <point name="function.agli043_ref_rep" order="5" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 複製時的預設與清空
# Memo...........:
# Usage..........: CALL agli043_ref_rep()
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION agli043_ref_rep()
   SELECT glaald INTO g_glag_m.glagld FROM ooef_t,ooag_t,glaa_t
    WHERE ooef001 = ooag004 AND ooefent = ooagent
      AND ooef017 = glaacomp AND ooefent = glaaent
      AND ooagent = g_enterprise
      AND ooag001 = g_user 
   LET g_glag_m_t.glagld = g_glag_m.glagld    
   CALL agli043_glagld_desc()
   
   LET g_glag_m.glag001 = ''
   LET g_glag_m_t.glag001 = ''
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_glag_m.glagld
   LET g_ref_fields[2] = g_glag_m.glag001
   CALL ap_ref_array2(g_ref_fields,"SELECT glacl004 FROM glacl_t,glaa_t WHERE glacl001=glaa004 AND glaclent='"||g_enterprise||"' AND glaald =? AND glacl002=? AND glacl003='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_glag_m.glag001_desc = g_rtn_fields[1]
   DISPLAY BY NAME g_glag_m.glag001_desc
END FUNCTION]]>
