  <section id="agli043.input" ver="3" status="" src="s">
    <![CDATA[#+ 資料輸入
PRIVATE FUNCTION agli043_input(p_cmd)
   DEFINE  p_cmd                 LIKE type_t.chr1
   DEFINE  l_cmd_t               LIKE type_t.chr1
   DEFINE  l_cmd                 LIKE type_t.chr1
   DEFINE  l_ac_t                LIKE type_t.num5                #未取消的ARRAY CNT 
   DEFINE  l_n                   LIKE type_t.num5                #檢查重複用  
   DEFINE  l_cnt                 LIKE type_t.num5                #檢查重複用  
   DEFINE  l_lock_sw             LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert        LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete        LIKE type_t.num5                #可刪除否  
   DEFINE  l_count               LIKE type_t.num5
   DEFINE  l_i                   LIKE type_t.num5
   DEFINE  l_insert              BOOLEAN
   DEFINE  ls_return             STRING
   DEFINE  l_var_keys            DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys          DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                DYNAMIC ARRAY OF STRING
   DEFINE  l_fields              DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak        DYNAMIC ARRAY OF STRING
   DEFINE  lb_reproduce          BOOLEAN
   DEFINE  li_reproduce          LIKE type_t.num5
   DEFINE  li_reproduce_target   LIKE type_t.num5
   #add-point:input段define
   {<point name="input.define"/>}
   #end add-point    
   
   #先做狀態判定
   IF p_cmd = 'r' THEN
      LET l_cmd_t = 'r'
      LET p_cmd   = 'a'
   ELSE
      LET l_cmd_t = p_cmd
   END IF   
   
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF  
 
   CALL cl_set_head_visible("","YES")  
 
   LET l_insert = FALSE
   LET g_action_choice = ""
 
   #add-point:input段define_sql
   {<point name="input.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT glagstus,glag002,glag003,glag004,glag002,glag003,glagmodid,glagmoddt,glagownid, 
       glagowndp,glagcrtid,glagcrtdp,glagcrtdt FROM glag_t WHERE glagent=? AND glagld=? AND glag001=?  
       AND glag002=? AND glag003=? FOR UPDATE"
   #add-point:input段define_sql
   {<point name="input.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE agli043_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR
 
 
   
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   LET g_qryparam.state = 'i'
   
   #控制key欄位可否輸入
   CALL agli043_set_entry(p_cmd)
   #add-point:set_entry後
   {<point name="input.after_set_entry"/>}
   #end add-point
   CALL agli043_set_no_entry(p_cmd)
   #add-point:set_no_entry後
   {<point name="input.after_set_no_entry"/>}
   #end add-point
 
   DISPLAY BY NAME g_glag_m.glagld,g_glag_m.glaacomp,g_glag_m.glag001
   
   LET lb_reproduce = FALSE
   
   #add-point:進入修改段前
   {<point name="input.before_input"/>}
   #end add-point
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
]]>
  </section>
  <section id="agli043.input.body" ver="7" status="" src="s">
    <![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_glag_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂單身ACTION
         
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_glag_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL agli043_b_fill(g_wc2) 
            IF g_rec_b != 0 THEN
               CALL fgl_set_arr_curr(l_ac)
            END IF
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN agli043_cl USING g_enterprise,
                                               g_glag_m.glagld
                                               ,g_glag_m.glag001
 
                                               
               IF STATUS THEN
                  CALL cl_err("OPEN agli043_cl:", STATUS, 1)
                  CLOSE agli043_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_glag_d[l_ac].glag002 IS NOT NULL
               AND g_glag_d[l_ac].glag003 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_glag_d_t.* = g_glag_d[l_ac].*  #BACKUP
               CALL agli043_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL agli043_set_no_entry_b(l_cmd)
               OPEN agli043_bcl USING g_enterprise,g_glag_m.glagld,
                                                g_glag_m.glag001,
 
                                                g_glag_d_t.glag002
                                                ,g_glag_d_t.glag003
 
               IF STATUS THEN
                  CALL cl_err("OPEN agli043_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH agli043_bcl INTO g_glag_d[l_ac].glagstus,g_glag_d[l_ac].glag002,g_glag_d[l_ac].glag003, 
                      g_glag_d[l_ac].glag004
                  IF SQLCA.sqlcode THEN
                      CALL cl_err(g_glag_d_t.glag002,SQLCA.sqlcode,1)
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL agli043_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
        
         BEFORE INSERT
            
            INITIALIZE g_glag_d_t.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_glag_d[l_ac].* TO NULL
            
            #公用欄位預設值
            #此段落由子樣板a14產生    
      LET g_glag2_d[l_ac].glagownid = g_user
      LET g_glag2_d[l_ac].glagowndp = g_dept
      LET g_glag2_d[l_ac].glagcrtid = g_user
      LET g_glag2_d[l_ac].glagcrtdp = g_dept 
      LET g_glag2_d[l_ac].glagcrtdt = cl_get_current()
      LET g_glag2_d[l_ac].glagmodid = ""
      LET g_glag2_d[l_ac].glagmoddt = ""
      LET g_glag_d[l_ac].glagstus = ""
 
  
            
            #一般欄位預設值
            
            
            
            LET g_glag_d_t.* = g_glag_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL agli043_set_entry_b(l_cmd)
            #add-point:set_entry_b後
            {<point name="input.body.before_insert.set_entry_b"/>}
            #end add-point
            CALL agli043_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_glag_d[li_reproduce_target].* = g_glag_d[li_reproduce].*
               LET g_glag2_d[li_reproduce_target].* = g_glag2_d[li_reproduce].*
 
               LET g_glag_d[g_glag_d.getLength()].glag002 = NULL
               LET g_glag_d[g_glag_d.getLength()].glag003 = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
 
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM glag_t 
             WHERE glagent = g_enterprise AND glagld = g_glag_m.glagld
               AND glag001 = g_glag_m.glag001
 
               AND glag002 = g_glag_d[l_ac].glag002
               AND glag003 = g_glag_d[l_ac].glag003
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               CALL s_transaction_begin()
               #add-point:單身新增前
               {<point name="input.body.b_insert" mark="Y"/>}
               #end add-point
               INSERT INTO glag_t
                           (glagent,
                            glagld,glag001,
                            glag002,glag003
                            ,glagstus,glag004,glagmodid,glagmoddt,glagownid,glagowndp,glagcrtid,glagcrtdp,glagcrtdt) 
                     VALUES(g_enterprise,
                            g_glag_m.glagld,g_glag_m.glag001,
                            g_glag_d[l_ac].glag002,g_glag_d[l_ac].glag003
                            ,g_glag_d[l_ac].glagstus,g_glag_d[l_ac].glag004,g_glag2_d[l_ac].glagmodid, 
                                g_glag2_d[l_ac].glagmoddt,g_glag2_d[l_ac].glagownid,g_glag2_d[l_ac].glagowndp, 
                                g_glag2_d[l_ac].glagcrtid,g_glag2_d[l_ac].glagcrtdp,g_glag2_d[l_ac].glagcrtdt) 

               #add-point:單身新增中
               {<point name="input.body.m_insert"/>}
               #end add-point
               LET p_cmd = 'u'
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_glag_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("glag_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR "INSERT O.K"
               LET g_rec_b=g_rec_b+1
               DISPLAY g_rec_b TO FORMONLY.cnt
            END IF
            
            #add-point:單身新增後
            {<point name="input.body.after_insert"/>}
            #end add-point
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_glag_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_glag_d.deleteElement(l_ac)
               NEXT FIELD glag002
            END IF
            IF g_glag_d_t.glag002 IS NOT NULL
               AND g_glag_d_t.glag003 IS NOT NULL
 
               THEN
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete"/>}
               #end add-point
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               IF agli043_before_delete() THEN 
                  CALL s_transaction_end('Y','0')
               ELSE 
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               END IF 
               CLOSE agli043_bcl
               LET l_count = g_glag_d.getLength()
            END IF 
            
            #add-point:單身刪除後
            {<point name="input.body.a_delete"/>}
            #end add-point
              
         AFTER DELETE 
            #add-point:單身AFTER DELETE 
            {<point name="input.body.after_delete"/>}
            #end add-point
 
                  #此段落由子樣板a01產生
         BEFORE FIELD glagstus
            #add-point:BEFORE FIELD glagstus
            {<point name="input.b.page1.glagstus" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD glagstus
            
            #add-point:AFTER FIELD glagstus
            {<point name="input.a.page1.glagstus" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE glagstus
            #add-point:ON CHANGE glagstus
            {<point name="input.g.page1.glagstus" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD glag002
            
            #add-point:AFTER FIELD glag002
            {<point name="input.a.page1.glag002" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD glag002
            #add-point:BEFORE FIELD glag002
            {<point name="input.b.page1.glag002" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE glag002
            #add-point:ON CHANGE glag002
            {<point name="input.g.page1.glag002" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD glag003
            
            #add-point:AFTER FIELD glag003
            {<point name="input.a.page1.glag003" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD glag003
            #add-point:BEFORE FIELD glag003
            {<point name="input.b.page1.glag003" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE glag003
            #add-point:ON CHANGE glag003
            {<point name="input.g.page1.glag003" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD glag004
            
            #add-point:AFTER FIELD glag004
            {<point name="input.a.page1.glag004" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD glag004
            #add-point:BEFORE FIELD glag004
            {<point name="input.b.page1.glag004" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE glag004
            #add-point:ON CHANGE glag004
            {<point name="input.g.page1.glag004" />}
            #END add-point
 
 
                  #Ctrlp:input.c.page1.glagstus
         ON ACTION controlp INFIELD glagstus
            #add-point:ON ACTION controlp INFIELD glagstus
            {<point name="input.c.page1.glagstus" />}
            #END add-point
 
         #Ctrlp:input.c.page1.glag002
         ON ACTION controlp INFIELD glag002
            #add-point:ON ACTION controlp INFIELD glag002
            {<point name="input.c.page1.glag002" />}
            #END add-point
 
         #Ctrlp:input.c.page1.glag003
         ON ACTION controlp INFIELD glag003
            #add-point:ON ACTION controlp INFIELD glag003
            {<point name="input.c.page1.glag003" />}
            #END add-point
 
         #Ctrlp:input.c.page1.glag004
         ON ACTION controlp INFIELD glag004
            #add-point:ON ACTION controlp INFIELD glag004
            {<point name="input.c.page1.glag004" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_glag_d[l_ac].* = g_glag_d_t.*
               CLOSE agli043_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_glag_d[l_ac].glag002,-263,1)
               LET g_glag_d[l_ac].* = g_glag_d_t.*
            ELSE
               #寫入修改者/修改日期資訊
               LET g_glag2_d[l_ac].glagmodid = g_user 
LET g_glag2_d[l_ac].glagmoddt = cl_get_current()
 
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
         
               UPDATE glag_t SET (glagld,glag001,glagstus,glag002,glag003,glag004,glagmodid,glagmoddt, 
                   glagownid,glagowndp,glagcrtid,glagcrtdp,glagcrtdt) = (g_glag_m.glagld,g_glag_m.glag001, 
                   g_glag_d[l_ac].glagstus,g_glag_d[l_ac].glag002,g_glag_d[l_ac].glag003,g_glag_d[l_ac].glag004, 
                   g_glag2_d[l_ac].glagmodid,g_glag2_d[l_ac].glagmoddt,g_glag2_d[l_ac].glagownid,g_glag2_d[l_ac].glagowndp, 
                   g_glag2_d[l_ac].glagcrtid,g_glag2_d[l_ac].glagcrtdp,g_glag2_d[l_ac].glagcrtdt)
                WHERE glagent = g_enterprise AND glagld = g_glag_m.glagld 
                 AND glag001 = g_glag_m.glag001 
 
                 AND glag002 = g_glag_d_t.glag002 #項次   
                 AND glag003 = g_glag_d_t.glag003  
 
                 
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
               
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("glag_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                  #WHEN SQLCA.sqlcode #其他錯誤
                  #   CALL cl_err("glag_t",SQLCA.sqlcode,1)  
                  #   CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_glag_m.glagld
               LET gs_keys_bak[1] = g_glagld_t
               LET gs_keys[2] = g_glag_m.glag001
               LET gs_keys_bak[2] = g_glag001_t
               LET gs_keys[3] = g_glag_d[g_detail_idx].glag002
               LET gs_keys_bak[3] = g_glag_d_t.glag002
               LET gs_keys[4] = g_glag_d[g_detail_idx].glag003
               LET gs_keys_bak[4] = g_glag_d_t.glag003
               CALL agli043_update_b('glag_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
                     LET g_log1 = util.JSON.stringify(g_glag_m),util.JSON.stringify(g_glag_d_t)
                     LET g_log2 = util.JSON.stringify(g_glag_m),util.JSON.stringify(g_glag_d[l_ac])
                     IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                        CALL s_transaction_end('N','0')
                     END IF
               END CASE
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
            END IF
 
         AFTER INPUT
            #若單身還沒有輸入資料, 強制切換至單身
            IF g_glag_d.getLength() = 0 THEN
               NEXT FIELD glag002
            END IF
            #add-point:input段after input 
            {<point name="input.body.after_input"/>}
            #end add-point    
            
         ON ACTION controlo   
            CALL FGL_SET_ARR_CURR(g_glag_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_glag_d.getLength()+1
            
      END INPUT
 
 
      
      DISPLAY ARRAY g_glag2_d TO s_detail2.* ATTRIBUTES(COUNT=g_rec_b)  
      
         BEFORE ROW
            CALL agli043_b_fill(g_wc2) 
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail2")
            LET l_ac = g_detail_idx
            DISPLAY g_detail_idx TO FORMONLY.idx
            CALL agli043_ui_detailshow()
        
         BEFORE DISPLAY 
            CALL FGL_SET_ARR_CURR(g_detail_idx)      
         
         #add-point:page2自定義行為
         {<point name="input.body2.action"/>}
         #end add-point
         
      END DISPLAY
 
      
 
      
      #add-point:input段more_input
      {<point name="input.more_inputarray"/>}
      #end add-point    
      
      BEFORE DIALOG
         #add-point:input段before_dialog
         {<point name="input.before_dialog"/>}
         #end add-point 
         #新增時強制從單頭開始填
         IF p_cmd = 'a' THEN
            NEXT FIELD glagld
        ELSE
            CASE g_aw
               WHEN "s_detail1"
                  NEXT FIELD glagstus
               WHEN "s_detail2"
                  NEXT FIELD glag002_2
 
            END CASE
         END IF
   
      ON ACTION controlf
         CALL cl_set_focus_form(ui.Interface.getRootNode()) RETURNING g_fld_name,g_frm_name
         CALL cl_fldhelp(g_frm_name,g_fld_name,g_lang)
 
      ON ACTION controlr
         CALL cl_show_req_fields()
 
      ON ACTION controls
         IF g_header_hidden THEN
            CALL gfrm_curr.setElementHidden("vb_master",0)
            CALL gfrm_curr.setElementImage("controls","small/arr-u.png")
            LET g_header_hidden = 0     #visible
         ELSE
            CALL gfrm_curr.setElementHidden("vb_master",1)
            CALL gfrm_curr.setElementImage("controls","small/arr-d.png")
