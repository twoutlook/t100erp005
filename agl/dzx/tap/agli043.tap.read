  <section id="agli043.reproduce" ver="5" status="" src="s">
    <![CDATA[#+ 資料複製
PRIVATE FUNCTION agli043_reproduce()
   DEFINE l_newno     LIKE glag_t.glagld 
   DEFINE l_oldno     LIKE glag_t.glagld 
   DEFINE l_newno02     LIKE glag_t.glag001 
   DEFINE l_oldno02     LIKE glag_t.glag001 
 
   DEFINE l_master    RECORD LIKE glag_t.*
   DEFINE l_detail    RECORD LIKE glag_t.*
 
   DEFINE l_cnt       LIKE type_t.num5
   #add-point:reproduce段define
   {<point name="reproduce.define"/>}
   #end add-point
 
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF     
 
   IF g_glag_m.glagld IS NULL
      OR g_glag_m.glag001 IS NULL
 
      THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   LET g_glagld_t = g_glag_m.glagld
