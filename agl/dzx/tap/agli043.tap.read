  <section id="agli043.b_fill" ver="9" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION agli043_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_glag_d.clear()
   CALL g_glag2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE glagstus,glag002,glag003,glag004,glag002,glag003,glagmodid,glagmoddt, 
       glagownid,glagowndp,glagcrtid,glagcrtdp,glagcrtdt,t1.gzzal003 ,t2.dzeb003 ,t3.oofa011 ,t4.oofa011 , 
       t5.ooefl003 ,t6.oofa011 ,t7.ooefl003 FROM glag_t",   
               "",
               
                              " LEFT JOIN gzzal_t t1 ON t1.gzzal001=glag002 AND t1.gzzal002='"||g_lang||"' ",
               " LEFT JOIN dzeb_t t2 ON t2.dzeb002=glag004  ",
               " LEFT JOIN oofa_t t3 ON t3.oofaent='"||g_enterprise||"' AND t3.oofa002='2' AND t3.oofa003=glagmodid  ",
               " LEFT JOIN oofa_t t4 ON t4.oofaent='"||g_enterprise||"' AND t4.oofa002='2' AND t4.oofa003=glagownid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=glagowndp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t6 ON t6.oofaent='"||g_enterprise||"' AND t6.oofa002='2' AND t6.oofa003=glagcrtid  ",
               " LEFT JOIN ooefl_t t7 ON t7.ooeflent='"||g_enterprise||"' AND t7.ooefl001=glagcrtdp AND t7.ooefl002='"||g_dlang||"' ",
 
               " WHERE glagent= ? AND glagld=? AND glag001=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("glag_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF agli043_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY glag_t.glag002,glag_t.glag003"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE agli043_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR agli043_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_glag_m.glagld,g_glag_m.glag001
                                               
      FOREACH b_fill_cs INTO g_glag_d[l_ac].glagstus,g_glag_d[l_ac].glag002,g_glag_d[l_ac].glag003,g_glag_d[l_ac].glag004, 
          g_glag2_d[l_ac].glag002,g_glag2_d[l_ac].glag003,g_glag2_d[l_ac].glagmodid,g_glag2_d[l_ac].glagmoddt, 
          g_glag2_d[l_ac].glagownid,g_glag2_d[l_ac].glagowndp,g_glag2_d[l_ac].glagcrtid,g_glag2_d[l_ac].glagcrtdp, 
          g_glag2_d[l_ac].glagcrtdt,g_glag_d[l_ac].glag002_desc,g_glag_d[l_ac].glag004_desc,g_glag2_d[l_ac].glagmodid_desc, 
          g_glag2_d[l_ac].glagownid_desc,g_glag2_d[l_ac].glagowndp_desc,g_glag2_d[l_ac].glagcrtid_desc, 
          g_glag2_d[l_ac].glagcrtdp_desc
                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_glag2_d[l_ac].glagownid_desc = cl_get_username(g_glag2_d[l_ac].glagownid)
      #LET g_glag2_d[l_ac].glagowndp_desc = cl_get_deptname(g_glag2_d[l_ac].glagowndp)
      #LET g_glag2_d[l_ac].glagcrtid_desc = cl_get_username(g_glag2_d[l_ac].glagcrtid)
      #LET g_glag2_d[l_ac].glagcrtdp_desc = cl_get_deptname(g_glag2_d[l_ac].glagcrtdp)
      #LET g_glag2_d[l_ac].glagmodid_desc = cl_get_username(g_glag2_d[l_ac].glagmodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_glag_d.deleteElement(g_glag_d.getLength())
      CALL g_glag2_d.deleteElement(g_glag2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE agli043_pb   
   
END FUNCTION
]]>
  </section>
