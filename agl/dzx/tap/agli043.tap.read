  <section id="agli043.modify" ver="5" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION agli043_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_glag_m.glagld IS NULL
   OR g_glag_m.glag001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE agli043_master_referesh USING g_glag_m.glagld,g_glag_m.glag001 INTO g_glag_m.glagld,g_glag_m.glag001, 
       g_glag_m.glagld_desc
 
   ERROR ""
  
   LET g_glagld_t = g_glag_m.glagld
   LET g_glag001_t = g_glag_m.glag001
 
   CALL s_transaction_begin()
   
   OPEN agli043_cl USING g_enterprise,g_glag_m.glagld
                                                       ,g_glag_m.glag001
 
   IF STATUS THEN
      CALL cl_err("OPEN agli043_cl:", STATUS, 1)
      CLOSE agli043_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH agli043_cl INTO g_glag_m.glagld,g_glag_m.glagld_desc,g_glag_m.glaacomp,g_glag_m.glag001,g_glag_m.glag001_desc 

 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_glag_m.glagld,SQLCA.sqlcode,0)
      CLOSE agli043_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL agli043_show()
   WHILE TRUE
      LET g_glagld_t = g_glag_m.glagld
      LET g_glag001_t = g_glag_m.glag001
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL agli043_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_glag_m.* = g_glag_m_t.*
         CALL agli043_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_glag_m.glagld != g_glagld_t 
      OR g_glag_m.glag001 != g_glag001_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE glag_t SET glagld = g_glag_m.glagld
                                       , glag001 = g_glag_m.glag001
 
          WHERE glagent = g_enterprise AND glagld = g_glagld_t
            AND glag001 = g_glag001_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("glag_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("glag_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_glag_m.glagld,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE agli043_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_glag_m.glagld,'U')
 
   CALL agli043_b_fill("1=1")
   
END FUNCTION   
]]>
  </section>
