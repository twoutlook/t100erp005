################################################################################
PRIVATE FUNCTION agli043_glagld_desc()
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_glag_m.glagld
   CALL ap_ref_array2(g_ref_fields,"SELECT glaacomp FROM glaa_t WHERE glaaent='"||g_enterprise||"' AND glaald=? ","") RETURNING g_rtn_fields
   LET g_glag_m.glaacomp = g_rtn_fields[1]
   DISPLAY BY NAME g_glag_m.glaacomp

   INITIALIZE g_ref_fields TO NULL
