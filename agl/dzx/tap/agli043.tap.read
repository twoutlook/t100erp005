  <section id="agli043.detail_reproduce" ver="3" status="" src="s">
    <![CDATA[#+ 單身自動複製
PRIVATE FUNCTION agli043_detail_reproduce()
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE glag_t.*
 
 
   #add-point:delete段define
   {<point name="detail_reproduce.define"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE agli043_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE agli043_detail AS ",
                "SELECT * FROM glag_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO agli043_detail SELECT * FROM glag_t 
                                         WHERE glagent = g_enterprise AND glagld = g_glagld_t
                                         AND glag001 = g_glag001_t
 
   
   #將key修正為調整後   
   UPDATE agli043_detail 
      #更新key欄位
      SET glagld = g_glag_m.glagld
          , glag001 = g_glag_m.glag001
 
      #更新共用欄位
      #此段落由子樣板a13產生
       , glagownid = g_user
       , glagowndp = g_dept
       , glagcrtid = g_user
       , glagcrtdp = g_dept 
       , glagcrtdt = ld_date
       , glagmodid = "" 
       , glagmoddt = "" 
      #, glagstus = "Y"
 
 
                                       
  
   #將資料塞回原table   
   INSERT INTO glag_t SELECT * FROM agli043_detail
   
   IF SQLCA.sqlcode THEN
      CALL cl_err("reproduce",SQLCA.sqlcode,1)
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE agli043_detail
   
   #add-point:單身複製後1
   {<point name="detail_reproduce.body.table1.a_insert"/>}
   #end add-point
 
 
   
 
   
   #多語言複製段落
   
   
   CALL s_transaction_end('Y','0')
   
   #已新增完, 調整資料內容(修改時使用)
   LET g_glagld_t = g_glag_m.glagld
   LET g_glag001_t = g_glag_m.glag001
 
   
   DROP TABLE agli043_detail
   
END FUNCTION
