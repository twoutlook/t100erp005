   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT glagld,'','',glag001,''", 
                      " FROM glag_t",
                      " WHERE glagent= ? AND glagld=? AND glag001=? FOR UPDATE"
