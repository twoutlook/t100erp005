   #end add-point    
   
   #清畫面欄位內容
   CLEAR FORM                    
   CALL g_glca_d.clear()   
   CALL g_glca2_d.clear()  
 
 
   INITIALIZE g_glaa_m.* LIKE glaa_t.*             #DEFAULT 設定
   
   LET g_glaald_t = NULL
 
   
   CALL s_transaction_begin()
   WHILE TRUE
      #公用欄位給值(單頭)
      #此段落由子樣板a14產生    
      LET g_glaa_m.glaastus = "N"
 
 
 
      #append欄位給值
      
     
      #一般欄位給值
            LET g_glaa_m.glaa014 = "N"
      LET g_glaa_m.glaa008 = "N"
 
  
