  <section id="agli171.input.body" ver="7" status="" src="s">
    <![CDATA[   
      #Page1 預設值產生於此處
      INPUT ARRAY g_glca_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_glca_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL agli171_b_fill()
            LET g_rec_b = g_glca_d.getLength()
            #add-point:資料輸入前
            {<point name="input.d.before_input"/>}
            #end add-point
         
         BEFORE ROW
            #add-point:modify段before row2
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET l_insert = FALSE
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET g_detail_idx = l_ac
            
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
         
            CALL s_transaction_begin()
            OPEN agli171_cl USING g_enterprise,g_glaa_m.glaald
            IF STATUS THEN
               CALL cl_err("OPEN agli171_cl:", STATUS, 1)
               CLOSE agli171_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            
            LET g_rec_b = g_glca_d.getLength()
            
            IF g_rec_b >= l_ac 
               AND g_glca_d[l_ac].glca001 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_glca_d_t.* = g_glca_d[l_ac].*  #BACKUP
               LET g_glca_d_o.* = g_glca_d[l_ac].*  #BACKUP
               CALL agli171_set_entry_b(l_cmd)
               #add-point:modify段after_set_entry_b
               {<point name="input.body.after_set_entry_b"/>}
               #end add-point  
               CALL agli171_set_no_entry_b(l_cmd)
               IF NOT agli171_lock_b("glca_t","'1'") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH agli171_bcl INTO g_glca_d[l_ac].glca001,g_glca_d[l_ac].glca002,g_glca_d[l_ac].glca003, 
                      g_glca_d[l_ac].glca004,g_glca_d[l_ac].glca005
                  IF SQLCA.sqlcode THEN
                     CALL cl_err(g_glca_d_t.glca001,SQLCA.sqlcode,1)
                     LET l_lock_sw = "Y"
                  END IF
                  
                  LET g_bfill = "N"
                  CALL agli171_show()
                  LET g_bfill = "Y"
                  
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
        
         BEFORE INSERT
            
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_glca_d[l_ac].* TO NULL 
            INITIALIZE g_glca_d_t.* TO NULL 
            INITIALIZE g_glca_d_o.* TO NULL 
                  LET g_glca_d[l_ac].glca004 = "N"
      LET g_glca_d[l_ac].glca005 = "N"
 
            LET g_glca_d_t.* = g_glca_d[l_ac].*     #新輸入資料
            LET g_glca_d_o.* = g_glca_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL agli171_set_entry_b(l_cmd)
            #add-point:modify段after_set_entry_b
            {<point name="input.body.insert.after_set_entry_b"/>}
            #end add-point
            CALL agli171_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_glca_d[li_reproduce_target].* = g_glca_d[li_reproduce].*
 
               LET g_glca_d[li_reproduce_target].glca001 = NULL
 
            END IF
            #公用欄位給值(單身)
            
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            #add-point:單身新增
            {<point name="input.body.b_a_insert"/>}
            #end add-point
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM glca_t 
             WHERE glcaent = g_enterprise AND glcald = g_glaa_m.glaald
 
               AND glca001 = g_glca_d[l_ac].glca001
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前
               {<point name="input.body.b_insert"/>}
               #end add-point
            
               #同步新增到同層的table
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_glaa_m.glaald
               LET gs_keys[2] = g_glca_d[g_detail_idx].glca001
               CALL agli171_insert_b('glca_t',gs_keys,"'1'")
                           
               #add-point:單身新增後
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_glca_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("glca_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL agli171_b_fill()
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert2"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR 'INSERT O.K'
               LET g_rec_b = g_rec_b + 1
            END IF
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_glca_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_glca_d.deleteElement(l_ac)
               NEXT FIELD glca001
            END IF
         
            IF g_glca_d[l_ac].glca001 IS NOT NULL
 
               THEN 
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete" mark="Y"/>}
               #end add-point 
               
               DELETE FROM glca_t
                WHERE glcaent = g_enterprise AND glcald = g_glaa_m.glaald AND
 
                      glca001 = g_glca_d_t.glca001
 
                  
               #add-point:單身刪除中
               {<point name="input.body.m_delete"/>}
               #end add-point 
               
               IF SQLCA.sqlcode THEN
                  CALL cl_err("glca_t",SQLCA.sqlcode,1)
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               ELSE
                  LET g_rec_b = g_rec_b-1
                  
                  #add-point:單身刪除後
                  {<point name="input.body.a_delete"/>}
                  #end add-point
                  CALL s_transaction_end('Y','0')
               END IF 
               CLOSE agli171_bcl
               LET l_count = g_glca_d.getLength()
            END IF 
            
                           INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_glaa_m.glaald
               LET gs_keys[2] = g_glca_d[g_detail_idx].glca001
 
              
         AFTER DELETE 
            #add-point:單身刪除後2
            {<point name="input.body.after_delete"/>}
            #end add-point
                           CALL agli171_delete_b('glca_t',gs_keys,"'1'")
 
