      {<point name="insert.default"/>}
      #end add-point 
     
      CALL agli171_input("a")
      
      #add-point:單頭輸入後
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_glaa_m.* = g_glaa_m_t.*
         CALL agli171_show()
         CALL cl_err('',9001,0)
         CALL s_transaction_end('N','0')
         EXIT WHILE
      END IF
      
      CALL g_glca_d.clear()
      CALL g_glca2_d.clear()
 
 
      LET g_rec_b = 0
      CALL s_transaction_end('Y','0')
      EXIT WHILE
        
   END WHILE
   
   #將新增的資料併入搜尋條件中
   LET g_state = "Y"
   LET g_current_idx = 1 
   
   LET g_glaald_t = g_glaa_m.glaald
 
   
   LET g_wc = "(",g_wc,  
              " OR ( glaaent = '" ||g_enterprise|| "' AND",
              " glaald = '", g_glaa_m.glaald CLIPPED, "' "
 
              , ")) "
   
   CLOSE agli171_cl
   
END FUNCTION
