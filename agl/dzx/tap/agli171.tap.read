  <section id="agli171.b_fill" ver="7" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION agli171_b_fill()
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_glca_d.clear()    #g_glca_d 單頭及單身 
   CALL g_glca2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF agli171_fill_chk(1) THEN
   
      LET g_sql = "SELECT  UNIQUE glca001,glca002,glca003,glca004,glca005 ,t1.gzcbl004 FROM glca_t", 
             
                  " INNER JOIN glaa_t ON glaald = glcald ",
 
                  #"",
                  
                  "",
                                 " LEFT JOIN gzcbl_t t1 ON t1.gzcbl001='8303' AND t1.gzcbl002=glca001 AND t1.gzcbl003='"||g_lang||"' ",
 
                  " WHERE glcaent=? AND glcald=?"
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      #add-point:b_fill段sql_before
      {<point name="b_fill.body.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table1) THEN
         LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
      END IF
      
      #子單身的WC
      
      
      LET g_sql = g_sql, " ORDER BY glca_t.glca001"
      
      #add-point:單身填充控制
      {<point name="b_fill.sql"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE agli171_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR agli171_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_glaa_m.glaald
                                               
      FOREACH b_fill_cs INTO g_glca_d[l_ac].glca001,g_glca_d[l_ac].glca002,g_glca_d[l_ac].glca003,g_glca_d[l_ac].glca004, 
          g_glca_d[l_ac].glca005,g_glca_d[l_ac].glca001_desc
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               CALL cl_err( '', 9035, 1)
            END IF
            EXIT FOREACH
         END IF
         
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
   #判斷是否填充
   IF agli171_fill_chk(2) THEN
      LET g_sql = "SELECT  UNIQUE glab001,glab002,glab003,glab005,glab011 ,t2.gzcbl004 FROM glab_t", 
             
                  " INNER JOIN glaa_t ON glaald = glabld ",
 
                  "",
                  
                                 " LEFT JOIN gzcbl_t t2 ON t2.gzcbl001='8318' AND t2.gzcbl002=glab003 AND t2.gzcbl003='"||g_lang||"' ",
 
                  " WHERE glabent=? AND glabld=?"   
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      #add-point:b_fill段sql_before
      {<point name="b_fill.body2.fill_sql"/>}
      #end add-point
