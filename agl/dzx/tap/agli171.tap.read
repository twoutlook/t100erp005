      LET l_sql_rank = "SELECT DISTINCT glaastus,glaald,DENSE_RANK() OVER(ORDER BY glaald ",g_order,") AS RANK ",
                        " FROM glaa_t ",
                              " LEFT JOIN glab_t ON glabent = glaaent AND glaald = glabld ",
