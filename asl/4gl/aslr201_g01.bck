               WHERE imaa156=l_imaa156
                 AND imaa126=l_imaa126
              select sum(qty03) INTO l_sr9.qty03 from rep09_temp
               WHERE imaa156=l_imaa156
                 AND imaa126=l_imaa126
              select sum(amt) INTO l_sr9.amt from rep09_temp
               WHERE imaa156=l_imaa156
                 AND imaa126=l_imaa126
              LET l_sr9.qty04=1
              LET l_sr9.qty05=1
              INSERT INTO rep09_temp values (l_sr9.*)
              update rep09_temp set qty04=amt/l_sr9.amt ,
                                    qty05=qty03/l_sr9.qty03,
                                    qty02=qty01/l_sr9.qty01                                    
               WHERE imaa156=l_imaa156
                 AND imaa126=l_imaa126              
              END IF 
              #end add-point:rep.sub01.foreach
             
           END FOREACH
           
           LET l_num=0
           INITIALIZE  l_sr9.* TO NULL
           LET l_sql="SELECT DISTINCT imaa132,sum(qty01),sum(qty03),sum(amt) 
                        FROM rep09_temp WHERE types=1 
                       GROUP BY imaa132
                       ORDER BY 1"
           DECLARE aslr201_g01_repcur09_02 CURSOR FROM l_sql
