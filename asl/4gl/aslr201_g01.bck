            IF sr6.types=1 AND cl_null(l_imaa126_t) AND cl_null(l_imaa156_t) THEN
               LET l_imaa126_t=sr6.imaa126
               LET l_imaa156_t=sr6.imaa156
            END IF 
        
         
        
