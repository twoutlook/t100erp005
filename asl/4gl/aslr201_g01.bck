################################################################################
PRIVATE FUNCTION aslr201_g01_rep_subrep10(p_xmjd001,p_xmjd002)
 DEFINE l_sr10       sr10_r
 DEFINE l_sql       STRING,          
        p_xmjd001   LIKE xmjd_t.xmjd001,
        p_xmjd002   LIKE xmjd_t.xmjd002
 DEFINE l_cnt       LIKE type_t.num10
 DEFINE i           LIKE type_t.num10
 DEFINE l_sub_sql   STRING
 DEFINE l_imaa126   LIKE imaa_t.imaa126,
        l_imaa156   LIKE imaa_t.imaa156
 DEFINE l_amount DYNAMIC ARRAY OF record
                   amount    varchar(100),    #单价范围
                   bamt      decimal(20,6),
                   eamt      decimal(20,6)
                 end record
        
           drop table rep10_temp
           create temp table rep10_temp 
          (                      
           amount     varchar(100),    #单价范围
           qty00      decimal(20,6),   #总款数
           qty01      decimal(20,6),   #订货款量
           qty02      decimal(20,6),   #订货sku数
           qty03      decimal(20,6),   #订货数量
           amt        decimal(20,6),   #零售金额
           qty04      decimal(20,6),   #零售金额占比
           qty05      decimal(20,6),   #订货款数占比
           types      varchar(10)      #1明细2小计3合计
           )         
           
           LET l_amount[1].amount ='0-199'
           LET l_amount[1].bamt=0
           LET l_amount[1].eamt=199
           LET l_amount[2].amount ='200-399'
           LET l_amount[2].bamt=200
           LET l_amount[2].eamt=399
           LET l_amount[3].amount ='400-599'
           LET l_amount[3].bamt=400
           LET l_amount[3].eamt=599
           LET l_amount[4].amount ='600-799'
           LET l_amount[4].bamt=600
           LET l_amount[4].eamt=799
           LET l_amount[5].amount ='800-999'
           LET l_amount[5].bamt=800
           LET l_amount[5].eamt=999
           LET l_amount[6].amount ='1000以上'
           LET l_amount[6].bamt=1000
           LET l_amount[6].eamt=''
           
           
           #订货数量,金额
           LET l_sql="
           SELECT sum(xmjd015),sum(xmjd016)  FROM xmjd_t
            WHERE xmjd001='"||p_xmjd001||"' 
              AND xmjd002='"||p_xmjd002||"'
              AND xmjdent='"||g_enterprise||"'
              AND xmjd009>=? AND xmjd009<=?  "
           DECLARE aslr201_g01_repcur10_01 CURSOR FROM l_sql 
           
           #订货款数
           LET l_sql="
           SELECT count(DISTINCT xmjd008) FROM xmjd_t
            WHERE xmjd001='"||p_xmjd001||"' 
              AND xmjd002='"||p_xmjd002||"'
              AND xmjdent='"||g_enterprise||"'
              AND xmjd015<>0
              AND xmjd009>=? AND xmjd009<=? "
           DECLARE aslr201_g01_repcur10_02 CURSOR FROM l_sql 
           
           #总款数
           LET l_sql="
           SELECT count(DISTINCT xmjd008) FROM xmjd_t
            WHERE xmjd001='"||p_xmjd001||"' 
              AND xmjd002='"||p_xmjd002||"'
              AND xmjdent='"||g_enterprise||"'
              AND xmjd015<>0
              AND xmjd009>=? AND xmjd009<=? "
           DECLARE aslr201_g01_repcur10_03 CURSOR FROM l_sql 
           
            #订货sku数
            LET l_Sql=
            "select count(*)  from 
             (SELECT  distinct xmjd008||xmjd010 FROM xmjd_t
               WHERE xmjd001='"||p_xmjd001||"' 
                 AND xmjd002='"||p_xmjd002||"'
                 AND xmjdent='"||g_enterprise||"'
                 AND xmjd015<>0
                 AND xmjd009>=? AND xmjd009<=?)"
             DECLARE aslr201_g01_repcur10_04 CURSOR FROM l_sql
                   
           FOR i=1 TO 5 
              INITIALIZE  l_sr10.* TO NULL
              LET l_sr10.types=1
              LET l_sr10.amount=l_amount[i].amount
              EXECUTE aslr201_g01_repcur10_01 USING  l_amount[i].bamt,l_amount[i].eamt
                 INTO l_sr10.qty03,l_sr10.amt                 
              EXECUTE aslr201_g01_repcur10_02 USING  l_amount[i].bamt,l_amount[i].eamt
                 INTO l_sr10.qty01
              EXECUTE aslr201_g01_repcur10_03 USING  l_amount[i].bamt,l_amount[i].eamt
                 INTO l_sr10.qty00
              EXECUTE aslr201_g01_repcur10_04 USING  l_amount[i].bamt,l_amount[i].eamt
                 INTO l_sr10.qty02
              INSERT INTO rep10_temp values (l_sr10.*)  
           END for
           
           #           
           INITIALIZE  l_sr10.* TO NULL 
           LET l_sr10.types=1    
           LET l_sr10.amount=l_amount[6].amount           
           #订货数量
           SELECT sum(xmjd015),sum(xmjd016) INTO l_sr10.qty03,l_sr10.amt  
             FROM xmjd_t
            WHERE xmjd001=p_xmjd001
              AND xmjd002=p_xmjd002
              AND xmjdent=g_enterprise
              AND xmjd009>=l_amount[6].bamt
           #订货款数
           SELECT count(DISTINCT xmjd008) into l_sr10.qty01 FROM xmjd_t
            WHERE xmjd001=p_xmjd001
              AND xmjd002=p_xmjd002
              AND xmjdent=g_enterprise
              AND xmjd015<>0
              AND xmjd009>=l_amount[6].bamt           
           #总款数
           SELECT count(DISTINCT xmjd008) into l_sr10.qty00  FROM xmjd_t
            WHERE xmjd001=p_xmjd001
              AND xmjd002=p_xmjd002
              AND xmjdent=g_enterprise
              AND (xmjd015=0 OR xmjd015 is null)
              AND xmjd009>=l_amount[6].bamt           
            #订货sku数
             select count(*) into l_sr10.qty02 from 
             (SELECT  distinct xmjd008||xmjd010 FROM xmjd_t
               WHERE xmjd001=p_xmjd001
                 AND xmjd002=p_xmjd002
                 AND xmjdent=g_enterprise
                 AND xmjd015<>0
                 AND xmjd009>=l_amount[6].bamt) 
 
                      
              #小计
              INITIALIZE  l_sr10.* TO NULL    
              LET l_sr10.amount='TOTAL'
              LET l_sr10.types=2
              select sum(qty00) INTO l_sr10.qty00 from rep10_temp
               WHERE types=1
              select sum(qty01) INTO l_sr10.qty01 from rep10_temp
               WHERE types=1
              select sum(qty02) INTO l_sr10.qty02 from rep10_temp
               WHERE types=1
              select sum(qty03) INTO l_sr10.qty03 from rep10_temp
               WHERE types=1
              select sum(amt) INTO l_sr10.amt from rep10_temp
               WHERE types=1
              LET l_sr10.qty04=1
              LET l_sr10.qty05=1
              INSERT INTO rep10_temp values (l_sr10.*)
              update rep10_temp set qty04=amt/l_sr10.amt ,
                                    qty05=qty01/l_sr10.qty01                                                
              
              #end add-point:rep.sub01.foreach
                
             
END FUNCTION

 
