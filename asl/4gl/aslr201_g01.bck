                 LET g_errparam.code   = SQLCA.sqlcode
                 LET g_errparam.popup  = FALSE
                 CALL cl_err()                  
                 EXIT FOREACH 
              END IF
              LET l_num=0
              #季节
              FOREACH aslr201_g01_repcur09_imaa132 USING l_sr9.imaa126,l_sr9.imaa156 INTO l_sr9.imaa132
                 #订货款数
                 SELECT count(DISTINCT imaa001) INTO l_sr9.qty01 FROM imaa_t,xmjd_t
                  WHERE xmjd008=imaa001 
                    AND xmjd001=p_xmjd001
                    AND xmjd002=p_xmjd002
                    AND xmjdent=g_enterprise
                    AND imaaent=xmjdent
                    AND xmjd015<>0
                    AND imaa126=l_sr9.imaa126
                    AND imaa156=l_sr9.imaa156
                    AND imaa132=l_sr9.imaa132
                  GROUP BY xmjd001,xmjd002,xmjdent,imaa132,imaa126,imaa156
                 
                 #订货数量
                 SELECT sum(xmjd015) INTO l_sr9.qty03 FROM imaa_t,xmjd_t
