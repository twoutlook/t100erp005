                         AND xmjdent='"||g_enterprise||"'
                       order by 1 "
           DECLARE aslr201_g01_repcur09_imaa132 CURSOR FROM l_sql 
           #品牌,性别
           LET l_sql=" select distinct imaa126,imaa156 
                       FROM imaa_t,xmjd_t
                      WHERE xmjd008=imaa001
                        AND imaaent=xmjdent
                        AND xmjd001='"||p_xmjd001||"' 
