                    AND imaaent=xmjdent
                    AND xmjd001=p_xmjd001
                    AND xmjd002=p_xmjd002
                    AND xmjdent=g_enterprise
                    AND xmjd015 is not null
                    AND imaa156=l_sr9.imaa156
                    AND imaa132=l_sr9.imaa132
                    AND imaa126=l_sr9.imaa126
                  GROUP BY xmjd001,xmjd002,xmjdent,imaa132,imaa126,imaa156
                 #订货金额
                SELECT sum(xmjd016) INTO l_sr9.amt FROM imaa_t,xmjd_t
                 WHERE xmjd008=imaa001
                   AND imaaent=xmjdent
                   AND xmjd001=p_xmjd001
                   AND xmjd002=p_xmjd002
                   AND xmjdent=g_enterprise
                   AND xmjd015 is not null
                   AND imaa132=l_sr9.imaa132
                   AND imaa156=l_sr9.imaa156
                   AND imaa126=l_sr9.imaa126
                 GROUP BY xmjd001,xmjd002,xmjdent,imaa132,imaa126,imaa156
               
                 LET l_sr9.types=1
                 INSERT INTO rep09_temp values(l_sr9.*)                 
