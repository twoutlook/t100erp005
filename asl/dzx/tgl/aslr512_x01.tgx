   l_xmdl050_desc LIKE type_t.chr1000, 
   l_imaa154 LIKE type_t.chr30, 
   rtax_t_rtax006 LIKE rtax_t.rtax006, 
   l_rtax006_desc LIKE type_t.chr30, 
   l_imaa133 LIKE type_t.chr30, 
