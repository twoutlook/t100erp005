DEFINE sr RECORD 
   l_xmdk000 LIKE type_t.chr100, 
   xmdkdocno LIKE xmdk_t.xmdkdocno, 
   xmdkdocdt LIKE xmdk_t.xmdkdocdt, 
   xmdk001 LIKE xmdk_t.xmdk001, 
   l_xmdkstus LIKE type_t.chr100, 
