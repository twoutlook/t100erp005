  <field cite_std="N" name="nmck_t.nmck004" ver="3" src="s" column="nmck004" table="nmck_t" attribute="" type="" req="N" i_zoom="q_nmas_01" c_zoom="" chk_ref="v_nmas002_1" items="" default="" max_compare="" max="" min_compare="" min="" can_edit="Y" can_query="N" widget="ButtonEdit" status="u">
    <![CDATA[if 款別 = 10 現金 then 
　　select nmas002 , nmas003 　　
   開窗 where 條件：　帳戶運用類型nmag002  = '5'  #零用金
end if 

if 款別 = 20 銀行電匯款 then 
　　select nmas002 , nmas003 　　
   開窗 where 條件：　帳戶運用類型nmag002  = '1'  #不限制存取 
end if 

if 款別 = 30 票據  then 
   開窗 帳戶運用類型nmag002  = '4'  #票據往來  
end if 

串帳戶類型
select nmag002 from nmag_t, nmaa_t 
   where nmaa003 = nmag001   # 帳戶類型  


帶值: 幣別 nmas003 
]]>
  </field>
