  <field cite_std="N" name="nmck_t.nmck003" ver="1" src="s" column="nmck003" table="nmck_t" attribute="" type="" req="Y" i_zoom="q_ooag001" c_zoom="" chk_ref="v_ooag001" items="" default="" max_compare="" max="" min_compare="" min="" can_edit="Y" can_query="N" widget="ButtonEdit" status="">
