  <section id="anmt420.b_fill" ver="8" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION anmt420_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2    STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE nmcd008,nmcd002,nmcd003,nmcd001,nmcd004,nmcd005,nmcd006,nmcd007,nmcd010, 
       nmcd002,nmcd003,nmcdownid,nmcdowndp,nmcdcrtid,nmcdcrtdp,nmcdcrtdt,nmcdmodid,nmcdmoddt ,t1.nmaal003 , 
       t2.oofa011 ,t3.oofa011 ,t4.oofa011 ,t5.ooefl003 ,t6.oofa011 ,t7.ooefl003 ,t8.oofa011 FROM nmcd_t", 

               "",
                              " LEFT JOIN nmaal_t t1 ON t1.nmaalent='"||g_enterprise||"' AND t1.nmaal001=nmcd001 AND t1.nmaal002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t2 ON t2.oofaent='"||g_enterprise||"' AND t2.oofa002='2' AND t2.oofa003=nmcd005  ",
               " LEFT JOIN oofa_t t3 ON t3.oofaent='"||g_enterprise||"' AND t3.oofa002='2' AND t3.oofa003=nmcd007  ",
               " LEFT JOIN oofa_t t4 ON t4.oofaent='"||g_enterprise||"' AND t4.oofa002='2' AND t4.oofa003=nmcdownid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=nmcdowndp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t6 ON t6.oofaent='"||g_enterprise||"' AND t6.oofa002='2' AND t6.oofa003=nmcdcrtid  ",
               " LEFT JOIN ooefl_t t7 ON t7.ooeflent='"||g_enterprise||"' AND t7.ooefl001=nmcdcrtdp AND t7.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t8 ON t8.oofaent='"||g_enterprise||"' AND t8.oofa002='2' AND t8.oofa003=nmcdmodid  ",
 
               " WHERE nmcdent= ? AND 1=1 AND ", p_wc2 
    
   LET g_sql = g_sql, cl_sql_add_filter("nmcd_t"),
                      " ORDER BY nmcd_t.nmcd002,nmcd_t.nmcd003"
   
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE anmt420_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR anmt420_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_nmcd_d.clear()
   CALL g_nmcd2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_nmcd_d[l_ac].nmcd008,g_nmcd_d[l_ac].nmcd002,g_nmcd_d[l_ac].nmcd003,g_nmcd_d[l_ac].nmcd001, 
       g_nmcd_d[l_ac].nmcd004,g_nmcd_d[l_ac].nmcd005,g_nmcd_d[l_ac].nmcd006,g_nmcd_d[l_ac].nmcd007,g_nmcd_d[l_ac].nmcd010, 
       g_nmcd2_d[l_ac].nmcd002,g_nmcd2_d[l_ac].nmcd003,g_nmcd2_d[l_ac].nmcdownid,g_nmcd2_d[l_ac].nmcdowndp, 
       g_nmcd2_d[l_ac].nmcdcrtid,g_nmcd2_d[l_ac].nmcdcrtdp,g_nmcd2_d[l_ac].nmcdcrtdt,g_nmcd2_d[l_ac].nmcdmodid, 
       g_nmcd2_d[l_ac].nmcdmoddt,g_nmcd_d[l_ac].nmcd001_desc,g_nmcd_d[l_ac].nmcd005_desc,g_nmcd_d[l_ac].nmcd007_desc, 
       g_nmcd2_d[l_ac].nmcdownid_desc,g_nmcd2_d[l_ac].nmcdowndp_desc,g_nmcd2_d[l_ac].nmcdcrtid_desc, 
       g_nmcd2_d[l_ac].nmcdcrtdp_desc,g_nmcd2_d[l_ac].nmcdmodid_desc
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL anmt420_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec THEN
      IF g_error_show = 1 THEN
         CALL cl_err("nmcd_t",9035,1)
      END IF
   END IF 
   LET g_error_show = 0
   
 
   
   CALL g_nmcd_d.deleteElement(g_nmcd_d.getLength())   
   CALL g_nmcd2_d.deleteElement(g_nmcd2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_nmcd_d.getLength()
      LET g_nmcd2_d[l_ac].nmcd002 = g_nmcd_d[l_ac].nmcd002 
      LET g_nmcd2_d[l_ac].nmcd003 = g_nmcd_d[l_ac].nmcd003 
 
   END FOR
   
   IF g_cnt > g_nmcd_d.getLength() THEN
      LET l_ac = g_nmcd_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_nmcd_d.getLength()
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE anmt420_pb
   
END FUNCTION
]]>
  </section>
