#單身 type 宣告
PRIVATE TYPE type_g_nmcd_d RECORD
       nmcd008 LIKE nmcd_t.nmcd008, 
   nmcd002 LIKE nmcd_t.nmcd002, 
   nmaf002_desc LIKE type_t.chr80, 
   nmcd003 LIKE nmcd_t.nmcd003, 
   nmcd001 LIKE nmcd_t.nmcd001, 
   nmcd001_desc LIKE type_t.chr500, 
   nmcd004 LIKE nmcd_t.nmcd004, 
   nmcd004_desc LIKE type_t.chr80, 
