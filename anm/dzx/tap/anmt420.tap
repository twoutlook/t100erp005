   LET g_forupd_sql = "SELECT nmcd008,nmcd002,nmcd003,nmcd001,nmcd004,nmcd005,nmcd006,nmcd007,nmcd010, 
       nmcd002,nmcd003,nmcdownid,nmcdowndp,nmcdcrtid,nmcdcrtdp,nmcdcrtdt,nmcdmodid,nmcdmoddt FROM nmcd_t  
       WHERE nmcdent=? AND nmcd002=? AND nmcd003=? FOR UPDATE"
