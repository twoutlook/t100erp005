<![CDATA[#+ 修改單身後其他table連動
PRIVATE FUNCTION anmi100_update_b(ps_table,ps_keys,ps_keys_bak,ps_page)
   DEFINE ps_page          STRING
   DEFINE ps_table         STRING
   DEFINE ps_keys          DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ps_keys_bak      DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ls_group         STRING
   DEFINE li_idx           LIKE type_t.num5 
   DEFINE lb_chk           BOOLEAN
   DEFINE l_new_key        DYNAMIC ARRAY OF STRING
   DEFINE l_old_key        DYNAMIC ARRAY OF STRING
   DEFINE l_field_key      DYNAMIC ARRAY OF STRING
   #add-point:update_b段define
   {<point name="update_b.define"/>}
   #end add-point     
   
   #判斷key是否有改變
   LET lb_chk = TRUE
   FOR li_idx = 1 TO ps_keys.getLength()
      IF ps_keys[li_idx] <> ps_keys_bak[li_idx] THEN
         LET lb_chk = FALSE
         EXIT FOR
      END IF
   END FOR
   
   #不需要做處理
   IF lb_chk THEN
      RETURN
   END IF
    
   #判斷是否是同一群組的table
   LET ls_group = "nmab_t,"
   IF ls_group.getIndexOf(ps_table,1) > 0 AND ps_table <> "nmab_t" THEN
      #add-point:update_b段修改前
      {<point name="update_b.b_update" mark="Y"/>}
      #end add-point     
      UPDATE nmab_t 
         SET (nmab001
              ,nmab008,nmab003,nmab009,nmab002,nmab004,nmab005,nmab006,nmab007,nmabstus,nmabmodid,nmabmoddt,nmabownid,nmabowndp,nmabcrtid,nmabcrtdp,nmabcrtdt) 
              = 
             (ps_keys[1]
              ,g_nmab_d[l_ac].nmab008,g_nmab_d[l_ac].nmab003,g_nmab_d[l_ac].nmab009,g_nmab_d[l_ac].nmab002, 
                  g_nmab_d[l_ac].nmab004,g_nmab_d[l_ac].nmab005,g_nmab_d[l_ac].nmab006,g_nmab_d[l_ac].nmab007, 
                  g_nmab_d[l_ac].nmabstus,g_nmab1_info_d[l_ac].nmabmodid,g_nmab1_info_d[l_ac].nmabmoddt, 
                  g_nmab1_info_d[l_ac].nmabownid,g_nmab1_info_d[l_ac].nmabowndp,g_nmab1_info_d[l_ac].nmabcrtid, 
                  g_nmab1_info_d[l_ac].nmabcrtdp,g_nmab1_info_d[l_ac].nmabcrtdt) 
         WHERE nmab001 = ps_keys_bak[1]
      #add-point:update_b段修改中
      {<point name="update_b.m_update"/>}
      #end add-point 
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            CALL cl_err("nmab_t","std-00009",1)
            CALL s_transaction_end('N','0')
         WHEN SQLCA.sqlcode #其他錯誤
            CALL cl_err("nmab_t",SQLCA.sqlcode,1)  
            CALL s_transaction_end('N','0')
         OTHERWISE
            
      END CASE
      #add-point:update_b段修改後
      {<point name="update_b.a_update"/>}
      #end add-point 
      RETURN
   END IF
   
 
   
END FUNCTION
]]>
