#單身 type 宣告
PRIVATE TYPE type_g_nmab_d RECORD
       nmab008 LIKE nmab_t.nmab008, 
   nmab008_desc LIKE type_t.chr500, 
   nmab001 LIKE nmab_t.nmab001, 
   nmabl003 LIKE nmabl_t.nmabl003, 
   nmabl004 LIKE nmabl_t.nmabl004, 
   nmab003 LIKE nmab_t.nmab003, 
   nmab009 LIKE nmab_t.nmab009, 
   nmab002 LIKE nmab_t.nmab002, 
   nmab004 LIKE nmab_t.nmab004, 
   nmab005 LIKE nmab_t.nmab005, 
   nmab006 LIKE nmab_t.nmab006, 
   nmab007 LIKE nmab_t.nmab007, 
   nmabstus LIKE nmab_t.nmabstus
       END RECORD
PRIVATE TYPE type_g_nmab1_info_d RECORD
       nmab001 LIKE nmab_t.nmab001, 
   nmabmodid LIKE nmab_t.nmabmodid, 
   nmabmodid_desc LIKE type_t.chr500, 
   nmabmoddt DATETIME YEAR TO SECOND, 
   nmabownid LIKE nmab_t.nmabownid, 
   nmabownid_desc LIKE type_t.chr500, 
   nmabowndp LIKE nmab_t.nmabowndp, 
   nmabowndp_desc LIKE type_t.chr500, 
   nmabcrtid LIKE nmab_t.nmabcrtid, 
   nmabcrtid_desc LIKE type_t.chr500, 
   nmabcrtdp LIKE nmab_t.nmabcrtdp, 
   nmabcrtdp_desc LIKE type_t.chr500, 
   nmabcrtdt DATETIME YEAR TO SECOND
       END RECORD
 
 
DEFINE g_detail_multi_table_t    RECORD
      nmabl001 LIKE nmabl_t.nmabl001,
      nmabl002 LIKE nmabl_t.nmabl002,
      nmabl003 LIKE nmabl_t.nmabl003,
      nmabl004 LIKE nmabl_t.nmabl004
      END RECORD
 
#模組變數(Module Variables)
DEFINE g_nmab_d          DYNAMIC ARRAY OF type_g_nmab_d #單身變數
DEFINE g_nmab_d_t        type_g_nmab_d                  #單身備份
DEFINE g_nmab_d_o        type_g_nmab_d                  #單身備份
