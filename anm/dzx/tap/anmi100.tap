  <section id="anmi100.modify" ver="9" status="" src="s">
<![CDATA[#+ 資料修改
PRIVATE FUNCTION anmi100_modify()
   DEFINE  l_cmd                  LIKE type_t.chr1
   DEFINE  l_ac_t                 LIKE type_t.num5                #未取消的ARRAY CNT 
   DEFINE  l_n                    LIKE type_t.num5                #檢查重複用  
   DEFINE  l_cnt                  LIKE type_t.num5                #檢查重複用  
   DEFINE  l_lock_sw              LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert         LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete         LIKE type_t.num5                #可刪除否  
   DEFINE  l_count                LIKE type_t.num5
   DEFINE  l_i                    LIKE type_t.num5
   DEFINE  ls_return              STRING
   DEFINE  l_var_keys             DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys           DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                 DYNAMIC ARRAY OF STRING
   DEFINE  l_fields               DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak         DYNAMIC ARRAY OF STRING
   DEFINE  li_reproduce           LIKE type_t.num5
   DEFINE  li_reproduce_target    LIKE type_t.num5
   DEFINE  lb_reproduce           BOOLEAN
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point 
   
   LET g_action_choice = ""
   
   LET g_qryparam.state = "i"
 
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   
   #add-point:modify開始前
   {<point name="modify.define_sql" mark="Y"/>}
   #end add-point
   
   LET INT_FLAG = FALSE
   LET lb_reproduce = FALSE
 
   #add-point:modify段修改前
   {<point name="modify.before_input"/>}
   #end add-point
 
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #Page1 預設值產生於此處
      INPUT ARRAY g_nmab_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_detail_cnt,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
 
         #+ 此段落由子樣板a43產生
         ON ACTION update_item
            LET g_action_choice="update_item"
            IF cl_auth_chk_act("update_item") THEN
               
               #add-point:ON ACTION update_item
               {<point name="input.detail_input.page1.update_item" />}
               #END add-point
            END IF
 
 
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_nmab_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL anmi100_b_fill(g_wc2)
            LET g_detail_cnt = g_nmab_d.getLength()
         
         BEFORE ROW
            #add-point:modify段before row
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_cmd = ''
            LET l_ac = g_detail_idx
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            DISPLAY g_nmab_d.getLength() TO FORMONLY.cnt
         
            CALL s_transaction_begin()
            LET g_detail_cnt = g_nmab_d.getLength()
            
            IF g_detail_cnt >= l_ac 
               AND g_nmab_d[l_ac].nmab001 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_nmab_d_t.* = g_nmab_d[l_ac].*  #BACKUP
               LET g_nmab_d_o.* = g_nmab_d[l_ac].*  #BACKUP
               IF NOT anmi100_lock_b("nmab_t") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH anmi100_bcl INTO g_nmab_d[l_ac].nmab008,g_nmab_d[l_ac].nmab001,g_nmab_d[l_ac].nmab003, 
                      g_nmab_d[l_ac].nmab009,g_nmab_d[l_ac].nmab002,g_nmab_d[l_ac].nmab004,g_nmab_d[l_ac].nmab005, 
                      g_nmab_d[l_ac].nmab006,g_nmab_d[l_ac].nmab007,g_nmab_d[l_ac].nmabstus,g_nmab1_info_d[l_ac].nmab001, 
                      g_nmab1_info_d[l_ac].nmabmodid,g_nmab1_info_d[l_ac].nmabmoddt,g_nmab1_info_d[l_ac].nmabownid, 
                      g_nmab1_info_d[l_ac].nmabowndp,g_nmab1_info_d[l_ac].nmabcrtid,g_nmab1_info_d[l_ac].nmabcrtdp, 
                      g_nmab1_info_d[l_ac].nmabcrtdt
                  IF SQLCA.sqlcode THEN
                     CALL cl_err(g_nmab_d_t.nmab001,SQLCA.sqlcode,1)
                     LET l_lock_sw = "Y"
                  END IF
                  
                  CALL anmi100_detail_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            LET g_detail_multi_table_t.nmabl001 = g_nmab_d[l_ac].nmab001
LET g_detail_multi_table_t.nmabl002 = g_dlang
LET g_detail_multi_table_t.nmabl003 = g_nmab_d[l_ac].nmabl003
LET g_detail_multi_table_t.nmabl004 = g_nmab_d[l_ac].nmabl004
 
            #其他table進行lock
                        INITIALIZE l_var_keys TO NULL 
            INITIALIZE l_field_keys TO NULL 
            LET l_field_keys[01] = 'nmabl001'
            LET l_var_keys[01] = g_nmab_d[l_ac].nmab001
            LET l_field_keys[02] = 'nmabl002'
            LET l_var_keys[02] = g_dlang
            IF NOT cl_multitable_lock(l_var_keys,l_field_keys,'nmabl_t') THEN
               RETURN 
            END IF 
 
        
         BEFORE INSERT
            
            CALL s_transaction_begin()
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_nmab_d_t.* TO NULL
            INITIALIZE g_nmab_d_o.* TO NULL
            INITIALIZE g_nmab_d[l_ac].* TO NULL 
            #公用欄位給值(單身)
            #此段落由子樣板a14產生    
      LET g_nmab1_info_d[l_ac].nmabownid = g_user
      LET g_nmab1_info_d[l_ac].nmabowndp = g_dept
      LET g_nmab1_info_d[l_ac].nmabcrtid = g_user
      LET g_nmab1_info_d[l_ac].nmabcrtdp = g_dept 
      LET g_nmab1_info_d[l_ac].nmabcrtdt = cl_get_current()
      LET g_nmab1_info_d[l_ac].nmabmodid = ""
      LET g_nmab1_info_d[l_ac].nmabmoddt = ""
      LET g_nmab_d[l_ac].nmabstus = ""
 
 
                  LET g_nmab_d[l_ac].nmab002 = "1"
 
            LET g_nmab_d_t.* = g_nmab_d[l_ac].*     #新輸入資料
            LET g_nmab_d_o.* = g_nmab_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL anmi100_set_entry_b("a")
            CALL anmi100_set_no_entry_b("a")
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_nmab_d[li_reproduce_target].* = g_nmab_d[li_reproduce].*
               LET g_nmab1_info_d[li_reproduce_target].* = g_nmab1_info_d[li_reproduce].*
 
               LET g_nmab_d[g_nmab_d.getLength()].nmab001 = NULL
 
            END IF
            LET g_detail_multi_table_t.nmabl001 = g_nmab_d[l_ac].nmab001
LET g_detail_multi_table_t.nmabl002 = g_dlang
LET g_detail_multi_table_t.nmabl003 = g_nmab_d[l_ac].nmabl003
LET g_detail_multi_table_t.nmabl004 = g_nmab_d[l_ac].nmabl004
 
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM nmab_t 
             WHERE nmabent = g_enterprise AND nmab001 = g_nmab_d[l_ac].nmab001
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前
               {<point name="input.body.b_insert"/>}
               #end add-point
            
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_nmab_d[g_detail_idx].nmab001
               CALL anmi100_insert_b('nmab_t',gs_keys,"'1'")
                           
               #add-point:單身新增後
               {<point name="input.body.a_insert"/>}
               #end add-point
