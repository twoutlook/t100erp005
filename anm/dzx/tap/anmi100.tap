  <section id="anmi100.b_fill" ver="8" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION anmi100_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2    STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE nmab008,nmab001,nmab003,nmab009,nmab002,nmab004,nmab005,nmab006,nmab007, 
       nmabstus,nmab001,nmabmodid,nmabmoddt,nmabownid,nmabowndp,nmabcrtid,nmabcrtdp,nmabcrtdt ,t1.oocgl003 , 
       t2.ooag011 ,t3.ooag011 ,t4.ooefl003 ,t5.ooag011 ,t6.ooefl003 FROM nmab_t",
               " LEFT JOIN nmabl_t ON nmab001 = nmabl001 AND nmabl002 = '",g_lang,"'",
                              " LEFT JOIN oocgl_t t1 ON t1.oocglent='"||g_enterprise||"' AND t1.oocgl001=nmab008 AND t1.oocgl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t2 ON t2.ooagent='"||g_enterprise||"' AND t2.ooag001=nmabmodid  ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent='"||g_enterprise||"' AND t3.ooag001=nmabownid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=nmabowndp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent='"||g_enterprise||"' AND t5.ooag001=nmabcrtid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent='"||g_enterprise||"' AND t6.ooefl001=nmabcrtdp AND t6.ooefl002='"||g_dlang||"' ",
 
               " WHERE nmabent= ? AND 1=1 AND ", p_wc2 
    
   LET g_sql = g_sql, cl_sql_add_filter("nmab_t"),
                      " ORDER BY nmab_t.nmab001"
   
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE anmi100_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR anmi100_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_nmab_d.clear()
   CALL g_nmab1_info_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_nmab_d[l_ac].nmab008,g_nmab_d[l_ac].nmab001,g_nmab_d[l_ac].nmab003,g_nmab_d[l_ac].nmab009, 
       g_nmab_d[l_ac].nmab002,g_nmab_d[l_ac].nmab004,g_nmab_d[l_ac].nmab005,g_nmab_d[l_ac].nmab006,g_nmab_d[l_ac].nmab007, 
       g_nmab_d[l_ac].nmabstus,g_nmab1_info_d[l_ac].nmab001,g_nmab1_info_d[l_ac].nmabmodid,g_nmab1_info_d[l_ac].nmabmoddt, 
       g_nmab1_info_d[l_ac].nmabownid,g_nmab1_info_d[l_ac].nmabowndp,g_nmab1_info_d[l_ac].nmabcrtid, 
       g_nmab1_info_d[l_ac].nmabcrtdp,g_nmab1_info_d[l_ac].nmabcrtdt,g_nmab_d[l_ac].nmab008_desc,g_nmab1_info_d[l_ac].nmabmodid_desc, 
       g_nmab1_info_d[l_ac].nmabownid_desc,g_nmab1_info_d[l_ac].nmabowndp_desc,g_nmab1_info_d[l_ac].nmabcrtid_desc, 
       g_nmab1_info_d[l_ac].nmabcrtdp_desc
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL anmi100_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec THEN
      IF g_error_show = 1 THEN
         CALL cl_err("nmab_t",9035,1)
      END IF
   END IF 
   LET g_error_show = 0
   
 
   
   CALL g_nmab_d.deleteElement(g_nmab_d.getLength())   
   CALL g_nmab1_info_d.deleteElement(g_nmab1_info_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_nmab_d.getLength()
      LET g_nmab1_info_d[l_ac].nmab001 = g_nmab_d[l_ac].nmab001 
 
   END FOR
   
   IF g_cnt > g_nmab_d.getLength() THEN
      LET l_ac = g_nmab_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_nmab_d.getLength()
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE anmi100_pb
   
END FUNCTION
]]>
</section>
  <section id="anmi100.default_search" ver="4" status="" src="s">
<![CDATA[#+ 外部參數搜尋
PRIVATE FUNCTION anmi100_default_search()
   DEFINE li_idx  LIKE type_t.num5
   DEFINE li_cnt  LIKE type_t.num5
   DEFINE ls_wc   STRING
   #add-point:default_search段define
   {<point name="default_search.define"/>}
   #end add-point  
   
   IF NOT cl_null(g_argv[01]) THEN
      LET ls_wc = ls_wc, " nmab001 = '", g_argv[01], "' AND "
   END IF
   
 
   
   #add-point:default_search段after sql
   {<point name="default_search.after_sql"/>}
   #end add-point  
   
   IF NOT cl_null(ls_wc) THEN
      LET ls_wc = ls_wc.subString(1,ls_wc.getLength()-5)
      LET g_wc2 = ls_wc
   ELSE
      LET g_wc2 = " 1=1"
   END IF
 
END FUNCTION
]]>
</section>
  <section id="anmi100.delete" ver="2" status="" src="s">
<![CDATA[#+ 資料刪除
PRIVATE FUNCTION anmi100_delete()
   DEFINE li_idx           LIKE type_t.num5
   DEFINE li_detail_tmp    LIKE type_t.num5
   DEFINE l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE l_var_keys_bak  DYNAMIC ARRAY OF STRING
   DEFINE l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE l_vars          DYNAMIC ARRAY OF STRING
   DEFINE l_fields        DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point 
   
   #add-point:單身刪除前
   {<point name="delete.body.before_delete"/>}
   #end add-point
   
   CALL s_transaction_begin()
   
   LET li_detail_tmp = g_detail_idx
    
   #lock所有所選資料
   FOR li_idx = 1 TO g_nmab_d.getLength()
      LET g_detail_idx = li_idx
      #已選擇的資料
      IF g_curr_diag.isRowSelected(g_curr_diag.getCurrentItem(), li_idx) THEN 
         #確定是否有被鎖定
         IF NOT anmi100_lock_b("nmab_t") THEN
            #已被他人鎖定
            CALL s_transaction_end('N','0')
            RETURN
         END IF
      END IF
   END FOR
   
   #add-point:單身刪除詢問前
   {<point name="delete.body.b_delete_ask"/>}
   #end add-point  
   
   #詢問是否確定刪除所選資料
   IF NOT cl_ask_del_detail() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   FOR li_idx = 1 TO g_nmab_d.getLength()
      IF g_nmab_d[li_idx].nmab001 IS NOT NULL
 
         AND g_curr_diag.isRowSelected(g_curr_diag.getCurrentItem(), li_idx) THEN 
         
         #add-point:單身刪除前
         {<point name="delete.body.b_delete" mark="Y"/>}
         #end add-point   
         
         DELETE FROM nmab_t
          WHERE nmabent = g_enterprise AND 
                nmab001 = g_nmab_d[li_idx].nmab001
 
         #add-point:單身刪除中
         {<point name="delete.body.m_delete"/>}
         #end add-point  
                
         IF SQLCA.sqlcode THEN
            CALL cl_err("nmab_t",SQLCA.sqlcode,1)
            CALL s_transaction_end('N','0')
            RETURN
         ELSE
            LET g_detail_cnt = g_detail_cnt-1
            INITIALIZE l_var_keys_bak TO NULL 
                  INITIALIZE l_field_keys TO NULL 
                  LET l_field_keys[01] = 'nmabl001'
                  LET l_field_keys[02] = 'nmabl002'
                  LET l_var_keys_bak[01] = g_detail_multi_table_t.nmabl001
                  LET l_var_keys_bak[02] = g_detail_multi_table_t.nmabl002
                  CALL cl_multitable_delete(l_field_keys,l_var_keys_bak,'nmabl_t')
 
            #add-point:單身同步刪除前(同層table)
            {<point name="delete.body.a_delete"/>}
            #end add-point
            LET g_detail_idx = li_idx
                           INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_nmab_d[g_detail_idx].nmab001
 
            #add-point:單身同步刪除中(同層table)
            {<point name="delete.body.a_delete2"/>}
            #end add-point
                           CALL anmi100_delete_b('nmab_t',gs_keys,"'1'")
            #add-point:單身同步刪除後(同層table)
            {<point name="delete.body.a_delete3"/>}
            #end add-point
         END IF 
      END IF 
    
   END FOR
   CALL s_transaction_end('Y','0')
   
   LET g_detail_idx = li_detail_tmp
            
   #add-point:單身刪除後
   {<point name="delete.after"/>}
   #end add-point  
   
   #刷新資料
   CALL anmi100_b_fill(g_wc2)
            
END FUNCTION
]]>
</section>
  <section id="anmi100.delete_b" ver="3" status="" src="s">
<![CDATA[#+ 刪除單身後其他table連動
PRIVATE FUNCTION anmi100_delete_b(ps_table,ps_keys_bak,ps_page)
   DEFINE ps_table    STRING
   DEFINE ps_page     STRING
   DEFINE ps_keys_bak DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ls_group    STRING
   #add-point:delete_b段define
   {<point name="delete_b.define"/>}
   #end add-point     
 
   #判斷是否是同一群組的table
   LET ls_group = "nmab_t,"
   IF ls_group.getIndexOf(ps_table,1) > 0 AND ps_table <> 'nmab_t' THEN
   
      #add-point:delete_b段刪除前
      {<point name="delete_b.b_delete" mark="Y"/>}
      #end add-point     
   
      DELETE FROM nmab_t
       WHERE nmabent = g_enterprise AND
         nmab001 = ps_keys_bak[1]
 
      #add-point:delete_b段刪除中
      {<point name="delete_b.m_delete"/>}
      #end add-point  
         
      IF SQLCA.sqlcode THEN
         CALL cl_err("",SQLCA.sqlcode,0)
      END IF
      
      #add-point:delete_b段刪除後
      {<point name="delete_b.a_delete"/>}
      #end add-point
      
      RETURN
   END IF
   
 
   
END FUNCTION
]]>
</section>
  <section id="anmi100.description" ver="10" status="" src="s">
<![CDATA[#+ Version..: T100-ERP-1.00.00(SD版次:5,PR版次:5) Build-000133
#+ 
