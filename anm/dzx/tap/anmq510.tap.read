  <section id="anmq510.b_fill" ver="7" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION anmq510_b_fill()
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準
   {<point name="b_fill.define" edit="s"/>}
   #end add-point
   #add-point:b_fill段define-客製
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   LET ls_sql_rank = "SELECT  UNIQUE nmbbseq,nmbb042,'',nmbbdocno,nmbb030,nmbb028,'','',nmbb065,nmbb031, 
       nmbb004,nmbb006,nmbb008,nmbb045,'','',nmbb003,'',nmbb046,nmbbcomp  ,DENSE_RANK() OVER( ORDER BY nmbb_t.nmbbseq) AS RANK FROM nmbb_t", 

 
 
                     "",
                     " WHERE nmbbent=? AND nmbbcomp=? AND nmbbdocno=? AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("nmbb_t"),
                     " ORDER BY nmbb_t.nmbbseq"
 
   #add-point:b_fill段rank_sql_after
   {<point name="b_fill.rank_sql_after"/>}
   #end add-point
 
   LET g_sql = "SELECT COUNT(*) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT nmbbseq,nmbb042,'',nmbbdocno,nmbb030,nmbb028,'','',nmbb065,nmbb031,nmbb004,nmbb006, 
       nmbb008,nmbb045,'','',nmbb003,'',nmbb046,nmbbcomp",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE anmq510_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR anmq510_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_nmbb_d.clear()
 
   #add-point:陣列清空
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1   
 
   FOREACH b_fill_curs INTO g_nmbb_d[l_ac].nmbbseq,g_nmbb_d[l_ac].nmbb042,g_nmbb_d[l_ac].nmbastus,g_nmbb_d[l_ac].nmbbdocno, 
       g_nmbb_d[l_ac].nmbb030,g_nmbb_d[l_ac].nmbb028,g_nmbb_d[l_ac].nmbb028_desc,g_nmbb_d[l_ac].nmbadocdt, 
       g_nmbb_d[l_ac].nmbb065,g_nmbb_d[l_ac].nmbb031,g_nmbb_d[l_ac].nmbb004,g_nmbb_d[l_ac].nmbb006,g_nmbb_d[l_ac].nmbb008, 
       g_nmbb_d[l_ac].nmbb045,g_nmbb_d[l_ac].nmba004,g_nmbb_d[l_ac].nmbb046_desc,g_nmbb_d[l_ac].nmbb003, 
       g_nmbb_d[l_ac].nmbb003_desc,g_nmbb_d[l_ac].nmbb046,g_nmbb_d[l_ac].nmbbcomp
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #LET g_nmbb_d[l_ac].statepic = cl_get_actipic(g_nmbb_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL anmq510_detail_show("'1'")      
 
      CALL anmq510_nmbb_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
   LET g_error_show = 0
   
 
   
   CALL g_nmbb_d.deleteElement(g_nmbb_d.getLength())   
 
   #add-point:陣列長度調整
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = g_nmbb_d.getLength()
#  DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE anmq510_pb
 
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL anmq510_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL anmq510_detail_action_trans()
 
   IF g_nmbb_d.getLength() > 0 THEN
      LET l_ac = 1
      CALL anmq510_fetch()
   END IF
   
      CALL anmq510_filter_show('nmbbseq','b_nmbbseq')
   CALL anmq510_filter_show('nmbb042','b_nmbb042')
   CALL anmq510_filter_show('nmbbdocno','b_nmbbdocno')
   CALL anmq510_filter_show('nmbb030','b_nmbb030')
   CALL anmq510_filter_show('nmbb028','b_nmbb028')
   CALL anmq510_filter_show('nmbb065','b_nmbb065')
   CALL anmq510_filter_show('nmbb031','b_nmbb031')
   CALL anmq510_filter_show('nmbb004','b_nmbb004')
   CALL anmq510_filter_show('nmbb006','b_nmbb006')
   CALL anmq510_filter_show('nmbb008','b_nmbb008')
   CALL anmq510_filter_show('nmbb045','b_nmbb045')
   CALL anmq510_filter_show('nmbb003','b_nmbb003')
   CALL anmq510_filter_show('nmbb046','b_nmbb046')
   CALL anmq510_filter_show('nmbbcomp','b_nmbbcomp')
 
 
   #add-point:b_fill段結束前
   {<point name="b_fill.after"/>}
   #end add-point
 
END FUNCTION
]]>
  </section>
