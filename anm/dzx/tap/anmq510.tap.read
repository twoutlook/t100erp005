  <point name="function.anmq510_print" order="3" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL s_aooi150_ins (传入参数)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION anmq510_print()
   DEFINE l_i         LIKE type_t.num10
   
   IF g_nmbb_d.getLength() <= 0 THEN
      RETURN
   END IF

   #將所需資料插入臨時表
   DELETE FROM anmq510_xg_tmp;
   
   FOR l_i=1 TO g_nmbb_d.getLength()
       INSERT INTO anmq510_xg_tmp VALUES ( 
       l_i,g_nmbb_d[l_i].nmbbseq,g_nmbb_d[l_i].nmbb042,g_nmbb_d[l_i].nmbastus,g_nmbb_d[l_i].nmbbdocno, 
       g_nmbb_d[l_i].nmbb030,g_nmbb_d[l_i].nmbb028,g_nmbb_d[l_i].nmbb028_desc,g_nmbb_d[l_i].nmbadocdt, 
      #g_nmbb_d[l_i].nmbb065,g_nmbb_d[l_i].nmbb031,g_nmbb_d[l_i].nmbb004,g_nmbb_d[l_i].nmbb006,g_nmbb_d[l_i].nmbb045,                         #150616-00026#8 mark
       g_nmbb_d[l_i].nmbb065,g_nmbb_d[l_i].nmbb031,g_nmbb_d[l_i].nmbb004,g_nmbb_d[l_i].nmbb006,g_nmbb_d[l_i].nmbb008,g_nmbb_d[l_i].nmbb045,   #150616-00026#8
       g_nmbb_d[l_i].nmba004,g_nmbb_d[l_i].nmbb046_desc,g_nmbb_d[l_i].nmbb003,g_nmbb_d[l_i].nmbb003_desc, 
       g_nmbb_d[l_i].nmbb046,g_nmbb_d[l_i].nmbbcomp)        
   END FOR 
   
   CALL anmq510_x01('anmq510_xg_tmp',' 1=1')
END FUNCTION]]>
