   DECLARE anmq510_bcl CURSOR FROM g_forupd_sql
    
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_anmq510 WITH FORM cl_ap_formpath("anm",g_code)
   
      #瀏覽頁簽資料初始化
