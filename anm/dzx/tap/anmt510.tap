            IF cl_auth_chk_act("prog_nmba08") THEN
               
               #add-point:ON ACTION prog_nmba08
               {<point name="menu2.prog_nmba08" />}
               #END add-point
               
            END IF
 
 
 
            
            #應用 a32 樣板自動產生(Version:2)
         #簽核狀況
         ON ACTION bpm_status
