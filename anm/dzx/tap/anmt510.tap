               #END add-point
               
            END IF
 
 
 
         #應用 a43 樣板自動產生(Version:2)
         ON ACTION unverify
            LET g_action_choice="unverify"
            IF cl_auth_chk_act("unverify") THEN
               
               #add-point:ON ACTION unverify
               {<point name="menu2.unverify" />}
               #END add-point
               
            END IF
 
 
 
         #應用 a43 樣板自動產生(Version:2)
         ON ACTION prog_nmba002
            LET g_action_choice="prog_nmba002"
