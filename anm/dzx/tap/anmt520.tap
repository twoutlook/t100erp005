  <section id="anmt520.reproduce" ver="20" status="" src="s" readonly="">
    <![CDATA[#+ 資料複製
PRIVATE FUNCTION anmt520_reproduce()
   DEFINE l_newno     LIKE nmcq_t.nmcqcomp 
   DEFINE l_oldno     LIKE nmcq_t.nmcqcomp 
   DEFINE l_newno02     LIKE nmcq_t.nmcqdocno 
   DEFINE l_oldno02     LIKE nmcq_t.nmcqdocno 
 
   DEFINE l_master    RECORD LIKE nmcq_t.*
   DEFINE l_detail    RECORD LIKE nmcr_t.*
 
 
   DEFINE l_cnt       LIKE type_t.num10
   #add-point:reproduce段define
   {<point name="reproduce.define" edit="s"/>}
   #end add-point   
   #add-point:reproduce段define(客製用)
   {<point name="reproduce.define_customerization" edit="c"/>}
   #end add-point   
   
   #切換畫面
   
   LET g_master_insert = FALSE
   
   IF g_nmcq_m.nmcqcomp IS NULL
   OR g_nmcq_m.nmcqdocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
    
   LET g_nmcqcomp_t = g_nmcq_m.nmcqcomp
   LET g_nmcqdocno_t = g_nmcq_m.nmcqdocno
 
    
   LET g_nmcq_m.nmcqcomp = ""
   LET g_nmcq_m.nmcqdocno = ""
 
 
   CALL cl_set_head_visible("","YES")
 
   #公用欄位給予預設值
   #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_nmcq_m.nmcqownid = g_user
      LET g_nmcq_m.nmcqowndp = g_dept
      LET g_nmcq_m.nmcqcrtid = g_user
      LET g_nmcq_m.nmcqcrtdp = g_dept 
      LET g_nmcq_m.nmcqcrtdt = cl_get_current()
      LET g_nmcq_m.nmcqmodid = g_user
      LET g_nmcq_m.nmcqmoddt = cl_get_current()
      LET g_nmcq_m.nmcqstus = 'N'
 
 
   
   CALL s_transaction_begin()
   
   #add-point:複製輸入前
   {<point name="reproduce.head.b_input"/>}
   #end add-point
   
   #顯示狀態(stus)圖片
         #應用 a21 樣板自動產生(Version:2)
	  #根據當下狀態碼顯示圖片
      CASE g_nmcq_m.nmcqstus 
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "A"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/approved.png")
         WHEN "D"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/withdraw.png")
         WHEN "R"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/rejection.png")
         WHEN "W"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/signing.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
 
 
   
   #清空key欄位的desc
      LET g_nmcq_m.nmcqcomp_desc = ''
   DISPLAY BY NAME g_nmcq_m.nmcqcomp_desc
 
   
   CALL anmt520_input("r")
   
   IF INT_FLAG AND NOT g_master_insert THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = '' 
      LET g_errparam.code   = 9001 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      CALL s_transaction_end('N','0')
      LET INT_FLAG = 0
      DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
      DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
      LET INT_FLAG = 0
      INITIALIZE g_nmcq_m.* TO NULL
      INITIALIZE g_nmcr_d TO NULL
      INITIALIZE g_nmcr2_d TO NULL
 
      #add-point:複製取消後
      {<point name="reproduce.cancel" />}
      #end add-point
      CALL anmt520_show()
      RETURN
   END IF
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL anmt520_set_act_visible()   
   CALL anmt520_set_act_no_visible()
   
   #將新增的資料併入搜尋條件中
   LET g_nmcqcomp_t = g_nmcq_m.nmcqcomp
   LET g_nmcqdocno_t = g_nmcq_m.nmcqdocno
 
   
   #組合新增資料的條件
   LET g_add_browse = " nmcqent = '" ||g_enterprise|| "' AND",
                      " nmcqcomp = '", g_nmcq_m.nmcqcomp, "' "
                      ," AND nmcqdocno = '", g_nmcq_m.nmcqdocno, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL anmt520_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   #add-point:完成複製段落後
   {<point name="reproduce.after_reproduce" />}
   #end add-point
   
   CALL anmt520_idx_chk()
   
   #功能已完成,通報訊息中心
   CALL anmt520_msgcentre_notify('reproduce')
 
END FUNCTION
]]>
