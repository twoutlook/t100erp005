  <point name="function.anmt520_ins_nmbc" order="15" ver="3" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 插入銀存異動資料
# Memo...........:
# Usage..........: CALL anmt520_ins_nmbc(p_nmcrseq)
#                  RETURNING l_success
# Input parameter: p_nmcrseq      項次
# Return code....: l_success      出入結果
# Modify.........:
################################################################################
PRIVATE FUNCTION anmt520_ins_nmbc(p_nmcrseq)
   DEFINE p_nmcrseq   LIKE nmcr_t.nmcrseq
   DEFINE l_nmcr      RECORD LIKE nmcr_t.*
   DEFINE l_nmbc      RECORD LIKE nmbc_t.*
   DEFINE l_nmbb002   LIKE nmbb_t.nmbb002
   DEFINE l_nmbb026   LIKE nmbb_t.nmbb026
   DEFINE l_nmbb027   LIKE nmbb_t.nmbb027
   DEFINE l_success   LIKE type_t.num5
   
   #異動明細資料
   SELECT nmcr_t.* INTO l_nmcr.* FROM nmcr_t
    WHERE nmcrent=g_enterprise AND nmcrcomp=g_nmcq_m.nmcqcomp
      AND nmcrdocno=g_nmcq_m.nmcqdocno AND nmcrseq=p_nmcrseq
   #支票資料
   SELECT nmbb002,nmbb026,nmbb027 INTO l_nmbb002,l_nmbb026,l_nmbb027 
     FROM nmbb_t
    WHERE nmbbent=g_enterprise AND nmbbcomp=g_nmcq_m.nmcqcomp
      AND nmbb030=l_nmcr.nmcr001 AND nmbbdocno=l_nmcr.nmcr003
   LET l_success = TRUE   
   INITIALIZE l_nmbc.* TO NULL
   LET l_nmbc.nmbcent =g_enterprise
   LET l_nmbc.nmbccomp=g_nmcq_m.nmcqcomp
   LET l_nmbc.nmbcsite = g_nmcq_m.nmcqsite
   LET l_nmbc.nmbcdocno = g_nmcq_m.nmcqdocno
   LET l_nmbc.nmbcseq = l_nmcr.nmcrseq
   LET l_nmbc.nmbc001 = "anmt520"         #單據來源
   LET l_nmbc.nmbc002 = g_nmcq_m.nmcq003  #交易帳戶編碼 
   LET l_nmbc.nmbc003 = l_nmbb026         #交易對象
   LET l_nmbc.nmbc004 = l_nmbb027         #交易對象識別碼
   LET l_nmbc.nmbc005 = g_nmcq_m.nmcqdocdt#銀行日期
   LET l_nmbc.nmbc006 = '1'               #異動別
   LET l_nmbc.nmbc007 = l_nmbb002         #存提碼
   LET l_nmbc.nmbc100 = l_nmcr.nmcr100    #幣別
   LET l_nmbc.nmbc101 = l_nmcr.nmcr101    #匯率
   #票貼
   IF g_nmcq_m.nmcq001='4' THEN
      LET l_nmbc.nmbc103 = l_nmcr.nmcr106
      LET l_nmbc.nmbc113 = l_nmcr.nmcr116
   ELSE
      #兌現
      LET l_nmbc.nmbc103 = l_nmcr.nmcr103+l_nmcr.nmcr104-l_nmcr.nmcr107
      LET l_nmbc.nmbc113 = l_nmcr.nmcr113+l_nmcr.nmcr114-l_nmcr.nmcr117
   END IF
   
   LET l_nmbc.nmbc121 = l_nmcr.nmcr121    #本位幣二匯率
   #本位幣二金額
   IF g_glaa015='Y' THEN
      IF g_glaa017 = '1' THEN #原幣
         LET l_nmbc.nmbc123 = l_nmbc.nmbc103 * l_nmcr.nmcr121
      ELSE #本幣
         LET l_nmbc.nmbc123 = l_nmbc.nmbc113 * l_nmcr.nmcr121
      END IF
   END IF
   LET l_nmbc.nmbc131 = l_nmcr.nmcr131    #本位幣三匯率
   #本位幣三金額
   IF g_glaa019='Y' THEN
      IF g_glaa021 = '1' THEN
         LET l_nmbc.nmbc133 = l_nmbc.nmbc103 * l_nmcr.nmcr131 
      ELSE
