  <point name="function.anmt520_conf_chk" order="12" ver="3" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 審核檢查，檢查現金變動碼設置是否正確
# Memo...........:
# Usage..........: CALL anmt520_conf_chk()
################################################################################
PRIVATE FUNCTION anmt520_conf_chk()
   DEFINE l_sql        STRING
   DEFINE l_success    LIKE type_t.num5
   DEFINE l_i,l_cnt    LIKE type_t.num5
   DEFINE l_amt        LIKE glbc_t.glbc009
   DEFINE l_amt1       LIKE glbc_t.glbc009
   DEFINE l_amt2       LIKE glbc_t.glbc009
   DEFINE l_amt3       LIKE glbc_t.glbc009
   DEFINE l_sum1       LIKE glbc_t.glbc009
   DEFINE l_sum2       LIKE glbc_t.glbc009
   DEFINE l_sum3       LIKE glbc_t.glbc009
   DEFINE l_glbcseq1   LIKE glbc_t.glbcseq1
   DEFINE l_glbc004    LIKE glbc_t.glbc004
   DEFINE l_nmaistus   LIKE nmai_t.nmaistus
   DEFINE l_nmbb042    LIKE nmbb_t.nmbb042
   DEFINE l_nmbb042_o  LIKE nmbb_t.nmbb042
   
   LET l_success=TRUE
   #单身无数据，不可审核
   IF g_nmcr_d.getLength() < 1 THEN 
      LET l_success=FALSE
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = "aec-00020"
      LET g_errparam.extend = g_nmcq_m.nmcqdocno
      LET g_errparam.popup = TRUE
      CALL cl_err()                
      RETURN l_success
   END IF
   #檢查支票當前狀態是否做相應的操作
   CALL anmt520_set_nmbb042() RETURNING l_nmbb042

   FOR l_i=1 TO g_nmcr_d.getLength()
      SELECT nmbb042 INTO l_nmbb042_o FROM nmbb_t
       WHERE nmbbent=g_enterprise AND nmbbdocno=g_nmcr_d[l_i].nmcr003
         AND nmbb030=g_nmcr_d[l_i].nmcr001
      IF l_nmbb042_o<> l_nmbb042 THEN
         LET l_success=FALSE
         CASE
            WHEN g_nmcq_m.nmcq001 MATCHES '[245]'
               #CALL cl_errmsg("nmcqdocno",g_nmcq_m.nmcqdocno,"","anm-00173",1) 
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = "anm-00173"
               LET g_errparam.extend = g_nmcq_m.nmcqdocno
               LET g_errparam.popup = TRUE
               CALL cl_err()                
            WHEN g_nmcq_m.nmcq001 MATCHES '[678]'
               #CALL cl_errmsg("nmcqdocno",g_nmcq_m.nmcqdocno,"","anm-00174",1) 
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = "anm-00174"
               LET g_errparam.extend = g_nmcq_m.nmcqdocno
               LET g_errparam.popup = TRUE
               CALL cl_err()                
            WHEN g_nmcq_m.nmcq001='9'
               #CALL cl_errmsg("nmcqdocno",g_nmcq_m.nmcqdocno,"","anm-00175",1)
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = "anm-00175"
               LET g_errparam.extend = g_nmcq_m.nmcqdocno
               LET g_errparam.popup = TRUE
               CALL cl_err() 
         END CASE
      END IF
   END FOR
   #當異動類別為4：票貼、8：兌現時，檢查現金變動碼設置的是否正確
   IF g_nmcq_m.nmcq001 MATCHES '[48]' THEN
      FOR l_i=1 TO g_nmcr_d.getLength()
         SELECT COUNT(*) INTO l_cnt FROM glbc_t
          WHERE glbcent=g_enterprise AND glbcld=g_glaald AND glbcdocno=g_nmcq_m.nmcqdocno
            AND glbcseq=g_nmcr_d[l_i].nmcrseq
         IF l_cnt=0 THEN
            LET l_success = FALSE
            #CALL cl_errmsg('nmcqdocno',g_nmcq_m.nmcqdocno,'','agl-00221',1)
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = 'agl-00221'
            LET g_errparam.extend = g_nmcq_m.nmcqdocno
            LET g_errparam.popup = TRUE
            CALL cl_err() 
         ELSE
            SELECT SUM(glbc009),SUM(glbc012),SUM(glbc014)
              INTO l_sum1,l_sum2,l_sum3
              FROM glbc_t
             WHERE glbcent=g_enterprise AND glbcld=g_glaald
               AND glbcdocno=g_nmcq_m.nmcqdocno AND glbcseq=g_nmcr_d[l_i].nmcrseq
            IF cl_null(l_sum1) THEN LET l_sum1=0 END IF
            IF cl_null(l_sum2) THEN LET l_sum2=0 END IF
            IF cl_null(l_sum3) THEN LET l_sum3=0 END IF
            
            IF g_nmcq_m.nmcq001='4' THEN #托收
               LET l_amt  = g_nmcr2_d[l_i].nmcr106
               LET l_amt1 = g_nmcr2_d[l_i].nmcr116
            ELSE #兌現
               LET l_amt  = g_nmcr_d[l_i].nmcr103
               LET l_amt1 = g_nmcr_d[l_i].nmcr113
            END IF
            
            IF l_amt1<>l_sum1 THEN
               LET l_success = FALSE
               #CALL cl_errmsg('nmcqdocno',g_nmcq_m.nmcqdocno,'','anm-00146',1)
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 'anm-00146'
               LET g_errparam.extend = g_nmcq_m.nmcqdocno
               LET g_errparam.popup = TRUE
               CALL cl_err() 
            END IF
            IF g_glaa015='Y' THEN
               IF g_glaa017 = '1' THEN #原幣
                  LET l_amt2 = l_amt * g_nmcr_d[l_i].nmcr121
               ELSE #本幣
                  LET l_amt2 = l_amt1 * g_nmcr_d[l_i].nmcr121
               END IF
               IF l_amt2<>l_sum2 THEN
                  LET l_success = FALSE
                  #CALL cl_errmsg('nmcqdocno',g_nmcq_m.nmcqdocno,'','anm-00147',1)
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = 'anm-00147'
                  LET g_errparam.extend = g_nmcq_m.nmcqdocno
                  LET g_errparam.popup = TRUE
                  CALL cl_err() 
               END IF
            END IF
            IF g_glaa019='Y' THEN
               IF g_glaa021='1' THEN
                  LET l_amt3 = l_amt * g_nmcr_d[l_i].nmcr131
               ELSE
                  LET l_amt3 = l_amt1 * g_nmcr_d[l_i].nmcr131
               END IF
               IF l_amt3<>l_sum3 THEN
                  LET l_success = FALSE
                  #CALL cl_errmsg('nmcqdocno',g_nmcq_m.nmcqdocno,'','anm-00148',1)
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = 'anm-00148'
                  LET g_errparam.extend = g_nmcq_m.nmcqdocno
                  LET g_errparam.popup = TRUE
                  CALL cl_err() 
               END IF
            END IF
            #判斷現金變動碼設置是否正確
            LET l_sql = " SELECT glbcseq1,glbc004 FROM glbc_t ",
                        "  WHERE glbcent = ",g_enterprise,
                        "    AND glbcld = '",g_glaald,"'",
                        "    AND glbcdocno = '",g_nmcq_m.nmcqdocno,"'",
                        "    AND glbcseq=",g_nmcr_d[l_i].nmcrseq
            PREPARE anmt520_glbc_pre  FROM l_sql
            DECLARE anmt520_glbc_cs CURSOR FOR anmt520_glbc_pre
            FOREACH anmt520_glbc_cs INTO l_glbcseq1,l_glbc004
               IF SQLCA.SQLCODE THEN
                  #CALL cl_errmsg('','','',sqlca.sqlcode,0)
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = sqlca.sqlcode
                  LET g_errparam.extend = ''
                  LET g_errparam.popup = TRUE
                  CALL cl_err() 
                  LET l_success = FALSE
                  EXIT FOREACH
               END IF
               IF cl_null(l_glbc004) THEN 
                  LET l_success = FALSE
                  #CALL cl_errmsg('nmcqdocno',g_nmcq_m.nmcqdocno,'','anm-00091',1)
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = 'anm-00091'
                  LET g_errparam.extend = g_nmcq_m.nmcqdocno
                  LET g_errparam.popup = TRUE
                  CALL cl_err() 
               ELSE
                  SELECT nmaistus INTO l_nmaistus FROM nmai_t 
                   WHERE nmaient=g_enterprise AND nmai001=g_glaa005
                     AND nmai002=l_glbc004 
                  IF SQLCA.sqlcode THEN
                     LET l_success = FALSE
                     #CALL cl_errmsg('glbc004',l_glbc004,'',SQLCA.sqlcode,1)
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code = SQLCA.sqlcode
                     LET g_errparam.extend = l_glbc004
                     LET g_errparam.popup = TRUE
                     CALL cl_err() 
                  END IF
                  IF l_nmaistus<>'Y' THEN
                     LET l_success = FALSE
                     #CALL cl_errmsg('glbc004',l_glbc004,'','abg-00002',1)
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code = 'abg-00002'
                     LET g_errparam.extend = l_glbc004
                     LET g_errparam.popup = TRUE
                     CALL cl_err() 
                  END IF 
               END IF
            END FOREACH
         END IF
      END FOR
   END IF
   RETURN l_success
END FUNCTION]]>
