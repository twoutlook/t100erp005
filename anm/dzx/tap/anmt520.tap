  <section id="anmt520.main" ver="13" status="" src="s" readonly="">
    <![CDATA[#應用 a26 樣板自動產生(Version:4)
#+ 作業開始(主程式類型)
MAIN
   #add-point:main段define
   {<point name="main.define" edit="s"/>}
   #end add-point   
   #add-point:main段define(客製用)
   {<point name="main.define_customerization" edit="c"/>}
   #end add-point   
 
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("anm","")
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT nmcqsite,'',nmcqcomp,'',nmcq002,'',nmcqdocno,nmcq001,nmcqdocdt,nmcq003, 
       '','',nmcq007,nmcqstus,nmcqownid,'',nmcqowndp,'',nmcqcrtid,'',nmcqcrtdp,'',nmcqcrtdt,nmcqmodid, 
       '',nmcqmoddt,nmcq100,nmcq101,nmcq005,'',nmcq004,nmcq006", 
                      " FROM nmcq_t",
                      " WHERE nmcqent= ? AND nmcqcomp=? AND nmcqdocno=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE anmt520_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE t0.nmcqsite,t0.nmcqcomp,t0.nmcq002,t0.nmcqdocno,t0.nmcq001,t0.nmcqdocdt, 
       t0.nmcq003,t0.nmcq007,t0.nmcqstus,t0.nmcqownid,t0.nmcqowndp,t0.nmcqcrtid,t0.nmcqcrtdp,t0.nmcqcrtdt, 
       t0.nmcqmodid,t0.nmcqmoddt,t0.nmcq100,t0.nmcq101,t0.nmcq005,t0.nmcq004,t0.nmcq006,t1.ooefl003 , 
       t2.ooefl003 ,t3.ooag011 ,t4.nmaal003 ,t5.ooag011 ,t6.ooefl003 ,t7.ooag011 ,t8.ooefl003 ,t9.ooag011 , 
       t10.pmaal004",
               " FROM nmcq_t t0",
                              " LEFT JOIN ooefl_t t1 ON t1.ooeflent='"||g_enterprise||"' AND t1.ooefl001=t0.nmcqsite AND t1.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent='"||g_enterprise||"' AND t2.ooefl001=t0.nmcqcomp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent='"||g_enterprise||"' AND t3.ooag001=t0.nmcq002  ",
               " LEFT JOIN nmaal_t t4 ON t4.nmaalent='"||g_enterprise||"' AND t4.nmaal001=t0.nmcq003 AND t4.nmaal002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent='"||g_enterprise||"' AND t5.ooag001=t0.nmcqownid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent='"||g_enterprise||"' AND t6.ooefl001=t0.nmcqowndp AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent='"||g_enterprise||"' AND t7.ooag001=t0.nmcqcrtid  ",
               " LEFT JOIN ooefl_t t8 ON t8.ooeflent='"||g_enterprise||"' AND t8.ooefl001=t0.nmcqcrtdp AND t8.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t9 ON t9.ooagent='"||g_enterprise||"' AND t9.ooag001=t0.nmcqmodid  ",
               " LEFT JOIN pmaal_t t10 ON t10.pmaalent='"||g_enterprise||"' AND t10.pmaal001=t0.nmcq005 AND t10.pmaal002='"||g_dlang||"' ",
 
               " WHERE t0.nmcqent = '" ||g_enterprise|| "' AND t0.nmcqcomp = ? AND t0.nmcqdocno = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE anmt520_master_referesh FROM g_sql
 
    
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_anmt520 WITH FORM cl_ap_formpath("anm",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL anmt520_init()   
 
      #進入選單 Menu (="N")
      CALL anmt520_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_anmt520
      
   END IF 
   
   CLOSE anmt520_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
END MAIN
 
 
]]>
