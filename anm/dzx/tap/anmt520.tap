         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         RETURN
      END IF
      
      #add-point:單頭刪除後
      {<point name="delete.head.a_delete"/>}
      #end add-point
  
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM nmcr_t
       WHERE nmcrent = g_enterprise AND nmcrcomp = g_nmcq_m.nmcqcomp
         AND nmcrdocno = g_nmcq_m.nmcqdocno
 
 
      #add-point:單身刪除中
