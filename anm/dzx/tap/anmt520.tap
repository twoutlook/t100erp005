  <section id="anmt520.b_fill" ver="14" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION anmt520_b_fill()
   DEFINE p_wc2      STRING
   DEFINE li_idx     LIKE type_t.num10
   #add-point:b_fill段define
   {<point name="b_fill.define" edit="s"/>}
   #end add-point     
   #add-point:b_fill段define(客製用)
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point     
 
   CALL g_nmcr_d.clear()    #g_nmcr_d 單頭及單身 
   CALL g_nmcr2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF anmt520_fill_chk(1) THEN
      #切換上下筆時不重組SQL
      IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
         LET g_sql = "SELECT  UNIQUE nmcrseq,nmcrorga,nmcr001,nmcr002,nmcr003,nmcr100,nmcr101,nmcr103, 
             nmcr113,nmcr118,nmcr121,nmcr122,nmcr131,nmcr132,nmcr008,nmcr107,nmcr117,nmcr004,nmcr104, 
             nmcr114,nmcrseq,nmcr005,nmcr006,nmcr007,nmcr105,nmcr115,nmcr106,nmcr116  FROM nmcr_t",  
               
                     " INNER JOIN nmcq_t ON nmcqcomp = nmcrcomp ",
                     " AND nmcqdocno = nmcrdocno ",
 
                     #"",
                     
                     "",
                     
                     " WHERE nmcrent=? AND nmcrcomp=? AND nmcrdocno=?"
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before
         {<point name="b_fill.body.fill_sql"/>}
         #end add-point
         IF NOT cl_null(g_wc2_table1) THEN
            LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY nmcr_t.nmcrseq"
         
         #add-point:單身填充控制
         {<point name="b_fill.sql"/>}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE anmt520_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR anmt520_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_nmcq_m.nmcqcomp,g_nmcq_m.nmcqdocno
                                               
      FOREACH b_fill_cs INTO g_nmcr_d[l_ac].nmcrseq,g_nmcr_d[l_ac].nmcrorga,g_nmcr_d[l_ac].nmcr001,g_nmcr_d[l_ac].nmcr002, 
          g_nmcr_d[l_ac].nmcr003,g_nmcr_d[l_ac].nmcr100,g_nmcr_d[l_ac].nmcr101,g_nmcr_d[l_ac].nmcr103, 
          g_nmcr_d[l_ac].nmcr113,g_nmcr_d[l_ac].nmcr118,g_nmcr_d[l_ac].nmcr121,g_nmcr_d[l_ac].nmcr122, 
          g_nmcr_d[l_ac].nmcr131,g_nmcr_d[l_ac].nmcr132,g_nmcr_d[l_ac].nmcr008,g_nmcr_d[l_ac].nmcr107, 
          g_nmcr_d[l_ac].nmcr117,g_nmcr_d[l_ac].nmcr004,g_nmcr_d[l_ac].nmcr104,g_nmcr_d[l_ac].nmcr114, 
          g_nmcr2_d[l_ac].nmcrseq,g_nmcr2_d[l_ac].nmcr005,g_nmcr2_d[l_ac].nmcr006,g_nmcr2_d[l_ac].nmcr007, 
          g_nmcr2_d[l_ac].nmcr105,g_nmcr2_d[l_ac].nmcr115,g_nmcr2_d[l_ac].nmcr106,g_nmcr2_d[l_ac].nmcr116 

         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF
            EXIT FOREACH
         END IF
         
         LET l_ac = l_ac + 1
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
 
   
   #add-point:browser_fill段其他table處理
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_nmcr_d.deleteElement(g_nmcr_d.getLength())
   CALL g_nmcr2_d.deleteElement(g_nmcr2_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE anmt520_pb
 
   
   LET li_idx = l_ac
   
   #遮罩相關處理
   FOR l_ac = 1 TO g_nmcr_d.getLength()
      LET g_nmcr_d_mask_o[l_ac].* =  g_nmcr_d[l_ac].*
      CALL anmt520_nmcr_t_mask()
      LET g_nmcr_d_mask_n[l_ac].* =  g_nmcr_d[l_ac].*
   END FOR
   
   LET g_nmcr2_d_mask_o.* =  g_nmcr2_d.*
   FOR l_ac = 1 TO g_nmcr2_d.getLength()
      LET g_nmcr2_d_mask_o[l_ac].* =  g_nmcr2_d[l_ac].*
      CALL anmt520_nmcr_t_mask()
      LET g_nmcr2_d_mask_n[l_ac].* =  g_nmcr2_d[l_ac].*
   END FOR
 
   
   LET l_ac = li_idx
   
   CALL cl_ap_performance_next_end()
 
END FUNCTION
]]>
  </section>
