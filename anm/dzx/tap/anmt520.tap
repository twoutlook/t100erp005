  <point name="function.anmt520_glbc_ins" order="19" ver="4" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 插入現金變動碼
# Memo...........:
# Usage..........: CALL anmt520_glbc_ins()
# Modify.........:
################################################################################
PRIVATE FUNCTION anmt520_glbc_ins()
   DEFINE l_glbc         RECORD LIKE glbc_t.*
   DEFINE l_year         LIKE type_t.num5
   DEFINE l_month        LIKE type_t.num5
   DEFINE l_nmbb010      LIKE nmbb_t.nmbb010
   DEFINE l_nmbb026      LIKE nmbb_t.nmbb026
   
   LET l_year = YEAR(g_nmcq_m.nmcqdocdt)
   LET l_month = MONTH(g_nmcq_m.nmcqdocdt)

   LET l_glbc.glbcent   = g_enterprise
   LET l_glbc.glbcld    = g_glaald
   LET l_glbc.glbccomp  = g_nmcq_m.nmcqcomp
   LET l_glbc.glbcdocno = g_nmcq_m.nmcqdocno
   LET l_glbc.glbcseq   = g_nmcr_d[l_ac].nmcrseq
   LET l_glbc.glbc001   = l_year
   LET l_glbc.glbc002   = l_month

   SELECT MAX(glbcseq1) + 1 INTO l_glbc.glbcseq1
     FROM glbc_t
    WHERE glbcent = g_enterprise
      AND glbcld =  l_glbc.glbcld
      AND glbcdocno = l_glbc.glbcdocno
      AND glbcseq = l_glbc.glbcseq
      AND glbc001 = g_glbc_m.glbc001
      AND glac002 = g_glbc_m.glbc002
   IF cl_null(l_glbc.glbcseq1) THEN
      LET l_glbc.glbcseq1 = 1
   END IF

   LET l_glbc.glbc003   = '1'
   #支票資料
   SELECT nmbb010,nmbb026 INTO l_nmbb010,l_nmbb026 
     FROM nmbb_t,nmba_t 
    WHERE nmbaent=nmbbent AND nmbacomp=nmbbcomp AND nmbadocno=nmbbdocno 
      #AND nmbbent=g_enterprise AND nmba003='anmt510' AND nmbacomp=g_nmcq_m.nmcqcomp   #2015/01/30 mark by qiull
      AND nmbbent=g_enterprise AND nmbb029='30' AND nmbacomp=g_nmcq_m.nmcqcomp         #2015/01/30 add by qiull
      AND nmbb030=g_nmcr_d[l_ac].nmcr001
      AND (nmbb069 = 'N' OR nmbb069 IS NULL)                             #2015/01/16 add by qiull  
   LET l_glbc.glbc004   = l_nmbb010
   LET l_glbc.glbc005   = l_nmbb026
   LET l_glbc.glbc006   = g_nmcr_d[l_ac].nmcr100
   LET l_glbc.glbc007   = g_nmcr_d[l_ac].nmcr101
   #兌現
   LET l_glbc.glbc008   = g_nmcr_d[l_ac].nmcr103
   LET l_glbc.glbc009   = g_nmcr_d[l_ac].nmcr113
   
   LET l_glbc.glbc010   ='2'
   #--以下視主帳套有無啟用本位幣
   IF g_glaa015 = 'Y' THEN 
      LET l_glbc.glbc011 = g_nmcr_d[l_ac].nmcr121
      IF g_glaa017 = '1' THEN #原幣
         LET l_glbc.glbc012 = l_glbc.glbc008 * g_nmcr_d[l_ac].nmcr121
      ELSE #本幣
         LET l_glbc.glbc012 = l_glbc.glbc009 * g_nmcr_d[l_ac].nmcr121
      END IF
   END IF
   IF g_glaa019 = 'Y' THEN 
      LET l_glbc.glbc013 = g_nmcr_d[l_ac].nmcr131
      IF g_glaa021='1' THEN
         LET l_glbc.glbc014 = l_glbc.glbc008 * g_nmcr_d[l_ac].nmcr131
      ELSE
         LET l_glbc.glbc014 = l_glbc.glbc009 * g_nmcr_d[l_ac].nmcr131
      END IF
   END IF

   INSERT INTO glbc_t VALUES(l_glbc.*)

   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend = 'ins glbc_t'
      LET g_errparam.popup = TRUE
      CALL cl_err()

      CALL s_transaction_end('N','0')
   END IF
END FUNCTION]]>
  </point>
  <point name="function.anmt520_change_to_sql" order="20" ver="3" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#
PRIVATE FUNCTION anmt520_change_to_sql(p_wc)
   DEFINE p_wc       STRING
   DEFINE r_wc       STRING
   DEFINE tok        base.StringTokenizer
   DEFINE l_str      STRING

   LET tok = base.StringTokenizer.create(p_wc,",")
   WHILE tok.hasMoreTokens()
      IF cl_null(l_str) THEN
         LET l_str = tok.nextToken()
      ELSE
         LET l_str = l_str,"','",tok.nextToken()
      END IF
   END WHILE
   LET r_wc = "'",l_str,"'"

   RETURN r_wc
END FUNCTION]]>
