  <section id="anmt520.modify" ver="19" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION anmt520_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   DEFINE l_wc2_table1          STRING
 
 
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point    
   #add-point:modify段define(客製用)
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point    
   
   #保存單頭舊值
   LET g_nmcq_m_t.* = g_nmcq_m.*
   LET g_nmcq_m_o.* = g_nmcq_m.*
   
   IF g_nmcq_m.nmcqcomp IS NULL
   OR g_nmcq_m.nmcqdocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_nmcqcomp_t = g_nmcq_m.nmcqcomp
   LET g_nmcqdocno_t = g_nmcq_m.nmcqdocno
 
   CALL s_transaction_begin()
   
   OPEN anmt520_cl USING g_enterprise,g_nmcq_m.nmcqcomp,g_nmcq_m.nmcqdocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN anmt520_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE anmt520_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE anmt520_master_referesh USING g_nmcq_m.nmcqcomp,g_nmcq_m.nmcqdocno INTO g_nmcq_m.nmcqsite, 
       g_nmcq_m.nmcqcomp,g_nmcq_m.nmcq002,g_nmcq_m.nmcqdocno,g_nmcq_m.nmcq001,g_nmcq_m.nmcqdocdt,g_nmcq_m.nmcq003, 
       g_nmcq_m.nmcq007,g_nmcq_m.nmcqstus,g_nmcq_m.nmcqownid,g_nmcq_m.nmcqowndp,g_nmcq_m.nmcqcrtid,g_nmcq_m.nmcqcrtdp, 
       g_nmcq_m.nmcqcrtdt,g_nmcq_m.nmcqmodid,g_nmcq_m.nmcqmoddt,g_nmcq_m.nmcq100,g_nmcq_m.nmcq101,g_nmcq_m.nmcq005, 
       g_nmcq_m.nmcq004,g_nmcq_m.nmcq006,g_nmcq_m.nmcqsite_desc,g_nmcq_m.nmcqcomp_desc,g_nmcq_m.nmcq002_desc, 
       g_nmcq_m.nmcq003_desc,g_nmcq_m.nmcqownid_desc,g_nmcq_m.nmcqowndp_desc,g_nmcq_m.nmcqcrtid_desc, 
       g_nmcq_m.nmcqcrtdp_desc,g_nmcq_m.nmcqmodid_desc,g_nmcq_m.nmcq005_desc
   
   #檢查是否允許此動作
   IF NOT anmt520_action_chk() THEN
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_nmcq_m_mask_o.* =  g_nmcq_m.*
   CALL anmt520_nmcq_t_mask()
   LET g_nmcq_m_mask_n.* =  g_nmcq_m.*
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
 
   
   CALL anmt520_show()
   #add-point:modify段show之後
   {<point name="modify.after_show"/>}
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
 
 
    
   WHILE TRUE
      LET g_nmcqcomp_t = g_nmcq_m.nmcqcomp
      LET g_nmcqdocno_t = g_nmcq_m.nmcqdocno
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_nmcq_m.nmcqmodid = g_user 
LET g_nmcq_m.nmcqmoddt = cl_get_current()
LET g_nmcq_m.nmcqmodid_desc = cl_get_username(g_nmcq_m.nmcqmodid)
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      CALL anmt520_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE nmcq_t SET (nmcqmodid,nmcqmoddt) = (g_nmcq_m.nmcqmodid,g_nmcq_m.nmcqmoddt)
          WHERE nmcqent = g_enterprise AND nmcqcomp = g_nmcqcomp_t
            AND nmcqdocno = g_nmcqdocno_t
 
      END IF
    
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_nmcq_m.* = g_nmcq_m_t.*
         CALL anmt520_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_nmcq_m.nmcqcomp != g_nmcqcomp_t 
      OR g_nmcq_m.nmcqdocno != g_nmcqdocno_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE nmcr_t SET nmcrcomp = g_nmcq_m.nmcqcomp
                                       ,nmcrdocno = g_nmcq_m.nmcqdocno
 
          WHERE nmcrent = g_enterprise AND nmcrcomp = g_nmcqcomp_t
            AND nmcrdocno = g_nmcqdocno_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "nmcr_t" 
               LET g_errparam.code   = "std-00009" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "nmcr_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL anmt520_set_act_visible()   
   CALL anmt520_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " nmcqent = '" ||g_enterprise|| "' AND",
                      " nmcqcomp = '", g_nmcq_m.nmcqcomp, "' "
                      ," AND nmcqdocno = '", g_nmcq_m.nmcqdocno, "' "
 
   #填到對應位置
   CALL anmt520_browser_fill("")
 
   CLOSE anmt520_cl
   
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL anmt520_msgcentre_notify('modify')
 
END FUNCTION   
