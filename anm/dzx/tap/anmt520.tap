  <section id="anmt520.update_b" ver="10" status="" src="s" readonly="">
    <![CDATA[#+ 修改單身後其他table連動
PRIVATE FUNCTION anmt520_update_b(ps_table,ps_keys,ps_keys_bak,ps_page)
   DEFINE ps_table         STRING
   DEFINE ps_page          STRING
   DEFINE ps_keys          DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ps_keys_bak      DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ls_group         STRING
   DEFINE li_idx           LIKE type_t.num10 
   DEFINE lb_chk           BOOLEAN
   DEFINE l_new_key        DYNAMIC ARRAY OF STRING
   DEFINE l_old_key        DYNAMIC ARRAY OF STRING
   DEFINE l_field_key      DYNAMIC ARRAY OF STRING
   #add-point:update_b段define
   {<point name="update_b.define" edit="s"/>}
   #end add-point   
   #add-point:update_b段define(客製用)
   {<point name="update_b.define_customerization" edit="c"/>}
   #end add-point   
   
   LET g_update = TRUE   
   
   #判斷key是否有改變
   LET lb_chk = TRUE
   FOR li_idx = 1 TO ps_keys.getLength()
      IF ps_keys[li_idx] <> ps_keys_bak[li_idx] THEN
         LET lb_chk = FALSE
         EXIT FOR
      END IF
   END FOR
   
   #不需要做處理
   IF lb_chk THEN
      RETURN
   END IF
   
   #判斷是否是同一群組的table
   LET ls_group = "'1','2',"
   IF ls_group.getIndexOf(ps_page,1) > 0 AND ps_table <> "nmcr_t" THEN
      #add-point:update_b段修改前
      {<point name="update_b.before_update" mark="Y"/>}
      #end add-point 
      
      #將遮罩欄位還原
      CALL anmt520_nmcr_t_mask_restore('restore_mask_o')
               
      UPDATE nmcr_t 
         SET (nmcrcomp,nmcrdocno,
              nmcrseq
              ,nmcrorga,nmcr001,nmcr002,nmcr003,nmcr100,nmcr101,nmcr103,nmcr113,nmcr118,nmcr121,nmcr122,nmcr131,nmcr132,nmcr008,nmcr107,nmcr117,nmcr004,nmcr104,nmcr114,nmcr005,nmcr006,nmcr007,nmcr105,nmcr115,nmcr106,nmcr116) 
              = 
             (ps_keys[1],ps_keys[2],ps_keys[3]
              ,g_nmcr_d[g_detail_idx].nmcrorga,g_nmcr_d[g_detail_idx].nmcr001,g_nmcr_d[g_detail_idx].nmcr002, 
                  g_nmcr_d[g_detail_idx].nmcr003,g_nmcr_d[g_detail_idx].nmcr100,g_nmcr_d[g_detail_idx].nmcr101, 
                  g_nmcr_d[g_detail_idx].nmcr103,g_nmcr_d[g_detail_idx].nmcr113,g_nmcr_d[g_detail_idx].nmcr118, 
                  g_nmcr_d[g_detail_idx].nmcr121,g_nmcr_d[g_detail_idx].nmcr122,g_nmcr_d[g_detail_idx].nmcr131, 
                  g_nmcr_d[g_detail_idx].nmcr132,g_nmcr_d[g_detail_idx].nmcr008,g_nmcr_d[g_detail_idx].nmcr107, 
                  g_nmcr_d[g_detail_idx].nmcr117,g_nmcr_d[g_detail_idx].nmcr004,g_nmcr_d[g_detail_idx].nmcr104, 
                  g_nmcr_d[g_detail_idx].nmcr114,g_nmcr2_d[g_detail_idx].nmcr005,g_nmcr2_d[g_detail_idx].nmcr006, 
                  g_nmcr2_d[g_detail_idx].nmcr007,g_nmcr2_d[g_detail_idx].nmcr105,g_nmcr2_d[g_detail_idx].nmcr115, 
                  g_nmcr2_d[g_detail_idx].nmcr106,g_nmcr2_d[g_detail_idx].nmcr116) 
         WHERE nmcrent = g_enterprise AND nmcrcomp = ps_keys_bak[1] AND nmcrdocno = ps_keys_bak[2] AND nmcrseq = ps_keys_bak[3]
      #add-point:update_b段修改中
      {<point name="update_b.m_update"/>}
      #end add-point   
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "nmcr_t" 
            LET g_errparam.code   = "std-00009" 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            CALL s_transaction_end('N','0')
         WHEN SQLCA.sqlcode #其他錯誤
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "nmcr_t" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            CALL s_transaction_end('N','0')
         OTHERWISE
            
      END CASE
      
      #將遮罩欄位進行遮蔽
      CALL anmt520_nmcr_t_mask_restore('restore_mask_n')
               
      #add-point:update_b段修改後
      {<point name="update_b.after_update"/>}
      #end add-point  
   END IF
   
 
   
 
   
   #add-point:update_b段other
   {<point name="update_b.other"/>}
   #end add-point  
   
END FUNCTION
]]>
  </section>
