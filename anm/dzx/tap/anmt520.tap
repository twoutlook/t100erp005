   #end add-point     
   #add-point:delete段define(客製用)
   {<point name="delete.define_customerization" edit="c"/>}
   #end add-point     
   
   IF g_nmcq_m.nmcqcomp IS NULL
   OR g_nmcq_m.nmcqdocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
   
   
   
   CALL s_transaction_begin()
 
   OPEN anmt520_cl USING g_enterprise,g_nmcq_m.nmcqcomp,g_nmcq_m.nmcqdocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN anmt520_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE anmt520_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE anmt520_master_referesh USING g_nmcq_m.nmcqcomp,g_nmcq_m.nmcqdocno INTO g_nmcq_m.nmcqsite, 
       g_nmcq_m.nmcqcomp,g_nmcq_m.nmcq002,g_nmcq_m.nmcqdocno,g_nmcq_m.nmcq001,g_nmcq_m.nmcqdocdt,g_nmcq_m.nmcq003, 
       g_nmcq_m.nmcq007,g_nmcq_m.nmcqstus,g_nmcq_m.nmcqownid,g_nmcq_m.nmcqowndp,g_nmcq_m.nmcqcrtid,g_nmcq_m.nmcqcrtdp, 
       g_nmcq_m.nmcqcrtdt,g_nmcq_m.nmcqmodid,g_nmcq_m.nmcqmoddt,g_nmcq_m.nmcq100,g_nmcq_m.nmcq101,g_nmcq_m.nmcq005, 
       g_nmcq_m.nmcq004,g_nmcq_m.nmcq006,g_nmcq_m.nmcqsite_desc,g_nmcq_m.nmcqcomp_desc,g_nmcq_m.nmcq002_desc, 
       g_nmcq_m.nmcq003_desc,g_nmcq_m.nmcqownid_desc,g_nmcq_m.nmcqowndp_desc,g_nmcq_m.nmcqcrtid_desc, 
       g_nmcq_m.nmcqcrtdp_desc,g_nmcq_m.nmcqmodid_desc,g_nmcq_m.nmcq005_desc
   
   #檢查是否允許此動作
   IF NOT anmt520_action_chk() THEN
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_nmcq_m_mask_o.* =  g_nmcq_m.*
   CALL anmt520_nmcq_t_mask()
   LET g_nmcq_m_mask_n.* =  g_nmcq_m.*
   
   CALL anmt520_show()
   
   #add-point:delete段before ask
   {<point name="delete.before_ask"/>}
   #end add-point 
 
   IF cl_ask_del_master() THEN              #確認一下
   
      #add-point:單頭刪除前
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point   
      
      #應用 a47 樣板自動產生(Version:2)
      #刪除相關文件
      CALL anmt520_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
 
  
  
      #資料備份
      LET g_nmcqcomp_t = g_nmcq_m.nmcqcomp
      LET g_nmcqdocno_t = g_nmcq_m.nmcqdocno
 
 
      DELETE FROM nmcq_t
       WHERE nmcqent = g_enterprise AND nmcqcomp = g_nmcq_m.nmcqcomp
         AND nmcqdocno = g_nmcq_m.nmcqdocno
 
       
      #add-point:單頭刪除中
      {<point name="delete.head.m_delete"/>}
      #end add-point
       
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
