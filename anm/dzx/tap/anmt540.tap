  <point name="function.anmt540_nmbadocno_chk" order="3" ver="5" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[# nmbadocno 繳款單號欄位檢查
PRIVATE FUNCTION anmt540_nmbadocno_chk()
   DEFINE l_n             LIKE type_t.num5 
   
   #判斷資料是否存在   
   LET l_n = 0   
   SELECT count(*) INTO l_n
     FROM ooba_t,oobx_t
    WHERE oobaent = g_enterprise
      AND ooba001 = g_glaa024
      AND oobx004 = 'anmt540' 
      AND ooba002 = g_nmba_m.nmbadocno 
      AND oobaent = oobxent
      AND ooba002 = oobx001      
      
