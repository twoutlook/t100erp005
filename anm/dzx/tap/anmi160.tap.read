  <point name="function.anmi160_chk_nmai001" order="1" ver="3" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[# 現金異動碼表編碼檢查與參照表是否更新
PRIVATE FUNCTION anmi160_chk_nmai001(p_cmd)
DEFINE p_cmd  LIKE type_t.chr1
DEFINE l_cnt  LIKE type_t.num5
 
   IF NOT cl_null(g_nmai_m.nmai001) THEN 
      IF p_cmd = 'a' OR ( p_cmd = 'u' AND ( cl_null(g_nmai001_t) OR (g_nmai_m.nmai001 != g_nmai001_t))) THEN 
         IF NOT ap_chk_notDup(g_nmai_m.nmai001,"SELECT COUNT(*) FROM nmai_t WHERE "||"nmaient = '" ||g_enterprise|| "' AND "||"nmai001 = '"||g_nmai_m.nmai001 ||"'",'std-00004',0) THEN 
            LET g_nmai_m.nmai001 = g_nmai_m_t.nmai001
            RETURN FALSE
         END IF
      END IF
   END IF
   RETURN TRUE
END FUNCTION]]>
  </point>
  <point name="function.anmi160_chk_nmai001_2" order="2" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[# 檢查現金異動碼錶編碼是否存在參照表
PRIVATE FUNCTION anmi160_chk_nmai001_2(p_nmai001)
DEFINE p_nmai001    LIKE nmai_t.nmai001
DEFINE r_errno      LIKE type_t.chr20
DEFINE l_ooal002    LIKE ooal_t.ooal002
DEFINE l_ooalstus   LIKE ooal_t.ooalstus

   LET r_errno = ''

   SELECT ooal002,ooalstus INTO l_ooal002,l_ooalstus
     FROM ooal_t
    WHERE ooalent = g_enterprise
      AND ooal001 = 8
      AND ooal002 = p_nmai001

    CASE
       WHEN sqlca.sqlcode = 100   LET r_errno = 'anm-00022'
       WHEN l_ooalstus ='N'       LET r_errno = 'anm-00023'
     END CASE
     RETURN r_errno
END FUNCTION]]>
  </point>
  <point name="function.anmi160_chk_nmai003" order="3" ver="3" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[# 現金異動碼存在檢查
PRIVATE FUNCTION anmi160_chk_nmai003(p_cmd)
DEFINE p_cmd  LIKE type_t.chr1
 
   IF NOT cl_null(g_nmai_d[l_ac].nmai003) THEN 
      IF p_cmd = 'a' OR ( p_cmd = 'u' AND ( cl_null(g_nmai_d_t.nmai003) OR (g_nmai_d[l_ac].nmai003 != g_nmai_d_t.nmai003))) THEN 
         IF NOT ap_chk_isExist(g_nmai_d[l_ac].nmai003,"SELECT COUNT(*) FROM nmak_t WHERE "||"nmakent = '" ||g_enterprise|| "' AND "||"nmak001 = ? ",'anm-00004',0) THEN 
            LET g_nmai_d[l_ac].nmai003 = g_nmai_d_t.nmai003 
            RETURN FALSE
         END IF
         IF NOT ap_chk_isExist(g_nmai_d[l_ac].nmai003,"SELECT COUNT(*) FROM nmak_t WHERE "||"nmakent = '" ||g_enterprise|| "' AND "||"nmak001 = ? AND nmakstus = 'Y' ",'anm-00024',0) THEN 
            LET g_nmai_d[l_ac].nmai003 = g_nmai_d_t.nmai003 
            RETURN FALSE
         END IF         
      END IF
   END IF
   RETURN TRUE
END FUNCTION]]>
  </point>
  <point name="function.anmi160_chk_nmai002" order="4" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[# 現金異動碼重複性檢查
PRIVATE FUNCTION anmi160_chk_nmai002(p_cmd)
DEFINE p_cmd  LIKE type_t.chr1
   IF NOT cl_null(g_nmai_m.nmai001) AND NOT cl_null(g_nmai_d[l_ac].nmai002) THEN 
      IF p_cmd = 'a' OR ( p_cmd = 'u' AND ( p_cmd = 'u' AND (g_nmai_m.nmai001 != g_nmai001_t OR g_nmai_d[l_ac].nmai002 != g_nmai_d_t.nmai002))) THEN 
         IF NOT ap_chk_notDup(g_nmai_d[l_ac].nmai002,"SELECT COUNT(*) FROM nmai_t WHERE "||"nmaient = '" ||g_enterprise|| "' AND "||"nmai001 = '"||g_nmai_m.nmai001 ||"' AND "|| "nmai002 = '"||g_nmai_d[l_ac].nmai002 ||"'",'std-00004',0) THEN 
            LET g_nmai_d[l_ac].nmai002 = g_nmai_d_t.nmai002
            RETURN FALSE
         END IF
      END IF
   END IF
   RETURN TRUE
END FUNCTION]]>
  </point>
  <point name="function.anmi160_chk_nmai004" order="5" ver="3" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[# 序號格式檢查
PRIVATE FUNCTION anmi160_chk_nmai004(p_cmd)
DEFINE p_cmd  LIKE type_t.chr1
DEFINE l_cnt  LIKE type_t.num5
DEFINE i      LIKE type_t.num5 
  
   IF NOT cl_null(g_nmai_d[l_ac].nmai004) THEN 
      IF p_cmd = 'a' OR ( p_cmd = 'u' AND ( cl_null(g_nmai_d_t.nmai004) OR (g_nmai_d[l_ac].nmai004 != g_nmai_d_t.nmai004))) THEN 
         FOR i = 1 TO Length(g_nmai_d[l_ac].nmai004)
            IF g_nmai_d[l_ac].nmai004[i,i] NOT MATCHES '[0-9]' THEN 
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 'anm-00005'
               LET g_errparam.extend = g_nmai_d[l_ac].nmai004
               LET g_errparam.popup = FALSE
               CALL cl_err()
 
               LET g_nmai_d[l_ac].nmai004 = g_nmai_d_t.nmai004
               RETURN FALSE
            END IF 
         END FOR 
         LET l_cnt = 0
         SELECT COUNT(*) INTO l_cnt FROM nmai_t 
          WHERE nmaient = g_enterprise
            AND nmai001 = g_nmai_m.nmai001
            AND nmai004 = g_nmai_d[l_ac].nmai004
            AND nmaistus = 'Y'
         IF l_cnt > 0  THEN 
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = 'anm-00006'
            LET g_errparam.extend = g_nmai_d[l_ac].nmai004
            LET g_errparam.popup = FALSE
            CALL cl_err()

            LET g_nmai_d[l_ac].nmai004 = g_nmai_d_t.nmai004
            RETURN FALSE 
         END IF         
      END IF
   END IF
   RETURN TRUE
END FUNCTION]]>
