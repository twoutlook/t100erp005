  <section id="anmi160.b_fill" ver="8" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION anmi160_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_nmai_d.clear()
   CALL g_nmai1_info_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE nmai002,nmai003,nmai004,nmaistus,nmai002,nmaimodid,nmaimoddt,nmaiownid, 
       nmaiowndp,nmaicrtid,nmaicrtdp,nmaicrtdt,t1.nmakl003 ,t2.ooag011 ,t3.ooag011 ,t4.ooefl003 ,t5.ooag011 , 
       t6.ooefl003 FROM nmai_t",   
               " LEFT JOIN nmail_t ON nmai001 = nmail001 AND nmai002 = nmail002 AND nmail003 = '",g_lang,"'",
               
                              " LEFT JOIN nmakl_t t1 ON t1.nmaklent='"||g_enterprise||"' AND t1.nmakl001=nmai003 AND t1.nmakl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t2 ON t2.ooagent='"||g_enterprise||"' AND t2.ooag001=nmaimodid  ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent='"||g_enterprise||"' AND t3.ooag001=nmaiownid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=nmaiowndp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent='"||g_enterprise||"' AND t5.ooag001=nmaicrtid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent='"||g_enterprise||"' AND t6.ooefl001=nmaicrtdp AND t6.ooefl002='"||g_dlang||"' ",
 
               " WHERE nmaient= ? AND nmai001=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("nmai_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF anmi160_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY nmai_t.nmai002"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE anmi160_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR anmi160_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_nmai_m.nmai001
                                               
      FOREACH b_fill_cs INTO g_nmai_d[l_ac].nmai002,g_nmai_d[l_ac].nmai003,g_nmai_d[l_ac].nmai004,g_nmai_d[l_ac].nmaistus, 
          g_nmai1_info_d[l_ac].nmai002,g_nmai1_info_d[l_ac].nmaimodid,g_nmai1_info_d[l_ac].nmaimoddt, 
          g_nmai1_info_d[l_ac].nmaiownid,g_nmai1_info_d[l_ac].nmaiowndp,g_nmai1_info_d[l_ac].nmaicrtid, 
          g_nmai1_info_d[l_ac].nmaicrtdp,g_nmai1_info_d[l_ac].nmaicrtdt,g_nmai_d[l_ac].nmai003_desc, 
          g_nmai1_info_d[l_ac].nmaimodid_desc,g_nmai1_info_d[l_ac].nmaiownid_desc,g_nmai1_info_d[l_ac].nmaiowndp_desc, 
          g_nmai1_info_d[l_ac].nmaicrtid_desc,g_nmai1_info_d[l_ac].nmaicrtdp_desc
                             
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_nmai_d.deleteElement(g_nmai_d.getLength())
      CALL g_nmai1_info_d.deleteElement(g_nmai1_info_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE anmi160_pb   
   
END FUNCTION
]]>
  </section>
