  <point name="function.anmi171_insert_glab" cite_std="N" status="" ver="1" src="s" new="Y" order="1" mark_hard="N">
<![CDATA[#+ 單身插入資料
PRIVATE FUNCTION anmi171_insert_glab()
DEFINE   l_glab001      LIKE glab_t.glab001
DEFINE   l_glab002      LIKE glab_t.glab002
DEFINE   l_glab003      LIKE glab_t.glab003

   CALL s_transaction_begin()
   LET g_sql = " DELETE FROM glab_t WHERE glabent = '",g_enterprise,"'",
              "   AND glabld = '",g_glab_m.glabld,"' AND glab002 = '43' AND glab001 ='43' ",
              "   AND glab003 NOT IN (SELECT unique nmaj001 FROM nmaj_t WHERE nmajent='",g_enterprise,"'",
              "   AND nmajstus = 'Y')" 
   PREPARE glab_del FROM g_sql
   EXECUTE glab_del
   LET g_sql = "SELECT unique nmaj001 FROM nmaj_t",
               " WHERE nmajent = '",g_enterprise,"' AND nmajstus = 'Y'",
               "   AND nmaj001 NOT IN ( SELECT glab003 FROM glab_t ",
               "                         WHERE glabent='",g_enterprise,"'",
               "                           AND glabld = '",g_glab_m.glabld,"'",
               "                           AND glab002 = '43' AND glab001 ='43' )"             
   PREPARE nmaj001_pre FROM g_sql
   DECLARE nmaj001_cur CURSOR FOR nmaj001_pre
   FOREACH nmaj001_cur INTO l_glab003
      LET l_glab001 = '43'
      LET l_glab002 = '43'
      
      INSERT INTO glab_t(glabent,glabld,glab001,glab002,glab003,glab004,glab011)
                  VALUES (g_enterprise,g_glab_m.glabld,l_glab001,l_glab002,l_glab003,g_glab_m.glab004,'1')
      IF SQLCA.SQLcode  THEN
         LET g_success = 'N'
      ELSE
         LET g_success = 'Y'
      END IF
   END FOREACH 
   IF g_success = 'N' THEN
      CALL cl_err("glab_t",SQLCA.sqlcode,1)  
      CALL s_transaction_end('N','0') 
   ELSE
      CALL s_transaction_end('Y','0')
       ERROR "INSERT O.K"
   END IF
END FUNCTION]]>
</point>
  <point name="function.anmi171_glab005_chk" cite_std="N" status="" ver="1" src="s" new="Y" order="2" mark_hard="N">
<![CDATA[#+ glab005栏位检查
PRIVATE FUNCTION anmi171_glab005_chk()
   DEFINE l_n            LIKE type_t.num5
   DEFINE l_glac003      LIKE glac_t.glac003
   DEFINE l_glacstus     LIKE glac_t.glacstus
   
   LET g_errno=''
   
   SELECT glacstus,glac003 INTO l_glacstus,l_glac003
     FROM glac_t
    WHERE glacent = g_enterprise
      AND glac001 = g_glab_m.glab004
      AND glac002 = g_glab_d[l_ac].glab005
   CASE
      WHEN SQLCA.SQLCODE=100 LET g_errno='agl-00011'
      WHEN l_glacstus<>'Y'   LET g_errno='agl-00012'
      WHEN l_glac003 = '1'   LET g_errno='anm-00029'
   END CASE
   
   IF NOT cl_null(g_errno) THEN
      CALL cl_err(g_glab_d[l_ac].glab005,g_errno,1)
      RETURN FALSE
   ELSE
      RETURN TRUE
   END IF
END FUNCTION]]>
</point>
  <point name="function.anmi171_glabld_chk" cite_std="N" status="u" ver="1" src="s" new="Y" order="3" mark_hard="N">
<![CDATA[#+ glabld栏位检查
PRIVATE FUNCTION anmi171_glabld_chk(p_glabld)
DEFINE l_n         LIKE type_t.num5
DEFINE p_glabld    LIKE glab_t.glabld
DEFINE r_success   LIKE type_t.num5
DEFINE l_glaastus  LIKE glaa_t.glaastus

   IF NOT ap_chk_notDup(g_glab_m.glabld,"SELECT COUNT(*) FROM glab_t WHERE "||"glabent = '" ||g_enterprise|| "' AND glab001 = '43' AND "||"glabld = '"||p_glabld ||"'",'std-00004',0) THEN 
      LET g_glab_m.glabld_desc = ''
      LET g_glab_m.glaacomp = ''
      LET g_glab_m.glaacomp_desc = ''
      LET g_glab_m.glab004 = ''
      LET g_glab_m.glab004_desc = ''
      LET g_glab_m.glaa014 = 'N'
      LET g_glab_m.glaa008 = 'N'
      DISPLAY g_glab_m.glabld_desc,g_glab_m.glaacomp,g_glab_m.glaacomp_desc,g_glab_m.glaa014,
              g_glab_m.glaa008,g_glab_m.glab004,g_glab_m.glab004_desc
           TO glabld_desc,glaacomp,glaacomp_desc,glaa014,glaa008,glab004,glab004_desc 
      RETURN FALSE
   END IF
   LET g_errno=''
   LET r_success=TRUE
   
   #是否有權限
   CALL s_ld_chk_authorization(g_user,p_glabld) RETURNING r_success
   IF r_success = FALSE THEN
      LET g_errno='axr-00022'
   END IF 
   
   IF cl_null(g_errno) THEN
      SELECT glaastus,glaacomp,glaa004,glaa014,glaa008
        INTO l_glaastus,g_glab_m.glaacomp,g_glab_m.glab004,g_glab_m.glaa014,g_glab_m.glaa008
      FROM glaa_t 
      WHERE glaaent=g_enterprise AND glaald = p_glabld 
      
      CASE
         WHEN SQLCA.sqlcode=100    LET g_errno='aoo-00017'
         WHEN l_glaastus<>'Y'      LET g_errno='agl-00051'
         #WHEN g_glab_m.glaa008='N' LET g_errno='agl-00105'
      END CASE
   END IF
   #報錯判斷
   IF NOT cl_null(g_errno) THEN
      CALL cl_err(p_glabld,g_errno,1)
      LET g_glab_m.glabld_desc = ''
      LET g_glab_m.glaacomp = ''
      LET g_glab_m.glaacomp_desc = ''
      LET g_glab_m.glab004 = ''
      LET g_glab_m.glab004_desc = ''
      LET g_glab_m.glaa014 = 'N'
      LET g_glab_m.glaa008 = 'N'
      LET r_success=FALSE
   ELSE
      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_glab_m.glabld
      CALL ap_ref_array2(g_ref_fields,"SELECT glaal002 FROM glaal_t WHERE glaalent='"||g_enterprise||"' AND glaalld=? AND glaal001='"||g_dlang||"'","") RETURNING g_rtn_fields
      LET g_glab_m.glabld_desc = '', g_rtn_fields[1] , ''
      
      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_glab_m.glaacomp
      CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
      LET g_glab_m.glaacomp_desc = g_rtn_fields[1]
   
      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_glab_m.glab004
      CALL ap_ref_array2(g_ref_fields,"SELECT ooall004 FROM ooall_t WHERE ooallent='"||g_enterprise||"' AND ooall001='0' AND ooall002=? AND ooall003='"||g_dlang||"'","") RETURNING g_rtn_fields
      LET g_glab_m.glab004_desc = g_rtn_fields[1]
   END IF
   DISPLAY g_glab_m.glabld_desc,g_glab_m.glaacomp,g_glab_m.glaacomp_desc,g_glab_m.glaa014,
           g_glab_m.glaa008,g_glab_m.glab004,g_glab_m.glab004_desc
        TO glabld_desc,glaacomp,glaacomp_desc,glaa014,glaa008,glab004,glab004_desc
   RETURN r_success
END FUNCTION]]>
</point>
  <point name="function.anmi171_reproduce1" cite_std="N" status="u" ver="1" src="s" new="Y" order="4" mark_hard="N">
<![CDATA[# 資料複製
PRIVATE FUNCTION anmi171_reproduce1()
   {<Local define>}
   DEFINE l_newno     LIKE glab_t.glabld 
   DEFINE l_oldno     LIKE glab_t.glabld 

   DEFINE l_master    RECORD LIKE glab_t.*
   DEFINE l_detail    RECORD LIKE glab_t.*
   DEFINE l_cnt       LIKE type_t.num5
   {</Local define>}
   #add-point:reproduce段define
   DEFINE l_n       LIKE type_t.num5
   #end add-point
 
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF     
 
   IF g_glab_m.glabld IS NULL

      THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   LET g_glabld_t = g_glab_m.glabld

    
   CALL anmi171_set_entry('a')
   CALL anmi171_set_no_entry('a')
   
   CALL cl_set_head_visible("","YES")
   
      LET g_glab_m.glabld_desc = ''
   DISPLAY BY NAME g_glab_m.glabld_desc

 
   INPUT l_newno #FROM 

    FROM glabld 

         ATTRIBUTE(FIELD ORDER FORM)
         
         AFTER FIELD glabld 
            #add-point:AFTER FIELD glabld
            IF NOT cl_null(l_newno) THEN 
               IF NOT anmi171_glabld_chk(l_newno) THEN 
                  LET l_newno = ''
                  NEXT FIELD glabld
               END IF
               SELECT count(*) INTO l_n
                 FROM glaa_t
                WHERE glaald = l_newno
                  AND glaastus = 'Y'   
                  AND glaa004 = g_glab_m_t.glab004
               IF l_n = 0 THEN 
                  CALL cl_err(l_newno,'anm-00030',1)
                  LET l_newno = ''
                  LET g_glab_m.glabld_desc = ''
                  LET g_glab_m.glaacomp = ''
                  LET g_glab_m.glaacomp_desc = ''
                  LET g_glab_m.glab004 = ''
                  LET g_glab_m.glab004_desc = ''
                  LET g_glab_m.glaa014 = 'N'
                  LET g_glab_m.glaa008 = 'N'
                  DISPLAY g_glab_m.glabld_desc TO glabld_desc
                  DISPLAY g_glab_m.glaacomp,g_glab_m.glaacomp_desc TO glaacomp,glaacomp_desc
                  DISPLAY g_glab_m.glaa014 TO glaa014
                  DISPLAY g_glab_m.glaa008 TO glaa008
                  DISPLAY g_glab_m.glab004,g_glab_m.glab004_desc TO glab004,glab004_desc
                  NEXT FIELD glabld
               END IF
            END IF 
            #END add-point
         

         
      #add-point:複製段落開窗/欄位控管/自定義action
      ON ACTION controlp INFIELD glabld
            #add-point:ON ACTION controlp INFIELD glabld
#此段落由子樣板a07產生            
            #開窗i段
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = "i"
            LET g_qryparam.reqry = FALSE

            LET g_qryparam.default1 = l_newno             #給予default值
            LET g_qryparam.default2 = "" #g_glab_m.glaald #帳別編號
            LET g_qryparam.where = " glaa004 = '",g_glab_m_t.glab004,"' AND (glaa008 = 'Y' OR glaa014 = 'Y')"
            #給予arg
            LET g_qryparam.arg1 = g_user
            LET g_qryparam.arg2 = g_dept
            CALL q_authorised_ld()                                #呼叫開窗

            LET l_newno = g_qryparam.return1              #將開窗取得的值回傳到變數
            LET g_qryparam.where = NULL
            
