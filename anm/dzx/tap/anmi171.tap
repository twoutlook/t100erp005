  <section id="anmi171.b_fill" ver="12" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION anmi171_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_glab_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE glab001,glab002,glab003,glab005,glab011,t1.nmajl003 ,t2.glacl004 FROM glab_t", 
          
               "",
               
                              " LEFT JOIN nmajl_t t1 ON t1.nmajlent='"||g_enterprise||"' AND t1.nmajl001=glab003 AND t1.nmajl002='"||g_dlang||"' ",
               " LEFT JOIN glacl_t t2 ON t2.glaclent='"||g_enterprise||"' AND t2.glacl001=glab004 AND t2.glacl002=glab005 AND t2.glacl003='"||g_dlang||"' ",
 
               " WHERE glabent= ? AND glabld=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("glab_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF anmi171_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY glab_t.glab001,glab_t.glab002,glab_t.glab003"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE anmi171_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR anmi171_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_glab_m.glabld
                                               
      FOREACH b_fill_cs INTO g_glab_d[l_ac].glab001,g_glab_d[l_ac].glab002,g_glab_d[l_ac].glab003,g_glab_d[l_ac].glab005, 
          g_glab_d[l_ac].glab011,g_glab_d[l_ac].glab003_desc,g_glab_d[l_ac].glab005_desc
                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_glab_d.deleteElement(g_glab_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE anmi171_pb   
   
END FUNCTION
]]>
</section>
