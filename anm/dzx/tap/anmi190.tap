<![CDATA[            #此段落由子樣板a05產生
            DISPLAY '' to nmam001_desc
            IF  NOT cl_null(g_nmam_m.nmam001) THEN 
               #輸入值須存在[T:參照表檔].[C:參照表號]、[C:參照表類型]=9且為有效資料,
               #若資料不存在則詢問「輸入的參照表號不存在參照表檔,請問是否要新增此參照表號?」，
               #若選擇是則呼叫應用元件s_aooi070_ins()做參照表號的新增；
               #否則停留在本欄位
               CALL anmi190_nmam001_chk(g_nmam_m.nmam001)  RETURNING g_errno
               IF NOT cl_null(g_errno) THEN
                  IF g_errno = 'aoo-00076' THEN
                     IF NOT cl_ask_confirm('aoo-00076') THEN
                        LET g_nmam_m.nmam001 = ''
                        LET g_nmam_m.nmam001_desc =''
                        DISPLAY BY NAME g_nmam_m.nmam001_desc 
                        NEXT FIELD nmam001
                     ELSE
                        CALL s_transaction_begin()
                        LET l_success = 0
                        CALL s_aooi070_ins(7,g_nmam_m.nmam001) RETURNING l_success
                        IF l_success = 1 THEN
                           CALL s_transaction_end('Y','1')
                        ELSE
                           CALL s_transaction_end('N','1')
                        END IF 
                     END IF
                  ELSE
                     CALL cl_err(g_nmam_m.nmam001,g_errno,1)
                     LET g_nmam_m.nmam001 = g_nmam001_t
                     CALL anmi190_ooall004(g_nmam_m.nmam001)
                     DISPLAY BY NAME g_nmam_m.nmam001_desc 
                     NEXT FIELD  nmam001
                  END IF
               END IF
               IF p_cmd = 'a' OR ( p_cmd = 'u' AND ( p_cmd = 'u' AND (g_nmam_m.nmam001 != g_nmam001_t ))) THEN 
                  IF NOT ap_chk_notDup(g_nmam_m.nmam001,"SELECT COUNT(*) FROM nmam_t WHERE "||"nmament = '" ||g_enterprise|| "' AND "||"nmam001 = '"||g_nmam_m.nmam001 ||"'",'std-00004',0) THEN 
                     LET g_nmam_m.nmam001 = g_nmam001_t 
                     CALL anmi190_ooall004(g_nmam_m.nmam001)
                     DISPLAY BY NAME g_nmam_m.nmam001_desc 
                     NEXT FIELD CURRENT
                  END IF
               END IF
            ELSE
               LET g_nmam_m.nmam001_desc = ''
               DISPLAY BY NAME g_nmam_m.nmam001_desc 
            END IF 
            #带参照编号说明
            CALL anmi190_ooall004(g_nmam_m.nmam001)
            DISPLAY BY NAME g_nmam_m.nmam001_desc       ]]>
</point>
