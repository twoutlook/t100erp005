  <section id="anmi172.b_fill" ver="6" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION anmi172_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_nmad_d.clear()
   CALL g_nmad2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE nmad002,nmad003,nmadstus,nmad002,nmadownid,nmadowndp,nmadcrtid,nmadcrtdp, 
       nmadcrtdt,nmadmodid,nmadmoddt,t1.nmajl003 ,t2.nmail004 ,t3.oofa011 ,t4.ooefl003 ,t5.oofa011 , 
       t6.ooefl003 ,t7.oofa011 FROM nmad_t",   
               "",
               
                              " LEFT JOIN nmajl_t t1 ON t1.nmajlent='"||g_enterprise||"' AND t1.nmajl001=nmad002 AND t1.nmajl002='"||g_dlang||"' ",
               " LEFT JOIN nmail_t t2 ON t2.nmailent='"||g_enterprise||"' AND t2.nmail001=nmad001 AND t2.nmail002=nmad003 AND t2.nmail003='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t3 ON t3.oofaent='"||g_enterprise||"' AND t3.oofa002='2' AND t3.oofa003=nmadownid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=nmadowndp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t5 ON t5.oofaent='"||g_enterprise||"' AND t5.oofa002='2' AND t5.oofa003=nmadcrtid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent='"||g_enterprise||"' AND t6.ooefl001=nmadcrtdp AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t7 ON t7.oofaent='"||g_enterprise||"' AND t7.oofa002='2' AND t7.oofa003=nmadmodid  ",
 
               " WHERE nmadent= ? AND nmad001=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("nmad_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF anmi172_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY nmad_t.nmad002"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE anmi172_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR anmi172_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_nmad_m.nmad001
                                               
      FOREACH b_fill_cs INTO g_nmad_d[l_ac].nmad002,g_nmad_d[l_ac].nmad003,g_nmad_d[l_ac].nmadstus,g_nmad2_d[l_ac].nmad002, 
          g_nmad2_d[l_ac].nmadownid,g_nmad2_d[l_ac].nmadowndp,g_nmad2_d[l_ac].nmadcrtid,g_nmad2_d[l_ac].nmadcrtdp, 
          g_nmad2_d[l_ac].nmadcrtdt,g_nmad2_d[l_ac].nmadmodid,g_nmad2_d[l_ac].nmadmoddt,g_nmad_d[l_ac].nmad002_desc, 
          g_nmad_d[l_ac].nmad003_desc,g_nmad2_d[l_ac].nmadownid_desc,g_nmad2_d[l_ac].nmadowndp_desc, 
          g_nmad2_d[l_ac].nmadcrtid_desc,g_nmad2_d[l_ac].nmadcrtdp_desc,g_nmad2_d[l_ac].nmadmodid_desc 

                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_nmad2_d[l_ac].nmadownid_desc = cl_get_username(g_nmad2_d[l_ac].nmadownid)
      #LET g_nmad2_d[l_ac].nmadowndp_desc = cl_get_deptname(g_nmad2_d[l_ac].nmadowndp)
      #LET g_nmad2_d[l_ac].nmadcrtid_desc = cl_get_username(g_nmad2_d[l_ac].nmadcrtid)
      #LET g_nmad2_d[l_ac].nmadcrtdp_desc = cl_get_deptname(g_nmad2_d[l_ac].nmadcrtdp)
      #LET g_nmad2_d[l_ac].nmadmodid_desc = cl_get_username(g_nmad2_d[l_ac].nmadmodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_nmad_d.deleteElement(g_nmad_d.getLength())
      CALL g_nmad2_d.deleteElement(g_nmad2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE anmi172_pb   
   
END FUNCTION
