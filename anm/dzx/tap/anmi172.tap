  <point name="function.anmi172_insert_nmad" cite_std="N" status="" ver="1" src="s" new="Y" order="1" mark_hard="N">
<![CDATA[#單身插入資料
PRIVATE FUNCTION anmi172_insert_nmad()
DEFINE   l_nmaj001      LIKE nmaj_t.nmaj001
DEFINE   l_time         LIKE nmad_t.nmadcrtdt

   CALL s_transaction_begin()
   LET g_sql = "SELECT unique nmaj001 FROM nmaj_t",
               " WHERE nmajstus = 'Y' "             
   PREPARE nmaj001_pre FROM g_sql
   DECLARE nmaj001_cur CURSOR FOR nmaj001_pre
   FOREACH nmaj001_cur INTO l_nmaj001
      LET l_time = cl_get_current()
      INSERT INTO nmad_t(nmadent,nmad001,nmad002,nmadstus,nmadownid,nmadowndp,nmadcrtid,nmadcrtdp,nmadcrtdt)
                  VALUES (g_enterprise,g_nmad_m.nmad001,l_nmaj001,'Y',g_user,g_dept,g_user,g_dept,l_time)
      IF SQLCA.SQLcode  THEN
         LET g_success = 'N'
      ELSE
         LET g_success = 'Y'
      END IF
   END FOREACH 
   IF g_success = 'N' THEN
      CALL cl_err("glab_t",SQLCA.sqlcode,1)  
      CALL s_transaction_end('N','0') 
   ELSE
      CALL s_transaction_end('Y','0')
       ERROR "INSERT O.K"
   END IF
END FUNCTION]]>
</point>
  <point name="function.anmi172_nmad001_chk" cite_std="N" status="" ver="1" src="s" new="Y" order="2" mark_hard="N">
<![CDATA[#現金變動參照表有效性檢查
PRIVATE FUNCTION anmi172_nmad001_chk(p_nmad001)
   DEFINE p_nmad001 LIKE nmad_t.nmad001
   DEFINE r_success LIKE type_t.num5

   LET r_success = TRUE           
   IF NOT cl_null(p_nmad001) THEN
      #檢查是否存在
      IF r_success THEN
         IF NOT ap_chk_isExist(p_nmad001,"SELECT COUNT(*) FROM ooal_t WHERE ooalent = '"
            ||g_enterprise||"' AND ooal001 = '8' AND ooal002 = ? ","agl-00001",0 ) THEN
            LET r_success = FALSE
         END IF
      END IF
      #檢查是否有效
      IF r_success THEN
         IF NOT ap_chk_isExist(p_nmad001,"SELECT COUNT(*) FROM ooal_t WHERE ooalent = '"
            ||g_enterprise||"' AND ooal001 = '8' AND ooal002 = ? AND ooalstus = 'Y' ","agl-00002",0 ) THEN
            LET r_success = FALSE
         END IF
      END IF
   END IF
   RETURN r_success
END FUNCTION]]>
</point>
