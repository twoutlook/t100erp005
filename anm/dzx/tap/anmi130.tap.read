  <section id="anmi130.detail_reproduce" ver="8" status="" src="s" readonly="">
    <![CDATA[#+ 單身自動複製
PRIVATE FUNCTION anmi130_detail_reproduce()
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE nmaf_t.*
 
 
   #add-point:delete段define
   {<point name="detail_reproduce.define" edit="s"/>}
   #end add-point    
   #add-point:delete段define
   {<point name="detail_reproduce.define_customerization" edit="c"/>}
   #end add-point 
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE anmi130_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE anmi130_detail AS ",
                "SELECT * FROM nmaf_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO anmi130_detail SELECT * FROM nmaf_t 
                                         WHERE nmafent = g_enterprise AND nmaf001 = g_nmaf001_t
                                         AND nmaf002 = g_nmaf002_t
 
   
   #將key修正為調整後   
   UPDATE anmi130_detail 
      #更新key欄位
      SET nmaf001 = g_nmaf_m.nmaf001
          , nmaf002 = g_nmaf_m.nmaf002
 
      #更新共用欄位
      #應用 a13 樣板自動產生(Version:3)
       , nmafownid = g_user 
       , nmafowndp = g_dept
       , nmafcrtid = g_user
       , nmafcrtdp = g_dept 
       , nmafcrtdt = ld_date
       , nmafmodid = g_user
       , nmafmoddt = ld_date
      #, nmafstus = "Y" 
 
 
                                       
   #將資料塞回原table   
   INSERT INTO nmaf_t SELECT * FROM anmi130_detail
   
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "reproduce" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE anmi130_detail
   
   #add-point:單身複製後1
   {<point name="detail_reproduce.body.table1.a_insert"/>}
   #end add-point
 
 
   
 
   
   #多語言複製段落
   
   
   CALL s_transaction_end('Y','0')
   
   #已新增完, 調整資料內容(修改時使用)
   LET g_nmaf001_t = g_nmaf_m.nmaf001
   LET g_nmaf002_t = g_nmaf_m.nmaf002
 
   
   DROP TABLE anmi130_detail
   
END FUNCTION
]]>
