  <section id="anmi130.b_fill" ver="16" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION anmi130_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define" edit="s"/>}
   #end add-point     
   #add-point:b_fill段define
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
   
   #先清空單身變數內容
   CALL g_nmaf_d.clear()
   CALL g_nmaf2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE nmaf004,nmaf005,nmaf006,nmaf007,nmaf009,nmaf008,nmaf010,nmaf011,nmaf003, 
       nmaf012,nmafstus,nmaf004,nmafownid,nmafowndp,nmafcrtid,nmafcrtdp,nmafcrtdt,nmafmodid,nmafmoddt, 
       t1.ooag011 ,t2.ooefl003 ,t3.ooag011 ,t4.ooefl003 ,t5.ooag011 FROM nmaf_t",   
               "",
               
                              " LEFT JOIN ooag_t t1 ON t1.ooagent='"||g_enterprise||"' AND t1.ooag001=nmafownid  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent='"||g_enterprise||"' AND t2.ooefl001=nmafowndp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent='"||g_enterprise||"' AND t3.ooag001=nmafcrtid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=nmafcrtdp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent='"||g_enterprise||"' AND t5.ooag001=nmafmodid  ",
 
               " WHERE nmafent= ? AND nmaf001=? AND nmaf002=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("nmaf_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF anmi130_fill_chk(1) THEN
      IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
         LET g_sql = g_sql, " ORDER BY nmaf_t.nmaf004"
         #add-point:b_fill段fill_before
         {<point name="b_fill.fill_before"/>}
         #end add-point
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE anmi130_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR anmi130_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_nmaf_m.nmaf001,g_nmaf_m.nmaf002
                                               
      FOREACH b_fill_cs INTO g_nmaf_d[l_ac].nmaf004,g_nmaf_d[l_ac].nmaf005,g_nmaf_d[l_ac].nmaf006,g_nmaf_d[l_ac].nmaf007, 
          g_nmaf_d[l_ac].nmaf009,g_nmaf_d[l_ac].nmaf008,g_nmaf_d[l_ac].nmaf010,g_nmaf_d[l_ac].nmaf011, 
          g_nmaf_d[l_ac].nmaf003,g_nmaf_d[l_ac].nmaf012,g_nmaf_d[l_ac].nmafstus,g_nmaf2_d[l_ac].nmaf004, 
          g_nmaf2_d[l_ac].nmafownid,g_nmaf2_d[l_ac].nmafowndp,g_nmaf2_d[l_ac].nmafcrtid,g_nmaf2_d[l_ac].nmafcrtdp, 
          g_nmaf2_d[l_ac].nmafcrtdt,g_nmaf2_d[l_ac].nmafmodid,g_nmaf2_d[l_ac].nmafmoddt,g_nmaf2_d[l_ac].nmafownid_desc, 
          g_nmaf2_d[l_ac].nmafowndp_desc,g_nmaf2_d[l_ac].nmafcrtid_desc,g_nmaf2_d[l_ac].nmafcrtdp_desc, 
          g_nmaf2_d[l_ac].nmafmodid_desc
                             
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #應用 a12 樣板自動產生(Version:3)
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF 
            EXIT FOREACH
         END IF
      
         LET l_ac = l_ac + 1
         
      END FOREACH
 
            CALL g_nmaf_d.deleteElement(g_nmaf_d.getLength())
      CALL g_nmaf2_d.deleteElement(g_nmaf2_d.getLength())
 
      
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_nmaf_d.getLength()
      LET g_nmaf_d_mask_o[l_ac].* =  g_nmaf_d[l_ac].*
      CALL anmi130_nmaf_t_mask()
      LET g_nmaf_d_mask_n[l_ac].* =  g_nmaf_d[l_ac].*
   END FOR
   
   LET g_nmaf2_d_mask_o.* =  g_nmaf2_d.*
   FOR l_ac = 1 TO g_nmaf2_d.getLength()
      LET g_nmaf2_d_mask_o[l_ac].* =  g_nmaf2_d[l_ac].*
      CALL anmi130_nmaf_t_mask()
      LET g_nmaf2_d_mask_n[l_ac].* =  g_nmaf2_d[l_ac].*
   END FOR
 
 
   FREE anmi130_pb   
   
END FUNCTION
]]>
  </section>
