PRIVATE FUNCTION anmi130_reproduce()
   DEFINE l_newno     LIKE nmaf_t.nmaf001 
   DEFINE l_oldno     LIKE nmaf_t.nmaf001 
   DEFINE l_newno02     LIKE nmaf_t.nmaf002 
   DEFINE l_oldno02     LIKE nmaf_t.nmaf002 
 
