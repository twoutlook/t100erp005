# 刪除銀存收支異動檔 nmbc_t
PRIVATE FUNCTION anmt460_nmbc_del()

   DELETE FROM nmbc_t
    WHERE nmbcent = g_enterprise
      AND nmbcdocno = g_nmck_m.nmckdocno
      AND nmbccomp = g_nmck_m.nmckcomp
      
