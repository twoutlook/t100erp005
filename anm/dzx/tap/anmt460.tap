      WHEN SQLCA.SQLCODE = 100 LET g_errno = 'aoo-00094'
      WHEN l_ooefstus = 'N'    LET g_errno = 'aoo-00095'
      WHEN l_ooef206<>'Y'      LET g_errno = 'anm-00272'
   END CASE
END FUNCTION]]>
