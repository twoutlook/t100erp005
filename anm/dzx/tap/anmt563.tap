    <![CDATA[
################################################################################
# Descriptions...: 來源資料檢查
# Memo...........:
# Usage..........: CALL anmt563_stev_chk(p_str)
#                  RETURNING r_success
# Input parameter: p_str          範圍
# Return code....: r_success      檢查成功否
#                : r_stev016      可結算金額
# Date & Author..: 2015/06/08 By 01727
# Modify.........:
################################################################################
PRIVATE FUNCTION anmt563_stev_chk(p_str,p_seq)
   DEFINE p_str         STRING
   DEFINE p_seq         LIKE nmee_t.nmeeseq
   DEFINE r_success     LIKE type_t.num5
   DEFINE r_stev016     LIKE stev_t.stev016
   DEFINE l_sql         STRING
   DEFINE l_count       LIKE type_t.num5
   DEFINE l_stevdocno   LIKE stev_t.stevdocno
   DEFINE l_stevseq     LIKE stev_t.stevseq
   DEFINE l_nmee012     LIKE nmee_t.nmee012

   LET l_sql = "SELECT stevdocno,stevseq,stev016 - CASE WHEN stev025 IS NULL THEN 0 ELSE stev025 END ",
               "  FROM steu_t,stev_t   ",
               " WHERE stevent = steuent   AND stevdocno = steudocno  ",
               "   AND steustus = 'Y'      AND ",p_str CLIPPED
   PREPARE anmt563_stev_chk_prep FROM l_sql
   DECLARE anmt563_stev_chk_curs CURSOR FOR anmt563_stev_chk_prep

   LET l_sql = "SELECT COUNT(*) FROM (",l_sql,")"
   PREPARE anmt563_stev_cnt_prep FROM l_sql
   DECLARE anmt563_stev_cnt_curs CURSOR FOR anmt563_stev_cnt_prep

   LET r_success = TRUE

   #資料存在性
   LET l_count = 0
   EXECUTE anmt563_stev_cnt_curs INTO l_count
   IF cl_null(l_count) OR l_count = 0 THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "anm-00356" 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      LET r_success = FALSE
      RETURN r_success,r_stev016
   END IF

   #資料有效性
   LET r_stev016 = 0      LET l_nmee012 = 0
   FOREACH anmt563_stev_chk_curs INTO l_stevdocno,l_stevseq,r_stev016
      #取得在途金額
      SELECT SUM(nmee012) INTO l_nmee012 FROM nmee_t t1,nmba_t
       WHERE nmbaent = nmeeent     AND nmbadocno = nmeedocno
         AND nmbacomp = nmeecomp   AND nmbastus = 'N'   #作廢的不考慮、已確認的已經回寫stev025了
         AND nmee003 = l_stevdocno AND nmee004 = l_stevseq
         AND NOT EXISTS (SELECT 1 FROM nmee_t t2 WHERE nmeeent = g_enterprise AND nmeedocno = g_nmba_m.nmbadocno
                            AND nmeecomp = g_nmba_m.nmbacomp AND nmeeseq = p_seq AND t1.nmeeent = t2.nmeeent
                            AND t1.nmeecomp = t2.nmeecomp AND t1.nmeedocno = t2.nmeedocno AND t1.nmeeseq = t2.nmeeseq)
      IF cl_null(l_nmee012) THEN LET l_nmee012 = 0 END IF

      LET r_stev016 = r_stev016 - l_nmee012
      IF r_stev016 <= 0 THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = "anm-00357" 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         LET r_success = FALSE
         RETURN r_success,r_stev016
      END IF

   END FOREACH

   RETURN r_success,r_stev016

END FUNCTION]]>
