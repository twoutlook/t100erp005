################################################################################
PRIVATE FUNCTION anmt563_glaa_get()
   LET g_ooef001=''
   LET g_ooef016=''
   LET g_ooef006=''
   LET g_glaald=''
   LET g_glaa001=''
   LET g_glaa002=''
   LET g_glaa015=''
   LET g_glaa016=''
   LET g_glaa017=''
   LET g_glaa018=''
   LET g_glaa019=''
   LET g_glaa020=''
   LET g_glaa021=''
   LET g_glaa022=''
   LET g_glaa003=''
   LET g_glaa024=''
   
   SELECT ooef001,ooef016,ooef006 INTO g_ooef001,g_ooef016,g_ooef006
     FROM ooef_t
    WHERE ooefent = g_enterprise
      AND ooef001 = g_nmba_m.nmbacomp
      
    SELECT glaald,glaa001,glaa002,glaa003,glaa015,glaa016,glaa017,
           glaa018,glaa019,glaa020,glaa021,glaa022,glaa024                        
     INTO g_glaald,g_glaa001,g_glaa002,g_glaa003,g_glaa015,g_glaa016,g_glaa017,
          g_glaa018,g_glaa019,g_glaa020,g_glaa021,g_glaa022,g_glaa024 
     FROM glaa_t
    WHERE glaaent = g_enterprise 
      AND glaacomp = g_nmba_m.nmbacomp
      AND glaa014 = 'Y'
END FUNCTION]]>
