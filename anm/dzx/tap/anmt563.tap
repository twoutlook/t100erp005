               IF SQLCA.sqlcode THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "g_nmba_m" 
                  LET g_errparam.code   = SQLCA.sqlcode 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  CALL s_transaction_end('N','0')
                  NEXT FIELD CURRENT
               END IF
               
               #add-point:單頭新增中
               {<point name="input.head.m_insert"/>}
               #end add-point
               
               
               
               
               #add-point:單頭新增後
               {<point name="input.head.a_insert"/>}
               #end add-point
               CALL s_transaction_end('Y','0') 
               
               IF l_cmd_t = 'r' AND p_cmd = 'a' THEN
                  CALL anmt563_detail_reproduce()
                  #因應特定程式需求, 重新刷新單身資料
                  CALL anmt563_b_fill()
               END IF
               
               #add-point:單頭新增後
               {<point name="input.head.a_insert2"/>}
               #end add-point
               
               LET g_master_insert = TRUE
               
               LET p_cmd = 'u'
            ELSE
               CALL s_transaction_begin()
            
               #add-point:單頭修改前
               {<point name="input.head.b_update" mark="Y"/>}
               #end add-point
               
               #將遮罩欄位還原
               CALL anmt563_nmba_t_mask_restore('restore_mask_o')
               
               UPDATE nmba_t SET (nmbasite,nmba008,nmba001,nmbacomp,nmbadocno,nmbadocdt,nmba003,nmbastus, 
                   nmba004,nmba005,nmba007,nmba006,nmba002,nmbaownid,nmbaowndp,nmbacrtid,nmbacrtdp,nmbacrtdt, 
                   nmbamodid,nmbamoddt,nmbacnfid,nmbacnfdt) = (g_nmba_m.nmbasite,g_nmba_m.nmba008,g_nmba_m.nmba001, 
                   g_nmba_m.nmbacomp,g_nmba_m.nmbadocno,g_nmba_m.nmbadocdt,g_nmba_m.nmba003,g_nmba_m.nmbastus, 
                   g_nmba_m.nmba004,g_nmba_m.nmba005,g_nmba_m.nmba007,g_nmba_m.nmba006,g_nmba_m.nmba002, 
                   g_nmba_m.nmbaownid,g_nmba_m.nmbaowndp,g_nmba_m.nmbacrtid,g_nmba_m.nmbacrtdp,g_nmba_m.nmbacrtdt, 
                   g_nmba_m.nmbamodid,g_nmba_m.nmbamoddt,g_nmba_m.nmbacnfid,g_nmba_m.nmbacnfdt)
                WHERE nmbaent = g_enterprise AND nmbacomp = g_nmbacomp_t
                  AND nmbadocno = g_nmbadocno_t
 
               IF SQLCA.sqlcode THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "nmba_t" 
                  LET g_errparam.code   = SQLCA.sqlcode 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  CALL s_transaction_end('N','0')
                  NEXT FIELD CURRENT
               END IF
               
               #add-point:單頭修改中
               {<point name="input.head.m_update"/>}
               #end add-point
               
               
               
               
               #將遮罩欄位進行遮蔽
               CALL anmt563_nmba_t_mask_restore('restore_mask_n')
               
               #修改歷程記錄(單頭修改)
               LET g_log1 = util.JSON.stringify(g_nmba_m_t)
               LET g_log2 = util.JSON.stringify(g_nmba_m)
               IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                  CALL s_transaction_end('N','0')
               ELSE
                  CALL s_transaction_end('Y','0')
               END IF
               
               #add-point:單頭修改後
               {<point name="input.head.a_update"/>}
               #end add-point
            END IF
            
            LET g_nmbacomp_t = g_nmba_m.nmbacomp
            LET g_nmbadocno_t = g_nmba_m.nmbadocno
 
            
      END INPUT
   
]]>
