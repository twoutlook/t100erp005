   LET g_nmba_m.nmbadocno = ""
 
 
   CALL cl_set_head_visible("","YES")
 
   #公用欄位給予預設值
   #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_nmba_m.nmbaownid = g_user
      LET g_nmba_m.nmbaowndp = g_dept
      LET g_nmba_m.nmbacrtid = g_user
      LET g_nmba_m.nmbacrtdp = g_dept 
