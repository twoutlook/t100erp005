  <point name="statechange.a_update" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   CASE
      WHEN lc_state = 'Y'
         CALL anmt563_confirm_chk() RETURNING l_success
         IF NOT l_success THEN LET l_tot_success = FALSE END IF
         CALL anmt563_stev_upd(" 1=1","+") RETURNING l_success
         IF NOT l_success THEN LET l_tot_success = FALSE END IF
         CALL anmt563_nmbc_insert() RETURNING l_success
         IF NOT l_success THEN LET l_tot_success = FALSE END IF
      WHEN lc_state = 'N'
         LET l_count = 0
         SELECT COUNT(*) FROM nmbt_t WHERE nmbtent = g_enterprise
            AND nmbt001 = g_nmba_m.nmbadocno
         IF cl_null(l_count) THEN LET l_count = 0 END IF
         IF l_count > 0 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "" 
            LET g_errparam.code   = 'axr-00361'
            LET g_errparam.popup  = FALSE 
            CALL cl_err()
         END IF
         CALL anmt563_stev_upd(" 1=1","-") RETURNING l_success
         IF NOT l_success THEN LET l_tot_success = FALSE END IF
         DELETE FROM nmbc_t WHERE nmbcent = g_enterprise
            AND nmbccomp  = g_nmba_m.nmbacomp
            AND nmbcdocno = g_nmba_m.nmbadocno
   END CASE]]>
