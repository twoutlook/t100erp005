         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY nmee_t.nmeeseq"
         
         #add-point:單身填充控制
         {<point name="b_fill.sql2"/>}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE anmt563_pb2 FROM g_sql
         DECLARE b_fill_cs2 CURSOR FOR anmt563_pb2
      END IF
    
      LET l_ac = 1
      
      OPEN b_fill_cs2 USING g_enterprise,g_nmba_m.nmbacomp,g_nmba_m.nmbadocno
                                               
      FOREACH b_fill_cs2 INTO g_nmbb2_d[l_ac].nmeeseq,g_nmbb2_d[l_ac].nmeesite,g_nmbb2_d[l_ac].nmee001, 
          g_nmbb2_d[l_ac].nmee003,g_nmbb2_d[l_ac].nmee004,g_nmbb2_d[l_ac].nmee005,g_nmbb2_d[l_ac].nmee006, 
          g_nmbb2_d[l_ac].nmee007,g_nmbb2_d[l_ac].nmee008,g_nmbb2_d[l_ac].nmee009,g_nmbb2_d[l_ac].nmee018, 
          g_nmbb2_d[l_ac].nmee010,g_nmbb2_d[l_ac].nmee011,g_nmbb2_d[l_ac].nmee012,g_nmbb2_d[l_ac].nmee013, 
          g_nmbb2_d[l_ac].nmee002,g_nmbb2_d[l_ac].nmee014,g_nmbb2_d[l_ac].nmee019,g_nmbb2_d[l_ac].nmeesite_desc, 
          g_nmbb2_d[l_ac].nmee001_desc,g_nmbb2_d[l_ac].nmee005_desc,g_nmbb2_d[l_ac].nmee006_desc,g_nmbb2_d[l_ac].nmee014_desc 

         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
