               " LEFT JOIN mhael_t t7 ON t7.mhaelent='"||g_enterprise||"' AND t7.mhael001=nmee005 AND t7.mhael022='"||g_dlang||"' ",
               " LEFT JOIN stael_t t8 ON t8.staelent='"||g_enterprise||"' AND t8.stael001=nmee006 AND t8.stael002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t9 ON t9.ooeflent='"||g_enterprise||"' AND t9.ooefl001=nmee014 AND t9.ooefl002='"||g_dlang||"' ",
 
