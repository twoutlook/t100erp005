   {<point name="input.define_customerization" edit="c"/>}
   #end add-point  
   
   #先做狀態判定
   IF p_cmd = 'r' THEN
      LET l_cmd_t = 'r'
      LET p_cmd   = 'a'
   ELSE
      LET l_cmd_t = p_cmd
   END IF   
   
   #將資料輸出到畫面上
   DISPLAY BY NAME g_nmbs_m.nmbssite,g_nmbs_m.nmbssite_desc,g_nmbs_m.nmbsld,g_nmbs_m.nmbsld_desc,g_nmbs_m.nmbscomp_desc, 
