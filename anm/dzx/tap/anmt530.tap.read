  <point name="function.anmt530_show_ref" order="1" ver="2" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[# 單頭參考欄位帶值
PRIVATE FUNCTION anmt530_show_ref()
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_nmbs_m.nmbsld
   CALL ap_ref_array2(g_ref_fields,"SELECT glaal002 FROM glaal_t WHERE glaalent='"||g_enterprise||"' AND glaalld=? AND glaal001='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_nmbs_m.nmbsld_desc = '', g_rtn_fields[1] , ''
   DISPLAY BY NAME g_nmbs_m.nmbsld_desc
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_nmbs_m.nmbsld
   CALL ap_ref_array2(g_ref_fields,"SELECT glaacomp,glaa001 FROM glaa_t WHERE glaaent='"||g_enterprise||"' AND glaald=? ","") RETURNING g_rtn_fields
   LET g_nmbs_m.nmbscomp = '', g_rtn_fields[1] , ''
   LET g_nmbs_m.glaa001 = '', g_rtn_fields[2] , ''
   DISPLAY BY NAME g_nmbs_m.nmbscomp
   DISPLAY BY NAME g_nmbs_m.glaa001
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_nmbs_m.nmbscomp
   CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_nmbs_m.nmbscomp_desc = '', g_rtn_fields[1] , ''
   DISPLAY BY NAME g_nmbs_m.nmbscomp_desc
   
   SELECT glaa001,glaa005,glaa004,glaa008,glaa014,glaa015,glaa016,glaa017,glaa018,glaa019,glaa020,glaa021,glaa022 
     INTO g_glaa001,g_glaa005,g_glaa004,g_glaa008,g_glaa014,g_glaa015,g_glaa016,
          g_glaa017,g_glaa018,g_glaa019,g_glaa020,g_glaa021,g_glaa022 
     FROM glaa_t
    WHERE glaaent = g_enterprise
      AND glaald = g_nmbs_m.nmbsld
      
   IF NOT cl_null(g_nmbs_m.nmbscomp) THEN 
      CALL cl_get_para(g_enterprise,g_nmbs_m.nmbscomp,'S-FIN-4004') RETURNING g_para_data  #資金模組匯率來源
   END IF
END FUNCTION]]>
  </point>
  <point name="function.anmt530_nmbt002_get" order="2" ver="3" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[# 根據nmbt002抓取收票/異動票據資料
PRIVATE FUNCTION anmt530_nmbt002_get()
   
   IF g_nmbt_d[l_ac].nmbt001 = '1'  THEN 
      SELECT nmbadocdt,nmbb042
        INTO g_nmbt_d[l_ac].nmbadocdt,g_nmbt_d[l_ac].nmbb042    
        FROM nmba_t,nmbb_t
       WHERE nmbaent = nmbbent
         AND nmbacomp = nmbbcomp
         AND nmbadocno = nmbbdocno
         AND nmbaent = g_enterprise
         AND nmbacomp = g_nmbs_m.nmbscomp
         AND nmbadocno = g_nmbt_d[l_ac].nmbt002
   END IF
   
   IF g_nmbt_d[l_ac].nmbt001 = '2'  THEN 
      SELECT nmcqdocdt,nmcq001
        INTO g_nmbt_d[l_ac].nmbadocdt,g_nmbt_d[l_ac].nmbb042    
        FROM nmcq_t
       WHERE nmcqent = g_enterprise
         AND nmcqcomp = g_nmbs_m.nmbscomp
         AND nmcqdocno = g_nmbt_d[l_ac].nmbt002
   END IF
      
   LET g_nmbt3_d[l_ac].nmbadocdt = g_nmbt_d[l_ac].nmbadocdt
   LET g_nmbt3_d[l_ac].nmbb042 = g_nmbt_d[l_ac].nmbb042
   
   DISPLAY g_nmbt_d[l_ac].nmbadocdt TO s_detail1[l_ac].nmbadocdt_2
   DISPLAY g_nmbt_d[l_ac].nmbb042 TO s_detail1[l_ac].nmbb042
   DISPLAY g_nmbt3_d[l_ac].nmbadocdt TO s_detail3[l_ac].nmbadocdt_2
   DISPLAY g_nmbt3_d[l_ac].nmbb042 TO s_detail3[l_ac].nmbb042
   
END FUNCTION]]>
  </point>
  <point name="function.anmt530_nmcr_get" order="3" ver="2" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[# 整批抓取異動票據資料插入單身
PRIVATE FUNCTION anmt530_nmcr_get(p_nmbt002)
   DEFINE p_nmbt002      LIKE nmbt_t.nmbt002
   DEFINE l_nmcrdocno    LIKE nmcr_t.nmcrdocno
   DEFINE l_nmcrseq      LIKE nmcr_t.nmcrseq
   DEFINE l_nmbtseq      LIKE nmbt_t.nmbtseq
   DEFINE l_nmbt012      LIKE nmbt_t.nmbt012
   DEFINE l_nmbt014      LIKE nmbt_t.nmbt014
   DEFINE l_nmbt017      LIKE nmbt_t.nmbt017
   DEFINE l_nmbt018      LIKE nmbt_t.nmbt018
   DEFINE l_nmbt019      LIKE nmbt_t.nmbt019
   DEFINE l_nmbt021      LIKE nmbt_t.nmbt021
   DEFINE l_nmbt022      LIKE nmbt_t.nmbt022
   DEFINE l_nmbt025      LIKE nmbt_t.nmbt025
   DEFINE l_nmbt121      LIKE nmbt_t.nmbt121
   DEFINE l_nmbt131      LIKE nmbt_t.nmbt131
   DEFINE l_nmbt123      LIKE nmbt_t.nmbt123
   DEFINE l_nmbt133      LIKE nmbt_t.nmbt133
   DEFINE l_nmcr001      LIKE nmcr_t.nmcr001
   DEFINE l_nmbt100      LIKE nmbt_t.nmbt100
   DEFINE l_nmbt101      LIKE nmbt_t.nmbt101
   DEFINE l_nmbt103      LIKE nmbt_t.nmbt103
   DEFINE l_nmbt113      LIKE nmbt_t.nmbt113
   DEFINE l_nmcq001      LIKE nmcq_t.nmcq001
   DEFINE l_ooam003      LIKE ooam_t.ooam003
   
   CALL s_transaction_begin()
   LET g_success = 'Y'
   
   LET g_sql = " SELECT nmcrdocno,nmcrseq,nmcr001,nmcq100,nmcq101,nmcr103,nmcr113 ",
               "   FROM nmcr_t,nmcq_t ",
               "  WHERE nmcrent = '",g_enterprise,"'",
               "    AND nmcrcomp = '",g_nmbs_m.nmbscomp,"'",
               "    AND nmcrdocno = '",p_nmbt002,"'",
               "    AND nmcrent = nmcqent ",
               "    AND nmcrcomp = nmcqcomp ",
               "    AND nmcrdocno = nmcqdocno "
   PREPARE nmcr_pre FROM g_sql
   DECLARE nmcr_cur CURSOR FOR nmcr_pre
   FOREACH nmcr_cur INTO l_nmcrdocno,l_nmcrseq,l_nmcr001,l_nmbt100,l_nmbt101,l_nmbt103,l_nmbt113
      SELECT MAX(nmbtseq) + 1 INTO l_nmbtseq
        FROM nmbt_t
       WHERE nmbtent = g_enterprise
         AND nmbtld = g_nmbs_m.nmbsld
         AND nmbtdocno = g_nmbs_m.nmbsdocno
       
      IF cl_null(l_nmbtseq) THEN 
         LET l_nmbtseq = 1
      END IF
      #抓取其他欄位的預設值
      SELECT nmbb031,nmbb003,nmcrcomp,nmba001,nmba004,nmba008
        INTO l_nmbt012,l_nmbt014,
             l_nmbt017,l_nmbt018,
             l_nmbt021,l_nmbt025
        FROM nmcr_t,nmba_t,nmbb_t
       WHERE nmbaent = nmbbent
         AND nmbacomp = nmbbcomp
         AND nmbadocno = nmbbdocno
         AND nmcr001 = nmbb030
         AND nmcrent = g_enterprise
         AND nmcrcomp = g_nmbs_m.nmbscomp
         AND nmcrdocno = l_nmcrdocno 
         
      IF g_glaa014 = 'Y' THEN 
         IF g_glaa015 = 'Y' THEN                         
             #主帳套本位幣二匯率
             IF g_glaa017 = '1' THEN 
                LET l_ooam003 = l_nmbt100
             ELSE
                LET l_ooam003 = g_glaa001
             END IF
                                        #日期;          來源幣別   目的幣別; 匯類類型
             CALL anmt530_get_exrate(g_nmbs_m.nmbsdocdt,l_ooam003,g_glaa016,g_glaa018)                      
             RETURNING l_nmbt121  
             LET l_nmbt123 = l_nmbt113 * l_nmbt121   
         END IF
          
         IF g_glaa019 = 'Y' THEN                         
            #主帳套本位幣三匯率
            IF g_glaa021 = '1' THEN 
               LET l_ooam003 = l_nmbt100
            ELSE
               LET l_ooam003 = g_glaa001
            END IF
                                       #日期;           來源幣別   目的幣別; 匯類類型
            CALL anmt530_get_exrate(g_nmbs_m.nmbsdocdt,l_ooam003,g_glaa020,g_glaa022)
            RETURNING l_nmbt131   
            LET l_nmbt133 = l_nmbt113 * l_nmbt131  
         END IF
      END IF
      
      #平行賬套,金額匯率重新計算
      IF g_glaa014 <> 'Y' AND g_glaa008 = 'Y' THEN 
         LET l_nmbt101 = 0
         LET l_nmbt113 = 0
      
         #平行賬套匯率
                                    #日期;             來源幣別   目的幣別; 匯類類型
         CALL anmt530_get_exrate(g_nmbs_m.nmbsdocdt,l_nmbt100,g_glaa001,g_para_data)                      
         RETURNING l_nmbt101   
         LET l_nmbt113 = l_nmbt103 * l_nmbt101
         
         IF g_glaa015 = 'Y' THEN                         
             #主帳套本位幣二匯率
             IF g_glaa017 = '1' THEN 
                LET l_ooam003 = l_nmbt100
             ELSE
                LET l_ooam003 = g_glaa001
             END IF
                                        #日期;           來源幣別   目的幣別; 匯類類型
             CALL anmt530_get_exrate(g_nmbs_m.nmbsdocdt,l_ooam003,g_glaa016,g_glaa018)                      
             RETURNING l_nmbt121   
             LET l_nmbt123 = l_nmbt113 * l_nmbt121   
         END IF
          
         IF g_glaa019 = 'Y' THEN                         
            #主帳套本位幣三匯率
            IF g_glaa021 = '1' THEN 
               LET l_ooam003 = l_nmbt100
            ELSE
               LET l_ooam003 = g_glaa001
            END IF
                                       #日期;           來源幣別   目的幣別; 匯類類型
            CALL anmt530_get_exrate(g_nmbs_m.nmbsdocdt,l_ooam003,g_glaa020,g_glaa022)
            RETURNING l_nmbt131   
            LET l_nmbt133 = l_nmbt113 * l_nmbt131   
         END IF
      END IF 
      
      LET l_nmbt022 = l_nmbt021
      SELECT ooeg004 INTO l_nmbt019
        FROM ooeg_t
       WHERE ooegent = g_enterprise
         AND ooeg001 = l_nmbt018
      
      INSERT INTO nmbt_t(nmbtent,nmbtcomp,nmbtld,nmbtdocno,nmbtseq,nmbt001,nmbt002,nmbt003,
                         nmbt011,nmbt100,nmbt101,nmbt103,nmbt113,nmbt012,nmbt013,nmbt014,
                         nmbt017,nmbt018,nmbt019,nmbt021,nmbt022,nmbt025,nmbt121,nmbt123,
                         nmbt131,nmbt133) 
                  VALUES(g_enterprise,g_nmbs_m.nmbscomp,g_nmbs_m.nmbsld,g_nmbs_m.nmbsdocno,l_nmbtseq,
                         '2',l_nmcrdocno,l_nmcrseq,l_nmcr001,l_nmbt100,l_nmbt101,l_nmbt103,l_nmbt113,
                         l_nmbt012,g_user,l_nmbt014,l_nmbt017,l_nmbt018,l_nmbt019,l_nmbt021,l_nmbt022,
                         l_nmbt025,l_nmbt121,l_nmbt123,l_nmbt131,l_nmbt133)
      IF SQLCA.SQLcode  THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "ins nmbt"
         LET g_errparam.popup = TRUE
         CALL cl_err()
  
         LET g_success = 'N'         
      END IF
      
      SELECT nmcq001
        INTO l_nmcq001    
        FROM nmcq_t
       WHERE nmcqent = g_enterprise
         AND nmcqcomp = g_nmbs_m.nmbscomp
         AND nmcqdocno = l_nmcrdocno
      CALL anmt530_nmbv_ins(g_nmbs_m.nmbsdocno,l_nmbtseq,'2',p_nmbt002,l_nmcq001,l_nmcrseq,l_nmbt121,l_nmbt123,l_nmbt131,l_nmbt133)
    
   END FOREACH 
   IF g_success = 'Y' THEN
      CALL s_transaction_end('Y',0)
   ELSE
      CALL s_transaction_end('N',0)
   END IF
   
   CALL anmt530_b_fill()
END FUNCTION]]>
  </point>
