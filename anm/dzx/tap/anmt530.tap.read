      CALL s_voucher_gen_nm(p_type,p_ld,p_date,p_slip,p_sum_type,p_wc,'Y','anmt530') RETURNING r_success
   #END IF
   
   #檢查科目是否為空
   LET l_sql = "SELECT COUNT(*) FROM s_voucher_nm_tmp ",
               " WHERE glaq002 IS NULL "
   PREPARE chk_glaq002 FROM l_sql
   EXECUTE chk_glaq002 INTO l_n   
   IF l_n > 0 THEN  
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'axr-00068'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()
      RETURN
