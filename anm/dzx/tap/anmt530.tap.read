  <point name="function.anmt530_nmbt002_chk" order="5" ver="3" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[# nmbt002重複性檢查
PRIVATE FUNCTION anmt530_nmbt002_chk(p_cmd)
   DEFINE l_n    LIKE type_t.num5
   DEFINE p_cmd  LIKE type_t.chr1
   
   LET g_errno = ''
   LET l_n = 0
   IF g_nmbt_d[l_ac].nmbt001 = '2' THEN  
      IF NOT cl_null(g_nmbt_d[l_ac].nmbt002) AND NOT cl_null(g_nmbt_d[l_ac].nmbt003) THEN 
         SELECT COUNT(*) INTO l_n 
           FROM nmcq_t,nmcr_t
          WHERE nmcqent = nmcrent
            AND nmcqcomp = nmcrcomp
            AND nmcqdocno = nmcrdocno
            AND nmcqent = g_enterprise
            AND nmcqcomp = g_nmbs_m.nmbscomp
            AND nmcqdocno = g_nmbt_d[l_ac].nmbt002
            AND nmcrseq = g_nmbt_d[l_ac].nmbt003
            
         IF l_n = 0 THEN 
            LET g_errno = 'anm-00195'
            RETURN
         END IF
      END IF
      
       #檢查應收票據性質 
      LET l_n = 0   
      IF NOT cl_null(g_nmbt_d[l_ac].nmbt002) THEN
         
         SELECT COUNT(*) INTO l_n  FROM gzcb_t WHERE gzcb002 IN (SELECT nmcq001 FROM nmcq_t WHERE nmcqent = g_enterprise AND nmcqdocno = g_nmbt_d[l_ac].nmbt002 AND nmcqcomp = g_nmbs_m.nmbscomp)
                                                 AND gzcb005 = 'Y'
                                                 AND gzcb001 = '8714'
         IF l_n = 0 THEN
            LET g_errno = 'anm-00302'
            RETURN
         END IF         
      END IF
      
   END IF
   
   LET l_n = 0
   IF p_cmd = 'a' THEN 
      IF g_nmbt_d[l_ac].nmbt001 = '1' THEN 
         SELECT COUNT(*) INTO l_n
           FROM nmbt_t
          WHERE nmbtent = g_enterprise
            AND nmbtld = g_nmbs_m.nmbsld
            AND nmbt002 = g_nmbt_d[l_ac].nmbt002 
      END IF
      IF g_nmbt_d[l_ac].nmbt001 = '2' THEN 
         SELECT COUNT(*) INTO l_n
           FROM nmbt_t
          WHERE nmbtent = g_enterprise
            AND nmbtld = g_nmbs_m.nmbsld
            AND nmbt002 = g_nmbt_d[l_ac].nmbt002 
            AND nmbt003 = g_nmbt_d[l_ac].nmbt003
      END IF
   END IF
   IF p_cmd = 'u' THEN 
      IF g_nmbt_d[l_ac].nmbt001 = '1' THEN 
         SELECT COUNT(*) INTO l_n
           FROM nmbt_t
          WHERE nmbtent = g_enterprise
            AND nmbt002 = g_nmbt_d[l_ac].nmbt002
            AND nmbtld = g_nmbs_m.nmbsld
            AND nmbtdocno = g_nmbs_m.nmbsdocno
            AND nmbtseq <> g_nmbt_d[l_ac].nmbtseq
      END IF
      IF g_nmbt_d[l_ac].nmbt001 = '2' THEN 
         SELECT COUNT(*) INTO l_n
           FROM nmbt_t
          WHERE nmbtent = g_enterprise
            AND nmbt002 = g_nmbt_d[l_ac].nmbt002
            AND nmbt003 = g_nmbt_d[l_ac].nmbt003
            AND nmbtld = g_nmbs_m.nmbsld
            AND nmbtdocno = g_nmbs_m.nmbsdocno
            AND nmbtseq <> g_nmbt_d[l_ac].nmbtseq
      END IF
   END IF
   
   IF l_n > 0 THEN 
      LET g_errno = 'anm-00171'
   END IF
 
END FUNCTION]]>
  </point>
  <point name="function.anmt530_get_exrate" order="6" ver="2" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[# 抓取本位幣二三匯率
PRIVATE FUNCTION anmt530_get_exrate(p_date,p_ooam003,p_ooan002,p_type)
   DEFINE p_date       LIKE nmbs_t.nmbsdocdt
   DEFINE p_ooam003    LIKE ooam_t.ooam003
   DEFINE p_ooan002    LIKE ooan_t.ooan002
   DEFINE p_type       LIKE glaa_t.glaa018
   DEFINE r_exrate     LIKE nmbt_t.nmbt121
   DEFINE l_nmbb028    LIKE nmbb_t.nmbb028
   
                          #匯率參照表;帳套;         日期;  來源幣別
   CALL s_aooi160_get_exrate('2',g_nmbs_m.nmbsld,p_date,p_ooam003,
                             #目的幣別; 交易金額; 匯類類型
                             p_ooan002,0,p_type)
   RETURNING r_exrate  
   
   RETURN r_exrate   
   
END FUNCTION]]>
  </point>
  <point name="function.anmt530_nmbt002_default" order="7" ver="3" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[# 根據nmbt002抓取其他欄位的預設值
PRIVATE FUNCTION anmt530_nmbt002_default()
   DEFINE l_ooam003      LIKE ooam_t.ooam003
   DEFINE l_nmbb028      LIKE nmbb_t.nmbb028
   DEFINE l_nmcq001      LIKE nmcq_t.nmcq001
   
   IF g_nmbt_d[l_ac].nmbt001 = '1' THEN 
      SELECT nmbb030,nmbb004,nmbb005,nmbb006,nmbb007,
             nmbb031,nmbb003,nmba001,nmba004,nmba008
        INTO g_nmbt_d[l_ac].nmbt011,g_nmbt_d[l_ac].nmbt100,
             g_nmbt_d[l_ac].nmbt101,g_nmbt_d[l_ac].nmbt103,
             g_nmbt_d[l_ac].nmbt113,g_nmbt_d[l_ac].nmbt012,
             g_nmbt_d[l_ac].nmbt014,g_nmbt2_d[l_ac].nmbt018,
             g_nmbt2_d[l_ac].nmbt021,g_nmbt2_d[l_ac].nmbt025 
        FROM nmba_t,nmbb_t
       WHERE nmbaent = nmbbent
         AND nmbacomp = nmbbcomp
         AND nmbadocno = nmbbdocno
         AND nmbaent = g_enterprise
         AND nmbacomp = g_nmbs_m.nmbscomp
         AND nmbadocno = g_nmbt_d[l_ac].nmbt002  
      LET g_nmbt_d[l_ac].nmbt003 = 1         
   END IF
   
   IF g_nmbt_d[l_ac].nmbt001 = '2' THEN 
      IF NOT cl_null(g_nmbt_d[l_ac].nmbt002) AND NOT cl_null(g_nmbt_d[l_ac].nmbt003) THEN 
         SELECT nmcr001,nmcq100,nmcq101,nmcr103,nmcr113,
                nmbb031,nmbb003,nmba001,nmba004,nmba008
           INTO g_nmbt_d[l_ac].nmbt011,g_nmbt_d[l_ac].nmbt100,
                g_nmbt_d[l_ac].nmbt101,g_nmbt_d[l_ac].nmbt103,
                g_nmbt_d[l_ac].nmbt113,g_nmbt_d[l_ac].nmbt012,
                g_nmbt_d[l_ac].nmbt014,g_nmbt2_d[l_ac].nmbt018,
                g_nmbt2_d[l_ac].nmbt021,g_nmbt2_d[l_ac].nmbt025
           FROM nmcr_t,nmcq_t,nmba_t,nmbb_t
          WHERE nmbaent = nmbbent
            AND nmbacomp = nmbbcomp
            AND nmbadocno = nmbbdocno
            AND nmcrent = nmcqent
            AND nmcrcomp = nmcqcomp
            AND nmcrdocno = nmcqdocno
            AND nmcr001 = nmbb030
            AND nmcrent = g_enterprise
            AND nmcrcomp = g_nmbs_m.nmbscomp
            AND nmcrdocno = g_nmbt_d[l_ac].nmbt002  
            AND nmcrseq = g_nmbt_d[l_ac].nmbt003
      ELSE
          SELECT nmcq100,nmcq101 INTO g_nmbt_d[l_ac].nmbt100,g_nmbt_d[l_ac].nmbt101
                FROM nmcq_t WHERE nmcqent = g_enterprise
                              AND nmcqdocno = g_nmbt_d[l_ac].nmbt002 
                              AND nmcqcomp = g_nmbs_m.nmbscomp
