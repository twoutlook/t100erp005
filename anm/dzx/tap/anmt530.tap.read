#  LET l_nmbv.nmbv113 = 0                         #借方本幣金額
#  LET l_nmbv.nmbv114 = l_nmcr113                 #貸方本幣金額
#  LET l_nmbv.nmbv123 = 0                         #本位幣二借方金額
#  LET l_nmbv.nmbv124 = p_nmbt123                 #本位幣二貸方金額
#  LET l_nmbv.nmbv133 = 0                         #本位幣三借方金額
