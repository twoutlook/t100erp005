  <point name="function.anmt530_ooac_chk" order="41" ver="3" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 檢查單別是否可以憑證預覽
# Memo...........:
# Usage..........: CALL anmt530_ooac_chk()
#                  RETURNING r_success
# Input parameter: 
# Return code....: r_success TRUE/FALSE
# Date & Author..: 20141204 By huangrh
# Modify.........:
################################################################################
PRIVATE FUNCTION anmt530_ooac_chk()
DEFINE r_success      LIKE type_t.num5
DEFINE l_slip         LIKE ooac_t.ooac002
DEFINE l_glaa024      LIKE glaa_t.glaa024
DEFINE l_ooac004      LIKE ooac_t.ooac004


   LET r_success=TRUE
   IF cl_null(g_nmbs_m.nmbsdocno) OR cl_null(g_nmbs_m.nmbsld) THEN
      LET r_success=FALSE
      RETURN r_success
   END IF

   #CALL s_aooi200_get_slip(g_nmbs_m.nmbsdocno) RETURNING r_success,l_slip      #2014/12/29 liuym mark 
   CALL s_aooi200_fin_get_slip(g_nmbs_m.nmbsdocno) RETURNING r_success,l_slip   #2014/12/29 liuym add 
   IF NOT r_success THEN
      RETURN r_success
   END IF   

   #根據帳套取得參照表號
   SELECT glaa024 INTO l_glaa024
     FROM glaa_t
    WHERE glaaent=g_enterprise
      AND glaald=g_nmbs_m.nmbsld
      AND glaastus='Y'

   #根据单据别+参照表号+参数编号获取参数值ooac004
   SELECT ooac004 INTO l_ooac004 
    FROM ooac_t 
    WHERE ooacent=g_enterprise 
      AND ooac001=l_glaa024
      AND ooac002=l_slip 
      AND ooac003='D-FIN-0030'
  
   IF l_ooac004='Y' THEN
      RETURN r_success
   ELSE
      LET r_success=FALSE
      RETURN r_success
   END IF   
                    
END FUNCTION]]>
