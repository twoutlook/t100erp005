  <point name="function.anmt530_confirm" order="30" ver="3" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[# 資料確認
PRIVATE FUNCTION anmt530_confirm()
   DEFINE l_nmbt002     LIKE nmbt_t.nmbt002
   DEFINE l_nmbt001     LIKE nmbt_t.nmbt001
   
   CALL s_transaction_begin()
   #CALL cl_showmsg_init()
   CALL cl_err_collect_init()
   LET g_success = 'Y'
   
   LET g_sql = "SELECT DISTINCT nmbt002,nmbt001 ",
               "  FROM nmbt_t ",
               " WHERE nmbtent = '",g_enterprise,"'",
               "   AND nmbtld = '",g_nmbs_m.nmbsld,"'",
               "   AND nmbtdocno = '",g_nmbs_m.nmbsdocno,"'"
   PREPARE anmt530_pre FROM g_sql
   DECLARE anmt530_cur CURSOR FOR anmt530_pre
   
   FOREACH anmt530_cur INTO l_nmbt002,l_nmbt001
      IF l_nmbt001 = '1' THEN 
         UPDATE nmba_t SET nmba007 = g_nmbs_m.nmbsdocno
          WHERE nmbaent = g_enterprise
            AND nmbacomp = g_nmbs_m.nmbscomp
            AND nmbadocno = l_nmbt002
      END IF
      
      IF l_nmbt001 = '2' THEN 
         UPDATE nmcq_t SET nmcq007 = g_nmbs_m.nmbsdocno
          WHERE nmcqent = g_enterprise
            AND nmcqcomp = g_nmbs_m.nmbscomp
            AND nmcqdocno = l_nmbt002
      END IF 
      
      IF SQLCA.SQLcode  THEN
         #CALL cl_errmsg('update',l_nmbt002,'',SQLCA.SQLCODE,1)
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.SQLCODE
         LET g_errparam.extend = "update"
         LET g_errparam.popup = TRUE
         CALL cl_err()
         LET g_success = 'N'         
      END IF
   END FOREACH 
   
   IF g_success = 'N' THEN
      CALL s_transaction_end('N',1)
      #CALL cl_err_showmsg()
   ELSE
      CALL s_transaction_end('Y',1)
   END IF
   CALL cl_err_collect_show()
END FUNCTION]]>
