   DEFINE r_start_no     LIKE glap_t.glapdocno
   DEFINE r_end_no       LIKE glap_t.glapdocno
   DEFINE l_n            LIKE type_t.num5
   DEFINE l_sql          STRING
    
   CALL s_transaction_begin()
   
   #IF g_flag = 'N' THEN 
      #传入参数赋值
      LET p_type = '1'
      LET p_ld = g_nmbs_m.nmbsld
      LET p_sum_type = '0'
