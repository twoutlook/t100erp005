      LET g_errparam.code = SQLCA.SQLCODE   #(ver:78)
      LET g_errparam.popup = TRUE 
      CLOSE anmt470_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE anmt470_master_referesh USING g_nmbs_m.nmbsld,g_nmbs_m.nmbsdocno INTO g_nmbs_m.nmbssite,g_nmbs_m.nmbsld, 
       g_nmbs_m.nmbscomp,g_nmbs_m.nmbsdocno,g_nmbs_m.nmbsdocdt,g_nmbs_m.nmbs001,g_nmbs_m.nmbs003,g_nmbs_m.nmbs004, 
