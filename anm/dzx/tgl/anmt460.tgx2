      RETURN
   END IF
   
   
   
   CALL s_transaction_begin()
 
   OPEN anmt460_cl USING g_enterprise,g_nmck_m.nmckcomp,g_nmck_m.nmckdocno
   IF SQLCA.SQLCODE THEN   #(ver:78)
      INITIALIZE g_errparam TO NULL 
