               LET g_nmec_d[l_ac].* = g_nmec_d_t.*
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code = 9001 
               LET g_errparam.popup = FALSE 
               CLOSE anmt561_bcl
               CALL s_transaction_end('N','0')
               CALL cl_err()
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = g_nmec_d[l_ac].nmecseq 
               LET g_errparam.code = -263 
               LET g_errparam.popup = TRUE 
               CALL cl_err()
               LET g_nmec_d[l_ac].* = g_nmec_d_t.*
            ELSE
            
               #add-point:單身修改前 name="input.body.b_update"
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               #寫入修改者/修改日期資訊(單身)
               
      
               #將遮罩欄位還原
               CALL anmt561_nmec_t_mask_restore('restore_mask_o')
      
               UPDATE nmec_t SET (nmeccomp,nmecdocno,nmecseq,nmecsite,nmec001,nmec002,nmec003,nmec004, 
                   nmec005,nmec012,nmec006,nmec007,nmec008,nmec009,nmec011,nmec015,nmec016,nmec017,nmec021, 
                   nmec027,nmec013,nmec028,nmec014,nmec024,nmec022,nmec026) = (g_nmba_m.nmbacomp,g_nmba_m.nmbadocno, 
                   g_nmec_d[l_ac].nmecseq,g_nmec_d[l_ac].nmecsite,g_nmec_d[l_ac].nmec001,g_nmec_d[l_ac].nmec002, 
                   g_nmec_d[l_ac].nmec003,g_nmec_d[l_ac].nmec004,g_nmec_d[l_ac].nmec005,g_nmec_d[l_ac].nmec012, 
                   g_nmec_d[l_ac].nmec006,g_nmec_d[l_ac].nmec007,g_nmec_d[l_ac].nmec008,g_nmec_d[l_ac].nmec009, 
                   g_nmec_d[l_ac].nmec011,g_nmec_d[l_ac].nmec015,g_nmec_d[l_ac].nmec016,g_nmec_d[l_ac].nmec017, 
                   g_nmec_d[l_ac].nmec021,g_nmec_d[l_ac].nmec027,g_nmec_d[l_ac].nmec013,g_nmec_d[l_ac].nmec028, 
                   g_nmec_d[l_ac].nmec014,g_nmec_d[l_ac].nmec024,g_nmec_d[l_ac].nmec022,g_nmec_d[l_ac].nmec026) 

                WHERE nmecent = g_enterprise AND nmeccomp = g_nmba_m.nmbacomp 
                  AND nmecdocno = g_nmba_m.nmbadocno 
 
                  AND nmecseq = g_nmec_d_t.nmecseq #項次   
 
                  
               #add-point:單身修改中 name="input.body.m_update"
               {<point name="input.body.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     LET g_nmec_d[l_ac].* = g_nmec_d_t.*
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "nmec_t" 
                     LET g_errparam.code = "std-00009" 
                     LET g_errparam.popup = TRUE 
                     CALL s_transaction_end('N','0')
                     CALL cl_err()
                     
                  WHEN SQLCA.SQLCODE #其他錯誤
                     LET g_nmec_d[l_ac].* = g_nmec_d_t.*  
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "nmec_t:",SQLERRMESSAGE 
                     LET g_errparam.code = SQLCA.SQLCODE 
                     LET g_errparam.popup = TRUE 
                     CALL s_transaction_end('N','0')
                     CALL cl_err()                   
                     
                  OTHERWISE
                     #資料多語言用-增/改
                     
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_nmba_m.nmbacomp
               LET gs_keys_bak[1] = g_nmbacomp_t
               LET gs_keys[2] = g_nmba_m.nmbadocno
               LET gs_keys_bak[2] = g_nmbadocno_t
               LET gs_keys[3] = g_nmec_d[g_detail_idx].nmecseq
               LET gs_keys_bak[3] = g_nmec_d_t.nmecseq
               CALL anmt561_update_b('nmec_t',gs_keys,gs_keys_bak,"'1'")
               END CASE
 
               #將遮罩欄位進行遮蔽
               CALL anmt561_nmec_t_mask_restore('restore_mask_n')
               
               #判斷key是否有改變
               INITIALIZE gs_keys TO NULL
               IF NOT(g_nmec_d[g_detail_idx].nmecseq = g_nmec_d_t.nmecseq 
 
                  ) THEN
                  LET gs_keys[01] = g_nmba_m.nmbacomp
                  LET gs_keys[gs_keys.getLength()+1] = g_nmba_m.nmbadocno
 
                  LET gs_keys[gs_keys.getLength()+1] = g_nmec_d_t.nmecseq
 
                  CALL anmt561_key_update_b(gs_keys,'nmec_t')
               END IF
               
               #修改歷程記錄(單身修改)
               LET g_log1 = util.JSON.stringify(g_nmba_m),util.JSON.stringify(g_nmec_d_t)
               LET g_log2 = util.JSON.stringify(g_nmba_m),util.JSON.stringify(g_nmec_d[l_ac])
               IF NOT cl_log_modified_record_d(g_log1,g_log2) THEN 
                  CALL s_transaction_end('N','0')
               END IF
               
               #add-point:單身修改後 name="input.body.a_update"
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            #add-point:單身after_row name="input.body.after_row"
            {<point name="input.body.after_row"/>}
            #end add-point
            CALL anmt561_unlock_b("nmec_t","'1'")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock
            #add-point:單身after_row2 name="input.body.after_row2"
            {<point name="input.body.after_row2"/>}
            #end add-point
              
         AFTER INPUT
            #add-point:input段after input  name="input.body.after_input"
            {<point name="input.body.after_input"/>}
            #end add-point 
    
         ON ACTION controlo    
            IF l_insert THEN
               LET li_reproduce = l_ac_t
               LET li_reproduce_target = l_ac
               LET g_nmec_d[li_reproduce_target].* = g_nmec_d[li_reproduce].*
 
               LET g_nmec_d[li_reproduce_target].nmecseq = NULL
 
            ELSE
               CALL FGL_SET_ARR_CURR(g_nmec_d.getLength()+1)
               LET lb_reproduce = TRUE
               LET li_reproduce = l_ac
               LET li_reproduce_target = g_nmec_d.getLength()+1
            END IF
            
         #ON ACTION cancel
         #   LET INT_FLAG = 1
         #   LET g_detail_idx = 1
         #   EXIT DIALOG 
 
      END INPUT
      
 
      
 
 
 

