         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY nmbu_t.nmbuseq"
         
         #add-point:單身填充控制 name="b_fill.sql2"
         {<point name="b_fill.sql2"/>}
         #end add-point
         
