                     " AND nmbadocno = nmbudocno ",
 
                     "",
                     
                                    " LEFT JOIN ooefl_t t5 ON t5.ooeflent="||g_enterprise||" AND t5.ooefl001=nmbuorga AND t5.ooefl002='"||g_dlang||"' ",
 
                     " WHERE nmbuent=? AND nmbucomp=? AND nmbudocno=?"   
