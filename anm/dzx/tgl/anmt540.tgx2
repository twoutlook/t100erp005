   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE anmt540_pb
   FREE anmt540_pb2
 
 
   
   LET li_idx = l_ac
   
   #遮罩相關處理
   FOR l_ac = 1 TO g_nmbb_d.getLength()
      LET g_nmbb_d_mask_o[l_ac].* =  g_nmbb_d[l_ac].*
      CALL anmt540_nmbb_t_mask()
      LET g_nmbb_d_mask_n[l_ac].* =  g_nmbb_d[l_ac].*
   END FOR
   
