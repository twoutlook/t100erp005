      CALL anmt540_nmbu_t_mask()
      LET g_nmbb3_d_mask_n[l_ac].* =  g_nmbb3_d[l_ac].*
   END FOR
 
   
   LET l_ac = li_idx
   
   CALL cl_ap_performance_next_end()
 
