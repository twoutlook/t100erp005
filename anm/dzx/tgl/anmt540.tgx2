         LET g_sql = "SELECT  DISTINCT nmbuseq,nmbu001,nmbuorga,nmbu003,nmbu004,nmbu005,nmbu006,nmbu007, 
             nmbu002 ,t5.ooefl003 FROM nmbu_t",   
