             s_detail1[1].glaq038,s_detail1[1].glaq038_desc 
      
         
      
                  #應用 a01 樣板自動產生(Version:2)
         BEFORE FIELD nmde001
            #add-point:BEFORE FIELD nmde001 name="query.b.page1.nmde001"
            {<point name="query.b.page1.nmde001" />}
            #END add-point
 
 
         #應用 a02 樣板自動產生(Version:2)
