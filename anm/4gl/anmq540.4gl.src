# Descriptions...: 查詢條件初始化
# Memo...........:
# Usage..........: CALL anmq540_def()
#                  RETURNING ---
# Input parameter: 
# Return code....: 
# Date & Author..: 2016/06/28 By 01727
# Modify.........:
################################################################################
PRIVATE FUNCTION anmq540_def()
   DEFINE l_sql      STRING   #160816-00012#4 Add
   DEFINE l_wc       STRING   #160816-00012#4 Add
   DEFINE l_count    LIKE type_t.num5  #160816-00012#4 Add
   
   IF cl_null(g_master_t.nmbasite) THEN
      LET g_master.sdate = g_today
      LET g_master.edate = g_today
      #取得預設的資金中心,因新增階段的時候,並不會知道site,所以以登入人員做為依據
      CALL s_fin_get_account_center('',g_user,'6',g_master.edate) RETURNING g_sub_success,g_master.nmbasite,g_errno
      LET g_master.nmbasite_desc = s_desc_get_department_desc(g_master.nmbasite)
      CALL s_anm_get_comp_wc('6',g_master.nmbasite,g_master.edate) RETURNING g_comp_wc
      SELECT ooef017 INTO g_master.nmbacomp FROM ooef_t
       WHERE ooefent = g_enterprise
         AND ooef001 = g_master.nmbasite
      #160816-00012#4 Add  ---(S)---
      #检查用户是否有资金中心对应法人的权限
      CALL s_axrt300_get_site(g_user,'','3') RETURNING l_wc
      LET l_count = 0
      LET l_sql = "SELECT COUNT(*) FROM ooef_t WHERE ooefent = '",g_enterprise,"' ",
                  "   AND ooef001 = '",g_master.nmbacomp,"'",
                  "   AND ooef003 = 'Y'",
                  "   AND ",l_wc CLIPPED
      PREPARE anmp410_count_prep FROM l_sql
      EXECUTE anmp410_count_prep INTO l_count
      IF cl_null(l_count) THEN LET l_count = 0 END IF
      IF l_count = 0 THEN
         LET g_master.nmbacomp = ''           
         DISPLAY BY NAME g_master.nmbacomp
      END IF
      #160816-00012#4 Add  ---(E)---   
      LET g_master.nmbacomp_desc = s_desc_get_department_desc(g_master.nmbacomp)
      DISPLAY BY NAME g_master.nmbasite_desc,g_master.nmbacomp_desc
   ELSE
      LET g_master.* = g_master_t.*
   END IF

   CALL anmq540_set_entry()

END FUNCTION

