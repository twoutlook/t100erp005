               "                          AND xrde004 = nmbbseq   AND xrde003 = nmbadocno     AND xrdald = '",l_glaald,"' ",
               "                         AND xrdastus = 'N'      AND xrdeent = '",g_enterprise,"' ) xrde139, ",
               "                  (SELECT CASE WHEN SUM (isbb103) IS NULL THEN 0 ELSE SUM (isbb103) END FROM isba_t, isbb_t ",
               "                    WHERE     isbaent = isbbent   AND isbadocno = isbbdocno   AND isbacomp = isbbcomp ",
               "                          AND isbb003 = nmbbseq   AND isbb002 = nmbadocno     AND isbastus <> 'X' ",
               "                          AND isbaent = '",g_enterprise,"' AND isbacomp = '",g_master.nmbacomp,"') isbb103, ",
               "                  (SELECT CASE WHEN SUM (isbb104) IS NULL THEN 0 ELSE SUM (isbb104) END FROM isba_t, isbb_t ",
               "                    WHERE     isbaent = isbbent   AND isbadocno = isbbdocno   AND isbacomp = isbbcomp ",
               "                           AND isbb003 = nmbbseq   AND isbb002 = nmbadocno     AND isbastus <> 'X' ",
               "                          AND isbaent = '",g_enterprise,"' AND isbacomp = '",g_master.nmbacomp,"') isbb104, ",
               "                  (SELECT MAX(nmbc005) FROM nmbc_t WHERE nmbcent = nmbbent AND nmbc012 = nmbb030 AND nmbccomp = nmbbcomp AND nmbc003 = nmbb026) l_nmck012 ",
               "             FROM nmba_t, nmbb_t ",
               "             WHERE     nmbaent = nmbbent    AND nmbadocno = nmbbdocno ",
               "                 AND nmbacomp = nmbbcomp   AND nmbastus <> 'X' ",
               "                  AND nmbaent = ",g_enterprise," ",
               "                  AND ( (nmba003 = 'anmt510' AND nmbb042 NOT IN ('5', '6', '7', '9')) OR nmba003 = 'anmt540') ",
               "                  AND NOT EXISTS (SELECT 1 FROM apde_t, apda_t ",
               "                          WHERE     apdaent = apdeent      AND apdadocno = apdedocno ",
               "                                         AND apdald = apdeld       AND apdald = '",l_glaald,"' ",
               "                                         AND apde003 = nmbadocno   AND nmba001 = 'anmt510' ",
               "                                         AND apde003 = '16'        AND apde006 = '30') ",
               "                 AND nmbadocdt BETWEEN '",g_master.sdate,"' AND '",g_master.edate,"' ",
               "                 AND nmbacomp = '",g_master.nmbacomp,"') ",
       " WHERE ((nmbb006 - nmbb008 - xrde109 > 0 AND isbb103 = 0) OR (isbb103 - nmbb008 - xrde109 > 0 AND isbb103 > 0))"   
   #161125-00036#8-----e
   
   PREPARE anmq540_prep FROM l_sql
   EXECUTE anmq540_prep

END FUNCTION

