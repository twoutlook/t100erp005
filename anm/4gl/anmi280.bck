PRIVATE FUNCTION anmi280_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   
   #end add-point
   DEFINE  l_cmd                  LIKE type_t.chr1
   DEFINE  l_ac_t                 LIKE type_t.num10               #未取消的ARRAY CNT 
   DEFINE  l_n                    LIKE type_t.num10               #檢查重複用  
   DEFINE  l_cnt                  LIKE type_t.num10               #檢查重複用  
   DEFINE  l_lock_sw              LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert         LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete         LIKE type_t.num5                #可刪除否  
   DEFINE  l_count                LIKE type_t.num10
   DEFINE  l_i                    LIKE type_t.num10
   DEFINE  ls_return              STRING
   DEFINE  l_var_keys             DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys           DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                 DYNAMIC ARRAY OF STRING
   DEFINE  l_fields               DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak         DYNAMIC ARRAY OF STRING
   DEFINE  li_reproduce           LIKE type_t.num10
   DEFINE  li_reproduce_target    LIKE type_t.num10
   DEFINE  lb_reproduce           BOOLEAN
   DEFINE  l_insert               BOOLEAN
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   
   #end add-point 
   
   #add-point:Function前置處理  name="modify.pre_function"
   
   #end add-point
   
   LET g_action_choice = ""
   
   LET g_qryparam.state = "i"
 
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   
   #add-point:modify開始前 name="modify.define_sql"
   
   #end add-point
   
   LET INT_FLAG = FALSE
   LET lb_reproduce = FALSE
   LET l_insert = FALSE
 
   #關閉被遮罩相關欄位輸入, 無法確定USER是否會需要輸入此欄位
   #因此先行關閉, 若有需要可於下方add-point中自行開啟
   CALL cl_mask_set_no_entry()
 
   #add-point:modify段修改前 name="modify.before_input"
   
   #end add-point
 
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #Page1 預設值產生於此處
      INPUT ARRAY g_nmav_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_detail_cnt,WITHOUT DEFAULTS, #MAXCOUNT = g_max_rec,
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_nmav_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL anmi280_b_fill(g_wc2)
            LET g_detail_cnt = g_nmav_d.getLength()
         
         BEFORE ROW
            #add-point:modify段before row name="input.body.before_row2"
            
            #end add-point  
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_cmd = ''
            LET l_ac_t = l_ac 
            LET l_ac = g_detail_idx
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            DISPLAY g_nmav_d.getLength() TO FORMONLY.cnt
         
            CALL s_transaction_begin()
            LET g_detail_cnt = g_nmav_d.getLength()
            
            IF g_detail_cnt >= l_ac 
               AND g_nmav_d[l_ac].nmav001 IS NOT NULL
               AND g_nmav_d[l_ac].nmav002 IS NOT NULL
               AND g_nmav_d[l_ac].nmav003 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_nmav_d_t.* = g_nmav_d[l_ac].*  #BACKUP
               LET g_nmav_d_o.* = g_nmav_d[l_ac].*  #BACKUP
               IF NOT anmi280_lock_b("nmav_t") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH anmi280_bcl INTO g_nmav_d[l_ac].nmav001,g_nmav_d[l_ac].nmav002,g_nmav_d[l_ac].nmav003, 
                      g_nmav_d[l_ac].nmav004,g_nmav_d[l_ac].nmav005,g_nmav_d[l_ac].nmavstus,g_nmav2_d[l_ac].nmav001, 
                      g_nmav2_d[l_ac].nmav002,g_nmav2_d[l_ac].nmav003,g_nmav2_d[l_ac].nmavownid,g_nmav2_d[l_ac].nmavowndp, 
                      g_nmav2_d[l_ac].nmavcrtid,g_nmav2_d[l_ac].nmavcrtdp,g_nmav2_d[l_ac].nmavcrtdt, 
                      g_nmav2_d[l_ac].nmavmodid,g_nmav2_d[l_ac].nmavmoddt
                  IF SQLCA.sqlcode THEN
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = g_nmav_d_t.nmav001,":",SQLERRMESSAGE  
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     LET l_lock_sw = "Y"
                  END IF
                  
                  #遮罩相關處理
                  LET g_nmav_d_mask_o[l_ac].* =  g_nmav_d[l_ac].*
                  CALL anmi280_nmav_t_mask()
                  LET g_nmav_d_mask_n[l_ac].* =  g_nmav_d[l_ac].*
                  
                  CALL anmi280_detail_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            CALL anmi280_set_entry_b(l_cmd)
            CALL anmi280_set_no_entry_b(l_cmd)
            #add-point:modify段before row name="input.body.before_row"
            
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
 
 
            #其他table進行lock
            
 
 
        
         BEFORE INSERT
            
            LET l_insert = TRUE
            IF s_transaction_chk("N",0) THEN
               CALL s_transaction_begin()
            END IF
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_nmav_d_t.* TO NULL
            INITIALIZE g_nmav_d_o.* TO NULL
            INITIALIZE g_nmav_d[l_ac].* TO NULL 
            #公用欄位給值(單身)
            #應用 a14 樣板自動產生(Version:5)    
      #公用欄位新增給值  
      LET g_nmav2_d[l_ac].nmavownid = g_user
      LET g_nmav2_d[l_ac].nmavowndp = g_dept
      LET g_nmav2_d[l_ac].nmavcrtid = g_user
      LET g_nmav2_d[l_ac].nmavcrtdp = g_dept 
      LET g_nmav2_d[l_ac].nmavcrtdt = cl_get_current()
      LET g_nmav2_d[l_ac].nmavmodid = g_user
      LET g_nmav2_d[l_ac].nmavmoddt = cl_get_current()
      LET g_nmav_d[l_ac].nmavstus = ''
 
 
 
            #自定義預設值(單身2)
                  LET g_nmav_d[l_ac].nmavstus = "Y"
 
            #add-point:modify段before備份 name="input.body.before_bak"
            
            #end add-point
            LET g_nmav_d_t.* = g_nmav_d[l_ac].*     #新輸入資料
            LET g_nmav_d_o.* = g_nmav_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_nmav_d[li_reproduce_target].* = g_nmav_d[li_reproduce].*
               LET g_nmav2_d[li_reproduce_target].* = g_nmav2_d[li_reproduce].*
 
               LET g_nmav_d[g_nmav_d.getLength()].nmav001 = NULL
               LET g_nmav_d[g_nmav_d.getLength()].nmav002 = NULL
               LET g_nmav_d[g_nmav_d.getLength()].nmav003 = NULL
 
            END IF
            
 
 
            CALL anmi280_set_entry_b(l_cmd)
            CALL anmi280_set_no_entry_b(l_cmd)
            #add-point:modify段before insert name="input.body.before_insert"
            
            #end add-point  
  
         AFTER INSERT
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(1) INTO l_count FROM nmav_t 
             WHERE nmavent = g_enterprise AND nmav001 = g_nmav_d[l_ac].nmav001
                                       AND nmav002 = g_nmav_d[l_ac].nmav002
                                       AND nmav003 = g_nmav_d[l_ac].nmav003
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前 name="input.body.b_insert"
               
               #end add-point
            
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_nmav_d[g_detail_idx].nmav001
               LET gs_keys[2] = g_nmav_d[g_detail_idx].nmav002
               LET gs_keys[3] = g_nmav_d[g_detail_idx].nmav003
               CALL anmi280_insert_b('nmav_t',gs_keys,"'1'")
                           
               #add-point:單身新增後 name="input.body.a_insert"
               
               #end add-point
            ELSE    
               INITIALIZE g_nmav_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = 'INSERT' 
               LET g_errparam.code   = "std-00006" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "nmav_t:",SQLERRMESSAGE 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL s_transaction_end('N','0')                    
               CALL cl_err()
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL anmi280_b_fill(g_wc2)
               #資料多語言用-增/改
               
               #add-point:input段-after_insert name="input.body.a_insert2"
               
               #end add-point
               CALL s_transaction_end('Y','0')
               ##ERROR 'INSERT O.K'
               LET g_detail_cnt = g_detail_cnt + 1
               
               LET g_wc2 = g_wc2, " OR (nmav001 = '", g_nmav_d[l_ac].nmav001, "' "
                                  ," AND nmav002 = '", g_nmav_d[l_ac].nmav002, "' "
                                  ," AND nmav003 = '", g_nmav_d[l_ac].nmav003, "' "
 
                                  ,")"
            END IF                
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' THEN
               LET l_cmd='d'
            ELSE
               #add-point:單身刪除ask前 name="input.body.b_delete_ask"
               
               #end add-point   
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "" 
                  LET g_errparam.code   = -263 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  CANCEL DELETE
               END IF
               
               #add-point:單身刪除前 name="input.body.b_delete"
               
               #end add-point   
               
               DELETE FROM nmav_t
                WHERE nmavent = g_enterprise AND 
                      nmav001 = g_nmav_d_t.nmav001
                      AND nmav002 = g_nmav_d_t.nmav002
                      AND nmav003 = g_nmav_d_t.nmav003
 
                      
               #add-point:單身刪除中 name="input.body.m_delete"
               
               #end add-point  
                      
               IF SQLCA.sqlcode THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "nmav_t:",SQLERRMESSAGE 
