{<section id="anmi280.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION anmi280_b_fill(p_wc2)              #BODY FILL UP
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   
   #end add-point
   DEFINE p_wc2    STRING
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   
   #end add-point
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前 name="b_fill.sql_before"
   
   #end add-point
 
   LET g_sql = "SELECT  DISTINCT t0.nmav001,t0.nmav002,t0.nmav003,t0.nmav004,t0.nmav005,t0.nmavstus, 
       t0.nmav001,t0.nmav002,t0.nmav003,t0.nmavownid,t0.nmavowndp,t0.nmavcrtid,t0.nmavcrtdp,t0.nmavcrtdt, 
       t0.nmavmodid,t0.nmavmoddt ,t1.nmanl003 ,t2.ooag011 ,t3.ooefl003 ,t4.ooag011 ,t5.ooefl003 ,t6.ooag011 FROM nmav_t t0", 
 
               "",
                              " LEFT JOIN nmanl_t t1 ON t1.nmanlent="||g_enterprise||" AND t1.nmanl001=t0.nmav001 AND t1.nmanl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t2 ON t2.ooagent="||g_enterprise||" AND t2.ooag001=t0.nmavownid  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent="||g_enterprise||" AND t3.ooefl001=t0.nmavowndp AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t4 ON t4.ooagent="||g_enterprise||" AND t4.ooag001=t0.nmavcrtid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent="||g_enterprise||" AND t5.ooefl001=t0.nmavcrtdp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent="||g_enterprise||" AND t6.ooag001=t0.nmavmodid  ",
 
               " WHERE t0.nmavent= ?  AND  1=1 AND (", p_wc2, ") " 
   #add-point:b_fill段sql wc name="b_fill.sql_wc"
   
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("nmav_t"),
                      " ORDER BY t0.nmav001,t0.nmav002,t0.nmav003"
   
   #add-point:b_fill段sql之後 name="b_fill.sql_after"
   
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"nmav_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE anmi280_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR anmi280_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_nmav_d.clear()
   CALL g_nmav2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_nmav_d[l_ac].nmav001,g_nmav_d[l_ac].nmav002,g_nmav_d[l_ac].nmav003,g_nmav_d[l_ac].nmav004, 
       g_nmav_d[l_ac].nmav005,g_nmav_d[l_ac].nmavstus,g_nmav2_d[l_ac].nmav001,g_nmav2_d[l_ac].nmav002, 
       g_nmav2_d[l_ac].nmav003,g_nmav2_d[l_ac].nmavownid,g_nmav2_d[l_ac].nmavowndp,g_nmav2_d[l_ac].nmavcrtid, 
       g_nmav2_d[l_ac].nmavcrtdp,g_nmav2_d[l_ac].nmavcrtdt,g_nmav2_d[l_ac].nmavmodid,g_nmav2_d[l_ac].nmavmoddt, 
       g_nmav_d[l_ac].nmav001_desc,g_nmav2_d[l_ac].nmavownid_desc,g_nmav2_d[l_ac].nmavowndp_desc,g_nmav2_d[l_ac].nmavcrtid_desc, 
       g_nmav2_d[l_ac].nmavcrtdp_desc,g_nmav2_d[l_ac].nmavmodid_desc
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充 name="b_fill.fill"
      
      #end add-point
      
      CALL anmi280_detail_show()      
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
 
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   LET g_error_show = 0
   
 
   
   CALL g_nmav_d.deleteElement(g_nmav_d.getLength())   
   CALL g_nmav2_d.deleteElement(g_nmav2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_nmav_d.getLength()
      LET g_nmav2_d[l_ac].nmav001 = g_nmav_d[l_ac].nmav001 
      LET g_nmav2_d[l_ac].nmav002 = g_nmav_d[l_ac].nmav002 
      LET g_nmav2_d[l_ac].nmav003 = g_nmav_d[l_ac].nmav003 
 
      #add-point:b_fill段key值相關欄位 name="b_fill.keys.fill"
      
      #end add-point
   END FOR
   
   IF g_cnt > g_nmav_d.getLength() THEN
      LET l_ac = g_nmav_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_nmav_d.getLength()
      LET g_nmav_d_mask_o[l_ac].* =  g_nmav_d[l_ac].*
      CALL anmi280_nmav_t_mask()
      LET g_nmav_d_mask_n[l_ac].* =  g_nmav_d[l_ac].*
   END FOR
   
   LET g_nmav2_d_mask_o.* =  g_nmav2_d.*
   FOR l_ac = 1 TO g_nmav2_d.getLength()
      LET g_nmav2_d_mask_o[l_ac].* =  g_nmav2_d[l_ac].*
      CALL anmi280_nmav_t_mask()
      LET g_nmav2_d_mask_n[l_ac].* =  g_nmav2_d[l_ac].*
   END FOR
 
   
   LET l_ac = g_cnt
 
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_nmav_d.getLength()
   DISPLAY g_detail_idx TO FORMONLY.idx
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE anmi280_pb
   
END FUNCTION
 
