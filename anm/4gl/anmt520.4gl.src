{<section id="anmt520.set_no_entry" >}
#+ 單頭欄位關閉設定
PRIVATE FUNCTION anmt520_set_no_entry(p_cmd)
   #add-point:set_no_entry段define(客製用) name="set_no_entry.define_customerization"
   
   #end add-point     
   DEFINE p_cmd   LIKE type_t.chr1   
   #add-point:set_no_entry段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="set_no_entry.define"
   DEFINE l_success  LIKE type_t.num5     #151130-00015#2 
   DEFINE l_slip     LIKE type_t.chr10  #151130-00015#2
   DEFINE l_dfin0033  LIKE type_t.chr80 #151130-00015#2
   DEFINE l_para_date LIKE type_t.dat   #150813-00015#12 add
   #end add-point     
   
   #add-point:Function前置處理  name="set_no_entry.pre_function"
   
   #end add-point
   
   IF p_cmd = 'u' AND g_chkey = 'N' THEN
      CALL cl_set_comp_entry("nmcqcomp,nmcqdocno",FALSE)
      #根據azzi850使用者身分開關特定欄位
      IF NOT cl_null(g_no_entry) THEN
         CALL cl_set_comp_entry(g_no_entry,FALSE)
      END IF
      #add-point:set_no_entry段欄位控制 name="set_no_entry.field_control"
     
      #end add-point 
   END IF 
   
   IF p_cmd = 'u' THEN  #docno,ld欄位確認是絕對關閉
      CALL cl_set_comp_entry("nmcqdocno",FALSE)
   END IF 
 
#  IF p_cmd = 'u' THEN  #docdt欄位依照設定關閉(FALSE則為設定不同意修正) #(ver:78)
      IF NOT cl_chk_update_docdt() THEN
         CALL cl_set_comp_entry("nmcqdocdt",FALSE)
      END IF
#  END IF 
   
   #add-point:set_no_entry段欄位控制後 name="set_no_entry.after_control"
   #150707-00001#7--(s)
   IF p_cmd = 'u' THEN
      #150803-00018#1 take out nmcq100
      CALL cl_set_comp_entry("nmcqsite,nmcq001",FALSE)
   END IF 
   #150707-00001#7--(e)
   #151130-00015#2 -begin -add by XZG 20151218
#   IF NOT cl_null(g_nmcq_m.nmcqdocno) THEN   #150813-00015#12 mark
   IF NOT cl_null(g_nmcq_m.nmcqdocno) AND p_cmd = 'u' THEN    #150813-00015#12 add
      #获取单别
      CALL s_aooi200_fin_get_slip(g_nmcq_m.nmcqdocno) RETURNING l_success,l_slip
      #是否可改日期
      CALL s_fin_get_doc_para(g_glaald,g_nmcq_m.nmcqcomp,l_slip,'D-FIN-0033') RETURNING l_dfin0033
      #抓取关帐日期
      CALL cl_get_para(g_enterprise,g_nmcq_m.nmcqcomp,'S-FIN-4007') RETURNING l_para_date #150813-00015#12 add
#      IF l_dfin0033 = "N"  THEN  #150813-00015#12 mark
      IF l_dfin0033 = "N" AND g_nmcq_m.nmcqdocdt <= l_para_date THEN  #150813-00015#12 add
         CALL cl_set_comp_entry("nmcqdocdt",FALSE)
      ELSE 
         CALL cl_set_comp_entry("nmcqdocdt",TRUE)
      END IF          
   END IF 
   #151130-00015#2  -end 
   #end add-point 
 
END FUNCTION
 
