       g_nmcq_m.nmcqcrtid_desc,g_nmcq_m.nmcqcrtdp_desc,g_nmcq_m.nmcqmodid_desc,g_nmcq_m.nmcqcnfid_desc, 
       g_nmcq_m.nmcq005_desc
   
   
   #檢查是否允許此動作
   IF NOT anmt520_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_nmcq_m_mask_o.* =  g_nmcq_m.*
   CALL anmt520_nmcq_t_mask()
   LET g_nmcq_m_mask_n.* =  g_nmcq_m.*
   
   CALL anmt520_show()
   
   #add-point:delete段before ask name="delete.before_ask"
   #150813-00015#12--add--str--
   IF g_nmcq_m.nmcqstus <> 'N' THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'agl-00313'
      LET g_errparam.extend = 'modify'
      LET g_errparam.popup = TRUE
      CALL cl_err()
      CLOSE anmt520_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF 
   
   #檢查當單據日期小於等於關帳日期時，不可異動單據
   CALL s_fin_date_close_chk('',g_nmcq_m.nmcqcomp,'ANM',g_nmcq_m.nmcqdocdt) RETURNING l_success
   IF l_success = FALSE THEN
      CLOSE anmt520_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   #150813-00015#12--add--end
   #end add-point 
 
   IF cl_ask_del_master() THEN              #確認一下
   
      #add-point:單頭刪除前 name="delete.head.b_delete"
      
      #end add-point   
      
      #應用 a47 樣板自動產生(Version:4)
