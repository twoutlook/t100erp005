       g_nmcq_m.nmcqcrtdt,g_nmcq_m.nmcqmodid,g_nmcq_m.nmcqmoddt,g_nmcq_m.nmcqcnfid,g_nmcq_m.nmcqcnfdt, 
       g_nmcq_m.nmcq100,g_nmcq_m.nmcq101,g_nmcq_m.nmcq005,g_nmcq_m.nmcq004,g_nmcq_m.nmcq006,g_nmcq_m.nmcqsite_desc, 
