      CALL anmt520_set_pk_array()
      #add-point:相關文件刪除前 name="delete.befroe.related_document_remove"
      #160122-00001#23 By 07900--add---str
      LET l_n = 0 
      #單身存在當前用戶沒有權限的交易帳戶編碼
      SELECT COUNT(*) INTO l_n FROM nmcq_t
       WHERE nmcqent = g_enterprise AND nmcqdocno = g_nmcq_m.nmcqdocno
         AND nmcq003 NOT IN( SELECT UNIQUE nmll001 FROM nmll_t WHERE nmllent = g_enterprise AND nmll002 = g_user AND nmllstus='Y')
         AND nmcq003 NOT IN( SELECT UNIQUE nmlm001 FROM nmlm_t WHERE nmlment = g_enterprise AND nmlm002 = g_dept AND nmlmstus='Y')
         AND TRIM(nmcq003) IS NOT NULL
      IF l_n > 0 THEN
         IF NOT cl_ask_confirm('anm-00395') THEN
            CLOSE anmt520_cl
            CALL s_transaction_end('N','0')
            RETURN
         END IF
      END IF
      
      #160122-00001#23 By 07900--add---end
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
  
  
      #資料備份
      LET g_nmcqcomp_t = g_nmcq_m.nmcqcomp
      LET g_nmcqdocno_t = g_nmcq_m.nmcqdocno
 
 
      DELETE FROM nmcq_t
       WHERE nmcqent = g_enterprise AND nmcqcomp = g_nmcq_m.nmcqcomp
         AND nmcqdocno = g_nmcq_m.nmcqdocno
 
       
      #add-point:單頭刪除中 name="delete.head.m_delete"
      IF NOT s_aooi200_fin_del_docno(g_glaald,g_nmcq_m.nmcqdocno,g_nmcq_m.nmcqdocdt) THEN
         CALL s_transaction_end('N','0')
