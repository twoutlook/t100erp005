   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("anm","")
 
   #add-point:作業初始化 name="main.init"
   #161104-00046#14-----s
   #建立與單頭array相同的temptable
   CALL s_aooi200def_create('','g_nmcq_m','','','','','','')RETURNING g_sub_success
   
   #單別控制組
   LET g_user_dept_wc = ''
   CALL s_fin_get_user_dept_control('nmcqcomp','','nmcqent','nmcqdocno') RETURNING g_user_dept_wc
   #開窗使用
   LET g_user_dept_wc_q = g_user_dept_wc
   
   CALL s_control_get_docno_sql(g_user,g_dept) RETURNING g_sub_success,g_user_slip_wc  
   #161104-00046#14-----e
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define name="main.define_sql"
   
   #end add-point
   LET g_forupd_sql = " SELECT nmcqsite,'',nmcqcomp,'',nmcq002,'',nmcqdocno,nmcq001,nmcqdocdt,nmcq003, 
       '','',nmcq007,nmcqstus,nmcqownid,'',nmcqowndp,'',nmcqcrtid,'',nmcqcrtdp,'',nmcqcrtdt,nmcqmodid, 
       '',nmcqmoddt,nmcqcnfid,'',nmcqcnfdt,nmcq100,nmcq101,nmcq005,'',nmcq004,nmcq006", 
                      " FROM nmcq_t",
                      " WHERE nmcqent= ? AND nmcqcomp=? AND nmcqdocno=? FOR UPDATE"
   #add-point:SQL_define name="main.after_define_sql"
   
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE anmt520_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.nmcqsite,t0.nmcqcomp,t0.nmcq002,t0.nmcqdocno,t0.nmcq001,t0.nmcqdocdt, 
       t0.nmcq003,t0.nmcq007,t0.nmcqstus,t0.nmcqownid,t0.nmcqowndp,t0.nmcqcrtid,t0.nmcqcrtdp,t0.nmcqcrtdt, 
       t0.nmcqmodid,t0.nmcqmoddt,t0.nmcqcnfid,t0.nmcqcnfdt,t0.nmcq100,t0.nmcq101,t0.nmcq005,t0.nmcq004, 
       t0.nmcq006,t1.ooefl003 ,t2.ooefl003 ,t3.ooag011 ,t4.nmaal003 ,t5.ooag011 ,t6.ooefl003 ,t7.ooag011 , 
       t8.ooefl003 ,t9.ooag011 ,t10.ooag011 ,t11.pmaal004",
               " FROM nmcq_t t0",
                              " LEFT JOIN ooefl_t t1 ON t1.ooeflent="||g_enterprise||" AND t1.ooefl001=t0.nmcqsite AND t1.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent="||g_enterprise||" AND t2.ooefl001=t0.nmcqcomp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent="||g_enterprise||" AND t3.ooag001=t0.nmcq002  ",
               " LEFT JOIN nmaal_t t4 ON t4.nmaalent="||g_enterprise||" AND t4.nmaal001=t0.nmcq003 AND t4.nmaal002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=t0.nmcqownid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent="||g_enterprise||" AND t6.ooefl001=t0.nmcqowndp AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent="||g_enterprise||" AND t7.ooag001=t0.nmcqcrtid  ",
               " LEFT JOIN ooefl_t t8 ON t8.ooeflent="||g_enterprise||" AND t8.ooefl001=t0.nmcqcrtdp AND t8.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t9 ON t9.ooagent="||g_enterprise||" AND t9.ooag001=t0.nmcqmodid  ",
               " LEFT JOIN ooag_t t10 ON t10.ooagent="||g_enterprise||" AND t10.ooag001=t0.nmcqcnfid  ",
               " LEFT JOIN pmaal_t t11 ON t11.pmaalent="||g_enterprise||" AND t11.pmaal001=t0.nmcq005 AND t11.pmaal002='"||g_dlang||"' ",
 
