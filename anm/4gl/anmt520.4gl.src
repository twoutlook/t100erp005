################################################################################
# Descriptions...: 取消審核檢查
# Memo...........:
# Usage..........: CALL anmt520_unconf_chk()
#                  RETURNING r_success
# Return code....: r_success      執行結果
# Modify.........:
################################################################################
PRIVATE FUNCTION anmt520_unconf_chk()
   DEFINE r_success          LIKE type_t.num5
   DEFINE l_i                LIKE type_t.num5
   DEFINE l_nmbb042          LIKE nmbb_t.nmbb042
   DEFINE l_cnt              LIKE type_t.num5   #150707-00001#7
   
   LET r_success=TRUE
   
   #150707-00001#7--(s)
   LET l_cnt = 0
   SELECT COUNT(nmbt002) INTO l_cnt
     FROM nmbs_t,nmbt_t
    WHERE nmbsent = nmbtent
      AND nmbtld = nmbtld
      AND nmbsdocno = nmbtdocno
      AND nmbsent = g_enterprise
      AND nmbt002 = g_nmcq_m.nmcqdocno
      AND nmbsstus <> 'X'   
   IF l_cnt > 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'anm-00299'
      LET g_errparam.extend = g_nmcq_m.nmcqdocno
      LET g_errparam.popup = TRUE
      CALL cl_err() 
      LET r_success=FALSE      
      RETURN r_success
   END IF      
   #150707-00001#7--(e)
   FOR l_i=1 TO g_nmcr_d.getLength()
      SELECT nmbb042 INTO l_nmbb042 FROM nmbb_t
       WHERE nmbbent=g_enterprise AND nmbbdocno=g_nmcr_d[l_i].nmcr003
         AND nmbb030=g_nmcr_d[l_i].nmcr001
      IF l_nmbb042<>g_nmcq_m.nmcq001 THEN
         LET r_success=FALSE
         #CALL cl_errmsg("nmcr001",g_nmcr_d[l_i].nmcr001,"","agl-00183",1)
         INITIALIZE g_errparam TO NULL
        #LET g_errparam.code = "agl-00183"
         LET g_errparam.code = "anm-00366"
         LET g_errparam.extend = g_nmcr_d[l_i].nmcr001
         LET g_errparam.popup = TRUE
         CALL cl_err() 
      END IF
   END FOR
   RETURN r_success
END FUNCTION

