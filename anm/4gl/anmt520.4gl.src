################################################################################
# Descriptions...: 重新推算貼現息與銀行撥入
# Memo...........:
# Usage..........: CALL anmt520_recalc()
# Date & Author..: 15/07/20 By apo(#150707-00001#7)
# Modify.........:
################################################################################
PRIVATE FUNCTION anmt520_recalc()
   LET g_nmcr2_d[l_ac].nmcr105=(g_nmcr_d[l_ac].nmcr103+g_nmcr_d[l_ac].nmcr104) * g_nmcr2_d[l_ac].nmcr006/100 * g_nmcr2_d[l_ac].nmcr007/360
   CALL s_curr_round_ld('1',g_glaald,g_nmcq_m.nmcq100,g_nmcr2_d[l_ac].nmcr105,2)  
    RETURNING g_sub_success,g_errno,g_nmcr2_d[l_ac].nmcr105                       
   LET g_nmcr2_d[l_ac].nmcr115=g_nmcr2_d[l_ac].nmcr105 * g_nmcq_m.nmcq101
   CALL s_curr_round_ld('1',g_glaald,g_glaa001,g_nmcr2_d[l_ac].nmcr115,2)         
    RETURNING g_sub_success,g_errno,g_nmcr2_d[l_ac].nmcr115                       
   LET g_nmcr2_d[l_ac].nmcr106=g_nmcr_d[l_ac].nmcr103+g_nmcr_d[l_ac].nmcr104-g_nmcr2_d[l_ac].nmcr105 - g_nmcr_d[l_ac].nmcr107
   CALL s_curr_round_ld('1',g_glaald,g_nmcq_m.nmcq100,g_nmcr2_d[l_ac].nmcr106,2)  
    RETURNING g_sub_success,g_errno,g_nmcr2_d[l_ac].nmcr106                       
   LET g_nmcr2_d[l_ac].nmcr116=g_nmcr2_d[l_ac].nmcr106 * g_nmcq_m.nmcq101
   CALL s_curr_round_ld('1',g_glaald,g_glaa001,g_nmcr2_d[l_ac].nmcr116,2)         
    RETURNING g_sub_success,g_errno,g_nmcr2_d[l_ac].nmcr116        
   DISPLAY BY NAME g_nmcr2_d[l_ac].nmcr105,g_nmcr2_d[l_ac].nmcr115,g_nmcr2_d[l_ac].nmcr106,g_nmcr2_d[l_ac].nmcr116
END FUNCTION

################################################################################
# Descriptions...: 匯率變動重計
# Memo...........:
# Usage..........: CALL anmt520_recalc_rate()
# Date & Author..: 15/07/21 By apo(#150707-00001#7)
# Modify.........:
################################################################################
PRIVATE FUNCTION anmt520_recalc_rate()
DEFINE l_i     LIKE type_t.num5
DEFINE l_ooam003      LIKE ooam_t.ooam003
DEFINE l_nmbb066      LIKE nmbb_t.nmbb066
DEFINE l_nmbb067      LIKE nmbb_t.nmbb067
DEFINE l_nmbb068      LIKE nmbb_t.nmbb068

   IF g_nmcr_d.getLength() > 0 AND (g_nmcq_m.nmcq101 != g_nmcq_m_t.nmcq101 OR g_nmcq_m_t.nmcq101 IS NULL) THEN
      IF cl_ask_confirm('anm-02935') THEN
         FOR l_i = 1 TO g_nmcr_d.getLength()
            LET g_nmcr_d[l_i].nmcr113=g_nmcr_d[l_i].nmcr103*g_nmcq_m.nmcq101
            CALL s_curr_round_ld('1',g_glaald,g_glaa001,g_nmcr_d[l_i].nmcr113,2)
                 RETURNING g_sub_success,g_errno,g_nmcr_d[l_i].nmcr113
            LET g_nmcr_d[l_i].nmcr114=g_nmcr_d[l_i].nmcr104*g_nmcq_m.nmcq101
            CALL s_curr_round_ld('1',g_glaald,g_glaa001,g_nmcr_d[l_i].nmcr114,2)
                 RETURNING g_sub_success,g_errno,g_nmcr_d[l_i].nmcr114
            LET g_nmcr2_d[l_i].nmcr115=g_nmcr2_d[l_i].nmcr105*g_nmcq_m.nmcq101
            CALL s_curr_round_ld('1',g_glaald,g_glaa001,g_nmcr2_d[l_i].nmcr115,2)
                 RETURNING g_sub_success,g_errno,g_nmcr2_d[l_i].nmcr115
            LET g_nmcr2_d[l_i].nmcr116=g_nmcr2_d[l_i].nmcr106*g_nmcq_m.nmcq101
            CALL s_curr_round_ld('1',g_glaald,g_glaa001,g_nmcr2_d[l_i].nmcr116,2)
                 RETURNING g_sub_success,g_errno,g_nmcr2_d[l_i].nmcr116
            LET g_nmcr_d[l_i].nmcr117=g_nmcr_d[l_i].nmcr107*g_nmcq_m.nmcq101
            CALL s_curr_round_ld('1',g_glaald,g_glaa001,g_nmcr_d[l_i].nmcr117,2)
                 RETURNING g_sub_success,g_errno,g_nmcr_d[l_i].nmcr117
            
            SELECT nmbb066,nmbb067,nmbb068
              INTO l_nmbb066,l_nmbb067,l_nmbb068
              FROM nmbb_t
              LEFT OUTER JOIN nmba_t ON nmbaent = nmbbent AND nmbacomp = nmbbcomp AND nmbadocno = nmbbdocno 
             WHERE nmbaent=g_enterprise
              #AND nmba003='anmt510'                  #150922-00021#4 mark
               AND nmbb030=g_nmcr_d[l_i].nmcr001
               AND nmbbdocno = g_nmcr_d[l_i].nmcr003 #150922-00021#4
               AND (nmbb069 = 'N' OR nmbb069 IS NULL)
            IF cl_null(l_nmbb066) THEN LET l_nmbb066=0 END IF
            IF cl_null(l_nmbb067) THEN LET l_nmbb067=0 END IF
            IF cl_null(l_nmbb068) THEN LET l_nmbb068=0 END IF
            #本幣匯差
            LET g_nmcr_d[l_i].nmcr118=g_nmcr_d[l_i].nmcr113-l_nmbb066
            
            #本位幣二
            IF g_glaa015 = 'Y' THEN
               #依據換匯基準推算
               LET l_ooam003 = ''
               IF g_glaa017 = '1' THEN
                  LET l_ooam003 = g_nmcq_m.nmcq100
               ELSE
                  LET l_ooam003 = g_glaa001
               END IF
                                        #     帳套;       日期;           來源幣別
               CALL s_aooi160_get_exrate('2',g_glaald,g_nmcq_m.nmcqdocdt,l_ooam003,
                                         #目的幣別; 交易金額; 匯類類型
                                         g_glaa016,0,g_glaa018)
                    RETURNING g_nmcr_d[l_i].nmcr121
              #CALL s_curr_round_ld('1',g_glaald,g_glaa016,g_nmcr_d[l_i].nmcr121,3) #160822-00018#1 mark
               CALL s_curr_round_ld('1',g_glaald,l_ooam003,g_nmcr_d[l_i].nmcr121,3) #160822-00018#1
                    RETURNING g_sub_success,g_errno,g_nmcr_d[l_i].nmcr121
               IF g_glaa017 = '1' THEN #原幣
                  LET g_nmcr_d[l_i].nmcr122 = g_nmcr_d[l_i].nmcr103 * g_nmcr_d[l_i].nmcr121-l_nmbb067
               ELSE #本幣
                  LET g_nmcr_d[l_i].nmcr122 = g_nmcr_d[l_i].nmcr113 * g_nmcr_d[l_i].nmcr121-l_nmbb067
               END IF
               CALL s_curr_round_ld('1',g_glaald,g_glaa016,g_nmcr_d[l_i].nmcr122,2)
                    RETURNING g_sub_success,g_errno,g_nmcr_d[l_i].nmcr122
            END IF
            
            #本位幣三
            IF g_glaa019 = 'Y' THEN
               #依據換匯基準推算
               LET l_ooam003 = ''
               IF g_glaa021 = '1' THEN
                  LET l_ooam003 = g_nmcq_m.nmcq100
               ELSE
                  LET l_ooam003 = g_glaa001
               END IF
                                        #     帳套;       日期;           來源幣別
               CALL s_aooi160_get_exrate('2',g_glaald,g_nmcq_m.nmcqdocdt,l_ooam003,
                                         #目的幣別; 交易金額; 匯率類型
                                         g_glaa020,0,g_glaa022)
                    RETURNING g_nmcr_d[l_i].nmcr131
              #CALL s_curr_round_ld('1',g_glaald,g_glaa020,g_nmcr_d[l_i].nmcr131,3) #160822-00018#1 mark
               CALL s_curr_round_ld('1',g_glaald,l_ooam003,g_nmcr_d[l_i].nmcr131,3) #160822-00018#1
                    RETURNING g_sub_success,g_errno,g_nmcr_d[l_i].nmcr131
               IF g_glaa021 = '1' THEN
                  LET g_nmcr_d[l_i].nmcr132 = g_nmcr_d[l_i].nmcr103 * g_nmcr_d[l_i].nmcr131-l_nmbb068
               ELSE
                  LET g_nmcr_d[l_i].nmcr132 = g_nmcr_d[l_i].nmcr113 * g_nmcr_d[l_i].nmcr131-l_nmbb068
               END IF
               CALL s_curr_round_ld('1',g_glaald,g_glaa020,g_nmcr_d[l_i].nmcr132,2)
                    RETURNING g_sub_success,g_errno,g_nmcr_d[l_i].nmcr132
            END IF
            UPDATE nmcr_t SET (nmcr113,nmcr114,nmcr115,nmcr116,nmcr117,
                               nmcr118,nmcr121,nmcr122,nmcr131,nmcr132
                              ) =
            (g_nmcr_d[l_i].nmcr113,g_nmcr_d[l_i].nmcr114,g_nmcr2_d[l_i].nmcr115,g_nmcr2_d[l_i].nmcr116,g_nmcr_d[l_i].nmcr117,
             g_nmcr_d[l_i].nmcr118,g_nmcr_d[l_i].nmcr121,g_nmcr_d[l_i].nmcr122,g_nmcr_d[l_i].nmcr131,g_nmcr_d[l_i].nmcr132)
             WHERE nmcrent = g_enterprise
               AND nmcrcomp = g_nmcq_m.nmcqcomp
               AND nmcrdocno = g_nmcq_m.nmcqdocno
               AND nmcrseq = g_nmcr_d[l_i].nmcrseq
            DISPLAY BY NAME g_nmcr_d[l_i].nmcr113,g_nmcr_d[l_i].nmcr114,g_nmcr2_d[l_i].nmcr115,g_nmcr2_d[l_i].nmcr116,g_nmcr_d[l_i].nmcr117,
                            g_nmcr_d[l_i].nmcr118,g_nmcr_d[l_i].nmcr121,g_nmcr_d[l_i].nmcr122,g_nmcr_d[l_i].nmcr131,g_nmcr_d[l_i].nmcr132
         END FOR
      END IF
   END IF
END FUNCTION

################################################################################
# Descriptions...: 编辑后立即审核
# Memo...........:
# Usage..........: CALL anmt520_immediately_conf() 
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 2015/12/08 By 07166
# Modify.........:
################################################################################
PRIVATE FUNCTION anmt520_immediately_conf()
#151125-00006#3--s
   DEFINE l_success         LIKE type_t.num5
   DEFINE l_doc_success     LIKE type_t.num5
   DEFINE l_slip            LIKE ooba_t.ooba002
   DEFINE l_dfin0031        LIKE type_t.chr1
   DEFINE l_count           LIKE type_t.num5
   IF cl_null(g_glaald)           THEN RETURN END IF   #無資料直接返回不做處理
   IF cl_null(g_nmcq_m.nmcqcomp)  THEN RETURN END IF   #無資料直接返回不做處理
   IF cl_null(g_nmcq_m.nmcqdocno) THEN RETURN END IF   #無資料直接返回不做處理
    
   LET l_count = 0
   SELECT COUNT(*) INTO l_count FROM nmcr_t WHERE nmcrent = g_enterprise
      AND nmcrdocno = g_nmcq_m.nmcqdocno
      
   IF cl_null(l_count) THEN LET l_count = 0 END IF
   IF l_count = 0 THEN
      RETURN 
   END IF                   #单身無資料直接返回不做處理
   
   #取得單別
   CALL s_aooi200_fin_get_slip(g_nmcq_m.nmcqdocno) RETURNING l_success,l_slip
   #取得單別設置的"是否直接審核"參數
   CALL s_fin_get_doc_para(g_glaald,g_nmcq_m.nmcqcomp,l_slip,'D-FIN-0031') RETURNING l_dfin0031

   IF cl_null(l_dfin0031) OR l_dfin0031 MATCHES '[Nn]' THEN RETURN END IF #參數值為空或為N則不做直接審核邏輯
   IF NOT cl_ask_confirm('aap-00403') THEN RETURN END IF  #询问是否立即审核?
   
   
   CALL s_transaction_begin()
   CALL cl_err_collect_init()
   LET l_doc_success = TRUE
        
   IF NOT s_anmt520_conf_chk(g_nmcq_m.nmcqcomp,g_nmcq_m.nmcqdocno) THEN
      LET l_doc_success = FALSE
   END IF
   
   IF NOT s_anmt520_conf_upd(g_nmcq_m.nmcqcomp,g_nmcq_m.nmcqdocno) THEN
      LET l_doc_success = FALSE
   END IF 
   
   #異動狀態碼欄位/修改人/修改日期
   LET g_nmcq_m.nmcqmoddt = cl_get_current()
   LET g_nmcq_m.nmcqcnfdt = cl_get_current()
   UPDATE nmcq_t SET nmcqstus = 'Y',
                     nmcqmodid= g_user,
                     nmcqmoddt= g_nmcq_m.nmcqmoddt,
                     nmcqcnfid= g_user,
                     nmcqcnfdt= g_nmcq_m.nmcqcnfdt
    WHERE nmcqent = g_enterprise AND nmcqcomp = g_nmcq_m.nmcqcomp
      AND nmcqdocno = g_nmcq_m.nmcqdocno
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      LET l_doc_success = FALSE
   END IF
   IF l_doc_success = TRUE THEN
      CALL s_transaction_end('Y',1)
   ELSE
      CALL s_transaction_end('N',1)
      CALL cl_err_collect_show()
   END IF
#151125-00006#3--e
END FUNCTION

################################################################################
# Descriptions...: 检查是否有异动 
# Memo...........:
# Usage..........: CALL anmt520_glbc_chk(p_glbcdocno,p_glbcld,p_glbc001,p_glbc002,p_glbcseq)
# Date & Author..: 2016/12/30 By 01531
# Modify.........:
################################################################################
PRIVATE FUNCTION anmt520_glbc_chk(p_glbcdocno,p_glbcld,p_glbc001,p_glbc002,p_glbcseq)
   DEFINE r_flag      LIKE type_t.num5
   DEFINE l_glbc009   LIKE glbc_t.glbc009
   DEFINE l_glbc012   LIKE glbc_t.glbc012
   DEFINE l_glbc014   LIKE glbc_t.glbc014
   DEFINE p_glbcdocno LIKE glbc_t.glbcdocno
   DEFINE p_glbcld    LIKE glbc_t.glbcld
   DEFINE p_glbc001   LIKE glbc_t.glbc001
   DEFINE p_glbc002   LIKE glbc_t.glbc002
   DEFINE p_glbcseq   LIKE glbc_t.glbcseq
   DEFINE l_nmck103   LIKE nmck_t.nmck103
   DEFINE l_nmck113   LIKE nmck_t.nmck113
   DEFINE l_nmck123   LIKE nmck_t.nmck123
   DEFINE l_nmck133   LIKE nmck_t.nmck133
   DEFINE l_nmcr106   LIKE nmcr_t.nmcr106
   DEFINE l_nmcr116   LIKE nmcr_t.nmcr116
   DEFINE l_nmcr103   LIKE nmcr_t.nmcr103
   DEFINE l_nmcr113   LIKE nmcr_t.nmcr113
   DEFINE l_nmcr121   LIKE nmcr_t.nmcr121
   DEFINE l_nmcr131   LIKE nmcr_t.nmcr131
   DEFINE l_nmcq001   LIKE nmcq_t.nmcq001
      LET r_flag = TRUE
      CALL anmt520_glaa()
      SELECT SUM(glbc009),SUM(glbc012),SUM(glbc014)
        INTO l_glbc009,l_glbc012,l_glbc014
        FROM glbc_t
       WHERE glbcent=g_enterprise AND glbcld=p_glbcld
         AND glbcdocno=p_glbcdocno AND glbc001=p_glbc001
         AND glbc002=p_glbc002
         AND glbcseq = p_glbcseq
      IF cl_null(l_glbc009) THEN LET l_glbc009 = 0 END IF         
      IF cl_null(l_glbc012) THEN LET l_glbc012 = 0 END IF      
      IF cl_null(l_glbc014) THEN LET l_glbc014 = 0 END IF             
      
      SELECT nmcr106,nmcr116,nmcr103,nmcr113,nmcr121,nmcr131
        INTO l_nmcr106,l_nmcr116,l_nmcr103,l_nmcr113,l_nmcr121,l_nmcr131
        FROM nmcr_t
       WHERE nmcrent = g_enterprise
         AND nmcrcomp = g_nmcq_m.nmcqcomp
         AND nmcrdocno = p_glbcdocno
         AND nmcrseq = p_glbcseq
         
      SELECT nmcq001 INTO l_nmcq001 FROM nmcq_t
       WHERE nmcqent=g_enterprise AND nmcqcomp=g_nmcq_m.nmcqcomp AND nmcqdocno=p_glbcdocno
      IF l_nmcq001='4' THEN
         LET l_nmck113=l_nmcr116
