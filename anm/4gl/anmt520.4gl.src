#+抓取支票相關資料,對一些欄位賦預設值
PRIVATE FUNCTION anmt520_nmbb030()
   DEFINE l_nmbb065   LIKE nmbb_t.nmbb065
   DEFINE l_nmbb045   LIKE nmbb_t.nmbb045
   
#   SELECT DISTINCT nmbb042,nmbbdocno,nmbb026,pmaal004,nmbb031,nmbb006,nmbb065,nmbb045
#     INTO g_nmcr_d[l_ac].nmcr002,g_nmcr_d[l_ac].nmcr003,g_nmcr_d[l_ac].nmbb026,g_nmcr_d[l_ac].nmbb026_desc,
#          g_nmcr_d[l_ac].nmbb031,g_nmcr_d[l_ac].nmcr103,l_nmbb065,l_nmbb045
#     FROM nmba_t,nmbb_t LEFT OUTER JOIN pmaal_t ON nmbbent=pmaalent  AND nmbb026 = pmaal001 AND pmaal002=g_lang
#    WHERE nmbaent=nmbbent AND nmbacomp=nmbbcomp AND nmbadocno=nmbbdocno 
#      AND nmbbent=g_enterprise AND nmbacomp=g_nmcq_m.nmcqcomp
#      AND nmba003='anmt510' AND nmbb030=g_nmcr_d[l_ac].nmcr001

   SELECT DISTINCT nmbb042,nmbb026,pmaal004,nmbb031,nmbb006,nmbb065,nmbb045,nmbadocno
     INTO g_nmcr_d[l_ac].nmcr002,g_nmcr_d[l_ac].nmbb026,g_nmcr_d[l_ac].nmbb026_desc,
          g_nmcr_d[l_ac].nmbb031,g_nmcr_d[l_ac].nmcr103,l_nmbb065,l_nmbb045,g_nmcr_d[l_ac].nmcr003
     FROM nmba_t,nmbb_t LEFT OUTER JOIN pmaal_t ON nmbbent=pmaalent  AND nmbb026 = pmaal001 AND pmaal002=g_lang
    WHERE nmbaent=nmbbent AND nmbacomp=nmbbcomp AND nmbadocno=nmbbdocno
      AND nmbbent=g_enterprise AND nmbacomp=g_nmcq_m.nmcqcomp
      #AND nmba003='anmt510' AND nmbb030=g_nmcr_d[l_ac].nmcr001          #2015/01/30 mark by qiull
      AND nmbb029='30' AND nmbb030=g_nmcr_d[l_ac].nmcr001                #2015/01/30 add by qiull
      AND (nmbb069 = 'N' OR nmbb069 IS NULL)                             #2015/01/16 add by qiull
      
   LET g_nmcr_d[l_ac].nmcr113=g_nmcr_d[l_ac].nmcr103*g_nmcq_m.nmcq101
   CALL s_curr_round_ld('1',g_glaald,g_glaa001,g_nmcr_d[l_ac].nmcr113,2)   #150707-00001#7
        RETURNING g_sub_success,g_errno,g_nmcr_d[l_ac].nmcr113             #150707-00001#7
   
   DISPLAY BY NAME g_nmcr_d[l_ac].nmcr002,g_nmcr_d[l_ac].nmcr003,g_nmcr_d[l_ac].nmbb026,g_nmcr_d[l_ac].nmbb026_desc,
                   g_nmcr_d[l_ac].nmcr103,g_nmcr_d[l_ac].nmcr113
                   
   DISPLAY g_nmcr_d[l_ac].nmbb031 TO s_detail1[l_ac].nmbb031_2
   IF g_nmcq_m.nmcq001 MATCHES '[48]' THEN
      LET g_nmcr_d[l_ac].nmcr004=g_nmcr_d[l_ac].nmbb031-l_nmbb065+1
      LET g_nmcr_d[l_ac].nmcr104=g_nmcr_d[l_ac].nmcr103*l_nmbb045/100*g_nmcr_d[l_ac].nmcr004/360
      CALL s_curr_round_ld('1',g_glaald,g_nmcq_m.nmcq100,g_nmcr_d[l_ac].nmcr104,2)   #150707-00001#7
           RETURNING g_sub_success,g_errno,g_nmcr_d[l_ac].nmcr104                    #150707-00001#7
      LET g_nmcr_d[l_ac].nmcr114=g_nmcr_d[l_ac].nmcr104*g_nmcq_m.nmcq101
      CALL s_curr_round_ld('1',g_glaald,g_glaa001,g_nmcr_d[l_ac].nmcr114,2)   #150707-00001#7
           RETURNING g_sub_success,g_errno,g_nmcr_d[l_ac].nmcr114             #150707-00001#7      
      DISPLAY BY NAME g_nmcr_d[l_ac].nmcr004,g_nmcr_d[l_ac].nmcr104,g_nmcr_d[l_ac].nmcr114
   END IF
END FUNCTION

#+根據單頭“異動類別”，甚至抓取的支票票況條件
PRIVATE FUNCTION anmt520_set_nmbb042()
   DEFINE l_nmbb042   LIKE nmbb_t.nmbb042
   #2托收、4票貼、5轉付
   IF g_nmcq_m.nmcq001 MATCHES '[245]' THEN
      LET l_nmbb042='1'
   END IF
   
   IF g_nmcq_m.nmcq001 MATCHES '[678]' THEN
      LET l_nmbb042='2'
   END IF
   
   IF g_nmcq_m.nmcq001 MATCHES '[9]' THEN
      LET l_nmbb042='5'
   END IF
   
   RETURN l_nmbb042
END FUNCTION

#+檢查支票編號
PRIVATE FUNCTION anmt520_nmcr001_chk()
DEFINE l_nmbb042   LIKE nmbb_t.nmbb042
DEFINE l_nmbastus  LIKE nmba_t.nmbastus
DEFINE l_nmbb004   LIKE nmbb_t.nmbb004
DEFINE l_n         LIKE type_t.num5
DEFINE l_nmcq003   LIKE nmcq_t.nmcq003
DEFINE l_nmbb031   LIKE nmbb_t.nmbb031
DEFINE l_nmba003   LIKE nmba_t.nmba003   #150922-00021#4
   
   LET g_errno=""
   
   #150922-00021#4 add nmba003
   SELECT nmbb042,nmbastus,nmbb004,nmbb031,nmba003
     INTO l_nmbb042,l_nmbastus,l_nmbb004,l_nmbb031,l_nmba003
     FROM nmba_t,nmbb_t
    WHERE nmbaent=nmbbent AND nmbacomp=nmbbcomp AND nmbadocno=nmbbdocno 
      AND nmbbent=g_enterprise AND nmbacomp=g_nmcq_m.nmcqcomp 
      #AND nmba003='anmt510' AND nmbb030=g_nmcr_d[l_ac].nmcr001           #2015/01/30 mark by qiull
      AND nmbb029='30' AND nmbb030=g_nmcr_d[l_ac].nmcr001                 #2015/01/30 add by qiull
      AND (nmbb069 = 'N' OR nmbb069 IS NULL)                              #2015/01/16 add by qiull
   CASE
      WHEN SQLCA.sqlcode=100  LET g_errno='anm-00172'
     #WHEN l_nmbastus NOT MATCHES '[YV]'    LET g_errno='anm-00176' #150922-00021#4 mark
#      WHEN l_nmbastus NOT MATCHES '[Y]' AND l_nmba003 = 'anmt510'  LET g_errno='anm-00176' #150922-00021#4 #160913-00032#1 mark
      #WHEN l_nmbastus NOT MATCHES '[YV]' AND l_nmba003 = 'anmt510'  LET g_errno='anm-00176' #160913-00032#1 add #161026-00010#1 mark
      WHEN l_nmbastus NOT MATCHES '[V]' AND l_nmba003 = 'anmt510'  LET g_errno='anm-00176'   #161026-00010#1 add
     #WHEN l_nmbastus NOT MATCHES '[V]' AND l_nmba003 = 'anmt540'  LET g_errno='anm-02955'          #151021 by 03538 mark  #150922-00021#4
      WHEN l_nmbastus NOT MATCHES '[V]' AND l_nmba003 MATCHES 'anmt54[01]'  LET g_errno='anm-02955' #151021 by 03538      
      WHEN l_nmbb004<>g_nmcq_m.nmcq100      LET g_errno='anm-00177'
      WHEN g_nmcq_m.nmcq001 MATCHES '[245]' AND l_nmbb042<>'1' LET g_errno='anm-00173'
      WHEN g_nmcq_m.nmcq001 MATCHES '[678]' AND l_nmbb042<>'2' LET g_errno='anm-00174'
      WHEN g_nmcq_m.nmcq001='9' AND l_nmbb042<>'5' LET g_errno='anm-00175'
   END CASE
   IF cl_null(g_errno) THEN
      SELECT COUNT(*) INTO l_n 
        FROM nmcq_t LEFT JOIN nmcr_t ON nmcrent = nmcqent AND nmcqcomp = nmcrcomp AND nmcqdocno = nmcrdocno 
       WHERE nmcqent=g_enterprise AND nmcqcomp=g_nmcq_m.nmcqcomp AND nmcqdocno=g_nmcq_m.nmcqdocno
         AND nmcq001=g_nmcq_m.nmcq001 AND nmcr001=g_nmcr_d[l_ac].nmcr001
         AND nmcr003=g_nmcr_d[l_ac].nmcr003 AND nmcrseq<>g_nmcr_d[l_ac].nmcrseq
      IF l_n>0 THEN
         LET g_errno='anm-00182'
         LET g_nmcr_d[l_ac].nmcr003=NULL
      END IF
   END IF
   
   #檢查托收与兑现的交易帐户要一致
   IF g_nmcq_m.nmcq001 = '8' THEN
      IF cl_null(g_errno) THEN
         SELECT nmcq003 INTO l_nmcq003
           FROM nmcq_t,nmcr_t
          WHERE nmcqent = nmcrent AND nmcqcomp = nmcrcomp AND nmcqdocno = nmcrdocno
            AND nmcqent = g_enterprise
            AND nmcqstus = 'Y'
            AND nmcq001 = '2'
            AND nmcr001 = g_nmcr_d[l_ac].nmcr001
         IF NOT cl_null(l_nmcq003) THEN
            IF l_nmcq003 <> g_nmcq_m.nmcq003 THEN
               LET g_errno='anm-00300'
            END IF
         END IF
      END IF
   END IF
   
   IF g_nmcq_m.nmcq001 = '8' THEN  #应收票据兌現
      IF cl_null(g_errno) THEN
         IF g_nmcq_m.nmcqdocdt < l_nmbb031 THEN
            IF NOT cl_ask_confirm("anm-00303") THEN
               LET g_errno='anm-00303'
            END IF
         END IF
      END IF
   END IF
END FUNCTION

#+獲取本位幣二三匯率和匯差
PRIVATE FUNCTION anmt520_get_exrate()
   DEFINE l_ooam003      LIKE ooam_t.ooam003
   DEFINE l_nmbb066      LIKE nmbb_t.nmbb066
   DEFINE l_nmbb067      LIKE nmbb_t.nmbb067
   DEFINE l_nmbb068      LIKE nmbb_t.nmbb068
   
   SELECT nmbb066,nmbb067,nmbb068
     INTO l_nmbb066,l_nmbb067,l_nmbb068
     FROM nmbb_t
     LEFT OUTER JOIN nmba_t ON nmbaent = nmbbent AND nmbacomp = nmbbcomp AND nmbadocno = nmbbdocno
    WHERE nmbaent=g_enterprise
     #AND nmba003='anmt510'                  #150807-00007#1 mark
      AND nmbb030=g_nmcr_d[l_ac].nmcr001     #150707-00001#7 remark  #2015/01/30 mark by qiull
      AND nmbbdocno = g_nmcr_d[l_ac].nmcr003 #150807-00007#1
      #WHERE nmbaent=g_enterprise AND nmbb029='30' AND nmbb030=g_nmcr_d[l_ac].nmcr001 #150707-00001#7 mark #2015/01/30 add by qiull
      AND (nmbb069 = 'N' OR nmbb069 IS NULL)          #2015/01/16 add by qiull
   IF cl_null(l_nmbb066) THEN LET l_nmbb066=0 END IF
   IF cl_null(l_nmbb067) THEN LET l_nmbb067=0 END IF
   IF cl_null(l_nmbb068) THEN LET l_nmbb068=0 END IF
   #本幣匯差
   LET g_nmcr_d[l_ac].nmcr118=g_nmcr_d[l_ac].nmcr113-l_nmbb066
   #本位幣二
   IF g_glaa015 = 'Y' THEN
      #來源幣別
      LET l_ooam003 = ''
      IF g_glaa017 = '1' THEN
         LET l_ooam003 = g_nmcq_m.nmcq100
      ELSE   #表示帳簿幣別
         LET l_ooam003 = g_glaa001           #帳套使用幣別
      END IF
                               #     帳套;       日期;           來源幣別
      CALL s_aooi160_get_exrate('2',g_glaald,g_nmcq_m.nmcqdocdt,l_ooam003,
                                #目的幣別; 交易金額; 匯類類型
                                g_glaa016,0,g_glaa018)
           RETURNING g_nmcr_d[l_ac].nmcr121
     #CALL s_curr_round_ld('1',g_glaald,g_glaa016,g_nmcr_d[l_ac].nmcr121,3) #150707-00001#7 #160822-00018#1 mark
      CALL s_curr_round_ld('1',g_glaald,l_ooam003,g_nmcr_d[l_ac].nmcr121,3)                 #160822-00018#1
           RETURNING g_sub_success,g_errno,g_nmcr_d[l_ac].nmcr121           #150707-00001#7
      IF g_glaa017 = '1' THEN #原幣
         LET g_nmcr_d[l_ac].nmcr122 = g_nmcr_d[l_ac].nmcr103 * g_nmcr_d[l_ac].nmcr121-l_nmbb067
      ELSE #本幣
         LET g_nmcr_d[l_ac].nmcr122 = g_nmcr_d[l_ac].nmcr113 * g_nmcr_d[l_ac].nmcr121-l_nmbb067
      END IF
      CALL s_curr_round_ld('1',g_glaald,g_glaa016,g_nmcr_d[l_ac].nmcr122,2) #150707-00001#7
           RETURNING g_sub_success,g_errno,g_nmcr_d[l_ac].nmcr122           #150707-00001#7
   END IF
   
   #本位幣三
   IF g_glaa019 = 'Y' THEN
      #來源幣別
      LET l_ooam003 = ''
      IF g_glaa021 = '1' THEN
         LET l_ooam003 = g_nmcq_m.nmcq100
      ELSE   #表示帳簿幣別
         LET l_ooam003 = g_glaa001           #帳套使用幣別
      END IF
                               #     帳套;       日期;           來源幣別
      CALL s_aooi160_get_exrate('2',g_glaald,g_nmcq_m.nmcqdocdt,l_ooam003,
                                #目的幣別; 交易金額; 匯類類型
                                g_glaa020,0,g_glaa022)
           RETURNING g_nmcr_d[l_ac].nmcr131
     #CALL s_curr_round_ld('1',g_glaald,g_glaa020,g_nmcr_d[l_ac].nmcr131,3) #150707-00001#7 #160822-00018#1 mark
      CALL s_curr_round_ld('1',g_glaald,l_ooam003,g_nmcr_d[l_ac].nmcr131,3)                 #160822-00018#1
           RETURNING g_sub_success,g_errno,g_nmcr_d[l_ac].nmcr131           #150707-00001#7
      IF g_glaa021 = '1' THEN
         LET g_nmcr_d[l_ac].nmcr132 = g_nmcr_d[l_ac].nmcr103 * g_nmcr_d[l_ac].nmcr131-l_nmbb068
      ELSE
         LET g_nmcr_d[l_ac].nmcr132 = g_nmcr_d[l_ac].nmcr113 * g_nmcr_d[l_ac].nmcr131-l_nmbb068
      END IF
      CALL s_curr_round_ld('1',g_glaald,g_glaa020,g_nmcr_d[l_ac].nmcr132,2) #150707-00001#7
           RETURNING g_sub_success,g_errno,g_nmcr_d[l_ac].nmcr132           #150707-00001#7
   END IF
END FUNCTION

################################################################################
# Descriptions...: 取消審核，還原支票票況等資料
# Memo...........:
# Usage..........: CALL anmt520_unconf_upd()
################################################################################
PRIVATE FUNCTION anmt520_unconf_upd()
   DEFINE l_sql        STRING
   DEFINE l_success    LIKE type_t.num5
   DEFINE l_nmbb042    LIKE nmbb_t.nmbb042
   DEFINE l_nmcr001    LIKE nmcr_t.nmcr001
   DEFINE l_nmcr003    LIKE nmcr_t.nmcr003
   
   LET l_sql="SELECT nmcr001,nmcr003 FROM nmcr_t ",
             " WHERE nmcrent=",g_enterprise," AND nmcrcomp='",g_nmcq_m.nmcqcomp,"'",
             "   AND nmcrdocno='",g_nmcq_m.nmcqdocno,"'"
   PREPARE anmt520_unconf_pr FROM l_sql
   DECLARE anmt520_unconf_cs CURSOR FOR anmt520_unconf_pr

   LET l_success=TRUE
   CASE 
      WHEN g_nmcq_m.nmcq001 MATCHES '[245]' LET l_nmbb042='1'
      WHEN g_nmcq_m.nmcq001 MATCHES '[678]' LET l_nmbb042='2'
      WHEN g_nmcq_m.nmcq001='9' LET l_nmbb042='5'
   END CASE
   FOREACH anmt520_unconf_cs INTO l_nmcr001,l_nmcr003
      IF SQLCA.SQLCODE THEN
         #CALL cl_errmsg('FOREACH anmt520_unconf_c','','',SQLCA.SQLCODE,1)
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.SQLCODE
         LET g_errparam.extend = "FOREACH anmt520_unconf_c"
         LET g_errparam.popup = TRUE
         CALL cl_err()
         LET l_success = FALSE
         EXIT FOREACH
      END IF
      UPDATE nmbb_t SET nmbb042=l_nmbb042
       WHERE nmbbent=g_enterprise AND nmbbcomp=g_nmcq_m.nmcqcomp 
         AND nmbb030=l_nmcr001 AND nmbbdocno=l_nmcr003
      IF SQLCA.sqlcode THEN
         #CALL cl_errmsg("nmbb_t","","",SQLCA.sqlcode,1)  
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "nmbb_t"
         LET g_errparam.popup = TRUE
         CALL cl_err()
         LET l_success = FALSE
      END IF
      #還原票貼資料
      IF g_nmcq_m.nmcq001='4' THEN
         UPDATE nmbb_t SET nmbb047 = NULL,nmbb048 = NULL,nmbb049 = NULL,
                           nmbb050 = NULL,nmbb051 = NULL,nmbb052 = NULL
          WHERE nmbbent=g_enterprise AND nmbbcomp=g_nmcq_m.nmcqcomp 
            AND nmbb030=l_nmcr001 AND nmbbdocno=l_nmcr003
         IF SQLCA.sqlcode THEN
            #CALL cl_errmsg("nmbb_t","","",SQLCA.sqlcode,1) 
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "nmbb_t"
            LET g_errparam.popup = TRUE
            CALL cl_err()            
            LET l_success = FALSE
         END IF 
      END IF
      #轉付時還原轉付對象資料
      IF g_nmcq_m.nmcq001='5' THEN
         UPDATE nmbb_t SET nmbb046 = NULL
          WHERE nmbbent=g_enterprise AND nmbbcomp=g_nmcq_m.nmcqcomp 
            AND nmbb030=l_nmcr001 AND nmbbdocno=l_nmcr003
         IF SQLCA.sqlcode THEN
            #CALL cl_errmsg("nmbb_t","","",SQLCA.sqlcode,1)  
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "nmbb_t"
            LET g_errparam.popup = TRUE
            CALL cl_err() 
            LET l_success = FALSE
         END IF 
      END IF
      #托收時還原交易對象
     #IF g_nmcq_m.nmcq001='2' THEN              #150707-00001#3 mark
      IF g_nmcq_m.nmcq001 MATCHES '[24]' THEN   #150707-00001#3
         UPDATE nmbb_t SET nmbb003 = NULL
          WHERE nmbbent=g_enterprise AND nmbbcomp=g_nmcq_m.nmcqcomp 
            AND nmbb030=l_nmcr001 AND nmbbdocno=l_nmcr003
         IF SQLCA.sqlcode THEN
            #CALL cl_errmsg("nmbb_t","","",SQLCA.sqlcode,1)  
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "nmbb_t"
            LET g_errparam.popup = TRUE
            CALL cl_err() 
            LET l_success = FALSE
         END IF 
      END IF
   END FOREACH
   #刪除銀存異動資料nmbc_t
   IF g_nmcq_m.nmcq001 MATCHES '[48]' THEN
      DELETE FROM nmbc_t 
       WHERE nmbcent = g_enterprise
         AND nmbcdocno = g_nmcq_m.nmcqdocno
         AND nmbccomp = g_nmcq_m.nmcqcomp
   
      IF SQLCA.SQLcode  THEN
         #CALL cl_errmsg("del nmbc_t","","",SQLCA.sqlcode,1)  
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "del nmbc_t"
         LET g_errparam.popup = TRUE
         CALL cl_err() 
         LET l_success = FALSE                        
      END IF
   END IF
   RETURN l_success
END FUNCTION

################################################################################
# Descriptions...: 審核檢查，檢查現金變動碼設置是否正確
# Memo...........:
# Usage..........: CALL anmt520_conf_chk()
################################################################################
PRIVATE FUNCTION anmt520_conf_chk()
   DEFINE l_sql        STRING
   DEFINE l_success    LIKE type_t.num5
   DEFINE l_i,l_cnt    LIKE type_t.num5
   DEFINE l_amt        LIKE glbc_t.glbc009
   DEFINE l_amt1       LIKE glbc_t.glbc009
   DEFINE l_amt2       LIKE glbc_t.glbc009
   DEFINE l_amt3       LIKE glbc_t.glbc009
   DEFINE l_sum1       LIKE glbc_t.glbc009
   DEFINE l_sum2       LIKE glbc_t.glbc009
   DEFINE l_sum3       LIKE glbc_t.glbc009
   DEFINE l_glbcseq1   LIKE glbc_t.glbcseq1
   DEFINE l_glbc004    LIKE glbc_t.glbc004
   DEFINE l_nmaistus   LIKE nmai_t.nmaistus
   DEFINE l_nmbb042    LIKE nmbb_t.nmbb042
   DEFINE l_nmbb042_o  LIKE nmbb_t.nmbb042
   DEFINE l_efin5001   LIKE type_t.chr1
   DEFINE l_ooba002    LIKE ooba_t.ooba002
   DEFINE l_glaald     LIKE glaa_t.glaald
   
   LET l_success=TRUE
   #单身无数据，不可审核
   IF g_nmcr_d.getLength() < 1 THEN 
      LET l_success=FALSE
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = "aec-00020"
      LET g_errparam.extend = g_nmcq_m.nmcqdocno
      LET g_errparam.popup = TRUE
      CALL cl_err()                
      RETURN l_success
   END IF
   
   #150602-00057#2 by 02291 add
   SELECT glaald INTO l_glaald FROM glaa_t
   WHERE glaaent = g_enterprise AND glaacomp = g_nmcq_m.nmcqcomp
     AND glaa014 = 'Y'
   CALL s_aooi200_fin_get_slip(g_nmcq_m.nmcqdocno) RETURNING l_success,l_ooba002
   CALL s_fin_chk_E5001(l_glaald,g_nmcq_m.nmcqcomp,l_ooba002) RETURNING l_efin5001
   IF l_efin5001 = 'N' THEN
      IF g_nmcq_m.nmcqcrtid = g_user THEN
         LET l_success=FALSE
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'axr-00346'
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()
         CALL s_transaction_end('N','0')
         RETURN l_success
      END IF
   END IF   
   #150602-00057#2 by 02291 add
   
   #檢查支票當前狀態是否做相應的操作
   CALL anmt520_set_nmbb042() RETURNING l_nmbb042

   FOR l_i=1 TO g_nmcr_d.getLength()
      SELECT nmbb042 INTO l_nmbb042_o FROM nmbb_t
       WHERE nmbbent=g_enterprise AND nmbbdocno=g_nmcr_d[l_i].nmcr003
         AND nmbb030=g_nmcr_d[l_i].nmcr001
      IF l_nmbb042_o<> l_nmbb042 THEN
         LET l_success=FALSE
         CASE
            WHEN g_nmcq_m.nmcq001 MATCHES '[245]'
               #CALL cl_errmsg("nmcqdocno",g_nmcq_m.nmcqdocno,"","anm-00173",1) 
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = "anm-00173"
               LET g_errparam.extend = g_nmcq_m.nmcqdocno
               LET g_errparam.popup = TRUE
               CALL cl_err()                
            WHEN g_nmcq_m.nmcq001 MATCHES '[678]'
               #CALL cl_errmsg("nmcqdocno",g_nmcq_m.nmcqdocno,"","anm-00174",1) 
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = "anm-00174"
               LET g_errparam.extend = g_nmcq_m.nmcqdocno
               LET g_errparam.popup = TRUE
               CALL cl_err()                
            WHEN g_nmcq_m.nmcq001='9'
               #CALL cl_errmsg("nmcqdocno",g_nmcq_m.nmcqdocno,"","anm-00175",1)
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = "anm-00175"
               LET g_errparam.extend = g_nmcq_m.nmcqdocno
               LET g_errparam.popup = TRUE
               CALL cl_err() 
         END CASE
      END IF
   END FOR
   #當異動類別為4：票貼、8：兌現時，檢查現金變動碼設置的是否正確
   IF g_nmcq_m.nmcq001 MATCHES '[48]' THEN
      FOR l_i=1 TO g_nmcr_d.getLength()
         SELECT COUNT(*) INTO l_cnt FROM glbc_t
          WHERE glbcent=g_enterprise AND glbcld=g_glaald AND glbcdocno=g_nmcq_m.nmcqdocno
            AND glbcseq=g_nmcr_d[l_i].nmcrseq
         IF l_cnt=0 THEN
            LET l_success = FALSE
            #CALL cl_errmsg('nmcqdocno',g_nmcq_m.nmcqdocno,'','agl-00221',1)
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = 'agl-00221'
            LET g_errparam.extend = g_nmcq_m.nmcqdocno
            LET g_errparam.popup = TRUE
            CALL cl_err() 
         ELSE
            SELECT SUM(glbc009),SUM(glbc012),SUM(glbc014)
              INTO l_sum1,l_sum2,l_sum3
              FROM glbc_t
             WHERE glbcent=g_enterprise AND glbcld=g_glaald
               AND glbcdocno=g_nmcq_m.nmcqdocno AND glbcseq=g_nmcr_d[l_i].nmcrseq
            IF cl_null(l_sum1) THEN LET l_sum1=0 END IF
            IF cl_null(l_sum2) THEN LET l_sum2=0 END IF
            IF cl_null(l_sum3) THEN LET l_sum3=0 END IF
            
            IF g_nmcq_m.nmcq001='4' THEN #托收
               LET l_amt  = g_nmcr2_d[l_i].nmcr106
               LET l_amt1 = g_nmcr2_d[l_i].nmcr116
            ELSE #兌現
               LET l_amt  = g_nmcr_d[l_i].nmcr103
               LET l_amt1 = g_nmcr_d[l_i].nmcr113
            END IF
            
            IF l_amt1<>l_sum1 THEN
               LET l_success = FALSE
               #CALL cl_errmsg('nmcqdocno',g_nmcq_m.nmcqdocno,'','anm-00146',1)
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 'anm-00146'
               LET g_errparam.extend = g_nmcq_m.nmcqdocno
               LET g_errparam.popup = TRUE
               CALL cl_err() 
            END IF
            IF g_glaa015='Y' THEN
               IF g_glaa017 = '1' THEN #原幣
                  LET l_amt2 = l_amt * g_nmcr_d[l_i].nmcr121
               ELSE #本幣
                  LET l_amt2 = l_amt1 * g_nmcr_d[l_i].nmcr121
               END IF

               IF l_amt2<>l_sum2 THEN
                  LET l_success = FALSE
                  #CALL cl_errmsg('nmcqdocno',g_nmcq_m.nmcqdocno,'','anm-00147',1)
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = 'anm-00147'
                  LET g_errparam.extend = g_nmcq_m.nmcqdocno
                  LET g_errparam.popup = TRUE
                  CALL cl_err() 
               END IF
            END IF
            IF g_glaa019='Y' THEN
               IF g_glaa021='1' THEN
                  LET l_amt3 = l_amt * g_nmcr_d[l_i].nmcr131
               ELSE
                  LET l_amt3 = l_amt1 * g_nmcr_d[l_i].nmcr131
               END IF
              
               IF l_amt3<>l_sum3 THEN
                  LET l_success = FALSE
                  #CALL cl_errmsg('nmcqdocno',g_nmcq_m.nmcqdocno,'','anm-00148',1)
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = 'anm-00148'
                  LET g_errparam.extend = g_nmcq_m.nmcqdocno
                  LET g_errparam.popup = TRUE
                  CALL cl_err() 
               END IF
            END IF
            #判斷現金變動碼設置是否正確
            LET l_sql = " SELECT glbcseq1,glbc004 FROM glbc_t ",
                        "  WHERE glbcent = ",g_enterprise,
                        "    AND glbcld = '",g_glaald,"'",
                        "    AND glbcdocno = '",g_nmcq_m.nmcqdocno,"'",
                        "    AND glbcseq=",g_nmcr_d[l_i].nmcrseq
            PREPARE anmt520_glbc_pre  FROM l_sql
            DECLARE anmt520_glbc_cs CURSOR FOR anmt520_glbc_pre
            FOREACH anmt520_glbc_cs INTO l_glbcseq1,l_glbc004
               IF SQLCA.SQLCODE THEN
                  #CALL cl_errmsg('','','',sqlca.sqlcode,0)
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = sqlca.sqlcode
                  LET g_errparam.extend = ''
                  LET g_errparam.popup = TRUE
                  CALL cl_err() 
                  LET l_success = FALSE
                  EXIT FOREACH
               END IF
               IF cl_null(l_glbc004) THEN 
                  LET l_success = FALSE
                  #CALL cl_errmsg('nmcqdocno',g_nmcq_m.nmcqdocno,'','anm-00091',1)
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = 'anm-00091'
                  LET g_errparam.extend = g_nmcq_m.nmcqdocno
                  LET g_errparam.popup = TRUE
                  CALL cl_err() 
               ELSE
                  SELECT nmaistus INTO l_nmaistus FROM nmai_t 
                   WHERE nmaient=g_enterprise AND nmai001=g_glaa005
                     AND nmai002=l_glbc004 
                  IF SQLCA.sqlcode THEN
                     LET l_success = FALSE
                     #CALL cl_errmsg('glbc004',l_glbc004,'',SQLCA.sqlcode,1)
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code = SQLCA.sqlcode
                     LET g_errparam.extend = l_glbc004
                     LET g_errparam.popup = TRUE
                     CALL cl_err() 
                  END IF
                  IF l_nmaistus<>'Y' THEN
                     LET l_success = FALSE
                     #CALL cl_errmsg('glbc004',l_glbc004,'','abg-00002',1)
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code = 'sub-01302' #abg-00002  #160318-00005#28  By 07900 --mod  
                     LET g_errparam.extend = l_glbc004
                     #160318-00005#28 by 07900 --add--str
                     LET g_errparam.replace[1] = 'anmi160'
                     LET g_errparam.replace[2] = cl_get_progname("anmi160",g_lang,"2")
                     LET g_errparam.exeprog ='anmi160'
                     #160318-00005#28 by 07900 --add--end
                     LET g_errparam.popup = TRUE
                     CALL cl_err() 
                  END IF 
               END IF
            END FOREACH
         END IF
      END FOR
   END IF
   RETURN l_success
END FUNCTION

################################################################################
# Descriptions...: 更新支票票況，票貼等資料
# Memo...........:
# Usage..........: CALL anmt520_conf_upd()
################################################################################
PRIVATE FUNCTION anmt520_conf_upd()
   DEFINE l_sql        STRING
   DEFINE l_success    LIKE type_t.num5
   DEFINE l_nmbb042    LIKE nmbb_t.nmbb042
   DEFINE l_nmcrseq    LIKE nmcr_t.nmcrseq
   DEFINE l_nmcr001    LIKE nmcr_t.nmcr001
   DEFINE l_nmcr003    LIKE nmcr_t.nmcr003
   DEFINE l_nmcr004    LIKE nmcr_t.nmcr004
   DEFINE l_nmcr005    LIKE nmcr_t.nmcr005
   DEFINE l_nmcr006    LIKE nmcr_t.nmcr006
   DEFINE l_nmcr007    LIKE nmcr_t.nmcr007
   DEFINE l_nmcr106    LIKE nmcr_t.nmcr106
   DEFINE l_nmcr116    LIKE nmcr_t.nmcr116
   
   LET l_sql="SELECT nmcrseq,nmcr001,nmcr003,nmcr004,nmcr005,nmcr006,nmcr007,nmcr106,nmcr116 FROM nmcr_t ",
             " WHERE nmcrent=",g_enterprise," AND nmcrcomp='",g_nmcq_m.nmcqcomp,"'",
             "   AND nmcrdocno='",g_nmcq_m.nmcqdocno,"'"
   PREPARE anmt520_conf_upd_pr FROM l_sql
   DECLARE anmt520_conf_upd_cs CURSOR FOR anmt520_conf_upd_pr

   LET l_success=TRUE
   FOREACH anmt520_conf_upd_cs INTO l_nmcrseq,l_nmcr001,l_nmcr003,l_nmcr004,l_nmcr005,l_nmcr006,
                                    l_nmcr007,l_nmcr106,l_nmcr116
      IF SQLCA.SQLCODE THEN
         #CALL cl_errmsg('FOREACH anmt520_conf_upd_cs','','',SQLCA.SQLCODE,1)
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.SQLCODE
         LET g_errparam.extend = 'FOREACH anmt520_conf_upd_cs'
         LET g_errparam.popup = TRUE
         CALL cl_err() 
         LET l_success = FALSE
         EXIT FOREACH
      END IF
      UPDATE nmbb_t SET nmbb042=g_nmcq_m.nmcq001
       WHERE nmbbent=g_enterprise AND nmbbcomp=g_nmcq_m.nmcqcomp 
         AND nmbbdocno=l_nmcr003 AND nmbb030=l_nmcr001
      IF SQLCA.sqlcode THEN
         #CALL cl_errmsg("nmbb_t","","",SQLCA.sqlcode,1)  
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.SQLCODE
         LET g_errparam.extend = 'nmbb_t'
         LET g_errparam.popup = TRUE
         CALL cl_err() 
         LET l_success = FALSE
      END IF
      #更新票貼資料
      IF g_nmcq_m.nmcq001='4' THEN
         UPDATE nmbb_t SET nmbb047 = l_nmcr004,
                           nmbb048 = l_nmcr005,
                           nmbb049 = l_nmcr006,
                           nmbb050 = l_nmcr007,
                           nmbb051 = l_nmcr106,
                           nmbb052 = l_nmcr116
          WHERE nmbbent=g_enterprise AND nmbbcomp=g_nmcq_m.nmcqcomp 
            AND nmbbdocno=l_nmcr003 AND nmbb030=l_nmcr001
         IF SQLCA.sqlcode THEN
            #CALL cl_errmsg("nmbb_t","","",SQLCA.sqlcode,1)  
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.SQLCODE
            LET g_errparam.extend = 'nmbb_t'
            LET g_errparam.popup = TRUE
            CALL cl_err() 
            LET l_success = FALSE
         END IF 
      END IF
      #轉付時更新轉付對象
      IF g_nmcq_m.nmcq001='5' THEN
         UPDATE nmbb_t SET nmbb046 = g_nmcq_m.nmcq005
          WHERE nmbbent=g_enterprise AND nmbbcomp=g_nmcq_m.nmcqcomp 
            AND nmbbdocno=l_nmcr003 AND nmbb030=l_nmcr001
         IF SQLCA.sqlcode THEN
            #CALL cl_errmsg("nmbb_t","","",SQLCA.sqlcode,1)  
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.SQLCODE
            LET g_errparam.extend = 'nmbb_t'
            LET g_errparam.popup = TRUE
            CALL cl_err() 
            LET l_success = FALSE
         END IF 
      END IF
      #托收時更新交易對象
     #IF g_nmcq_m.nmcq001='2' THEN              #150707-00001#3 mark
      IF g_nmcq_m.nmcq001 MATCHES '[24]' THEN   #150707-00001#3
         UPDATE nmbb_t SET nmbb003 = g_nmcq_m.nmcq003
          WHERE nmbbent=g_enterprise AND nmbbcomp=g_nmcq_m.nmcqcomp 
            AND nmbbdocno=l_nmcr003 AND nmbb030=l_nmcr001
         IF SQLCA.sqlcode THEN
            #CALL cl_errmsg("nmbb_t","","",SQLCA.sqlcode,1)  
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.SQLCODE
            LET g_errparam.extend = 'nmbb_t'
            LET g_errparam.popup = TRUE
            CALL cl_err() 
            LET l_success = FALSE
         END IF 
      END IF
      #插入銀存收支異動資料
      IF g_nmcq_m.nmcq001 MATCHES '[48]' THEN
         CALL anmt520_ins_nmbc(l_nmcrseq) RETURNING l_success
      END IF
   END FOREACH
   RETURN l_success
END FUNCTION

################################################################################
# Descriptions...: 票據異動帳務處理
# Memo...........:
# Usage..........: CALL anmt520_open_anmt530()
################################################################################
PRIVATE FUNCTION anmt520_open_anmt530()
   DEFINE l_nmcq008     LIKE nmcq_t.nmcq008
   DEFINE l_nmcq009     LIKE nmcq_t.nmcq009
   DEFINE la_param      RECORD
          prog          STRING,
          param         DYNAMIC ARRAY OF STRING
                        END RECORD
   DEFINE ls_js         STRING
   DEFINE l_n           LIKE type_t.num5
   DEFINE l_glaald      LIKE glaa_t.glaald
   DEFINE l_glaa008     LIKE glaa_t.glaa008
   DEFINE l_oobn002     LIKE oobn_t.oobn002
   DEFINE l_oobn003     LIKE oobn_t.oobn003
   DEFINE l_success     LIKE type_t.num5
   
   IF g_nmcq_m.nmcqstus<>'Y' THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = "anm-00196"
      LET g_errparam.extend = g_nmcq_m.nmcqdocno
      LET g_errparam.popup = FALSE
      CALL cl_err()

      RETURN
   END IF
   
   SELECT nmcq008,nmcq009 INTO l_nmcq008,l_nmcq009 FROM nmcq_t
   WHERE nmcqent=g_enterprise AND nmcqcomp=g_nmcq_m.nmcqcomp AND nmcqdocno=g_nmcq_m.nmcqdocno
   IF NOT cl_null(g_nmcq_m.nmcq007) OR NOT cl_null(l_nmcq008) OR NOT cl_null(l_nmcq009) THEN
      LET la_param.prog = 'anmt530'
      LET la_param.param[1] = g_glaald
      LET la_param.param[2] = g_nmcq_m.nmcq007         
      LET ls_js = util.JSON.stringify(la_param )
      CALL cl_cmdrun(ls_js)
   ELSE
      LET l_glaald=NULL
      #判斷法人是否開啟多帳套
      SELECT COUNT(*) INTO l_n FROM glaa_t 
      WHERE glaaent=g_enterprise AND glaacomp=g_nmcq_m.nmcqcomp AND glaa008='Y'
      IF l_n>1 THEN
         LET l_glaa008='Y'
      ELSE
         LET l_glaald=g_glaald
         LET l_glaa008='N'
      END IF
      #帳務單號
      #CALL s_aooi200_get_slip(g_nmcq_m.nmcqdocno) RETURNING l_success,l_oobn002       #2014/12/29 liuym mark
      CALL s_aooi200_fin_get_slip(g_nmcq_m.nmcqdocno) RETURNING l_success,l_oobn002    #2014/12/29 liuym add
      SELECT oobn003 INTO l_oobn003 FROM oobn_t
      WHERE oobnent=g_enterprise AND oobn001='anmt520' AND oobn002=l_oobn002
      #產生帳務資料
                     #法人          #票據異動 #平行記帳否 #帳別     #單別    #單號
#      CALL anmt530_01(g_nmcq_m.nmcqcomp,"Y", l_glaa008, l_glaald,l_oobn003,g_nmcq_m.nmcqdocno)   #150707-00001#4 mark
   END IF
END FUNCTION

################################################################################
# Descriptions...: 插入銀存異動資料
# Memo...........:
# Usage..........: CALL anmt520_ins_nmbc(p_nmcrseq)
#                  RETURNING l_success
# Input parameter: p_nmcrseq      項次
# Return code....: l_success      出入結果
# Modify.........:
################################################################################
PRIVATE FUNCTION anmt520_ins_nmbc(p_nmcrseq)
