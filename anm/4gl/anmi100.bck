#+ 單身陣列填充
PRIVATE FUNCTION anmi100_b_fill(p_wc2)              #BODY FILL UP
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   
   #end add-point
   DEFINE p_wc2    STRING
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   
   #end add-point
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前 name="b_fill.sql_before"
   
   #end add-point
 
   LET g_sql = "SELECT  DISTINCT t0.nmab008,t0.nmab001,t0.nmab003,t0.nmab009,t0.nmab002,t0.nmab004,t0.nmab005, 
       t0.nmab006,t0.nmab007,t0.nmab010,t0.nmabstus,t0.nmab001,t0.nmabmodid,t0.nmabmoddt,t0.nmabownid, 
       t0.nmabowndp,t0.nmabcrtid,t0.nmabcrtdp,t0.nmabcrtdt ,t1.oocgl003 ,t2.nmanl003 ,t3.ooag011 ,t4.ooag011 , 
       t5.ooefl003 ,t6.ooag011 ,t7.ooefl003 FROM nmab_t t0",
               " LEFT JOIN nmabl_t ON nmablent = "||g_enterprise||" AND nmab001 = nmabl001 AND nmabl002 = '",g_dlang,"'",
                              " LEFT JOIN oocgl_t t1 ON t1.oocglent="||g_enterprise||" AND t1.oocgl001=t0.nmab008 AND t1.oocgl002='"||g_dlang||"' ",
               " LEFT JOIN nmanl_t t2 ON t2.nmanlent="||g_enterprise||" AND t2.nmanl001=t0.nmab010 AND t2.nmanl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent="||g_enterprise||" AND t3.ooag001=t0.nmabmodid  ",
               " LEFT JOIN ooag_t t4 ON t4.ooagent="||g_enterprise||" AND t4.ooag001=t0.nmabownid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent="||g_enterprise||" AND t5.ooefl001=t0.nmabowndp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent="||g_enterprise||" AND t6.ooag001=t0.nmabcrtid  ",
               " LEFT JOIN ooefl_t t7 ON t7.ooeflent="||g_enterprise||" AND t7.ooefl001=t0.nmabcrtdp AND t7.ooefl002='"||g_dlang||"' ",
 
               " WHERE t0.nmabent= ?  AND  1=1 AND (", p_wc2, ") " 
   #add-point:b_fill段sql wc name="b_fill.sql_wc"
   
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("nmab_t"),
                      " ORDER BY t0.nmab001"
   
   #add-point:b_fill段sql之後 name="b_fill.sql_after"
   
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"nmab_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE anmi100_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR anmi100_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_nmab_d.clear()
   CALL g_nmab1_info_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_nmab_d[l_ac].nmab008,g_nmab_d[l_ac].nmab001,g_nmab_d[l_ac].nmab003,g_nmab_d[l_ac].nmab009, 
       g_nmab_d[l_ac].nmab002,g_nmab_d[l_ac].nmab004,g_nmab_d[l_ac].nmab005,g_nmab_d[l_ac].nmab006,g_nmab_d[l_ac].nmab007, 
       g_nmab_d[l_ac].nmab010,g_nmab_d[l_ac].nmabstus,g_nmab1_info_d[l_ac].nmab001,g_nmab1_info_d[l_ac].nmabmodid, 
       g_nmab1_info_d[l_ac].nmabmoddt,g_nmab1_info_d[l_ac].nmabownid,g_nmab1_info_d[l_ac].nmabowndp, 
       g_nmab1_info_d[l_ac].nmabcrtid,g_nmab1_info_d[l_ac].nmabcrtdp,g_nmab1_info_d[l_ac].nmabcrtdt, 
       g_nmab_d[l_ac].nmab008_desc,g_nmab_d[l_ac].nmab010_desc,g_nmab1_info_d[l_ac].nmabmodid_desc,g_nmab1_info_d[l_ac].nmabownid_desc, 
       g_nmab1_info_d[l_ac].nmabowndp_desc,g_nmab1_info_d[l_ac].nmabcrtid_desc,g_nmab1_info_d[l_ac].nmabcrtdp_desc 
 
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充 name="b_fill.fill"
      
      #end add-point
      
      CALL anmi100_detail_show()      
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
 
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   LET g_error_show = 0
   
 
   
   CALL g_nmab_d.deleteElement(g_nmab_d.getLength())   
   CALL g_nmab1_info_d.deleteElement(g_nmab1_info_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_nmab_d.getLength()
      LET g_nmab1_info_d[l_ac].nmab001 = g_nmab_d[l_ac].nmab001 
 
      #add-point:b_fill段key值相關欄位 name="b_fill.keys.fill"
      
      #end add-point
   END FOR
   
   IF g_cnt > g_nmab_d.getLength() THEN
      LET l_ac = g_nmab_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_nmab_d.getLength()
      LET g_nmab_d_mask_o[l_ac].* =  g_nmab_d[l_ac].*
      CALL anmi100_nmab_t_mask()
      LET g_nmab_d_mask_n[l_ac].* =  g_nmab_d[l_ac].*
   END FOR
   
   LET g_nmab1_info_d_mask_o.* =  g_nmab1_info_d.*
   FOR l_ac = 1 TO g_nmab1_info_d.getLength()
      LET g_nmab1_info_d_mask_o[l_ac].* =  g_nmab1_info_d[l_ac].*
      CALL anmi100_nmab_t_mask()
      LET g_nmab1_info_d_mask_n[l_ac].* =  g_nmab1_info_d[l_ac].*
   END FOR
 
   
   LET l_ac = g_cnt
 
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_nmab_d.getLength()
   DISPLAY g_detail_idx TO FORMONLY.idx
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE anmi100_pb
   
END FUNCTION
 
