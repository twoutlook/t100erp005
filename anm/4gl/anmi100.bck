#+ 資料修改
PRIVATE FUNCTION anmi100_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   
   #end add-point
   DEFINE  l_cmd                  LIKE type_t.chr1
   DEFINE  l_ac_t                 LIKE type_t.num10               #未取消的ARRAY CNT 
   DEFINE  l_n                    LIKE type_t.num10               #檢查重複用  
   DEFINE  l_cnt                  LIKE type_t.num10               #檢查重複用  
   DEFINE  l_lock_sw              LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert         LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete         LIKE type_t.num5                #可刪除否  
   DEFINE  l_count                LIKE type_t.num10
   DEFINE  l_i                    LIKE type_t.num10
   DEFINE  ls_return              STRING
   DEFINE  l_var_keys             DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys           DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                 DYNAMIC ARRAY OF STRING
   DEFINE  l_fields               DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak         DYNAMIC ARRAY OF STRING
   DEFINE  li_reproduce           LIKE type_t.num10
   DEFINE  li_reproduce_target    LIKE type_t.num10
   DEFINE  lb_reproduce           BOOLEAN
   DEFINE  l_insert               BOOLEAN
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   
   #end add-point 
   
   #add-point:Function前置處理  name="modify.pre_function"
   
   #end add-point
   
   LET g_action_choice = ""
   
   LET g_qryparam.state = "i"
 
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   
   #add-point:modify開始前 name="modify.define_sql"
   
   #end add-point
   
   LET INT_FLAG = FALSE
   LET lb_reproduce = FALSE
   LET l_insert = FALSE
 
   #關閉被遮罩相關欄位輸入, 無法確定USER是否會需要輸入此欄位
   #因此先行關閉, 若有需要可於下方add-point中自行開啟
   CALL cl_mask_set_no_entry()
 
   #add-point:modify段修改前 name="modify.before_input"
   
   #end add-point
 
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #Page1 預設值產生於此處
      INPUT ARRAY g_nmab_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_detail_cnt,WITHOUT DEFAULTS, #MAXCOUNT = g_max_rec,
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
 
         #應用 a43 樣板自動產生(Version:4)
         ON ACTION update_item
            LET g_action_choice="update_item"
            IF cl_auth_chk_act("update_item") THEN
               
               #add-point:ON ACTION update_item name="input.detail_input.page1.update_item"
               #150602-00010#1--(s)
               IF NOT cl_null(g_nmab_d[l_ac].nmab001) THEN
                  CALL n_nmabl(g_nmab_d[l_ac].nmab001)    # n_itlal:對應多語言表格
                  LET g_ref_fields[1] = g_nmab_d[l_ac].nmab001
                     CALL ap_ref_array2(g_ref_fields," SELECT nmabl003,nmabl004 FROM nmabl_t WHERE nmablent = '"
                        ||g_enterprise||"' AND nmabl001 = ? AND nmabl002 = '"||g_dlang||"'","") RETURNING g_rtn_fields
                  LET g_nmab_d[l_ac].nmabl003 = g_rtn_fields[1]
                  LET g_nmab_d[l_ac].nmabl004 = g_rtn_fields[2]
                  DISPLAY BY NAME g_nmab_d[l_ac].nmabl003,g_nmab_d[l_ac].nmabl004
               END IF   
               #150602-00010#1--(e)
               #END add-point
            END IF
 
 
 
 
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_nmab_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL anmi100_b_fill(g_wc2)
            LET g_detail_cnt = g_nmab_d.getLength()
         
         BEFORE ROW
            #add-point:modify段before row name="input.body.before_row2"
            
            #end add-point  
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_cmd = ''
            LET l_ac_t = l_ac 
            LET l_ac = g_detail_idx
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            DISPLAY g_nmab_d.getLength() TO FORMONLY.cnt
         
            CALL s_transaction_begin()
            LET g_detail_cnt = g_nmab_d.getLength()
            
            IF g_detail_cnt >= l_ac 
               AND g_nmab_d[l_ac].nmab001 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_nmab_d_t.* = g_nmab_d[l_ac].*  #BACKUP
               LET g_nmab_d_o.* = g_nmab_d[l_ac].*  #BACKUP
               IF NOT anmi100_lock_b("nmab_t") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH anmi100_bcl INTO g_nmab_d[l_ac].nmab008,g_nmab_d[l_ac].nmab001,g_nmab_d[l_ac].nmab003, 
                      g_nmab_d[l_ac].nmab009,g_nmab_d[l_ac].nmab002,g_nmab_d[l_ac].nmab004,g_nmab_d[l_ac].nmab005, 
                      g_nmab_d[l_ac].nmab006,g_nmab_d[l_ac].nmab007,g_nmab_d[l_ac].nmab010,g_nmab_d[l_ac].nmabstus, 
                      g_nmab1_info_d[l_ac].nmab001,g_nmab1_info_d[l_ac].nmabmodid,g_nmab1_info_d[l_ac].nmabmoddt, 
                      g_nmab1_info_d[l_ac].nmabownid,g_nmab1_info_d[l_ac].nmabowndp,g_nmab1_info_d[l_ac].nmabcrtid, 
                      g_nmab1_info_d[l_ac].nmabcrtdp,g_nmab1_info_d[l_ac].nmabcrtdt
