   LET g_forupd_sql = " SELECT nmbasite,'',nmba002,'',nmbacomp,'',nmbadocno,nmbadocdt,nmba013,nmba014, 
       nmba007,nmba003,nmbastus,nmba001,'',nmba004,'',nmba005,nmba006,nmba008,nmba009,nmba010,nmba011, 
