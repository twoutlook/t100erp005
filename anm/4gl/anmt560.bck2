   LET g_sql = " SELECT DISTINCT t0.nmbasite,t0.nmba002,t0.nmbacomp,t0.nmbadocno,t0.nmbadocdt,t0.nmba013, 
       t0.nmba014,t0.nmba007,t0.nmba003,t0.nmbastus,t0.nmba001,t0.nmba004,t0.nmba005,t0.nmba006,t0.nmba008, 
