{<section id="anmt560.modify" >}
#+ 資料修改
PRIVATE FUNCTION anmt560_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   
   #end add-point    
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   DEFINE l_wc2_table1          STRING
 
 
   #add-point:modify段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   #150813-00015#17--add--str--
   DEFINE l_success   LIKE type_t.num5  
   DEFINE l_slip      LIKE type_t.chr10
   DEFINE l_dfin0033  LIKE type_t.chr80
   DEFINE l_para_date LIKE type_t.dat
   DEFINE l_glaald    LIKE glaa_t.glaald
   #150813-00015#17--add--en
   #end add-point    
   
   #add-point:Function前置處理  name="modify.pre_function"
   
   #end add-point
   
   #保存單頭舊值
   LET g_nmba_m_t.* = g_nmba_m.*
   LET g_nmba_m_o.* = g_nmba_m.*
   
   IF g_nmba_m.nmbacomp IS NULL
   OR g_nmba_m.nmbadocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_nmbacomp_t = g_nmba_m.nmbacomp
   LET g_nmbadocno_t = g_nmba_m.nmbadocno
 
   CALL s_transaction_begin()
   
   OPEN anmt560_cl USING g_enterprise,g_nmba_m.nmbacomp,g_nmba_m.nmbadocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN anmt560_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CLOSE anmt560_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE anmt560_master_referesh USING g_nmba_m.nmbacomp,g_nmba_m.nmbadocno INTO g_nmba_m.nmbasite, 
       g_nmba_m.nmba002,g_nmba_m.nmbacomp,g_nmba_m.nmbadocno,g_nmba_m.nmbadocdt,g_nmba_m.nmba013,g_nmba_m.nmba014, 
       g_nmba_m.nmba007,g_nmba_m.nmba003,g_nmba_m.nmbastus,g_nmba_m.nmba001,g_nmba_m.nmba004,g_nmba_m.nmba005, 
       g_nmba_m.nmba006,g_nmba_m.nmba008,g_nmba_m.nmba009,g_nmba_m.nmba010,g_nmba_m.nmba011,g_nmba_m.nmbaownid, 
       g_nmba_m.nmbaowndp,g_nmba_m.nmbacrtid,g_nmba_m.nmbacrtdp,g_nmba_m.nmbacrtdt,g_nmba_m.nmbamodid, 
       g_nmba_m.nmbamoddt,g_nmba_m.nmbacnfid,g_nmba_m.nmbacnfdt,g_nmba_m.nmbasite_desc,g_nmba_m.nmba002_desc, 
       g_nmba_m.nmbacomp_desc,g_nmba_m.nmba001_desc,g_nmba_m.nmba004_desc,g_nmba_m.nmbaownid_desc,g_nmba_m.nmbaowndp_desc, 
       g_nmba_m.nmbacrtid_desc,g_nmba_m.nmbacrtdp_desc,g_nmba_m.nmbamodid_desc,g_nmba_m.nmbacnfid_desc 
 
   
   #檢查是否允許此動作
   IF NOT anmt560_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_nmba_m_mask_o.* =  g_nmba_m.*
   CALL anmt560_nmba_t_mask()
   LET g_nmba_m_mask_n.* =  g_nmba_m.*
   
   
   
   #add-point:modify段show之前 name="modify.before_show"
   
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
 
   
   CALL anmt560_show()
   #add-point:modify段show之後 name="modify.after_show"
   #150813-00015#17--add--str--
   IF g_nmba_m.nmbastus <> 'N' THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'agl-00313'
      LET g_errparam.extend = 'modify'
      LET g_errparam.popup = TRUE
      CALL cl_err()
      CLOSE anmt560_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF 
   #获取单别
   CALL s_aooi200_fin_get_slip(g_nmba_m.nmbadocno) RETURNING l_success,l_slip
   SELECT glaald INTO l_glaald FROM glaa_t
    WHERE glaaent = g_enterprise AND glaa014 = 'Y'
      AND glaacomp = g_nmba_m.nmbacomp
   #是否可改日期
   CALL s_fin_get_doc_para(l_glaald,g_nmba_m.nmbacomp,l_slip,'D-FIN-0033') RETURNING l_dfin0033
   IF l_dfin0033 = 'N' THEN
