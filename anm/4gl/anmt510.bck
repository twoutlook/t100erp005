      LET p_wc = " nmba003 = 'anmt510' "
   ELSE
      LET p_wc = g_wc," AND nmba003 = 'anmt510'"
   END IF 
   
   #160816-00012#3 add s---
   CALL s_axrt300_get_site(g_user,'','3') RETURNING l_comp_str
   LET l_comp_str = cl_replace_str(l_comp_str,"ooef017","nmbacomp")
