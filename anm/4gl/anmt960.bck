                  #end add-point
                  "  LEFT JOIN nmbw_t ON nmbwent = nmbqent AND nmbqdocno = nmbwdocno", "  ", 
                  #add-point:browser_fill段sql(nmbw_t1) name="browser_fill.join.nmbw_t1"
                  
