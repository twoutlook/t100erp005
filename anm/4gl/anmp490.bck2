         #END IF
         #CALL s_anmt480_ins_nmbc(l_nmchdocno,g_input.nmckcomp,1,l_nmck012)RETURNING g_sub_success
         #151013-00019#11 mark end---
         CALL cl_err_collect_show()
         IF NOT g_sub_success THEN
            CALL s_transaction_end('N','0')
