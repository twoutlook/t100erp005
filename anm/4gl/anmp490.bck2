DEFINE l_pmaf005       LIKE pmaf_t.pmaf005   #SWIFT代號
DEFINE l_nmabl004      LIKE nmabl_t.nmabl004 #名稱
DEFINE l_nmaa005       LIKE nmaa_t.nmaa005
DEFINE l_nmas003       LIKE nmas_t.nmas003                  
DEFINE l_pmaa027       LIKE pmaa_t.pmaa027
DEFINE l_oofc012       LIKE oofc_t.oofc012
DEFINE l_in_txt        STRING 
DEFINE l_nmck014       LIKE nmck_t.nmck014
DEFINE l_str           STRING
 
   INITIALIZE l_tmp.* TO NULL
   SELECT ooef002  INTO l_ooef002  FROM ooef_t  WHERE ooefent  = g_enterprise AND ooef001  = g_input.nmcksite     
   SELECT ooefl006 INTO l_ooefl006 FROM ooefl_t WHERE ooeflent = g_enterprise AND ooefl001 = g_input.nmckcomp AND ooefl002 = g_dlang   
   SELECT ooef012  INTO l_ooef012  FROM ooef_t  WHERE ooefent  = g_enterprise AND ooef001  = g_input.nmckcomp
   SELECT oofb017  INTO l_oofb017  FROM oofb_t 
    WHERE oofbent  = g_enterprise 
      AND oofb002 = l_ooef012 AND oofb008 = '1' AND oofb010 = 'Y'
   SELECT nmck008,nmck100,nmck103,nmck014,nmck015,nmck037,nmck016  
     INTO l_nmck.nmck008,l_nmck.nmck100,l_nmck.nmck103,l_nmck.nmck014,l_nmck.nmck015,l_nmck.nmck037,l_nmck.nmck016 
     FROM nmck_t WHERE nmckent = g_enterprise
      AND nmckdocno = g_detail_d[p_ac].nmckdocno
      AND nmckcomp = g_input.nmckcomp
   SELECT pmaf005 INTO l_pmaf005  #SWIFT代號 pmaf005 
     FROM pmaf_t 
    WHERE pmafent = g_enterprise AND pmaf001 = g_detail_d[p_ac].nmck005
      AND pmaf002 = g_detail_d[p_ac].nmck013 AND pmaf003 = l_nmck.nmck014
   SELECT nmabl004 INTO l_nmabl004 FROM nmabl_t WHERE nmablent = g_enterprise AND nmabl001 = g_detail_d[p_ac].nmck013 AND nmabl002 = g_dlang
   
   LET l_nmck014 = l_nmck.nmck014
   LET l_nmck.nmck014 = '/'||l_nmck.nmck014
   
   #台幣或日幣捕兩個0其他小數點兩位不足右補0 數字直接*100 EX:100*100 = 10000 /200.1*100=20010 /200.21*100=20021
   LET l_nmck.nmck103 = l_nmck.nmck103*100


   #150918-00001#4-----s
   SELECT nmab008  INTO l_nmab008
     FROM nmab_t  
    WHERE nmabent = g_enterprise 
      AND nmab001 = g_detail_d[p_ac].nmck013
   #150918-00001#4-----e
      
   #150918-00001#4-----s(MARK)
#   SELECT pmaa007,pmaa027  INTO l_pmaa007,l_pmaa027
#     FROM pmaa_t  
#    WHERE pmaaent = g_enterprise 
#      AND pmaa001 = g_detail_d[p_ac].nmck005
   #150918-00001#4-----e(MARK)
   
   #國別   
   #SELECT oocgl003 INTO l_oocgl003 FROM oocgl_t WHERE oocglent = g_enterprise AND oocgl001 = l_pmaa007 AND oocgl002 = g_dlang
   #地址 帳單地址
#   SELECT oofb017  INTO l_oofb017_2  
#     FROM oofb_t  
#    WHERE oofbent  = g_enterprise AND oofb002 = l_pmaa027 AND oofb008 = '1' AND oofb010 = 'Y'
   SELECT nmas003 INTO l_nmas003
     FROM nmaa_t,nmas_t
    WHERE nmaaent = g_enterprise
      AND nmaaent = nmasent
      AND nmaa001 = nmas001
      AND nmas002 = g_input.nmck004
      AND nmaa002 IN (select ooef001 FROM ooef_t WHERE ooefent = g_enterprise
                                                 AND ooef017 = g_input.nmckcomp) 
   SELECT nmaa005 INTO l_nmaa005
     FROM nmaa_t,nmas_t 
    WHERE nmaaent = nmasent AND nmaaent = g_enterprise 
      AND nmas001 = nmaa001
      AND nmas002 = g_input.nmck004
   #連絡電話 
   SELECT oofc012 INTO l_oofc012
     FROM oofc_t 
