   LET l_tmp.tmp007 = l_nmck.nmck103  USING "&&&&&&&&&&&&&&&"
   #交易國別代號           173-174
   #150918-00001#4-----s
   IF cl_null(l_nmab008) THEN LET l_nmab008 = ' ' END IF
   LET l_tmp.tmp008 = l_nmab008 
#   IF cl_null(l_pmaa007) THEN LET l_pmaa007 = ' ' END IF
#   LET l_tmp.tmp008 = l_pmaa007
   #150918-00001#4-----e
   #交易國別名稱           175-194
   CALL anmp490_convert(20,l_oocgl003) RETURNING l_tmp.tmp009
   #匯款性質編號           195-197
   LET l_tmp.tmp010 = '710'
