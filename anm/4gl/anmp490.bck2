PRIVATE FUNCTION anmp490_getnmck_1(p_ac)
DEFINE l_tmp           RECORD
            tmp001        CHAR(10),     #1-10
            tmp002        STRING,     #11-45  法人對外名稱
            tmp003        STRING,     #46-150 名稱及地址
            tmp004        STRING,     #151-151
            tmp005        CHAR(3),    #152-154 幣別
            tmp006        STRING,     #155-169 金額
            tmp007        CHAR(2),    #170-171 國別代號
            tmp008        STRING,     #172-191 名稱
            tmp009        STRING,     #192-194   
            tmp010        STRING,     #195-229             
            tmp011        STRING,     #230-230  
            tmp012        STRING,     #231-265 帳號 
            tmp013        STRING,     #266-300 名稱
            tmp014        STRING,     #301-405 地址
            tmp015        STRING,     #406-417 SWIFT代號
            tmp016        STRING,     #418-452 名稱
            tmp017        CHAR(257),  #453-709(空白)
            tmp018        CHAR(3),    #710-712 
            tmp019        CHAR(140),  #713-852 付款明細(空白) 
            tmp020        STRING,     #853-864 授權扣帳帳號1
            tmp021        CHAR(3),    #865-867 授權扣帳幣別1
            tmp022        STRING,     #868-879 授權扣帳帳號2
            tmp023        CHAR(3),    #880-882 授權扣帳幣別2
            tmp024        CHAR(12),   #883-894 匯率議價編號
            tmp025        CHAR(1),    #895-895 匯款繳款方式
            tmp026        CHAR(1),    #896-896 費用繳款方式
            tmp027        CHAR(20),   #897-916 聯絡電話（申請人）
            tmp028        CHAR(1)     #917-917 拍發兩通電文
                       END RECORD  
DEFINE l_nmck          RECORD
             nmck008     LIKE nmck_t.nmck008,
             nmck014     LIKE nmck_t.nmck014,
             nmck015     LIKE nmck_t.nmck015,
             nmck100     LIKE nmck_t.nmck100,             
             nmck103     LIKE nmck_t.nmck103          
                       END RECORD
DEFINE l_ooef012       LIKE ooef_t.ooef012
DEFINE l_ooef002       LIKE ooef_t.ooef002
DEFINE l_ooefl006      LIKE ooefl_t.ooefl006
DEFINE l_oofb017       LIKE oofb_t.oofb017
DEFINE l_oofb017_2     LIKE oofb_t.oofb017 
#DEFINE l_pmaa007       LIKE pmaa_t.pmaa007   #國別     #150918-00001#4 MARK 改取nmab008
DEFINE l_nmab008       LIKE nmab_t.nmab008              #150918-00001#4  add nmab008
DEFINE l_oocgl003      LIKE oocgl_t.oocgl003 #交易國別名稱
DEFINE l_pmaf005       LIKE pmaf_t.pmaf005   #SWIFT代號
DEFINE l_nmabl004      LIKE nmabl_t.nmabl004 #名稱
DEFINE l_nmaa005       LIKE nmaa_t.nmaa005
DEFINE l_nmas003       LIKE nmas_t.nmas003
DEFINE p_ac            LIKE type_t.num5      #第幾筆                  
DEFINE l_pmaa027       LIKE pmaa_t.pmaa027
DEFINE l_oofc012       LIKE oofc_t.oofc012
DEFINE l_in_txt        STRING 



 
   INITIALIZE l_tmp.* TO NULL
   SELECT ooef002  INTO l_ooef002  FROM ooef_t  WHERE ooefent  = g_enterprise AND ooef001  = g_input.nmcksite     
   SELECT ooefl006 INTO l_ooefl006 FROM ooefl_t WHERE ooeflent = g_enterprise AND ooefl001 = g_input.nmckcomp AND ooefl002 = g_dlang   
   SELECT ooef012  INTO l_ooef012  FROM ooef_t  WHERE ooefent  = g_enterprise AND ooef001  = g_input.nmckcomp
   SELECT oofb017  INTO l_oofb017  
     FROM oofb_t  WHERE oofbent  = g_enterprise 
      AND oofb002 = l_ooef012 AND oofb008 = '1' AND oofb010 = 'Y'
   SELECT nmck008,nmck100,nmck103,nmck014,nmck015  
     INTO l_nmck.nmck008,l_nmck.nmck100,l_nmck.nmck103,l_nmck.nmck014,l_nmck.nmck015 
     FROM nmck_t WHERE nmckent = g_enterprise
      AND nmckdocno = g_detail_d[p_ac].nmckdocno
      AND nmckcomp = g_input.nmckcomp
   SELECT pmaf005 INTO l_pmaf005  #SWIFT代號 pmaf005 406-417
     FROM pmaf_t 
    WHERE pmafent = g_enterprise AND pmaf001 = g_detail_d[p_ac].nmck005
      AND pmaf002 = g_detail_d[p_ac].nmck013 AND pmaf003 = l_nmck.nmck014
   SELECT nmabl004 INTO l_nmabl004 FROM nmabl_t WHERE nmablent = g_enterprise AND nmabl001 = g_detail_d[p_ac].nmck013 AND nmabl002 = g_dlang

   LET l_nmck.nmck014 = '/'||l_nmck.nmck014
   #台幣或日幣捕兩個0其他小數點兩位不足右補0 數字直接*100 EX:100*100 = 10000 /200.1*100=20010 /200.21*100=20021
   LET l_nmck.nmck103 = l_nmck.nmck103*100

   #150918-00001#4-----s
   SELECT nmab008  INTO l_nmab008
     FROM nmab_t  
    WHERE nmabent = g_enterprise 
      AND nmab001 = g_detail_d[p_ac].nmck013  
      
#-----s(MARK)
#   SELECT pmaa007,pmaa027  INTO l_pmaa007,l_pmaa027
#     FROM pmaa_t  
#    WHERE pmaaent = g_enterprise 
#      AND pmaa001 = g_detail_d[p_ac].nmck005
#-----e(MARK)
   #150918-00001#4-----e

   #國別   
  # SELECT oocgl003 INTO l_oocgl003 FROM oocgl_t WHERE oocglent = g_enterprise AND oocgl001 = l_pmaa007 AND oocgl002 = g_dlang
  #地址 帳單地址
  #   SELECT oofb017  INTO l_oofb017_2  FROM oofb_t  
  #    WHERE oofbent  = g_enterprise 
  #      AND oofb002 = l_pmaa027 AND oofb008 = '1' AND oofb010 = 'Y'
   SELECT nmas003 INTO l_nmas003
     FROM nmaa_t,nmas_t
    WHERE nmaaent = g_enterprise
      AND nmaaent = nmasent
      AND nmaa001 = nmas001
      AND nmas002 = g_input.nmck004
      AND nmaa002 IN (select ooef001 FROM ooef_t WHERE ooefent = g_enterprise
                                                 AND ooef017 = g_input.nmckcomp)
   IF l_nmas003 = 'TWD' THEN 
      LET l_tmp.tmp025 ='1'
      LET l_tmp.tmp026 ='1'
   ELSE
      LET l_tmp.tmp025 = '2'
      LET l_tmp.tmp026 = '2'
   END IF      
   
   SELECT nmaa005 INTO l_nmaa005
     FROM nmaa_t,nmas_t 
    WHERE nmaaent = nmasent AND nmaaent = g_enterprise 
      AND nmas001 = nmaa001
      AND nmas002 = g_input.nmck004
    
   #連絡電話 主要 
   SELECT oofc012 INTO l_oofc012
     FROM oofc_t 
    WHERE oofcent = g_enterprise
      AND oofc002 = l_ooef012 AND oofc008 = '1' AND oofc010 = 'Y'
   
   #ooef002申請人統一編號 1-10    
   LET l_tmp.tmp001 = l_ooef002       
   #ooefl006法人對外名    11-45   
   CALL anmp490_convert(35,l_ooefl006) RETURNING l_tmp.tmp002    
   #oofb017名稱及地址     46-150
   CALL anmp490_convert(105,l_oofb017) RETURNING l_tmp.tmp003 
   #匯款人身分別 空白     151-151  
   LET l_tmp.tmp004 = l_tmp.tmp004    USING "#"
   #匯款幣別             152-154
   LET l_tmp.tmp005 = l_nmck.nmck100 
   #匯款金額             155-169
   LET l_tmp.tmp006 = l_nmck.nmck103  USING "&&&&&&&&&&&&&&&"
   #國別                 170-171
   #150918-00001#4-----s
   IF cl_null(l_nmab008) THEN LET l_nmab008 = ' ' END IF
   LET l_tmp.tmp007 = l_nmab008 
#   IF cl_null(l_pmaa007) THEN LET l_pmaa007 = ' ' END IF
#   LET l_tmp.tmp007 = l_pmaa007  
   #150918-00001#4-----e
   
   #交易國別名稱          172-192 
   CALL anmp490_convert(20,l_oocgl003) RETURNING l_tmp.tmp008   
   #匯款性質編號          192-194
   LET l_tmp.tmp009 = 710           
   #匯款性質編號其他補充說明 195-229  
   LET l_tmp.tmp010 = l_tmp.tmp010    USING "###################################"
   #受款人身分別            230-230
   LET l_tmp.tmp011 = 3 
   #受益人 帳號             231-265
   CALL anmp490_convert(35,l_nmck.nmck014) RETURNING l_tmp.tmp012 
   #名稱                   266-300
   CALL anmp490_convert(35,l_nmck.nmck015) RETURNING l_tmp.tmp013 
   #名稱及地址1             301-405
   CALL anmp490_convert(105,l_oofb017_2) RETURNING l_tmp.tmp014  
   #SWIFT代號               406-417 SWIFT代號 418-452 名稱
   IF NOT cl_null(l_pmaf005) THEN
      CALL anmp490_convert(12,l_pmaf005) RETURNING l_tmp.tmp015 
      CALL anmp490_convert(35,l_nmabl004) RETURNING l_tmp.tmp016      
   ELSE
      CALL anmp490_convert(12,l_pmaf005) RETURNING l_tmp.tmp015
      CALL anmp490_convert(35,l_nmabl004) RETURNING l_tmp.tmp016  
   END IF
   #名稱及地址             453-709
   LET l_tmp.tmp017 = ' ' 
   #費用明細               710-712
   IF l_nmck.nmck008 = '1'THEN LET l_tmp.tmp018 = 'OUR' END IF
   IF l_nmck.nmck008 = '2'THEN LET l_tmp.tmp018 = 'BEN' END IF
   IF l_nmck.nmck008 = '3'THEN LET l_tmp.tmp018 = 'SHA' END IF
   #付款明細(空白)         713-852
   LET l_tmp.tmp019 = ' '
   #授權扣帳帳號1          853-882 授權扣帳帳號/授權扣帳幣別
   IF l_tmp.tmp025 = 1 THEN 
      CALL anmp490_convert(12,l_nmaa005) RETURNING l_tmp.tmp020 #853-864 
      LET l_tmp.tmp021 = 'TWD'                                  #865-867
      CALL anmp490_convert(12,'') RETURNING l_tmp.tmp022 #868-879
      LET l_tmp.tmp023 = ' '      
   ELSE
      CALL anmp490_convert(12,'') RETURNING l_tmp.tmp020 #853-864 
      LET l_tmp.tmp021 = ' '
      CALL anmp490_convert(12,l_nmaa005) RETURNING l_tmp.tmp022 #868-879
      LET l_tmp.tmp023 = l_nmck.nmck100                         #880-882
   END IF       
   #匯率議價編號            883-894
   LET l_tmp.tmp024 = ' '
   #匯款繳款方式/費用繳款方式895-896 25/26
   #聯絡電話（申請人）       897-916    
   CALL anmp490_convert(20,l_oofc012)RETURNING l_tmp.tmp027
   #拍發兩通電文             917-917
   LET l_tmp.tmp028 = 'N'  


   #組成一長條的數字資料
   LET l_in_txt = l_tmp.tmp001,l_tmp.tmp002,l_tmp.tmp003,l_tmp.tmp004,l_tmp.tmp005,
                  l_tmp.tmp006,l_tmp.tmp007,l_tmp.tmp008,l_tmp.tmp009,l_tmp.tmp010,
                  l_tmp.tmp011,l_tmp.tmp012,l_tmp.tmp013,l_tmp.tmp014,l_tmp.tmp015,
                  l_tmp.tmp016,l_tmp.tmp017,l_tmp.tmp018,l_tmp.tmp019,l_tmp.tmp020,
                  l_tmp.tmp021,l_tmp.tmp022,l_tmp.tmp023,l_tmp.tmp024,l_tmp.tmp025,
                  l_tmp.tmp026,l_tmp.tmp027,l_tmp.tmp028
   
                  
     
             
   RETURN l_in_txt            
                       
END FUNCTION

