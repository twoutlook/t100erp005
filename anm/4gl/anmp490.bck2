   LET l_tmp.tmp011 = ' ' 
   #受款人身分            233-233
   LET l_tmp.tmp012 = '3'
   #15.16.17(空)         234-283
   LET l_tmp.tmp013 = l_nmck014
   #名稱 #地址                  284-318/319-423
   LET l_str = l_nmck.nmck015,l_oofb017_2
   CALL anmp490_convert(140,l_str) RETURNING l_tmp.tmp014                  
  # CALL anmp490_convert(105,l_oofb017_2) RETURNING l_tmp.tmp015  
   #受款人 swift/fisc    424-424 if  nmck037 ='1' then  'F' 本行 then 'F' else ='S' 
   IF l_nmck.nmck037 = '1' THEN
      LET l_tmp.tmp016 = 'F' 
   ELSE
      LET l_tmp.tmp016 = 'S'
   END IF
   #swift  代號         425-436 
   IF l_tmp.tmp016 = 'F' THEN
      CALL anmp490_convert(12,'') RETURNING l_tmp.tmp017
   ELSE
      CALL anmp490_convert(12,l_pmaf005) RETURNING l_tmp.tmp017
   END IF
   #名稱#地址               437-471  472-576
   IF l_tmp.tmp016 = 'F' THEN
      CALL anmp490_convert(35,'') RETURNING l_tmp.tmp018 
      CALL anmp490_convert(105,'') RETURNING l_tmp.tmp019 
      LET l_tmp.tmp018 = l_tmp.tmp018,l_tmp.tmp019 
   ELSE
      LET l_str = l_nmck.nmck015,l_oofb017_2
      CALL anmp490_convert(140,l_str) RETURNING l_tmp.tmp018 
      #CALL anmp490_convert(105,l_oofb017_2) RETURNING l_tmp.tmp019 
   END IF
   #(空)                   577-728
   LET l_tmp.tmp020 = ' '
   #費用明細               729-731
   CASE
