            tmp032        STRING,     #1030-1137
            tmp033        CHAR(35),   #1138-1172(空)
            tmp034        CHAR(1)     #1173-1173       
                       END RECORD  
DEFINE l_nmck          RECORD
             nmck008     LIKE nmck_t.nmck008,
             nmck014     LIKE nmck_t.nmck014,
             nmck015     LIKE nmck_t.nmck015,
             nmck016     LIKE nmck_t.nmck016,
             nmck037     LIKE nmck_t.nmck037,
             nmck100     LIKE nmck_t.nmck100,             
             nmck103     LIKE nmck_t.nmck103          
                       END RECORD
DEFINE p_ac            LIKE type_t.num5      #第幾筆
DEFINE l_ooef012       LIKE ooef_t.ooef012
DEFINE l_ooef002       LIKE ooef_t.ooef002
DEFINE l_ooefl006      LIKE ooefl_t.ooefl006
DEFINE l_oofb017       LIKE oofb_t.oofb017
DEFINE l_oofb017_2     LIKE oofb_t.oofb017 
