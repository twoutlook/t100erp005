#+ 資料修改
PRIVATE FUNCTION anmt420_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   
   #end add-point
   DEFINE  l_cmd                  LIKE type_t.chr1
   DEFINE  l_ac_t                 LIKE type_t.num10               #未取消的ARRAY CNT 
   DEFINE  l_n                    LIKE type_t.num10               #檢查重複用  
   DEFINE  l_cnt                  LIKE type_t.num10               #檢查重複用  
   DEFINE  l_lock_sw              LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert         LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete         LIKE type_t.num5                #可刪除否  
   DEFINE  l_count                LIKE type_t.num10
   DEFINE  l_i                    LIKE type_t.num10
   DEFINE  ls_return              STRING
   DEFINE  l_var_keys             DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys           DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                 DYNAMIC ARRAY OF STRING
   DEFINE  l_fields               DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak         DYNAMIC ARRAY OF STRING
   DEFINE  li_reproduce           LIKE type_t.num10
   DEFINE  li_reproduce_target    LIKE type_t.num10
   DEFINE  lb_reproduce           BOOLEAN
   DEFINE  l_insert               BOOLEAN
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   DEFINE  l_nmcd008              LIKE nmcd_t.nmcd008   #160829-00029#1 Add
   #end add-point 
   
   #add-point:Function前置處理  name="modify.pre_function"
   
   #end add-point
   
   LET g_action_choice = ""
   
   LET g_qryparam.state = "i"
 
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   
   #add-point:modify開始前 name="modify.define_sql"
   
   #end add-point
   
   LET INT_FLAG = FALSE
   LET lb_reproduce = FALSE
   LET l_insert = FALSE
 
   #關閉被遮罩相關欄位輸入, 無法確定USER是否會需要輸入此欄位
   #因此先行關閉, 若有需要可於下方add-point中自行開啟
   CALL cl_mask_set_no_entry()
 
   #add-point:modify段修改前 name="modify.before_input"
   LET g_errshow = 1
   LET g_flag = 'Y'
   #end add-point
 
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #Page1 預設值產生於此處
      INPUT ARRAY g_nmcd_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_detail_cnt,WITHOUT DEFAULTS, #MAXCOUNT = g_max_rec,
                  INSERT ROW = FALSE, 
                  DELETE ROW = FALSE,
                  APPEND ROW = FALSE)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_nmcd_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL anmt420_b_fill(g_wc2)
            LET g_detail_cnt = g_nmcd_d.getLength()
         
         BEFORE ROW
            #add-point:modify段before row name="input.body.before_row2"
            
            #end add-point  
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_cmd = ''
            LET l_ac_t = l_ac 
            LET l_ac = g_detail_idx
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            DISPLAY g_nmcd_d.getLength() TO FORMONLY.cnt
         
            CALL s_transaction_begin()
            LET g_detail_cnt = g_nmcd_d.getLength()
            
            IF g_detail_cnt >= l_ac 
               AND g_nmcd_d[l_ac].nmcd002 IS NOT NULL
               AND g_nmcd_d[l_ac].nmcd003 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_nmcd_d_t.* = g_nmcd_d[l_ac].*  #BACKUP
               LET g_nmcd_d_o.* = g_nmcd_d[l_ac].*  #BACKUP
               IF NOT anmt420_lock_b("nmcd_t") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH anmt420_bcl INTO g_nmcd_d[l_ac].nmcd008,g_nmcd_d[l_ac].nmcd002,g_nmcd_d[l_ac].nmcd003, 
                      g_nmcd_d[l_ac].nmcd001,g_nmcd_d[l_ac].nmcd004,g_nmcd_d[l_ac].nmcd005,g_nmcd_d[l_ac].nmcd006, 
                      g_nmcd_d[l_ac].nmcd007,g_nmcd_d[l_ac].nmcd010,g_nmcd2_d[l_ac].nmcd002,g_nmcd2_d[l_ac].nmcd003, 
                      g_nmcd2_d[l_ac].nmcdownid,g_nmcd2_d[l_ac].nmcdowndp,g_nmcd2_d[l_ac].nmcdcrtid, 
                      g_nmcd2_d[l_ac].nmcdcrtdp,g_nmcd2_d[l_ac].nmcdcrtdt,g_nmcd2_d[l_ac].nmcdmodid, 
                      g_nmcd2_d[l_ac].nmcdmoddt
                  IF SQLCA.sqlcode THEN
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = g_nmcd_d_t.nmcd002,":",SQLERRMESSAGE  
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     LET l_lock_sw = "Y"
                  END IF
                  
                  #遮罩相關處理
                  LET g_nmcd_d_mask_o[l_ac].* =  g_nmcd_d[l_ac].*
                  CALL anmt420_nmcd_t_mask()
                  LET g_nmcd_d_mask_n[l_ac].* =  g_nmcd_d[l_ac].*
                  
                  CALL anmt420_detail_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            CALL anmt420_set_entry_b(l_cmd)
            CALL anmt420_set_no_entry_b(l_cmd)
