#+ 單身陣列填充
PRIVATE FUNCTION anmt420_b_fill(p_wc2)              #BODY FILL UP
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   
   #end add-point
   DEFINE p_wc2    STRING
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   IF g_flag = 'N' THEN 
      RETURN
   END IF
   #end add-point
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前 name="b_fill.sql_before"
  #LET p_wc2 = p_wc2," AND nmcdcomp = '",g_nmcdcomp,"' AND nmcd009 = 'N' "   #160829-00029#1 Mark
   LET p_wc2 = p_wc2," AND nmcd009 = 'N' "                                   #160829-00029#1 Add
   #end add-point
 
   LET g_sql = "SELECT  DISTINCT t0.nmcd008,t0.nmcd002,t0.nmcd003,t0.nmcd001,t0.nmcd004,t0.nmcd005,t0.nmcd006, 
       t0.nmcd007,t0.nmcd010,t0.nmcd002,t0.nmcd003,t0.nmcdownid,t0.nmcdowndp,t0.nmcdcrtid,t0.nmcdcrtdp, 
       t0.nmcdcrtdt,t0.nmcdmodid,t0.nmcdmoddt ,t1.ooag011 ,t2.ooag011 ,t3.ooag011 ,t4.ooefl003 ,t5.ooag011 , 
       t6.ooefl003 ,t7.ooag011 FROM nmcd_t t0",
               "",
                              " LEFT JOIN ooag_t t1 ON t1.ooagent="||g_enterprise||" AND t1.ooag001=t0.nmcd005  ",
               " LEFT JOIN ooag_t t2 ON t2.ooagent="||g_enterprise||" AND t2.ooag001=t0.nmcd007  ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent="||g_enterprise||" AND t3.ooag001=t0.nmcdownid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent="||g_enterprise||" AND t4.ooefl001=t0.nmcdowndp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=t0.nmcdcrtid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent="||g_enterprise||" AND t6.ooefl001=t0.nmcdcrtdp AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent="||g_enterprise||" AND t7.ooag001=t0.nmcdmodid  ",
 
               " WHERE t0.nmcdent= ?  AND  1=1 AND (", p_wc2, ") " 
   #add-point:b_fill段sql wc name="b_fill.sql_wc"
   #160829-00029#1 Mark ---(S)---
  #LET g_sql = g_sql CLIPPED," AND (nmcd001 IN(",g_sql_bank,") 
  #                                   OR TRIM(nmcd001) IS NULL)"  #160122-00001#3    #160122-00001#3 by 07900 --mod
   #160829-00029#1 Mark ---(S)---
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料  #160122-00001#3

   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("nmcd_t"),
                      " ORDER BY t0.nmcd002,t0.nmcd003"
   
   #add-point:b_fill段sql之後 name="b_fill.sql_after"
   
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"nmcd_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE anmt420_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR anmt420_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_nmcd_d.clear()
   CALL g_nmcd2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_nmcd_d[l_ac].nmcd008,g_nmcd_d[l_ac].nmcd002,g_nmcd_d[l_ac].nmcd003,g_nmcd_d[l_ac].nmcd001, 
       g_nmcd_d[l_ac].nmcd004,g_nmcd_d[l_ac].nmcd005,g_nmcd_d[l_ac].nmcd006,g_nmcd_d[l_ac].nmcd007,g_nmcd_d[l_ac].nmcd010, 
       g_nmcd2_d[l_ac].nmcd002,g_nmcd2_d[l_ac].nmcd003,g_nmcd2_d[l_ac].nmcdownid,g_nmcd2_d[l_ac].nmcdowndp, 
       g_nmcd2_d[l_ac].nmcdcrtid,g_nmcd2_d[l_ac].nmcdcrtdp,g_nmcd2_d[l_ac].nmcdcrtdt,g_nmcd2_d[l_ac].nmcdmodid, 
       g_nmcd2_d[l_ac].nmcdmoddt,g_nmcd_d[l_ac].nmcd005_desc,g_nmcd_d[l_ac].nmcd007_desc,g_nmcd2_d[l_ac].nmcdownid_desc, 
       g_nmcd2_d[l_ac].nmcdowndp_desc,g_nmcd2_d[l_ac].nmcdcrtid_desc,g_nmcd2_d[l_ac].nmcdcrtdp_desc, 
       g_nmcd2_d[l_ac].nmcdmodid_desc
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充 name="b_fill.fill"
      CALL anmt420_nmcd004_desc()
      CALL anmt420_nmcd005_desc()
      CALL anmt420_nmcd007_desc()
      #end add-point
      
      CALL anmt420_detail_show()      
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
 
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   LET g_error_show = 0
   
 
   
   CALL g_nmcd_d.deleteElement(g_nmcd_d.getLength())   
   CALL g_nmcd2_d.deleteElement(g_nmcd2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_nmcd_d.getLength()
      LET g_nmcd2_d[l_ac].nmcd002 = g_nmcd_d[l_ac].nmcd002 
      LET g_nmcd2_d[l_ac].nmcd003 = g_nmcd_d[l_ac].nmcd003 
 
      #add-point:b_fill段key值相關欄位 name="b_fill.keys.fill"
      
      #end add-point
   END FOR
   
   IF g_cnt > g_nmcd_d.getLength() THEN
      LET l_ac = g_nmcd_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_nmcd_d.getLength()
      LET g_nmcd_d_mask_o[l_ac].* =  g_nmcd_d[l_ac].*
      CALL anmt420_nmcd_t_mask()
      LET g_nmcd_d_mask_n[l_ac].* =  g_nmcd_d[l_ac].*
   END FOR
   
   LET g_nmcd2_d_mask_o.* =  g_nmcd2_d.*
   FOR l_ac = 1 TO g_nmcd2_d.getLength()
      LET g_nmcd2_d_mask_o[l_ac].* =  g_nmcd2_d[l_ac].*
      CALL anmt420_nmcd_t_mask()
      LET g_nmcd2_d_mask_n[l_ac].* =  g_nmcd2_d[l_ac].*
   END FOR
 
   
   LET l_ac = g_cnt
 
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_nmcd_d.getLength()
   DISPLAY g_detail_idx TO FORMONLY.idx
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE anmt420_pb
   
END FUNCTION
 
