             s_detail1[1].nmcd001,s_detail1[1].nmcd001_desc,s_detail1[1].nmcd004,s_detail1[1].nmcd004_desc, 
             s_detail1[1].nmcd005,s_detail1[1].nmcd006,s_detail1[1].nmcd007,s_detail1[1].nmcd010,s_detail2[1].nmcdownid, 
