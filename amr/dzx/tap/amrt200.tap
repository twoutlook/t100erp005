  <point name="input.a.mrdc003" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[            LET g_mrdc_m.mrdc003_desc = ' '
            DISPLAY BY NAME g_mrdc_m.mrdc003_desc
            IF NOT cl_null(g_mrdc_m.mrdc003) THEN
               IF p_cmd = 'a' OR (p_cmd = 'u' AND (g_mrdc_m.mrdc003 != g_mrdc_m_t.mrdc003 OR g_mrdc_m_t.mrdc003 IS NULL )) THEN
                  INITIALIZE g_chkparam.* TO NULL
                  LET g_chkparam.arg1 = g_mrdc_m.mrdc003
                  IF NOT cl_chk_exist("v_ooag001") THEN
                     LET g_mrdc_m.mrdc003 = g_mrdc_m_t.mrdc003
                     CALL amrt200_mrdc003_ref()
                     NEXT FIELD CURRENT
                  END IF
               END IF
            END IF
            IF cl_null(g_mrdc_m.mrdc004) THEN
               CALL amrt200_mrdc003_values()
               CALL amrt200_mrdc004_ref()
            END IF
            CALL amrt200_mrdc003_ref()]]>
  </point>
