  <section id="amrt200.main" ver="4" status="" src="s">
    <![CDATA[#+ 此段落由子樣板a26產生
#OPTIONS SHORT CIRCUIT
#+ 作業開始 
MAIN
   #add-point:main段define
   {<point name="main.define"/>}
   #end add-point   
 
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("amr","")
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT mrdcdocno,'',mrdcdocdt,mrdc001,mrdc002,mrdc003,'',mrdc004,'',mrdcsite, 
       mrdcstus,mrdc005,mrdc006,'',mrdc007,mrdc008,mrdcownid,'',mrdcowndp,'',mrdccrtid,'',mrdccrtdp, 
       '',mrdccrtdt,mrdcmodid,'',mrdcmoddt,mrdccnfid,'',mrdccnfdt", 
                      " FROM mrdc_t",
                      " WHERE mrdcent= ? AND mrdcdocno=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE amrt200_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE mrdcdocno,mrdcdocdt,mrdc001,mrdc002,mrdc003,mrdc004,mrdcsite,mrdcstus, 
       mrdc005,mrdc006,mrdc007,mrdc008,mrdcownid,mrdcowndp,mrdccrtid,mrdccrtdp,mrdccrtdt,mrdcmodid,mrdcmoddt, 
       mrdccnfid,mrdccnfdt,t1.oofa011 ,t2.ooefl003 ,t3.ooefl003 ,t4.oofa011 ,t5.ooefl003 ,t6.oofa011 , 
       t7.ooefl003 ,t8.oofa011 ,t9.oofa011",
               " FROM mrdc_t",
                              " LEFT JOIN oofa_t t1 ON t1.oofaent='"||g_enterprise||"' AND t1.oofa002='2' AND t1.oofa003=mrdc003  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent='"||g_enterprise||"' AND t2.ooefl001=mrdc004 AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent='"||g_enterprise||"' AND t3.ooefl001=mrdc006 AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t4 ON t4.oofaent='"||g_enterprise||"' AND t4.oofa002='2' AND t4.oofa003=mrdcownid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=mrdcowndp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t6 ON t6.oofaent='"||g_enterprise||"' AND t6.oofa002='2' AND t6.oofa003=mrdccrtid  ",
               " LEFT JOIN ooefl_t t7 ON t7.ooeflent='"||g_enterprise||"' AND t7.ooefl001=mrdccrtdp AND t7.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t8 ON t8.oofaent='"||g_enterprise||"' AND t8.oofa002='2' AND t8.oofa003=mrdcmodid  ",
               " LEFT JOIN oofa_t t9 ON t9.oofaent='"||g_enterprise||"' AND t9.oofa002='2' AND t9.oofa003=mrdccnfid  ",
 
               " WHERE mrdcent = '" ||g_enterprise|| "' AND mrdcdocno = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE amrt200_master_referesh FROM g_sql
 
 
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_amrt200 WITH FORM cl_ap_formpath("amr",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL amrt200_init()   
 
      #進入選單 Menu (="N")
      CALL amrt200_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_amrt200
      
   END IF 
   
   CLOSE amrt200_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
   
END MAIN
 
 
]]>
  </section>
  <section id="amrt200.modify" ver="6" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION amrt200_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_mrdc_m.mrdcdocno IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE amrt200_master_referesh USING g_mrdc_m.mrdcdocno INTO g_mrdc_m.mrdcdocno,g_mrdc_m.mrdcdocdt, 
       g_mrdc_m.mrdc001,g_mrdc_m.mrdc002,g_mrdc_m.mrdc003,g_mrdc_m.mrdc004,g_mrdc_m.mrdcsite,g_mrdc_m.mrdcstus, 
       g_mrdc_m.mrdc005,g_mrdc_m.mrdc006,g_mrdc_m.mrdc007,g_mrdc_m.mrdc008,g_mrdc_m.mrdcownid,g_mrdc_m.mrdcowndp, 
       g_mrdc_m.mrdccrtid,g_mrdc_m.mrdccrtdp,g_mrdc_m.mrdccrtdt,g_mrdc_m.mrdcmodid,g_mrdc_m.mrdcmoddt, 
       g_mrdc_m.mrdccnfid,g_mrdc_m.mrdccnfdt,g_mrdc_m.mrdc003_desc,g_mrdc_m.mrdc004_desc,g_mrdc_m.mrdc006_desc, 
       g_mrdc_m.mrdcownid_desc,g_mrdc_m.mrdcowndp_desc,g_mrdc_m.mrdccrtid_desc,g_mrdc_m.mrdccrtdp_desc, 
       g_mrdc_m.mrdcmodid_desc,g_mrdc_m.mrdccnfid_desc
 
   ERROR ""
  
   LET g_mrdcdocno_t = g_mrdc_m.mrdcdocno
 
   CALL s_transaction_begin()
   
   OPEN amrt200_cl USING g_enterprise,g_mrdc_m.mrdcdocno
   IF STATUS THEN
      CALL cl_err("OPEN amrt200_cl:", STATUS, 1)
      CLOSE amrt200_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE amrt200_master_referesh USING g_mrdc_m.mrdcdocno INTO g_mrdc_m.mrdcdocno,g_mrdc_m.mrdcdocdt, 
       g_mrdc_m.mrdc001,g_mrdc_m.mrdc002,g_mrdc_m.mrdc003,g_mrdc_m.mrdc004,g_mrdc_m.mrdcsite,g_mrdc_m.mrdcstus, 
       g_mrdc_m.mrdc005,g_mrdc_m.mrdc006,g_mrdc_m.mrdc007,g_mrdc_m.mrdc008,g_mrdc_m.mrdcownid,g_mrdc_m.mrdcowndp, 
       g_mrdc_m.mrdccrtid,g_mrdc_m.mrdccrtdp,g_mrdc_m.mrdccrtdt,g_mrdc_m.mrdcmodid,g_mrdc_m.mrdcmoddt, 
       g_mrdc_m.mrdccnfid,g_mrdc_m.mrdccnfdt,g_mrdc_m.mrdc003_desc,g_mrdc_m.mrdc004_desc,g_mrdc_m.mrdc006_desc, 
       g_mrdc_m.mrdcownid_desc,g_mrdc_m.mrdcowndp_desc,g_mrdc_m.mrdccrtid_desc,g_mrdc_m.mrdccrtdp_desc, 
       g_mrdc_m.mrdcmodid_desc,g_mrdc_m.mrdccnfid_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_mrdc_m.mrdcdocno,SQLCA.sqlcode,0)
      CLOSE amrt200_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   CALL amrt200_show()
   WHILE TRUE
      LET g_mrdcdocno_t = g_mrdc_m.mrdcdocno
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_mrdc_m.mrdcmodid = g_user 
LET g_mrdc_m.mrdcmoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      CALL amrt200_input("u")
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_mrdc_m.* = g_mrdc_m_t.*
         CALL amrt200_show()
         CALL cl_err('',9001,0)
         CALL s_transaction_end('N','0')
         EXIT WHILE
      END IF 
      
      #若有modid跟moddt則進行update
      UPDATE mrdc_t SET (mrdcmodid,mrdcmoddt) = (g_mrdc_m.mrdcmodid,g_mrdc_m.mrdcmoddt)
       WHERE mrdcent = g_enterprise AND mrdcdocno = g_mrdcdocno_t
 
                  
      #若單頭key欄位有變更
      IF g_mrdc_m.mrdcdocno != g_mrdcdocno_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE mrdd_t SET mrdddocno = g_mrdc_m.mrdcdocno
 
          WHERE mrddent = g_enterprise AND mrdddocno = g_mrdcdocno_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("mrdd_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("mrdd_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_mrdc_m.mrdcdocno,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE amrt200_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_mrdc_m.mrdcdocno,'U')
 
END FUNCTION   
]]>
