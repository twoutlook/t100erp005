               LET g_action_choice="statechange"
               
            ON ACTION first
               CALL amri007_fetch("F") 
               LET g_current_row = g_current_idx
            
            ON ACTION next
               CALL amri007_fetch("N")
               LET g_current_row = g_current_idx
            
            ON ACTION jump
               CALL amri007_fetch("/")
               LET g_current_row = g_current_idx
            
            ON ACTION previous
               CALL amri007_fetch("P")
               LET g_current_row = g_current_idx
            
            ON ACTION last 
               CALL amri007_fetch("L")  
               #CALL cl_navigator_setting(g_current_idx, g_current_cnt)
               #CALL fgl_set_arr_curr(g_current_idx)
               LET g_current_row = g_current_idx
            
