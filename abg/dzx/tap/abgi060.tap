  <point name="function.abgi060_bgae001_desc" cite_std="N" status="" ver="1" src="s" new="Y" order="1" cite_ver="" mark_hard="N">
<![CDATA[################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL abgi060_bgae001_desc( (传入参数)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 14/02/12 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION abgi060_bgae001_desc(p_bgae006,p_bgae001)
DEFINE  p_bgae001  LIKE bgae_t.bgae001
DEFINE  p_bgae006  LIKE bgae_t.bgae006
           INITIALIZE g_ref_fields TO NULL
            LET g_ref_fields[1] = p_bgae001
            LET g_ref_fields[2] = p_bgae006
            CALL ap_ref_array2(g_ref_fields,"SELECT bgael002 FROM bgael_t WHERE bgaelent='"||g_enterprise||"' AND bgael001=? AND bgael006=? AND bgael003='"||g_dlang||"'","") RETURNING g_rtn_fields
            LET g_bgae_d[l_ac].bgael003 = '', g_rtn_fields[1] , ''
            DISPLAY BY NAME g_bgae_d[l_ac].bgael003
END FUNCTION]]>
</point>
  <point name="function.abgi060_bgae009_desc" cite_std="N" status="u" ver="1" src="s" new="Y" order="2" cite_ver="" mark_hard="N">
<![CDATA[################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL abgi060_bgae009_desc(p_bgae006,p_bgae009)
#                  RETURNING 回传参数
# Input parameter: p_bgae006,
#                  p_bgae009
# Return code....: 無
# Date & Author..: 14/02/12 By yuhuabao
# Modify.........:
################################################################################
PRIVATE FUNCTION abgi060_bgae009_desc(p_bgae006,p_bgae009)
DEFINE  p_bgae009  LIKE bgae_t.bgae009
DEFINE  p_bgae006  LIKE bgae_t.bgae006            
            INITIALIZE g_ref_fields TO NULL
            LET g_ref_fields[1] = p_bgae009
            LET g_ref_fields[2] = g_bgae_m.bgae006
            CALL ap_ref_array2(g_ref_fields,"SELECT bgael003 FROM bgael_t WHERE bgaelent='"||g_enterprise||"' AND bgael001=? AND bgael006=? AND bgael002='"||g_dlang||"'","") RETURNING g_rtn_fields
            IF INFIELD(bgae009) THEN
               SELECT bgae004 INTO g_bgae_d[l_ac].bgae011
                 FROM bgae_t
                WHERE bgaeent = g_enterprise
                  AND bgae001 = p_bgae009
                  AND bgae006 = p_bgae006
                  AND bgae007 = '1'
            DISPLAY BY NAME g_bgae_d[l_ac].bgae011
           END IF
           RETURN g_rtn_fields[1]
END FUNCTION]]>
</point>
  <point name="function.abgi060_bgae001_chk" cite_std="N" status="" ver="1" src="s" new="Y" order="3" cite_ver="" mark_hard="N">
<![CDATA[################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL abgi060_bgae001_chk(p_bgae006,p_bgae001)
#                  RETURNING 
# Input parameter: p_bgae006
#                : p_bgae001
# Return code....: 無
# Date & Author..: 14/02/12 By yuhuabao
# Modify.........:
################################################################################
PRIVATE FUNCTION abgi060_bgae001_chk(p_bgae006,p_bgae001)
DEFINE   p_bgae006   LIKE bgae_t.bgae006
DEFINE   p_bgae001   LIKE bgae_t.bgae001
DEFINE   l_cnt       LIKE type_t.num5
DEFINE  l_res LIKE type_t.chr1
DEFINE  l_bgae001 STRING 
   LET g_errno = ''
   SELECT COUNT(bgae001) INTO l_cnt 
     FROM bgae_t
    WHERE bgaeent = g_enterprise
      AND bgae001 = p_bgae001
      AND bgae006 = p_bgae006
      AND bgae007 = '1'
    IF l_cnt <> 0  THEN 
       LET g_errno = 'abg-00009'
    END IF 

    LET l_bgae001 = g_bgae_d[l_ac].bgae001
    IF l_bgae001.getIndexOf('+',1) > 0 OR
       l_bgae001.getIndexOf('-',1) > 0 OR
       l_bgae001.getIndexOf('*',1) > 0 OR
       l_bgae001.getIndexOf('/',1) > 0 OR
       l_bgae001.getIndexOf('=',1) > 0 OR 
       l_bgae001.getIndexOf('(',1) > 0 OR
       l_bgae001.getIndexOf(')',1) > 0 THEN 
       LET g_errno = 'abg-00050'
     END IF
  
END FUNCTION]]>
</point>
  <point name="function.abgi060_bgae009_chk" cite_std="N" status="" ver="1" src="s" new="Y" order="4" cite_ver="" mark_hard="N">
<![CDATA[################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL abgi060_bgae009_chk(p_bgae006,p_bgae009)
#                  RETURNING 回传参数
# Input parameter: p_bgae006
#                : p_bgae009
# Return code....: 無
# Date & Author..: 14/02/12 By yuhuabao
# Modify.........:
################################################################################
PRIVATE FUNCTION abgi060_bgae009_chk(p_bgae006,p_bgae009)
DEFINE p_bgae006   LIKE bgae_t.bgae006
   DEFINE p_bgae009   LIKE bgae_t.bgae009
   DEFINE l_bgaestus  LIKE bgae_t.bgaestus
   DEFINE l_bgae006   LIKE bgae_t.bgae006
   DEFINE   l_cnt       LIKE type_t.num5
    LET g_errno = ''
    SELECT bgaestus,bgae006 INTO l_bgaestus,l_bgae006 FROM bgae_t
     WHERE bgaeent = g_enterprise
       AND bgae001 = p_bgae009
       AND bgae006 = p_bgae006
       AND bgae007 = '1'
    CASE
       WHEN sqlca.sqlcode = 100      LET g_errno = 'abg-00013'
       WHEN l_bgaestus ='N'          LET g_errno = 'abg-00006'
     END CASE
     
    IF NOT cl_null(g_bgae_d[l_ac].bgae009) AND NOT cl_null(g_bgae_d[l_ac].bgae010) THEN
    
       SELECT COUNT(DISTINCT bgae004) INTO l_cnt 
         FROM bgae_t
        WHERE bgaeent = g_enterprise
          AND bgae001 BETWEEN g_bgae_d[l_ac].bgae009 AND g_bgae_d[l_ac].bgae010
          AND bgae006 = p_bgae006
          AND bgae007 = '1'
       IF l_cnt > 1 THEN
          LET g_errno  = 'abg-00051'
       END IF
   END IF
END FUNCTION]]>
</point>
  <point name="function.abgi060_bgae006_chk" cite_std="N" status="" ver="1" src="s" new="Y" order="5" cite_ver="" mark_hard="N">
<![CDATA[################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL abgi060_bgae006_chk(p_bgae006)
# Input parameter: p_bgae006
# Return code....: 無
# Date & Author..: 14/02/12 By yuhuabao
# Modify.........:
################################################################################
PRIVATE FUNCTION abgi060_bgae006_chk(p_bgae006)
DEFINE p_bgae006   LIKE bgae_t.bgae006
    DEFINE l_cnt   LIKE type_t.num5

    LET g_errno = ''
    LET l_cnt = 0
    SELECT count(*) INTO l_cnt FROM bgae_t
     WHERE bgaeent = g_enterprise
       AND bgae006 = p_bgae006
       AND bgae007 = '1'
     IF l_cnt = 0 THEN 
        LET g_errno = 'abg-00005'
        RETURN
     END IF 

     SELECT count(*) INTO l_cnt FROM bgae_t
     WHERE bgaeent = g_enterprise
       AND bgae006 = p_bgae006
       AND bgaestus = 'Y'  
       AND bgae007  = '1'       
     IF l_cnt = 0 THEN 
        LET g_errno = 'abg-00006'
        RETURN
     END IF
END FUNCTION]]>
</point>
