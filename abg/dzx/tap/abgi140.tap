  <point name="function.abgi140_bgao001_chk" order="5" ver="1" cite_std="" cite_ver="" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 預算項目組別檢查
################################################################################
PRIVATE FUNCTION abgi140_bgao001_chk()
DEFINE  l_cnt  LIKE type_t.num5
   LET l_cnt = 0
   LET g_errno = ''
   SELECT COUNT(bgae006) INTO l_cnt FROM bgae_t
    WHERE bgaeent = g_enterprise
      AND bgae006 = g_bgao_m.bgao001
    IF l_cnt <=0 THEN
       LET g_errno = 'abg-00005'
    END IF
    LET l_cnt = 0
   SELECT COUNT(bgae006) INTO l_cnt FROM bgae_t
    WHERE bgaeent = g_enterprise
      AND bgae006 = g_bgao_m.bgao001
      AND bgaestus = 'Y'
   IF l_cnt <= 0 THEN
      LET g_errno = 'abg-00006'
   END IF   
END FUNCTION]]>
