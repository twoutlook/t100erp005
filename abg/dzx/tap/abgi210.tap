  <point name="function.abgi210_ins_b" cite_std="N" status="u" ver="1" src="s" new="Y" order="1" cite_ver="" mark_hard="N">
<![CDATA[#自動產生單身
PRIVATE FUNCTION abgi210_ins_b()
   DEFINE  r_success   LIKE type_t.num5
   DEFINE  l_i         LIKE type_t.num5
   DEFINE  l_bgaw      RECORD LIKE bgaw_t.*
   
   LET r_success = TRUE
   INITIALIZE l_bgaw  TO NULL
   FOR l_i = 1  TO 21
       INSERT INTO bgaw_t (bgawent,bgaw001,bgawstus,bgaw002,bgaw003,bgaw004,
                           bgaw005,bgaw006,bgawownid,bgawowndp,
                           bgawcrtid,bgawcrtdp,bgawcrtdt) 
       VALUES (g_enterprise,g_bgaw_m.bgaw001,g_bgaw_m.bgawstus,l_i,'1',l_i,
               'N','2',g_user,g_dept,
                g_user,g_dept,g_today)  
               
       IF SQLCA.SQLCODE THEN
          CALL cl_err('ins_b',sqlca.sqlcode,1) 
          LET  r_success = FALSE         
          EXIT FOR
       END IF 
       
   END FOR
   RETURN r_success
  
END FUNCTION]]>
</point>
  <point name="function.abgi210_statechange" cite_std="N" status="u" ver="1" src="s" new="Y" order="2" cite_ver="" mark_hard="N">
<![CDATA[#+
PRIVATE FUNCTION abgi210_statechange()
   DEFINE lc_state LIKE type_t.chr5
   ERROR ""     #清空畫面右下側ERROR區塊

   IF g_bgaw_m.bgaw001 IS NULL THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF

   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         CASE g_bgaw_m.bgawstus
            WHEN "1"
               HIDE OPTION "unconfirmed"
            WHEN "2"
               HIDE OPTION "confirm"
         END CASE
         
      ON ACTION unconfirmed
         LET lc_state = "1"
         EXIT MENU
      ON ACTION confirm
         LET lc_state = "2"
         EXIT MENU

   END MENU
   
   IF (lc_state <> "1" 
      AND lc_state <> "2"
      ) OR 
      cl_null(lc_state) THEN
      RETURN
   END IF
   
   #add-point:stus修改前

   #end add-point
      
   UPDATE bgaw_t SET bgawstus = lc_state,
                     bgawmodid = g_user,
                     bgawmoddt = g_today     
    WHERE bgawent = g_enterprise AND bgaw001= g_bgaw_m.bgaw001
  
   IF SQLCA.sqlcode THEN
      CALL cl_err("",SQLCA.sqlcode,0)
   ELSE
      CASE lc_state
         WHEN "1"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "2"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirm.png")
      END CASE
   END IF
END FUNCTION]]>
</point>
  <point name="function.abgi210_pre" cite_std="N" status="u" ver="1" src="s" new="Y" order="3" cite_ver="" mark_hard="N">
<![CDATA[#+预览
PRIVATE FUNCTION abgi210_pre(p_bgaw001)
    DEFINE p_bgaw001     LIKE bgaw_t.bgaw001
    DEFINE l_bgaw004     LIKE bgaw_t.bgaw004
    DEFINE l_bgaw005     LIKE bgaw_t.bgaw005
    DEFINE l_bgaw006     LIKE bgaw_t.bgaw006
    DEFINE l_sql         STRING
    #单头隐藏标记
    DEFINE l_flag1       LIKE type_t.num5
    DEFINE l_flag2       LIKE type_t.num5
    DEFINE l_flag3       LIKE type_t.num5
    DEFINE l_flag4       LIKE type_t.num5
    DEFINE l_flag5       LIKE type_t.num5

    #資料檢查
    #單頭key不可空白
    IF cl_null(p_bgaw001) THEN
       CALL cl_err('',-400,1)
       RETURN 
    END IF 
    #隐藏标记初始化
    LET l_flag1 = 0    
    LET l_flag2 = 0
    LET l_flag3 = 0
    LET l_flag4 = 0
    LET l_flag5 = 0

    #開出畫面    
    OPEN WINDOW w_abgi210_s01 WITH FORM cl_ap_formpath("abg","abgi210_s01")
   
    LET l_sql = " SELECT bgaw004,bgaw005,bgaw006 FROM bgaw_t ",
                "  WHERE bgawent = '",g_enterprise,"'" ,
                "    AND bgaw001 = '",p_bgaw001,"'"               

    PREPARE abgi210_pre  FROM l_sql
    DECLARE abgi210_cs CURSOR FOR abgi210_pre
    FOREACH abgi210_cs INTO l_bgaw004,l_bgaw005,l_bgaw006
        IF SQLCA.sqlcode THEN
           CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
           CLOSE WINDOW w_abgi210_s01
           RETURN
        END IF
        #期别
        IF l_bgaw004 = '1' THEN
           IF l_bgaw005 = 'N' THEN
              CALL cl_set_comp_visible("lbl_bgaz006,bgaz006,bgaz006_1",FALSE)
           ELSE
              IF l_bgaw006 = '2' THEN    #显示單身，单头隐藏
                 CALL cl_set_comp_visible("bgaz006_1",FALSE)
                 LET l_flag1 =l_flag1+1
              ELSE
                 CALL cl_set_comp_visible("bgaz006",FALSE)
              END IF 
           END IF 
        END IF 
        
        #部门
        IF l_bgaw004 = '2' THEN
           IF l_bgaw005 = 'N' THEN
              CALL cl_set_comp_visible("lbl_bgaz007,bgaz007,bgaz007_1",FALSE)
           ELSE
              IF l_bgaw006 = '2' THEN    #显示單身，单头隐藏
                 CALL cl_set_comp_visible("bgaz007_1",FALSE)
                 LET l_flag1 =l_flag1+1
              ELSE
                 CALL cl_set_comp_visible("bgaz007",FALSE)
              END IF 
           END IF 
        END IF 
        #利润成本中心
        IF l_bgaw004 = '3' THEN
           IF l_bgaw005 = 'N' THEN
              CALL cl_set_comp_visible("bgaz008,bgaz008_1",FALSE)
           ELSE
              IF l_bgaw006 = '2' THEN    #显示單身，单头隐藏
                 CALL cl_set_comp_visible("bgaz008_1",FALSE)
                LET l_flag1 =l_flag1+1
              ELSE
                 CALL cl_set_comp_visible("bgaz008",FALSE)
              END IF 
           END IF 
        END IF  
        #区域
        IF l_bgaw004 = '4' THEN
           IF l_bgaw005 = 'N' THEN
              CALL cl_set_comp_visible("bgaz009,bgaz009_1",FALSE)
           ELSE
              IF l_bgaw006 = '2' THEN    #显示單身，单头隐藏
                 CALL cl_set_comp_visible("bgaz009_1",FALSE)
                 LET l_flag1 =l_flag1+1
              ELSE
                 CALL cl_set_comp_visible("bgaz009",FALSE)
              END IF 
           END IF 
        END IF 
        #交易客商
        IF l_bgaw004 = '5' THEN
           IF l_bgaw005 = 'N' THEN
              CALL cl_set_comp_visible("bgaz010,bgaz010_1",FALSE)
           ELSE
              IF l_bgaw006 = '2' THEN    #显示單身，单头隐藏
                 CALL cl_set_comp_visible("bgaz010_1",FALSE)
                 #LET l_flag5 ='1'
                 LET l_flag1 =l_flag1+1
              ELSE
                 CALL cl_set_comp_visible("bgaz010",FALSE)
              END IF 
           END IF 
        END IF 
        #收款客商
        IF l_bgaw004 = '6' THEN
           IF l_bgaw005 = 'N' THEN
              CALL cl_set_comp_visible("bgaz011,bgaz011_1",FALSE)
           ELSE
              IF l_bgaw006 = '2' THEN    #显示單身，单头隐藏
                 CALL cl_set_comp_visible("bgaz011_1",FALSE)
                 LET l_flag2 =l_flag2+1
              ELSE
                 CALL cl_set_comp_visible("bgaz011",FALSE)
              END IF 
           END IF 
        END IF 
        #客群
        IF l_bgaw004 = '7' THEN
           IF l_bgaw005 = 'N' THEN
              CALL cl_set_comp_visible("bgaz012,bgaz012_1",FALSE)
           ELSE
              IF l_bgaw006 = '2' THEN    #显示單身，单头隐藏
                 CALL cl_set_comp_visible("bgaz012_1",FALSE)
                 LET l_flag2 =l_flag2+1
              ELSE
                 CALL cl_set_comp_visible("bgaz012",FALSE)
              END IF 
           END IF 
        END IF 
        #产品分类
        IF l_bgaw004 = '8' THEN
           IF l_bgaw005 = 'N' THEN
              CALL cl_set_comp_visible("bgaz013,bgaz013_1",FALSE)
           ELSE
              IF l_bgaw006 = '2' THEN    #显示單身，单头隐藏
                 CALL cl_set_comp_visible("bgaz013_1",FALSE)
                 LET l_flag2 =l_flag2+1
              ELSE
                 CALL cl_set_comp_visible("bgaz013",FALSE)
              END IF 
           END IF 
        END IF 
        #人员
        IF l_bgaw004 = '9' THEN
           IF l_bgaw005 = 'N' THEN
              CALL cl_set_comp_visible("bgaz014,bgaz014_1",FALSE)
           ELSE
              IF l_bgaw006 = '2' THEN    #显示單身，单头隐藏
                 CALL cl_set_comp_visible("bgaz014_1",FALSE)
                 LET l_flag2 =l_flag2+1
              ELSE
                 CALL cl_set_comp_visible("bgaz014",FALSE)
              END IF 
           END IF 
        END IF 
        #專案編號
