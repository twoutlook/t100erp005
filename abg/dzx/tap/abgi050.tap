  <section id="abgi050.modify" ver="4" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION abgi050_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_bgad_m.bgad001 IS NULL
   OR g_bgad_m.bgad003 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE abgi050_master_referesh USING g_bgad_m.bgad001,g_bgad_m.bgad003 INTO g_bgad_m.bgad001,g_bgad_m.bgad003, 
       g_bgad_m.bgad001_desc,g_bgad_m.bgad003_desc
 
   ERROR ""
  
   LET g_bgad001_t = g_bgad_m.bgad001
   LET g_bgad003_t = g_bgad_m.bgad003
 
   CALL s_transaction_begin()
   
   OPEN abgi050_cl USING g_enterprise,g_bgad_m.bgad001
                                                       ,g_bgad_m.bgad003
 
   IF STATUS THEN
      CALL cl_err("OPEN abgi050_cl:", STATUS, 1)
      CLOSE abgi050_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH abgi050_cl INTO g_bgad_m.bgad001,g_bgad_m.bgad001_desc,g_bgad_m.bgad003,g_bgad_m.bgad003_desc 

 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_bgad_m.bgad001,SQLCA.sqlcode,0)
      CLOSE abgi050_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL abgi050_show()
   WHILE TRUE
      LET g_bgad001_t = g_bgad_m.bgad001
      LET g_bgad003_t = g_bgad_m.bgad003
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL abgi050_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_bgad_m.* = g_bgad_m_t.*
         CALL abgi050_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_bgad_m.bgad001 != g_bgad001_t 
      OR g_bgad_m.bgad003 != g_bgad003_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE bgad_t SET bgad001 = g_bgad_m.bgad001
                                       , bgad003 = g_bgad_m.bgad003
 
          WHERE bgadent = g_enterprise AND bgad001 = g_bgad001_t
            AND bgad003 = g_bgad003_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("bgad_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("bgad_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_bgad_m.bgad001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE abgi050_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_bgad_m.bgad001,'U')
 
   CALL abgi050_b_fill("1=1")
   
END FUNCTION   
]]>
  </section>
