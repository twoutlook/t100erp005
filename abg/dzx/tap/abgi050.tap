  <section id="abgi050.b_fill" ver="7" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION abgi050_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_bgad_d.clear()
   CALL g_bgad2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE bgad002,bgad004,bgadstus,bgad002,bgadownid,bgadowndp,bgadcrtid,bgadcrtdp, 
       bgadcrtdt,bgadmodid,bgadmoddt,t1.bgael003 ,t2.nmail004 ,t3.oofa011 ,t4.ooefl003 ,t5.oofa011 , 
       t6.ooefl003 ,t7.oofa011 FROM bgad_t",   
               "",
               
                              " LEFT JOIN bgael_t t1 ON t1.bgaelent='"||g_enterprise||"' AND t1.bgael006=bgad001 AND t1.bgael001=bgad002 AND t1.bgael002='"||g_dlang||"' ",
               " LEFT JOIN nmail_t t2 ON t2.nmailent='"||g_enterprise||"' AND t2.nmail001=bgad003 AND t2.nmail002=bgad004 AND t2.nmail003='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t3 ON t3.oofaent='"||g_enterprise||"' AND t3.oofa002='2' AND t3.oofa003=bgadownid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=bgadowndp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t5 ON t5.oofaent='"||g_enterprise||"' AND t5.oofa002='2' AND t5.oofa003=bgadcrtid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent='"||g_enterprise||"' AND t6.ooefl001=bgadcrtdp AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t7 ON t7.oofaent='"||g_enterprise||"' AND t7.oofa002='2' AND t7.oofa003=bgadmodid  ",
 
               " WHERE bgadent= ? AND bgad001=? AND bgad003=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("bgad_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF abgi050_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY bgad_t.bgad002"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE abgi050_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR abgi050_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_bgad_m.bgad001,g_bgad_m.bgad003
                                               
      FOREACH b_fill_cs INTO g_bgad_d[l_ac].bgad002,g_bgad_d[l_ac].bgad004,g_bgad_d[l_ac].bgadstus,g_bgad2_d[l_ac].bgad002, 
          g_bgad2_d[l_ac].bgadownid,g_bgad2_d[l_ac].bgadowndp,g_bgad2_d[l_ac].bgadcrtid,g_bgad2_d[l_ac].bgadcrtdp, 
          g_bgad2_d[l_ac].bgadcrtdt,g_bgad2_d[l_ac].bgadmodid,g_bgad2_d[l_ac].bgadmoddt,g_bgad_d[l_ac].bgad002_desc, 
          g_bgad_d[l_ac].bgad004_desc,g_bgad2_d[l_ac].bgadownid_desc,g_bgad2_d[l_ac].bgadowndp_desc, 
          g_bgad2_d[l_ac].bgadcrtid_desc,g_bgad2_d[l_ac].bgadcrtdp_desc,g_bgad2_d[l_ac].bgadmodid_desc 

                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_bgad2_d[l_ac].bgadownid_desc = cl_get_username(g_bgad2_d[l_ac].bgadownid)
      #LET g_bgad2_d[l_ac].bgadowndp_desc = cl_get_deptname(g_bgad2_d[l_ac].bgadowndp)
      #LET g_bgad2_d[l_ac].bgadcrtid_desc = cl_get_username(g_bgad2_d[l_ac].bgadcrtid)
      #LET g_bgad2_d[l_ac].bgadcrtdp_desc = cl_get_deptname(g_bgad2_d[l_ac].bgadcrtdp)
      #LET g_bgad2_d[l_ac].bgadmodid_desc = cl_get_username(g_bgad2_d[l_ac].bgadmodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_bgad_d.deleteElement(g_bgad_d.getLength())
      CALL g_bgad2_d.deleteElement(g_bgad2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE abgi050_pb   
   
END FUNCTION
]]>
