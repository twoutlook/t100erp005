   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE abgi050_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE bgad001,bgad003,t1.ooall004 ,t2.ooall004",
               " FROM bgad_t",
                              " LEFT JOIN ooall_t t1 ON t1.ooallent='"||g_enterprise||"' AND t1.ooall001='11' AND t1.ooall002=bgad001 AND t1.ooall003='"||g_dlang||"' ",
               " LEFT JOIN ooall_t t2 ON t2.ooallent='"||g_enterprise||"' AND t2.ooall001='8' AND t2.ooall002=bgad003 AND t2.ooall003='"||g_dlang||"' ",
 
               " WHERE bgadent = '" ||g_enterprise|| "' AND bgad001 = ? AND bgad003 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE abgi050_master_referesh FROM g_sql
 
 
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
