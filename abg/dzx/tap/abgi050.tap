  <section id="abgi050.delete" ver="6" status="" src="s">
    <![CDATA[#+ 資料刪除
PRIVATE FUNCTION abgi050_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_bgad_m.bgad001 IS NULL
   OR g_bgad_m.bgad003 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE abgi050_master_referesh USING g_bgad_m.bgad001,g_bgad_m.bgad003 INTO g_bgad_m.bgad001,g_bgad_m.bgad003, 
       g_bgad_m.bgad001_desc,g_bgad_m.bgad003_desc
   
   CALL abgi050_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN abgi050_cl USING g_enterprise,g_bgad_m.bgad001
                                                       ,g_bgad_m.bgad003
 
   IF STATUS THEN
      CALL cl_err("OPEN abgi050_cl:", STATUS, 1)
      CLOSE abgi050_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH abgi050_cl INTO g_bgad_m.bgad001,g_bgad_m.bgad001_desc,g_bgad_m.bgad003,g_bgad_m.bgad003_desc 

   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_bgad_m.bgad001,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL abgi050_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM bgad_t WHERE bgadent = g_enterprise AND bgad001 = g_bgad_m.bgad001
                                                               AND bgad003 = g_bgad_m.bgad003
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("bgad_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_bgad_d.clear() 
      CALL g_bgad2_d.clear()       
 
     
      CALL abgi050_ui_browser_refresh()  
      CALL abgi050_ui_headershow()  
      CALL abgi050_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL abgi050_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL abgi050_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE abgi050_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_bgad_m.bgad001,'D')
    
END FUNCTION
]]>
