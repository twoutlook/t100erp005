      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_abgi050
      
   END IF 
   
   CLOSE abgi050_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
   
END MAIN
 
 
