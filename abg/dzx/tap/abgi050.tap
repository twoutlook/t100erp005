   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT bgad001,'',bgad003,''", 
                      " FROM bgad_t",
                      " WHERE bgadent= ? AND bgad001=? AND bgad003=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
