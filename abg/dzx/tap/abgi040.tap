  <point name="function.abgi040_bgae001_chk" cite_std="N" status="" ver="1" src="s" new="Y" order="1" cite_ver="" mark_hard="N">
<![CDATA[################################################################################
# Descriptions...: 檢查bgae001欄位
# Memo...........: 不能包含+-*/=()
# Usage..........: CALL abgi040_bgae001_chk()
#                  RETURNING TRUE/FALSE
# Input parameter: 無
# Return code....: true/false
# Date & Author..: 14/02/12 By yuhuabao
# Modify.........:
################################################################################
PRIVATE FUNCTION abgi040_bgae001_chk()
DEFINE  l_res LIKE type_t.chr1
DEFINE  l_bgae001 STRING 
    LET l_bgae001 = g_bgae_d[l_ac].bgae001
    LET l_res = TRUE
    IF l_bgae001.getIndexOf('+',1) > 0 OR
       l_bgae001.getIndexOf('-',1) > 0 OR
       l_bgae001.getIndexOf('*',1) > 0 OR
       l_bgae001.getIndexOf('/',1) > 0 OR
       l_bgae001.getIndexOf('=',1) > 0 OR 
       l_bgae001.getIndexOf('(',1) > 0 OR
       l_bgae001.getIndexOf(')',1) > 0 THEN 
       LET l_res =  FALSE
     END IF
    RETURN l_res
END FUNCTION]]>
</point>
