               LET g_errparam.extend = g_bgaa_d[l_ac].bgaa001 
               LET g_errparam.code   = -263 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               LET g_bgaa_d[l_ac].* = g_bgaa_d_t.*
