  <point name="function.abgi120_vaild" order="4" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION abgi120_vaild()
   DEFINE l_bgamstus   LIKE bgam_t.bgamstus
   DEFINE l_vaild      LIKE gzzd_t.gzzd005

   IF cl_null(g_bgam_m.bgam001) THEN RETURN END IF

   SELECT DISTINCT bgamstus INTO l_bgamstus FROM bgam_t
    WHERE bgament = g_enterprise
      AND bgam001 = g_bgam_m.bgam001
      AND bgam002 = g_bgam_m.bgam002

   CALL s_transaction_begin()

   IF l_bgamstus = 'Y' THEN
      IF cl_ask_confirm('abg-00029') THEN
         SELECT gzzd005 INTO l_vaild FROM gzzd_t WHERE gzzd001 = 'abgi030' AND gzzd003 = 'vaild'
         UPDATE bgam_t SET bgamstus = 'N'  WHERE bgament = g_enterprise
            AND bgam001 = g_bgam_m.bgam001
            AND bgam002 = g_bgam_m.bgam002
      END IF
   ELSE
      IF cl_ask_confirm('abg-00028') THEN
         UPDATE bgam_t SET bgamstus = 'Y'  WHERE bgament = g_enterprise
            AND bgam001 = g_bgam_m.bgam001
            AND bgam002 = g_bgam_m.bgam002
      END IF
   END IF
   IF SQLCA.SQLCODE THEN
      CALL s_transaction_end('N','0')
   ELSE
      CALL s_transaction_end('Y','0')
      CALL cl_set_comp_att_text('lbl_vaild',l_vaild)
   END IF
END FUNCTION]]>
