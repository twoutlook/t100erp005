  <section id="abgi120.b_fill" ver="11" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION abgi120_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_bgam_d.clear()
   CALL g_bgam2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE bgam003,bgam004,bgam005,bgam006,bgam007,bgamstus,bgam003,bgamownid,bgamowndp, 
       bgamcrtid,bgamcrtdp,bgamcrtdt,bgammodid,bgammoddt,t1.oofa011 ,t2.ooefl003 ,t3.oofa011 ,t4.ooefl003 , 
       t5.oofa011 FROM bgam_t",   
               "",
               
                              " LEFT JOIN oofa_t t1 ON t1.oofaent='"||g_enterprise||"' AND t1.oofa002='2' AND t1.oofa003=bgamownid  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent='"||g_enterprise||"' AND t2.ooefl001=bgamowndp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t3 ON t3.oofaent='"||g_enterprise||"' AND t3.oofa002='2' AND t3.oofa003=bgamcrtid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=bgamcrtdp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t5 ON t5.oofaent='"||g_enterprise||"' AND t5.oofa002='2' AND t5.oofa003=bgammodid  ",
 
               " WHERE bgament= ? AND bgam001=? AND bgam002=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("bgam_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF abgi120_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY bgam_t.bgam003"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE abgi120_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR abgi120_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_bgam_m.bgam001,g_bgam_m.bgam002
                                               
      FOREACH b_fill_cs INTO g_bgam_d[l_ac].bgam003,g_bgam_d[l_ac].bgam004,g_bgam_d[l_ac].bgam005,g_bgam_d[l_ac].bgam006, 
          g_bgam_d[l_ac].bgam007,g_bgam_d[l_ac].bgamstus,g_bgam2_d[l_ac].bgam003,g_bgam2_d[l_ac].bgamownid, 
          g_bgam2_d[l_ac].bgamowndp,g_bgam2_d[l_ac].bgamcrtid,g_bgam2_d[l_ac].bgamcrtdp,g_bgam2_d[l_ac].bgamcrtdt, 
          g_bgam2_d[l_ac].bgammodid,g_bgam2_d[l_ac].bgammoddt,g_bgam2_d[l_ac].bgamownid_desc,g_bgam2_d[l_ac].bgamowndp_desc, 
          g_bgam2_d[l_ac].bgamcrtid_desc,g_bgam2_d[l_ac].bgamcrtdp_desc,g_bgam2_d[l_ac].bgammodid_desc 

                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_bgam2_d[l_ac].bgamownid_desc = cl_get_username(g_bgam2_d[l_ac].bgamownid)
      #LET g_bgam2_d[l_ac].bgamowndp_desc = cl_get_deptname(g_bgam2_d[l_ac].bgamowndp)
