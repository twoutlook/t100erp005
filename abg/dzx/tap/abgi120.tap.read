  <section id="abgi120.input.body" ver="9" status="" src="s">
    <![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_bgam_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = FALSE,
                  DELETE ROW = FALSE,
                  APPEND ROW = FALSE)
 
         #自訂單身ACTION
         
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_bgam_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL abgi120_b_fill(g_wc2) 
            IF g_rec_b != 0 THEN
               CALL fgl_set_arr_curr(l_ac)
            END IF
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN abgi120_cl USING g_enterprise,
                                               g_bgam_m.bgam001
                                               ,g_bgam_m.bgam002
 
                                               
               IF STATUS THEN
                  CALL cl_err("OPEN abgi120_cl:", STATUS, 1)
                  CLOSE abgi120_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_bgam_d[l_ac].bgam003 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_bgam_d_t.* = g_bgam_d[l_ac].*  #BACKUP
               LET g_bgam_d_o.* = g_bgam_d[l_ac].*  #BACKUP
               CALL abgi120_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL abgi120_set_no_entry_b(l_cmd)
               OPEN abgi120_bcl USING g_enterprise,g_bgam_m.bgam001,
                                                g_bgam_m.bgam002,
 
                                                g_bgam_d_t.bgam003
 
               IF STATUS THEN
                  CALL cl_err("OPEN abgi120_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH abgi120_bcl INTO g_bgam_d[l_ac].bgam003,g_bgam_d[l_ac].bgam004,g_bgam_d[l_ac].bgam005, 
                      g_bgam_d[l_ac].bgam006,g_bgam_d[l_ac].bgam007,g_bgam_d[l_ac].bgamstus
                  IF SQLCA.sqlcode THEN
                      CALL cl_err(g_bgam_d_t.bgam003,SQLCA.sqlcode,1)
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL abgi120_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
        
         BEFORE INSERT
            
            INITIALIZE g_bgam_d_t.* TO NULL
            INITIALIZE g_bgam_d_o.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_bgam_d[l_ac].* TO NULL
            
            #公用欄位預設值
            #此段落由子樣板a14產生    
      LET g_bgam2_d[l_ac].bgamownid = g_user
      LET g_bgam2_d[l_ac].bgamowndp = g_dept
      LET g_bgam2_d[l_ac].bgamcrtid = g_user
      LET g_bgam2_d[l_ac].bgamcrtdp = g_dept 
      LET g_bgam2_d[l_ac].bgamcrtdt = cl_get_current()
      LET g_bgam2_d[l_ac].bgammodid = ""
      LET g_bgam2_d[l_ac].bgammoddt = ""
      LET g_bgam_d[l_ac].bgamstus = ""
 
  
            
            #一般欄位預設值
                  LET g_bgam_d[l_ac].bgamstus = "Y"
 
            
            
            LET g_bgam_d_t.* = g_bgam_d[l_ac].*     #新輸入資料
            LET g_bgam_d_o.* = g_bgam_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL abgi120_set_entry_b(l_cmd)
            #add-point:set_entry_b後
            {<point name="input.body.before_insert.set_entry_b"/>}
            #end add-point
            CALL abgi120_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_bgam_d[li_reproduce_target].* = g_bgam_d[li_reproduce].*
               LET g_bgam2_d[li_reproduce_target].* = g_bgam2_d[li_reproduce].*
 
               LET g_bgam_d[g_bgam_d.getLength()].bgam003 = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
 
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM bgam_t 
             WHERE bgament = g_enterprise AND bgam001 = g_bgam_m.bgam001
               AND bgam002 = g_bgam_m.bgam002
 
               AND bgam003 = g_bgam_d[l_ac].bgam003
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               CALL s_transaction_begin()
               #add-point:單身新增前
               {<point name="input.body.b_insert" mark="Y"/>}
               #end add-point
               INSERT INTO bgam_t
                           (bgament,
                            bgam001,bgam002,bgam008,
                            bgam003
                            ,bgam004,bgam005,bgam006,bgam007,bgamstus,bgamownid,bgamowndp,bgamcrtid,bgamcrtdp,bgamcrtdt,bgammodid,bgammoddt) 
                     VALUES(g_enterprise,
                            g_bgam_m.bgam001,g_bgam_m.bgam002,g_bgam_m.bgam008,
                            g_bgam_d[l_ac].bgam003
                            ,g_bgam_d[l_ac].bgam004,g_bgam_d[l_ac].bgam005,g_bgam_d[l_ac].bgam006,g_bgam_d[l_ac].bgam007, 
                                g_bgam_d[l_ac].bgamstus,g_bgam2_d[l_ac].bgamownid,g_bgam2_d[l_ac].bgamowndp, 
                                g_bgam2_d[l_ac].bgamcrtid,g_bgam2_d[l_ac].bgamcrtdp,g_bgam2_d[l_ac].bgamcrtdt, 
                                g_bgam2_d[l_ac].bgammodid,g_bgam2_d[l_ac].bgammoddt)
               #add-point:單身新增中
               {<point name="input.body.m_insert"/>}
               #end add-point
               LET p_cmd = 'u'
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_bgam_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("bgam_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR "INSERT O.K"
               LET g_rec_b=g_rec_b+1
               DISPLAY g_rec_b TO FORMONLY.cnt
            END IF
            
            #add-point:單身新增後
            {<point name="input.body.after_insert"/>}
            #end add-point
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_bgam_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
