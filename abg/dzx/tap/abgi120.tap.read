  <point name="function.abgi120_load" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#根据单头产生单身资料
PRIVATE FUNCTION abgi120_load(p_bgam001,p_bgam002,p_bgam008)
   DEFINE p_bgam001 LIKE bgam_t.bgam001
   DEFINE p_bgam002 LIKE bgam_t.bgam002
   DEFINE p_bgam008 LIKE bgam_t.bgam008
   DEFINE l_bgam    RECORD LIKE bgam_t.*
   DEFINE ls_sql    STRING 
   DEFINE l_success LIKE type_t.num5

   IF cl_null(p_bgam001) OR cl_null(p_bgam002) OR cl_null(p_bgam008) THEN
      LET l_success = FALSE
      RETURN l_success
   END IF 
   IF NOT cl_ask_confirm('abg-00071') THEN
      LET l_success = FALSE
      RETURN l_success
   END IF 
   LET l_success = FALSE 
   LET l_bgam.bgament = g_enterprise
   LET l_bgam.bgam001 = p_bgam001
   LET l_bgam.bgam002 = p_bgam002
   LET l_bgam.bgam008 = p_bgam008
   LET l_bgam.bgamstus = 'Y'
   LET l_bgam.bgamownid = g_user
   LET l_bgam.bgamowndp = g_dept
   LET l_bgam.bgamcrtid = g_user
   LET l_bgam.bgamcrtdp = g_dept
   LET l_bgam.bgamcrtdt = cl_get_current()
   LET l_bgam.bgammodid = ''
   LET l_bgam.bgammoddt = ''
   LET ls_sql = " SELECT bgac002,glav002,glav005,glav006,glav007 FROM bgac_t ",
                "   LEFT OUTER JOIN glav_t ON glavent = bgacent AND glav001 = '",p_bgam002,"' ",
                "    AND glav002 = '",p_bgam008,"' AND glav004 = bgac002 ",
                "  WHERE bgacent = '",g_enterprise,"' AND bgac001 = '",p_bgam001,"'"
   PREPARE abgi120_load_pre FROM ls_sql
   DECLARE abgi120_load_cs CURSOR FOR abgi120_load_pre
   
   CALL s_transaction_begin()
   DELETE FROM bgam_t WHERE bgament = g_enterprise AND bgam001 = p_bgam001 AND bgam002 = p_bgam002
   IF SQLCA.sqlcode THEN
      CALL s_transaction_end('N','1')
      RETURN 
   END IF
   
   FOREACH abgi120_load_cs INTO l_bgam.bgam003,l_bgam.bgam004,l_bgam.bgam005,l_bgam.bgam006,l_bgam.bgam007
      IF cl_null(l_bgam.bgam004) OR cl_null(l_bgam.bgam005) OR cl_null(l_bgam.bgam006) OR cl_null(l_bgam.bgam007) THEN
         SELECT glav002,glav005,glav006,glav007 
           INTO l_bgam.bgam004,l_bgam.bgam005,l_bgam.bgam006,l_bgam.bgam007
           FROM glav_t WHERE glavent = g_enterprise
            AND glav001 = p_bgam002 AND glav002 = p_bgam008+1 AND glav004 = l_bgam.bgam003
         IF cl_null(l_bgam.bgam004) OR cl_null(l_bgam.bgam005) OR cl_null(l_bgam.bgam006) OR cl_null(l_bgam.bgam007) THEN
            CALL cl_err('','abg-00070',1)
            LET l_success = FALSE 
            EXIT FOREACH
         ELSE
            INSERT INTO bgam_t values(l_bgam.*)
            IF SQLCA.sqlcode THEN
               CALL s_transaction_end('N','1')
               LET l_success = FALSE 
               EXIT FOREACH
            ELSE
               LET l_success = TRUE
            END IF
         END IF     
      ELSE
         INSERT INTO bgam_t values(l_bgam.*)
         IF SQLCA.sqlcode THEN
            CALL s_transaction_end('N','1')
            LET l_success = FALSE 
            EXIT FOREACH
         ELSE
            LET l_success = TRUE
         END IF
      END IF 
   END FOREACH 
   
   IF l_success THEN 
      CALL s_transaction_end('Y','1')
   ELSE
      CALL s_transaction_end('N','0')
   END IF 
   RETURN l_success
END FUNCTION]]>
  </point>
  <point name="function.abgi120_ooefl001_ref" order="2" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION abgi120_ooefl001_ref(p_ooefl001)
   DEFINE p_ooefl001 LIKE ooefl_t.ooefl001
   DEFINE r_ooefl003 LIKE ooefl_t.ooefl003

   LET r_ooefl003  = ''
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = p_ooefl001
   CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET r_ooefl003 = g_rtn_fields[1]
   RETURN r_ooefl003
END FUNCTION]]>
