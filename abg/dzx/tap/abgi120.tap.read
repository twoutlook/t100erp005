   LET ld_date = cl_get_current()
   
   DROP TABLE abgi120_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE abgi120_detail AS ",
                "SELECT * FROM bgam_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO abgi120_detail SELECT * FROM bgam_t 
                                         WHERE bgament = g_enterprise AND bgam001 = g_bgam001_t
                                         AND bgam002 = g_bgam002_t
 
   
