  <section id="abgi120.delete" ver="6" status="" src="s">
    <![CDATA[#+ 資料刪除
PRIVATE FUNCTION abgi120_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_bgam_m.bgam001 IS NULL
   OR g_bgam_m.bgam002 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE abgi120_master_referesh USING g_bgam_m.bgam001,g_bgam_m.bgam002 INTO g_bgam_m.bgam001,g_bgam_m.bgam002, 
       g_bgam_m.bgam008,g_bgam_m.bgam001_desc,g_bgam_m.bgam002_desc
   
   CALL abgi120_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN abgi120_cl USING g_enterprise,g_bgam_m.bgam001
                                                       ,g_bgam_m.bgam002
 
   IF STATUS THEN
      CALL cl_err("OPEN abgi120_cl:", STATUS, 1)
      CLOSE abgi120_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH abgi120_cl INTO g_bgam_m.bgam001,g_bgam_m.bgam001_desc,g_bgam_m.bgam002,g_bgam_m.bgam002_desc, 
       g_bgam_m.bgam008
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_bgam_m.bgam001,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL abgi120_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM bgam_t WHERE bgament = g_enterprise AND bgam001 = g_bgam_m.bgam001
                                                               AND bgam002 = g_bgam_m.bgam002
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("bgam_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_bgam_d.clear() 
      CALL g_bgam2_d.clear()       
 
     
      CALL abgi120_ui_browser_refresh()  
      CALL abgi120_ui_headershow()  
      CALL abgi120_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL abgi120_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL abgi120_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE abgi120_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_bgam_m.bgam001,'D')
    
END FUNCTION
