  <point name="function.abgi120_oofa003_ref" order="3" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION abgi120_oofa003_ref(p_oofa003)
   DEFINE p_oofa003 LIKE oofa_t.oofa003
   DEFINE r_oofa011 LIKE oofa_t.oofa011

   LET r_oofa011  = ''
   INITIALIZE g_ref_fields TO NULL
