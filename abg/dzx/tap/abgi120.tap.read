    <![CDATA[#+ 資料修改
PRIVATE FUNCTION abgi120_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_bgam_m.bgam001 IS NULL
   OR g_bgam_m.bgam002 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE abgi120_master_referesh USING g_bgam_m.bgam001,g_bgam_m.bgam002 INTO g_bgam_m.bgam001,g_bgam_m.bgam002, 
       g_bgam_m.bgam008,g_bgam_m.bgam001_desc,g_bgam_m.bgam002_desc
 
   ERROR ""
  
   LET g_bgam001_t = g_bgam_m.bgam001
   LET g_bgam002_t = g_bgam_m.bgam002
 
   CALL s_transaction_begin()
   
   OPEN abgi120_cl USING g_enterprise,g_bgam_m.bgam001
                                                       ,g_bgam_m.bgam002
 
   IF STATUS THEN
      CALL cl_err("OPEN abgi120_cl:", STATUS, 1)
      CLOSE abgi120_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH abgi120_cl INTO g_bgam_m.bgam001,g_bgam_m.bgam001_desc,g_bgam_m.bgam002,g_bgam_m.bgam002_desc, 
       g_bgam_m.bgam008
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_bgam_m.bgam001,SQLCA.sqlcode,0)
      CLOSE abgi120_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL abgi120_show()
   WHILE TRUE
      LET g_bgam001_t = g_bgam_m.bgam001
      LET g_bgam002_t = g_bgam_m.bgam002
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL abgi120_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_bgam_m.* = g_bgam_m_t.*
         CALL abgi120_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_bgam_m.bgam001 != g_bgam001_t 
      OR g_bgam_m.bgam002 != g_bgam002_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE bgam_t SET bgam001 = g_bgam_m.bgam001
                                       , bgam002 = g_bgam_m.bgam002
 
          WHERE bgament = g_enterprise AND bgam001 = g_bgam001_t
            AND bgam002 = g_bgam002_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("bgam_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("bgam_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_bgam_m.bgam001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE abgi120_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_bgam_m.bgam001,'U')
 
   CALL abgi120_b_fill("1=1")
   
END FUNCTION   
]]>
  </section>
