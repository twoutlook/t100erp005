  <section id="abgi120.detail_reproduce" ver="4" status="" src="s">
    <![CDATA[#+ 單身自動複製
PRIVATE FUNCTION abgi120_detail_reproduce()
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE bgam_t.*
 
 
   #add-point:delete段define
   {<point name="detail_reproduce.define"/>}
