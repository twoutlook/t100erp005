  <section id="abgi090.b_fill" ver="7" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION abgi090_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_bgai_d.clear()
   CALL g_bgai2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE bgai003,bgai004,bgai005,bgai006,bgai007,bgaistus,bgai003,bgai004,bgai005, 
       bgaiownid,bgaiowndp,bgaicrtid,bgaicrtdp,bgaicrtdt,bgaimodid,bgaimoddt,t1.oofa011 ,t2.bgahl006 , 
       t3.bgael003 ,t4.oofa011 ,t5.ooefl003 ,t6.oofa011 ,t7.ooefl003 ,t8.oofa011 FROM bgai_t",   
               "",
               
                              " LEFT JOIN oofa_t t1 ON t1.oofaent='"||g_enterprise||"' AND t1.oofa002='2' AND t1.oofa003=bgai003  ",
               " LEFT JOIN bgahl_t t2 ON t2.bgahlent='"||g_enterprise||"' AND t2.bgahl004=bgai004 AND t2.bgahl005='"||g_dlang||"' ",
               " LEFT JOIN bgael_t t3 ON t3.bgaelent='"||g_enterprise||"' AND t3.bgael001=bgai001 AND t3.bgael006=bgai006 AND t3.bgael002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t4 ON t4.oofaent='"||g_enterprise||"' AND t4.oofa002='2' AND t4.oofa003=bgaiownid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=bgaiowndp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t6 ON t6.oofaent='"||g_enterprise||"' AND t6.oofa002='2' AND t6.oofa003=bgaicrtid  ",
               " LEFT JOIN ooefl_t t7 ON t7.ooeflent='"||g_enterprise||"' AND t7.ooefl001=bgaicrtdp AND t7.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t8 ON t8.oofaent='"||g_enterprise||"' AND t8.oofa002='2' AND t8.oofa003=bgaimodid  ",
 
               " WHERE bgaient= ? AND bgai001=? AND bgai002=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("bgai_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF abgi090_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY bgai_t.bgai003,bgai_t.bgai004,bgai_t.bgai005"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE abgi090_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR abgi090_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_bgai_m.bgai001,g_bgai_m.bgai002
                                               
      FOREACH b_fill_cs INTO g_bgai_d[l_ac].bgai003,g_bgai_d[l_ac].bgai004,g_bgai_d[l_ac].bgai005,g_bgai_d[l_ac].bgai006, 
          g_bgai_d[l_ac].bgai007,g_bgai_d[l_ac].bgaistus,g_bgai2_d[l_ac].bgai003,g_bgai2_d[l_ac].bgai004, 
          g_bgai2_d[l_ac].bgai005,g_bgai2_d[l_ac].bgaiownid,g_bgai2_d[l_ac].bgaiowndp,g_bgai2_d[l_ac].bgaicrtid, 
          g_bgai2_d[l_ac].bgaicrtdp,g_bgai2_d[l_ac].bgaicrtdt,g_bgai2_d[l_ac].bgaimodid,g_bgai2_d[l_ac].bgaimoddt, 
          g_bgai_d[l_ac].bgai003_desc,g_bgai_d[l_ac].bgai004_desc,g_bgai_d[l_ac].bgai006_desc,g_bgai2_d[l_ac].bgaiownid_desc, 
          g_bgai2_d[l_ac].bgaiowndp_desc,g_bgai2_d[l_ac].bgaicrtid_desc,g_bgai2_d[l_ac].bgaicrtdp_desc, 
          g_bgai2_d[l_ac].bgaimodid_desc
                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_bgai2_d[l_ac].bgaiownid_desc = cl_get_username(g_bgai2_d[l_ac].bgaiownid)
      #LET g_bgai2_d[l_ac].bgaiowndp_desc = cl_get_deptname(g_bgai2_d[l_ac].bgaiowndp)
      #LET g_bgai2_d[l_ac].bgaicrtid_desc = cl_get_username(g_bgai2_d[l_ac].bgaicrtid)
      #LET g_bgai2_d[l_ac].bgaicrtdp_desc = cl_get_deptname(g_bgai2_d[l_ac].bgaicrtdp)
      #LET g_bgai2_d[l_ac].bgaimodid_desc = cl_get_username(g_bgai2_d[l_ac].bgaimodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_bgai_d.deleteElement(g_bgai_d.getLength())
      CALL g_bgai2_d.deleteElement(g_bgai2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE abgi090_pb   
   
END FUNCTION
]]>
  </section>
