               LET g_bgai2_d[l_ac].bgaimodid = g_user 
LET g_bgai2_d[l_ac].bgaimoddt = cl_get_current()
 
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
         
               UPDATE bgai_t SET (bgai001,bgai002,bgai003,bgai004,bgai005,bgai006,bgai007,bgaistus,bgaiownid, 
                   bgaiowndp,bgaicrtid,bgaicrtdp,bgaicrtdt,bgaimodid,bgaimoddt) = (g_bgai_m.bgai001, 
