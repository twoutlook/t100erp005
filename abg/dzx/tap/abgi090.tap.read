               CALL abgi090_set_no_entry_b(l_cmd)
               OPEN abgi090_bcl USING g_enterprise,g_bgai_m.bgai001,
                                                g_bgai_m.bgai002,
 
                                                g_bgai_d_t.bgai003
                                                ,g_bgai_d_t.bgai004
                                                ,g_bgai_d_t.bgai005
 
               IF STATUS THEN
                  CALL cl_err("OPEN abgi090_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH abgi090_bcl INTO g_bgai_d[l_ac].bgai003,g_bgai_d[l_ac].bgai004,g_bgai_d[l_ac].bgai005, 
