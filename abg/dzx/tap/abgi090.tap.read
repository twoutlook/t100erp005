  <section id="abgi090.modify" ver="3" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION abgi090_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_bgai_m.bgai001 IS NULL
   OR g_bgai_m.bgai002 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE abgi090_master_referesh USING g_bgai_m.bgai001,g_bgai_m.bgai002 INTO g_bgai_m.bgai001,g_bgai_m.bgai002 

 
   ERROR ""
  
   LET g_bgai001_t = g_bgai_m.bgai001
   LET g_bgai002_t = g_bgai_m.bgai002
 
   CALL s_transaction_begin()
   
   OPEN abgi090_cl USING g_enterprise,g_bgai_m.bgai001
                                                       ,g_bgai_m.bgai002
 
   IF STATUS THEN
      CALL cl_err("OPEN abgi090_cl:", STATUS, 1)
      CLOSE abgi090_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH abgi090_cl INTO g_bgai_m.bgai001,g_bgai_m.bgai002,g_bgai_m.bgail004
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_bgai_m.bgai001,SQLCA.sqlcode,0)
      CLOSE abgi090_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL abgi090_show()
   WHILE TRUE
      LET g_bgai001_t = g_bgai_m.bgai001
      LET g_bgai002_t = g_bgai_m.bgai002
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL abgi090_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_bgai_m.* = g_bgai_m_t.*
         CALL abgi090_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_bgai_m.bgai001 != g_bgai001_t 
      OR g_bgai_m.bgai002 != g_bgai002_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE bgai_t SET bgai001 = g_bgai_m.bgai001
                                       , bgai002 = g_bgai_m.bgai002
 
          WHERE bgaient = g_enterprise AND bgai001 = g_bgai001_t
            AND bgai002 = g_bgai002_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("bgai_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("bgai_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_bgai_m.bgai001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE abgi090_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_bgai_m.bgai001,'U')
 
   CALL abgi090_b_fill("1=1")
   
END FUNCTION   
]]>
  </section>
