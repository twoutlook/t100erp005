  <section id="abgi030.b_fill" ver="7" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION abgi030_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_bgac_d.clear()
   CALL g_bgac2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE bgac002,bgac003,bgac004,bgac005,bgacstus,bgac002,bgac003,bgacownid,bgacowndp, 
       bgaccrtid,bgaccrtdp,bgaccrtdt,bgacmodid,bgacmoddt,t1.ooag011 ,t2.ooefl003 ,t3.ooag011 ,t4.ooefl003 , 
       t5.ooag011 FROM bgac_t",   
               "",
               
                              " LEFT JOIN ooag_t t1 ON t1.ooagent='"||g_enterprise||"' AND t1.ooag001=bgacownid  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent='"||g_enterprise||"' AND t2.ooefl001=bgacowndp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent='"||g_enterprise||"' AND t3.ooag001=bgaccrtid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=bgaccrtdp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent='"||g_enterprise||"' AND t5.ooag001=bgacmodid  ",
 
               " WHERE bgacent= ? AND bgac001=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("bgac_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF abgi030_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY bgac_t.bgac002"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE abgi030_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR abgi030_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_bgac_m.bgac001
                                               
      FOREACH b_fill_cs INTO g_bgac_d[l_ac].bgac002,g_bgac_d[l_ac].bgac003,g_bgac_d[l_ac].bgac004,g_bgac_d[l_ac].bgac005, 
          g_bgac_d[l_ac].bgacstus,g_bgac2_d[l_ac].bgac002,g_bgac2_d[l_ac].bgac003,g_bgac2_d[l_ac].bgacownid, 
          g_bgac2_d[l_ac].bgacowndp,g_bgac2_d[l_ac].bgaccrtid,g_bgac2_d[l_ac].bgaccrtdp,g_bgac2_d[l_ac].bgaccrtdt, 
          g_bgac2_d[l_ac].bgacmodid,g_bgac2_d[l_ac].bgacmoddt,g_bgac2_d[l_ac].bgacownid_desc,g_bgac2_d[l_ac].bgacowndp_desc, 
          g_bgac2_d[l_ac].bgaccrtid_desc,g_bgac2_d[l_ac].bgaccrtdp_desc,g_bgac2_d[l_ac].bgacmodid_desc 

                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_bgac2_d[l_ac].bgacownid_desc = cl_get_username(g_bgac2_d[l_ac].bgacownid)
      #LET g_bgac2_d[l_ac].bgacowndp_desc = cl_get_deptname(g_bgac2_d[l_ac].bgacowndp)
      #LET g_bgac2_d[l_ac].bgaccrtid_desc = cl_get_username(g_bgac2_d[l_ac].bgaccrtid)
      #LET g_bgac2_d[l_ac].bgaccrtdp_desc = cl_get_deptname(g_bgac2_d[l_ac].bgaccrtdp)
      #LET g_bgac2_d[l_ac].bgacmodid_desc = cl_get_username(g_bgac2_d[l_ac].bgacmodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_bgac_d.deleteElement(g_bgac_d.getLength())
      CALL g_bgac2_d.deleteElement(g_bgac2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE abgi030_pb   
   
END FUNCTION
