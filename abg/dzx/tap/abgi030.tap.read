   END IF  
 
   CALL cl_set_head_visible("","YES")  
 
   LET l_insert = FALSE
   LET g_action_choice = ""
 
   #add-point:input段define_sql
   {<point name="input.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT bgac002,bgac003,bgac004,bgac005,bgacstus,bgac002,bgac003,bgacownid,bgacowndp, 
       bgaccrtid,bgaccrtdp,bgaccrtdt,bgacmodid,bgacmoddt FROM bgac_t WHERE bgacent=? AND bgac001=? AND  
       bgac002=? FOR UPDATE"
   #add-point:input段define_sql
   {<point name="input.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
