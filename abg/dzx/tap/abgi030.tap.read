  <point name="function.abgi030_get_date" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#+ 錄入預算週期開始、結束日期，計算出日期對應的季別、期別、周別
PRIVATE FUNCTION abgi030_get_date()
   DEFINE l_cnt      LIKE type_t.num5
   DEFINE l_sql      STRING
   
   #開啟畫面
   OPEN WINDOW w_abgi030_s01 WITH FORM cl_ap_formpath("abg","abgi030_s01")
   
      DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
      
         #錄入開始
         INPUT BY NAME g_bgac_s.l_date1,g_bgac_s.l_date2,g_bgac_s.l_date3  ATTRIBUTE(WITHOUT DEFAULTS)
            
            BEFORE INPUT
               LET g_bgac_s.l_date1 = Null
               LET g_bgac_s.l_date2 = Null
               LET g_bgac_s.l_date3 = Null
               
            AFTER FIELD l_date1
               IF NOT cl_null(g_bgac_s.l_date1) THEN
                  IF g_bgac_s.l_date1 > g_bgac_s.l_date2 AND NOT cl_null(g_bgac_s.l_date2) THEN
                     CALL cl_err(g_bgac_s.l_date1,'amm-00080',1)
                     NEXT FIELD l_date1
                  END IF
                  IF g_bgac_s.l_date1 < g_bgac_s.l_date3 AND NOT cl_null(g_bgac_s.l_date3) THEN
                     CALL cl_err(g_bgac_s.l_date1,'abg-00031',1)
                     NEXT FIELD l_date1
                  END IF
               END IF
            AFTER FIELD l_date2
               IF NOT cl_null(g_bgac_s.l_date2) THEN
                  IF g_bgac_s.l_date1 > g_bgac_s.l_date2 AND NOT cl_null(g_bgac_s.l_date1) THEN
                     CALL cl_err(g_bgac_s.l_date2,'amm-00081',1)
                     NEXT FIELD l_date2
                  END IF
               END IF
            AFTER FIELD l_date3
               IF NOT cl_null(g_bgac_s.l_date3) THEN
                  IF g_bgac_s.l_date1 < g_bgac_s.l_date3 AND NOT cl_null(g_bgac_s.l_date1) THEN
                     CALL cl_err(g_bgac_s.l_date1,'abg-00031',1)
                     NEXT FIELD l_date3
                  END IF
               END IF
         
         END INPUT
         
         ON ACTION accept
            ACCEPT DIALOG

         ON ACTION cancel
            LET INT_FLAG = TRUE
            EXIT DIALOG

         ON ACTION close
            LET INT_FLAG = TRUE
            EXIT DIALOG

         ON ACTION exit
            LET INT_FLAG = TRUE
            EXIT DIALOG

      END DIALOG
      IF INT_FLAG THEN
         LET INT_FLAG = 0
      ELSE
         LET l_cnt = 0
         SELECT COUNT(*) INTO l_cnt FROM bgac_t 
          WHERE bgacent = g_enterprise
            AND bgac001 = g_bgac_m.bgac001
            AND bgac002 BETWEEN g_bgac_s.l_date1 AND g_bgac_s.l_date2
         IF cl_null(l_cnt) THEN LET l_cnt = 0 END IF
         IF l_cnt > 1 THEN
            IF cl_ask_confirm('abg-00032') THEN      
               CALL abgi030_get_datadetail()
            END IF
         ELSE
            CALL abgi030_get_datadetail()
         END IF
      END IF
   
   #畫面關閉
   CLOSE WINDOW w_abgi030_s01
      
END FUNCTION]]>
  </point>
  <point name="function.abgi030_get_datadetail" order="2" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#+ 計算出符合條件的會計期間資料
PRIVATE FUNCTION abgi030_get_datadetail()
DEFINE l_date     LIKE type_t.dat    #計算日期
DEFINE l_date_s   LIKE type_t.dat    #實際開始日期
DEFINE l_date_e   LIKE type_t.dat    #實際結束日期
DEFINE l_week     LIKE type_t.num5   #周别
DEFINE l_ww       LIKE type_t.num5   #星期
DEFINE l_quarter  DYNAMIC ARRAY OF LIKE type_t.dat  #季度
DEFINE l_month    DYNAMIC ARRAY OF LIKE type_t.dat  #期別
DEFINE l_year_3   LIKE type_t.num5     #實際開始年度
DEFINE l_month_3  LIKE type_t.num5     #實際開始月份
DEFINE l_day_3    LIKE type_t.num5     #實際開始日期
DEFINE l_i        LIKE type_t.num5     #期別
DEFINE l_m        LIKE type_t.num5     #季别
DEFINE l_flag     LIKE type_t.chr1

   LET l_date_s = g_bgac_s.l_date3
   #實際結束日期 = 實際開始日期 + 1年 - 一天
   LET l_year_3 = YEAR(g_bgac_s.l_date3)
   LET l_month_3= MONTH(g_bgac_s.l_date3)
   LET l_day_3  = DAY(g_bgac_s.l_date3)
   LET l_date_e = MDY(l_month_3,l_day_3,l_year_3 + 1) - 1
   
   #計算季度、期別
   CASE
      WHEN g_bgac_m.bgac006 = '1'
         IF l_month_3 + 1 > 12 THEN LET l_year_3 = l_year_3 + 1 LET l_month_3 = 1 END IF
         LET l_month[1] = MDY(l_month_3 + 1,l_day_3,l_year_3) - 1   #第一期結束日期
         IF l_month_3 + 2 > 12 THEN LET l_year_3 = l_year_3 + 1 LET l_month_3 = l_month_3 + 2 - 12 END IF
         LET l_month[2] = MDY(l_month_3 + 2,l_day_3,l_year_3) - 1   #第二期結束日期
         IF l_month_3 + 3 > 12 THEN LET l_year_3 = l_year_3 + 1 LET l_month_3 = l_month_3 + 3 - 12 END IF
         LET l_month[3] = MDY(l_month_3 + 3,l_day_3,l_year_3) - 1   #第三期結束日期
         IF l_month_3 + 4 > 12 THEN LET l_year_3 = l_year_3 + 1 LET l_month_3 = l_month_3 + 4 - 12 END IF
         LET l_month[4] = MDY(l_month_3 + 4,l_day_3,l_year_3) - 1   #第四期結束日期
         IF l_month_3 + 5 > 12 THEN LET l_year_3 = l_year_3 + 1 LET l_month_3 = l_month_3 + 5 - 12 END IF
         LET l_month[5] = MDY(l_month_3 + 5,l_day_3,l_year_3) - 1   #第五期結束日期
         IF l_month_3 + 6 > 12 THEN LET l_year_3 = l_year_3 + 1 LET l_month_3 = l_month_3 + 6 - 12 END IF
         LET l_month[6] = MDY(l_month_3 + 6,l_day_3,l_year_3) - 1   #第六期結束日期
         IF l_month_3 + 7 > 12 THEN LET l_year_3 = l_year_3 + 1 LET l_month_3 = l_month_3 + 7 - 12 END IF
         LET l_month[7] = MDY(l_month_3 + 7,l_day_3,l_year_3) - 1   #第七期結束日期
         IF l_month_3 + 8 > 12 THEN LET l_year_3 = l_year_3 + 1 LET l_month_3 = l_month_3 + 8 - 12 END IF
         LET l_month[8] = MDY(l_month_3 + 8,l_day_3,l_year_3) - 1   #第八期結束日期
         IF l_month_3 + 9 > 12 THEN LET l_year_3 = l_year_3 + 1 LET l_month_3 = l_month_3 + 9 - 12 END IF
         LET l_month[9] = MDY(l_month_3 + 9,l_day_3,l_year_3) - 1   #第九期結束日期
         IF l_month_3 +10 > 12 THEN LET l_year_3 = l_year_3 + 1 LET l_month_3 = l_month_3 +10 - 12 END IF
         LET l_month[10]= MDY(l_month_3 +10,l_day_3,l_year_3) - 1   #第十期結束日期
         IF l_month_3 +11 > 12 THEN LET l_year_3 = l_year_3 + 1 LET l_month_3 = l_month_3 +11 - 12 END IF
         LET l_month[11]= MDY(l_month_3 +11,l_day_3,l_year_3) - 1   #第十一期結束日期
         LET l_month[12]= MDY(l_month_3,l_day_3,l_year_3 + 1) - 1   #第十二期結束日期
         
         IF l_month_3 + 3 > 12 THEN LET l_year_3 = l_year_3 + 1 LET l_month_3 = l_month_3 + 3 - 12 END IF
         LET l_quarter[1] = MDY(l_month_3 + 3,l_day_3,l_year_3) - 1 #第一季結束日期
         IF l_month_3 + 6 > 12 THEN LET l_year_3 = l_year_3 + 1 LET l_month_3 = l_month_3 + 6 - 12 END IF
         LET l_quarter[2] = MDY(l_month_3 + 6,l_day_3,l_year_3) - 1 #第二季結束日期
         IF l_month_3 + 9 > 12 THEN LET l_year_3 = l_year_3 + 1 LET l_month_3 = l_month_3 + 9 - 12 END IF
         LET l_quarter[3] = MDY(l_month_3 + 9,l_day_3,l_year_3) - 1 #第三季結束日期
         LET l_quarter[4] = MDY(l_month_3,l_day_3,l_year_3 + 1) - 1 #第四季結束日期
      WHEN g_bgac_m.bgac006 = '2'
         LET l_month[1] = g_bgac_s.l_date3 + 28 - 1   #第一期結束日期
         LET l_month[2] = g_bgac_s.l_date3 + 56 - 1   #第二期結束日期
         LET l_month[3] = g_bgac_s.l_date3 + 84 - 1   #第三期結束日期
         LET l_month[4] = g_bgac_s.l_date3 +112 - 1   #第四期結束日期
         LET l_month[5] = g_bgac_s.l_date3 +140 - 1   #第五期結束日期
         LET l_month[6] = g_bgac_s.l_date3 +168 - 1   #第六期結束日期
         LET l_month[7] = g_bgac_s.l_date3 +196 - 1   #第七期結束日期
         LET l_month[8] = g_bgac_s.l_date3 +224 - 1   #第八期結束日期
         LET l_month[9] = g_bgac_s.l_date3 +252 - 1   #第九期結束日期
         LET l_month[10]= g_bgac_s.l_date3 +280 - 1   #第十期結束日期
         LET l_month[11]= g_bgac_s.l_date3 +308 - 1   #第十一期結束日期
         LET l_month[12]= g_bgac_s.l_date3 +336 - 1   #第十二期結束日期
         LET l_month[12]= MDY(l_month_3,l_day_3,l_year_3 + 1) - 1          #第十三期結束日期
         LET l_quarter[1] = g_bgac_s.l_date3 + 84 - 1 #第一季結束日期
         LET l_quarter[2] = g_bgac_s.l_date3 +168 - 1 #第二季結束日期
         LET l_quarter[3] = g_bgac_s.l_date3 +252 - 1 #第三季結束日期
         LET l_quarter[4] = MDY(l_month_3,l_day_3,l_year_3 + 1) - 1        #第四季結束日期
      WHEN g_bgac_m.bgac006 = '3'
         LET l_month[1] = g_bgac_s.l_date3 + 28 - 1   #第一期結束日期
         LET l_month[2] = g_bgac_s.l_date3 + 56 - 1   #第二期結束日期
         LET l_month[3] = g_bgac_s.l_date3 + 91 - 1   #第三期結束日期
         LET l_month[4] = g_bgac_s.l_date3 +119 - 1   #第四期結束日期
         LET l_month[5] = g_bgac_s.l_date3 +147 - 1   #第五期結束日期
         LET l_month[6] = g_bgac_s.l_date3 +182 - 1   #第六期結束日期
         LET l_month[7] = g_bgac_s.l_date3 +210 - 1   #第七期結束日期
         LET l_month[8] = g_bgac_s.l_date3 +238 - 1   #第八期結束日期
         LET l_month[9] = g_bgac_s.l_date3 +273 - 1   #第九期結束日期
         LET l_month[10]= g_bgac_s.l_date3 +301 - 1   #第十期結束日期
         LET l_month[11]= g_bgac_s.l_date3 +329 - 1   #第十一期結束日期
         LET l_month[12]= MDY(l_month_3,l_day_3,l_year_3 + 1) - 1         #第十二期結束日期
         LET l_quarter[1] = g_bgac_s.l_date3 + 91 - 1 #第一季結束日期
         LET l_quarter[2] = g_bgac_s.l_date3 +182 - 1 #第二季結束日期
         LET l_quarter[3] = g_bgac_s.l_date3 +273 - 1 #第三季結束日期
         LET l_quarter[4] = MDY(l_month_3,l_day_3,l_year_3 + 1) - 1        #第四季結束日期
   END CASE

   LET l_week = 0
   LET l_date = l_date_s

   LET l_flag = 'Y'
   CALL s_transaction_begin()
   
   DELETE FROM bgac_t WHERE bgacent = g_enterprise AND bgac001 = g_bgac_m.bgac001

   WHILE l_date <= l_date_e
     #計算周別
      #計算當前是星期几
      LET l_ww = WEEKDAY(l_date)
      #第一週、星期一時 周別加一
      IF l_ww = 0 OR l_week = 0 THEN LET l_week = l_week + 1 END IF
      IF l_week >52 THEN LET l_week = 52 END IF
      #計算日期小於開始日期，繼續循環
      IF l_date < g_bgac_s.l_date1 THEN
         LET l_date = l_date + 1
         CONTINUE WHILE
      END IF
      #計算日期所屬月份
      FOR l_i = 1 TO 13
         IF l_date <= l_month[l_i] THEN
            EXIT FOR
         END IF
      END FOR
      LET l_m = 1
      IF l_i >= 4  THEN LET l_m = 2 END IF
      IF l_i >= 7  THEN LET l_m = 3 END IF
      IF l_i >= 10 THEN LET l_m = 4 END IF
      
      INSERT INTO bgac_t (bgacent,bgac001,bgac006,
                          bgac002,bgac003,bgac004,bgac005,
                          bgacstus,bgacownid,bgacowndp,bgaccrtid,bgaccrtdp,bgaccrtdt,bgacmodid,bgacmoddt) 
                     VALUES(g_enterprise,g_bgac_m.bgac001,g_bgac_m.bgac006,
                            l_date,l_m,l_i,l_week,
                            'Y',g_user,g_dept,g_user,g_dept,TODAY,g_user,TODAY)
      
      IF SQLCA.SQLcode THEN
         LET l_flag = 'N'
         EXIT WHILE
      END IF
      #計算日期大於等於結束日期，跳出循環
      IF l_date >= g_bgac_s.l_date2 THEN
         EXIT WHILE
      ELSE
         LET l_date = l_date + 1
      END IF
   END WHILE
   
   IF l_flag = 'Y' THEN
      CALL s_transaction_end('Y','0')
   ELSE
      CALL s_transaction_end('N','0')
   END IF
   
END FUNCTION]]>
  </point>
