  <section id="abgi030.input.body" ver="7" status="" src="s">
    <![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_bgac_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂單身ACTION
         
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_bgac_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL abgi030_b_fill(g_wc2) 
            IF g_rec_b != 0 THEN
               CALL fgl_set_arr_curr(l_ac)
            END IF
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN abgi030_cl USING g_enterprise,
                                               g_bgac_m.bgac001
 
                                               
               IF STATUS THEN
                  CALL cl_err("OPEN abgi030_cl:", STATUS, 1)
                  CLOSE abgi030_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_bgac_d[l_ac].bgac002 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_bgac_d_t.* = g_bgac_d[l_ac].*  #BACKUP
               LET g_bgac_d_o.* = g_bgac_d[l_ac].*  #BACKUP
               CALL abgi030_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL abgi030_set_no_entry_b(l_cmd)
               OPEN abgi030_bcl USING g_enterprise,g_bgac_m.bgac001,
 
                                                g_bgac_d_t.bgac002
 
               IF STATUS THEN
                  CALL cl_err("OPEN abgi030_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH abgi030_bcl INTO g_bgac_d[l_ac].bgac002,g_bgac_d[l_ac].bgac003,g_bgac_d[l_ac].bgac004, 
                      g_bgac_d[l_ac].bgac005,g_bgac_d[l_ac].bgacstus,g_bgac2_d[l_ac].bgac002,g_bgac2_d[l_ac].bgac003, 
                      g_bgac2_d[l_ac].bgacownid,g_bgac2_d[l_ac].bgacowndp,g_bgac2_d[l_ac].bgaccrtid, 
                      g_bgac2_d[l_ac].bgaccrtdp,g_bgac2_d[l_ac].bgaccrtdt,g_bgac2_d[l_ac].bgacmodid, 
                      g_bgac2_d[l_ac].bgacmoddt
                  IF SQLCA.sqlcode THEN
                      CALL cl_err(g_bgac_d_t.bgac002,SQLCA.sqlcode,1)
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL abgi030_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
        
         BEFORE INSERT
            
            INITIALIZE g_bgac_d_t.* TO NULL
            INITIALIZE g_bgac_d_o.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_bgac_d[l_ac].* TO NULL
            
            #公用欄位預設值
            #此段落由子樣板a14產生    
      LET g_bgac2_d[l_ac].bgacownid = g_user
      LET g_bgac2_d[l_ac].bgacowndp = g_dept
      LET g_bgac2_d[l_ac].bgaccrtid = g_user
      LET g_bgac2_d[l_ac].bgaccrtdp = g_dept 
      LET g_bgac2_d[l_ac].bgaccrtdt = cl_get_current()
      LET g_bgac2_d[l_ac].bgacmodid = ""
      LET g_bgac2_d[l_ac].bgacmoddt = ""
      LET g_bgac_d[l_ac].bgacstus = ""
 
  
            
            #一般欄位預設值
                  LET g_bgac_d[l_ac].bgacstus = "Y"
 
            
            
            LET g_bgac_d_t.* = g_bgac_d[l_ac].*     #新輸入資料
            LET g_bgac_d_o.* = g_bgac_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL abgi030_set_entry_b(l_cmd)
            #add-point:set_entry_b後
            {<point name="input.body.before_insert.set_entry_b"/>}
            #end add-point
            CALL abgi030_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_bgac_d[li_reproduce_target].* = g_bgac_d[li_reproduce].*
               LET g_bgac2_d[li_reproduce_target].* = g_bgac2_d[li_reproduce].*
 
               LET g_bgac_d[g_bgac_d.getLength()].bgac002 = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
 
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM bgac_t 
             WHERE bgacent = g_enterprise AND bgac001 = g_bgac_m.bgac001
 
               AND bgac002 = g_bgac_d[l_ac].bgac002
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               CALL s_transaction_begin()
               #add-point:單身新增前
               {<point name="input.body.b_insert" mark="Y"/>}
               #end add-point
               INSERT INTO bgac_t
                           (bgacent,
                            bgac001,bgac006,
                            bgac002
                            ,bgac003,bgac004,bgac005,bgacstus,bgac003,bgacownid,bgacowndp,bgaccrtid,bgaccrtdp,bgaccrtdt,bgacmodid,bgacmoddt) 
                     VALUES(g_enterprise,
                            g_bgac_m.bgac001,g_bgac_m.bgac006,
                            g_bgac_d[l_ac].bgac002
                            ,g_bgac_d[l_ac].bgac003,g_bgac_d[l_ac].bgac004,g_bgac_d[l_ac].bgac005,g_bgac_d[l_ac].bgacstus, 
                                g_bgac_d[l_ac].bgac003,g_bgac2_d[l_ac].bgacownid,g_bgac2_d[l_ac].bgacowndp, 
                                g_bgac2_d[l_ac].bgaccrtid,g_bgac2_d[l_ac].bgaccrtdp,g_bgac2_d[l_ac].bgaccrtdt, 
                                g_bgac2_d[l_ac].bgacmodid,g_bgac2_d[l_ac].bgacmoddt)
               #add-point:單身新增中
               {<point name="input.body.m_insert"/>}
               #end add-point
               LET p_cmd = 'u'
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_bgac_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("bgac_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
