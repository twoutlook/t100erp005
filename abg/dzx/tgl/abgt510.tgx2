   LET g_forupd_sql = " SELECT bgeg002,'',bgeg003,bgeg004,'','','',bgeg011,bgeg001,bgeg005,bgeg006,bgeg010, 
       bgegstus,bgeg007,'',bgeg013,'',bgeg016,'',bgeg019,'',bgeg022,bgeg009,bgeg014,'',bgeg017,'',bgeg020, 
