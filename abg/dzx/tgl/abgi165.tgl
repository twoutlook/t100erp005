   IF g_bgas_m.bgas001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code = "std-00003" 
      LET g_errparam.popup = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_bgas001_t = g_bgas_m.bgas001
 
   CALL s_transaction_begin()
   
   OPEN abgi165_cl USING g_enterprise,g_bgas_m.bgas001
   IF SQLCA.SQLCODE THEN   #(ver:78)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN abgi165_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE   #(ver:78)
      LET g_errparam.popup = TRUE 
      CLOSE abgi165_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE abgi165_master_referesh USING g_bgas_m.bgas001 INTO g_bgas_m.bgas002,g_bgas_m.bgas003,g_bgas_m.bgas001, 
       g_bgas_m.bgas009,g_bgas_m.bgas004,g_bgas_m.bgas005,g_bgas_m.bgas008,g_bgas_m.bgas010,g_bgas_m.bgasstus, 
       g_bgas_m.bgasownid,g_bgas_m.bgasowndp,g_bgas_m.bgascrtid,g_bgas_m.bgascrtdp,g_bgas_m.bgascrtdt, 
       g_bgas_m.bgasmodid,g_bgas_m.bgasmoddt,g_bgas_m.bgas110,g_bgas_m.bgas111,g_bgas_m.bgas112,g_bgas_m.bgas113, 
       g_bgas_m.bgas114,g_bgas_m.bgas115,g_bgas_m.bgas210,g_bgas_m.bgas211,g_bgas_m.bgas212,g_bgas_m.bgas213, 
       g_bgas_m.bgas214,g_bgas_m.bgas215,g_bgas_m.bgas216,g_bgas_m.bgas217,g_bgas_m.bgas218,g_bgas_m.bgas219, 
       g_bgas_m.bgas310,g_bgas_m.bgas311,g_bgas_m.bgas312,g_bgas_m.bgas313,g_bgas_m.bgas314,g_bgas_m.bgas315, 
       g_bgas_m.bgas410,g_bgas_m.bgas411,g_bgas_m.bgas412,g_bgas_m.bgas414,g_bgas_m.bgas415,g_bgas_m.bgas413, 
       g_bgas_m.bgas416,g_bgas_m.bgas417,g_bgas_m.bgas418,g_bgas_m.bgas419,g_bgas_m.bgas420,g_bgas_m.bgas421, 
       g_bgas_m.bgas422,g_bgas_m.bgas423,g_bgas_m.bgasownid_desc,g_bgas_m.bgasowndp_desc,g_bgas_m.bgascrtid_desc, 
       g_bgas_m.bgascrtdp_desc,g_bgas_m.bgasmodid_desc
   
   #檢查是否允許此動作
   IF NOT abgi165_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
