       g_bgas_m.bgasmodid,g_bgas_m.bgasmoddt,g_bgas_m.bgas110,g_bgas_m.bgas111,g_bgas_m.bgas112,g_bgas_m.bgas113, 
       g_bgas_m.bgas114,g_bgas_m.bgas115,g_bgas_m.bgas210,g_bgas_m.bgas211,g_bgas_m.bgas212,g_bgas_m.bgas213, 
