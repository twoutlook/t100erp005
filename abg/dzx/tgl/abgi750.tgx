{<section id="abgi750.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION abgi750_b_fill(p_wc2)
   #add-point:b_fill段define name="b_fill.define_customerization"
   {<point name="b_fill.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE p_wc2      STRING
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   {<point name="b_fill.define"/>}
   #end add-point     
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   {<point name="b_fill.pre_function"/>}
   #end add-point
   
   #先清空單身變數內容
   CALL g_bggc_d.clear()
   CALL g_bggc2_d.clear()

 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  DISTINCT bggc004,bggc005,bggc006,bggc004,bggcownid,bggcowndp,bggccrtid,bggccrtdp,bggccrtdt,bggcmodid,bggcmoddt,t2.bgall005 ,t3.bgall005 ,t4.ooag011 ,t5.ooefl003 ,t6.ooag011 ,t7.ooefl003 ,t8.ooag011 FROM bggc_t",   
               "",
               
                              " LEFT JOIN bgall_t t2 ON t2.bgallent="||g_enterprise||" AND t2.bgall001=bggc001 AND t2.bgall002=bggc002 AND t2.bgall003=bggc005 AND t2.bgall004='"||g_dlang||"' ",
               " LEFT JOIN bgall_t t3 ON t3.bgallent="||g_enterprise||" AND t3.bgall001=bggc001 AND t3.bgall002=bggc002 AND t3.bgall003=bggc006 AND t3.bgall004='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t4 ON t4.ooagent="||g_enterprise||" AND t4.ooag001=bggcownid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent="||g_enterprise||" AND t5.ooefl001=bggcowndp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent="||g_enterprise||" AND t6.ooag001=bggccrtid  ",
               " LEFT JOIN ooefl_t t7 ON t7.ooeflent="||g_enterprise||" AND t7.ooefl001=bggccrtdp AND t7.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t8 ON t8.ooagent="||g_enterprise||" AND t8.ooag001=bggcmodid  ",

               " WHERE bggcent= ? AND bggc001=? AND bggc002=? AND bggc003=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("bggc_t")
   END IF
   
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF abgi750_fill_chk(1) THEN
      IF (g_action_choice = "query" OR cl_null(g_action_choice))
         #add-point:b_fill段long_sql_if name="b_fill.long_sql_if"
         {<point name="b_fill.long_sql_if"/>}
         #end add-point
      THEN
         LET g_sql = g_sql, " ORDER BY bggc_t.bggc004"
         #add-point:b_fill段fill_before name="b_fill.fill_before"
         {<point name="b_fill.fill_before"/>}
         #end add-point
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE abgi750_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR abgi750_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
   #  OPEN b_fill_cs USING g_enterprise,g_bggc_m.bggc001,g_bggc_m.bggc002,g_bggc_m.bggc003   #(ver:49)
                                               
      FOREACH b_fill_cs USING g_enterprise,g_bggc_m.bggc001,g_bggc_m.bggc002,g_bggc_m.bggc003 INTO g_bggc_d[l_ac].bggc004,g_bggc_d[l_ac].bggc005,g_bggc_d[l_ac].bggc006,g_bggc2_d[l_ac].bggc004,g_bggc2_d[l_ac].bggcownid,g_bggc2_d[l_ac].bggcowndp,g_bggc2_d[l_ac].bggccrtid,g_bggc2_d[l_ac].bggccrtdp,g_bggc2_d[l_ac].bggccrtdt,g_bggc2_d[l_ac].bggcmodid,g_bggc2_d[l_ac].bggcmoddt,g_bggc_d[l_ac].bggc005_desc,g_bggc_d[l_ac].bggc006_desc,g_bggc2_d[l_ac].bggcownid_desc,g_bggc2_d[l_ac].bggcowndp_desc,g_bggc2_d[l_ac].bggccrtid_desc,g_bggc2_d[l_ac].bggccrtdp_desc,g_bggc2_d[l_ac].bggcmodid_desc   #(ver:49)
                             
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充 name="b_fill.fill"
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #應用 a12 樣板自動產生(Version:4)




        
         #add-point:單身資料抓取 name="bfill.foreach"
         {<point name="bfill.foreach"/>}
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF 
            EXIT FOREACH
         END IF
      
         LET l_ac = l_ac + 1
         
      END FOREACH
 
            CALL g_bggc_d.deleteElement(g_bggc_d.getLength())
      CALL g_bggc2_d.deleteElement(g_bggc2_d.getLength())

      
   END IF
   
   #add-point:b_fill段more name="b_fill.more"
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_bggc_d.getLength()
      LET g_bggc_d_mask_o[l_ac].* =  g_bggc_d[l_ac].*
      CALL abgi750_bggc_t_mask()
      LET g_bggc_d_mask_n[l_ac].* =  g_bggc_d[l_ac].*
   END FOR
   
   LET g_bggc2_d_mask_o.* =  g_bggc2_d.*
   FOR l_ac = 1 TO g_bggc2_d.getLength()
      LET g_bggc2_d_mask_o[l_ac].* =  g_bggc2_d[l_ac].*
      CALL abgi750_bggc_t_mask()
      LET g_bggc2_d_mask_n[l_ac].* =  g_bggc2_d[l_ac].*
   END FOR

 
   FREE abgi750_pb   
   
END FUNCTION
