   DECLARE abgi750_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.bggc001,t0.bggc003,t0.bggc002,t1.bgaal003 ,t2.bggbl004 ,t4.ooefl003",
               " FROM bggc_t t0",
                              " LEFT JOIN bgaal_t t1 ON t1.bgaalent="||g_enterprise||" AND t1.bgaal001=t0.bggc001 AND t1.bgaal002='"||g_dlang||"' ",
               " LEFT JOIN bggbl_t t2 ON t2.bggblent="||g_enterprise||" AND t2.bggbl001='0' AND t2.bggbl002=t0.bggc003 AND t2.bggbl003='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent="||g_enterprise||" AND t4.ooefl001=t0.bggc002 AND t4.ooefl002='"||g_dlang||"' ",

               " WHERE t0.bggcent = " ||g_enterprise|| " AND t0.bggc001 = ? AND t0.bggc002 = ? AND t0.bggc003 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define name="main.after_refresh_sql"
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE abgi750_master_referesh FROM g_sql
 
