{<section id="abgi750.modify" >}
#+ 資料修改
PRIVATE FUNCTION abgi750_modify()
   #add-point:modify段define name="modify.define_customerization"
   {<point name="modify.define_customerization" edit="c" mark="Y"/>}
   #end add-point    
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   {<point name="modify.define"/>}
   #end add-point  
   
   #add-point:Function前置處理  name="modify.pre_function"
   {<point name="modify.pre_function"/>}
   #end add-point
   
   IF g_bggc_m.bggc001 IS NULL
   OR g_bggc_m.bggc002 IS NULL
   OR g_bggc_m.bggc003 IS NULL

   THEN
      INITIALIZE g_errparam TO NULL  
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
   LET g_bggc001_t = g_bggc_m.bggc001
   LET g_bggc002_t = g_bggc_m.bggc002
   LET g_bggc003_t = g_bggc_m.bggc003

   CALL s_transaction_begin()
   
   OPEN abgi750_cl USING g_enterprise,g_bggc_m.bggc001,g_bggc_m.bggc002,g_bggc_m.bggc003
   IF SQLCA.SQLCODE THEN   #(ver:49)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN abgi750_cl:" 
      LET g_errparam.code   = SQLCA.SQLCODE   #(ver:49)
      LET g_errparam.popup  = TRUE 
      CLOSE abgi750_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE abgi750_master_referesh USING g_bggc_m.bggc001,g_bggc_m.bggc002,g_bggc_m.bggc003 INTO g_bggc_m.bggc001,g_bggc_m.bggc003,g_bggc_m.bggc002,g_bggc_m.bggc001_desc,g_bggc_m.bggc003_desc,g_bggc_m.bggc002_desc
   
   #遮罩相關處理
   LET g_bggc_m_mask_o.* =  g_bggc_m.*
   CALL abgi750_bggc_t_mask()
   LET g_bggc_m_mask_n.* =  g_bggc_m.*
   
   CALL s_transaction_end('Y','0')
 
   CALL abgi750_show()
   WHILE TRUE
      LET g_bggc001_t = g_bggc_m.bggc001
      LET g_bggc002_t = g_bggc_m.bggc002
      LET g_bggc003_t = g_bggc_m.bggc003

 
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL abgi750_input("u")     #欄位更改
      
      #add-point:modify段修改後 name="modify.after_input"
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         LET g_bggc_m.* = g_bggc_m_t.*
         CALL abgi750_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更(更新單身table的key欄位值)
      IF g_bggc_m.bggc001 != g_bggc001_t 
      OR g_bggc_m.bggc002 != g_bggc002_t 
      OR g_bggc_m.bggc003 != g_bggc003_t 

      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前 name="modify.b_key_update"
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #add-point:單頭(偽)修改中 name="modify.m_key_update"
         {<point name="modify.m_key_update"/>}
         #end add-point
         

         
         #add-point:單頭(偽)修改後 name="modify.a_key_update"
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("modify,modify_detail,delete,reproduce", TRUE)
   CALL abgi750_set_act_visible()
   CALL abgi750_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " bggcent = " ||g_enterprise|| " AND",
                      " bggc001 = '", g_bggc_m.bggc001, "' "
                      ," AND bggc002 = '", g_bggc_m.bggc002, "' "
                      ," AND bggc003 = '", g_bggc_m.bggc003, "' "

   #填到對應位置
   CALL abgi750_browser_fill("")
 
   CALL abgi750_idx_chk()
 
   CLOSE abgi750_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL abgi750_msgcentre_notify('modify')
   
END FUNCTION   
