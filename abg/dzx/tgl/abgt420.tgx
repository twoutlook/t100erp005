   LET g_forupd_sql = " SELECT bgde001,'',bgde002,'','',bgde003,bgde005,bgde006,'',bgde100,bgde101", 
                      " FROM bgde_t",
                      " WHERE bgdeent= ? AND bgde001=? AND bgde002=? FOR UPDATE"
   #add-point:SQL_define name="main.after_define_sql"
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE abgt420_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.bgde001,t0.bgde002,t0.bgde003,t0.bgde005,t0.bgde006,t0.bgde100,t0.bgde101,t1.bgaal003 ,t2.ooefl003 ,t3.xcatl003",
               " FROM bgde_t t0",
                              " LEFT JOIN bgaal_t t1 ON t1.bgaalent="||g_enterprise||" AND t1.bgaal001=t0.bgde001 AND t1.bgaal002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent="||g_enterprise||" AND t2.ooefl001=t0.bgde002 AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN xcatl_t t3 ON t3.xcatlent="||g_enterprise||" AND t3.xcatl001=t0.bgde006 AND t3.xcatl002='"||g_dlang||"' ",

               " WHERE t0.bgdeent = " ||g_enterprise|| " AND t0.bgde001 = ? AND t0.bgde002 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define name="main.after_refresh_sql"
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE abgt420_master_referesh FROM g_sql
 
   #該樣板不需此段落#add-point:main段define_sql name="main.body.define_sql"
   #該樣板不需此段落{<point name="main.body.define_sql" mark="Y"/>}
   #該樣板不需此段落#end add-point 
   #該樣板不需此段落LET g_forupd_sql = "SELECT bgde004,bgde008,bgde007,bgde902a,bgde902b,bgde902d,bgde902e,bgde902f,bgde902g,bgde902h,bgde902c,bgde902,bgdestus,bgde004,bgdeownid,bgdeowndp,bgdecrtid,bgdecrtdp,bgdecrtdt,bgdemodid,bgdemoddt,bgdecnfid,bgdecnfdt,bgdepstid,bgdepstdt FROM bgde_t WHERE bgdeent=? AND bgde001=? AND bgde002=? AND bgde004=? AND bgde008=? FOR UPDATE"
   #該樣板不需此段落#add-point:main段define_sql name="main.body.after_define_sql"
   #該樣板不需此段落{<point name="main.body.after_define_sql"/>}
   #該樣板不需此段落#end add-point 
   #該樣板不需此段落LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   #該樣板不需此段落LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   #該樣板不需此段落DECLARE abgt420_bcl CURSOR FROM g_forupd_sql
    

   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call name="main.servicecall"
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_abgt420 WITH FORM cl_ap_formpath("abg",g_code)
   
