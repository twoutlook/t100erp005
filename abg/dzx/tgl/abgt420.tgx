            {<point name="b_fill.body2.long_sql_if"/>}
            #end add-point
         THEN
                  CALL g_bgde3_d.clear()

            LET g_sql = "SELECT  DISTINCT bgdf008,bgdf004,bgdf005,bgdf009,bgdf007,bgdf100,bgdf902a,bgdf902b,bgdf902d,bgdf902e,bgdf902f,bgdf902g,bgdf902h,bgdf902c,bgdf902 FROM bgdf_t",    
