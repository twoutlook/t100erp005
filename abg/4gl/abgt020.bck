               "    AND bgaw001 = '",p_bgaw001,"' "
   PREPARE sel_bgawp1 FROM l_sql
   DECLARE sel_bgawc1 CURSOR FOR sel_bgawp1

   FOREACH sel_bgawc1 INTO l_bgaw.*
