   LET g_bgbh007_t = NULL   #albireo 160126 #160129-00015#1
   CALL abgt020_construct_visible('c')
   #end add-point 
   
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
      
      #單頭
      CONSTRUCT BY NAME g_wc ON bgbh001,bgbh002,bgbh003,bgbh004,bgbh005,bgbh007,bgbh006,bgbhstus,bgbhownid, 
          bgbhowndp,bgbhcrtid,bgbhcrtdp,bgbhcrtdt,bgbhmodid,bgbhmoddt,bgbhcnfid,bgbhcnfdt
 
         BEFORE CONSTRUCT
            #add-point:cs段before_construct name="cs.head.before_construct"
            
            #end add-point 
            
         #公用欄位開窗相關處理
         #應用 a11 樣板自動產生(Version:3)
         #共用欄位查詢處理  
         ##----<<bgbhcrtdt>>----
         AFTER FIELD bgbhcrtdt
