################################################################################
# Descriptions...: 預算編號,預算項目,預算組織檢核
# Memo...........:
# Usage..........: 
# Date & Author..: 150724 By albireo
# Modify.........:
################################################################################
PRIVATE FUNCTION abgt020_bgaa_bgae_bgaj_chk(p_bgaa001,p_bgae001,p_bgaj002)
   DEFINE p_bgaa001   LIKE bgaa_t.bgaa001
   DEFINE p_bgae001   LIKE bgae_t.bgae001
   DEFINE p_bgaj002   LIKE bgaj_t.bgaj002
   DEFINE r_errno     LIKE gzze_t.gzze001
   DEFINE r_success   LIKE type_t.num5
   
   LET r_errno = NULL   LET r_success = TRUE
   IF NOT cl_null(p_bgaa001)THEN
      #單獨檢查預算編號
      CALL s_abg_bgaa001_chk(p_bgaa001)RETURNING g_sub_success,g_errno
      IF NOT g_sub_success THEN
         LET r_success = FALSE
         LET r_errno = g_errno
         RETURN r_success,r_errno
      END IF
   END IF
     
   IF NOT cl_null(p_bgaj002)THEN
      #單獨檢查組織
     # CALL s_abg_ooef001_chk(p_bgaj002)
   END IF
   
   IF NOT cl_null(p_bgae001) AND NOT cl_null(p_bgaa001)THEN
      IF g_bgaa.bgaa012 = 'Y' THEN
         IF NOT cl_null(p_bgaj002)THEN
            #科目檢合
            CALL s_aap_glac002_chk(p_bgae001,g_glaald) RETURNING g_sub_success,g_errno
            IF NOT g_sub_success THEN
               LET r_success = FALSE
               LET r_errno  = g_errno
               RETURN r_success,r_errno
            END IF         
         END IF
      ELSE
         #預算項目必須存在預算編號中的預算項目參照表中
         CALL s_abg_bgae001_chk(p_bgaa001,'',p_bgae001,'1')
            RETURNING g_sub_success,g_errno
         IF NOT g_sub_success THEN
            LET r_success = FALSE
            LET r_errno  = g_errno
            RETURN r_success,r_errno
         END IF                 
      END IF
      #要有可預設樣表
      CALL s_abg_bgay_exist_chk(p_bgaa001,p_bgae001)
         RETURNING g_sub_success,g_errno
      IF NOT g_sub_success THEN
         LET r_success = FALSE
         LET r_errno  = g_errno
         RETURN r_success,r_errno
      END IF            
   END IF
   
   #檢查組織跟預算編號
   IF NOT cl_null(p_bgaa001) AND NOT cl_null(p_bgaj002)THEN
      CALL s_abg_cre_bg_licence_chk(p_bgaa001,p_bgaj002,g_user)
         RETURNING g_sub_success,g_errno
      IF NOT g_sub_success THEN
         LET r_success = FALSE
         LET r_errno  = g_errno
         RETURN r_success,r_errno
      END IF     
   END IF
   
   #檢查abgi100存在並有效
   IF NOT cl_null(p_bgaj002) AND NOT cl_null(p_bgaa001)
      AND NOT cl_null(p_bgae001)THEN
      CALL s_abg_bgai_chk(p_bgaa001,p_bgaj002,p_bgae001)
         RETURNING g_sub_success,g_errno
      IF NOT g_sub_success THEN
         LET r_success = FALSE
         LET r_errno = g_errno
         RETURN r_success,r_errno
      END IF
   END IF
   
   RETURN r_success,r_errno
END FUNCTION

################################################################################
# Descriptions...: 預算編號幣別轉換匯率
# Memo...........:
# Usage..........: 
# Date & Author..: 150727 By albireo
# Modify.........:
################################################################################
PRIVATE FUNCTION abgt020_get_rate(p_bgaa001,p_dat,p_curr)
   DEFINE p_bgaa001   LIKE bgaa_t.bgaa001
   DEFINE p_dat       LIKE type_t.dat
   DEFINE p_curr      LIKE ooai_t.ooai001
   DEFINE r_rate         LIKE bgav_t.bgav004      
   DEFINE r_success      LIKE type_t.num5         
   DEFINE l_bgaa004      LIKE bgaa_t.bgaa004      #
   DEFINE l_bgaa003      LIKE bgaa_t.bgaa003      #預算幣別
   
   LET r_success = TRUE
   LET r_rate = 1
   #抓取預算編號的幣別
   SELECT bgaa003,bgaa004 INTO l_bgaa003,l_bgaa004 FROM bgaa_t
    WHERE bgaaent = g_enterprise
      AND bgaa001 = p_bgaa001
   
   SELECT bgav004 INTO r_rate FROM bgav_t
    WHERE bgavent = g_enterprise
      AND bgav001 = p_bgaa001
      AND bgav002 = l_bgaa003
      AND bgav005 = p_curr
      AND bgav003 = (SELECT max(bgav003) FROM bgav_t WHERE bgavent = g_enterprise
                                                        AND bgav001 = p_bgaa001
                                                        AND bgav003 <=p_dat)
      AND bgavstus = 'Y'
   IF cl_null(r_rate) THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'abg-00027'
      LET g_errparam.extend = p_dat
      LET g_errparam.popup = FALSE
      CALL cl_err()
      RETURN r_success,r_rate
   END IF
   
   RETURN r_success,r_rate   
END FUNCTION

