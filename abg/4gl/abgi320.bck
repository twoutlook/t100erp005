{<section id="abgi320.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION abgi320_b_fill(p_wc2)
   #add-point:b_fill段define name="b_fill.define_customerization"
   
   #end add-point
   DEFINE p_wc2      STRING
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   
   #end add-point     
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   #先清空單身變數內容
   CALL g_bgcb_d.clear()
   CALL g_bgcb2_d.clear()
 
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   
   #end add-point
   
   LET g_sql = "SELECT  DISTINCT bgcb005,bgcb006,bgcb007,bgcb005,bgcbownid,bgcbowndp,bgcbcrtid,bgcbcrtdp, 
       bgcbcrtdt,bgcbmodid,bgcbmoddt,t1.ooag011 ,t2.ooefl003 ,t3.ooag011 ,t4.ooefl003 ,t5.ooag011 FROM bgcb_t", 
          
               "",
               
                              " LEFT JOIN ooag_t t1 ON t1.ooagent="||g_enterprise||" AND t1.ooag001=bgcbownid  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent="||g_enterprise||" AND t2.ooefl001=bgcbowndp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent="||g_enterprise||" AND t3.ooag001=bgcbcrtid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent="||g_enterprise||" AND t4.ooefl001=bgcbcrtdp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=bgcbmodid  ",
 
               " WHERE bgcbent= ? AND bgcb001=? AND bgcb002=? AND bgcb003=? AND bgcb004=? AND bgcb009=? AND bgcb010=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("bgcb_t")
   END IF
   
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF abgi320_fill_chk(1) THEN
      IF (g_action_choice = "query" OR cl_null(g_action_choice))
         #add-point:b_fill段long_sql_if name="b_fill.long_sql_if"
         
         #end add-point
      THEN
         LET g_sql = g_sql, " ORDER BY bgcb_t.bgcb005"
         #add-point:b_fill段fill_before name="b_fill.fill_before"
         
         #end add-point
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE abgi320_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR abgi320_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
   #  OPEN b_fill_cs USING g_enterprise,g_bgcb_m.bgcb001,g_bgcb_m.bgcb002,g_bgcb_m.bgcb003,g_bgcb_m.bgcb004,g_bgcb_m.bgcb009,g_bgcb_m.bgcb010   #(ver:49)
                                               
      FOREACH b_fill_cs USING g_enterprise,g_bgcb_m.bgcb001,g_bgcb_m.bgcb002,g_bgcb_m.bgcb003,g_bgcb_m.bgcb004, 
          g_bgcb_m.bgcb009,g_bgcb_m.bgcb010 INTO g_bgcb_d[l_ac].bgcb005,g_bgcb_d[l_ac].bgcb006,g_bgcb_d[l_ac].bgcb007, 
          g_bgcb2_d[l_ac].bgcb005,g_bgcb2_d[l_ac].bgcbownid,g_bgcb2_d[l_ac].bgcbowndp,g_bgcb2_d[l_ac].bgcbcrtid, 
          g_bgcb2_d[l_ac].bgcbcrtdp,g_bgcb2_d[l_ac].bgcbcrtdt,g_bgcb2_d[l_ac].bgcbmodid,g_bgcb2_d[l_ac].bgcbmoddt, 
          g_bgcb2_d[l_ac].bgcbownid_desc,g_bgcb2_d[l_ac].bgcbowndp_desc,g_bgcb2_d[l_ac].bgcbcrtid_desc, 
          g_bgcb2_d[l_ac].bgcbcrtdp_desc,g_bgcb2_d[l_ac].bgcbmodid_desc   #(ver:49)
                             
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充 name="b_fill.fill"
         
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #應用 a12 樣板自動產生(Version:4)
 
 
 
 
        
         #add-point:單身資料抓取 name="bfill.foreach"
         
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF 
            EXIT FOREACH
         END IF
      
         LET l_ac = l_ac + 1
         
      END FOREACH
 
            CALL g_bgcb_d.deleteElement(g_bgcb_d.getLength())
      CALL g_bgcb2_d.deleteElement(g_bgcb2_d.getLength())
 
      
   END IF
   
   #add-point:b_fill段more name="b_fill.more"
   
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_bgcb_d.getLength()
      LET g_bgcb_d_mask_o[l_ac].* =  g_bgcb_d[l_ac].*
      CALL abgi320_bgcb_t_mask()
      LET g_bgcb_d_mask_n[l_ac].* =  g_bgcb_d[l_ac].*
   END FOR
   
   LET g_bgcb2_d_mask_o.* =  g_bgcb2_d.*
   FOR l_ac = 1 TO g_bgcb2_d.getLength()
      LET g_bgcb2_d_mask_o[l_ac].* =  g_bgcb2_d[l_ac].*
      CALL abgi320_bgcb_t_mask()
      LET g_bgcb2_d_mask_n[l_ac].* =  g_bgcb2_d[l_ac].*
   END FOR
 
 
   FREE abgi320_pb   
   
END FUNCTION
 
