                          FROM s_browse[1].b_bgcb001,s_browse[1].b_bgcb002,s_browse[1].b_bgcb003,s_browse[1].b_bgcb004, 
                              s_browse[1].b_bgcb009,s_browse[1].b_bgcb010
 
