################################################################################
# Descriptions...: 檢核預算料件
# Usage..........: CALL abgi320_chk_bgcb008(p_bgcb008)
# Input parameter: p_bgcb008 預算料件
# Date & Author..: #160927-00007#2 161018 By TSD.james
################################################################################
PRIVATE FUNCTION abgi320_chk_bgcb008(p_bgcb008)
   DEFINE p_bgcb008     LIKE bgcb_t.bgcb008
   DEFINE l_bgeastus    LIKE bgea_t.bgeastus
   
   LET g_errno = NULL
   LET l_bgeastus = NULL
   SELECT bgeastus
     INTO l_bgeastus
     FROM bgea_t
    WHERE bgeaent = g_enterprise
      AND bgea001 = g_bgcb_m.bgcb001
      AND bgea002 = g_bgcb_m.bgcb002
      AND bgea003 = p_bgcb008
      
   CASE
      #預算編號+預算組織+預算料號需存在abgi170
      WHEN SQLCA.SQLCODE = 100
         LET g_errno = 'abg-00167'  

      #需有效
      WHEN l_bgeastus='N'
         LET g_errno = 'aap-00118'

      OTHERWISE
         LET g_errno = SQLCA.SQLCODE USING '-------'
   END CASE 
END FUNCTION

################################################################################
# Descriptions...: 預算組織檢核
# Usage..........: CALL abgi320_chk_bgcb002(p_bgcb002)
#                  RETURNING r_success,r_errno
# Input parameter: p_bgcb002 預算組織
# Return code....: r_success 檢核成功否
#                : r_errno 錯誤訊息代碼
################################################################################
PRIVATE FUNCTION abgi320_chk_bgcb002(p_bgcb002)
   DEFINE p_bgcb002   LIKE bgcb_t.bgcb002
   DEFINE r_success   LIKE type_t.num5
   DEFINE r_errno     LIKE gzze_t.gzze001
   DEFINE l_ooef      RECORD
                      ooef205  LIKE ooef_t.ooef205,
                      ooefstus LIKE ooef_t.ooefstus
                      END RECORD

   LET r_success = TRUE
   LET r_errno   = ''
   INITIALIZE l_ooef.* TO NULL
   SELECT ooef205,ooefstus
     INTO l_ooef.*
     FROM ooef_t
    WHERE ooefent = g_enterprise
      AND ooef001 = p_bgcb002

   CASE
      WHEN SQLCA.SQLCODE = 100
         LET r_success = FALSE
         LET r_errno = 'aoo-00094'
      WHEN l_ooef.ooefstus <> 'Y'
         LET r_success = FALSE
         LET r_errno = 'apm-00249'
      WHEN l_ooef.ooef205 <> 'Y'
         LET r_success = FALSE
         LET r_errno = 'axm-00159'
   END CASE

   RETURN r_success,r_errno
END FUNCTION

 
