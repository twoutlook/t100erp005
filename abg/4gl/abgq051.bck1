               LET g_qryparam.where = " glfa002 = '1' AND glfa016 = '1' " #161003-00014#13 add glfa016 = '1'
            ELSE
               LET g_qryparam.where = " glfa002 = '2' AND glfa016 = '1' " #161003-00014#13 add glfa016 = '1'
            END IF
            CALL q_glfa001()                          #呼叫開窗
            LET  g_input.l_glfa001 = g_qryparam.return1
            DISPLAY  g_input.l_glfa001 TO l_glfa001  #顯示到畫面上
            NEXT FIELD l_glfa001 
            
         ON ACTION controlp INFIELD l_bgbc003
            #161129-00019#10 --s mark
            ##161006-00005#23  add----s
            #CALL s_fin_abg_center_sons_query(g_input.l_bgbc001,'','')
            #CALL s_fin_account_center_sons_str() RETURNING l_orga  
