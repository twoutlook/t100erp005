                                   l_chk,l_year,l_curry,l_glfa009,l_glfa008,
                                   l_strmon,assest,glfb0031,amt1,amt2,             
                                   amt3,amt4,item_desc,glfb0032,amt5,             
                                   amt6,amt7,amt8)
                           VALUES (l_x01_tmp.bgbc001_desc,l_x01_tmp.l_bgbc002,l_x01_tmp.l_glfa001,l_x01_tmp.bgbc003_desc,l_x01_tmp.l_type,      
                                   l_x01_tmp.l_chk,l_x01_tmp.l_year,l_x01_tmp.l_curry,l_x01_tmp.l_glfa009,l_x01_tmp.l_glfa008,   
                                   l_x01_tmp.l_strmon,l_x01_tmp.assest,l_x01_tmp.glfb0031,l_x01_tmp.amt1,l_x01_tmp.amt2,         
                                   l_x01_tmp.amt3,l_x01_tmp.amt4,l_x01_tmp.item_desc,l_x01_tmp.glfb0032,l_x01_tmp.amt5,         
                                   l_x01_tmp.amt6,l_x01_tmp.amt7,l_x01_tmp.amt8)
      #161108-00017#5   add---e
   END FOR

