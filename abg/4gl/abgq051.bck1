       amt5            LIKE type_t.num20_6, 
       amt6            LIKE type_t.num20_6, 
       amt7            LIKE type_t.num20_6, 
       amt8            LIKE type_t.num20_6       
            END RECORD
   DEFINE l_i LIKE type_t.num5
   
   DEFINE l_bgbc001_desc  LIKE type_t.chr100
   DEFINE l_bgbc002       LIKE bgbc_t.bgbc002
   DEFINE l_glfa001       LIKE glfa_t.glfa001
   DEFINE l_bgbc003_desc  LIKE type_t.chr100
   DEFINE l_type          LIKE type_t.chr100
   DEFINE l_chk           LIKE type_t.chr100 #含追加挪用金額
   DEFINE l_year          LIKE xrea_t.xrea001
   DEFINE l_curry         LIKE type_t.chr100
   DEFINE l_glfa009       LIKE glfa_t.glfa009
   DEFINE l_glfa008       LIKE type_t.chr100
   DEFINE l_strmon        LIKE type_t.chr100
     
   LET l_bgbc001_desc = g_input.l_bgbc001,':',g_input.bgbc001_desc
   LET l_bgbc002      = g_input.l_bgbc002
   LET l_glfa001      = g_input.l_glfa001
   LET l_bgbc003_desc = g_input.l_bgbc003,':',g_input.bgbc003_desc
   CASE g_input.l_type
      WHEN 0
         SELECT gzzd005 INTO l_type
           FROM gzzd_t 
          WHERE gzzd003 = 'cbo_l_type.1' AND gzzd002 = g_dlang AND gzzd001 = 'abgq051'
      WHEN 1
         SELECT gzzd005 INTO l_type
           FROM gzzd_t 
          WHERE gzzd003 = 'cbo_l_type.2' AND gzzd002 = g_dlang AND gzzd001 = 'abgq051'
      WHEN 2
         SELECT gzzd005 INTO l_type
           FROM gzzd_t 
          WHERE gzzd003 = 'cbo_l_type.3' AND gzzd002 = g_dlang AND gzzd001 = 'abgq051'
   END CASE 
   LET l_chk     = g_input.l_chk
   LET l_year    = g_input.l_year 
   CASE g_input.l_curry 
      WHEN 0
         SELECT gzzd005 INTO l_curry
           FROM gzzd_t 
          WHERE gzzd003 = 'cbo_l_curry.1' AND gzzd002 = g_dlang AND gzzd001 = 'abgq051'
      WHEN 1
      SELECT gzzd005 INTO l_curry
           FROM gzzd_t 
          WHERE gzzd003 = 'cbo_l_curry.2' AND gzzd002 = g_dlang AND gzzd001 = 'abgq051'
   END CASE
   LET l_curry = l_curry,':',g_input.curry_desc             
   LET l_glfa008 = s_desc_gzcbl004_desc('8705',g_input.l_glfa008)   
   LET l_glfa009 = g_input.l_glfa009
   IF g_prog MATCHES 'abgq051' THEN
      LET l_strmon = g_input.l_strmon
   ELSE
      LET l_strmon = g_input.l_strmon||'-'||g_input.l_endmon
   END IF   
   DELETE FROM l_x01_tmp
   FOR l_i = 1 TO g_bgbc_d.getLength()
      INITIALIZE l_x01_tmp.* TO NULL 
      LET l_x01_tmp.bgbc001_desc=  l_bgbc001_desc
      LET l_x01_tmp.l_bgbc002   =  l_bgbc002   
      LET l_x01_tmp.l_glfa001   =  l_glfa001   
      LET l_x01_tmp.bgbc003_desc=  l_bgbc003_desc
      LET l_x01_tmp.l_type      =  l_type      
      LET l_x01_tmp.l_chk       =  l_chk       
      LET l_x01_tmp.l_year      =  l_year      
      LET l_x01_tmp.l_curry     =  l_curry     
      LET l_x01_tmp.l_glfa009   =  l_glfa009   
      LET l_x01_tmp.l_glfa008   =  l_glfa008   
      LET l_x01_tmp.l_strmon    =  l_strmon    
      LET l_x01_tmp.assest      = g_bgbc_d[l_i].assest
      LET l_x01_tmp.glfb0031    = g_bgbc_d[l_i].glfb0031
      LET l_x01_tmp.amt1        = g_bgbc_d[l_i].amt1    
      LET l_x01_tmp.amt2        = g_bgbc_d[l_i].amt2    
      LET l_x01_tmp.amt3        = g_bgbc_d[l_i].amt3    
      LET l_x01_tmp.amt4        = g_bgbc_d[l_i].amt4    
      LET l_x01_tmp.item_desc   = g_bgbc_d[l_i].item_desc
      LET l_x01_tmp.glfb0032    = g_bgbc_d[l_i].glfb0032
      LET l_x01_tmp.amt5        = g_bgbc_d[l_i].amt5    
      LET l_x01_tmp.amt6        = g_bgbc_d[l_i].amt6    
      LET l_x01_tmp.amt7        = g_bgbc_d[l_i].amt7    
      LET l_x01_tmp.amt8        = g_bgbc_d[l_i].amt8     
      #INSERT INTO abgq051_x01_tmp VALUES(l_x01_tmp.*)   #161108-00017#5   mark
      #161108-00017#5   add---s
