{<section id="abgi150.state_change" >}
   #應用 a09 樣板自動產生(Version:17)
#+ 確認碼變更 
PRIVATE FUNCTION abgi150_statechange()
   #add-point:statechange段define(客製用) name="statechange.define_customerization"
   
   #end add-point  
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="statechange.define"
   
   #end add-point  
   
   #add-point:Function前置處理 name="statechange.before"
   
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_bgap_m.bgap001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN abgi150_cl USING g_enterprise,g_bgap_m.bgap001
   IF STATUS THEN
      CLOSE abgi150_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN abgi150_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE abgi150_master_referesh USING g_bgap_m.bgap001 INTO g_bgap_m.bgap003,g_bgap_m.bgap001,g_bgap_m.bgap002, 
       g_bgap_m.bgap005,g_bgap_m.bgap004,g_bgap_m.bgapstus,g_bgap_m.bgapownid,g_bgap_m.bgapowndp,g_bgap_m.bgapcrtid, 
       g_bgap_m.bgapcrtdp,g_bgap_m.bgapcrtdt,g_bgap_m.bgapmodid,g_bgap_m.bgapmoddt,g_bgap_m.bgapownid_desc, 
       g_bgap_m.bgapowndp_desc,g_bgap_m.bgapcrtid_desc,g_bgap_m.bgapcrtdp_desc,g_bgap_m.bgapmodid_desc 
 
   
 
   #檢查是否允許此動作
   IF NOT abgi150_action_chk() THEN
      CLOSE abgi150_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_bgap_m.bgap003,g_bgap_m.bgap003_desc,g_bgap_m.bgap001,g_bgap_m.bgap002,g_bgap_m.bgapl005, 
       g_bgap_m.bgapl003,g_bgap_m.bgap005,g_bgap_m.bgap004,g_bgap_m.bgapstus,g_bgap_m.bgapownid,g_bgap_m.bgapownid_desc, 
       g_bgap_m.bgapowndp,g_bgap_m.bgapowndp_desc,g_bgap_m.bgapcrtid,g_bgap_m.bgapcrtid_desc,g_bgap_m.bgapcrtdp, 
       g_bgap_m.bgapcrtdp_desc,g_bgap_m.bgapcrtdt,g_bgap_m.bgapmodid,g_bgap_m.bgapmodid_desc,g_bgap_m.bgapmoddt 
 
 
   CASE g_bgap_m.bgapstus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/open.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/valid.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_bgap_m.bgapstus
            
            WHEN "N"
               HIDE OPTION "open"
            WHEN "Y"
               HIDE OPTION "valid"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      
      #end add-point
      
      
	  
      ON ACTION open
         IF cl_auth_chk_act("open") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.open"
            
            #end add-point
         END IF
         EXIT MENU
      ON ACTION valid
         IF cl_auth_chk_act("valid") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.valid"
            
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      ) OR 
      g_bgap_m.bgapstus = lc_state OR cl_null(lc_state) THEN
      CLOSE abgi150_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   #確認
   IF lc_state = 'Y' THEN
      CALL abgi150_conf_upd_chk() RETURNING g_sub_success,g_errno
      IF NOT g_sub_success THEN  #依交易類型,確認單身是否有值
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "conf_error" 
         LET g_errparam.code   = g_errno
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         RETURN
      END IF
   END IF
   #end add-point
   
   LET g_bgap_m.bgapmodid = g_user
   LET g_bgap_m.bgapmoddt = cl_get_current()
   LET g_bgap_m.bgapstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE bgap_t 
      SET (bgapstus,bgapmodid,bgapmoddt) 
        = (g_bgap_m.bgapstus,g_bgap_m.bgapmodid,g_bgap_m.bgapmoddt)     
    WHERE bgapent = g_enterprise AND bgap001 = g_bgap_m.bgap001
 
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/open.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/valid.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE abgi150_master_referesh USING g_bgap_m.bgap001 INTO g_bgap_m.bgap003,g_bgap_m.bgap001, 
          g_bgap_m.bgap002,g_bgap_m.bgap005,g_bgap_m.bgap004,g_bgap_m.bgapstus,g_bgap_m.bgapownid,g_bgap_m.bgapowndp, 
          g_bgap_m.bgapcrtid,g_bgap_m.bgapcrtdp,g_bgap_m.bgapcrtdt,g_bgap_m.bgapmodid,g_bgap_m.bgapmoddt, 
          g_bgap_m.bgapownid_desc,g_bgap_m.bgapowndp_desc,g_bgap_m.bgapcrtid_desc,g_bgap_m.bgapcrtdp_desc, 
          g_bgap_m.bgapmodid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_bgap_m.bgap003,g_bgap_m.bgap003_desc,g_bgap_m.bgap001,g_bgap_m.bgap002,g_bgap_m.bgapl005, 
          g_bgap_m.bgapl003,g_bgap_m.bgap005,g_bgap_m.bgap004,g_bgap_m.bgapstus,g_bgap_m.bgapownid,g_bgap_m.bgapownid_desc, 
          g_bgap_m.bgapowndp,g_bgap_m.bgapowndp_desc,g_bgap_m.bgapcrtid,g_bgap_m.bgapcrtid_desc,g_bgap_m.bgapcrtdp, 
          g_bgap_m.bgapcrtdp_desc,g_bgap_m.bgapcrtdt,g_bgap_m.bgapmodid,g_bgap_m.bgapmodid_desc,g_bgap_m.bgapmoddt 
 
   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
   IF SQLCA.sqlcode THEN
   ELSE
      #單身狀態碼更新
      UPDATE bgaq_t 
         SET (bgaqstus,bgaqmodid,bgaqmoddt) 
           = (g_bgap_m.bgapstus,g_bgap_m.bgapmodid,g_bgap_m.bgapmoddt)     
       WHERE bgaqent = g_enterprise AND bgaq001 = g_bgap_m.bgap001
       
      #單身狀態碼更新
      UPDATE bgar_t 
         SET (bgarstus,bgarmodid,bgarmoddt) 
           = (g_bgap_m.bgapstus,g_bgap_m.bgapmodid,g_bgap_m.bgapmoddt)     
       WHERE bgarent = g_enterprise AND bgar001 = g_bgap_m.bgap001 
       
   END IF
   #end add-point
 
   #add-point:statechange段結束前 name="statechange.after"
   
   #end add-point  
 
   CLOSE abgi150_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL abgi150_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION
 
 
 
 
