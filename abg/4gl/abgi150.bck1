################################################################################
# Descriptions...: 預帶參考交易對象的說明
# Memo...........:
# Usage..........: CALL abgi150_bgapl_ref()
# Date & Author..: 160720 By 06821
# Modify.........:
################################################################################
PRIVATE FUNCTION abgi150_bgapl_ref()
DEFINE l_sql        STRING 
DEFINE r_success    LIKE type_t.num5

   LET r_success = TRUE
   
   IF cl_null(g_flag) THEN RETURN r_success END IF
   IF cl_null(g_bgap_m.bgap001) THEN RETURN r_success END IF
   IF cl_null(g_bgap_m.bgap003) THEN RETURN r_success END IF
   
   IF g_flag = '1' THEN #第一次新增,由pmaa帶出
      #先刪除後新增
      DELETE FROM bgapl_t WHERE bgaplent = g_enterprise AND bgapl001 = g_bgap_m.bgap001
      
      #寫入參考交易對象說明
      LET l_sql = " INSERT INTO bgapl_t ",
                  " SELECT pmaalent,'",g_bgap_m.bgap001,"',pmaal002,pmaal003,pmaal005,pmaal004 ",
                  "   FROM pmaal_t ",
                  "  WHERE pmaalent ='",g_enterprise,"'",
                  "    AND pmaal001 = '",g_bgap_m.bgap003,"'"
      PREPARE abgi150_bgapl_t FROM l_sql
      EXECUTE abgi150_bgapl_t
   ELSE

      IF cl_null(g_bgap001_o) THEN  #修改狀態應該要有舊值
         LET r_success = FALSE
         RETURN r_success
      END IF 
      
      #寫入參考交易對象說明
      LET l_sql = " INSERT INTO bgapl_t ",
                  " SELECT bgaplent,'",g_bgap_m.bgap001,"',bgapl002,bgapl003,bgapl004,bgapl005 ",
                  "   FROM bgapl_t ",
                  "  WHERE bgaplent ='",g_enterprise,"'",
                  "    AND bgapl001 = '",g_bgap001_o,"'"
      PREPARE abgi150_bgapl2_t FROM l_sql
      EXECUTE abgi150_bgapl2_t

      DELETE FROM bgapl_t WHERE bgaplent = g_enterprise AND bgapl001 = g_bgap001_o
      
   END IF
   
   IF SQLCA.sqlcode THEN
      LET r_success = FALSE
      RETURN r_success
   END IF

   LET g_bgap_m.bgapl005 = ''
   LET g_bgap_m.bgapl003 = ''
   SELECT bgapl005,bgapl003 
     INTO g_bgap_m.bgapl005,g_bgap_m.bgapl003 
     FROM bgapl_t
    WHERE bgaplent = g_enterprise AND bgapl001 = g_bgap_m.bgap001 AND bgapl002 = g_dlang
   DISPLAY BY NAME g_bgap_m.bgapl005,g_bgap_m.bgapl003

   RETURN r_success
END FUNCTION

################################################################################
# Descriptions...: 執行確認檢核
# Memo...........:
# Usage..........: CALL abgi150_conf_upd_chk()
# Return code....: r_success  成功否    
#                : r_errno    錯誤訊息
# Date & Author..: 161006 By 06821
# Modify.........:
################################################################################
PRIVATE FUNCTION abgi150_conf_upd_chk()
DEFINE l_cnt       LIKE type_t.num5
DEFINE r_success   LIKE type_t.num5
DEFINE r_errno     LIKE gzze_t.gzze001

   LET r_success = TRUE
   LET r_errno = ''
   LET l_cnt = 0
   
   IF cl_null(g_bgap_m.bgap002) THEN 
      LET r_success = FALSE
      LET r_errno = 'apm-00350'
      RETURN r_success,r_errno
   END IF
   
   CASE g_bgap_m.bgap002
      WHEN 1 #供應商
         SELECT COUNT(1) INTO l_cnt FROM bgaq_t 
          WHERE bgaqent = g_enterprise AND bgaq001 = g_bgap_m.bgap001 AND bgaq002 = '2'
         IF cl_null(l_cnt) THEN LET l_cnt = 0 END IF
         IF l_cnt = 0 THEN
            LET r_success = FALSE
            LET r_errno = 'abg-00162'
         END IF

      WHEN 2 #客戶
         SELECT COUNT(1) INTO l_cnt FROM bgaq_t 
          WHERE bgaqent = g_enterprise AND bgaq001 = g_bgap_m.bgap001 AND bgaq002 = '1'
         IF cl_null(l_cnt) THEN LET l_cnt = 0 END IF
         IF l_cnt = 0 THEN
            LET r_success = FALSE
            LET r_errno = 'abg-00161'
         END IF
         
      WHEN 3 #兩者
         SELECT COUNT(1) INTO l_cnt FROM bgaq_t 
          WHERE bgaqent = g_enterprise AND bgaq001 = g_bgap_m.bgap001
         IF cl_null(l_cnt) THEN LET l_cnt = 0 END IF
         IF l_cnt = 0 THEN
            LET r_success = FALSE
            LET r_errno = 'abg-00163'
         END IF
         
   END CASE
  

   RETURN r_success,r_errno

END FUNCTION

 
