      #應用 a13 樣板自動產生(Version:4)
       , bgarownid = g_user 
       , bgarowndp = g_dept
       , bgarcrtid = g_user
       , bgarcrtdp = g_dept 
       , bgarcrtdt = ld_date
       , bgarmodid = g_user
       , bgarmoddt = ld_date
