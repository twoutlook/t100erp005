{<section id="abgi370.other_function" readonly="Y" >}

################################################################################
# Descriptions...: 抓取預算周期帶出期季別
# Usage..........: CALL abgi370_ins_b()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 20160808 By brianwu70331
# Modify.........:
################################################################################
PRIVATE FUNCTION abgi370_ins_b()
DEFINE l_sql         STRING
DEFINE l_date        DATETIME YEAR TO SECOND
DEFINE r_success     LIKE type_t.num5
DEFINE l_bgci004     LIKE bgci_t.bgci004 

   LET l_date = cl_get_current()
   LET r_success = TRUE
   #選擇期別/季別
   CASE g_bgci_m.bgci006
      WHEN "1"
         LET l_sql = " SELECT DISTINCT bgac004", 
                       " FROM bgac_t ",
                      " WHERE bgacent = ",g_enterprise," ",
                        " AND bgac001 = '",g_bgci_m.l_bgaa002,"' "
      WHEN "2"
         LET l_sql = " SELECT DISTINCT bgac003", 
                       " FROM bgac_t ",
                      " WHERE bgacent = ",g_enterprise," ",
                        " AND bgac001 = '",g_bgci_m.l_bgaa002,"' "
   END CASE
   #將選出來的丟進FOREACH
   PREPARE abgi370_sel_bgac_p1 FROM l_sql
   DECLARE abgi370_sel_bgac_c1 CURSOR FOR abgi370_sel_bgac_p1
   FOREACH abgi370_sel_bgac_c1 INTO l_bgci004
   
      INSERT INTO bgci_t
             (bgcient,bgci001,bgci002,bgci003,bgci006,
              bgci004,bgci005,bgcistus,bgciownid,bgciowndp,
              bgcicrtid,bgcicrtdp,bgcicrtdt,bgcimodid,bgcimoddt) 
       VALUES(g_enterprise,g_bgci_m.bgci001,g_bgci_m.bgci002,g_bgci_m.bgci003,g_bgci_m.bgci006,
              l_bgci004,0,'Y',g_user,g_dept,
              g_user,g_dept,l_date,'','')
             
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = 'bgci_t',SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         LET r_success = FALSE
         EXIT FOREACH
      END IF                     
   END FOREACH  
   RETURN r_success
END FUNCTION

 
