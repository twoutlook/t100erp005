  <section id="apjm210.b_fill" ver="4" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION apjm210_b_fill()
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_pjbb_d.clear()    #g_pjbb_d 單頭及單身 
   CALL g_pjbb2_d.clear()
   CALL g_pjbb3_d.clear()
   CALL g_pjbb4_d.clear()
   CALL g_pjbb5_d.clear()
   CALL g_pjbb6_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF apjm210_fill_chk(1) THEN
   
      LET g_sql = "SELECT  UNIQUE pjbb002,pjbb003,pjbb004,'',pjbb005,pjbb006,pjbb007,pjbb008,pjbb009, 
          pjbb010,'',pjbb011,'',pjbb012 FROM pjbb_t",   
                  " INNER JOIN pjba_t ON pjba001 = pjbb001 ",
 
                  #"",
                  " LEFT JOIN pjbd_t ON pjbbent = pjbdent AND pjbb001 = pjbd001 AND pjbb002 = pjbd002 ",
               " LEFT JOIN pjbe_t ON pjbbent = pjbeent AND pjbb001 = pjbe001 AND pjbb002 = pjbe002 ",
               " LEFT JOIN pjbf_t ON pjbbent = pjbfent AND pjbb001 = pjbf001 AND pjbb002 = pjbf002 ",
               " LEFT JOIN pjbg_t ON pjbbent = pjbgent AND pjbb001 = pjbg001 AND pjbb002 = pjbg002 ",
                  "",
                  " WHERE pjbbent=? AND pjbb001=?"
      #add-point:b_fill段sql_before
      {<point name="b_fill.body.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table1) THEN
         LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
      END IF
      
      #子單身的WC
         IF NOT cl_null(g_wc2_table3) THEN 
      LET g_sql = g_sql CLIPPED," AND ", g_wc2_table3 CLIPPED
   END IF 
   IF NOT cl_null(g_wc2_table4) THEN 
      LET g_sql = g_sql CLIPPED," AND ", g_wc2_table4 CLIPPED
   END IF 
   IF NOT cl_null(g_wc2_table5) THEN 
      LET g_sql = g_sql CLIPPED," AND ", g_wc2_table5 CLIPPED
   END IF 
   IF NOT cl_null(g_wc2_table6) THEN 
      LET g_sql = g_sql CLIPPED," AND ", g_wc2_table6 CLIPPED
   END IF 
 
      
      LET g_sql = g_sql, " ORDER BY pjbb_t.pjbb002"
      
      #add-point:單身填充控制
      {<point name="b_fill.sql"/>}
      #end add-point
      
      PREPARE apjm210_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR apjm210_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_pjba_m.pjba001
 
                                               
      FOREACH b_fill_cs INTO g_pjbb_d[l_ac].pjbb002,g_pjbb_d[l_ac].pjbb003,g_pjbb_d[l_ac].pjbb004,g_pjbb_d[l_ac].pjbb004_desc, 
          g_pjbb_d[l_ac].pjbb005,g_pjbb_d[l_ac].pjbb006,g_pjbb_d[l_ac].pjbb007,g_pjbb_d[l_ac].pjbb008, 
          g_pjbb_d[l_ac].pjbb009,g_pjbb_d[l_ac].pjbb010,g_pjbb_d[l_ac].pjbb010_desc,g_pjbb_d[l_ac].pjbb011, 
          g_pjbb_d[l_ac].pjbb011_desc,g_pjbb_d[l_ac].pjbb012
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               CALL cl_err( '', 9035, 1)
            END IF
            EXIT FOREACH
         END IF
         
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
   #判斷是否填充
   IF apjm210_fill_chk(2) THEN
      LET g_sql = "SELECT  UNIQUE pjbc002,'',pjbc003,'',pjbc004,pjbc005,pjbc006 FROM pjbc_t",   
                  " INNER JOIN pjba_t ON pjba001 = pjbc001 ",
 
                  "",
                  
                  " WHERE pjbcent=? AND pjbc001=?"   
      #add-point:b_fill段sql_before
      {<point name="b_fill.body2.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table2) THEN
         LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
      END IF
      
      #子單身的WC
      
      
      LET g_sql = g_sql, " ORDER BY pjbc_t.pjbc002,pjbc_t.pjbc003"
      
      #add-point:單身填充控制
      {<point name="b_fill.sql2"/>}
      #end add-point
      
      PREPARE apjm210_pb2 FROM g_sql
      DECLARE b_fill_cs2 CURSOR FOR apjm210_pb2
      
      LET l_ac = 1
      
      OPEN b_fill_cs2 USING g_enterprise,g_pjba_m.pjba001
 
                                               
      FOREACH b_fill_cs2 INTO g_pjbb2_d[l_ac].pjbc002,g_pjbb2_d[l_ac].pjbc002_desc,g_pjbb2_d[l_ac].pjbc003, 
          g_pjbb2_d[l_ac].pjbc003_desc,g_pjbb2_d[l_ac].pjbc004,g_pjbb2_d[l_ac].pjbc005,g_pjbb2_d[l_ac].pjbc006 

         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill2.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            CALL cl_err( '', 9035, 0 )
            EXIT FOREACH
         END IF
         
      END FOREACH
   END IF
 
 
   
   #add-point:browser_fill段其他table處理
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_pjbb_d.deleteElement(g_pjbb_d.getLength())
   CALL g_pjbb2_d.deleteElement(g_pjbb2_d.getLength())
   CALL g_pjbb3_d.deleteElement(g_pjbb3_d.getLength())
   CALL g_pjbb4_d.deleteElement(g_pjbb4_d.getLength())
   CALL g_pjbb5_d.deleteElement(g_pjbb5_d.getLength())
   CALL g_pjbb6_d.deleteElement(g_pjbb6_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE apjm210_pb
   FREE apjm210_pb2
 
 
   
END FUNCTION
]]>
</section>
