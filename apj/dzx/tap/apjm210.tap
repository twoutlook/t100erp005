  <point name="delete.head.m_delete" cite_std="N" status="u" ver="" src="s" new="Y">
<![CDATA[      #若專案編號是用自動編碼編出來的,那刪除時,需要同步更新最大流水號檔(看是否要把流水號-1)
      LET l_pjaa007 = NULL
      SELECT pjaa007 INTO l_pjaa007 FROM pjaa_t
       WHERE pjaaent = g_enterprise AND pjaa001 = g_pjba_m.pjba000
      IF NOT cl_null(l_pjaa007) THEN
         CALL s_aooi390_del_max_serial_no(l_pjaa007,g_pjba_m.pjba001) RETURNING g_success
         IF NOT g_success THEN
            CALL s_transaction_end('N','1')
            RETURN
         END IF
      END IF]]>
