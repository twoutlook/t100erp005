   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT pjba000,'',pjba001,pjba002,'','',pjba003,'',pjba004,'',pjba005,pjba006, 
       pjbastus,pjbaownid,'',pjbaowndp,'',pjbacrtid,'',pjbacrtdp,'',pjbacrtdt,pjbamodid,'',pjbamoddt, 
       pjbacnfid,'',pjbacnfdt,'','','','','','','','','','','','','',''", 
                      " FROM pjba_t",
                      " WHERE pjbaent= ? AND pjba001=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE apjm250_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE pjba000,pjba001,pjba002,pjba003,pjba004,pjba005,pjba006,pjbastus,pjbaownid, 
       pjbaowndp,pjbacrtid,pjbacrtdp,pjbacrtdt,pjbamodid,pjbamoddt,pjbacnfid,pjbacnfdt,t1.pjaal003 , 
       t2.pjbal003 ,t3.ooail003 ,t4.oofa011 ,t5.ooefl003 ,t6.oofa011 ,t7.ooefl003 ,t8.oofa011 ,t9.oofa011", 

               " FROM pjba_t",
                              " LEFT JOIN pjaal_t t1 ON t1.pjaalent='"||g_enterprise||"' AND t1.pjaal001=pjba000 AND t1.pjaal002='"||g_dlang||"' ",
               " LEFT JOIN pjbal_t t2 ON t2.pjbalent='"||g_enterprise||"' AND t2.pjbal001=pjba003 AND t2.pjbal002='"||g_dlang||"' ",
               " LEFT JOIN ooail_t t3 ON t3.ooailent='"||g_enterprise||"' AND t3.ooail001=pjba004 AND t3.ooail002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t4 ON t4.oofaent='"||g_enterprise||"' AND t4.oofa002='2' AND t4.oofa003=pjbaownid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=pjbaowndp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t6 ON t6.oofaent='"||g_enterprise||"' AND t6.oofa002='2' AND t6.oofa003=pjbacrtid  ",
               " LEFT JOIN ooefl_t t7 ON t7.ooeflent='"||g_enterprise||"' AND t7.ooefl001=pjbacrtdp AND t7.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t8 ON t8.oofaent='"||g_enterprise||"' AND t8.oofa002='2' AND t8.oofa003=pjbamodid  ",
               " LEFT JOIN oofa_t t9 ON t9.oofaent='"||g_enterprise||"' AND t9.oofa002='2' AND t9.oofa003=pjbacnfid  ",
 
               " WHERE pjbaent = '" ||g_enterprise|| "' AND pjba001 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE apjm250_master_referesh FROM g_sql
 
 
 
   
   IF g_bgjob = "Y" THEN
