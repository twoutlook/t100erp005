   #add-point:insert段define
   {<point name="insert.define"/>}
   #end add-point    
   
   #清畫面欄位內容
   CLEAR FORM                    
   CALL g_pjbm_d.clear()   
 
 
   INITIALIZE g_pjba_m.* LIKE pjba_t.*             #DEFAULT 設定
   
   LET g_pjba001_t = NULL
 
   
