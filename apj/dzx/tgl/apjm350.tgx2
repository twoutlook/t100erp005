{<section id="apjm350.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION apjm350_b_fill()
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   {<point name="b_fill.define_customerization" edit="c" mark="Y"/>}
   #end add-point     
   DEFINE p_wc2      STRING
   DEFINE li_idx     LIKE type_t.num10
   #add-point:b_fill段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   {<point name="b_fill.define"/>} 
   #end add-point     
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   {<point name="b_fill.pre_function"/>}
   #end add-point
   
   #清空第一階單身
   CALL g_pjbv_d.clear()
 
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF apjm350_fill_chk(1) THEN
      #切換上下筆時不重組SQL
      IF (g_action_choice = "query" OR cl_null(g_action_choice))
      #add-point:b_fill段long_sql_if name="b_fill.long_sql_if"
      {<point name="b_fill.long_sql_if"/>}
      #end add-point
      THEN
         LET g_sql = "SELECT  DISTINCT pjbv002,pjbv030,pjbv003,pjbv004,pjbv005,pjbv006,pjbv007,pjbv008, 
             pjbv009,pjbv010,pjbv011,pjbv012,pjbv013,pjbv014,pjbv015,pjbv016,pjbv017,pjbv018,pjbv903, 
             pjbv904 ,t1.oocql004 ,t2.ooefl003 ,t5.ooag011 ,t6.ooefl003 ,t8.oocql004 ,t9.oocql004 ,t10.oocql004 FROM pjbv_t", 
                
                     " INNER JOIN pjbn_t ON pjbnent = " ||g_enterprise|| " AND pjbn001 = pjbv001 ",
                     " AND pjbn900 = pjbv900 ",
 
                     #" LEFT JOIN pjbvl_t ON pjbvlent = "||g_enterprise||" AND pjbn001 = pjbvl001 AND pjbv002 = pjbvl002 AND pjbn900 = pjbvl900 AND pjbvl003 = '",g_dlang,"'",
                     
                     " LEFT JOIN pjbvl_t ON pjbvlent = "||g_enterprise||" AND pjbn001 = pjbvl001 AND pjbv002 = pjbvl002 AND pjbn900 = pjbvl900 AND pjbvl003 = '",g_dlang,"'",
                     #下層單身所需的join條件
 
                                    " LEFT JOIN oocql_t t1 ON t1.oocqlent="||g_enterprise||" AND t1.oocql001='8004' AND t1.oocql002=pjbv003 AND t1.oocql003='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent="||g_enterprise||" AND t2.ooefl001=pjbv004 AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=pjbv009  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent="||g_enterprise||" AND t6.ooefl001=pjbv010 AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t8 ON t8.oocqlent="||g_enterprise||" AND t8.oocql001='8005' AND t8.oocql002=pjbv013 AND t8.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t9 ON t9.oocqlent="||g_enterprise||" AND t9.oocql001='8005' AND t9.oocql002=pjbv014 AND t9.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t10 ON t10.oocqlent="||g_enterprise||" AND t10.oocql001='8008' AND t10.oocql002=pjbv903 AND t10.oocql003='"||g_dlang||"' ",
 
                     " WHERE pjbvent=? AND pjbv001=? AND pjbv900=?"
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before name="b_fill.body.fill_sql"
         {<point name="b_fill.body.fill_sql"/>}
         #end add-point
         IF NOT cl_null(g_wc2_table1) THEN
            LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY pjbv_t.pjbv002"
         
         #add-point:單身填充控制 name="b_fill.sql"
         {<point name="b_fill.sql"/>}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE apjm350_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR apjm350_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
   #  OPEN b_fill_cs USING g_enterprise,g_pjbn_m.pjbn001,g_pjbn_m.pjbn900   #(ver:78)
                                               
      FOREACH b_fill_cs USING g_enterprise,g_pjbn_m.pjbn001,g_pjbn_m.pjbn900 INTO g_pjbv_d[l_ac].pjbv002, 
          g_pjbv_d[l_ac].pjbv030,g_pjbv_d[l_ac].pjbv003,g_pjbv_d[l_ac].pjbv004,g_pjbv_d[l_ac].pjbv005, 
          g_pjbv_d[l_ac].pjbv006,g_pjbv_d[l_ac].pjbv007,g_pjbv_d[l_ac].pjbv008,g_pjbv_d[l_ac].pjbv009, 
          g_pjbv_d[l_ac].pjbv010,g_pjbv_d[l_ac].pjbv011,g_pjbv_d[l_ac].pjbv012,g_pjbv_d[l_ac].pjbv013, 
          g_pjbv_d[l_ac].pjbv014,g_pjbv_d[l_ac].pjbv015,g_pjbv_d[l_ac].pjbv016,g_pjbv_d[l_ac].pjbv017, 
          g_pjbv_d[l_ac].pjbv018,g_pjbv_d[l_ac].pjbv903,g_pjbv_d[l_ac].pjbv904,g_pjbv_d[l_ac].pjbv003_desc, 
          g_pjbv_d[l_ac].pjbv004_desc,g_pjbv_d[l_ac].pjbv009_desc,g_pjbv_d[l_ac].pjbv010_desc,g_pjbv_d[l_ac].pjbv013_desc, 
          g_pjbv_d[l_ac].pjbv014_desc,g_pjbv_d[l_ac].pjbv903_desc   #(ver:78)
         IF SQLCA.SQLCODE THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code = SQLCA.SQLCODE 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充 name="b_fill.fill"
         {<point name="b_fill.fill"/>}
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code = 9035 
               LET g_errparam.popup = TRUE 
               CALL cl_err()
            END IF
            EXIT FOREACH
         END IF
         
         LET l_ac = l_ac + 1
      END FOREACH
      LET g_error_show = 0
   
   END IF
    
 
   
   #add-point:browser_fill段其他table處理 name="browser_fill.other_fill"
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_pjbv_d.deleteElement(g_pjbv_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE apjm350_pb
 
   
   LET li_idx = l_ac
   
   #遮罩相關處理
   FOR l_ac = 1 TO g_pjbv_d.getLength()
      LET g_pjbv_d_mask_o[l_ac].* =  g_pjbv_d[l_ac].*
      CALL apjm350_pjbv_t_mask()
      LET g_pjbv_d_mask_n[l_ac].* =  g_pjbv_d[l_ac].*
   END FOR
   
 
   
   LET l_ac = li_idx
   
   CALL cl_ap_performance_next_end()
 
END FUNCTION
