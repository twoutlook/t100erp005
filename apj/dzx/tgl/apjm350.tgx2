{<section id="apjm350.modify" >}
#+ 資料修改
PRIVATE FUNCTION apjm350_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   {<point name="modify.define_customerization" edit="c" mark="Y"/>}
   #end add-point    
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   DEFINE l_wc2_table1          STRING
 
 
   #add-point:modify段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   {<point name="modify.define"/>} 
   #end add-point    
   
   #add-point:Function前置處理  name="modify.pre_function"
   {<point name="modify.pre_function"/>}
   #end add-point
   
   #保存單頭舊值
   LET g_pjbn_m_t.* = g_pjbn_m.*
   LET g_pjbn_m_o.* = g_pjbn_m.*
   
   IF g_pjbn_m.pjbn001 IS NULL
   OR g_pjbn_m.pjbn900 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code = "std-00003" 
      LET g_errparam.popup = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_pjbn001_t = g_pjbn_m.pjbn001
   LET g_pjbn900_t = g_pjbn_m.pjbn900
 
   CALL s_transaction_begin()
   
   OPEN apjm350_cl USING g_enterprise,g_pjbn_m.pjbn001,g_pjbn_m.pjbn900
   IF SQLCA.SQLCODE THEN   #(ver:78)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN apjm350_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE   #(ver:78)
      LET g_errparam.popup = TRUE 
      CLOSE apjm350_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE apjm350_master_referesh USING g_pjbn_m.pjbn001,g_pjbn_m.pjbn900 INTO g_pjbn_m.pjbn000,g_pjbn_m.pjbn001, 
       g_pjbn_m.pjbn002,g_pjbn_m.pjbn009,g_pjbn_m.pjbn900,g_pjbn_m.pjbn003,g_pjbn_m.pjbn004,g_pjbn_m.pjbn005, 
       g_pjbn_m.pjbn006,g_pjbn_m.pjbn010,g_pjbn_m.pjbn011,g_pjbn_m.pjbn012,g_pjbn_m.pjbn013,g_pjbn_m.pjbn014, 
       g_pjbn_m.pjbn015,g_pjbn_m.pjbnstus,g_pjbn_m.pjbnownid,g_pjbn_m.pjbnowndp,g_pjbn_m.pjbncrtid,g_pjbn_m.pjbncrtdp, 
       g_pjbn_m.pjbncrtdt,g_pjbn_m.pjbnmodid,g_pjbn_m.pjbnmoddt,g_pjbn_m.pjbncnfid,g_pjbn_m.pjbncnfdt, 
       g_pjbn_m.pjbn000_desc,g_pjbn_m.pjbn003_desc,g_pjbn_m.pjbn004_desc,g_pjbn_m.pjbn010_desc,g_pjbn_m.pjbn012_desc, 
       g_pjbn_m.pjbn014_desc,g_pjbn_m.pjbnownid_desc,g_pjbn_m.pjbnowndp_desc,g_pjbn_m.pjbncrtid_desc, 
       g_pjbn_m.pjbncrtdp_desc,g_pjbn_m.pjbnmodid_desc,g_pjbn_m.pjbncnfid_desc
   
   #檢查是否允許此動作
   IF NOT apjm350_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_pjbn_m_mask_o.* =  g_pjbn_m.*
   CALL apjm350_pjbn_t_mask()
   LET g_pjbn_m_mask_n.* =  g_pjbn_m.*
   
   
   
   #add-point:modify段show之前 name="modify.before_show"
   {<point name="modify.before_show"/>}
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
 
   
   CALL apjm350_show()
   #add-point:modify段show之後 name="modify.after_show"
   {<point name="modify.after_show"/>}
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
 
 
    
   WHILE TRUE
      LET g_pjbn001_t = g_pjbn_m.pjbn001
      LET g_pjbn900_t = g_pjbn_m.pjbn900
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_pjbn_m.pjbnmodid = g_user 
LET g_pjbn_m.pjbnmoddt = cl_get_current()
LET g_pjbn_m.pjbnmodid_desc = cl_get_username(g_pjbn_m.pjbnmodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      LET g_master_commit = "N"
      CALL apjm350_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後 name="modify.after_input"
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE pjbn_t SET (pjbnmodid,pjbnmoddt) = (g_pjbn_m.pjbnmodid,g_pjbn_m.pjbnmoddt)
          WHERE pjbnent = g_enterprise AND pjbn001 = g_pjbn001_t
            AND pjbn900 = g_pjbn900_t
 
      END IF
    
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         #若單頭無commit則還原
         IF g_master_commit = "N" THEN
            LET g_pjbn_m.* = g_pjbn_m_t.*
            CALL apjm350_show()
         END IF
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code = 9001 
         LET g_errparam.popup = FALSE 
         CALL cl_err()
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_pjbn_m.pjbn001 != g_pjbn_m_t.pjbn001
      OR g_pjbn_m.pjbn900 != g_pjbn_m_t.pjbn900
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前 name="modify.body.b_fk_update"
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE pjbv_t SET pjbv001 = g_pjbn_m.pjbn001
                                       ,pjbv900 = g_pjbn_m.pjbn900
 
          WHERE pjbvent = g_enterprise AND pjbv001 = g_pjbn_m_t.pjbn001
            AND pjbv900 = g_pjbn_m_t.pjbn900
 
            
         #add-point:單身fk修改中 name="modify.body.m_fk_update"
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            #   INITIALIZE g_errparam TO NULL 
            #   LET g_errparam.extend = "pjbv_t" 
            #   LET g_errparam.code = "std-00009" 
            #   LET g_errparam.popup = TRUE 
            #   CALL cl_err()
            #   CALL s_transaction_end('N','0')
            #   CONTINUE WHILE
            WHEN SQLCA.SQLCODE #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "pjbv_t:",SQLERRMESSAGE 
               LET g_errparam.code = SQLCA.SQLCODE 
               LET g_errparam.popup = TRUE 
               CALL s_transaction_end('N','0')
               CALL cl_err()
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後 name="modify.body.a_fk_update"
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         LET l_new_key[01] = g_enterprise
LET l_old_key[01] = g_enterprise
LET l_field_key[01] = 'pjbvlent'
LET l_new_key[02] = g_pjbn_m.pjbn001
LET l_old_key[02] = g_pjbn001_t
LET l_field_key[02] = 'pjbvl001'
LET l_new_key[03] = g_pjbn_m.pjbn900
LET l_old_key[03] = g_pjbn900_t
LET l_field_key[03] = 'pjbvl900'
CALL cl_multitable_key_upd(l_new_key, l_old_key, l_field_key, 'pjbvl_t')
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL apjm350_set_act_visible()   
   CALL apjm350_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " pjbnent = " ||g_enterprise|| " AND",
                      " pjbn001 = '", g_pjbn_m.pjbn001, "' "
                      ," AND pjbn900 = '", g_pjbn_m.pjbn900, "' "
 
   #填到對應位置
   CALL apjm350_browser_fill("")
 
   CLOSE apjm350_cl
   
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL apjm350_msgcentre_notify('modify')
 
END FUNCTION 
