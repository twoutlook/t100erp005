       g_pjbn_m.pjbn006,g_pjbn_m.pjbn010,g_pjbn_m.pjbn011,g_pjbn_m.pjbn012,g_pjbn_m.pjbn013,g_pjbn_m.pjbn014, 
       g_pjbn_m.pjbn015,g_pjbn_m.pjbnstus,g_pjbn_m.pjbnownid,g_pjbn_m.pjbnowndp,g_pjbn_m.pjbncrtid,g_pjbn_m.pjbncrtdp, 
       g_pjbn_m.pjbncrtdt,g_pjbn_m.pjbnmodid,g_pjbn_m.pjbnmoddt,g_pjbn_m.pjbncnfid,g_pjbn_m.pjbncnfdt, 
       g_pjbn_m.pjbn000_desc,g_pjbn_m.pjbn003_desc,g_pjbn_m.pjbn004_desc,g_pjbn_m.pjbn010_desc,g_pjbn_m.pjbn012_desc, 
       g_pjbn_m.pjbn014_desc,g_pjbn_m.pjbnownid_desc,g_pjbn_m.pjbnowndp_desc,g_pjbn_m.pjbncrtid_desc, 
       g_pjbn_m.pjbncrtdp_desc,g_pjbn_m.pjbnmodid_desc,g_pjbn_m.pjbncnfid_desc
   
   #遮罩相關處理
   LET g_pjbn_m_mask_o.* =  g_pjbn_m.*
   CALL apjm350_pjbn_t_mask()
   LET g_pjbn_m_mask_n.* =  g_pjbn_m.*
   
