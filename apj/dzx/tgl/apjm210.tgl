   {<point name="filter.pre_function"/>}
   #end add-point
   
   #切換畫面
   IF NOT g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",1)
      CALL gfrm_curr.setElementHidden("worksheet",0)
      LET g_main_hidden = 1
   END IF   
 
   LET INT_FLAG = 0
 
   LET g_qryparam.state = 'c'
 
   LET g_wc_filter_t = g_wc_filter.trim()
   LET g_wc_t = g_wc
 
   LET g_wc = cl_replace_str(g_wc, g_wc_filter_t, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON pjba001,pjba000,pjba002,pjba003,pjba004,pjba005
                          FROM s_browse[1].b_pjba001,s_browse[1].b_pjba000,s_browse[1].b_pjba002,s_browse[1].b_pjba003, 
                              s_browse[1].b_pjba004,s_browse[1].b_pjba005
 
         BEFORE CONSTRUCT
               DISPLAY apjm210_filter_parser('pjba001') TO s_browse[1].b_pjba001
            DISPLAY apjm210_filter_parser('pjba000') TO s_browse[1].b_pjba000
            DISPLAY apjm210_filter_parser('pjba002') TO s_browse[1].b_pjba002
            DISPLAY apjm210_filter_parser('pjba003') TO s_browse[1].b_pjba003
            DISPLAY apjm210_filter_parser('pjba004') TO s_browse[1].b_pjba004
            DISPLAY apjm210_filter_parser('pjba005') TO s_browse[1].b_pjba005
      
