                     " INNER JOIN pjba_t ON pjbaent = " ||g_enterprise|| " AND pjba001 = pjbb001 ",
 
                     #"",
                     " LEFT JOIN pjbd_t ON pjbbent = pjbdent AND pjbb001 = pjbd001 AND pjbb002 = pjbd002 ",
               " LEFT JOIN pjbe_t ON pjbbent = pjbeent AND pjbb001 = pjbe001 AND pjbb002 = pjbe002 ",
               " LEFT JOIN pjbf_t ON pjbbent = pjbfent AND pjbb001 = pjbf001 AND pjbb002 = pjbf002 ",
