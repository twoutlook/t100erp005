   #add-point:browser_fill段其他table處理 name="browser_fill.other_fill"
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_pjbb_d.deleteElement(g_pjbb_d.getLength())
   CALL g_pjbb2_d.deleteElement(g_pjbb2_d.getLength())
 
   
 
   LET l_ac = g_cnt
