                  "  LEFT JOIN pjbb_t ON pjbbent = pjbaent AND pjba001 = pjbb001 ", "  ", 
                  #add-point:browser_fill段sql(pjbb_t1) name="browser_fill.join.pjbb_t1"
                  {<point name="browser_fill.join.pjbb_t1"/>}
                  #end add-point
                  "  LEFT JOIN pjbc_t ON pjbcent = pjbaent AND pjba001 = pjbc001", "  ", 
                  #add-point:browser_fill段sql(pjbc_t1) name="browser_fill.join.pjbc_t1"
                  {<point name="browser_fill.join.pjbc_t1"/>}
