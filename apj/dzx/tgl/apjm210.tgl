   END IF 
   IF NOT cl_null(g_wc2_table6) THEN 
      LET g_sql = g_sql CLIPPED," AND ", g_wc2_table6 CLIPPED
   END IF 
 
         
         LET g_sql = g_sql, " ORDER BY pjbb_t.pjbb002"
         
         #add-point:單身填充控制 name="b_fill.sql"
         {<point name="b_fill.sql"/>}
