   END IF
   
   #顯示最新的資料
   EXECUTE apjm310_master_referesh USING g_pjbn_m.pjbn001,g_pjbn_m.pjbn900 INTO g_pjbn_m.pjbn000,g_pjbn_m.pjbn001, 
