PRIVATE FUNCTION apjm400_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   {<point name="modify.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   {<point name="modify.define"/>}
   #end add-point
   
   #add-point:Function前置處理 name="modify.pre_function"
   {<point name="modify.pre_function"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_pjba_m.pjba001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_pjba001_t = g_pjba_m.pjba001
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN apjm400_cl USING g_enterprise,g_pjba_m.pjba001
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN apjm400_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CLOSE apjm400_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE apjm400_master_referesh USING g_pjba_m.pjba001 INTO g_pjba_m.pjba001,g_pjba_m.pjba011,g_pjba_m.pjba023, 
       g_pjba_m.pjbastus,g_pjba_m.pjba027,g_pjba_m.pjba028,g_pjba_m.pjba029,g_pjba_m.pjba030,g_pjba_m.pjba031, 
       g_pjba_m.pjba032,g_pjba_m.pjba035,g_pjba_m.pjba033,g_pjba_m.pjba034,g_pjba_m.pjba040,g_pjba_m.pjba036, 
       g_pjba_m.pjba037,g_pjba_m.pjba038,g_pjba_m.pjba039,g_pjba_m.pjba045,g_pjba_m.pjba041,g_pjba_m.pjba042, 
       g_pjba_m.pjba043,g_pjba_m.pjba044,g_pjba_m.pjba050,g_pjba_m.pjba046,g_pjba_m.pjba047,g_pjba_m.pjba048, 
       g_pjba_m.pjba049,g_pjba_m.pjbaownid,g_pjba_m.pjbaowndp,g_pjba_m.pjbacrtid,g_pjba_m.pjbacrtdp, 
       g_pjba_m.pjbacrtdt,g_pjba_m.pjbamodid,g_pjba_m.pjbamoddt,g_pjba_m.pjbacnfid,g_pjba_m.pjbacnfdt, 
       g_pjba_m.pjba001_desc,g_pjba_m.pjbaownid_desc,g_pjba_m.pjbaowndp_desc,g_pjba_m.pjbacrtid_desc, 
       g_pjba_m.pjbacrtdp_desc,g_pjba_m.pjbamodid_desc,g_pjba_m.pjbacnfid_desc
 
   #檢查是否允許此動作
   IF NOT apjm400_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_pjba_m_mask_o.* =  g_pjba_m.*
   CALL apjm400_pjba_t_mask()
   LET g_pjba_m_mask_n.* =  g_pjba_m.*
   
   
 
   #顯示資料
   CALL apjm400_show()
   
   WHILE TRUE
      LET g_pjba_m.pjba001 = g_pjba001_t
 
      
      #寫入修改者/修改日期資訊
      LET g_pjba_m.pjbamodid = g_user 
LET g_pjba_m.pjbamoddt = cl_get_current()
LET g_pjba_m.pjbamodid_desc = cl_get_username(g_pjba_m.pjbamodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
      #end add-point
 
      #資料輸入
      CALL apjm400_input("u")     
 
      #add-point:modify段修改後 name="modify.after_input"
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         LET g_pjba_m.* = g_pjba_m_t.*
         CALL apjm400_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE pjba_t SET (pjbamodid,pjbamoddt) = (g_pjba_m.pjbamodid,g_pjba_m.pjbamoddt)
       WHERE pjbaent = g_enterprise AND pjba001 = g_pjba001_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL apjm400_set_act_visible()
   CALL apjm400_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " pjbaent = " ||g_enterprise|| " AND",
                      " pjba001 = '", g_pjba_m.pjba001, "' "
 
   #填到對應位置
   CALL apjm400_browser_fill(g_wc,"")
 
   CLOSE apjm400_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL apjm400_msgcentre_notify('modify')
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   

