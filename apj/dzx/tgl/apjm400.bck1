   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define name="main.define_sql"
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT pjba001,'',pjba011,pjba023,pjbastus,pjba027,pjba028,pjba029,pjba030,pjba031, 
       pjba032,pjba035,pjba033,pjba034,pjba040,pjba036,pjba037,pjba038,pjba039,pjba045,pjba041,pjba042, 
       pjba043,pjba044,pjba050,pjba046,pjba047,pjba048,pjba049,pjbaownid,'',pjbaowndp,'',pjbacrtid,'', 
       pjbacrtdp,'',pjbacrtdt,pjbamodid,'',pjbamoddt,pjbacnfid,'',pjbacnfdt", 
                      " FROM pjba_t",
                      " WHERE pjbaent= ? AND pjba001=? FOR UPDATE"
   #add-point:SQL_define name="main.after_define_sql"
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE apjm400_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.pjba001,t0.pjba011,t0.pjba023,t0.pjbastus,t0.pjba027,t0.pjba028, 
       t0.pjba029,t0.pjba030,t0.pjba031,t0.pjba032,t0.pjba035,t0.pjba033,t0.pjba034,t0.pjba040,t0.pjba036, 
       t0.pjba037,t0.pjba038,t0.pjba039,t0.pjba045,t0.pjba041,t0.pjba042,t0.pjba043,t0.pjba044,t0.pjba050, 
       t0.pjba046,t0.pjba047,t0.pjba048,t0.pjba049,t0.pjbaownid,t0.pjbaowndp,t0.pjbacrtid,t0.pjbacrtdp, 
       t0.pjbacrtdt,t0.pjbamodid,t0.pjbamoddt,t0.pjbacnfid,t0.pjbacnfdt,t1.pjbal003 ,t2.ooag011 ,t3.ooefl003 , 
       t4.ooag011 ,t5.ooefl003 ,t6.ooag011 ,t7.ooag011",
               " FROM pjba_t t0",
                              " LEFT JOIN pjbal_t t1 ON t1.pjbalent="||g_enterprise||" AND t1.pjbal001=t0.pjba001 AND t1.pjbal002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t2 ON t2.ooagent="||g_enterprise||" AND t2.ooag001=t0.pjbaownid  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent="||g_enterprise||" AND t3.ooefl001=t0.pjbaowndp AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t4 ON t4.ooagent="||g_enterprise||" AND t4.ooag001=t0.pjbacrtid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent="||g_enterprise||" AND t5.ooefl001=t0.pjbacrtdp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent="||g_enterprise||" AND t6.ooag001=t0.pjbamodid  ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent="||g_enterprise||" AND t7.ooag001=t0.pjbacnfid  ",
 
               " WHERE t0.pjbaent = " ||g_enterprise|| " AND t0.pjba001 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define name="main.after_refresh_sql"
   {<point name="main.after_refresh_sql"/>}
