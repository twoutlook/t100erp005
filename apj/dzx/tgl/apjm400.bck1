      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF
   
   #將資料輸出到畫面上
   DISPLAY BY NAME g_pjba_m.pjba001,g_pjba_m.pjba001_desc,g_pjba_m.pjba011,g_pjba_m.pjba023,g_pjba_m.pjbastus, 
