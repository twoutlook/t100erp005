       g_pjba_m.pjbamodid_desc,g_pjba_m.pjbamoddt,g_pjba_m.pjbacnfid,g_pjba_m.pjbacnfid_desc,g_pjba_m.pjbacnfdt 

   
   CALL cl_set_head_visible("","YES")  
   
   #a-新增,r-複製,u-修改
   IF p_cmd = 'r' THEN
      #此段落的r動作等同於a
      LET p_cmd = 'a'
   END IF
 
   LET l_insert = FALSE
   LET g_action_choice = ""
 
   LET g_qryparam.state = "i"
   
   #控制key欄位可否輸入
   CALL apjm400_set_entry(p_cmd)
