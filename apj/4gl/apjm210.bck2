   LET l_sql =  "SELECT MAX(pjbb002) + 1 FROM pjbb_t WHERE pjbbent = ",g_enterprise,
             "  AND pjbb001 = '",g_pjba_m.pjba001,"'"
 
   PREPARE sel_max_pjbb002_2 FROM l_sql
