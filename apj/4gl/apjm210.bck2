      CALL apjm210_pjbg_t_mask()
      LET g_pjbb6_d_mask_n[l_ac].* =  g_pjbb6_d[l_ac].*
   END FOR
 
      
   #add-point:單身填充後 name="b_fill2.after_fill"
   
