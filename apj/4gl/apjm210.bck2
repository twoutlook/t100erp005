      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_pjba_m.pjba000,g_pjba_m.pjba000_desc,g_pjba_m.pjba001,g_pjba_m.pjba020,g_pjba_m.pjbal003, 
