DEFINE l_max_num   LIKE pjbb_t.pjbb002
DEFINE l_max_pjbb003 LIKE type_t.num5
DEFINE l_sql       STRING
DEFINE l_format    STRING
   SELECT pjaa009 INTO l_pjaa009 FROM pjaa_t
       WHERE pjaaent = g_enterprise AND pjaa001 = g_pjba_m.pjba000
