PRIVATE FUNCTION apjm210_get_auto_no(p_pjaa009,p_flag)
DEFINE p_pjaa009  LIKE pjaa_t.pjaa009
DEFINE p_flag     LIKE type_t.chr1  
DEFINE r_pjbb002  LIKE pjbb_t.pjbb002
DEFINE l_i        LIKE type_t.num5
   FOR l_i = 1 TO p_pjaa009
       IF l_i = 1 THEN
          LET r_pjbb002 = "0"
       ELSE
          IF l_i = p_pjaa009 AND p_flag = 'N' THEN
             LET r_pjbb002 = r_pjbb002,"1"
             EXIT FOR
          END IF
          LET r_pjbb002 = r_pjbb002,"0"
       END IF
   END FOR
   RETURN r_pjbb002
END FUNCTION

PRIVATE FUNCTION apjm210_get_pjbb002_pjbb003()
