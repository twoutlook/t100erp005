               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               EXIT FOREACH
            END IF
            
            LET l_ac = l_ac + 1
            
         END FOREACH
               CALL g_pjbb6_d.deleteElement(g_pjbb6_d.getLength())
 
      END IF
   END IF
 
 
      
   LET g_pjbb3_d_mask_o.* =  g_pjbb3_d.*
   FOR l_ac = 1 TO g_pjbb3_d.getLength()
      LET g_pjbb3_d_mask_o[l_ac].* =  g_pjbb3_d[l_ac].*
      CALL apjm210_pjbd_t_mask()
      LET g_pjbb3_d_mask_n[l_ac].* =  g_pjbb3_d[l_ac].*
