#检查是否有已审核的专案编号
PRIVATE FUNCTION apjm210_pjba001_exist()
DEFINE l_n LIKE type_t.num5
  IF NOT cl_null(g_pjba_m.pjba001) THEN
     LET l_n = 0
     SELECT COUNT(*) INTO l_n FROM pjac_t   #判断专案是否审核
        WHERE pjacent = g_enterprise AND pjac001 = g_pjba_m.pjba001 AND pjacstus = 'Y'
     IF l_n = 0 THEN
        RETURN FALSE
     END IF
  ELSE
     RETURN FALSE
  END IF
RETURN TRUE
END FUNCTION

