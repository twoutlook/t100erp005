      LET g_pjbb5_d[l_ac].pjbf013 = g_pjbb5_d[l_ac].pjbf011 * l_rate
      CALL s_num_round(l_para,g_pjbb5_d[l_ac].pjbf013,6) RETURNING g_pjbb5_d[l_ac].pjbf013

   END IF
END FUNCTION
#获取pjbg_t原币金额
PRIVATE FUNCTION apjm210_get_pjbg_money(p_oodb005,p_oodb006,p_field)
DEFINE l_para   STRING 
DEFINE p_oodb005 LIKE oodb_t.oodb005
DEFINE p_oodb006 LIKE oodb_t.oodb006
DEFINE p_field STRING

   CALL cl_get_para(g_enterprise,g_site,'E-COM-0006') RETURNING l_para    
   IF cl_null(p_oodb005) AND cl_null(p_oodb006) THEN
      RETURN
   END IF
#   LET g_pjbb6_d[l_ac].pjbg007 = p_oodb006
   IF p_field = 'pjbg004' AND g_pjbb6_d[l_ac].pjbg004 <> 0 THEN
      LET g_pjbb6_d[l_ac].pjbg008 = g_pjbb6_d[l_ac].pjbg004 * (1+p_oodb006/100) 
      CALL s_num_round(l_para,g_pjbb6_d[l_ac].pjbg008,6) RETURNING g_pjbb6_d[l_ac].pjbg008    
   END IF
   
   IF p_field = 'pjbg008' AND g_pjbb6_d[l_ac].pjbg008 <> 0 THEN
      LET g_pjbb6_d[l_ac].pjbg004 = g_pjbb6_d[l_ac].pjbg008 / (1+p_oodb006/100) 
      CALL s_num_round(l_para,g_pjbb6_d[l_ac].pjbg004,6) RETURNING g_pjbb6_d[l_ac].pjbg004    
   END IF
END FUNCTION
#获取本币金额
PRIVATE FUNCTION apjm210_get_pjbg_local_money()
DEFINE l_ooef017 LIKE ooef_t.ooef017
DEFINE l_ooef016 LIKE ooef_t.ooef016
DEFINE l_ooef019 LIKE ooef_t.ooef019   
DEFINE l_scc40      LIKE type_t.chr2        #匯率类型
DEFINE l_rate       LIKE ooan_t.ooan005 
DEFINE l_para   STRING 
   IF NOT cl_null(g_pjba_m.pjba004) AND NOT cl_null(g_pjbb6_d[l_ac].pjbg004) AND NOT cl_null(g_pjbb6_d[l_ac].pjbg008)  THEN
      CALL cl_get_para(g_enterprise,g_site,'E-COM-0006') RETURNING l_para   
      SELECT ooef017,ooef016,ooef019 INTO l_ooef017,l_ooef016,l_ooef019 FROM ooef_t
      WHERE ooefent = g_enterprise AND ooef001 = g_site
      CALL cl_get_para(g_enterprise,g_site,'S-BAS-0010') RETURNING l_scc40
      CALL s_aooi160_get_exrate('1',g_site,g_today,g_pjba_m.pjba004,l_ooef016,0,l_scc40) RETURNING l_rate
      IF cl_null(l_rate) THEN
         LET l_rate = 1           
      END IF
      LET g_pjbb6_d[l_ac].pjbg009 = g_pjbb6_d[l_ac].pjbg004 * l_rate
      CALL s_num_round(l_para,g_pjbb6_d[l_ac].pjbg009,6) RETURNING g_pjbb6_d[l_ac].pjbg009
      LET g_pjbb6_d[l_ac].pjbg010 = g_pjbb6_d[l_ac].pjbg008 * l_rate
      CALL s_num_round(l_para,g_pjbb6_d[l_ac].pjbg010,6) RETURNING g_pjbb6_d[l_ac].pjbg010

   END IF
END FUNCTION
