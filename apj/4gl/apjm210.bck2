DEFINE g_pjba_m_t    type_g_pjba_m
   DEFINE p_pjba001       LIKE pjba_t.pjba001
   DEFINE p_pjba003       LIKE pjba_t.pjba003
   DEFINE l_pjbb   RECORD LIKE pjbb_t.*
   DEFINE l_date          LIKE type_t.dat
   DEFINE l_pjbc   RECORD LIKE pjbc_t.*
