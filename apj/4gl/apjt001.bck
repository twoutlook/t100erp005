         LET INT_FLAG = 0
         LET g_pjca_m.* = g_pjca_m_t.*
         CALL apjt001_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更(更新單身table的key欄位值)
      IF g_pjca_m.pjcald != g_pjcald_t 
      OR g_pjca_m.pjca001 != g_pjca001_t 
      OR g_pjca_m.pjca003 != g_pjca003_t 
      OR g_pjca_m.pjca004 != g_pjca004_t 
