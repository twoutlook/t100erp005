                  CLOSE apjt001_cl
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "OPEN apjt001_cl:" 
                  LET g_errparam.code   = STATUS 
                  LET g_errparam.popup  = TRUE 
                  CALL s_transaction_end('N','0')
                  CALL cl_err()
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            IF g_rec_b >= l_ac 
               AND g_pjca_d[l_ac].pjca002 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_pjca_d_t.* = g_pjca_d[l_ac].*  #BACKUP
