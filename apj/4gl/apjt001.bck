{<section id="apjt001.insert" >}
#+ 資料新增
PRIVATE FUNCTION apjt001_insert()
   #add-point:insert段define name="insert.define_customerization"
   
   #end add-point   
   #add-point:insert段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="insert.define"
   
   #end add-point
   
   #add-point:Function前置處理  name="insert.before"
   
   #end add-point    
   
   #清除相關資料
   CLEAR FORM                    
   CALL g_pjca_d.clear()
 
 
   INITIALIZE g_pjca_m.* LIKE pjca_t.*             #DEFAULT 設定
   LET g_pjcald_t = NULL
   LET g_pjca001_t = NULL
   LET g_pjca003_t = NULL
   LET g_pjca004_t = NULL
   LET g_pjca005_t = NULL
 
   LET g_master_insert = FALSE
   CALL s_transaction_begin()
   WHILE TRUE
     
      #單頭預設值
            LET g_pjca_m.pjca001 = "1"
 
     
      #add-point:單頭預設值 name="insert.default"
      #预设当前site的法人，主账套，年度期别，成本计算类型
      CALL s_axc_set_site_default() RETURNING g_pjca_m.pjcacomp,g_pjca_m.pjcald,g_pjca_m.pjca004,
                                           g_pjca_m.pjca005,g_pjca_m.pjca003
      CALL s_desc_get_department_desc(g_pjca_m.pjcacomp) RETURNING g_pjca_m.pjcacomp_desc
      CALL s_desc_get_ld_desc(g_pjca_m.pjcald) RETURNING g_pjca_m.pjcald_desc
      CALL apjt001_pjca003_desc() 
      LET g_pjca_m_t.* = g_pjca_m.*
      LET g_pjca_m_o.* = g_pjca_m.*      
      #end add-point 
 
      CALL apjt001_input("a")
      
      #add-point:單頭輸入後 name="insert.after_insert"
      
      #end add-point
      
      IF INT_FLAG AND NOT g_master_insert THEN
         DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
         DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
         INITIALIZE g_pjca_m.* TO NULL
         INITIALIZE g_pjca_d TO NULL
 
         CALL apjt001_show()
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         LET g_pjca_m.* = g_pjca_m_t.*
         CALL apjt001_show()
