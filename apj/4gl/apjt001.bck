   LET g_pjca_m.pjca001 = ""
   LET g_pjca_m.pjca003 = ""
   LET g_pjca_m.pjca004 = ""
   LET g_pjca_m.pjca005 = ""
 
   LET g_master_insert = FALSE
   CALL apjt001_set_entry('a')
   CALL apjt001_set_no_entry('a')
   
   CALL cl_set_head_visible("","YES")
   CALL s_transaction_begin()
   
   #add-point:複製輸入前 name="reproduce.head.b_input"
   LET g_pjca_m.pjca001 = '1'
   #end add-point
   
   #清空key欄位的desc
