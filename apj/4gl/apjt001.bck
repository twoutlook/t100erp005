   DISPLAY BY NAME g_pjca_m.pjcald_desc
   LET g_pjca_m.pjca003_desc = ''
   DISPLAY BY NAME g_pjca_m.pjca003_desc
 
   
   CALL apjt001_input("r")
    
   IF INT_FLAG AND NOT g_master_insert THEN
      DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
      DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
      INITIALIZE g_pjca_m.* TO NULL
      INITIALIZE g_pjca_d TO NULL
 
      CALL apjt001_show()
      LET INT_FLAG = 0
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = '' 
      LET g_errparam.code   = 9001 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN 
   END IF
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("modify,modify_detail,delete,reproduce", TRUE)
   CALL apjt001_set_act_visible()
   CALL apjt001_set_act_no_visible()
 
   #將新增的資料併入搜尋條件中
   LET g_state = "insert"
   
   LET g_pjcald_t = g_pjca_m.pjcald
   LET g_pjca001_t = g_pjca_m.pjca001
   LET g_pjca003_t = g_pjca_m.pjca003
   LET g_pjca004_t = g_pjca_m.pjca004
   LET g_pjca005_t = g_pjca_m.pjca005
 
   
   #組合新增資料的條件
   LET g_add_browse = " pjcaent = " ||g_enterprise|| " AND",
                      " pjcald = '", g_pjca_m.pjcald, "' "
                      ," AND pjca001 = '", g_pjca_m.pjca001, "' "
                      ," AND pjca003 = '", g_pjca_m.pjca003, "' "
                      ," AND pjca004 = '", g_pjca_m.pjca004, "' "
                      ," AND pjca005 = '", g_pjca_m.pjca005, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL apjt001_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   LET g_curr_diag = ui.DIALOG.getCurrent()
   CALL apjt001_idx_chk()
   
   #add-point:完成複製段落後 name="reproduce.after_reproduce"
   
   #end add-point
   
   #功能已完成,通報訊息中心
   CALL apjt001_msgcentre_notify('reproduce')
   
