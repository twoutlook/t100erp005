      RETURN
   END IF
 
   ERROR ""
   LET g_pjcald_t = g_pjca_m.pjcald
   LET g_pjca001_t = g_pjca_m.pjca001
   LET g_pjca003_t = g_pjca_m.pjca003
   LET g_pjca004_t = g_pjca_m.pjca004
   LET g_pjca005_t = g_pjca_m.pjca005
 
