   OPEN apjt001_cl USING g_enterprise,g_pjca_m.pjcald,g_pjca_m.pjca001,g_pjca_m.pjca003,g_pjca_m.pjca004,g_pjca_m.pjca005
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN apjt001_cl:" 
      LET g_errparam.code   =  STATUS 
      LET g_errparam.popup  = TRUE 
      CLOSE apjt001_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
