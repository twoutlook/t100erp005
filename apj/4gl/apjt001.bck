   #顯示最新的資料
   EXECUTE apjt001_master_referesh USING g_pjca_m.pjcald,g_pjca_m.pjca001,g_pjca_m.pjca003,g_pjca_m.pjca004, 
       g_pjca_m.pjca005 INTO g_pjca_m.pjcacomp,g_pjca_m.pjcald,g_pjca_m.pjca004,g_pjca_m.pjca005,g_pjca_m.pjca003, 
