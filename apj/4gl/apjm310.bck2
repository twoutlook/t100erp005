PRIVATE FUNCTION apjm310_get_pjbo002_pjbo003()
DEFINE l_pjaa009   LIKE pjaa_t.pjaa009 
DEFINE l_get_str   STRING
DEFINE l_max_num   LIKE pjbo_t.pjbo002
DEFINE l_max_pjbo003 LIKE type_t.num5
DEFINE l_sql       STRING
DEFINE l_format    STRING
   SELECT pjaa009 INTO l_pjaa009 FROM pjaa_t
       WHERE pjaaent = g_enterprise AND pjaa001 = g_pjbn_m.pjbn000
   CALL apjm310_get_pjaa009_format(l_pjaa009) RETURNING l_format   
   LET l_sql =  "SELECT MAX(pjbo002) + 1 FROM pjbo_t WHERE pjboent = ",g_enterprise,
             "  AND pjbo001 = '",g_pjbn_m.pjbn001,"'"
 
   PREPARE sel_max_pjbo002_2 FROM l_sql
   EXECUTE sel_max_pjbo002_2 INTO l_max_num
   IF cl_null(l_max_num) THEN
      CALL apjm310_get_auto_no(l_pjaa009,'N') RETURNING l_max_num
   ELSE
      LET l_max_num = l_max_num USING l_format 
   END IF
   LET g_pjbo_d[l_ac].pjbo002 = l_max_num
   CALL apjm310_get_auto_no(l_pjaa009,'Y') RETURNING g_pjbo_d[l_ac].pjbo003
