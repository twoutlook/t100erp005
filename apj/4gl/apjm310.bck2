      SELECT pjbb002,pjbb003,pjbb004,pjbb005,pjbb006,pjbb007,pjbb008,pjbb009,pjbb010,pjbb011,pjbb012 
        INTO l_pjbb.pjbb002,l_pjbb.pjbb003,l_pjbb.pjbb004,l_pjbb.pjbb005,l_pjbb.pjbb006,l_pjbb.pjbb007,
