   DEFINE l_detail    RECORD LIKE pjbo_t.* #此變數樣板目前無使用
   DEFINE l_detail2    RECORD LIKE pjbp_t.* #此變數樣板目前無使用
 
 
   DEFINE l_detail3    RECORD LIKE pjbq_t.* #此變數樣板目前無使用
 
   DEFINE l_detail4    RECORD LIKE pjbr_t.* #此變數樣板目前無使用
 
   DEFINE l_detail5    RECORD LIKE pjbs_t.* #此變數樣板目前無使用
 
   DEFINE l_detail6    RECORD LIKE pjbt_t.* #此變數樣板目前無使用
 
 
   DEFINE l_cnt       LIKE type_t.num10
   #add-point:reproduce段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="reproduce.define"
   
   #end add-point   
   
   #add-point:Function前置處理  name="reproduce.pre_function"
   
   #end add-point
   
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF
   
   LET g_master_insert = FALSE
   
   IF g_pjbn_m.pjbn001 IS NULL
   OR g_pjbn_m.pjbn900 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
    
   LET g_pjbn001_t = g_pjbn_m.pjbn001
   LET g_pjbn900_t = g_pjbn_m.pjbn900
 
    
   LET g_pjbn_m.pjbn001 = ""
   LET g_pjbn_m.pjbn900 = ""
 
 
   CALL cl_set_head_visible("","YES")
 
   #公用欄位給予預設值
   #應用 a14 樣板自動產生(Version:5)    
      #公用欄位新增給值  
      LET g_pjbn_m.pjbnownid = g_user
      LET g_pjbn_m.pjbnowndp = g_dept
      LET g_pjbn_m.pjbncrtid = g_user
      LET g_pjbn_m.pjbncrtdp = g_dept 
      LET g_pjbn_m.pjbncrtdt = cl_get_current()
      LET g_pjbn_m.pjbnmodid = g_user
      LET g_pjbn_m.pjbnmoddt = cl_get_current()
      LET g_pjbn_m.pjbnstus = 'N'
 
 
 
   
   CALL s_transaction_begin()
   
   #add-point:複製輸入前 name="reproduce.head.b_input"
 
   #end add-point
   
   #顯示狀態(stus)圖片
         #應用 a21 樣板自動產生(Version:3)
	  #根據當下狀態碼顯示圖片
      CASE g_pjbn_m.pjbnstus 
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/valid.png")
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/open.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/void.png")
         
      END CASE
 
 
 
   
   #清空key欄位的desc
   
   
   CALL apjm310_input("r")
   
   IF INT_FLAG AND NOT g_master_insert THEN
      LET INT_FLAG = 0
      DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
      DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
      LET INT_FLAG = 0
      INITIALIZE g_pjbn_m.* TO NULL
      INITIALIZE g_pjbo_d TO NULL
      INITIALIZE g_pjbo2_d TO NULL
      INITIALIZE g_pjbo3_d TO NULL
      INITIALIZE g_pjbo4_d TO NULL
      INITIALIZE g_pjbo5_d TO NULL
      INITIALIZE g_pjbo6_d TO NULL
 
      #add-point:複製取消後 name="reproduce.cancel"
      
      #end add-point
      CALL apjm310_show()
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = '' 
      LET g_errparam.code   = 9001 
      LET g_errparam.popup  = FALSE 
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL apjm310_set_act_visible()   
   CALL apjm310_set_act_no_visible()
   
   #將新增的資料併入搜尋條件中
   LET g_pjbn001_t = g_pjbn_m.pjbn001
   LET g_pjbn900_t = g_pjbn_m.pjbn900
 
   
   #組合新增資料的條件
   LET g_add_browse = " pjbnent = " ||g_enterprise|| " AND",
                      " pjbn001 = '", g_pjbn_m.pjbn001, "' "
                      ," AND pjbn900 = '", g_pjbn_m.pjbn900, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL apjm310_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   #add-point:完成複製段落後 name="reproduce.after_reproduce"
   
   #end add-point
   
   CALL apjm310_idx_chk()
   
   LET g_data_owner = g_pjbn_m.pjbnownid      
   LET g_data_dept  = g_pjbn_m.pjbnowndp
   
   #功能已完成,通報訊息中心
   CALL apjm310_msgcentre_notify('reproduce')
 
END FUNCTION
 
