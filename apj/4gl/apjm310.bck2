################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL apjm310_pjbp_set_color()
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By lixh
# Modify.........:
################################################################################
PRIVATE FUNCTION apjm310_pjbp_set_color()
DEFINE   l_pjbu005     LIKE pjbu_t.pjbu005


   INITIALIZE g_pjbo6_d_color[l_ac].* TO NULL

   DECLARE sel_pjbp_cs CURSOR FOR
    SELECT pjbu005 FROM pjbu_t
     WHERE pjbuent = g_enterprise
       AND pjbu001 = g_pjbn_m.pjbn001
       AND pjbu002 = g_pjbo6_d[l_ac].pjbp002
       AND pjbu003 = g_pjbo6_d[l_ac].pjbp003 
       AND pjbu004 = g_pjbn_m.pjbn900  
    
   FOREACH sel_pjbp_cs INTO l_pjbu005 
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "FOREACH:"
         LET g_errparam.popup = TRUE
         CALL cl_err()

         EXIT FOREACH
      END IF
      
      LET l_pjbu005 = cl_replace_str(l_pjbu005,'pjbc','pjbp')

      CASE l_pjbu005
      
         WHEN 'pjbp002' 
            LET  g_pjbo6_d_color[l_ac].pjbp002 = "red" 
            LET  g_pjbo6_d_color[l_ac].pjbp002_desc = "red"  
            
         WHEN 'pjbp003' 
            LET  g_pjbo6_d_color[l_ac].pjbp003 = "red" 
            LET  g_pjbo6_d_color[l_ac].pjbp003_desc = "red" 
          
         WHEN 'pjbp004'          
            LET  g_pjbo6_d_color[l_ac].pjbp004 = "red"  
            
         WHEN 'pjbp005'            
            LET  g_pjbo6_d_color[l_ac].pjbp005 = "red"  
            
         WHEN 'pjbp006'          
            LET  g_pjbo6_d_color[l_ac].pjbp006 = "red"
            
      END CASE
   END FOREACH
END FUNCTION

