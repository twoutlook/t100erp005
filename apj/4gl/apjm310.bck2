#+ 單身陣列填充2
PRIVATE FUNCTION apjm310_b_fill2(pi_idx)
   #add-point:b_fill2段define(客製用) name="b_fill2.define_customerization"
   
   #end add-point
   DEFINE pi_idx                 LIKE type_t.num10
   DEFINE li_ac                  LIKE type_t.num10
   DEFINE li_detail_idx_tmp      LIKE type_t.num10
   DEFINE ls_chk                 LIKE type_t.chr1
   #add-point:b_fill2段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill2.define"
   DEFINE r_success              LIKE type_t.num5
   #end add-point
   
   #add-point:Function前置處理  name="b_fill2.pre_function"
   
   #end add-point
   
   LET li_ac = l_ac 
   
   IF g_detail_idx <= 0 THEN
      RETURN
   END IF
   
   LET li_detail_idx_tmp = g_detail_idx
   
   IF apjm310_fill_chk(3) THEN
      IF (pi_idx = 3 OR pi_idx = 0 ) AND g_pjbo_d.getLength() > 0 THEN
               CALL g_pjbo2_d.clear()
 
         
         #取得該單身上階單身的idx
         LET g_detail_idx = g_detail_idx_list[1]
         
         LET g_sql = "SELECT  DISTINCT pjbq003,pjbq004,pjbq005,pjbq021,pjbq010,pjbq006,pjbq007,pjbq012, 
             pjbq013,pjbq014,pjbq015,pjbq008,pjbq009,pjbq016,pjbq017,pjbq018,pjbq019,pjbq020,pjbq011 , 
             t6.imaal003 ,t7.imaal004 ,t8.oocal003 ,t9.ooail003 FROM pjbq_t",    
                     "",
                                    " LEFT JOIN imaal_t t6 ON t6.imaalent="||g_enterprise||" AND t6.imaal001=pjbq005 AND t6.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t7 ON t7.imaalent="||g_enterprise||" AND t7.imaal001=pjbq005 AND t7.imaal002='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t8 ON t8.oocalent="||g_enterprise||" AND t8.oocal001=pjbq006 AND t8.oocal002='"||g_dlang||"' ",
               " LEFT JOIN ooail_t t9 ON t9.ooailent="||g_enterprise||" AND t9.ooail001=pjbq012 AND t9.ooail002='"||g_dlang||"' ",
 
                     " WHERE pjbqent=? AND pjbq001=? AND pjbq900=? AND pjbq002=?"
         
         IF NOT cl_null(g_wc2_table3) THEN
            LET g_sql = g_sql CLIPPED," AND ",g_wc2_table3 CLIPPED
         END IF
         
         LET g_sql = g_sql, " ORDER BY  pjbq_t.pjbq003" 
                            
         #add-point:單身填充前 name="b_fill2.before_fill3"
         
         #end add-point
         
         #先清空資料
               CALL g_pjbo2_d.clear()
 
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE apjm310_pb3 FROM g_sql
         DECLARE b_fill_curs3 CURSOR FOR apjm310_pb3
         
         OPEN b_fill_curs3 USING g_enterprise,g_pjbn_m.pjbn001,g_pjbn_m.pjbn900,g_pjbo_d[g_detail_idx].pjbo002 
 
         LET l_ac = 1
         FOREACH b_fill_curs3 INTO g_pjbo2_d[l_ac].pjbq003,g_pjbo2_d[l_ac].pjbq004,g_pjbo2_d[l_ac].pjbq005, 
             g_pjbo2_d[l_ac].pjbq021,g_pjbo2_d[l_ac].pjbq010,g_pjbo2_d[l_ac].pjbq006,g_pjbo2_d[l_ac].pjbq007, 
             g_pjbo2_d[l_ac].pjbq012,g_pjbo2_d[l_ac].pjbq013,g_pjbo2_d[l_ac].pjbq014,g_pjbo2_d[l_ac].pjbq015, 
             g_pjbo2_d[l_ac].pjbq008,g_pjbo2_d[l_ac].pjbq009,g_pjbo2_d[l_ac].pjbq016,g_pjbo2_d[l_ac].pjbq017, 
             g_pjbo2_d[l_ac].pjbq018,g_pjbo2_d[l_ac].pjbq019,g_pjbo2_d[l_ac].pjbq020,g_pjbo2_d[l_ac].pjbq011, 
             g_pjbo2_d[l_ac].pjbq005_desc,g_pjbo2_d[l_ac].pjbq005_desc_desc,g_pjbo2_d[l_ac].pjbq006_desc, 
             g_pjbo2_d[l_ac].pjbq012_desc 
            IF SQLCA.sqlcode THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               EXIT FOREACH
