################################################################################
# Descriptions...: 發票資訊子報表
# Memo...........:
# Usage..........: CALL aisr310_g01_subrep05(sr5)
# Date & Author..: 2016/11/15 By 08732
# Modify.........:
################################################################################
PRIVATE REPORT aisr310_g01_subrep05(sr5)
DEFINE sr5            sr5_r
DEFINE l_isat103_sum  LIKE isat_t.isat103  
DEFINE l_isat104_sum  LIKE isat_t.isat104 
DEFINE l_isat105_sum  LIKE isat_t.isat105 
DEFINE l_isat113_sum  LIKE isat_t.isat113 
DEFINE l_isat114_sum  LIKE isat_t.isat114 
DEFINE l_isat115_sum  LIKE isat_t.isat115
DEFINE l_isat003_show LIKE type_t.chr10
    
    
    ORDER EXTERNAL BY sr5.isatdocno
    
    
    FORMAT
        
            
        ON EVERY ROW
            IF sr5.l_isai002 = '1' THEN
               LET l_isat003_show = 'N'
            ELSE
               LET l_isat003_show = 'Y'
            END IF
            
            PRINTX l_isat003_show
            PRINTX g_grNumFmt.*
            PRINTX sr5.*
            
        AFTER GROUP OF sr5.isatdocno
           
           LET l_isat103_sum = GROUP SUM(sr5.isat103)
           LET l_isat104_sum = GROUP SUM(sr5.isat104)
           LET l_isat105_sum = GROUP SUM(sr5.isat105)
           LET l_isat113_sum = GROUP SUM(sr5.isat113)
           LET l_isat114_sum = GROUP SUM(sr5.isat114)
           LET l_isat115_sum = GROUP SUM(sr5.isat115)
           
           PRINTX l_isat103_sum,l_isat104_sum,l_isat105_sum,
                  l_isat113_sum,l_isat114_sum,l_isat115_sum
END REPORT

 
{</section>}
 
