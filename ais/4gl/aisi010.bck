# 交易對象帶值
PRIVATE FUNCTION aisi010_isaa014_get()
   DEFINE l_pmaa027   LIKE pmaa_t.pmaa027
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_isaa_m.isaa014
   CALL ap_ref_array2(g_ref_fields,"SELECT pmaa003,pmaa027,pmaa016 FROM pmaa_t WHERE pmaaent='"||g_enterprise||"' AND pmaa001=? ","") RETURNING g_rtn_fields
   LET g_isaa_m.isaa015 = '', g_rtn_fields[1] , ''
   LET l_pmaa027 = '', g_rtn_fields[2] , ''
   LET g_isaa_m.isaa019 = '', g_rtn_fields[3] , ''
   DISPLAY g_isaa_m.isaa015 TO isaa015
   DISPLAY g_isaa_m.isaa019 TO isaa019
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_isaa_m.isaa014
   CALL ap_ref_array2(g_ref_fields,"SELECT pmaal004 FROM pmaal_t WHERE pmaalent='"||g_enterprise||"' AND pmaal001=? ","") RETURNING g_rtn_fields
   LET g_isaa_m.isaa016 = '', g_rtn_fields[1] , ''
   DISPLAY g_isaa_m.isaa016 TO isaa016
   
   SELECT oofc012 
     INTO g_isaa_m.isaa017
     FROM oofc_t 
    WHERE oofcent = g_enterprise
      AND oofc008 = '1'   #一般電話 
      AND oofc010 = 'Y' 
      AND oofc002 = l_pmaa027
   
   #151117-00001#1 mark ------
   #SELECT oofb017 
   #  INTO g_isaa_m.isaa018
   #  FROM oofb_t
   # WHERE oofbent = g_enterprise
   #   AND oofb002 = l_pmaa027
   #   AND oofb008 = '1'
   #   AND oofb010 = 'Y' 
   #DISPLAY g_isaa_m.isaa018 TO isaa018
   #151117-00001#1 mark end---
   #151117-00001#1 add ------
   SELECT oofb017 INTO g_isaa_m.isaa031
     FROM oofb_t,ooef_t 
    WHERE oofbent = g_enterprise
      AND oofb008  ='1' # 登記地址
      AND oofb002 = ooef012 
      AND ooef001 = g_isaa_m.isaa001
      AND oofb010 ='Y'
   DISPLAY BY NAME g_isaa_m.isaa031
   #151117-00001#1 add end---
   
   SELECT oofc012 
     INTO g_isaa_m.isaa021
     FROM oofc_t 
    WHERE oofcent = g_enterprise
