         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL aisi010_set_act_visible()   
   CALL aisi010_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " isaaent = " ||g_enterprise|| " AND",
                      " isaa001 = '", g_isaa_m.isaa001, "' "
 
