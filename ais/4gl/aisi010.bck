   #end add-point  
   
   #add-point:Function前置處理 name="statechange.before"
   
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_isaa_m.isaa001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN aisi010_cl USING g_enterprise,g_isaa_m.isaa001
   IF STATUS THEN
      CLOSE aisi010_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aisi010_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE aisi010_master_referesh USING g_isaa_m.isaa001 INTO g_isaa_m.isaa001,g_isaa_m.isaa002,g_isaa_m.isaa003, 
       g_isaa_m.isaa030,g_isaa_m.isaa027,g_isaa_m.isaa031,g_isaa_m.isaa004,g_isaa_m.isaa028,g_isaa_m.isaa005, 
       g_isaa_m.isaa029,g_isaa_m.isaa006,g_isaa_m.isaa032,g_isaa_m.isaa007,g_isaa_m.isaa008,g_isaa_m.isaa009, 
       g_isaa_m.isaa022,g_isaa_m.isaa023,g_isaa_m.isaa010,g_isaa_m.isaa012,g_isaa_m.isaa013,g_isaa_m.isaastus, 
       g_isaa_m.isaa024,g_isaa_m.isaa011,g_isaa_m.isaa025,g_isaa_m.isaa026,g_isaa_m.isaa014,g_isaa_m.isaa015, 
       g_isaa_m.isaa016,g_isaa_m.isaa017,g_isaa_m.isaa018,g_isaa_m.isaa019,g_isaa_m.isaa020,g_isaa_m.isaa021, 
       g_isaa_m.isaaownid,g_isaa_m.isaaowndp,g_isaa_m.isaacrtid,g_isaa_m.isaacrtdp,g_isaa_m.isaacrtdt, 
       g_isaa_m.isaamodid,g_isaa_m.isaamoddt,g_isaa_m.isaaownid_desc,g_isaa_m.isaaowndp_desc,g_isaa_m.isaacrtid_desc, 
       g_isaa_m.isaacrtdp_desc,g_isaa_m.isaamodid_desc
   
 
   #檢查是否允許此動作
   IF NOT aisi010_action_chk() THEN
      CLOSE aisi010_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_isaa_m.isaa001,g_isaa_m.ooef003,g_isaa_m.ooefl006,g_isaa_m.ooall004,g_isaa_m.ooef002, 
       g_isaa_m.isaa002,g_isaa_m.isaa003,g_isaa_m.isaa030,g_isaa_m.isaa027,g_isaa_m.isaa031,g_isaa_m.isaa004, 
       g_isaa_m.isaa028,g_isaa_m.isaa005,g_isaa_m.isaa029,g_isaa_m.isaa006,g_isaa_m.isaa032,g_isaa_m.isaa007, 
       g_isaa_m.isaa008,g_isaa_m.isaa009,g_isaa_m.ooefl004_1,g_isaa_m.isaa022,g_isaa_m.isaa023,g_isaa_m.isaa010, 
       g_isaa_m.isaa012,g_isaa_m.isaa013,g_isaa_m.isaastus,g_isaa_m.isaa024,g_isaa_m.isaa011,g_isaa_m.isaa025, 
       g_isaa_m.l_isaa025_desc,g_isaa_m.isaa026,g_isaa_m.isaa014,g_isaa_m.isaa015,g_isaa_m.isaa016,g_isaa_m.isaa017, 
       g_isaa_m.isaa018,g_isaa_m.isaa019,g_isaa_m.isaa020,g_isaa_m.isaa021,g_isaa_m.isaaownid,g_isaa_m.isaaownid_desc, 
       g_isaa_m.isaaowndp,g_isaa_m.isaaowndp_desc,g_isaa_m.isaacrtid,g_isaa_m.isaacrtid_desc,g_isaa_m.isaacrtdp, 
       g_isaa_m.isaacrtdp_desc,g_isaa_m.isaacrtdt,g_isaa_m.isaamodid,g_isaa_m.isaamodid_desc,g_isaa_m.isaamoddt 
 
 
   CASE g_isaa_m.isaastus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_isaa_m.isaastus
            
            WHEN "N"
               HIDE OPTION "inactive"
            WHEN "Y"
               HIDE OPTION "active"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      
      #end add-point
      
      
	  
      ON ACTION inactive
         IF cl_auth_chk_act("inactive") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.inactive"
         SELECT ooefstus INTO l_ooefstus 
           FROM ooef_t
          WHERE ooefent = g_enterprise
            AND ooef001 = g_isaa_m.isaa001
         IF l_ooefstus = 'Y' THEN 
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = 'ais-00118'
            LET g_errparam.extend = g_isaa_m.isaa001
            LET g_errparam.popup = TRUE
            CALL cl_err()
            CALL s_transaction_end('N','0')   #160812-00017#4 20160815 add by beckxie
            RETURN 
         END IF
            #end add-point
         END IF
         EXIT MENU
      ON ACTION active
         IF cl_auth_chk_act("active") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.active"
            
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      ) OR 
      g_isaa_m.isaastus = lc_state OR cl_null(lc_state) THEN
      CLOSE aisi010_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   
   #end add-point
   
   LET g_isaa_m.isaamodid = g_user
   LET g_isaa_m.isaamoddt = cl_get_current()
   LET g_isaa_m.isaastus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE isaa_t 
      SET (isaastus,isaamodid,isaamoddt) 
        = (g_isaa_m.isaastus,g_isaa_m.isaamodid,g_isaa_m.isaamoddt)     
    WHERE isaaent = g_enterprise AND isaa001 = g_isaa_m.isaa001
 
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE aisi010_master_referesh USING g_isaa_m.isaa001 INTO g_isaa_m.isaa001,g_isaa_m.isaa002, 
          g_isaa_m.isaa003,g_isaa_m.isaa030,g_isaa_m.isaa027,g_isaa_m.isaa031,g_isaa_m.isaa004,g_isaa_m.isaa028, 
          g_isaa_m.isaa005,g_isaa_m.isaa029,g_isaa_m.isaa006,g_isaa_m.isaa032,g_isaa_m.isaa007,g_isaa_m.isaa008, 
          g_isaa_m.isaa009,g_isaa_m.isaa022,g_isaa_m.isaa023,g_isaa_m.isaa010,g_isaa_m.isaa012,g_isaa_m.isaa013, 
          g_isaa_m.isaastus,g_isaa_m.isaa024,g_isaa_m.isaa011,g_isaa_m.isaa025,g_isaa_m.isaa026,g_isaa_m.isaa014, 
          g_isaa_m.isaa015,g_isaa_m.isaa016,g_isaa_m.isaa017,g_isaa_m.isaa018,g_isaa_m.isaa019,g_isaa_m.isaa020, 
          g_isaa_m.isaa021,g_isaa_m.isaaownid,g_isaa_m.isaaowndp,g_isaa_m.isaacrtid,g_isaa_m.isaacrtdp, 
          g_isaa_m.isaacrtdt,g_isaa_m.isaamodid,g_isaa_m.isaamoddt,g_isaa_m.isaaownid_desc,g_isaa_m.isaaowndp_desc, 
          g_isaa_m.isaacrtid_desc,g_isaa_m.isaacrtdp_desc,g_isaa_m.isaamodid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_isaa_m.isaa001,g_isaa_m.ooef003,g_isaa_m.ooefl006,g_isaa_m.ooall004,g_isaa_m.ooef002, 
          g_isaa_m.isaa002,g_isaa_m.isaa003,g_isaa_m.isaa030,g_isaa_m.isaa027,g_isaa_m.isaa031,g_isaa_m.isaa004, 
          g_isaa_m.isaa028,g_isaa_m.isaa005,g_isaa_m.isaa029,g_isaa_m.isaa006,g_isaa_m.isaa032,g_isaa_m.isaa007, 
          g_isaa_m.isaa008,g_isaa_m.isaa009,g_isaa_m.ooefl004_1,g_isaa_m.isaa022,g_isaa_m.isaa023,g_isaa_m.isaa010, 
          g_isaa_m.isaa012,g_isaa_m.isaa013,g_isaa_m.isaastus,g_isaa_m.isaa024,g_isaa_m.isaa011,g_isaa_m.isaa025, 
          g_isaa_m.l_isaa025_desc,g_isaa_m.isaa026,g_isaa_m.isaa014,g_isaa_m.isaa015,g_isaa_m.isaa016, 
          g_isaa_m.isaa017,g_isaa_m.isaa018,g_isaa_m.isaa019,g_isaa_m.isaa020,g_isaa_m.isaa021,g_isaa_m.isaaownid, 
          g_isaa_m.isaaownid_desc,g_isaa_m.isaaowndp,g_isaa_m.isaaowndp_desc,g_isaa_m.isaacrtid,g_isaa_m.isaacrtid_desc, 
          g_isaa_m.isaacrtdp,g_isaa_m.isaacrtdp_desc,g_isaa_m.isaacrtdt,g_isaa_m.isaamodid,g_isaa_m.isaamodid_desc, 
          g_isaa_m.isaamoddt
   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
   
   #end add-point
 
   #add-point:statechange段結束前 name="statechange.after"
   
   #end add-point  
 
   CLOSE aisi010_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aisi010_msgcentre_notify('statechange:'||lc_state)
   
