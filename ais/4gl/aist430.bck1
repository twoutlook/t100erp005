       t0.iscacrtid,t0.iscacrtdp,t0.iscacrtdt,t0.iscamodid,t0.iscamoddt,t0.isca701,t0.isca702,t0.isca703, 
       t0.isca704,t0.isca705,t0.isca707,t0.isca706,t0.isca708,t0.isca805,t0.isca806,t0.isca807,t0.isca801, 
       t0.isca803,t0.isca802,t0.isca804,t1.ooag011 ,t2.ooefl003 ,t3.ooag011 ,t4.ooefl003 ,t5.ooag011", 
 
               " FROM isca_t t0",
                              " LEFT JOIN ooag_t t1 ON t1.ooagent="||g_enterprise||" AND t1.ooag001=t0.iscaownid  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent="||g_enterprise||" AND t2.ooefl001=t0.iscaowndp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent="||g_enterprise||" AND t3.ooag001=t0.iscacrtid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent="||g_enterprise||" AND t4.ooefl001=t0.iscacrtdp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=t0.iscamodid  ",
 
               " WHERE t0.iscaent = " ||g_enterprise|| " AND t0.iscasite = ? AND t0.isca001 = ? AND t0.isca002 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
