{<section id="aist430.modify" >}
#+ 資料修改
PRIVATE FUNCTION aist430_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   
   #end add-point
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   
   #end add-point
   
   #add-point:Function前置處理 name="modify.pre_function"
   
   #end add-point
   
   #先確定key值無遺漏
   IF g_isca_m.iscasite IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_iscasite_t = g_isca_m.iscasite
   LET g_isca001_t = g_isca_m.isca001
   LET g_isca002_t = g_isca_m.isca002
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN aist430_cl USING g_enterprise,g_isca_m.iscasite,g_isca_m.isca001,g_isca_m.isca002
   IF SQLCA.SQLCODE THEN    #(ver:49)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aist430_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE
      LET g_errparam.popup = TRUE 
      CLOSE aist430_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aist430_master_referesh USING g_isca_m.iscasite,g_isca_m.isca001,g_isca_m.isca002 INTO g_isca_m.iscasite, 
       g_isca_m.isca809,g_isca_m.isca001,g_isca_m.isca002,g_isca_m.iscaownid,g_isca_m.iscaowndp,g_isca_m.iscacrtid, 
       g_isca_m.iscacrtdp,g_isca_m.iscacrtdt,g_isca_m.iscamodid,g_isca_m.iscamoddt,g_isca_m.isca701, 
       g_isca_m.isca702,g_isca_m.isca703,g_isca_m.isca704,g_isca_m.isca705,g_isca_m.isca707,g_isca_m.isca706, 
       g_isca_m.isca708,g_isca_m.isca805,g_isca_m.isca806,g_isca_m.isca807,g_isca_m.isca801,g_isca_m.isca803, 
       g_isca_m.isca802,g_isca_m.isca804,g_isca_m.iscaownid_desc,g_isca_m.iscaowndp_desc,g_isca_m.iscacrtid_desc, 
       g_isca_m.iscacrtdp_desc,g_isca_m.iscamodid_desc
 
   #檢查是否允許此動作
   IF NOT aist430_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_isca_m_mask_o.* =  g_isca_m.*
   CALL aist430_isca_t_mask()
   LET g_isca_m_mask_n.* =  g_isca_m.*
   
   
 
   #顯示資料
   CALL aist430_show()
   
   WHILE TRUE
      LET g_isca_m.iscasite = g_iscasite_t
      LET g_isca_m.isca001 = g_isca001_t
      LET g_isca_m.isca002 = g_isca002_t
 
      
      #寫入修改者/修改日期資訊
      LET g_isca_m.iscamodid = g_user 
LET g_isca_m.iscamoddt = cl_get_current()
LET g_isca_m.iscamodid_desc = cl_get_username(g_isca_m.iscamodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      
      #end add-point
 
      #資料輸入
      CALL aist430_input("u")     
 
      #add-point:modify段修改後 name="modify.after_input"
      
      #end add-point
      
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         LET g_isca_m.* = g_isca_m_t.*
         CALL aist430_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE isca_t SET (iscamodid,iscamoddt) = (g_isca_m.iscamodid,g_isca_m.iscamoddt)
       WHERE iscaent = g_enterprise AND iscasite = g_iscasite_t
         AND isca001 = g_isca001_t
         AND isca002 = g_isca002_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL aist430_set_act_visible()
   CALL aist430_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " iscaent = " ||g_enterprise|| " AND",
                      " iscasite = '", g_isca_m.iscasite, "' "
                      ," AND isca001 = '", g_isca_m.isca001, "' "
                      ," AND isca002 = '", g_isca_m.isca002, "' "
 
   #填到對應位置
   CALL aist430_browser_fill(g_wc,"")
 
   CLOSE aist430_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aist430_msgcentre_notify('modify')
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
 
