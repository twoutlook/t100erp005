#+ 資料刪除 
PRIVATE FUNCTION aist430_delete()
   #add-point:delete段define(客製用) name="delete.define_customerization"
   
   #end add-point
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="delete.define"
   
   #end add-point  
   
   #add-point:Function前置處理  name="delete.pre_function"
   
   #end add-point
   
   #先確定key值無遺漏
   IF g_isca_m.iscasite IS NULL
   OR g_isca_m.isca001 IS NULL
   OR g_isca_m.isca002 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
    
   LET g_iscasite_t = g_isca_m.iscasite
   LET g_isca001_t = g_isca_m.isca001
   LET g_isca002_t = g_isca_m.isca002
 
   
   
 
   OPEN aist430_cl USING g_enterprise,g_isca_m.iscasite,g_isca_m.isca001,g_isca_m.isca002
   IF SQLCA.SQLCODE THEN    #(ver:49)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aist430_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE
      LET g_errparam.popup = TRUE 
      CLOSE aist430_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aist430_master_referesh USING g_isca_m.iscasite,g_isca_m.isca001,g_isca_m.isca002 INTO g_isca_m.iscasite, 
       g_isca_m.isca809,g_isca_m.isca001,g_isca_m.isca002,g_isca_m.iscaownid,g_isca_m.iscaowndp,g_isca_m.iscacrtid, 
       g_isca_m.iscacrtdp,g_isca_m.iscacrtdt,g_isca_m.iscamodid,g_isca_m.iscamoddt,g_isca_m.isca701, 
       g_isca_m.isca702,g_isca_m.isca703,g_isca_m.isca704,g_isca_m.isca705,g_isca_m.isca707,g_isca_m.isca706, 
       g_isca_m.isca708,g_isca_m.isca805,g_isca_m.isca806,g_isca_m.isca807,g_isca_m.isca801,g_isca_m.isca803, 
       g_isca_m.isca802,g_isca_m.isca804,g_isca_m.iscaownid_desc,g_isca_m.iscaowndp_desc,g_isca_m.iscacrtid_desc, 
       g_isca_m.iscacrtdp_desc,g_isca_m.iscamodid_desc
   
   
   #檢查是否允許此動作
   IF NOT aist430_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_isca_m_mask_o.* =  g_isca_m.*
   CALL aist430_isca_t_mask()
   LET g_isca_m_mask_n.* =  g_isca_m.*
   
   #將最新資料顯示到畫面上
   CALL aist430_show()
   
   IF cl_ask_delete() THEN
 
      #add-point:單頭刪除前 name="delete.head.b_delete"
      
      #end add-point
 
      #應用 a47 樣板自動產生(Version:4)
      #刪除相關文件
      CALL aist430_set_pk_array()
      #add-point:相關文件刪除前 name="delete.befroe.related_document_remove"
      
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
 
 
      DELETE FROM isca_t 
       WHERE iscaent = g_enterprise AND iscasite = g_isca_m.iscasite 
         AND isca001 = g_isca_m.isca001 
         AND isca002 = g_isca_m.isca002 
 
 
      #add-point:單頭刪除中 name="delete.head.m_delete"
      
      #end add-point
         
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "isca_t:",SQLERRMESSAGE 
         LET g_errparam.code = SQLCA.SQLCODE
         LET g_errparam.popup = FALSE 
         CALL s_transaction_end('N','0')
         CALL cl_err()
      END IF
  
      
      
      #add-point:單頭刪除後 name="delete.head.a_delete"
      
      #end add-point
      
       
 
      #修改歷程記錄(刪除)
      LET g_log1 = util.JSON.stringify(g_isca_m)   #(ver:49)
      IF NOT cl_log_modified_record(g_log1,'') THEN    #(ver:49)
         CLOSE aist430_cl
         CALL s_transaction_end('N','0')
         RETURN
      END IF
      
      CLEAR FORM
      CALL aist430_ui_browser_refresh()
      
      #確保畫面上保有資料
      IF g_browser_cnt > 0 THEN
         #CALL aist430_browser_fill(g_wc,"")
         CALL aist430_fetch("P")
      ELSE
         CLEAR FORM
      END IF
      CALL s_transaction_end('Y','0')
   ELSE    
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE aist430_cl
 
   #功能已完成,通報訊息中心
   CALL aist430_msgcentre_notify('delete')
 
   #add-point:單頭刪除完成後 name="delete.a_delete"
   
   #end add-point
 
END FUNCTION
 
