#+ 選單功能實際執行處
PRIVATE FUNCTION aisp390_ui_dialog()
 
   #add-point:ui_dialog段define (客製用) name="ui_dialog.define_customerization"
   
   #end add-point
   DEFINE li_exit  LIKE type_t.num5    #判別是否為離開作業
   DEFINE li_idx   LIKE type_t.num10
   DEFINE ls_js    STRING
   DEFINE ls_wc    STRING
   DEFINE l_dialog ui.DIALOG
   DEFINE lc_param type_parameter
   #add-point:ui_dialog段define name="ui_dialog.define"
   DEFINE l_oofg_return      DYNAMIC ARRAY OF RECORD
                   oofg019       LIKE oofg_t.oofg019,   #field
                   oofg020       LIKE oofg_t.oofg020    #value
                             END RECORD
   DEFINE l_isac003          LIKE isac_t.isac003
   DEFINE l_prog             LIKE type_t.chr20
   #end add-point
   
   #add-point:ui_dialog段before dialog name="ui_dialog.before_dialog"
 
   #end add-point
 
   WHILE TRUE
      #add-point:ui_dialog段before dialog2 name="ui_dialog.before_dialog2"
      
      #end add-point
 
      DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
         #應用 a57 樣板自動產生(Version:3)
         INPUT BY NAME g_master.xrcasite,g_master.xrcald,g_master.xrcacomp,g_master.isaf056,g_master.isaf008, 
             g_master.l_ratetype,g_master.isafdocno,g_master.isafdocdt,g_master.isaesite,g_master.isae001, 
             g_master.isae014,g_master.isae008,g_master.l_limit,g_master.isae007,g_master.isae012,g_master.xrcadocno, 
             g_master.xrca007,g_master.xrca063 
            ATTRIBUTE(WITHOUT DEFAULTS)
            
            #自訂ACTION(master_input)
            
         
            BEFORE INPUT
               #add-point:資料輸入前 name="input.m.before_input"
               
               #end add-point
         
                     #應用 a02 樣板自動產生(Version:2)
         AFTER FIELD xrcasite
            
            #add-point:AFTER FIELD xrcasite name="input.a.xrcasite"
            IF NOT cl_null(g_master.xrcasite) THEN
               #以目前的資料重展結構,避免[帳套]有值時會比對錯誤,在s_fin_account_center_with_ld_chk做勾稽時會依據這個結構做是否有符合的帳套
               CALL s_fin_account_center_sons_query('3',g_master.xrcasite,g_today,'1')
               #如果帳務中心不同 且帳套有值 先依據現在的帳務中心跟帳套勾稽一次---(S)---
               #避免USER 在帳務中心跟帳套卡住走不了 增加對帳套有資料的處理
               IF g_master.xrcasite != g_master_t.xrcasite OR cl_null(g_master_t.xrcasite) THEN
                  CALL s_fin_account_center_with_ld_chk(g_master.xrcasite,g_master.xrcald,g_user,'3','N','',g_today) RETURNING g_sub_success,g_errno
                  IF NOT g_sub_success THEN
                     #勾稽失敗:目前的帳套不在這個帳務中心下 因此預設值給帳務中心的主帳套
                     CALL s_fin_orga_get_comp_ld(g_master.xrcasite) RETURNING g_sub_success,g_errno,g_master.xrcacomp,g_master.xrcald
                     #判斷這個主帳套使用者是否有權限
                     CALL s_fin_account_center_with_ld_chk(g_master.xrcasite,g_master.xrcald,g_user,'3','N','',g_today) RETURNING g_sub_success,g_errno
                  END IF
                  #判斷完成後 若勾稽失敗則回復舊值
                  IF NOT g_sub_success THEN
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code = g_errno
                     LET g_errparam.extend = ''
                     LET g_errparam.popup = TRUE
                     CALL cl_err()
                     LET g_master.xrcasite = g_master_t.xrcasite
                     CALL s_desc_get_department_desc(g_master.xrcasite) RETURNING g_master.xrcasite_desc
                     LET g_master.xrcald = g_master_t.xrcald
                     CALL s_desc_get_ld_desc(g_master.xrcald) RETURNING g_master.xrcald_desc
                     DISPLAY BY NAME g_master.xrcasite,g_master.xrcasite_desc,g_master.xrcald,g_master.xrcald_desc
                     NEXT FIELD CURRENT
                  END IF
               END IF
               #帳務中心與帳套關係
               CALL s_fin_account_center_with_ld_chk(g_master.xrcasite,g_master.xrcald,g_user,'3','N','',g_today) RETURNING g_sub_success,g_errno
               IF NOT cl_null(g_errno) THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = g_errno
                  LET g_errparam.extend = g_master.xrcasite
                  LET g_errparam.popup = TRUE
                  CALL cl_err()
                  LET g_master.xrcasite = g_master_t.xrcasite
                  CALL s_desc_get_department_desc(g_master.xrcasite) RETURNING g_master.xrcasite_desc
                  LET g_master.xrcald = g_master_t.xrcald
                  CALL s_desc_get_ld_desc(g_master.xrcald) RETURNING g_master.xrcald_desc
                  DISPLAY BY NAME g_master.xrcasite,g_master.xrcasite_desc,g_master.xrcald,g_master.xrcald_desc
                  NEXT FIELD CURRENT
               END IF
               #關帳日檢核
               CALL s_axrt300_date_chk(g_master.xrcasite,g_today) RETURNING g_sub_success
               IF NOT g_sub_success THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.extend = ""
                  LET g_errparam.code   = "axr-00299"
                  LET g_errparam.popup  = TRUE
                  CALL cl_err()
                  LET g_master.xrcasite = g_master_t.xrcasite
                  NEXT FIELD CURRENT
               END IF
               LET g_master_t.xrcasite = g_master.xrcasite
               CALL s_desc_get_department_desc(g_master.xrcasite) RETURNING g_master.xrcasite_desc
               DISPLAY BY NAME g_master.xrcasite_desc
               CALL s_fin_account_center_sons_query('3',g_master.xrcasite,g_today,'1')
               #據點
               CALL s_fin_account_center_sons_str() RETURNING g_wc_xrcasite
               CALL s_fin_get_wc_str(g_wc_xrcasite) RETURNING g_wc_xrcasite
               #取得帳務組織下所屬成員之帳別
               CALL s_fin_account_center_ld_str() RETURNING g_wc_ld
               CALL s_fin_get_wc_str(g_wc_ld) RETURNING g_wc_ld
               LET g_master_t.xrcasite = g_master.xrcasite
               #稅區
               SELECT ooef019 INTO g_ooef019
                 FROM ooef_t
                WHERE ooefent = g_enterprise
                  AND ooef001 = g_master.xrcacomp
               #稅區參數資料檔
               SELECT isai002,isai006,isai008 INTO g_isai.*
                 FROM isai_t
                WHERE isaient = g_enterprise
                  AND isai001 = g_ooef019
               IF NOT cl_null(g_master.isaf056)THEN
                  IF g_master.isaf056 = '1' THEN
                     #出貨
                     SELECT isao014 INTO g_master.isafdocno
                       FROM isao_t
                      WHERE isaoent = g_enterprise
                        AND isaosite = g_master.xrcasite
                  ELSE
                     #折讓
                     SELECT isao015 INTO g_master.isafdocno
                       FROM isao_t
                      WHERE isaoent = g_enterprise
                        AND isaosite = g_master.xrcasite
                  END IF
                  CALL s_aooi200_fin_get_slip_desc(g_master.isafdocno) RETURNING g_master.isafdocno_desc
                  DISPLAY BY NAME g_master.isafdocno,g_master.isafdocno_desc
               END IF
               #161024-00065#2 add ------
               #取得aoos020的參數S-FIN-2023(流通門店銷售立帳方式)
               CALL cl_get_para(g_enterprise,g_master.xrcacomp,'S-FIN-2023') RETURNING g_sfin2023
               #161024-00065#2 add end---
            END IF
            #END add-point
            
 
 
         #應用 a01 樣板自動產生(Version:2)
         BEFORE FIELD xrcasite
            #add-point:BEFORE FIELD xrcasite name="input.b.xrcasite"
            
            #END add-point
 
 
