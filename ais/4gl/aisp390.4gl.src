   DEFINE l_array       DYNAMIC ARRAY OF RECORD
                           chr  LIKE type_t.chr1000,
                           dat  LIKE type_t.dat
                        END RECORD
   DEFINE l_exist       LIKE type_t.chr1
   DEFINE l_strno1      LIKE xrca_t.xrcadocno
   DEFINE l_endno1      LIKE xrca_t.xrcadocno
   DEFINE l_strno2      LIKE xrca_t.xrcadocno
   DEFINE l_endno2      LIKE xrca_t.xrcadocno
   #end add-point
 
  #INITIALIZE lc_param TO NULL           #p類不可以清空
   CALL util.JSON.parse(ls_js,lc_param)  #r類作業被t類呼叫時使用, p類主要解開參數處
   LET li_p01_status = 1
 
  #IF lc_param.wc IS NOT NULL THEN
  #   LET g_bgjob = "T"       #特殊情況,此為t類作業鬆耦合串入報表主程式使用
  #END IF
 
   #add-point:process段前處理 name="process.pre_process"
 
   #end add-point
 
   #預先計算progressbar迴圈次數
   IF g_bgjob <> "Y" THEN
      #add-point:process段count_progress name="process.count_progress"
      
      #end add-point
   END IF
 
   #主SQL及相關FOREACH前置處理
#  DECLARE aisp390_process_cs CURSOR FROM ls_sql
#  FOREACH aisp390_process_cs INTO
   #add-point:process段process name="process.process"
   LET l_exist = "N"
   
   LET l_array[1].chr  = g_master.xrcasite    #帳務中心
   LET l_array[2].chr  = g_master.xrcald      #帳套
   LET l_array[3].chr  = g_master.xrcacomp    #法人
   LET l_array[4].chr  = g_master.isaf056     #發票性質
   LET l_array[5].chr  = g_master.isaf008     #發票類型
   LET l_array[6].chr  = g_master.l_ratetype  #匯率
   LET l_array[7].chr  = g_master.isafdocno   #發票單別
   LET l_array[1].dat  = g_master.isafdocdt   #發票日期
   LET l_array[8].chr  = g_master.isaesite    #營運據點
   LET l_array[9].chr  = g_master.isae001     #發票簿號
   LET l_array[2].dat  = g_master.isae014     #已開發票日期
   LET l_array[10].chr = g_master.isae008     #發票代碼
   LET l_array[11].chr = g_master.l_limit     #發票限額
   LET l_array[12].chr = g_master.isae007     #發票字軌
   LET l_array[13].chr = g_master.isae012     #下次列印號碼
   LET l_array[14].chr = g_master.xrcadocno   #應收帳款單號碼
   LET l_array[15].chr = g_master.xrca007     #帳款類別
   LET l_array[16].chr = g_master.xrca063     #整帳批序號
   LET l_array[17].chr = g_master.wc          #g_master.wc
   LET l_array[18].chr = g_wc_xrcasite        #g_wc_xrcasite
   
   
   CALL cl_err_collect_init()
   CALL s_aisp390_ins_entrance(l_array) RETURNING g_sub_success,l_exist,l_strno1,l_endno1,l_strno2,l_endno2
   IF l_exist = "N" THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'asf-00230'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()
   ELSE
      IF g_sub_success THEN
         #aist310成功單號
         IF NOT cl_null(l_strno1) THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = 'asf-00251'
            LET g_errparam.replace[1] = l_strno1
            LET g_errparam.replace[2] = l_endno1
            LET g_errparam.extend = ''
            LET g_errparam.popup = TRUE
            CALL cl_err()
         END IF
         #axrt300成功單號
         IF NOT cl_null(l_strno2) THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = 'asf-00251'
            LET g_errparam.replace[1] = l_strno2
            LET g_errparam.replace[2] = l_endno2
            LET g_errparam.extend = ''
            LET g_errparam.popup = TRUE
            CALL cl_err()
         END IF
      ELSE
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'aap-00187'   #單據產生失敗
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()
      END IF
   END IF
   CALL cl_err_collect_show()
   #end add-point
#  END FOREACH
 
   IF g_bgjob = "N" THEN
      #前景作業完成處理
      #add-point:process段foreground完成處理 name="process.foreground_finish"
      
      #end add-point
      CALL cl_ask_confirm3("std-00012","")
   ELSE
      #背景作業完成處理
      #add-point:process段background完成處理 name="process.background_finish"
      
      #end add-point
      CALL cl_schedule_exec_call(li_p01_status)
   END IF
 
   #呼叫訊息中心傳遞本關完成訊息
   CALL aisp390_msgcentre_notify()
 
END FUNCTION
 
