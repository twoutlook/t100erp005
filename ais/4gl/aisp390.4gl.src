            #END add-point
 
 
         #應用 a02 樣板自動產生(Version:2)
         AFTER FIELD isaf056
            
            #add-point:AFTER FIELD isaf056 name="input.a.isaf056"
            
            #END add-point
            
 
 
         #應用 a04 樣板自動產生(Version:3)
         ON CHANGE isaf056
            #add-point:ON CHANGE isaf056 name="input.g.isaf056"
            IF NOT cl_null(g_master.isaf056)THEN
               IF g_master.isaf056 = '1' THEN
                  #出貨
                  SELECT isao014 INTO g_master.isafdocno
                    FROM isao_t
                   WHERE isaoent = g_enterprise
                     AND isaosite = g_master.xrcasite
               ELSE
                  #折讓
                  SELECT isao015 INTO g_master.isafdocno
                    FROM isao_t
                   WHERE isaoent = g_enterprise
                     AND isaosite = g_master.xrcasite
               END IF
               CALL s_aooi200_fin_get_slip_desc(g_master.isafdocno) RETURNING g_master.isafdocno_desc
               DISPLAY BY NAME g_master.isafdocno,g_master.isafdocno_desc
            END IF
            #END add-point 
 
 
         #應用 a02 樣板自動產生(Version:2)
         AFTER FIELD isaf008
            
            #add-point:AFTER FIELD isaf008 name="input.a.isaf008"
            IF NOT cl_null(g_master.isaf008) THEN
               INITIALIZE g_chkparam.* TO NULL
               LET g_chkparam.arg1 = g_ooef019
               LET g_chkparam.arg2 = g_master.isaf008
               IF NOT cl_chk_exist("v_isac002_2") THEN
                  LET g_master.isaf008 = g_master_t.isaf008
                  NEXT FIELD CURRENT
               END IF
               
               
               #發票歸屬進銷項
               SELECT isac003 INTO g_isac003
                 FROM isac_t
                WHERE isacent = g_enterprise
                  AND isac001 = g_ooef019
                  AND isac002 = g_master.isaf008
               
               #l_isac003 <> 2:銷項發票
               IF l_isac003 <> '2' AND g_master.isaf056 = '1' THEN
