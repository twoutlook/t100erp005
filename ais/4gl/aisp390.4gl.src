################################################################################
# Descriptions...: 清空&給預設
# Memo...........:
# Usage..........: CALL aisp390_qbe_clear()
# Date & Author..: 2016/07/06 By Reanna
# Modify.........:
################################################################################
PRIVATE FUNCTION aisp390_qbe_clear()

   #取得預設帳務中心
   CALL s_aap_get_default_apcasite('1','','') RETURNING g_sub_success,g_errno,g_master.xrcasite,g_master.xrcald,g_master.xrcacomp
   CALL s_desc_get_department_desc(g_master.xrcasite) RETURNING g_master.xrcasite_desc
   CALL s_desc_get_ld_desc(g_master.xrcald) RETURNING g_master.xrcald_desc
   #161024-00065#2 add ------
   #取得aoos020的參數S-FIN-2023(流通門店銷售立帳方式)
   CALL cl_get_para(g_enterprise,g_master.xrcacomp,'S-FIN-2023') RETURNING g_sfin2023
   #161024-00065#2 add end---
   
   #稅區
   SELECT ooef019 INTO g_ooef019
     FROM ooef_t
    WHERE ooefent = g_enterprise
      AND ooef001 = g_master.xrcacomp
   #稅區參數資料檔
   #發票編碼方式/紅字發票與藍字發票共用發票簿/稅區所屬國家
   SELECT isai002,isai006,isai008 INTO g_isai.*
     FROM isai_t
    WHERE isaient = g_enterprise
      AND isai001 = g_ooef019
   CALL aisp390_visible()
   
   #立帳依據
   LET g_master.isaf056 = '1'
   #匯率
   LET g_master.l_ratetype = '1'
   #發票日
   LET g_master.isafdocdt = g_today
   
   
   #營運據點
   LET g_master.isaesite = g_site
   CALL s_desc_get_department_desc(g_master.isaesite) RETURNING g_master.isaesite_desc
   
   #取得帳務組織下所屬成員
   CALL s_fin_account_center_sons_query('3',g_master.xrcasite,g_master.isafdocdt,'1')
   #據點
   CALL s_fin_account_center_sons_str() RETURNING g_wc_xrcasite
   CALL s_fin_get_wc_str(g_wc_xrcasite) RETURNING g_wc_xrcasite
   #取得帳務組織下所屬成員之帳別
   CALL s_fin_account_center_ld_str() RETURNING g_wc_ld
   CALL s_fin_get_wc_str(g_wc_ld) RETURNING g_wc_ld
   
   CALL s_ld_sel_glaa(g_master.xrcald,'glaa024')
        RETURNING g_sub_success,g_glaa.*
   
   DISPLAY BY NAME g_master.xrcasite,g_master.xrcasite_desc,g_master.xrcald,g_master.xrcald_desc,g_master.xrcacomp,
                   g_master.isaf056,g_master.l_ratetype,g_master.isafdocdt,g_master.isaesite,g_master.isaesite_desc
   
   LET g_master_t.* = g_master.*
END FUNCTION

################################################################################
# Descriptions...: 發票簿檢核
# Memo...........:
# Usage..........: CALL aisp390_site_with_book_chk()
# Date & Author..: 2016/07/07 By Reanna
# Modify.........:
################################################################################
PRIVATE FUNCTION aisp390_site_with_book_chk(p_isaesite,p_isae001,p_isafdocdt,p_isaf008)
DEFINE p_isaesite    LIKE isae_t.isaesite   #營運據點
DEFINE p_isae001     LIKE isae_t.isae001    #發票簿
DEFINE p_isafdocdt   LIKE isaf_t.isafdocdt  #發票日期
DEFINE p_isaf008     LIKE isaf_t.isaf008    #發票類型
DEFINE r_success     LIKE type_t.num5
DEFINE r_errno       LIKE gzze_t.gzze001
DEFINE l_count       LIKE type_t.num10
DEFINE l_ooef017     LIKE ooef_t.ooef017
DEFINE l_isae012     LIKE isae_t.isae012

   LET r_success = TRUE
   LET r_errno  =''
   
   IF NOT cl_null(p_isaesite) AND NOT cl_null(p_isae001) AND
      NOT cl_null(p_isafdocdt) AND NOT cl_null(p_isaf008)THEN
      
      LET l_ooef017 = NULL
      SELECT ooef017 INTO l_ooef017 FROM ooef_t
       WHERE ooefent = g_enterprise
         AND ooef001 = p_isaesite
      
      #發票資料不符合，請檢查aisi056發票簿維護作業!!!
      LET l_count = 0
      SELECT COUNT(*) INTO l_count
       FROM isae_t
       WHERE isaeent = g_enterprise
         AND isaesite IN (p_isaesite,l_ooef017) 
         AND isae001 = p_isae001
         AND isae002 <= p_isafdocdt
         AND isae003 >= p_isafdocdt
         AND isae004 = p_isaf008
         AND isae019 = 'aisi056'
      IF cl_null(l_count)THEN LET l_count = 0 END IF
      IF l_count = 0 THEN
         IF g_isai.isai002 = '1' THEN
            LET r_errno ='ais-00335'
            LET r_success = FALSE
            RETURN r_success,r_errno
         ELSE
            LET r_errno ='ais-00030'
            LET r_success = FALSE
            RETURN r_success,r_errno
         END IF
      END IF
      
      #發票簿的發票號碼已經編製完畢，請重新輸入發票簿!
      SELECT isae012 INTO l_isae012
        FROM isae_t
       WHERE isaeent = g_enterprise
         AND isaesite IN (p_isaesite,l_ooef017)
         AND isae001 = p_isae001
         AND isae002 <= p_isafdocdt
         AND isae003 >= p_isafdocdt
         AND isae004 = p_isaf008
         AND isae019 = 'aisi056'
      IF cl_null(l_isae012)THEN
         LET r_errno = 'ais-00234'
         LET r_success = FALSE
         RETURN r_success,r_errno
      END IF
   END IF
   
   RETURN r_success,r_errno
END FUNCTION

################################################################################
# Descriptions...: 發票簿相關隱顯
# Memo...........:
# Usage..........: CALL aisp390_visible()
# Date & Author..: 2016/07/07 By Reanna
# Modify.........:
################################################################################
PRIVATE FUNCTION aisp390_visible()
   
   CALL cl_set_comp_visible('isae008',TRUE)
   CALL cl_set_comp_visible('isae007',TRUE)
   IF g_isai.isai002 = '1' THEN
      CALL cl_set_comp_visible('isae008',FALSE)
   ELSE
      CALL cl_set_comp_visible('isae007',FALSE)
   END IF
   
   CALL cl_set_comp_visible('group_3',TRUE)
   IF g_isac003 = '4' THEN  #發票類型為銷項折單
      IF g_isai.isai006 = 'N' THEN   #紅字發票與藍字發票共用發票簿
         CALL cl_set_comp_visible('group_3',FALSE)
      END IF
   END IF
   
END FUNCTION

