################################################################################
PRIVATE FUNCTION aisq380_ins_x01_tmp()
DEFINE l_i                 LIKE type_t.num5
DEFINE l_isatcomp_desc     LIKE type_t.chr100

   #單頭
   LET l_isatcomp_desc = g_isat_m.l_isatcomp,':',g_isat_m.l_isatcomp_desc 
   
   #單身
   LET l_i = 1
   FOR l_i = 1 TO g_isat_d.getLength()
      INSERT INTO aisq380_x01_tmp
         VALUES(g_enterprise,            g_isat_d[l_i].isatsite,    g_isat_d[l_i].isatcomp,
                l_isatcomp_desc,         g_isat_d[l_i].isatdocno,   g_isat_d[l_i].isatseq,
                g_isat_d[l_i].isaf002,   g_isat_d[l_i].isat009,     g_isat_d[l_i].isat007,
                g_isat_d[l_i].isat003,   g_isat_d[l_i].isat004,     g_isat_d[l_i].isat002,
                g_isat_d[l_i].isat001,   g_isat_d[l_i].isat028,     g_isat_d[l_i].isat029,
                g_isat_d[l_i].isat023,   g_isat_d[l_i].isat100,     g_isat_d[l_i].isat101,
                g_isat_d[l_i].isat103,   g_isat_d[l_i].isat104,     g_isat_d[l_i].isat105,
                g_isat_d[l_i].isat113,   g_isat_d[l_i].isat114,     g_isat_d[l_i].isat115
                )
   END FOR
   CALL aisq380_xg_visible()
END FUNCTION

################################################################################
# Descriptions...: 傳進XG報表隱藏欄位設置
# Memo...........: #160621-00018#1
# Usage..........: CALL aisq380_xg_visible()
# Date & Author..: 2016/09/19 By 08732
# Modify.........:
################################################################################
PRIVATE FUNCTION aisq380_xg_visible()
   
   DEFINE l_isat003  LIKE isat_t.isat003
   
   LET g_xg_visible = NULL
   LET l_isat003 = ''
   
   SELECT DISTINCT isai002 INTO l_isat003
     FROM isai_t
     LEFT JOIN isat_t ON isatent = isaient
     LEFT JOIN ooef_t ON ooefent = isaient AND ooef019 = isai001
         WHERE ooefent = g_enterprise
           AND ooef001 = isatcomp
           AND isatcomp = g_isat_m.l_isatcomp
           
   IF l_isat003 != 1 THEN
      LET g_xg_visible = "isat003"
   END IF
END FUNCTION

 
