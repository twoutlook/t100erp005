      #add-point:query段more_construct name="query.more_construct"
      INPUT BY NAME g_isat_m.l_isatcomp ATTRIBUTE (WITHOUT DEFAULTS)
         ON ACTION controlp INFIELD l_isatcomp
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'i'
            LET g_qryparam.reqry = FALSE
            LET g_qryparam.where = " ooef003 = 'Y'",
                                   " AND ooef001 IN ",g_wc_cs_comp
            LET g_qryparam.default1 = g_isat_m.l_isatcomp
            CALL q_ooef001()
            LET g_isat_m.l_isatcomp = g_qryparam.return1
            DISPLAY g_isat_m.l_isatcomp TO l_isatcomp
            CALL s_desc_get_department_desc(g_isat_m.l_isatcomp) RETURNING g_isat_m.l_isatcomp_desc
            DISPLAY BY NAME g_isat_m.l_isatcomp_desc
            NEXT FIELD l_isatcomp
            
         AFTER FIELD l_isatcomp
            LET g_isat_m.l_isatcomp_desc = ''
            DISPLAY BY NAME g_isat_m.l_isatcomp_desc
            IF NOT cl_null(g_isat_m.l_isatcomp) THEN
               IF g_isat_m.l_isatcomp != g_isat_m_o.l_isatcomp OR cl_null(g_isat_m_o.l_isatcomp) THEN

                  CALL aisq380_isatcomp_chk(g_isat_m.l_isatcomp) RETURNING g_sub_success,g_errno
                  IF NOT g_sub_success THEN                  
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code = g_errno
                     LET g_errparam.extend = ''
                     LET g_errparam.popup = TRUE
                     CALL cl_err()
                     LET g_isat_m.l_isatcomp = g_isat_m_o.l_isatcomp
                     LET g_isat_m.l_isatcomp_desc = g_isat_m_o.l_isatcomp_desc
                     DISPLAY BY NAME g_isat_m.l_isatcomp_desc
                     NEXT FIELD CURRENT
                  END IF
                 
                  LET l_count = NULL
                  SELECT count(1) INTO l_count 
                    FROM s_fin_tmp1
                   WHERE comp = g_isat_m.l_isatcomp
                     AND comp IN (SELECT ooef001 FROM s_fin_tmp1)
                     
                  IF cl_null(l_count) THEN
                     LET l_count = 0
                  END IF
                     
                  IF l_count = 0 THEN
                     INITIALIZE g_errparam TO NULL
