{<section id="aisq340.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION aisq340_b_fill()
   #add-point:b_fill段define-客製 name="b_fill.define_customerization"
   
   #end add-point
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準  (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   #160414-00018#25--s
   DEFINE l_pmaal004_sql  STRING
   DEFINE l_ooag011_sql   STRING
   DEFINE l_ooefl003_sql  STRING
   #160414-00018#25--e
   #end add-point
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   
   #end add-point
 
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   LET ls_sql_rank = "SELECT  UNIQUE isafcomp,'',isafdocno,isafdocdt,isaf055,'',isaf100,isaf056,'','', 
       '','','','','','','',''  ,DENSE_RANK() OVER( ORDER BY isaf_t.isafcomp,isaf_t.isafdocno) AS RANK FROM isaf_t", 
 
 
 
                     "",
                     " WHERE isafent= ? AND 1=1 AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("isaf_t"),
                     " ORDER BY isaf_t.isafcomp,isaf_t.isafdocno"
 
   #add-point:b_fill段rank_sql_after name="b_fill.rank_sql_after"
   
   #end add-point
 
   LET g_sql = "SELECT COUNT(1) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   #add-point:b_fill段rank_sql_after_count name="b_fill.rank_sql_after_count"
   
   #end add-point
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT isafcomp,'',isafdocno,isafdocdt,isaf055,'',isaf100,isaf056,'','','','','','', 
       '','','',''",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   #160414-00018#25--s
   LET l_pmaal004_sql = s_fin_get_trading_partner_abbr_sql('a','isafent','isaf055')
   LET l_ooag011_sql = s_fin_get_person_sql('b','isafent','isba001')
   LET l_ooefl003_sql = s_fin_get_department_sql('c','isafent','isba002')
   #160414-00018#25--e
   #LET g_sql = "SELECT  'N',isafcomp,'',isafdocno,      ",   #150127-00007#1 mark
   LET g_sql = "SELECT  isafcomp,'',isafdocno,      ",        #150127-00007#1
              #"        isafdocdt,isaf055,'',isaf100,isaf056, ",                          #160414-00018#25 mark
               "        isafdocdt,isaf055,",l_pmaal004_sql,",isaf100,isaf056, ",          #160414-00018#25
               "         isbc001,isbc002,isbadocno,isbadocdt,isba001, ",
              #"         '',isba002,'',isbc103,isbc104 ",                                 #160414-00018#25 mark
               "         ",l_ooag011_sql,",isba002,",l_ooefl003_sql,",isbc103,isbc104 ",  #160414-00018#25
               " FROM  isaf_t,isba_t,isbc_t ",
               " WHERE isafent = isbaent AND isbaent = isbcent ",
               "   AND isafent= ?  ",
               "   AND isafdocno = isbc004  ",
               "   AND (isbc002=' ' OR isbc002 is null ",
               "                    OR EXISTS (SELECT 1 FROM isat_t ",
               "                                WHERE isafent = isatent ",
               "                                  AND isatdocno = isafdocno ",
               "                                  AND isat003 = isbc001 ",
               "                                  AND isat004 = isbc002 ",
               "                                  AND ((isat014 LIKE '11%')OR (isat014 LIKE '21%')))) ",
               "   AND isbcdocno = isbadocno ",
               "   AND isbastus ='Y'",
               "   AND isaf050 <> '3'", #160604-00032#1
               "   AND ", ls_wc,cl_sql_add_filter("isaf_t")
   LET g_sql = g_sql," ORDER BY isafdocno "
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE aisq340_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aisq340_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_isaf_d.clear()
 
   #add-point:陣列清空 name="b_fill.array_clear"
   
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1   
 
   FOREACH b_fill_curs INTO g_isaf_d[l_ac].isafcomp,g_isaf_d[l_ac].isafcomp_desc,g_isaf_d[l_ac].isafdocno, 
       g_isaf_d[l_ac].isafdocdt,g_isaf_d[l_ac].isaf055,g_isaf_d[l_ac].isaf055_desc,g_isaf_d[l_ac].isaf100, 
