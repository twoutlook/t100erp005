   LET g_isaj_m.isaj003 = ""
   LET g_isaj_m.isaj005 = ""
   LET g_isaj_m.isaj006 = ""
   LET g_isaj_m.isaj007 = ""
   LET g_isaj_m.isaj019 = ""
   LET g_isaj_m.isaj020 = ""
 
    
   CALL aist410_set_entry("a")
   CALL aist410_set_no_entry("a")
   
   #公用欄位給予預設值
   #應用 a14 樣板自動產生(Version:5)    
      #公用欄位新增給值  
      LET g_isaj_m.isajownid = g_user
      LET g_isaj_m.isajowndp = g_dept
      LET g_isaj_m.isajcrtid = g_user
      LET g_isaj_m.isajcrtdp = g_dept 
      LET g_isaj_m.isajcrtdt = cl_get_current()
      LET g_isaj_m.isajmodid = g_user
      LET g_isaj_m.isajmoddt = cl_get_current()
      LET g_isaj_m.isajstus = 'Y'
 
 
 
   
   CALL s_transaction_begin()
   
   #add-point:複製輸入前 name="reproduce.head.b_input"
   
   #end add-point
   
   #顯示狀態(stus)圖片
         #應用 a21 樣板自動產生(Version:3)
	  #根據當下狀態碼顯示圖片
      CASE g_isaj_m.isajstus 
