      CALL cl_set_comp_required("isaj002",FALSE)
   ELSE
      CALL cl_set_comp_required("isaj002",TRUE)
   END IF
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_ooef019
   LET g_ref_fields[2] = g_isaj_m.isaj004
   CALL ap_ref_array2(g_ref_fields,"SELECT isacl004 FROM isacl_t WHERE isaclent='"||g_enterprise||"' AND isacl001=? AND isacl002 = ? AND isacl003='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_isaj_m.isaj004_desc = '', g_rtn_fields[1] , ''
   DISPLAY g_isaj_m.isaj004_desc TO isaj004_desc
   
   #150224-00005#18 add ------
   #彙加註記 Y>A N>NULL
   IF g_isaj_m.isaj017 = 'A' THEN
      LET g_isaj_m.isaj017 = 'Y'
   ELSE
      LET g_isaj_m.isaj017 = 'N'
