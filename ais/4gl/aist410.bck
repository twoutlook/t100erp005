   #add-point:Function前置處理  name="reproduce.pre_function"
   
   #end add-point
   
   #切換畫面
   
   #先確定key值無遺漏
   IF g_isaj_m.isajcomp IS NULL
      OR g_isaj_m.isajsite IS NULL
      OR g_isaj_m.isaj001 IS NULL
      OR g_isaj_m.isaj003 IS NULL
      OR g_isaj_m.isaj005 IS NULL
      OR g_isaj_m.isaj006 IS NULL
      OR g_isaj_m.isaj007 IS NULL
      OR g_isaj_m.isaj019 IS NULL
      OR g_isaj_m.isaj020 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
   
   #備份key值
   LET g_isajcomp_t = g_isaj_m.isajcomp
   LET g_isajsite_t = g_isaj_m.isajsite
   LET g_isaj001_t = g_isaj_m.isaj001
