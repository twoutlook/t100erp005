   #end add-point  
   
   #add-point:show段Function前置處理  name="show.before"
   SELECT ooef019 INTO g_ooef019
     FROM ooef_t
    WHERE ooefent = g_enterprise
      AND ooef001 = g_isaj_m.isajcomp
   SELECT isai002 INTO g_isai002
     FROM isai_t
    WHERE isaient = g_enterprise  
      AND isai001 = g_ooef019
   SELECT glaald INTO g_ld
     FROM glaa_t
    WHERE glaaent = g_enterprise
      AND glaacomp = g_isaj_m.isajcomp 
      AND glaa014 = 'Y'
   CALL cl_set_comp_visible("isaj005",TRUE)
