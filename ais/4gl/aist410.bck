       g_isaj_m.isaj008,g_isaj_m.isaj028,g_isaj_m.isajownid,g_isaj_m.isajowndp,g_isaj_m.isajcrtid,g_isaj_m.isajcrtdp, 
       g_isaj_m.isajcrtdt,g_isaj_m.isajmodid,g_isaj_m.isajmoddt,g_isaj_m.isajcnfid,g_isaj_m.isajcnfdt, 
       g_isaj_m.isajpstid,g_isaj_m.isajpstdt,g_isaj_m.isaj003_desc,g_isaj_m.isajsite_desc,g_isaj_m.isaj021_desc, 
       g_isaj_m.isajownid_desc,g_isaj_m.isajowndp_desc,g_isaj_m.isajcrtid_desc,g_isaj_m.isajcrtdp_desc, 
       g_isaj_m.isajmodid_desc,g_isaj_m.isajcnfid_desc,g_isaj_m.isajpstid_desc
 
