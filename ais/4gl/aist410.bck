   LET g_isaj020_t = g_isaj_m.isaj020
 
   
   #組合新增資料的條件
   LET g_add_browse = " isajent = " ||g_enterprise|| " AND",
                      " isajcomp = '", g_isaj_m.isajcomp, "' "
                      ," AND isajsite = '", g_isaj_m.isajsite, "' "
                      ," AND isaj001 = '", g_isaj_m.isaj001, "' "
                      ," AND isaj003 = '", g_isaj_m.isaj003, "' "
                      ," AND isaj005 = '", g_isaj_m.isaj005, "' "
                      ," AND isaj006 = '", g_isaj_m.isaj006, "' "
                      ," AND isaj007 = '", g_isaj_m.isaj007, "' "
                      ," AND isaj019 = '", g_isaj_m.isaj019, "' "
                      ," AND isaj020 = '", g_isaj_m.isaj020, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL aist410_browser_fill("","")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   #add-point:完成複製段落後 name="reproduce.after_reproduce"
   
   #end add-point
              
