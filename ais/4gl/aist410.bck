{<section id="aist410.delete" >}
#+ 資料刪除 
PRIVATE FUNCTION aist410_delete()
   #add-point:delete段define(客製用) name="delete.define_customerization"
   
   #end add-point
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="delete.define"
   
   #end add-point  
   
   #add-point:Function前置處理  name="delete.pre_function"
   
   #end add-point
   
   #先確定key值無遺漏
   IF g_isaj_m.isajcomp IS NULL
   OR g_isaj_m.isajsite IS NULL
   OR g_isaj_m.isaj001 IS NULL
   OR g_isaj_m.isaj003 IS NULL
   OR g_isaj_m.isaj005 IS NULL
   OR g_isaj_m.isaj006 IS NULL
   OR g_isaj_m.isaj007 IS NULL
   OR g_isaj_m.isaj019 IS NULL
   OR g_isaj_m.isaj020 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
    
   LET g_isajcomp_t = g_isaj_m.isajcomp
   LET g_isajsite_t = g_isaj_m.isajsite
   LET g_isaj001_t = g_isaj_m.isaj001
   LET g_isaj003_t = g_isaj_m.isaj003
   LET g_isaj005_t = g_isaj_m.isaj005
   LET g_isaj006_t = g_isaj_m.isaj006
   LET g_isaj007_t = g_isaj_m.isaj007
   LET g_isaj019_t = g_isaj_m.isaj019
   LET g_isaj020_t = g_isaj_m.isaj020
 
   
   
 
   OPEN aist410_cl USING g_enterprise,g_isaj_m.isajcomp,g_isaj_m.isajsite,g_isaj_m.isaj001,g_isaj_m.isaj003,g_isaj_m.isaj005,g_isaj_m.isaj006,g_isaj_m.isaj007,g_isaj_m.isaj019,g_isaj_m.isaj020
   IF SQLCA.SQLCODE THEN    #(ver:49)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aist410_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE
      LET g_errparam.popup = TRUE 
      CLOSE aist410_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aist410_master_referesh USING g_isaj_m.isajcomp,g_isaj_m.isajsite,g_isaj_m.isaj001,g_isaj_m.isaj003, 
       g_isaj_m.isaj005,g_isaj_m.isaj006,g_isaj_m.isaj007,g_isaj_m.isaj019,g_isaj_m.isaj020 INTO g_isaj_m.isajcomp, 
       g_isaj_m.isaj003,g_isaj_m.isaj004,g_isaj_m.isaj029,g_isaj_m.isaj005,g_isaj_m.isaj006,g_isaj_m.isaj007, 
       g_isaj_m.isajstus,g_isaj_m.isaj015,g_isaj_m.isaj103,g_isaj_m.isaj009,g_isaj_m.isaj016,g_isaj_m.isaj012, 
       g_isaj_m.isaj104,g_isaj_m.isaj010,g_isaj_m.isaj105,g_isaj_m.isaj013,g_isaj_m.isaj011,g_isaj_m.isajsite, 
       g_isaj_m.isaj001,g_isaj_m.isaj002,g_isaj_m.isajdocdt,g_isaj_m.isaj031,g_isaj_m.isaj018,g_isaj_m.isaj017, 
       g_isaj_m.isaj014,g_isaj_m.isaj032,g_isaj_m.isaj030,g_isaj_m.isaj019,g_isaj_m.isaj020,g_isaj_m.isaj021, 
       g_isaj_m.isaj022,g_isaj_m.isaj023,g_isaj_m.isaj024,g_isaj_m.isaj025,g_isaj_m.isaj026,g_isaj_m.isaj027, 
       g_isaj_m.isaj008,g_isaj_m.isaj028,g_isaj_m.isajownid,g_isaj_m.isajowndp,g_isaj_m.isajcrtid,g_isaj_m.isajcrtdp, 
       g_isaj_m.isajcrtdt,g_isaj_m.isajmodid,g_isaj_m.isajmoddt,g_isaj_m.isajcnfid,g_isaj_m.isajcnfdt, 
       g_isaj_m.isajpstid,g_isaj_m.isajpstdt,g_isaj_m.isaj003_desc,g_isaj_m.isajsite_desc,g_isaj_m.isaj021_desc, 
       g_isaj_m.isajownid_desc,g_isaj_m.isajowndp_desc,g_isaj_m.isajcrtid_desc,g_isaj_m.isajcrtdp_desc, 
       g_isaj_m.isajmodid_desc,g_isaj_m.isajcnfid_desc,g_isaj_m.isajpstid_desc
   
   
   #檢查是否允許此動作
   IF NOT aist410_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_isaj_m_mask_o.* =  g_isaj_m.*
   CALL aist410_isaj_t_mask()
   LET g_isaj_m_mask_n.* =  g_isaj_m.*
   
   #將最新資料顯示到畫面上
   CALL aist410_show()
   
   IF cl_ask_delete() THEN
 
      #add-point:單頭刪除前 name="delete.head.b_delete"
      
      #end add-point
 
      #應用 a47 樣板自動產生(Version:4)
      #刪除相關文件
      CALL aist410_set_pk_array()
      #add-point:相關文件刪除前 name="delete.befroe.related_document_remove"
      
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
 
 
      DELETE FROM isaj_t 
       WHERE isajent = g_enterprise AND isajcomp = g_isaj_m.isajcomp 
         AND isajsite = g_isaj_m.isajsite 
         AND isaj001 = g_isaj_m.isaj001 
         AND isaj003 = g_isaj_m.isaj003 
         AND isaj005 = g_isaj_m.isaj005 
         AND isaj006 = g_isaj_m.isaj006 
         AND isaj007 = g_isaj_m.isaj007 
         AND isaj019 = g_isaj_m.isaj019 
         AND isaj020 = g_isaj_m.isaj020 
 
 
      #add-point:單頭刪除中 name="delete.head.m_delete"
      
      #end add-point
         
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "isaj_t:",SQLERRMESSAGE 
         LET g_errparam.code = SQLCA.SQLCODE
         LET g_errparam.popup = FALSE 
         CALL s_transaction_end('N','0')
         CALL cl_err()
      END IF
  
      
      
      #add-point:單頭刪除後 name="delete.head.a_delete"
      
      #end add-point
      
       
 
      #修改歷程記錄(刪除)
      LET g_log1 = util.JSON.stringify(g_isaj_m)   #(ver:49)
      IF NOT cl_log_modified_record(g_log1,'') THEN    #(ver:49)
         CLOSE aist410_cl
         CALL s_transaction_end('N','0')
         RETURN
      END IF
      
      CLEAR FORM
      CALL aist410_ui_browser_refresh()
      
      #確保畫面上保有資料
      IF g_browser_cnt > 0 THEN
         #CALL aist410_browser_fill(g_wc,"")
         CALL aist410_fetch("P")
      ELSE
         CLEAR FORM
      END IF
      CALL s_transaction_end('Y','0')
   ELSE    
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE aist410_cl
 
   #功能已完成,通報訊息中心
   CALL aist410_msgcentre_notify('delete')
 
   #add-point:單頭刪除完成後 name="delete.a_delete"
   
   #end add-point
 
END FUNCTION
 
