      CALL cl_set_comp_visible("isaj005",TRUE)
   ELSE
      CALL cl_set_comp_visible("isaj005",FALSE)
   END IF
   IF NOT cl_null(g_isaj_m.isaj006) THEN 
      CALL cl_set_comp_entry("isaj008",FALSE)
   ELSE
      CALL cl_set_comp_entry("isaj008",TRUE)
   END IF
