################################################################################
# Descriptions...: 申報單位檢核
# Memo...........:
# Usage..........: CALL aist411_isajsite_chk(p_isaa001)
# Date & Author..: 2015/06/03 By Hans
# Modify.........:
################################################################################
PRIVATE FUNCTION aist411_isaj003_chk(p_isaa001)
DEFINE p_isaa001     LIKE isaa_t.isaa001
DEFINE l_isaastus    LIKE isaa_t.isaastus
DEFINE r_errno       LIKE gzze_t.gzze001
DEFINE r_success LIKE type_t.chr1

   LET r_success = TRUE
   LET r_errno =''

   SELECT isaastus INTO l_isaastus
     FROM isaa_t
    WHERE isaaent = g_enterprise
      AND isaa001 = p_isaa001


   CASE
      WHEN SQLCA.SQLCODE = '100'
         LET r_success = FALSE LET r_errno = 'ais-00193'
      WHEN l_isaastus != 'Y'
         LET r_success = FALSE LET r_errno = 'sub-01302'  #160318-00005#22 mod #'abm-00079'
   END CASE

   RETURN r_success,r_errno
END FUNCTION

################################################################################
# Descriptions...: 輸入年月不得小於已申報年月
# Memo...........:
# Usage..........: CALL aist411_mod_chk()
# Date & Author..: 2015/06/03 By Hans
# Modify.........:
################################################################################
PRIVATE FUNCTION aist411_mod_chk()
DEFINE l_isaa012 LIKE isaa_t.isaa012
DEFINE l_isaa013 LIKE isaa_t.isaa013
DEFINE l_day1    LIKE type_t.dat
DEFINE l_day2    LIKE type_t.dat
DEFINE r_success LIKE type_t.chr1
DEFINE r_errno   LIKE gzze_t.gzze001

     LET r_success = TRUE

   SELECT isaa012,isaa013,isaa007
     INTO l_isaa012,l_isaa013,g_isaa007
     FROM isaa_t
    WHERE isaaent = g_enterprise
      AND isaa001 = g_isaj_m.isaj003
      AND isaa022 = "Y"

   IF NOT cl_null(l_isaa012) THEN
      IF g_isaj_m.isaj019 < l_isaa012 THEN
            LET r_errno = 'ais-00190'
            LET r_success = FALSE
      END IF
      IF NOT cl_null(l_isaa013) THEN
         LET l_day1= MDY(g_isaj_m.isaj020,1,g_isaj_m.isaj019)
         LET l_day2= MDY(l_isaa013,1,l_isaa012)
         IF l_day1 <= l_day2 THEN
            LET r_errno = 'ais-00214'
            LET r_success = FALSE
         END IF
      END IF
   END IF
   
     #按月或者是按期
   IF NOT cl_null(g_isaj_m.isaj020) THEN
      IF g_isaa007 = '1' THEN
         IF g_isaj_m.isaj020 MOD 2 != 0 THEN  #只能打偶數期別
            LET r_errno = 'ais-00209'
            LET r_success = FALSE
         END IF
      END IF                     
   END IF

   RETURN r_success,r_errno
END FUNCTION

 
