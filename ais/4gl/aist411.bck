# Descriptions...: 取得發票下拉類型
# Memo...........:
# Usage..........: CALL aist411_isaj004_scc()
# Date & Author..: 2015/06/03 By Hans
# Modify.........:
################################################################################
PRIVATE FUNCTION aist411_isaj004_scc()
DEFINE l_sql       STRING
DEFINE l_isac002   LIKE isac_t.isac002
DEFINE l_isacl004  LIKE isacl_t.isacl004
DEFINE l_str1      STRING
DEFINE l_str2      STRING

   LET l_sql = " SELECT isac002,isacl004 ",
               "   FROM isac_t,isacl_t   ",
               "  WHERE isacent = isaclent AND isacent = '",g_enterprise,"' ",
               "    AND isac001 = isacl001 AND isac001 = '",g_ooef019,"'    ",
               "    AND isac002 = isacl002 AND isacl003 = '",g_dlang,"'     "
   #IF g_prog = 'aist411' THEN        #160705-00042#9 160713 by sakura mark
   IF g_prog MATCHES 'aist411' THEN   #160705-00042#9 160713 by sakura add
      LET l_sql = l_sql CLIPPED , " AND isac003 IN('2','4')   " #銷項
   ELSE
      LET l_sql = l_sql CLIPPED , " AND isac003 IN ('1','3')  " #進項
   END IF      
   LET l_sql = l_sql CLIPPED , " ORDER BY isac002  "              

   PREPARE aist411_isaj004_prep FROM l_sql
   DECLARE aist411_isaj004_curs CURSOR FOR aist411_isaj004_prep
   LET l_str1 = NULL
   LET l_str2 = NULL
   FOREACH aist411_isaj004_curs INTO l_isac002,l_isacl004
      LET l_isacl004 = l_isac002,".",l_isacl004
      LET l_str1 = l_str1,",",l_isac002
      LET l_str2 = l_str2,",",l_isacl004
   END FOREACH
   
   CALL cl_set_combo_items('isaj004',l_str1,l_str2) 
END FUNCTION

