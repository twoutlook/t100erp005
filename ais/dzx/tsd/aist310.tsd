  <act cite_std="N" id="compile" ver="1" src="s" type="all" gen_code="Y" status="">
    <![CDATA[SELECT  inag001,   #貨物編碼
        imaall003, #貨物名稱
        isag005,   #發票單位 
        isag008 ,  #稅率
        isag101 ,  #單價
        sum(isag004),   #數量
        sum(isag103),   #原幣稅前金額
        sum(isag104),   #原幣稅額
        sum(isag105),   #原幣稅後金額
        sum(isag113),   #本幣稅前金額
        sum(isag114),   #本幣稅額
        sum(isag115)    #本幣稅後金額 
from  發票來源頁籤
    where 本筆發票單號
       group by  inag001,   #貨物編碼
                 imaall003, #貨物名稱
                 isag006,   #發票單位 
                 isag008 ,  #稅率
                 isag009   #單價


isah_t 寫入處理

isahdocno = isagdocno #開票單號
isah003 = inag001  #發票貨物代碼
isah004 = imaal003 #發票貨物名稱
isah005 = isag006  #發票單位
isah006 = isag005  #數量
isah101 = isag009  #單價
isah103 = isag010  #原幣稅前金額
isah007 = isag008  #原幣稅率
isah104 = isag011  #原幣稅額
isah105 = isag012  #原幣稅後金額
isah113 = isag013  #本幣稅前金額
isah114 = isag014  #本幣稅額
isah115 = isag015  #本幣稅後金額]]>
  </act>
