   isaw001 LIKE isaw_t.isaw001, 
   isaw002 LIKE isaw_t.isaw002, 
   isaw003 LIKE isaw_t.isaw003, 
   isaw004 LIKE isaw_t.isaw004, 
   isaw005 LIKE isaw_t.isaw005, 
   isaw006 LIKE isaw_t.isaw006, 
