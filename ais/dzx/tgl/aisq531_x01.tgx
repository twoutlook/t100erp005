   isaw019 LIKE isaw_t.isaw019, 
   isaw020 LIKE isaw_t.isaw020, 
   isaw025 LIKE isaw_t.isaw025, 
   isaw026 LIKE isaw_t.isaw026, 
   l_isaw027_desc LIKE type_t.chr100, 
   isaw028 LIKE isaw_t.isaw028, 
   isaw029 LIKE isaw_t.isaw029, 
   l_isaw030_desc LIKE type_t.chr100, 
   isawsite LIKE isaw_t.isawsite, 
