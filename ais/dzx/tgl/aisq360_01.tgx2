            IF g_detail_cnt >= l_ac 
               AND g_isah_d[l_ac].isahcomp IS NOT NULL
               AND g_isah_d[l_ac].isahdocno IS NOT NULL
               AND g_isah_d[l_ac].isahseq IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_isah_d_t.* = g_isah_d[l_ac].*  #BACKUP
               LET g_isah_d_o.* = g_isah_d[l_ac].*  #BACKUP
               IF NOT aisq360_01_lock_b("isah_t") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aisq360_01_bcl INTO g_isah_d[l_ac].isahcomp,g_isah_d[l_ac].isahdocno,g_isah_d[l_ac].isahseq, 
                      g_isah_d[l_ac].isah011,g_isah_d[l_ac].isah003,g_isah_d[l_ac].isah004,g_isah_d[l_ac].isah013, 
                      g_isah_d[l_ac].isah012,g_isah_d[l_ac].isah106,g_isah_d[l_ac].isah105
