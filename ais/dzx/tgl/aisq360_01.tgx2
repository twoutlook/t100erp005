               LET g_isah_d[g_isah_d.getLength()].isahcomp = NULL
               LET g_isah_d[g_isah_d.getLength()].isahdocno = NULL
               LET g_isah_d[g_isah_d.getLength()].isahseq = NULL
 
            END IF
            
 
            CALL aisq360_01_set_entry_b(l_cmd)
            CALL aisq360_01_set_no_entry_b(l_cmd)
            #add-point:modify段before insert name="input.body.before_insert"
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(1) INTO l_count FROM isah_t 
             WHERE isahent = g_enterprise AND isahcomp = g_isah_d[l_ac].isahcomp
                                       AND isahdocno = g_isah_d[l_ac].isahdocno
                                       AND isahseq = g_isah_d[l_ac].isahseq
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前 name="input.body.b_insert"
               {<point name="input.body.b_insert"/>}
               #end add-point
            
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_isah_d[g_detail_idx].isahcomp
               LET gs_keys[2] = g_isah_d[g_detail_idx].isahdocno
               LET gs_keys[3] = g_isah_d[g_detail_idx].isahseq
               CALL aisq360_01_insert_b('isah_t',gs_keys,"'1'")
                           
               #add-point:單身新增後 name="input.body.a_insert"
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE    
               INITIALIZE g_isah_d[l_ac].* TO NULL
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = 'INSERT' 
               LET g_errparam.code   = "std-00006" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLCODE THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "isah_t:",SQLERRMESSAGE 
               LET g_errparam.code = SQLCA.SQLCODE
               LET g_errparam.popup = TRUE 
               CALL cl_err()
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL aisq360_01_b_fill(g_wc2)
