            #自定義預設值(單身1)
                  LET g_isah_d[l_ac].isah011 = "0"
      LET g_isah_d[l_ac].isah106 = "0"
      LET g_isah_d[l_ac].isah105 = "0"
 
            #add-point:modify段before備份 name="input.body.before_bak"
            {<point name="input.body.before_bak"/>}
            #end add-point
            LET g_isah_d_t.* = g_isah_d[l_ac].*     #新輸入資料
            LET g_isah_d_o.* = g_isah_d[l_ac].*     #新輸入資料
