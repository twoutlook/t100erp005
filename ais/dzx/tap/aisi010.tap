         ON ACTION controlp INFIELD isab006
            #add-point:ON ACTION controlp INFIELD isab006
            {<point name="input.c.page1.isab006" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_isab_d[l_ac].* = g_isab_d_t.*
               CLOSE aisi010_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_isab_d[l_ac].isab002,-263,1)
               LET g_isab_d[l_ac].* = g_isab_d_t.*
            ELSE
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               #寫入修改者/修改日期資訊(單身)
               
      
               UPDATE isab_t SET (isab001,isab002,isab003,isab004,isab005,isab006) = (g_isaa_m.isaa001, 
                   g_isab_d[l_ac].isab002,g_isab_d[l_ac].isab003,g_isab_d[l_ac].isab004,g_isab_d[l_ac].isab005, 
                   g_isab_d[l_ac].isab006)
                WHERE isabent = g_enterprise AND isab001 = g_isaa_m.isaa001 
 
                  AND isab002 = g_isab_d_t.isab002 #項次   
 
                  
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("isab_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                     LET g_isab_d[l_ac].* = g_isab_d_t.*
                  WHEN SQLCA.sqlcode #其他錯誤
                     CALL cl_err("isab_t",SQLCA.sqlcode,1)
                     LET g_isab_d[l_ac].* = g_isab_d_t.*                     
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_isaa_m.isaa001
               LET gs_keys_bak[1] = g_isaa001_t
               LET gs_keys[2] = g_isab_d[g_detail_idx].isab002
               LET gs_keys_bak[2] = g_isab_d_t.isab002
               CALL aisi010_update_b('isab_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
               END CASE
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            #add-point:單身after_row
            {<point name="input.body.after_row"/>}
            #end add-point
            CALL aisi010_unlock_b("isab_t","'1'")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock
            
              
         AFTER INPUT
            #add-point:input段after input 
            {<point name="input.body.after_input"/>}
            #end add-point 
 
         ON ACTION controlo    
            CALL FGL_SET_ARR_CURR(g_isab_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_isab_d.getLength()+1
            
         #ON ACTION cancel
         #   LET INT_FLAG = 1
         #   LET g_detail_idx = 1
         #   EXIT DIALOG 
 
      END INPUT
      
 
      
 
      
 
      
 
      
]]>
