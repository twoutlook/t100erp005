  <point name="function.aisi010_ref_show" cite_std="N" status="u" ver="1" src="s" new="Y" cite_ver="" order="1" mark_hard="N">
<![CDATA[# 參考欄位帶值
PRIVATE FUNCTION aisi010_ref_show()
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_isaa_m.isaa001
   CALL ap_ref_array2(g_ref_fields,"SELECT ooefl004 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_isaa_m.ooefl004 = '', g_rtn_fields[1] , ''
   DISPLAY g_isaa_m.ooefl004 TO ooefl004
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_isaa_m.isaa001
   CALL ap_ref_array2(g_ref_fields,"SELECT ooef019,ooef017,ooef003,ooef012 FROM ooef_t WHERE ooefent='"||g_enterprise||"' AND ooef001=? ","") RETURNING g_rtn_fields
   LET g_ooef019 = '', g_rtn_fields[1] , ''
   LET g_ooef017 = '', g_rtn_fields[2] , ''
   LET g_isaa_m.ooef003 = '', g_rtn_fields[3] , ''
   LET g_ooef012 = '', g_rtn_fields[4] , ''
   DISPLAY g_isaa_m.ooef003 TO ooef003
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_ooef017
   CALL ap_ref_array2(g_ref_fields,"SELECT ooef002 FROM ooef_t WHERE ooefent='"||g_enterprise||"' AND ooef001=? ","") RETURNING g_rtn_fields
   LET g_isaa_m.ooef002 = '', g_rtn_fields[1] , ''
   DISPLAY g_isaa_m.ooef002 TO ooef002
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_ooef019
   CALL ap_ref_array2(g_ref_fields,"SELECT ooall004 FROM ooall_t WHERE ooallent='"||g_enterprise||"' AND ooall001 = '2' AND ooall002 = ? AND ooall003='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_isaa_m.ooall004 = '', g_rtn_fields[1] , ''
   DISPLAY g_isaa_m.ooall004 TO ooall004
   
   LET g_isaa_m.oofb017 = ''
   SELECT oofb017 
     INTO g_isaa_m.oofb017
     FROM oofb_t,ooef_t 
    WHERE oofbent = g_enterprise
      AND oofb008  ='1' # 登記地址
      AND oofb002 = ooef012 
      AND ooef001 = g_isaa_m.isaa001
      AND oofb010 ='Y'
   DISPLAY g_isaa_m.oofb017 TO oofb017
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_isaa_m.isaa009
   CALL ap_ref_array2(g_ref_fields,"SELECT ooefl004 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_isaa_m.ooefl004_1 = '', g_rtn_fields[1] , ''
   DISPLAY g_isaa_m.ooefl004_1 TO ooefl004_1
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_isaa_m.isaa014
   CALL ap_ref_array2(g_ref_fields,"SELECT pmaa003 FROM pmaa_t WHERE pmaaent='"||g_enterprise||"' AND pmaa001=? ","") RETURNING g_rtn_fields
   LET g_isaa_m.isaa015 = '', g_rtn_fields[1] , ''
   DISPLAY g_isaa_m.isaa015 TO isaa015
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_isaa_m.isaa014
   CALL ap_ref_array2(g_ref_fields,"SELECT pmaal004 FROM pmaal_t WHERE pmaalent='"||g_enterprise||"' AND pmaal001=? ","") RETURNING g_rtn_fields
   LET g_isaa_m.isaa016 = '', g_rtn_fields[1] , ''
   DISPLAY g_isaa_m.isaa016 TO isaa016
   
END FUNCTION]]>
</point>
  <point name="function.aisi010_isaa014_get" cite_std="N" status="u" ver="1" src="s" new="Y" cite_ver="" order="2" mark_hard="N">
<![CDATA[# 交易對象帶值
PRIVATE FUNCTION aisi010_isaa014_get()
   DEFINE l_pmaa027   LIKE pmaa_t.pmaa027
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_isaa_m.isaa014
   CALL ap_ref_array2(g_ref_fields,"SELECT pmaa003,pmaa027,pmaa016 FROM pmaa_t WHERE pmaaent='"||g_enterprise||"' AND pmaa001=? ","") RETURNING g_rtn_fields
   LET g_isaa_m.isaa015 = '', g_rtn_fields[1] , ''
   LET l_pmaa027 = '', g_rtn_fields[2] , ''
   LET g_isaa_m.isaa019 = '', g_rtn_fields[3] , ''
   DISPLAY g_isaa_m.isaa015 TO isaa015
   DISPLAY g_isaa_m.isaa019 TO isaa019
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_isaa_m.isaa014
   CALL ap_ref_array2(g_ref_fields,"SELECT pmaal004 FROM pmaal_t WHERE pmaalent='"||g_enterprise||"' AND pmaal001=? ","") RETURNING g_rtn_fields
   LET g_isaa_m.isaa016 = '', g_rtn_fields[1] , ''
   DISPLAY g_isaa_m.isaa016 TO isaa016
   
   SELECT oofc012 
     INTO g_isaa_m.isaa017
     FROM oofc_t 
    WHERE oofcent = g_enterprise
      AND oofc008 = '1'   #一般電話 
      AND oofc010 = 'Y' 
      AND oofc002 = l_pmaa027
   
   SELECT oofb017 
     INTO g_isaa_m.isaa018
     FROM oofb_t
    WHERE oofbent = g_enterprise
      AND oofb002 = l_pmaa027
      AND oofb008 = '1'
      AND oofb010 = 'Y' 
   DISPLAY g_isaa_m.isaa018 TO isaa018
   
   SELECT oofc012 
     INTO g_isaa_m.isaa021
     FROM oofc_t 
    WHERE oofcent = g_enterprise
      AND oofc008 = '4'   #電子郵件 
      AND oofc010 = 'Y' 
      AND oofc002 = l_pmaa027
      
   DISPLAY g_isaa_m.isaa021 TO isaa021
END FUNCTION]]>
</point>
