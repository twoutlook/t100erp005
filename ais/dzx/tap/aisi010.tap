  <point name="function.aisi010_b_ref" cite_std="N" status="u" ver="" src="s" new="Y" cite_ver="" order="3">
<![CDATA[# 單身參考欄位帶值
PRIVATE FUNCTION aisi010_b_ref()
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_isab_d[l_ac].isab002
   CALL ap_ref_array2(g_ref_fields,"SELECT gzcbl004 FROM gzcbl_t WHERE gzcbl001='9712' AND gzcbl002=? AND gzcbl003='"||g_lang||"'","") RETURNING g_rtn_fields
   LET g_isab_d[l_ac].isab002_desc = '', g_rtn_fields[1] , ''
   DISPLAY g_isab_d[l_ac].isab002_desc TO s_detail1[l_ac].isab002_desc
END FUNCTION]]>
</point>
  <section id="aisi010.b_fill" ver="3" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aisi010_b_fill()
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_isab_d.clear()    #g_isab_d 單頭及單身 
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF aisi010_fill_chk(1) THEN
   
      LET g_sql = "SELECT  UNIQUE isab002,'',isab003,isab004,isab005,isab006 FROM isab_t",   
                  " INNER JOIN isaa_t ON isaa001 = isab001 ",
 
                  #"",
                  
                  "",
                  " WHERE isabent=? AND isab001=?"
      #add-point:b_fill段sql_before
      {<point name="b_fill.body.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table1) THEN
         LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
      END IF
      
      #子單身的WC
      
      
      LET g_sql = g_sql, " ORDER BY isab_t.isab002"
      
      #add-point:單身填充控制
      {<point name="b_fill.sql"/>}
      #end add-point
      
      PREPARE aisi010_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR aisi010_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_isaa_m.isaa001
 
                                               
      FOREACH b_fill_cs INTO g_isab_d[l_ac].isab002,g_isab_d[l_ac].isab002_desc,g_isab_d[l_ac].isab003, 
          g_isab_d[l_ac].isab004,g_isab_d[l_ac].isab005,g_isab_d[l_ac].isab006
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               CALL cl_err( '', 9035, 1)
            END IF
            EXIT FOREACH
         END IF
         
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
 
   
   #add-point:browser_fill段其他table處理
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_isab_d.deleteElement(g_isab_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE aisi010_pb
 
   
END FUNCTION
]]>
</section>
