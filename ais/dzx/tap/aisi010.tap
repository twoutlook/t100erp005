  <section id="aisi010.main" ver="3" status="" src="s">
<![CDATA[#+ 此段落由子樣板a26產生
#OPTIONS SHORT CIRCUIT
#+ 作業開始 
MAIN
   #add-point:main段define
   {<point name="main.define"/>}
   #end add-point   
 
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("ais","")
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = "SELECT isaa001,'','','',isaa002,isaa003,'',isaa004,isaa006,isaa005,'',isaa007, 
       isaa008,isaa009,'',isaa010,isaa011,isaa012,isaa013,isaastus,isaa014,isaa015,isaa016,isaa017,isaa018, 
       isaa019,isaa020,isaa021,isaaownid,'',isaaowndp,'',isaacrtid,'',isaacrtdp,'',isaacrtdt,isaamodid, 
       '',isaamoddt FROM isaa_t WHERE isaaent= ? AND isaa001=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   DECLARE aisi010_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE isaa001,isaa002,isaa003,isaa004,isaa006,isaa005,isaa007,isaa008,isaa009, 
       isaa010,isaa011,isaa012,isaa013,isaastus,isaa014,isaa015,isaa016,isaa017,isaa018,isaa019,isaa020, 
       isaa021,isaaownid,isaaowndp,isaacrtid,isaacrtdp,isaacrtdt,isaamodid,isaamoddt",
               " FROM isaa_t",
               " WHERE isaaent = '" ||g_enterprise|| "' AND isaa001 = ?"
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE aisi010_master_referesh FROM g_sql
 
   IF g_bgjob = "Y" THEN
 
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
 
   ELSE
      
      #畫面開啟 (identifier)
      OPEN WINDOW w_aisi010 WITH FORM cl_ap_formpath("ais",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL aisi010_init()   
 
      #進入選單 Menu (="N")
      CALL aisi010_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_aisi010
      
   END IF 
   
   CLOSE aisi010_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
   
END MAIN
 
 
]]>
</section>
  <section id="aisi010.modify" ver="4" status="" src="s">
<![CDATA[#+ 資料修改
PRIVATE FUNCTION aisi010_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_isaa_m.isaa001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE aisi010_master_referesh USING g_isaa_m.isaa001 INTO g_isaa_m.isaa001,g_isaa_m.isaa002,g_isaa_m.isaa003, 
       g_isaa_m.isaa004,g_isaa_m.isaa006,g_isaa_m.isaa005,g_isaa_m.isaa007,g_isaa_m.isaa008,g_isaa_m.isaa009, 
       g_isaa_m.isaa010,g_isaa_m.isaa011,g_isaa_m.isaa012,g_isaa_m.isaa013,g_isaa_m.isaastus,g_isaa_m.isaa014, 
       g_isaa_m.isaa015,g_isaa_m.isaa016,g_isaa_m.isaa017,g_isaa_m.isaa018,g_isaa_m.isaa019,g_isaa_m.isaa020, 
       g_isaa_m.isaa021,g_isaa_m.isaaownid,g_isaa_m.isaaowndp,g_isaa_m.isaacrtid,g_isaa_m.isaacrtdp, 
       g_isaa_m.isaacrtdt,g_isaa_m.isaamodid,g_isaa_m.isaamoddt
 
   ERROR ""
  
   LET g_isaa001_t = g_isaa_m.isaa001
 
   CALL s_transaction_begin()
   
   OPEN aisi010_cl USING g_enterprise,g_isaa_m.isaa001
 
   IF STATUS THEN
      CALL cl_err("OPEN aisi010_cl:", STATUS, 1)
      CLOSE aisi010_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aisi010_cl INTO g_isaa_m.isaa001,g_isaa_m.ooefl004,g_isaa_m.ooall004,g_isaa_m.ooef002,g_isaa_m.isaa002, 
       g_isaa_m.isaa003,g_isaa_m.oofb017,g_isaa_m.isaa004,g_isaa_m.isaa006,g_isaa_m.isaa005,g_isaa_m.ooef003, 
       g_isaa_m.isaa007,g_isaa_m.isaa008,g_isaa_m.isaa009,g_isaa_m.ooefl004_1,g_isaa_m.isaa010,g_isaa_m.isaa011, 
       g_isaa_m.isaa012,g_isaa_m.isaa013,g_isaa_m.isaastus,g_isaa_m.isaa014,g_isaa_m.isaa015,g_isaa_m.isaa016, 
       g_isaa_m.isaa017,g_isaa_m.isaa018,g_isaa_m.isaa019,g_isaa_m.isaa020,g_isaa_m.isaa021,g_isaa_m.isaaownid, 
       g_isaa_m.isaaownid_desc,g_isaa_m.isaaowndp,g_isaa_m.isaaowndp_desc,g_isaa_m.isaacrtid,g_isaa_m.isaacrtid_desc, 
       g_isaa_m.isaacrtdp,g_isaa_m.isaacrtdp_desc,g_isaa_m.isaacrtdt,g_isaa_m.isaamodid,g_isaa_m.isaamodid_desc, 
       g_isaa_m.isaamoddt
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_isaa_m.isaa001,SQLCA.sqlcode,0)
      CLOSE aisi010_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   CALL aisi010_show()
   WHILE TRUE
      LET g_isaa001_t = g_isaa_m.isaa001
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_isaa_m.isaamodid = g_user 
LET g_isaa_m.isaamoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL aisi010_input("u")     #欄位更改
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_isaa_m.* = g_isaa_m_t.*
         CALL aisi010_show()
         CALL cl_err('',9001,0)
         CALL s_transaction_end('N','0')
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_isaa_m.isaa001 != g_isaa001_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE isab_t SET isab001 = g_isaa_m.isaa001
 
          WHERE isabent = g_enterprise AND isab001 = g_isaa001_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("isab_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("isab_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_isaa_m.isaa001,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE aisi010_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_isaa_m.isaa001,'U')
 
END FUNCTION   
]]>
</section>
