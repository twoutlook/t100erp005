            {<point name="input.g.page1.isab004" />}
            #END add-point
 
         #----<<isab005>>----
         #此段落由子樣板a01產生
         BEFORE FIELD isab005
            #add-point:BEFORE FIELD isab005
            {<point name="input.b.page1.isab005" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD isab005
            
            #add-point:AFTER FIELD isab005
            {<point name="input.a.page1.isab005" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE isab005
            #add-point:ON CHANGE isab005
            {<point name="input.g.page1.isab005" />}
            #END add-point
 
         #----<<isab006>>----
         #此段落由子樣板a01產生
         BEFORE FIELD isab006
            #add-point:BEFORE FIELD isab006
            {<point name="input.b.page1.isab006" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD isab006
            
            #add-point:AFTER FIELD isab006
            {<point name="input.a.page1.isab006" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE isab006
            #add-point:ON CHANGE isab006
            {<point name="input.g.page1.isab006" />}
            #END add-point
 
 
         #---------------------<  Detail: page1  >---------------------
         #----<<isab002>>----
         #Ctrlp:input.c.page1.isab002
         ON ACTION controlp INFIELD isab002
            #add-point:ON ACTION controlp INFIELD isab002
            {<point name="input.c.page1.isab002" />}
            #END add-point
 
         #----<<isab002_desc>>----
         #----<<isab003>>----
         #Ctrlp:input.c.page1.isab003
         ON ACTION controlp INFIELD isab003
            #add-point:ON ACTION controlp INFIELD isab003
            {<point name="input.c.page1.isab003" />}
            #END add-point
 
         #----<<isab004>>----
         #Ctrlp:input.c.page1.isab004
         ON ACTION controlp INFIELD isab004
            #add-point:ON ACTION controlp INFIELD isab004
            {<point name="input.c.page1.isab004" />}
