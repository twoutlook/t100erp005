               IF p_cmd = 'a' OR ( p_cmd = 'u' AND (g_isaa_m.isaa001 != g_isaa001_t )) THEN 
                  IF NOT ap_chk_notDup("","SELECT COUNT(*) FROM isaa_t WHERE "||"isaaent = '" ||g_enterprise|| "' AND "||"isaa001 = '"||g_isaa_m.isaa001 ||"'",'std-00004',0) THEN 
                     NEXT FIELD CURRENT
                  END IF
               END IF
