               LET g_chkparam.arg1 = g_isaa_m.isaa001

                  
               #呼叫檢查存在並帶值的library
               IF cl_chk_exist("v_ooef001") THEN
                  #檢查成功時後續處理
                  #LET  = g_chkparam.return1
                  #DISPLAY BY NAME 
                  IF g_isaa_m.isaa001 != l_isaa001_t OR cl_null(l_isaa001_t) THEN 
                     LET g_isaa_m.isaa002 = ''
                     SELECT isao001 
                       INTO g_isaa_m.isaa002
                       FROM isao_t 
                      WHERE isaoent = g_enterprise
                        AND isaosite = g_isaa_m.isaa001 
                     DISPLAY g_isaa_m.isaa002 TO isaa002
                  
                     LET g_isaa_m.isaa004 = ''
                     SELECT oofc012 
                       INTO g_isaa_m.isaa004
                       FROM oofc_t 
                      WHERE oofcent = g_enterprise
                        AND oofc008 = '4'   #電子郵件 
                        AND oofc010 = 'Y' 
                        AND oofc002 = g_ooef012
                     DISPLAY g_isaa_m.isaa004 TO isaa004
                     
                     LET g_isaa_m.isaa005 = ''
                     SELECT oofc012 
                       INTO g_isaa_m.isaa005
                       FROM oofc_t 
                      WHERE oofcent = g_enterprise
                        AND oofc008 = '1'   #電話
                        AND oofc010 = 'Y' 
                        AND oofc002 = g_ooef012
                     DISPLAY g_isaa_m.isaa005 TO isaa005
                  
                     LET g_isaa_m.isaa006 = ''
                     SELECT oofc012 
                       INTO g_isaa_m.isaa006
                       FROM oofc_t 
                      WHERE oofcent = g_enterprise
                        AND oofc008 = '3'   #傳真
                        AND oofc010 = 'Y' 
                        AND oofc002 = g_ooef012
                     DISPLAY g_isaa_m.isaa006 TO isaa006
                  END IF

                  SELECT ooef017 INTO g_isaa_m.isaa009
                    FROM ooef_t
                   WHERE ooefent = g_enterprise
                     AND ooef001 = g_isaa_m.isaa001
                  CALL aisi010_ref_show()
                  DISPLAY g_isaa_m.isaa009 TO isaa009
                  
                  LET l_isaa001_t = g_isaa_m.isaa001
               ELSE
                  #檢查失敗時後續處理
                  LET g_isaa_m.isaa001 = g_isaa_m_t.isaa001
                  LET g_isaa_m.isaa004 = g_isaa_m_t.isaa004
                  LET g_isaa_m.isaa005 = g_isaa_m_t.isaa005
                  LET g_isaa_m.isaa006 = g_isaa_m_t.isaa006
                  LET g_isaa_m.isaa017 = g_isaa_m_t.isaa017
                  CALL aisi010_ref_show()
                  NEXT FIELD CURRENT
               END IF
            

            END IF 
]]>
