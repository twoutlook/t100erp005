   LET g_isafcomp_t = g_isaf_m.isafcomp
   LET g_isafdocno_t = g_isaf_m.isafdocno
 
    
   LET g_isaf_m.isafcomp = ""
   LET g_isaf_m.isafdocno = ""
 
 
   CALL cl_set_head_visible("","YES")
 
   #公用欄位給予預設值
   #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
