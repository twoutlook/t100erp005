  <point name="function.aist310_isag009_chk" order="18" ver="3" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[# 發票數量檢查
PRIVATE FUNCTION aist310_isag009_chk()
