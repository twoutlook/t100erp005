  <section id="aisi055.delete" ver="13" status="" src="s">
    <![CDATA[#+ 資料刪除
PRIVATE FUNCTION aisi055_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_isae_m.isaecomp IS NULL
   OR g_isae_m.isaesite IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE aisi055_master_referesh USING g_isae_m.isaecomp,g_isae_m.isaesite INTO g_isae_m.isaesite, 
       g_isae_m.isae016,g_isae_m.isae017,g_isae_m.isae018,g_isae_m.isaecomp,g_isae_m.isae019,g_isae_m.isaesite_desc, 
       g_isae_m.isaecomp_desc
   
   CALL aisi055_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN aisi055_cl USING g_enterprise,g_isae_m.isaecomp
                                                       ,g_isae_m.isaesite
 
   IF STATUS THEN
      CALL cl_err("OPEN aisi055_cl:", STATUS, 1)
      CLOSE aisi055_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aisi055_cl INTO g_isae_m.isaesite,g_isae_m.isaesite_desc,g_isae_m.isac001,g_isae_m.isac001_desc, 
       g_isae_m.isae016,g_isae_m.isae017,g_isae_m.isae018,g_isae_m.isaecomp,g_isae_m.isaecomp_desc,g_isae_m.isae019 

   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_isae_m.isaecomp,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL aisi055_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
