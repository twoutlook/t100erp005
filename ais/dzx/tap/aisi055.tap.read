  <section id="aisi055.b_fill" ver="13" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aisi055_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_isae_d.clear()
   CALL g_isae2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE isae001,isae002,isae003,isae004,isae005,isae006,isae007,isae008,isae009, 
       isae010,isae011,isae012,isae013,isae014,isae015,isaestus,isae004,isaemodid,isaemoddt,isaeownid, 
       isaeowndp,isaecrtid,isaecrtdp,isaecrtdt,t1.isacl004 ,t2.ooag011 ,t3.ooag011 ,t4.ooefl003 ,t5.ooag011 , 
       t6.ooefl003 FROM isae_t",   
               "",
               
                              " LEFT JOIN isacl_t t1 ON t1.isaclent='"||g_enterprise||"' AND t1.isacl001=isae001 AND t1.isacl002=isae004 AND t1.isacl003='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t2 ON t2.ooagent='"||g_enterprise||"' AND t2.ooag001=isaemodid  ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent='"||g_enterprise||"' AND t3.ooag001=isaeownid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=isaeowndp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent='"||g_enterprise||"' AND t5.ooag001=isaecrtid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent='"||g_enterprise||"' AND t6.ooefl001=isaecrtdp AND t6.ooefl002='"||g_dlang||"' ",
 
               " WHERE isaeent= ? AND isaecomp=? AND isaesite=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("isae_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF aisi055_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY isae_t.isae001,isae_t.isae002,isae_t.isae003,isae_t.isae004"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE aisi055_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR aisi055_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_isae_m.isaecomp,g_isae_m.isaesite
                                               
      FOREACH b_fill_cs INTO g_isae_d[l_ac].isae001,g_isae_d[l_ac].isae002,g_isae_d[l_ac].isae003,g_isae_d[l_ac].isae004, 
          g_isae_d[l_ac].isae005,g_isae_d[l_ac].isae006,g_isae_d[l_ac].isae007,g_isae_d[l_ac].isae008, 
          g_isae_d[l_ac].isae009,g_isae_d[l_ac].isae010,g_isae_d[l_ac].isae011,g_isae_d[l_ac].isae012, 
          g_isae_d[l_ac].isae013,g_isae_d[l_ac].isae014,g_isae_d[l_ac].isae015,g_isae_d[l_ac].isaestus, 
          g_isae2_d[l_ac].isae004,g_isae2_d[l_ac].isaemodid,g_isae2_d[l_ac].isaemoddt,g_isae2_d[l_ac].isaeownid, 
          g_isae2_d[l_ac].isaeowndp,g_isae2_d[l_ac].isaecrtid,g_isae2_d[l_ac].isaecrtdp,g_isae2_d[l_ac].isaecrtdt, 
          g_isae_d[l_ac].isae004_desc,g_isae2_d[l_ac].isaemodid_desc,g_isae2_d[l_ac].isaeownid_desc, 
          g_isae2_d[l_ac].isaeowndp_desc,g_isae2_d[l_ac].isaecrtid_desc,g_isae2_d[l_ac].isaecrtdp_desc 

                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_isae2_d[l_ac].isaeownid_desc = cl_get_username(g_isae2_d[l_ac].isaeownid)
      #LET g_isae2_d[l_ac].isaeowndp_desc = cl_get_deptname(g_isae2_d[l_ac].isaeowndp)
      #LET g_isae2_d[l_ac].isaecrtid_desc = cl_get_username(g_isae2_d[l_ac].isaecrtid)
      #LET g_isae2_d[l_ac].isaecrtdp_desc = cl_get_deptname(g_isae2_d[l_ac].isaecrtdp)
      #LET g_isae2_d[l_ac].isaemodid_desc = cl_get_username(g_isae2_d[l_ac].isaemodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_isae_d.deleteElement(g_isae_d.getLength())
      CALL g_isae2_d.deleteElement(g_isae2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE aisi055_pb   
   
END FUNCTION
]]>
  </section>
