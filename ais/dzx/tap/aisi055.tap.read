################################################################################
PRIVATE FUNCTION aisi055_isae007_chk()
   DEFINE  l_n             LIKE type_t.num5 
   DEFINE  l_isad006       LIKE isad_t.isad006
   DEFINE  r_success       BOOLEAN
   
   LET r_success = TRUE
   #判斷是否存在
   LET l_n = 0
   SELECT count(*) INTO l_n
     FROM isad_t
    WHERE isad005 = g_isae_d[g_detail_idx].isae007
      AND isad001 = g_isae_m.isac001
      AND isad002 = g_isae_m.isae016
      AND isad003 = g_isae_m.isae017
      AND isad004 = g_isae_m.isae018
      
    IF l_n = 0 THEN 
       CALL cl_err(g_isae_d[g_detail_idx].isae005,'ais-00005',1)
       RETURN FALSE
    END IF
    
    #判斷是否有效
    LET l_n = 0
   SELECT count(*) INTO l_n
     FROM isad_t
    WHERE isad005 = g_isae_d[g_detail_idx].isae007
      AND isad001 = g_isae_m.isac001
      AND isad002 = g_isae_m.isae016
      AND isad003 = g_isae_m.isae017
      AND isad004 = g_isae_m.isae018
      AND isadstus = 'Y'
      
    IF l_n = 0 THEN 
       CALL cl_err(g_isae_d[g_detail_idx].isae005,'ais-00006',1)
       LET r_success =FALSE
       RETURN r_success
    END IF
    
    SELECT isad006 INTO l_isad006
      FROM isad_t
     WHERE isad005 = g_isae_d[g_detail_idx].isae007
       AND isad001 = g_isae_m.isac001
       AND isad002 = g_isae_m.isae016
       AND isad003 = g_isae_m.isae017
       AND isad004 = g_isae_m.isae018
    
    IF g_isae_d[g_detail_idx].isae005 = 'N' THEN 
       IF l_isad006 = '4' THEN 
          CALL cl_err(g_isae_d[g_detail_idx].isae007,'ais-00053',1)
          LET r_success =FALSE
          RETURN r_success
       END IF 
    ELSE
       IF l_isad006 <> '4' THEN 
          CALL cl_err(g_isae_d[g_detail_idx].isae007,'ais-00054',1)
          LET r_success =FALSE
          RETURN r_success
       END IF 
    END IF 
   
    RETURN r_success
END FUNCTION]]>
