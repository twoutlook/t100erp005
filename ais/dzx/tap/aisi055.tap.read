   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE aisi055_detail AS ",
                "SELECT * FROM isae_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO aisi055_detail SELECT * FROM isae_t 
                                         WHERE isaeent = g_enterprise AND isaecomp = g_isaecomp_t
                                         AND isaesite = g_isaesite_t
 
   
   #將key修正為調整後   
   UPDATE aisi055_detail 
      #更新key欄位
      SET isaecomp = g_isae_m.isaecomp
          , isaesite = g_isae_m.isaesite
 
      #更新共用欄位
      #此段落由子樣板a13產生
       , isaeownid = g_user
       , isaeowndp = g_dept
       , isaecrtid = g_user
       , isaecrtdp = g_dept 
       , isaecrtdt = ld_date
       , isaemodid = "" 
       , isaemoddt = "" 
      #, isaestus = "Y"
 
 
                                       
  
   #將資料塞回原table   
   INSERT INTO isae_t SELECT * FROM aisi055_detail
   
   IF SQLCA.sqlcode THEN
      CALL cl_err("reproduce",SQLCA.sqlcode,1)
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE aisi055_detail
   
   #add-point:單身複製後1
   {<point name="detail_reproduce.body.table1.a_insert"/>}
   #end add-point
 
 
   
 
   
