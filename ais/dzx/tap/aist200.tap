                     CALL s_desc_get_department_desc(g_isbd_m.isbdcomp) RETURNING g_isbd_m.isbdcomp_desc
                     DISPLAY BY NAME g_isbd_m.isbdcomp_desc
                     CALL aist200_get_glaa()
                     NEXT FIELD CURRENT
                  END IF 
               END IF
            END IF
            CALL aist200_get_glaa()
            CALL s_desc_get_department_desc(g_isbd_m.isbdcomp) RETURNING g_isbd_m.isbdcomp_desc
            DISPLAY BY NAME g_isbd_m.isbdcomp_desc]]>
