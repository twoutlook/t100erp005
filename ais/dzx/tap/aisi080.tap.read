         ON ACTION controlp INFIELD isapl004
            #add-point:ON ACTION controlp INFIELD isapl004
            {<point name="input.c.page1.isapl004" />}
            #END add-point
 
         #Ctrlp:input.c.page1.isap003
         ON ACTION controlp INFIELD isap003
            #add-point:ON ACTION controlp INFIELD isap003
            {<point name="input.c.page1.isap003" />}
            #END add-point
 
         #Ctrlp:input.c.page1.isapstus
         ON ACTION controlp INFIELD isapstus
            #add-point:ON ACTION controlp INFIELD isapstus
            {<point name="input.c.page1.isapstus" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_isap_d[l_ac].* = g_isap_d_t.*
               CLOSE aisi080_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
