            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE isapstus
            #add-point:ON CHANGE isapstus
            {<point name="input.g.page1.isapstus" />}
            #END add-point
 
 
                  #Ctrlp:input.c.page1.isap002
         ON ACTION controlp INFIELD isap002
            #add-point:ON ACTION controlp INFIELD isap002
            {<point name="input.c.page1.isap002" />}
            #END add-point
 
