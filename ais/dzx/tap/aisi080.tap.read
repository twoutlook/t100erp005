               LET g_isap_d[li_reproduce_target].isap002 = NULL
 
            END IF
            #公用欄位給值(單身)
            #此段落由子樣板a14產生    
      LET g_isap_d[l_ac].isapstus = "N"
 
 
            LET g_detail_multi_table_t.isapl001 = g_isai_m.isai001
LET g_detail_multi_table_t.isapl002 = g_isap_d[l_ac].isap002
LET g_detail_multi_table_t.isapl003 = g_dlang
LET g_detail_multi_table_t.isapl004 = g_isap_d[l_ac].isapl004
 
