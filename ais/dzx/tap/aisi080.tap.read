            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            LET g_detail_multi_table_t.isapl001 = g_isai_m.isai001
LET g_detail_multi_table_t.isapl002 = g_isap_d[l_ac].isap002
LET g_detail_multi_table_t.isapl003 = g_dlang
LET g_detail_multi_table_t.isapl004 = g_isap_d[l_ac].isapl004
 
            #其他table進行lock
                        INITIALIZE l_var_keys TO NULL 
            INITIALIZE l_field_keys TO NULL 
            LET l_field_keys[01] = 'isapl001'
            LET l_var_keys[01] = g_isai_m.isai001
            LET l_field_keys[02] = 'isapl002'
            LET l_var_keys[02] = g_isap_d[l_ac].isap002
            LET l_field_keys[03] = 'isapl003'
            LET l_var_keys[03] = g_dlang
            IF NOT cl_multitable_lock(l_var_keys,l_field_keys,'isapl_t') THEN
               RETURN 
            END IF 
 
        
         BEFORE INSERT
            
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_isap_d[l_ac].* TO NULL 
            INITIALIZE g_isap_d_t.* TO NULL 
            INITIALIZE g_isap_d_o.* TO NULL 
                  LET g_isap_d[l_ac].isap003 = "N"
      LET g_isap_d[l_ac].isapstus = "Y"
 
            LET g_isap_d_t.* = g_isap_d[l_ac].*     #新輸入資料
            LET g_isap_d_o.* = g_isap_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL aisi080_set_entry_b(l_cmd)
            #add-point:modify段after_set_entry_b
            {<point name="input.body.insert.after_set_entry_b"/>}
            #end add-point
            CALL aisi080_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
