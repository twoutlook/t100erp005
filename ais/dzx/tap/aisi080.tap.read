            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("isap_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL aisi080_b_fill()
               #資料多語言用-增/改
                        INITIALIZE l_var_keys TO NULL 
         INITIALIZE l_field_keys TO NULL 
         INITIALIZE l_vars TO NULL 
         INITIALIZE l_fields TO NULL 
         INITIALIZE l_var_keys_bak TO NULL 
         IF g_isai_m.isai001 = g_detail_multi_table_t.isapl001 AND
         g_isap_d[l_ac].isap002 = g_detail_multi_table_t.isapl002 AND
         g_isap_d[l_ac].isapl004 = g_detail_multi_table_t.isapl004 THEN
         ELSE 
            LET l_var_keys[01] = g_isai_m.isai001
            LET l_field_keys[01] = 'isapl001'
            LET l_var_keys[02] = g_isap_d[l_ac].isap002
            LET l_field_keys[02] = 'isapl002'
