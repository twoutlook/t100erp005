            LET l_fields[02] = 'isaplent'
            LET l_var_keys_bak[01] = g_detail_multi_table_t.isapl001
            LET l_var_keys_bak[02] = g_detail_multi_table_t.isapl002
            LET l_var_keys_bak[03] = g_detail_multi_table_t.isapl003
            CALL cl_multitable(l_var_keys,l_field_keys,l_vars,l_fields,l_var_keys_bak,'isapl_t')
         END IF 
 
               END CASE
               
			   #修改歷程記錄
               LET g_log1 = util.JSON.stringify(g_isai_m),util.JSON.stringify(g_isap_d_t)
               LET g_log2 = util.JSON.stringify(g_isai_m),util.JSON.stringify(g_isap_d[l_ac])
               IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
