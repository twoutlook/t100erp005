               #寫入修改者/修改日期資訊(單身)
 
      
               UPDATE isap_t SET (isap001,isap002,isap003,isapstus) = (g_isai_m.isai001,g_isap_d[l_ac].isap002, 
                   g_isap_d[l_ac].isap003,g_isap_d[l_ac].isapstus)
                WHERE isapent = g_enterprise AND isap001 = g_isai_m.isai001 
 
                  AND isap002 = g_isap_d_t.isap002 #項次   
 
                  
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("isap_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                     LET g_isap_d[l_ac].* = g_isap_d_t.*
                  WHEN SQLCA.sqlcode #其他錯誤
                     CALL cl_err("isap_t",SQLCA.sqlcode,1)
                     LET g_isap_d[l_ac].* = g_isap_d_t.*                     
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_isai_m.isai001
               LET gs_keys_bak[1] = g_isai001_t
               LET gs_keys[2] = g_isap_d[g_detail_idx].isap002
               LET gs_keys_bak[2] = g_isap_d_t.isap002
