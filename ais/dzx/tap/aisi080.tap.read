  <section id="aisi080.b_fill" ver="4" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aisi080_b_fill()
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_isap_d.clear()    #g_isap_d 單頭及單身 
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF aisi080_fill_chk(1) THEN
   
      LET g_sql = "SELECT  UNIQUE isap002,isap003,isapstus  FROM isap_t",   
                  " INNER JOIN isai_t ON isai001 = isap001 ",
 
                  #" LEFT JOIN isapl_t ON isai001 = isapl001 AND isap002 = isapl002 AND isapl003 = '",g_lang,"'",
                  
                  " LEFT JOIN isapl_t ON isai001 = isapl001 AND isap002 = isapl002 AND isapl003 = '",g_lang,"'",
                  
                  " WHERE isapent=? AND isap001=?"
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      #add-point:b_fill段sql_before
      {<point name="b_fill.body.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table1) THEN
         LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
      END IF
      
      #子單身的WC
      
      
      LET g_sql = g_sql, " ORDER BY isap_t.isap002"
      
      #add-point:單身填充控制
      {<point name="b_fill.sql"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE aisi080_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR aisi080_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_isai_m.isai001
                                               
      FOREACH b_fill_cs INTO g_isap_d[l_ac].isap002,g_isap_d[l_ac].isap003,g_isap_d[l_ac].isapstus
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               CALL cl_err( '', 9035, 1)
            END IF
            EXIT FOREACH
         END IF
         
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
 
   
   #add-point:browser_fill段其他table處理
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_isap_d.deleteElement(g_isap_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE aisi080_pb
 
   
END FUNCTION
]]>
  </section>
