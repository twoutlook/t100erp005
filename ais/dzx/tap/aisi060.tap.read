  <section id="aisi060.delete" ver="5" status="" src="s">
    <![CDATA[#+ 資料刪除
PRIVATE FUNCTION aisi060_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_isan_m.isan001 IS NULL
   OR g_isan_m.isan002 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE aisi060_master_referesh USING g_isan_m.isan001,g_isan_m.isan002 INTO g_isan_m.isan001,g_isan_m.isan002, 
       g_isan_m.isan001_desc
   
   CALL aisi060_show()
   
   CALL s_transaction_begin()
   
   LET g_master_multi_table_t.isanl001 = g_isan_m.isan001
LET g_master_multi_table_t.isanl002 = g_isan_m.isan002
LET g_master_multi_table_t.isanl004 = g_isan_m.isanl004
 
 
   OPEN aisi060_cl USING g_enterprise,g_isan_m.isan001
                                                       ,g_isan_m.isan002
 
   IF STATUS THEN
      CALL cl_err("OPEN aisi060_cl:", STATUS, 1)
      CLOSE aisi060_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aisi060_cl INTO g_isan_m.isan001,g_isan_m.isan001_desc,g_isan_m.isan002,g_isan_m.isanl004
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_isan_m.isan001,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL aisi060_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM isan_t WHERE isanent = g_enterprise AND isan001 = g_isan_m.isan001
                                                               AND isan002 = g_isan_m.isan002
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("isan_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      INITIALIZE l_var_keys_bak TO NULL 
   INITIALIZE l_field_keys   TO NULL 
   LET l_var_keys_bak[01] = g_master_multi_table_t.isanl001
   LET l_field_keys[01] = 'isanl001'
   LET l_var_keys_bak[02] = g_master_multi_table_t.isanl002
   LET l_field_keys[02] = 'isanl002'
   LET l_var_keys_bak[03] = g_dlang
   LET l_field_keys[03] = 'isanl003'
   CALL cl_multitable_delete(l_field_keys,l_var_keys_bak,'isanl_t')
 
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_isan_d.clear() 
      CALL g_isan2_d.clear()       
 
     
      CALL aisi060_ui_browser_refresh()  
      CALL aisi060_ui_headershow()  
      CALL aisi060_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL aisi060_fetch('P')
      ELSE
