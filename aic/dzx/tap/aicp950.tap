  <point name="ui_dialog.more_input" order="" ver="3" cite_std="N" new="N" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[         INPUT ARRAY g_detail_d FROM s_detail1.* 
              ATTRIBUTE(COUNT = g_detail_cnt,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                        INSERT ROW = FALSE,
                        DELETE ROW = FALSE,
                        APPEND ROW = FALSE)
            BEFORE INPUT
               IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
                 CALL FGL_SET_ARR_CURR(g_detail_d.getLength()+1) 
                 LET g_insert = 'N' 
               END IF 
             
            BEFORE ROW
               LET l_ac = DIALOG.getCurrentRow("s_detail1")
               LET g_master_idx = l_ac
               DISPLAY l_ac TO FORMONLY.h_index
               CALL aicp950_fetch() 
               #單身1可編輯，需備份舊值
               LET g_detail_d_t.* = g_detail_d[l_ac].*               
            
            ON CHANGE b_sel
               #若sel=Y,則由系統自動將最近一次輸入之"多角流程編號"代入,反之將"多角流程編號"清空
               IF g_detail_d[l_ac].sel = 'Y' THEN
                  IF cl_null(g_detail_d[l_ac].pmds053) THEN
                     IF cl_null(g_pmds053_def) THEN
                        NEXT FIELD b_pmds053
                     ELSE
                        LET g_detail_d[l_ac].pmds053 = g_pmds053_def
                        #帶入值後要再做校驗
                        IF aicp950_pmds053_chk() THEN
                           LET g_detail_d_t.pmds053 = g_detail_d[l_ac].pmds053
                           CALL s_desc_get_icaa001_desc(g_detail_d[l_ac].pmds053)
                                RETURNING g_detail_d[l_ac].pmds053_desc
                        ELSE
                           LET g_detail_d[l_ac].pmds053 = g_detail_d_t.pmds053
                           CALL s_desc_get_icaa001_desc(g_detail_d[l_ac].pmds053)
                                RETURNING g_detail_d[l_ac].pmds053_desc
                           NEXT FIELD b_pmds053
                        END IF
                     END IF
                  END IF
               ELSE
                  LET g_detail_d[l_ac].pmds053 = ''
                  LET g_detail_d[l_ac].pmds053_desc = ''
               END IF
               
            AFTER FIELD b_pmds053
               LET g_detail_d[l_ac].pmds053_desc = ''
               IF cl_null(g_detail_d[l_ac].pmds053) THEN
                  IF g_detail_d[l_ac].sel = 'Y' THEN
                     LET g_detail_d[l_ac].sel = 'N'
                  END IF
               ELSE
                  IF g_detail_d[l_ac].pmds053 <> g_detail_d_t.pmds053 OR cl_null(g_detail_d_t.pmds053) THEN
                     IF aicp950_pmds053_chk() THEN
                        #紀錄最近一次輸入的多角流程代碼
                        LET g_pmds053_def = g_detail_d[l_ac].pmds053
                     ELSE
                        LET g_detail_d[l_ac].pmds053 = g_detail_d_t.pmds053
                        CALL s_desc_get_icaa001_desc(g_detail_d[l_ac].pmds053)
                             RETURNING g_detail_d[l_ac].pmds053_desc
                        NEXT FIELD CURRENT
                     END IF
                  END IF
                  #若xmdl088有輸入值,則由系統自動將"勾選"進行勾選
                  IF g_detail_d[l_ac].sel = 'N' THEN
                     LET g_detail_d[l_ac].sel = 'Y'
                  END IF
               END IF
               CALL s_desc_get_icaa001_desc(g_detail_d[l_ac].pmds053)
                    RETURNING g_detail_d[l_ac].pmds053_desc
               LET g_detail_d_t.pmds053 = g_detail_d[l_ac].pmds053
               
            ON ACTION controlp INFIELD b_pmds053
               INITIALIZE g_qryparam.* TO NULL
               LET g_qryparam.state = 'i'
               LET g_qryparam.reqry = FALSE
               LET g_qryparam.default1 = g_detail_d[l_ac].pmds053
			      LET g_qryparam.arg1 = '2'
			      LET g_qryparam.arg2 = '2'
			      LET g_qryparam.arg3 = 'N'
               LET g_qryparam.arg4 = g_detail_d[l_ac].pmdssite
               LET g_qryparam.arg5 = g_detail_d[l_ac].pmdlsite
			      CALL q_icaa001_2()                        #呼叫開窗
               LET g_detail_d[l_ac].pmds053 = g_qryparam.return1
               DISPLAY BY NAME g_detail_d[l_ac].pmds053
               CALL s_desc_get_icaa001_desc(g_detail_d[l_ac].pmds053)
                    RETURNING g_detail_d[l_ac].pmds053_desc
               NEXT FIELD b_pmds053
               
            ON ROW CHANGE
               UPDATE aicp950_tmp 
                  SET sel = g_detail_d[l_ac].sel, 
                      pmds053 = g_detail_d[l_ac].pmds053 
                WHERE pmdsdocno = g_detail_d[l_ac].pmdsdocno 
                  
         END INPUT               ]]>
  </point>
