  <point name="function.aicp950_sel_icac007" order="5" ver="3" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 取得多角出貨單別
# Memo...........:
# Usage..........: CALL aicp950_sel_icac007(p_icac001,p_icac002)
# Input parameter: 
# Return code....: 
# Date & Author..: 150706 By whitney
# Modify.........:
################################################################################
PRIVATE FUNCTION aicp950_sel_icac007(p_icac001,p_icac002)
DEFINE p_icac001    LIKE icac_t.icac001   #流程代碼
DEFINE p_icac002    LIKE icac_t.icac002   #站別
DEFINE r_success    LIKE type_t.num5      #執行結果
DEFINE r_icac007    LIKE icac_t.icac007   #出貨單單別

   LET r_success = TRUE
   LET r_icac007 = ''

   SELECT icac007 INTO r_icac007
     FROM icac_t
    WHERE icacent = g_enterprise
      AND icac001 = p_icac001
      AND icac002 = p_icac002
   IF cl_null(r_icac007) THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'aic-00168'    #流程代碼%1站別%2之出貨單單別為空！
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      LET g_errparam.replace[1] = p_icac001
      LET g_errparam.replace[2] = p_icac002
      CALL cl_err()
      LET r_success = FALSE
      RETURN r_success,r_icac007
   END IF
   
   RETURN r_success,r_icac007
END FUNCTION]]>
  </point>
