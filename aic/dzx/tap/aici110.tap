  <point name="function.aici110_get_site" order="5" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION aici110_get_site()
   IF g_icaf_m.icaf002 = 'ALL' THEN 
      LET g_ooef001 = g_site
   ELSE  
      LET g_ooef001 = g_icaf_m.icaf002
   END IF
   SELECT * INTO g_ooef.* FROM ooef_t WHERE ooefent = g_enterprise AND ooef001 = g_ooef001
END FUNCTION]]>
