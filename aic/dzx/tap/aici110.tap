   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_icaf_m.icaf001,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE aici110_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_icaf_m.icaf001,"U")
   
   LET g_worksheet_hidden = 0
   
