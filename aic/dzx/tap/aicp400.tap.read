  <point name="function.aicp400_success" order="2" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 成功記錄
# Memo...........:
# Usage..........: CALL aicp400_success(p_pmds,p_pmds041)
#                  
# Input parameter: p_pmds         單身一record
#                : p_pmds041      多角序號
# Return code....: 
#                : 
# Date & Author..: 2014/11/04 By stellar
# Modify.........:
################################################################################
PRIVATE FUNCTION aicp400_success(p_pmds,p_pmds041)
DEFINE p_pmds            type_g_detail_d
DEFINE p_pmds041         LIKE pmds_t.pmds041

   IF cl_null(g_success_cnt) THEN
      LET g_success_cnt = 1
   END IF
   
