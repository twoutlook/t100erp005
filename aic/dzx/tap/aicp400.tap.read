        RETURNING g_detail3_d[g_success_cnt].pmdsownid1_desc
   CALL s_desc_get_person_desc(g_detail3_d[g_success_cnt].pmdscrtid1)
        RETURNING g_detail3_d[g_success_cnt].pmdscrtid1_desc
   
   LET g_success_cnt = g_success_cnt +  1
   
