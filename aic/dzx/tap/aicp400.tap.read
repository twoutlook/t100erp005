  <point name="function.aicp400_process" order="1" ver="2" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL aicp400_process()
#                  
# Input parameter: 
# Return code....: 
# Date & Author..: 2014/11/03 By stellar
# Modify.........:
################################################################################
PRIVATE FUNCTION aicp400_process()
DEFINE l_pmds            type_g_detail_d
DEFINE l_trino           LIKE pmdl_t.pmdl031

DEFINE l_icab_now        RECORD
          icab002           LIKE icab_t.icab002,   #當站站別
          icab003           LIKE icab_t.icab003,   #當站營運據點
          icab004           LIKE icab_t.icab004,   #當站委外工單開立點否
          icab005           LIKE icab_t.icab005,   #中斷點否
          icab008           LIKE icab_t.icab008    #實體庫存否
                         END RECORD
DEFINE l_icab_next       RECORD
          icab002           LIKE icab_t.icab002,   #下站站別
          icab003           LIKE icab_t.icab003    #下站營運據點
                         END RECORD

DEFINE l_success         LIKE type_t.num5      #回傳執行結果
DEFINE l_pmdsdocno       LIKE pmds_t.pmdsdocno #回傳產生之入庫單號
DEFINE l_xmdkdocno       LIKE xmdk_t.xmdkdocno #回傳產生之出貨單號
DEFINE l_site            LIKE icab_t.icab003
DEFINE l_docno           LIKE pmds_t.pmdsdocno 
DEFINE l_icaa003         LIKE icaa_t.icaa003
DEFINE l_icaa004         LIKE icaa_t.icaa004
DEFINE l_icac007         LIKE icac_t.icac007
DEFINE l_icab002_last    LIKE type_t.num5
DEFINE l_pmdt087         LIKE pmdt_t.pmdt087
DEFINE l_pmdt027         LIKE pmdt_t.pmdt027

   #有選擇的入庫單
   LET g_sql = "SELECT pmdsdocno,pmdsdocdt,pmds006,",
               "       pmds007,pmds009,pmds008,",
               "       pmds002,pmds003,pmdsownid,pmdscrtid, ",
               "       pmds011,pmds014,pmds053,break,pmdl052,",
               "       pmds041 ",
               "  FROM aicp400_tmp ",
               " WHERE sel = 'Y' ",
               " ORDER BY pmdsdocno "
   PREPARE aicp400_process_pre FROM g_sql
   DECLARE aicp400_process_cur CURSOR WITH HOLD FOR aicp400_process_pre
   
   #當站多角貿易營運據點(升冪)
   LET g_sql = "SELECT icab002,icab003,icab004,",
               "       COALESCE(icab005,'N'),COALESCE(icab008,'N')",
               "  FROM icab_t",
               " WHERE icabent = ",g_enterprise,
               "   AND icab001 = ?",
               " ORDER BY icab002"
   PREPARE aicp400_process_icab_pre FROM g_sql
   DECLARE aicp400_process_icab_cur CURSOR FOR aicp400_process_icab_pre

   #下站多角貿易營運據點
   LET g_sql = "SELECT icab002,icab003",
               "  FROM icab_t",
               " WHERE icabent = ",g_enterprise,
               "   AND icab001 = ?",
               "   AND icab002 > ?",
               " ORDER BY icab002"
   PREPARE aicp400_process_icab_next_pre FROM g_sql
   DECLARE aicp400_process_icab_next_cur CURSOR FOR aicp400_process_icab_next_pre
   
   #當站多角貿易營運據點(降冪)
   LET g_sql = "SELECT icab002,icab003,icab004,",
               "       COALESCE(icab005,'N'),COALESCE(icab008,'N')",
               "  FROM icab_t",
               " WHERE icabent = ",g_enterprise,
               "   AND icab001 = ?",
               " ORDER BY icab002 DESC"
   PREPARE aicp400_process_icab_desc_pre FROM g_sql
   DECLARE aicp400_process_icab_desc_cur CURSOR FOR aicp400_process_icab_desc_pre

   #上站多角貿易營運據點
   LET g_sql = "SELECT icab002,icab003",
               "  FROM icab_t",
               " WHERE icabent = ",g_enterprise,
               "   AND icab001 = ?",
               "   AND icab002 < ?",
               " ORDER BY icab002 DESC"
   PREPARE aicp400_process_icab_pre_pre FROM g_sql
   DECLARE aicp400_process_icab_pre_cur CURSOR FOR aicp400_process_icab_pre_pre

   #入庫單找來源收貨單
   LET g_sql = "SELECT DISTINCT pmdt027",
               "  FROM pmdt_t",
               " WHERE pmdtent = ",g_enterprise,
               "   AND pmdtdocno = ?",
               "   AND pmdt027 IS NOT NULL"
   PREPARE aicp400_process_pmdt_pre FROM g_sql
   DECLARE aicp400_process_pmdt_cur CURSOR FOR aicp400_process_pmdt_pre
   
   #多角流程代碼icaa
   LET g_sql = "SELECT icaa003,icaa004",
               "  FROM icaa_t",
               " WHERE icaaent = ",g_enterprise,
               "   AND icaa001 = ?"
   PREPARE aicp400_process_icaa_pre FROM g_sql
   
   #中斷入庫單
   LET g_sql = "SELECT pmdsdocno",
               "  FROM pmds_t",
               " WHERE pmdssite = ?",
               "   AND pmds041 = ?",
               "   AND pmdsstus <> 'X'"
   PREPARE aicp400_process_pmdsdocno_pre FROM g_sql
   
   LET l_success = TRUE
   
   CALL s_aic_carry_create_temp_table_ship() RETURNING l_success
   IF NOT l_success THEN
      RETURN
   END IF
   
   CALL s_aic_carry_create_temp_table_storage() RETURNING l_success
   IF NOT l_success THEN
      RETURN
   END IF
   
   CALL s_tax_recount_tmp()

   LET g_success_cnt = 1
   LET g_fail_cnt = 0
   CALL g_detail3_d.clear()
   CALL g_detail4_d.clear()
   CALL cl_err_collect_init()   #匯總訊息-初始化
   
   FOREACH aicp400_process_cur INTO l_pmds.pmdsdocno,l_pmds.pmdsdocdt,l_pmds.pmds006,
                                    l_pmds.pmds007,l_pmds.pmds009,l_pmds.pmds008,
                                    l_pmds.pmds002,l_pmds.pmds003,l_pmds.pmdsownid,l_pmds.pmdscrtid,
                                    l_pmds.pmds011,l_pmds.pmds014,l_pmds.pmds053,l_pmds.break,l_pmds.pmdl052,
                                    l_pmds.pmds041
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = 'process_cur'
         LET g_errparam.popup = TRUE
         CALL cl_err()

         #失敗記錄
         CALL aicp400_fail(l_pmds.*,l_trino,'')

         EXIT FOREACH
      END IF
      
      CALL s_transaction_begin()
      LET g_fail_cnt = g_errcollect.getLength()  #先取得錯誤訊息的長度，大於此長度的表示是這張單子的錯誤訊息
      LET l_success = TRUE
      LET l_trino = ''
      LET l_xmdkdocno = ''
      LET l_pmdsdocno = ''
      
      IF l_pmds.pmds014 <> '4' THEN
      #採購多角性質非"統購統付"
      
         #多角流程代碼為空白時，錯誤
         IF cl_null(l_pmds.pmds053) THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = 'aic-00026'   #多角流程代碼不可為空
            LET g_errparam.extend = ''
            LET g_errparam.popup = TRUE
            
            CALL cl_err()
            CALL s_transaction_end('N',0)
            #失敗記錄
            CALL aicp400_fail(l_pmds.*,l_trino,'')
            CONTINUE FOREACH
         END IF
        
         #中斷續拋
         IF l_pmds.break = 'Y' THEN
            IF cl_null(l_pmds.pmds041) THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 'aic-00170'   #中斷續拋之多角序號不可為空！
               LET g_errparam.extend = 'l_pmds.pmdsdocno'
               LET g_errparam.popup = TRUE
               CALL cl_err()

               CALL s_transaction_end('N',0)
               #失敗記錄
               CALL aicp400_fail(l_pmds.*,l_trino,'')
               CONTINUE FOREACH
            END IF

            LET l_trino = l_pmds.pmds041
         ELSE
            #呼叫產生多角序號元件取得多角流程序號，並回寫入庫單單頭pmds041
            CALL s_aic_carry_gettrino(l_pmds.pmds053,'4',g_today,g_site)
                 RETURNING l_success,l_trino
            IF NOT l_success THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 'amm-00001'   #自動生成單號失敗！
               LET g_errparam.extend = ''
               LET g_errparam.popup = TRUE
                
               CALL cl_err()
           
               CALL s_transaction_end('N',0)
               #失敗記錄
               CALL aicp400_fail(l_pmds.*,l_trino,'')
               CONTINUE FOREACH
            END IF
         END IF
         
         #回寫入庫單頭多角序號、多角貿易已拋轉
         UPDATE pmds_t SET pmds041 = l_trino,
                           pmds050 = 'Y'
          WHERE pmdsent = g_enterprise
            AND pmdsdocno = l_pmds.pmdsdocno
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = ''
            LET g_errparam.popup = TRUE
            
            CALL cl_err()

            CALL s_transaction_end('N',0)
            #失敗記錄
            CALL aicp400_fail(l_pmds.*,l_trino,'')
            CONTINUE FOREACH
         END IF
         
         #若有收貨單一併回寫
         LET l_pmdt027 = ''
         OPEN aicp400_process_pmdt_cur USING l_pmds.pmdsdocno
         FOREACH aicp400_process_pmdt_cur INTO l_pmdt027
            IF SQLCA.sqlcode THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = SQLCA.sqlcode
               LET g_errparam.extend = ''
               LET g_errparam.popup = TRUE
            
               CALL cl_err()

               CALL s_transaction_end('N',0)
               #失敗記錄
               CALL aicp400_fail(l_pmds.*,l_trino,'')
               LET l_success = FALSE
               EXIT FOREACH
            END IF
            
            #回寫收貨單頭多角序號、多角貿易已拋轉
            UPDATE pmds_t SET pmds041 = l_trino,
                              pmds050 = 'Y'
             WHERE pmdsent = g_enterprise
               AND pmdsdocno = l_pmdt027
            IF SQLCA.sqlcode THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = SQLCA.sqlcode
               LET g_errparam.extend = ''
               LET g_errparam.popup = TRUE
             
               CALL cl_err()

               CALL s_transaction_end('N',0)
               #失敗記錄
               CALL aicp400_fail(l_pmds.*,l_trino,'')
               LET l_success = FALSE
               EXIT FOREACH
            END IF
            
         END FOREACH
         
         IF NOT l_success THEN
            CONTINUE FOREACH
         END IF

         #流程類型、出貨/收貨確立點
         LET l_icaa003 = ''
         LET l_icaa004 = ''
         EXECUTE aicp400_process_icaa_pre USING l_pmds.pmds053
         INTO l_icaa003,l_icaa004
         
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = ''
            LET g_errparam.popup = TRUE
            
            CALL cl_err()
            
            CALL s_transaction_end('N',0)
            #失敗記錄
            CALL aicp400_fail(l_pmds.*,l_trino,'')
            CONTINUE FOREACH
         END IF

         #中斷續拋，一律走正拋流程
         IF l_pmds.break = 'Y' THEN
            LET l_icaa004 = '1'
         END IF

         #正拋
         IF l_icaa004 = '1' THEN
            #跑站
            INITIALIZE l_icab_now.* TO NULL
            OPEN aicp400_process_icab_cur USING l_pmds.pmds053
            FOREACH aicp400_process_icab_cur INTO l_icab_now.icab002,l_icab_now.icab003,l_icab_now.icab004,
                                                  l_icab_now.icab005,l_icab_now.icab008
               IF SQLCA.sqlcode THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = SQLCA.sqlcode
                  LET g_errparam.extend = ''
                  LET g_errparam.popup = TRUE
                  
                  CALL cl_err()
                  LET l_success = FALSE
                  EXIT FOREACH
               END IF
               
               #若站別為第0站，不做處理
               IF l_icab_now.icab002 = 0 THEN
               
                  #若為中斷點，結束流程
                  IF l_icab_now.icab005 = 'Y' THEN
                     EXIT FOREACH
                  END IF   
                  
                  LET l_pmdsdocno = l_pmds.pmdsdocno
                  LET l_site = l_icab_now.icab003
                  CONTINUE FOREACH
               END IF
               
               #取下站
               INITIALIZE l_icab_next.* TO NULL
               OPEN aicp400_process_icab_next_cur USING l_pmds.pmds053,l_icab_now.icab002
               FOREACH aicp400_process_icab_next_cur INTO l_icab_next.icab002,l_icab_next.icab003
                  IF SQLCA.sqlcode THEN
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code = SQLCA.sqlcode
                     LET g_errparam.extend = ''
                     LET g_errparam.popup = TRUE
                     
                     CALL cl_err()
                     LET l_success = FALSE
                     EXIT FOREACH
                  END IF
                  EXIT FOREACH
               END FOREACH
               
               IF NOT l_success THEN
                  EXIT FOREACH
               END IF
               
               IF NOT cl_null(l_icab_next.icab002) THEN
               #若站別為中間站(即不是第0站也不是最終站)
               
                  #無實體庫存時，產生入庫單
                  IF l_icab_now.icab008 = 'N' THEN
                     #呼叫s_aic_carry_gen_tri_mr，產生中間站之多角收貨入庫單
                     CALL s_aic_carry_gen_tri_mr(l_site,l_xmdkdocno,l_icab_now.icab003,l_pmds.pmdsdocdt,l_pmds.pmds053,l_icab_now.icab002,l_pmds.pmdsdocno,'','')
                          RETURNING l_success,l_pmdsdocno
                          
                     IF NOT l_success THEN
                        EXIT FOREACH
                     END IF
                  ELSE
                  
                     EXECUTE aicp400_process_pmdsdocno_pre USING l_icab_now.icab003,l_trino INTO l_pmdsdocno
                     IF SQLCA.sqlcode THEN
                        INITIALIZE g_errparam TO NULL
                        LET g_errparam.code = SQLCA.sqlcode
                        LET g_errparam.extend = ''
                        LET g_errparam.popup = TRUE
                        
                        CALL cl_err()
                        LET l_success = FALSE
                        EXIT FOREACH
                     END IF
                     
                  END IF
                  
                  #出貨單別
                  CALL aicp400_sel_icac007(l_pmds.pmds053,l_icab_now.icab002) RETURNING l_success,l_icac007
                  IF NOT l_success THEN
                     EXIT FOREACH
                  END IF
                  
                  #呼叫s_aic_carry_gen_tri_bs，產生中間站之多角出貨單
                  CALL s_aic_carry_gen_tri_bs(l_site,l_pmdsdocno,'',l_icab_now.icab003,'','',l_pmds.pmdsdocdt,l_icac007,'',l_pmds.pmds053,l_icab_now.icab002,'')
                       RETURNING l_success,l_xmdkdocno
                  IF NOT l_success THEN
                     EXIT FOREACH
                  END IF
                  
                  LET l_site = l_icab_now.icab003
                                    
               ELSE
               #最終站
                              
                  #委外工單開立點 OR 採購指定最終供應商
                  IF l_icab_now.icab004 = 'Y' OR l_icaa003 = '3' THEN
                     #最終站非中斷點才產生入庫單
                     IF l_icab_now.icab005 = 'N' THEN
                  
                        #呼叫s_aic_carry_gen_tri_mr，產生最終站之多角收貨入庫單
                        CALL s_aic_carry_gen_tri_mr(l_site,l_xmdkdocno,l_icab_now.icab003,l_pmds.pmdsdocdt,l_pmds.pmds053,l_icab_now.icab002,l_pmds.pmdsdocno,'','')
                             RETURNING l_success,l_pmdsdocno
                        IF NOT l_success THEN
                           EXIT FOREACH
                        END IF
                        
                     ELSE
                     
                        EXECUTE aicp400_process_pmdsdocno_pre USING l_icab_now.icab003,l_trino INTO l_pmdsdocno
                        IF SQLCA.sqlcode THEN
                           INITIALIZE g_errparam TO NULL
                           LET g_errparam.code = SQLCA.sqlcode
                           LET g_errparam.extend = ''
                           LET g_errparam.popup = TRUE
                        
                           CALL cl_err()
                           LET l_success = FALSE
                           EXIT FOREACH
                        END IF
                        
                     END IF
                  END IF

                  #出貨單別
                  CALL aicp400_sel_icac007(l_pmds.pmds053,l_icab_now.icab002) RETURNING l_success,l_icac007

                  IF NOT l_success THEN
                     EXIT FOREACH
                  END IF

                  #呼叫s_aic_carry_gen_tri_bs，產生最終站之多角出貨單
                  CALL s_aic_carry_gen_tri_bs(l_site,l_pmdsdocno,'',l_icab_now.icab003,'','',l_pmds.pmdsdocdt,l_icac007,'',l_pmds.pmds053,l_icab_now.icab002,'')
                       RETURNING l_success,l_xmdkdocno
                          
                  IF NOT l_success THEN
                     EXIT FOREACH
                  END IF

               END IF
               
               #若為中斷點，結束流程
               IF l_icab_now.icab005 = 'Y' THEN
                  EXIT FOREACH
               END IF
                  
            END FOREACH
            
         #逆拋
         ELSE

            #若流程類型為'3.代採購指定最終供應商'且出貨/收貨確立點='2.最終站'
            LET l_icab002_last = TRUE
            LET l_pmdsdocno = ''
            LET l_xmdkdocno = ''

            #跑站(當站)
            INITIALIZE l_icab_now.* TO NULL
            OPEN aicp400_process_icab_desc_cur USING l_pmds.pmds053
            FOREACH aicp400_process_icab_desc_cur INTO l_icab_now.icab002,l_icab_now.icab003,l_icab_now.icab004,
                                                       l_icab_now.icab005,l_icab_now.icab008
               IF SQLCA.sqlcode THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = SQLCA.sqlcode
                  LET g_errparam.extend = 'FOREACH:aicp400_process_icab_desc_cur'
                  LET g_errparam.popup = TRUE
                  
                  CALL cl_err()
                  LET l_success = FALSE
                  EXIT FOREACH
               END IF

               #若站別為最終站，呼叫s_aic_carry_gen_tri_bs，產生最終站之多角出貨單
               IF l_icab002_last THEN
                  #若最終站中斷，不續拋
                  IF l_icab_now.icab005 = 'Y' THEN
                     EXIT FOREACH
                  END IF
               
                  LET l_site = l_icab_now.icab003
                  LET l_icab002_last = FALSE
                  
                  #出貨單別
                  CALL aicp400_sel_icac007(l_pmds.pmds053,l_icab_now.icab002) RETURNING l_success,l_icac007
                  IF NOT l_success THEN
                     EXIT FOREACH
                  END IF
                  
                  CALL s_aic_carry_gen_tri_bs(l_icab_now.icab003,l_pmds.pmdsdocno,'',l_icab_now.icab003,'','',l_pmds.pmdsdocdt,l_icac007,'',l_pmds.pmds053,l_icab_now.icab002,'')
                       RETURNING l_success,l_xmdkdocno
                  IF NOT l_success THEN
                     EXIT FOREACH
                  END IF
                  
                  CONTINUE FOREACH
               END IF

               #取上站
               INITIALIZE l_icab_next.* TO NULL
               OPEN aicp400_process_icab_pre_cur USING l_pmds.pmds053,l_icab_now.icab002
               FOREACH aicp400_process_icab_pre_cur INTO l_icab_next.icab002,l_icab_next.icab003
                  IF SQLCA.sqlcode THEN
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code = SQLCA.sqlcode
                     LET g_errparam.extend = ''
                     LET g_errparam.popup = TRUE
                     
                     CALL cl_err()
                     LET l_success = FALSE
                     EXIT FOREACH
                  END IF
                  EXIT FOREACH
               END FOREACH

               IF NOT l_success THEN
                  EXIT FOREACH
               END IF

               IF NOT cl_null(l_icab_next.icab002) THEN
               #若站別為中間站(即不是第0站也不是最終站)，呼叫 s_aic_carry_gen_tri_mr，產生中間站之多角收貨入庫單

                  #非中斷點 或 中斷有實體庫存，產生收貨單
                  IF l_icab_now.icab005 = 'N' OR l_icab_now.icab008 = 'Y' THEN
                     #呼叫s_aic_carry_gen_tri_mr，產生中間站之多角收貨入庫單
                     CALL s_aic_carry_gen_tri_mr(l_site,l_xmdkdocno,l_icab_now.icab003,l_pmds.pmdsdocdt,l_pmds.pmds053,l_icab_now.icab002,'','','')
                          RETURNING l_success,l_pmdsdocno

                     IF NOT l_success THEN
                        EXIT FOREACH
                     END IF
                  END IF

                  #非中斷點，產生出貨單
                  IF l_icab_now.icab005 = 'N' THEN
                     #出貨單別
                     CALL aicp400_sel_icac007(l_pmds.pmds053,l_icab_now.icab002) RETURNING l_success,l_icac007
                     IF NOT l_success THEN
                        EXIT FOREACH
                     END IF
                    
                     #呼叫s_aic_carry_gen_tri_bs，產生中間站之多角出貨單
                     CALL s_aic_carry_gen_tri_bs(l_icab_now.icab003,l_pmdsdocno,'',l_icab_now.icab003,'','',l_pmds.pmdsdocdt,l_icac007,'',l_pmds.pmds053,l_icab_now.icab002,'')
                          RETURNING l_success,l_xmdkdocno
                    
                     IF NOT l_success THEN
                        EXIT FOREACH
                     END IF
                  END IF

                  LET l_site = l_icab_now.icab003
               ELSE
               #第0站
               
                  #第0站非中斷點，產生收貨單
                  IF l_icab_now.icab005 = 'N' THEN

                     #呼叫s_aic_carry_gen_tri_mr，產生第0站之多角收貨入庫單
