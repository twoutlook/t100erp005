#單號過濾條件
PRIVATE FUNCTION aicp400_sel()
   #設定入庫單限制條件
   LET g_aicp400_sel = " pmdsent = ",g_enterprise,
                       " AND pmdssite = '",g_site,"' ",
                       " AND pmds000 IN ('3','20','6','12') ",    #3採購收貨入庫單,20委外採購收貨入庫,6採購入庫,12委外採購入庫
                       " AND pmds011 IN ('1','2') ",
                       " AND pmdsstus = 'S' ",
