    <![CDATA[
################################################################################
# Descriptions...: 委外工單開立點檢查
# Memo...........:
# Usage..........: CALL aici100_icab004_chk(p_icab001,p_icab002,p_icab003,p_icab004)
#                  RETURNING r_success
# Input parameter: p_icab001   #流程代碼
#                : p_icab002   #站別
#                : p_icab003   #營運據點
#                : p_icab004   #委外工單開立點
# Return code....: r_success   #檢查結果
#                : 
# Date & Author..: 150420 By earl
# Modify.........:
################################################################################
PRIVATE FUNCTION aici100_icab004_chk(p_icab001,p_icab002,p_icab003,p_icab004)
   DEFINE p_icab001      LIKE icab_t.icab001
   DEFINE p_icab002      LIKE icab_t.icab002
   DEFINE p_icab003      LIKE icab_t.icab003
   DEFINE p_icab004      LIKE icab_t.icab004
   DEFINE r_success      LIKE type_t.num5
   DEFINE l_icac010      LIKE icac_t.icac010
   DEFINE l_oobl002      LIKE oobl_t.oobl002
   
   LET r_success = TRUE
   
   IF cl_null(p_icab003) THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = ""
      LET g_errparam.code   = 'aps-00098'     #請先輸入對應的營運據點！
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      
      LET r_success = FALSE
      RETURN r_success
   END IF
   
   IF NOT cl_null(p_icab001) AND NOT cl_null(p_icab002) AND NOT cl_null(p_icab004) THEN
      
      LET l_icac010 = ''
      SELECT icac010 INTO l_icac010
        FROM icac_t
       WHERE icacent = g_enterprise
         AND icac001 = p_icab001
         AND icac002 = p_icab002
   
      IF p_icab004 = 'N' THEN
         LET l_oobl002 = "apmt500"   #採購單
      ELSE
         LET l_oobl002 = "apmt501"   #委外採購單
      END IF
            
      IF NOT s_aooi200_chk_slip(p_icab003,'',l_icac010,l_oobl002) THEN
         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF
   
   RETURN r_success
END FUNCTION]]>
  </point>
  <point name="function.aici100_icab004_icac010_chk" order="26" ver="6" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 委外工單-採購單單據別檢查
# Memo...........:
# Usage..........: CALL aici100_icab004_icac010_chk(p_icab001,p_icab002,p_icab004)
#                  RETURNING r_success
# Input parameter: p_icab001    流程代碼
#                : p_icab002    站別
#                : p_icab004    委外工單開立點
# Return code....: r_success    執行結果
#                : 
# Date & Author..: 150420 By earl
# Modify.........:
################################################################################
PRIVATE FUNCTION aici100_icab004_icac010_chk(p_icab001,p_icab002,p_icab004)
   DEFINE p_icab001    LIKE icab_t.icab001
   DEFINE p_icab002    LIKE icab_t.icab002
   DEFINE p_icab004    LIKE icab_t.icab004
   DEFINE r_success    LIKE type_t.num5
   DEFINE l_sql        STRING
   DEFINE l_icab002    LIKE icab_t.icab002
   DEFINE l_n          LIKE type_t.num5
   
   LET r_success = TRUE

   LET l_sql = "SELECT COUNT(icab002)",
               "  FROM icab_t",
               " WHERE icabent = ",g_enterprise,
               "   AND icab001 = '",p_icab001,"'",
               "   AND (icab004 = 'Y' OR icab002 = '",p_icab002,"')"
   PREPARE aici100_pb7 FROM l_sql

   LET l_sql = "SELECT icab002",
               "  FROM icab_t",
               " WHERE icabent = ",g_enterprise,
               "   AND icab001 = '",g_icaa_m.icaa001,"'",
               "   AND icab002 = (SELECT MAX(icab002) FROM icab_t",
               "                                     WHERE icabent = ",g_enterprise,
               "                                       AND icab001 = '",g_icaa_m.icaa001,"')"
   PREPARE aici100_pb8 FROM l_sql

   LET l_sql = "SELECT icab002",
               "  FROM icab_t",
               " WHERE icabent = ",g_enterprise,
               "   AND icab001 = '",p_icab001,"'",
               "   AND (icab004 = 'Y' OR icab002 = '",p_icab002,"')"
   PREPARE aici100_pb6 FROM l_sql
   DECLARE aici100_cs6 CURSOR FOR aici100_pb6

   #多角流程類型不為3.指定最終供應商時，最終站不可為委外工單開立點
   IF g_icaa_m.icaa003 <> '3' AND p_icab004 = 'Y' THEN
      LET l_icab002 = ''
      EXECUTE aici100_pb8 INTO l_icab002

      IF l_icab002 <= p_icab002 THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'aic-00010'
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()

         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF

   LET l_n = ''
   EXECUTE aici100_pb7 INTO l_n

   IF l_n > 0 THEN
      IF NOT cl_ask_confirm('aic-00133') THEN    #修改"委外工單開立點"，需連帶修改對應的"採購單單別"，是否願意修改？
         LET r_success = FALSE
         RETURN r_success
      END IF
   ELSE
      RETURN r_success
   END IF

   IF p_icab004 = 'Y' THEN   #委外工單
      LET l_icab002 = ''
      FOREACH aici100_cs6 INTO l_icab002
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = 'foreach:' 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            
            LET r_success = FALSE
            EXIT FOREACH
         END IF
      
         UPDATE icac_t
            SET icac010 = ''
          WHERE icacent = g_enterprise
            AND icac001 = p_icab001
            AND icac002 = l_icab002
         
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = 'update:' 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            
            LET r_success = FALSE
            EXIT FOREACH
         END IF

      END FOREACH

   ELSE    #非委外工單
      UPDATE icac_t
         SET icac010 = ''
       WHERE icacent = g_enterprise
         AND icac001 = p_icab001
         AND icac002 = p_icab002
         
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = 'update:' 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         
         LET r_success = FALSE
      END IF
   END IF
   
   RETURN r_success
END FUNCTION]]>
