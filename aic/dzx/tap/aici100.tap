  <section id="aici100.insert" ver="27" status="" src="s" readonly="">
    <![CDATA[#+ 資料新增
PRIVATE FUNCTION aici100_insert()
   #add-point:insert段define
   {<point name="insert.define" edit="s"/>}
   #end add-point    
   #add-point:insert段define(客製用)
   {<point name="insert.define_customerization" edit="c"/>}
   #end add-point    
   
   #清畫面欄位內容
   CLEAR FORM                    
   CALL g_icab_d.clear()   
   CALL g_icab2_d.clear()  
   CALL g_icab3_d.clear()  
   CALL g_icab4_d.clear()  
 
 
   INITIALIZE g_icaa_m.* LIKE icaa_t.*             #DEFAULT 設定
   
   LET g_icaa001_t = NULL
 
   
   LET g_master_insert = FALSE
   
   #add-point:insert段before
   {<point name="insert.before"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   WHILE TRUE
      #公用欄位給值(單頭)
      #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_icaa_m.icaaownid = g_user
      LET g_icaa_m.icaaowndp = g_dept
      LET g_icaa_m.icaacrtid = g_user
      LET g_icaa_m.icaacrtdp = g_dept 
      LET g_icaa_m.icaacrtdt = cl_get_current()
      LET g_icaa_m.icaamodid = g_user
      LET g_icaa_m.icaamoddt = cl_get_current()
      LET g_icaa_m.icaastus = 'Y'
 
 
 
      #append欄位給值
      
     
      #一般欄位給值
            LET g_icaa_m.icaa003 = "1"
      LET g_icaa_m.icaa004 = "1"
      LET g_icaa_m.icaa011 = "1"
      LET g_icaa_m.icaa014 = "1"
      LET g_icaa_m.icaa005 = "N"
      LET g_icaa_m.icaastus = "Y"
      LET g_icaa_m.icaa007 = "N"
      LET g_icaa_m.icaa008 = "0"
      LET g_icaa_m.icaa009 = "N"
      LET g_icaa_m.icaa010 = "1"
      LET g_icaa_m.icaa012 = "1"
 
  
      #add-point:單頭預設值
      {<point name="insert.default"/>}
      #end add-point 
      
      #保存單頭舊值(用於資料輸入錯誤還原預設值時使用)
      LET g_icaa_m_t.* = g_icaa_m.*
      LET g_icaa_m_o.* = g_icaa_m.*
      
      #顯示狀態(stus)圖片
            #應用 a21 樣板自動產生(Version:2)
	  #根據當下狀態碼顯示圖片
      CASE g_icaa_m.icaastus 
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
         
      END CASE
 
 
    
      CALL aici100_input("a")
      
      #add-point:單頭輸入後
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
      END IF
      
      IF NOT g_master_insert THEN
         DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
         DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
         INITIALIZE g_icaa_m.* TO NULL
         INITIALIZE g_icab_d TO NULL
         INITIALIZE g_icab2_d TO NULL
         INITIALIZE g_icab3_d TO NULL
         INITIALIZE g_icab4_d TO NULL
 
         #add-point:取消新增後
         {<point name="insert.cancel"/>}
         #end add-point 
         CALL aici100_show()
         RETURN
      END IF
      
      LET INT_FLAG = 0
      #CALL g_icab_d.clear()
      #CALL g_icab2_d.clear()
      #CALL g_icab3_d.clear()
      #CALL g_icab4_d.clear()
 
 
      LET g_rec_b = 0
      CALL s_transaction_end('Y','0')
      EXIT WHILE
        
   END WHILE
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL aici100_set_act_visible()   
   CALL aici100_set_act_no_visible()
   
   #將新增的資料併入搜尋條件中
   LET g_icaa001_t = g_icaa_m.icaa001
 
   
   #組合新增資料的條件
   LET g_add_browse = " icaaent = '" ||g_enterprise|| "' AND",
                      " icaa001 = '", g_icaa_m.icaa001, "' "
 
                      
   #add-point:組合新增資料的條件後
   {<point name="insert.after.add_browse"/>}
   #end add-point
      
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL aici100_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   CLOSE aici100_cl
   
   CALL aici100_idx_chk()
   
   #撈取異動後的資料(主要是帶出reference)
   EXECUTE aici100_master_referesh USING g_icaa_m.icaa001 INTO g_icaa_m.icaa001,g_icaa_m.icaa003,g_icaa_m.icaa004, 
       g_icaa_m.icaa011,g_icaa_m.icaa014,g_icaa_m.icaa005,g_icaa_m.icaa006,g_icaa_m.icaastus,g_icaa_m.icaasite, 
       g_icaa_m.icaa007,g_icaa_m.icaa008,g_icaa_m.icaa009,g_icaa_m.icaa010,g_icaa_m.icaa012,g_icaa_m.icaaownid, 
       g_icaa_m.icaaowndp,g_icaa_m.icaacrtid,g_icaa_m.icaacrtdp,g_icaa_m.icaacrtdt,g_icaa_m.icaamodid, 
       g_icaa_m.icaamoddt,g_icaa_m.icaa006_desc,g_icaa_m.icaaownid_desc,g_icaa_m.icaaowndp_desc,g_icaa_m.icaacrtid_desc, 
       g_icaa_m.icaacrtdp_desc,g_icaa_m.icaamodid_desc
   
   #遮罩相關處理
   LET g_icaa_m_mask_o.* =  g_icaa_m.*
   CALL aici100_icaa_t_mask()
   LET g_icaa_m_mask_n.* =  g_icaa_m.*
   
   #將資料顯示到畫面上
   DISPLAY BY NAME g_icaa_m.icaa001,g_icaa_m.icaal003,g_icaa_m.icaa003,g_icaa_m.icaa004,g_icaa_m.icaa011, 
       g_icaa_m.icaa014,g_icaa_m.icaa005,g_icaa_m.icaa006,g_icaa_m.icaa006_desc,g_icaa_m.icaastus,g_icaa_m.icaasite, 
       g_icaa_m.icaa007,g_icaa_m.icaa008,g_icaa_m.icaa009,g_icaa_m.icaa010,g_icaa_m.icaa012,g_icaa_m.icaaownid, 
       g_icaa_m.icaaownid_desc,g_icaa_m.icaaowndp,g_icaa_m.icaaowndp_desc,g_icaa_m.icaacrtid,g_icaa_m.icaacrtid_desc, 
       g_icaa_m.icaacrtdp,g_icaa_m.icaacrtdp_desc,g_icaa_m.icaacrtdt,g_icaa_m.icaamodid,g_icaa_m.icaamodid_desc, 
       g_icaa_m.icaamoddt
   
   #add-point:新增結束後
   {<point name="insert.after"/>}
   #end add-point 
   
   #功能已完成,通報訊息中心
   CALL aici100_msgcentre_notify('insert')
   
END FUNCTION
]]>
  </section>
