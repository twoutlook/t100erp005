  <point name="function.aici100_auto_slip" order="15" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 依據0站單別產生其他站別的單據別
# Memo...........:
# Usage..........: CALL aici100_auto_slip()
# Date & Author..: 2014/04/09 By Andy
# Modify.........:
################################################################################
PRIVATE FUNCTION aici100_auto_slip()
DEFINE l_cnt    LIKE type_t.num5

   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt FROM icab_t
     WHERE icabent = g_enterprise
       AND icab001 = g_icaa_m.icaa001
       AND icab002 = 0
   IF l_cnt = 0 THEN 
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'aic-00007'
      LET g_errparam.extend = g_icaa_m.icaa001
      LET g_errparam.popup = TRUE
      CALL cl_err()

      RETURN
   END IF

   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt FROM icac_t
     WHERE icacent = g_enterprise
       AND icac001 = g_icaa_m.icaa001
       AND icac002 <> 0
   IF l_cnt > 0 THEN 
      IF cl_ask_confirm('aic-00008') THEN
         DELETE FROM icac_t WHERE icacent = g_enterprise AND icac001 = g_icaa_m.icaa001 AND icac002 <> 0
      END IF      
   END IF
   INSERT INTO icac_t
   SELECT icabent,icabsite,icab001,icab002,icac003,icac004,icac005,icac006,
          icac007,icac008,icac009,icac010,icac011,icac012,icac013,icac014 
     FROM icab_t,(SELECT icac003,icac004,icac005,icac006,icac007,icac008,
                         icac009,icac010,icac011,icac012,icac013,icac014
                    FROM icac_t
                   WHERE icacent = g_enterprise
                     AND icac001 = g_icaa_m.icaa001
                     AND icac002 = 0 )                                      
    WHERE icabent = g_enterprise
      AND icab001 = g_icaa_m.icaa001
      AND icab002 <> 0   
END FUNCTION]]>
  </point>
  <point name="function.aici100_used" order="16" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION aici100_used()
DEFINE l_cnt1    LIKE type_t.num5
DEFINE l_cnt2    LIKE type_t.num5
DEFINE r_success LIKE type_t.num5

   LET r_success = TRUE
   LET l_cnt1 = 0
   SELECT COUNT(*) INTO l_cnt1 
     FROM pmdl_t
    WHERE pmdlent = g_enterprise
      AND pmdl031 = g_icaa_m.icaa001
   IF cl_null(l_cnt1) THEN LET l_cnt1 = 0 END IF   
   LET l_cnt2 = 0
   SELECT COUNT(*) INTO l_cnt2 
     FROM xmda_t
    WHERE xmdaent = g_enterprise
      AND xmda031 = g_icaa_m.icaa001
   IF cl_null(l_cnt2) THEN LET l_cnt2 = 0 END IF
   IF l_cnt1 > 0 OR l_cnt2 > 0 THEN 
      LET r_success = FALSE
   END IF
   RETURN r_success
END FUNCTION]]>
  </point>
  <point name="function.aici100_icab004_change" order="17" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION aici100_icab004_change()
DEFINE l_i   LIKE type_t.num5
   
   IF g_icab_d[l_ac].icab004 = 'Y' THEN 
      FOR l_i = 1 TO g_icab_d.getlength()
         IF g_icab_d[l_i].icab002 <> g_icab_d[l_ac].icab002 THEN 
            LET g_icab_d[l_i].icab004 = 'N'
         END IF
      END FOR
      DISPLAY ARRAY g_icab_d TO s_detail1.* ATTRIBUTES(COUNT=g_rec_b)
         BEFORE DISPLAY
            EXIT DISPLAY
      END DISPLAY       
   END IF
 
END FUNCTION]]>
