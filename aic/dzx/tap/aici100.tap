  <section id="aici100.modify" ver="24" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aici100_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   DEFINE l_wc2_table1          STRING
   DEFINE l_wc2_table2   STRING
 
   DEFINE l_wc2_table3   STRING
 
   DEFINE l_wc2_table4   STRING
 
 
 
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point    
   #add-point:modify段define(客製用)
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point    
   
   #保存單頭舊值
   LET g_icaa_m_t.* = g_icaa_m.*
   LET g_icaa_m_o.* = g_icaa_m.*
   
   IF g_icaa_m.icaa001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_icaa001_t = g_icaa_m.icaa001
 
   CALL s_transaction_begin()
   
   OPEN aici100_cl USING g_enterprise,g_icaa_m.icaa001
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aici100_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE aici100_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aici100_master_referesh USING g_icaa_m.icaa001 INTO g_icaa_m.icaa001,g_icaa_m.icaa003,g_icaa_m.icaa004, 
       g_icaa_m.icaa011,g_icaa_m.icaa014,g_icaa_m.icaa005,g_icaa_m.icaa006,g_icaa_m.icaastus,g_icaa_m.icaasite, 
       g_icaa_m.icaa007,g_icaa_m.icaa008,g_icaa_m.icaa009,g_icaa_m.icaa010,g_icaa_m.icaa012,g_icaa_m.icaaownid, 
       g_icaa_m.icaaowndp,g_icaa_m.icaacrtid,g_icaa_m.icaacrtdp,g_icaa_m.icaacrtdt,g_icaa_m.icaamodid, 
       g_icaa_m.icaamoddt,g_icaa_m.icaa006_desc,g_icaa_m.icaaownid_desc,g_icaa_m.icaaowndp_desc,g_icaa_m.icaacrtid_desc, 
       g_icaa_m.icaacrtdp_desc,g_icaa_m.icaamodid_desc
   
   #檢查是否允許此動作
   IF NOT aici100_action_chk() THEN
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_icaa_m_mask_o.* =  g_icaa_m.*
   CALL aici100_icaa_t_mask()
   LET g_icaa_m_mask_n.* =  g_icaa_m.*
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
   #LET l_wc2_table2 = g_wc2_table2
   #LET l_wc2_table2 = " 1=1"
 
   #LET l_wc2_table3 = g_wc2_table3
   #LET l_wc2_table3 = " 1=1"
 
   #LET l_wc2_table4 = g_wc2_table4
   #LET l_wc2_table4 = " 1=1"
 
 
 
   
   CALL aici100_show()
   #add-point:modify段show之後
   {<point name="modify.after_show"/>}
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
   #LET  g_wc2_table2 = l_wc2_table2 
 
   #LET  g_wc2_table3 = l_wc2_table3 
 
   #LET  g_wc2_table4 = l_wc2_table4 
 
 
 
    
   WHILE TRUE
      LET g_icaa001_t = g_icaa_m.icaa001
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_icaa_m.icaamodid = g_user 
LET g_icaa_m.icaamoddt = cl_get_current()
LET g_icaa_m.icaamodid_desc = cl_get_username(g_icaa_m.icaamodid)
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      CALL aici100_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE icaa_t SET (icaamodid,icaamoddt) = (g_icaa_m.icaamodid,g_icaa_m.icaamoddt)
          WHERE icaaent = g_enterprise AND icaa001 = g_icaa001_t
 
      END IF
    
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_icaa_m.* = g_icaa_m_t.*
         CALL aici100_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_icaa_m.icaa001 != g_icaa001_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE icab_t SET icab001 = g_icaa_m.icaa001
 
