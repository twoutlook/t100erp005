  <section id="aici100.state_change" ver="14" status="" src="s" readonly="">
    <![CDATA[   #應用 a09 樣板自動產生(Version:12)
#+ 確認碼變更 
PRIVATE FUNCTION aici100_statechange()
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define
   {<point name="statechange.define" edit="s"/>}
   #end add-point  
   #add-point:statechange段define(客製用)
   {<point name="statechange.define_customerization" edit="c"/>}
   #end add-point  
   
   #add-point:statechange段開始前
   {<point name="statechange.before"/>}
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_icaa_m.icaa001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN aici100_cl USING g_enterprise,g_icaa_m.icaa001
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aici100_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE aici100_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE aici100_master_referesh USING g_icaa_m.icaa001 INTO g_icaa_m.icaa001,g_icaa_m.icaa003,g_icaa_m.icaa004, 
       g_icaa_m.icaa011,g_icaa_m.icaa014,g_icaa_m.icaa005,g_icaa_m.icaa006,g_icaa_m.icaastus,g_icaa_m.icaasite, 
       g_icaa_m.icaa007,g_icaa_m.icaa008,g_icaa_m.icaa009,g_icaa_m.icaa010,g_icaa_m.icaa012,g_icaa_m.icaaownid, 
       g_icaa_m.icaaowndp,g_icaa_m.icaacrtid,g_icaa_m.icaacrtdp,g_icaa_m.icaacrtdt,g_icaa_m.icaamodid, 
       g_icaa_m.icaamoddt,g_icaa_m.icaa006_desc,g_icaa_m.icaaownid_desc,g_icaa_m.icaaowndp_desc,g_icaa_m.icaacrtid_desc, 
       g_icaa_m.icaacrtdp_desc,g_icaa_m.icaamodid_desc
 
   #檢查是否允許此動作
   IF NOT aici100_action_chk() THEN
      CLOSE aici100_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_icaa_m.icaa001,g_icaa_m.icaal003,g_icaa_m.icaa003,g_icaa_m.icaa004,g_icaa_m.icaa011, 
       g_icaa_m.icaa014,g_icaa_m.icaa005,g_icaa_m.icaa006,g_icaa_m.icaa006_desc,g_icaa_m.icaastus,g_icaa_m.icaasite, 
       g_icaa_m.icaa007,g_icaa_m.icaa008,g_icaa_m.icaa009,g_icaa_m.icaa010,g_icaa_m.icaa012,g_icaa_m.icaaownid, 
       g_icaa_m.icaaownid_desc,g_icaa_m.icaaowndp,g_icaa_m.icaaowndp_desc,g_icaa_m.icaacrtid,g_icaa_m.icaacrtid_desc, 
       g_icaa_m.icaacrtdp,g_icaa_m.icaacrtdp_desc,g_icaa_m.icaacrtdt,g_icaa_m.icaamodid,g_icaa_m.icaamodid_desc, 
       g_icaa_m.icaamoddt
 
   CASE g_icaa_m.icaastus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
      
   END CASE
 
   #add-point:資料刷新後
   {<point name="statechange.after_refresh"/>}
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_icaa_m.icaastus
            
            WHEN "N"
               HIDE OPTION "inactive"
            WHEN "Y"
               HIDE OPTION "active"
         END CASE
     
      #add-point:menu前
      {<point name="statechange.before_menu"/>}
      #end add-point
      
      
	  
      ON ACTION inactive
         IF cl_auth_chk_act("inactive") THEN
            LET lc_state = "N"
            #add-point:action控制
            {<point name="statechange.inactive"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION active
         IF cl_auth_chk_act("active") THEN
            LET lc_state = "Y"
            #add-point:action控制
            {<point name="statechange.active"/>}
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制
      {<point name="statechange.more_control"/>}
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      ) OR 
      g_icaa_m.icaastus = lc_state OR cl_null(lc_state) THEN
      CLOSE aici100_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前
   {<point name="statechange.b_update"/>}
   #end add-point
   
   LET g_icaa_m.icaamodid = g_user
   LET g_icaa_m.icaamoddt = cl_get_current()
   LET g_icaa_m.icaastus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE icaa_t 
      SET (icaastus,icaamodid,icaamoddt) 
        = (g_icaa_m.icaastus,g_icaa_m.icaamodid,g_icaa_m.icaamoddt)     
    WHERE icaaent = g_enterprise AND icaa001 = g_icaa_m.icaa001
 
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE aici100_master_referesh USING g_icaa_m.icaa001 INTO g_icaa_m.icaa001,g_icaa_m.icaa003, 
          g_icaa_m.icaa004,g_icaa_m.icaa011,g_icaa_m.icaa014,g_icaa_m.icaa005,g_icaa_m.icaa006,g_icaa_m.icaastus, 
          g_icaa_m.icaasite,g_icaa_m.icaa007,g_icaa_m.icaa008,g_icaa_m.icaa009,g_icaa_m.icaa010,g_icaa_m.icaa012, 
          g_icaa_m.icaaownid,g_icaa_m.icaaowndp,g_icaa_m.icaacrtid,g_icaa_m.icaacrtdp,g_icaa_m.icaacrtdt, 
          g_icaa_m.icaamodid,g_icaa_m.icaamoddt,g_icaa_m.icaa006_desc,g_icaa_m.icaaownid_desc,g_icaa_m.icaaowndp_desc, 
          g_icaa_m.icaacrtid_desc,g_icaa_m.icaacrtdp_desc,g_icaa_m.icaamodid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_icaa_m.icaa001,g_icaa_m.icaal003,g_icaa_m.icaa003,g_icaa_m.icaa004,g_icaa_m.icaa011, 
          g_icaa_m.icaa014,g_icaa_m.icaa005,g_icaa_m.icaa006,g_icaa_m.icaa006_desc,g_icaa_m.icaastus, 
          g_icaa_m.icaasite,g_icaa_m.icaa007,g_icaa_m.icaa008,g_icaa_m.icaa009,g_icaa_m.icaa010,g_icaa_m.icaa012, 
          g_icaa_m.icaaownid,g_icaa_m.icaaownid_desc,g_icaa_m.icaaowndp,g_icaa_m.icaaowndp_desc,g_icaa_m.icaacrtid, 
          g_icaa_m.icaacrtid_desc,g_icaa_m.icaacrtdp,g_icaa_m.icaacrtdp_desc,g_icaa_m.icaacrtdt,g_icaa_m.icaamodid, 
          g_icaa_m.icaamodid_desc,g_icaa_m.icaamoddt
   END IF
 
   #add-point:stus修改後
   {<point name="statechange.a_update"/>}
   #end add-point
 
   #add-point:statechange段結束前
   {<point name="statechange.after"/>}
   #end add-point  
 
   CLOSE aici100_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aici100_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION
 
 
]]>
