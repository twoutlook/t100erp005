      UPDATE icab_t
         SET icab005 = 'N',
             icab008 = 'N'   #150414---earl---add
       WHERE icabent = g_enterprise
         AND icab001 = g_icaa_m.icaa001
   END IF
 
   CALL aici100_b_fill()   #150414---earl---add
END FUNCTION

