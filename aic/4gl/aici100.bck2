PRIVATE FUNCTION aici100_icaa005_upd()
DEFINE l_cnt LIKE type_t.num5

   IF g_icaa_m.icaa005 = 'Y' THEN 
      LET l_cnt = 0
      SELECT COUNT(*) INTO l_cnt
        FROM icab_t
       WHERE icabent = g_enterprise
         AND icab001 = g_icaa_m.icaa001
         AND icab005 = 'Y'
      IF l_cnt = 0 THEN
         CALL s_transaction_begin()
         UPDATE icaa_t      
            SET icaa005 = 'N'
          WHERE icaaent = g_enterprise
            AND icaa001 = g_icaa_m.icaa001
         CALL s_transaction_end('Y','0')   
      END IF 
   END IF  
END FUNCTION

