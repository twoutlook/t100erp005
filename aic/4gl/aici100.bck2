PRIVATE FUNCTION aici100_icab004_icac010_chk(p_icab001,p_icab002,p_icab004)
   DEFINE p_icab001    LIKE icab_t.icab001
   DEFINE p_icab002    LIKE icab_t.icab002
   DEFINE p_icab004    LIKE icab_t.icab004
   DEFINE r_success    LIKE type_t.num5
   DEFINE l_sql        STRING
   DEFINE l_icab002    LIKE icab_t.icab002
   DEFINE l_n          LIKE type_t.num5
   
   LET r_success = TRUE

   LET l_sql = "SELECT COUNT(icab002)",
               "  FROM icab_t",
               " WHERE icabent = ",g_enterprise,
               "   AND icab001 = '",p_icab001,"'",
               "   AND (icab004 = 'Y' OR icab002 = '",p_icab002,"')"
   PREPARE aici100_pb7 FROM l_sql

   LET l_sql = "SELECT icab002",
               "  FROM icab_t",
               " WHERE icabent = ",g_enterprise,
               "   AND icab001 = '",g_icaa_m.icaa001,"'",
               "   AND icab002 = (SELECT MAX(icab002) FROM icab_t",
               "                                     WHERE icabent = ",g_enterprise,
               "                                       AND icab001 = '",g_icaa_m.icaa001,"')"
   PREPARE aici100_pb8 FROM l_sql

   LET l_sql = "SELECT icab002",
               "  FROM icab_t",
               " WHERE icabent = ",g_enterprise,
               "   AND icab001 = '",p_icab001,"'",
               "   AND (icab004 = 'Y' OR icab002 = '",p_icab002,"')"
   PREPARE aici100_pb6 FROM l_sql
   DECLARE aici100_cs6 CURSOR FOR aici100_pb6

   #多角流程類型不為3.指定最終供應商時，最終站不可為委外工單開立點
   IF g_icaa_m.icaa003 <> '3' AND p_icab004 = 'Y' THEN
      LET l_icab002 = ''
      EXECUTE aici100_pb8 INTO l_icab002

      IF l_icab002 <= p_icab002 THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'aic-00010'
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()

         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF

   LET l_n = ''
   EXECUTE aici100_pb7 INTO l_n

   IF l_n > 0 THEN
      IF NOT cl_ask_confirm('aic-00133') THEN    #修改"委外工單開立點"，需連帶修改對應的"採購單單別"，是否願意修改？
         LET r_success = FALSE
         RETURN r_success
      END IF
   ELSE
      RETURN r_success
   END IF

   IF p_icab004 = 'Y' THEN   #委外工單
      LET l_icab002 = ''
      FOREACH aici100_cs6 INTO l_icab002
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = 'foreach:' 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            
            LET r_success = FALSE
            EXIT FOREACH
         END IF
      
         UPDATE icac_t
            SET icac010 = ''
          WHERE icacent = g_enterprise
            AND icac001 = p_icab001
            AND icac002 = l_icab002
         
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = 'update:' 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            
            LET r_success = FALSE
            EXIT FOREACH
         END IF

      END FOREACH

   ELSE    #非委外工單
      UPDATE icac_t
         SET icac010 = ''
       WHERE icacent = g_enterprise
         AND icac001 = p_icab001
         AND icac002 = p_icab002
         
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = 'update:' 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         
         LET r_success = FALSE
      END IF
   END IF
   
   RETURN r_success
END FUNCTION

################################################################################
# Descriptions...: 單別檢查
# Memo...........:
# Usage..........: CALL aici100_docno_chk(p_site,p_ooba002_o,p_ooba002,p_type,p_oobl002)
#                  RETURNING r_success
# Input parameter: p_site      #營運據點
#                : p_ooba002_o #單據別(舊值)
#                : p_ooba002   #單據別
#                : p_type      #1.訂單單別2.出通單單別3.包裝單單別4.Invoice單別5.出貨單單別
#                :              6.簽收單單別7.銷退單單別8.採購單單別9.無採購收貨入庫單單別10.收貨入庫單單別
#                :              11.倉退單單別12.委外工單單別13.無訂單出貨單單別
#                : p_oobl002   #作業編號
# Return code....: r_success
#                : 
# Date & Author..: 150803 By earl
# Modify.........:
################################################################################
PRIVATE FUNCTION aici100_docno_chk(p_site,p_ooba002_o,p_ooba002,p_type,p_oobl002)
   DEFINE p_site      LIKE ooef_t.ooef001
   DEFINE p_ooba002_o LIKE ooba_t.ooba002
   DEFINE p_ooba002   LIKE ooba_t.ooba002
   DEFINE p_type      LIKE type_t.chr5
   DEFINE p_oobl002   LIKE oobl_t.oobl002
   
   DEFINE r_success   LIKE type_t.num5
   
   LET r_success = TRUE
   
   IF NOT cl_null(p_site) THEN
      #單據別變更或清空
      IF NOT cl_null(p_ooba002_o) THEN
         IF p_ooba002_o <> p_ooba002 OR cl_null(p_ooba002) THEN
         
            #多角單據流水號保持一致
            IF g_aic_doc THEN
               IF aici100_oobf_count(p_site,p_ooba002_o) THEN
                  #單據已使用
                  IF NOT cl_ask_confirm('aic-00215') THEN   #多角貿易設定為"多角單據流水號保持一致"且單別"%1"已有單據使用，修改後將不可改回，是否確定修改？
                     LET r_success = FALSE
                     RETURN r_success
                  END IF
               END IF
            END IF
            
         END IF
      END IF
      
      IF NOT cl_null(p_ooba002) AND NOT cl_null(p_oobl002) THEN
         IF p_ooba002 <> p_ooba002_o OR cl_null(p_ooba002_o) THEN
         
            #單據別檢查
            IF NOT s_aooi200_chk_slip(p_site,'',p_ooba002,p_oobl002) THEN
               LET r_success = FALSE
               RETURN r_success
            END IF
            
            #多角單據流水號保持一致
            IF g_aic_doc THEN
               IF NOT aici100_oobf_chk(p_site,p_ooba002,p_type) THEN
                  LET r_success = FALSE
                  RETURN r_success
               END IF
            END IF
         
         END IF
      END IF
   END IF
   
   RETURN r_success
END FUNCTION

