# Descriptions...: 判斷該單別使否已使用
# Memo...........:
# Usage..........: CALL aici100_oobf_count(p_site,p_oobf002)
#                  RETURNING r_success
# Input parameter: p_site     營運據點
#                : p_oobf002  單據別
# Return code....: r_success  TRUE已使用、FALSE未使用
#                : 
# Date & Author..: 150105 By earl
# Modify.........:
################################################################################
PRIVATE FUNCTION aici100_oobf_count(p_site,p_oobf002)
   DEFINE p_site        LIKE ooef_t.ooef001  #營運據點
   DEFINE p_oobf002     LIKE oobf_t.oobf002  #單據別
   DEFINE r_success     LIKE type_t.num5     #TRUE:已使用 FALSE:未使用
   
   DEFINE l_ooef004     LIKE ooef_t.ooef004
   DEFINE l_ooef005     LIKE ooef_t.ooef005
   DEFINE l_oobf004     LIKE oobf_t.oobf004
   
   LET r_success = FALSE
   
   IF NOT cl_null(p_site) AND NOT cl_null(p_oobf002) THEN
      #單據別參照表號,單據據點代號
      LET l_ooef004 = ''
      LET l_ooef005 = ''
      SELECT ooef004,ooef005
        INTO l_ooef004,l_ooef005
        FROM ooef_t
       WHERE ooefent = g_enterprise
         AND ooef001 = p_site
      
      #最大流水號
      LET l_oobf004 = ''
      SELECT MAX(oobf004) INTO l_oobf004
        FROM oobf_t
       WHERE oobfent = g_enterprise
         AND oobfsite = p_site
         AND oobf001 = l_ooef005
         AND oobf002 = p_oobf002
         
      IF l_oobf004 > 0 THEN
         LET r_success = TRUE
         RETURN r_success
      END IF
   END IF
   
   RETURN r_success
END FUNCTION

################################################################################
# Descriptions...: 取得單據參照表
# Date & Author..: 16/07/25 By 02097
# Modify.........:
################################################################################
PRIVATE FUNCTION aici100_get_glaa024(p_ooef001)
DEFINE p_ooef001        LIKE ooef_t.ooef001
DEFINE l_ooef017        LIKE ooef_t.ooef017
DEFINE l_glaa024        LIKE glaa_t.glaa024

   SELECT ooef017 INTO l_ooef017
     FROM ooef_t
    WHERE ooefent = g_enterprise
      AND ooef001 = p_ooef001
   
   SELECT glaa024 INTO l_glaa024
     FROM glaa_t
    WHERE glaaent = g_enterprise
      AND glaacomp= l_ooef017
      AND glaa014 = 'Y'
   
   RETURN l_glaa024
END FUNCTION

################################################################################
# Descriptions...: 應付單別檢查
# Memo...........:
# Usage..........: CALL aici100_icae004_chk(p_ooba001,p_ooba002)
#                  RETURNING r_success
# Input parameter: p_ooba001  #單據別參照表號
#                : p_ooba002  #單據別
# Return code....: r_success
#                : 
# Date & Author..: 160905 By 06948 160902-00001#1
# Modify.........:
################################################################################
PRIVATE FUNCTION aici100_icae004_chk(p_ooba001,p_ooba002)
   DEFINE p_ooba001    LIKE ooba_t.ooba001
   DEFINE p_ooba002    LIKE ooba_t.ooba002
   DEFINE r_success    LIKE type_t.num5
   
   LET r_success = TRUE
   
   IF NOT cl_null(p_ooba001) AND NOT cl_null(p_ooba002) THEN
      #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
      INITIALIZE g_chkparam.* TO NULL
                 
      #設定g_chkparam.*的參數
      LET g_chkparam.arg1 = p_ooba001
      LET g_chkparam.arg2 = p_ooba002
      LET g_errshow = TRUE #是否開窗                   
      LET g_chkparam.err_str[1] ="agl-00189:sub-01302|aooi200|",cl_get_progname("aooi200",g_lang,"2"),"|:EXEPROGaooi200"
      #呼叫檢查存在並帶值的library
      IF NOT cl_chk_exist("v_ooba002_12") THEN
         #檢查失敗時後續處理
         LET r_success = FALSE
      END IF
   END IF
   
   RETURN r_success
END FUNCTION

 
