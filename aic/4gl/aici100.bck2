PRIVATE FUNCTION aici100_oocql001_get(p_flag)
   DEFINE p_flag  LIKE gzcb_t.gzcb002
   DEFINE r_oocql001 LIKE oocql_t.oocql001
   DEFINE l_gzcb002  LIKE gzcb_t.gzcb002

   LET r_oocql001 = ''

   CASE p_flag
      WHEN '1'   #出貨單
         INITIALIZE g_ref_fields TO NULL
         LET g_ref_fields[1] = g_icaa_m.icaa001
         LET g_ref_fields[2] = g_icab3_d[l_ac].icad002
         CALL ap_ref_array2(g_ref_fields,"SELECT oobx003 FROM oobx_t,icac_t WHERE oobxent = icacent AND oobx001 = icac007 AND icacent = "||g_enterprise||" AND icac001 = ? AND icac002 = ? ","") RETURNING g_rtn_fields
         LET l_gzcb002 = g_rtn_fields[1]            
      WHEN '2'   #入庫單
         INITIALIZE g_ref_fields TO NULL
         LET g_ref_fields[1] = g_icaa_m.icaa001
         LET g_ref_fields[2] = g_icab3_d[l_ac].icad002
         CALL ap_ref_array2(g_ref_fields,"SELECT oobx003 FROM oobx_t,icac_t WHERE oobxent = icacent AND oobx001 = icac012 AND icacent = "||g_enterprise||" AND icac001 = ? AND icac002 = ? ","") RETURNING g_rtn_fields
         LET l_gzcb002 = g_rtn_fields[1]       
      WHEN '3'   #銷退單
         INITIALIZE g_ref_fields TO NULL
         LET g_ref_fields[1] = g_icaa_m.icaa001
         LET g_ref_fields[2] = g_icab3_d[l_ac].icad002
         CALL ap_ref_array2(g_ref_fields,"SELECT oobx003 FROM oobx_t,icac_t WHERE oobxent = icacent AND oobx001 = icac009 AND icacent = "||g_enterprise||" AND icac001 = ? AND icac002 = ? ","") RETURNING g_rtn_fields
         LET l_gzcb002 = g_rtn_fields[1]       
      WHEN '4'   #倉退單
         INITIALIZE g_ref_fields TO NULL
         LET g_ref_fields[1] = g_icaa_m.icaa001
         LET g_ref_fields[2] = g_icab3_d[l_ac].icad002
         CALL ap_ref_array2(g_ref_fields,"SELECT oobx003 FROM oobx_t,icac_t WHERE oobxent = icacent AND oobx001 = icac013 AND icacent = "||g_enterprise||" AND icac001 = ? AND icac002 = ? ","") RETURNING g_rtn_fields
         LET l_gzcb002 = g_rtn_fields[1]       
      WHEN '5'   #調撥單 
         LET l_gzcb002 = 'aint330'       
   END CASE

   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = l_gzcb002
   CALL ap_ref_array2(g_ref_fields,"SELECT gzcb004 FROM gzcb_t WHERE gzcb001='24' AND gzcb002=? ","") RETURNING g_rtn_fields
   LET r_oocql001 = g_rtn_fields[1]
   
   IF cl_null(r_oocql001) THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "axm-00704"   #系統分類碼%1，分類碼值%2對應的"系統應用欄位二"無值！
      LET g_errparam.popup  = FALSE 
      LET g_errparam.replace[1] = '24'
      LET g_errparam.replace[2] = l_gzcb002
      
      CALL cl_err()
   END IF
   
   RETURN r_oocql001
END FUNCTION

