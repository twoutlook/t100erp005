################################################################################
# Descriptions...: 單據別是否使用過檢查
# Memo...........:
# Usage..........: CALL aici100_oobf_chk(p_site,p_oobf002,p_type)
#                  RETURNING r_success
# Input parameter: p_site      #營運據點
#                : p_oobf002   #單據別
#                : p_type      #1.訂單單別2.出通單單別3.包裝單單別4.Invoice單別5.出貨單單別
#                :              6.簽收單單別7.銷退單單別8.採購單單別9.無採購收貨入庫單單別10.收貨入庫單單別
#                :              11.倉退單單別12.委外工單單別13.無訂單出貨單單別
# Return code....: r_success   #執行結果
#                : 
# Date & Author..: 150327 By earl
# Modify.........:
################################################################################
PRIVATE FUNCTION aici100_oobf_chk(p_site,p_oobf002,p_type)
   DEFINE p_site      LIKE ooef_t.ooef001
   DEFINE p_oobf002   LIKE oobf_t.oobf002
   DEFINE p_type      LIKE type_t.chr5
   DEFINE r_success   LIKE type_t.num5

   DEFINE l_sql       STRING
   DEFINE l_icac002   LIKE icac_t.icac002
   
   LET r_success = TRUE
   
   IF NOT cl_null(p_site) AND NOT cl_null(p_oobf002) THEN
      IF aici100_oobf_count(p_site,p_oobf002) THEN
         #若該單別存在oobf_t且最大流水號>0，表示已被使用，不可設定
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 'aic-00127'   #該單據別已被使用，不可設定！ 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
       
         LET r_success = FALSE
         RETURN r_success
      END IF
      
      IF NOT cl_null(p_type) THEN
         #輸入的單別不可存在於aici100中其他筆相同營運據點的資料內，避免單號出現重複的問題
         LET l_sql = "SELECT COUNT(icac002)",
                     "  FROM icab_t,icac_t",
                     " WHERE icabent = icacent AND icacent = ",g_enterprise,
                     "   AND icab001 = icac001",
                     "   AND icab002 = icac002",
                     "   AND icab003 = '",p_site,"'"
         CASE p_type
            WHEN '1'  #訂單單別
               LET l_sql = l_sql," AND icac003 = '",p_oobf002,"'"
            WHEN '2'  #出通單單別
               LET l_sql = l_sql," AND icac004 = '",p_oobf002,"'"
            WHEN '3'  #包裝單單別
               LET l_sql = l_sql," AND icac005 = '",p_oobf002,"'"
            WHEN '4'  #Invoice單別
               LET l_sql = l_sql," AND icac006 = '",p_oobf002,"'"
            WHEN '5'  #出貨單單別
               LET l_sql = l_sql," AND icac007 = '",p_oobf002,"'"
            WHEN '6'  #簽收單單別
               LET l_sql = l_sql," AND icac008 = '",p_oobf002,"'"
            WHEN '7'  #銷退單單別
               LET l_sql = l_sql," AND icac009 = '",p_oobf002,"'"
            WHEN '8'  #採購單單別
               LET l_sql = l_sql," AND icac010 = '",p_oobf002,"'"
            WHEN '9'  #無採購收貨入庫單單別
               LET l_sql = l_sql," AND icac011 = '",p_oobf002,"'"
            WHEN '10' #收貨入庫單單別
               LET l_sql = l_sql," AND icac012 = '",p_oobf002,"'"
            WHEN '11' #倉退單單別
               LET l_sql = l_sql," AND icac013 = '",p_oobf002,"'"
            WHEN '12' #委外工單單別
               LET l_sql = l_sql," AND icac014 = '",p_oobf002,"'"
            WHEN '13' #無訂單出貨單單別
               LET l_sql = l_sql," AND icac015 = '",p_oobf002,"'"
         END CASE
         
         PREPARE aici100_pb5 FROM l_sql
         LET l_icac002 = ''
         EXECUTE aici100_pb5 INTO l_icac002
          
         IF l_icac002 > 0 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "" 
            LET g_errparam.code   = 'aic-00128'   #該單據別已存在相同營運據點之資料，不可重複輸入！
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
          
            LET r_success = FALSE
            RETURN r_success
         END IF
      END IF
   
   END IF
      
   RETURN r_success
END FUNCTION

#抓取委外工單開立點
PRIVATE FUNCTION aici100_icab004_sel(p_icab001,p_icab002)
   DEFINE p_icab001    LIKE icab_t.icab001
   DEFINE p_icab002    LIKE icab_t.icab002
   DEFINE r_icab004    LIKE icab_t.icab004
   
   LET r_icab004 = ''
   SELECT icab004 INTO r_icab004
     FROM icab_t
    WHERE icabent = g_enterprise
      AND icab001 = p_icab001
      AND icab002 = p_icab002
   
   IF cl_null(r_icab004) THEN
      LET r_icab004 = 'N'
   END IF
   
   RETURN r_icab004
END FUNCTION

