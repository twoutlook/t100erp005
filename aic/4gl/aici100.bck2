################################################################################
# Descriptions...: 檢查是否跨法人
# Memo...........:
# Usage..........: CALL aici100_legal_chk(p_icab002,p_icab003)
#                  RETURNING r_success
# Input parameter: p_icab002  站別(舊值排除用)
#                : p_icab003  營運據點
# Return code....: r_success  執行結果
#                : 
# Date & Author..: 151109 By earl
# Modify.........:
################################################################################
PRIVATE FUNCTION aici100_legal_chk(p_icab002,p_icab003)
   DEFINE p_icab002    LIKE icab_t.icab002
   DEFINE p_icab003    LIKE icab_t.icab003
   DEFINE r_success    LIKE type_t.num5
   
   DEFINE l_icab       RECORD
           icab002      LIKE icab_t.icab002,
           icab003      LIKE icab_t.icab003
                       END RECORD
   DEFINE l_ooef017    LIKE ooef_t.ooef017
   
   LET r_success = TRUE
   
   LET g_sql = "SELECT ooef017",
               "  FROM ooef_t",
               " WHERE ooefent = ",g_enterprise,
               "   AND ooef001 = ?"
   PREPARE aici100_ooef017_pre FROM g_sql
   LET l_ooef017 = ''
   EXECUTE aici100_ooef017_pre USING p_icab003 INTO l_ooef017
   
   LET g_sql = "SELECT icab002,icab003 ",
               "  FROM icab_t,ooef_t",
               " WHERE icabent = ooefent AND ooefent = ",g_enterprise,
               "   AND icab003 = ooef001 AND ooef017 = '",l_ooef017,"'",
               "   AND icab001 = '",g_icaa_m.icaa001,"'"

   IF NOT cl_null(p_icab002) THEN
      LET g_sql = g_sql," AND icab002 <> '",p_icab002,"'"
   END IF
   PREPARE aici100_legal_pre FROM g_sql
   DECLARE aici100_legal_cs CURSOR FOR aici100_legal_pre
   
   CALL cl_err_collect_init()
   INITIALIZE l_icab.* TO NULL
   FOREACH aici100_legal_cs INTO l_icab.icab002,l_icab.icab003
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = 'FOREACH aici100_legal_cs' 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         
         LET r_success = FALSE
         EXIT FOREACH
      END IF

      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = ''
      LET g_errparam.code   = 'aic-00196'   #輸入營運據點'%1'之法人'%2'與站別'%3'營運據點'%4'之法人重複，請重新輸入！
      LET g_errparam.popup  = TRUE 
      LET g_errparam.replace[1] = p_icab003
      LET g_errparam.replace[2] = l_ooef017
      LET g_errparam.replace[3] = l_icab.icab002
      LET g_errparam.replace[4] = l_icab.icab003
      
      CALL cl_err()
      
      LET r_success = FALSE
   END FOREACH
   
   CALL cl_err_collect_show()
   RETURN r_success
END FUNCTION

