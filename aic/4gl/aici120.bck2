{<section id="aici120.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION aici120_b_fill(p_wc2)
   #add-point:b_fill段define name="b_fill.define_customerization"
   
   #end add-point
   DEFINE p_wc2      STRING
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   
   #end add-point     
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   #先清空單身變數內容
   CALL g_icag_d.clear()
   CALL g_icag2_d.clear()
 
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   
   #end add-point
   
   LET g_sql = "SELECT  DISTINCT icag003,icag004,icag005,icag008,icag007,icag003,icagownid,icagowndp, 
       icagcrtid,icagcrtdp,icagcrtdt,icagmodid,icagmoddt,t1.ooefl003 ,t2.ooail003 ,t3.ooag011 ,t4.ooefl003 , 
       t5.ooag011 ,t6.ooefl003 ,t7.ooag011 FROM icag_t",   
               "",
               
                              " LEFT JOIN ooefl_t t1 ON t1.ooeflent="||g_enterprise||" AND t1.ooefl001=icag004 AND t1.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooail_t t2 ON t2.ooailent="||g_enterprise||" AND t2.ooail001=icag008 AND t2.ooail002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent="||g_enterprise||" AND t3.ooag001=icagownid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent="||g_enterprise||" AND t4.ooefl001=icagowndp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=icagcrtid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent="||g_enterprise||" AND t6.ooefl001=icagcrtdp AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent="||g_enterprise||" AND t7.ooag001=icagmodid  ",
 
               " WHERE icagent= ? AND icag001=? AND icag002=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("icag_t")
   END IF
   
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF aici120_fill_chk(1) THEN
      IF (g_action_choice = "query" OR cl_null(g_action_choice))
         #add-point:b_fill段long_sql_if name="b_fill.long_sql_if"
         
         #end add-point
      THEN
         LET g_sql = g_sql, " ORDER BY icag_t.icag003"
         #add-point:b_fill段fill_before name="b_fill.fill_before"
         
         #end add-point
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE aici120_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR aici120_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_icag_m.icag001,g_icag_m.icag002
                                               
      FOREACH b_fill_cs INTO g_icag_d[l_ac].icag003,g_icag_d[l_ac].icag004,g_icag_d[l_ac].icag005,g_icag_d[l_ac].icag008, 
          g_icag_d[l_ac].icag007,g_icag2_d[l_ac].icag003,g_icag2_d[l_ac].icagownid,g_icag2_d[l_ac].icagowndp, 
          g_icag2_d[l_ac].icagcrtid,g_icag2_d[l_ac].icagcrtdp,g_icag2_d[l_ac].icagcrtdt,g_icag2_d[l_ac].icagmodid, 
          g_icag2_d[l_ac].icagmoddt,g_icag_d[l_ac].icag004_desc,g_icag_d[l_ac].icag008_desc,g_icag2_d[l_ac].icagownid_desc, 
          g_icag2_d[l_ac].icagowndp_desc,g_icag2_d[l_ac].icagcrtid_desc,g_icag2_d[l_ac].icagcrtdp_desc, 
          g_icag2_d[l_ac].icagmodid_desc
                             
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充 name="b_fill.fill"
         
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #應用 a12 樣板自動產生(Version:4)
 
 
 
 
        
         #add-point:單身資料抓取 name="bfill.foreach"
         
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF 
            EXIT FOREACH
         END IF
      
         LET l_ac = l_ac + 1
         
      END FOREACH
 
            CALL g_icag_d.deleteElement(g_icag_d.getLength())
      CALL g_icag2_d.deleteElement(g_icag2_d.getLength())
 
      
   END IF
   
   #add-point:b_fill段more name="b_fill.more"
   
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_icag_d.getLength()
      LET g_icag_d_mask_o[l_ac].* =  g_icag_d[l_ac].*
      CALL aici120_icag_t_mask()
      LET g_icag_d_mask_n[l_ac].* =  g_icag_d[l_ac].*
   END FOR
   
   LET g_icag2_d_mask_o.* =  g_icag2_d.*
   FOR l_ac = 1 TO g_icag2_d.getLength()
      LET g_icag2_d_mask_o[l_ac].* =  g_icag2_d[l_ac].*
      CALL aici120_icag_t_mask()
      LET g_icag2_d_mask_n[l_ac].* =  g_icag2_d[l_ac].*
   END FOR
 
 
   FREE aici120_pb   
   
END FUNCTION
 
