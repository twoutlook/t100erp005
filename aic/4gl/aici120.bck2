   EXECUTE aici120_master_referesh USING g_icag_m.icag001,g_icag_m.icag002 INTO g_icag_m.icag001,g_icag_m.icag002, 
       g_icag_m.icag006,g_icag_m.icagsite,g_icag_m.icag001_desc
   
   #遮罩相關處理
   LET g_icag_m_mask_o.* =  g_icag_m.*
   CALL aici120_icag_t_mask()
