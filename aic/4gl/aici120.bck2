PRIVATE FUNCTION aici120_icag_b()
DEFINE l_cnt       LIKE type_t.num5
DEFINE l_sql       STRING
DEFINE l_icag    RECORD
       icagent LIKE icag_t.icagent,
       icagsite LIKE icag_t.icagsite,
       icag001 LIKE icag_t.icag001,
       icag002 LIKE icag_t.icag002,
       icag003 LIKE icag_t.icag003, 
       icag004 LIKE icag_t.icag004,  
       icag005 LIKE icag_t.icag005,
       icag006 LIKE icag_t.icag006,
       icag007 LIKE icag_t.icag007,   
       icag008 LIKE icag_t.icag008, 
       icagownid LIKE icag_t.icagownid, 
       icagowndp LIKE icag_t.icagowndp, 
       icagcrtid LIKE icag_t.icagcrtid, 
       icagcrtdp LIKE icag_t.icagcrtdp, 
       icagcrtdt DATETIME YEAR TO SECOND, 
       icagmodid LIKE icag_t.icagmodid, 
       icagmoddt DATETIME YEAR TO SECOND
       END RECORD

   
   LET l_cnt = 0 
   SELECT COUNT(*) INTO l_cnt 
     FROM icag_t
    WHERE icagent = g_enterprise
      AND icag001 = g_icag_m.icag001
      AND icag002 = g_icag_m.icag002
  #IF g_icag_m.icag001 <> 'ALL' THEN #160922-00034#2 mark
 
   IF l_cnt > 0 THEN
      SELECT DISTINCT icag006 INTO l_icag.icag006
        FROM icag_t
       WHERE icagent = g_enterprise
         AND icag001 = g_icag_m.icag001
         AND icag002 = g_icag_m.icag002
      IF cl_null(l_icag.icag006) THEN LET l_icag.icag006 = '1' END IF 
      
      LET l_sql = " SELECT DISTINCT icab002,icab003 FROM icab_t ",
                  "  WHERE icabent = '",g_enterprise,"'",
                  "    AND icab001 = '",g_icag_m.icag001,"'",
                  "    AND icab002 NOT IN (SELECT icag003 FROM icag_t WHERE icagent ='",g_enterprise,"'",
                  "                         AND icag001 ='",g_icag_m.icag001,"' AND icag002 = '",g_icag_m.icag002,"')"
   ELSE
      LET l_icag.icag006 = '1'
      LET l_sql = " SELECT DISTINCT icab002,icab003 FROM icab_t ",
                  "  WHERE icabent = '",g_enterprise,"'",
                  "    AND icab001 = '",g_icag_m.icag001,"'"      
   END IF
   LET l_icag.icagent = g_enterprise
   LET l_icag.icagsite = g_site
   LET l_icag.icag001 = g_icag_m.icag001
   LET l_icag.icag002 = g_icag_m.icag002  
   LET l_icag.icag005 = '1'
   LET l_icag.icag007 = ''   
   LET l_icag.icag008 = '' 
   LET l_icag.icagownid = g_user
   LET l_icag.icagowndp = g_dept
   LET l_icag.icagcrtid = g_user 
   LET l_icag.icagcrtdp = g_dept 
   LET l_icag.icagcrtdt = cl_get_current() 
   LET l_icag.icagmodid = ''
   LET l_icag.icagmoddt = ''   
   PREPARE icag_pre FROM l_sql
   DECLARE icag_cur CURSOR FOR icag_pre
   FOREACH icag_cur INTO l_icag.icag003,l_icag.icag004
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "FOREACH:"
         LET g_errparam.popup = TRUE
         CALL cl_err()
         EXIT FOREACH
      END IF          
     #INSERT INTO icag_t VALUES(l_icag.*)  #160922-00034#2 mark
     #160922-00034#2-s-add 
      INSERT INTO icag_t (icagent,icagsite,
                         icag001,icag002,icag003,icag004,icag005,
                         icag006,icag007,icag008,
                         icagownid,icagowndp,icagcrtid,icagcrtdp,icagcrtdt,
                         icagmodid,icagmoddt) 
                  VALUES(g_enterprise,l_icag.icagsite,
                         l_icag.icag001,l_icag.icag002,l_icag.icag003,l_icag.icag004,l_icag.icag005,
