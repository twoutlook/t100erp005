      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_amhm320
      
   END IF 
   
   CLOSE amhm320_cl
   
   
 
   #add-point:作業離開前 name="main.exit"
