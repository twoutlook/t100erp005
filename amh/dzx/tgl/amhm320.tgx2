{<section id="amhm320.modify" >}
#+ 資料修改
PRIVATE FUNCTION amhm320_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   {<point name="modify.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   {<point name="modify.define"/>}
   #end add-point
   
   #add-point:Function前置處理 name="modify.pre_function"
   {<point name="modify.pre_function"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_mhag_m.mhag001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_mhag001_t = g_mhag_m.mhag001
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN amhm320_cl USING g_enterprise,g_mhag_m.mhag001
   IF SQLCA.SQLCODE THEN    #(ver:49)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN amhm320_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE
      LET g_errparam.popup = TRUE 
      CLOSE amhm320_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE amhm320_master_referesh USING g_mhag_m.mhag001 INTO g_mhag_m.mhagsite,g_mhag_m.mhag001,g_mhag_m.mhag002, 
       g_mhag_m.mhag003,g_mhag_m.mhag004,g_mhag_m.mhag005,g_mhag_m.mhag006,g_mhag_m.mhagunit,g_mhag_m.mhag007, 
       g_mhag_m.mhag008,g_mhag_m.mhag009,g_mhag_m.mhagstus,g_mhag_m.mhagownid,g_mhag_m.mhagowndp,g_mhag_m.mhagcrtid, 
       g_mhag_m.mhagcrtdp,g_mhag_m.mhagcrtdt,g_mhag_m.mhagmodid,g_mhag_m.mhagmoddt,g_mhag_m.mhagcnfid, 
       g_mhag_m.mhagcnfdt,g_mhag_m.mhagsite_desc,g_mhag_m.mhag001_desc,g_mhag_m.mhag004_desc,g_mhag_m.mhag008_desc, 
       g_mhag_m.mhagownid_desc,g_mhag_m.mhagowndp_desc,g_mhag_m.mhagcrtid_desc,g_mhag_m.mhagcrtdp_desc, 
       g_mhag_m.mhagmodid_desc,g_mhag_m.mhagcnfid_desc
 
   #檢查是否允許此動作
   IF NOT amhm320_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_mhag_m_mask_o.* =  g_mhag_m.*
   CALL amhm320_mhag_t_mask()
   LET g_mhag_m_mask_n.* =  g_mhag_m.*
   
   
 
   #顯示資料
   CALL amhm320_show()
   
   WHILE TRUE
      LET g_mhag_m.mhag001 = g_mhag001_t
 
      
      #寫入修改者/修改日期資訊
      LET g_mhag_m.mhagmodid = g_user 
LET g_mhag_m.mhagmoddt = cl_get_current()
LET g_mhag_m.mhagmodid_desc = cl_get_username(g_mhag_m.mhagmodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
      #end add-point
 
      #資料輸入
      CALL amhm320_input("u")     
 
      #add-point:modify段修改後 name="modify.after_input"
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         LET g_mhag_m.* = g_mhag_m_t.*
         CALL amhm320_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE mhag_t SET (mhagmodid,mhagmoddt) = (g_mhag_m.mhagmodid,g_mhag_m.mhagmoddt)
       WHERE mhagent = g_enterprise AND mhag001 = g_mhag001_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL amhm320_set_act_visible()
   CALL amhm320_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " mhagent = " ||g_enterprise|| " AND",
                      " mhag001 = '", g_mhag_m.mhag001, "' "
 
   #填到對應位置
   CALL amhm320_browser_fill(g_wc,"")
 
   CLOSE amhm320_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL amhm320_msgcentre_notify('modify')
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
