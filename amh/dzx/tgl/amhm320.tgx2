               DISPLAY amhm320_filter_parser('mhagsite') TO s_browse[1].b_mhagsite
            DISPLAY amhm320_filter_parser('mhag001') TO s_browse[1].b_mhag001
            DISPLAY amhm320_filter_parser('mhag002') TO s_browse[1].b_mhag002
            DISPLAY amhm320_filter_parser('mhag003') TO s_browse[1].b_mhag003
            DISPLAY amhm320_filter_parser('mhag004') TO s_browse[1].b_mhag004
            DISPLAY amhm320_filter_parser('mhag005') TO s_browse[1].b_mhag005
            DISPLAY amhm320_filter_parser('mhag006') TO s_browse[1].b_mhag006
            DISPLAY amhm320_filter_parser('mhag007') TO s_browse[1].b_mhag007
            DISPLAY amhm320_filter_parser('mhag008') TO s_browse[1].b_mhag008
      
