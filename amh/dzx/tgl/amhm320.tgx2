   {<point name="main.define"/>}
   #end add-point   
   
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("amh","")
 
   #add-point:作業初始化 name="main.init"
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define name="main.define_sql"
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT mhagsite,'',mhag001,'',mhag002,'','','','',mhag003,'','','','','','', 
       '','',mhag004,'',mhag005,mhag006,mhagunit,mhag007,mhag008,'',mhag009,mhagstus,mhagownid,'',mhagowndp, 
       '',mhagcrtid,'',mhagcrtdp,'',mhagcrtdt,mhagmodid,'',mhagmoddt,mhagcnfid,'',mhagcnfdt", 
                      " FROM mhag_t",
                      " WHERE mhagent= ? AND mhag001=? FOR UPDATE"
   #add-point:SQL_define name="main.after_define_sql"
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE amhm320_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.mhagsite,t0.mhag001,t0.mhag002,t0.mhag003,t0.mhag004,t0.mhag005, 
       t0.mhag006,t0.mhagunit,t0.mhag007,t0.mhag008,t0.mhag009,t0.mhagstus,t0.mhagownid,t0.mhagowndp, 
       t0.mhagcrtid,t0.mhagcrtdp,t0.mhagcrtdt,t0.mhagmodid,t0.mhagmoddt,t0.mhagcnfid,t0.mhagcnfdt,t1.ooefl003 , 
       t2.mhaf002 ,t6.oocql004 ,t7.oocql004 ,t8.ooag011 ,t9.ooefl003 ,t10.ooag011 ,t11.ooefl003 ,t12.ooag011 , 
       t13.ooag011",
               " FROM mhag_t t0",
                              " LEFT JOIN ooefl_t t1 ON t1.ooeflent="||g_enterprise||" AND t1.ooefl001=t0.mhagsite AND t1.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN mhaf_t t2 ON t2.mhafent="||g_enterprise||" AND t2.mhaf001=t0.mhag001  ",
               " LEFT JOIN oocql_t t6 ON t6.oocqlent="||g_enterprise||" AND t6.oocql001='2128' AND t6.oocql002=t0.mhag004 AND t6.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t7 ON t7.oocqlent="||g_enterprise||" AND t7.oocql001='2129' AND t7.oocql002=t0.mhag008 AND t7.oocql003='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t8 ON t8.ooagent="||g_enterprise||" AND t8.ooag001=t0.mhagownid  ",
               " LEFT JOIN ooefl_t t9 ON t9.ooeflent="||g_enterprise||" AND t9.ooefl001=t0.mhagowndp AND t9.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t10 ON t10.ooagent="||g_enterprise||" AND t10.ooag001=t0.mhagcrtid  ",
               " LEFT JOIN ooefl_t t11 ON t11.ooeflent="||g_enterprise||" AND t11.ooefl001=t0.mhagcrtdp AND t11.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t12 ON t12.ooagent="||g_enterprise||" AND t12.ooag001=t0.mhagmodid  ",
               " LEFT JOIN ooag_t t13 ON t13.ooagent="||g_enterprise||" AND t13.ooag001=t0.mhagcnfid  ",
 
               " WHERE t0.mhagent = " ||g_enterprise|| " AND t0.mhag001 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define name="main.after_refresh_sql"
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE amhm320_master_referesh FROM g_sql
 
    
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call name="main.servicecall"
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_amhm320 WITH FORM cl_ap_formpath("amh",g_code)
   
      #瀏覽頁簽資料初始化
