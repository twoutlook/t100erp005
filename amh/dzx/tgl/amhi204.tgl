      END IF
    
      #CALL g_mhad_d.clear()
      #CALL g_mhad2_d.clear()
 
      
      #add-point:單頭輸入後2 name="insert.after_insert2"
      {<point name="insert.after_insert2"/>}
      #end add-point
 
