          WHERE mhbcent = g_enterprise AND mhbcdocno = g_mhbcdocno_t
 
      END IF
    
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         #若單頭無commit則還原
         IF g_master_commit = "N" THEN
            LET g_mhbc_m.* = g_mhbc_m_t.*
