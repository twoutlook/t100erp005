       g_mhbc_m.mhbcownid_desc,g_mhbc_m.mhbcowndp_desc,g_mhbc_m.mhbccrtid_desc,g_mhbc_m.mhbccrtdp_desc, 
       g_mhbc_m.mhbcmodid_desc,g_mhbc_m.mhbccnfid_desc
   
