          g_mhbc_m.mhbc011,g_mhbc_m.mhbc012,g_mhbc_m.mhbc013,g_mhbc_m.mhbc013_desc,g_mhbc_m.mhbc014, 
          g_mhbc_m.mhbc014_desc,g_mhbc_m.mhbc015,g_mhbc_m.mhbcunit,g_mhbc_m.mhbcstus,g_mhbc_m.mhbcownid, 
