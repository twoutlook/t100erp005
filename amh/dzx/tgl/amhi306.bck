{<section id="amhi306.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION amhi306_b_fill(p_wc2)              #BODY FILL UP
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   {<point name="b_fill.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE p_wc2            STRING
   DEFINE ls_owndept_list  STRING  #(ver:35) add
   DEFINE ls_ownuser_list  STRING  #(ver:35) add
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   {<point name="b_fill.define"/>}
   #end add-point
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   {<point name="b_fill.pre_function"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前 name="b_fill.sql_before"
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  DISTINCT t0.mhanstus,t0.mhan001,t0.mhan002,t0.mhan003,t0.mhan004,t0.mhan005, 
       t0.mhan006,t0.mhan007,t0.mhan008,t0.mhan009,t0.mhan001,t0.mhanownid,t0.mhanowndp,t0.mhancrtid, 
       t0.mhancrtdp,t0.mhancrtdt,t0.mhanmodid,t0.mhanmoddt,t0.mhancnfid,t0.mhancnfdt ,t1.ooefl003 ,t2.mhael023 , 
       t3.ooefl003 ,t4.rtaxl003 ,t5.ooag011 ,t6.ooefl003 ,t7.ooag011 ,t8.ooefl003 ,t9.ooag011 ,t10.ooag011 FROM mhan_t t0", 

               "",
                              " LEFT JOIN ooefl_t t1 ON t1.ooeflent="||g_enterprise||" AND t1.ooefl001=t0.mhan003 AND t1.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN mhael_t t2 ON t2.mhaelent="||g_enterprise||" AND t2.mhael001=t0.mhan004 AND t2.mhael022='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent="||g_enterprise||" AND t3.ooefl001=t0.mhan005 AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN rtaxl_t t4 ON t4.rtaxlent="||g_enterprise||" AND t4.rtaxl001=t0.mhan007 AND t4.rtaxl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=t0.mhanownid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent="||g_enterprise||" AND t6.ooefl001=t0.mhanowndp AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent="||g_enterprise||" AND t7.ooag001=t0.mhancrtid  ",
               " LEFT JOIN ooefl_t t8 ON t8.ooeflent="||g_enterprise||" AND t8.ooefl001=t0.mhancrtdp AND t8.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t9 ON t9.ooagent="||g_enterprise||" AND t9.ooag001=t0.mhanmodid  ",
               " LEFT JOIN ooag_t t10 ON t10.ooagent="||g_enterprise||" AND t10.ooag001=t0.mhancnfid  ",
 
               " WHERE t0.mhanent= ?  AND  1=1 AND (", p_wc2, ") "
 
   #(ver:35) ---add start---
      #應用 a68 樣板自動產生(Version:1)
   #若是修改，須視權限加上條件
   IF g_action_choice = "modify" OR g_action_choice = "modify_detail" THEN
      LET ls_owndept_list = NULL
      LET ls_ownuser_list = NULL
 
      #若有設定部門權限
      CALL cl_get_owndept_list("mhan_t","modify") RETURNING ls_owndept_list
      IF NOT cl_null(ls_owndept_list) THEN
         LET g_sql = g_sql, " AND mhanowndp IN (",ls_owndept_list,")"
      END IF
 
      #若有設定個人權限
      CALL cl_get_ownuser_list("mhan_t","modify") RETURNING ls_ownuser_list
      IF NOT cl_null(ls_ownuser_list) THEN
         LET g_sql = g_sql," AND mhanownid IN (",ls_ownuser_list,")"
      END IF
   END IF
 
 
 
   #(ver:35) --- add end ---
 
   #add-point:b_fill段sql wc name="b_fill.sql_wc"
   {<point name="b_fill.sql_wc"/>}
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("mhan_t"),
                      " ORDER BY t0.mhan001"
   
   #add-point:b_fill段sql之後 name="b_fill.sql_after"
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"mhan_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE amhi306_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR amhi306_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_mhan_d.clear()
   CALL g_mhan2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_mhan_d[l_ac].mhanstus,g_mhan_d[l_ac].mhan001,g_mhan_d[l_ac].mhan002,g_mhan_d[l_ac].mhan003, 
       g_mhan_d[l_ac].mhan004,g_mhan_d[l_ac].mhan005,g_mhan_d[l_ac].mhan006,g_mhan_d[l_ac].mhan007,g_mhan_d[l_ac].mhan008, 
       g_mhan_d[l_ac].mhan009,g_mhan2_d[l_ac].mhan001,g_mhan2_d[l_ac].mhanownid,g_mhan2_d[l_ac].mhanowndp, 
       g_mhan2_d[l_ac].mhancrtid,g_mhan2_d[l_ac].mhancrtdp,g_mhan2_d[l_ac].mhancrtdt,g_mhan2_d[l_ac].mhanmodid, 
       g_mhan2_d[l_ac].mhanmoddt,g_mhan2_d[l_ac].mhancnfid,g_mhan2_d[l_ac].mhancnfdt,g_mhan_d[l_ac].mhan003_desc, 
       g_mhan_d[l_ac].mhan004_desc,g_mhan_d[l_ac].mhan005_desc,g_mhan_d[l_ac].mhan007_desc,g_mhan2_d[l_ac].mhanownid_desc, 
       g_mhan2_d[l_ac].mhanowndp_desc,g_mhan2_d[l_ac].mhancrtid_desc,g_mhan2_d[l_ac].mhancrtdp_desc, 
       g_mhan2_d[l_ac].mhanmodid_desc,g_mhan2_d[l_ac].mhancnfid_desc
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH b_fill_curs:",SQLERRMESSAGE 
         LET g_errparam.code = SQLCA.SQLCODE
         LET g_errparam.popup = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充 name="b_fill.fill"
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL amhi306_detail_show()      
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
 
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   LET g_error_show = 0
   
 
   
   CALL g_mhan_d.deleteElement(g_mhan_d.getLength())   
   CALL g_mhan2_d.deleteElement(g_mhan2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_mhan_d.getLength()
      LET g_mhan2_d[l_ac].mhan001 = g_mhan_d[l_ac].mhan001 
 
      #add-point:b_fill段key值相關欄位 name="b_fill.keys.fill"
      {<point name="b_fill.keys.fill"/>}
      #end add-point
   END FOR
   
   IF g_cnt > g_mhan_d.getLength() THEN
      LET l_ac = g_mhan_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_mhan_d.getLength()
      LET g_mhan_d_mask_o[l_ac].* =  g_mhan_d[l_ac].*
      CALL amhi306_mhan_t_mask()
      LET g_mhan_d_mask_n[l_ac].* =  g_mhan_d[l_ac].*
   END FOR
   
   LET g_mhan2_d_mask_o.* =  g_mhan2_d.*
   FOR l_ac = 1 TO g_mhan2_d.getLength()
      LET g_mhan2_d_mask_o[l_ac].* =  g_mhan2_d[l_ac].*
      CALL amhi306_mhan_t_mask()
      LET g_mhan2_d_mask_n[l_ac].* =  g_mhan2_d[l_ac].*
   END FOR
 
   
   LET l_ac = g_cnt
 
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   {<point name="b_fill.others.fill"/>}
