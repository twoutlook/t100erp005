               LET g_errparam.extend = g_mhan_d[l_ac].mhan001 
               LET g_errparam.code   = -263 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               LET g_mhan_d[l_ac].* = g_mhan_d_t.*
            ELSE
               #寫入修改者/修改日期資訊(單身)
               LET g_mhan2_d[l_ac].mhanmodid = g_user 
LET g_mhan2_d[l_ac].mhanmoddt = cl_get_current()
LET g_mhan2_d[l_ac].mhanmodid_desc = cl_get_username(g_mhan2_d[l_ac].mhanmodid)
            
               #add-point:單身修改前 name="input.body.b_update"
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               #將遮罩欄位還原
               CALL amhi306_mhan_t_mask_restore('restore_mask_o')
 
               UPDATE mhan_t SET (mhanstus,mhan001,mhan002,mhan003,mhan004,mhan005,mhan006,mhan007,mhan008, 
                   mhan009,mhanownid,mhanowndp,mhancrtid,mhancrtdp,mhancrtdt,mhanmodid,mhanmoddt,mhancnfid, 
                   mhancnfdt) = (g_mhan_d[l_ac].mhanstus,g_mhan_d[l_ac].mhan001,g_mhan_d[l_ac].mhan002, 
                   g_mhan_d[l_ac].mhan003,g_mhan_d[l_ac].mhan004,g_mhan_d[l_ac].mhan005,g_mhan_d[l_ac].mhan006, 
                   g_mhan_d[l_ac].mhan007,g_mhan_d[l_ac].mhan008,g_mhan_d[l_ac].mhan009,g_mhan2_d[l_ac].mhanownid, 
                   g_mhan2_d[l_ac].mhanowndp,g_mhan2_d[l_ac].mhancrtid,g_mhan2_d[l_ac].mhancrtdp,g_mhan2_d[l_ac].mhancrtdt, 
                   g_mhan2_d[l_ac].mhanmodid,g_mhan2_d[l_ac].mhanmoddt,g_mhan2_d[l_ac].mhancnfid,g_mhan2_d[l_ac].mhancnfdt) 

                WHERE mhanent = g_enterprise AND
                  mhan001 = g_mhan_d_t.mhan001 #項次   
 
                  
               #add-point:單身修改中 name="input.body.m_update"
               {<point name="input.body.m_update"/>}
               #end add-point
                  
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL s_transaction_end('N','0')
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "mhan_t" 
                     LET g_errparam.code = "std-00009" 
                     LET g_errparam.popup = TRUE 
                     CALL cl_err()
                  WHEN SQLCA.SQLCODE #其他錯誤
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "mhan_t:",SQLERRMESSAGE 
                     LET g_errparam.code = SQLCA.SQLCODE
                     LET g_errparam.popup = TRUE 
                     CALL s_transaction_end('N','0')
                     CALL cl_err()
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_mhan_d[g_detail_idx].mhan001
               LET gs_keys_bak[1] = g_mhan_d_t.mhan001
               CALL amhi306_update_b('mhan_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
                     #修改歷程記錄(修改)
                     LET g_log1 = util.JSON.stringify(g_mhan_d_t)
                     LET g_log2 = util.JSON.stringify(g_mhan_d[l_ac])
                     IF NOT cl_log_modified_record(g_log1,g_log2) THEN
                        CALL s_transaction_end('N','0')
                     END IF
               END CASE
               
               #將遮罩欄位進行遮蔽
               CALL amhi306_mhan_t_mask_restore('restore_mask_n')
               
               #add-point:單身修改後 name="input.body.a_update"
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            CALL amhi306_unlock_b("mhan_t")
            IF INT_FLAG THEN
               CALL s_transaction_end('N','0')
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               IF l_cmd = 'u' THEN
                  LET g_mhan_d[l_ac].* = g_mhan_d_t.*
               END IF
               #add-point:單身after row name="input.body.a_close"
               {<point name="input.body.a_close"/>}
               #end add-point
            ELSE
               CALL s_transaction_end('Y','0')
            END IF
            #其他table進行unlock
            
             #add-point:單身after row name="input.body.a_row"
            {<point name="input.body.a_row"/>}
            #end add-point
            
         AFTER INPUT
            #add-point:單身input後 name="input.body.a_input"
            {<point name="input.body.a_input"/>}
            #end add-point
            #錯誤訊息統整顯示
            #CALL cl_err_collect_show()
            #CALL cl_showmsg()
            
         ON ACTION controlo   
            IF l_insert THEN
               LET li_reproduce = l_ac_t
               LET li_reproduce_target = l_ac
               LET g_mhan_d[li_reproduce_target].* = g_mhan_d[li_reproduce].*
               LET g_mhan2_d[li_reproduce_target].* = g_mhan2_d[li_reproduce].*
 
               LET g_mhan_d[li_reproduce_target].mhan001 = NULL
 
            ELSE
               CALL FGL_SET_ARR_CURR(g_mhan_d.getLength()+1)
               LET lb_reproduce = TRUE
               LET li_reproduce = l_ac
               LET li_reproduce_target = g_mhan_d.getLength()+1
            END IF
            
      END INPUT
      
 
      
      DISPLAY ARRAY g_mhan2_d TO s_detail2.*
         ATTRIBUTES(COUNT=g_detail_cnt)  
      
         BEFORE DISPLAY 
            CALL amhi306_b_fill(g_wc2)
            CALL FGL_SET_ARR_CURR(g_detail_idx)
      
         BEFORE ROW
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail2")
            LET l_ac = g_detail_idx
            LET g_temp_idx = l_ac
            DISPLAY g_detail_idx TO FORMONLY.idx
            CALL cl_show_fld_cont() 
            
         #add-point:page2自定義行為 name="input.body2.action"
         {<point name="input.body2.action"/>}
         #end add-point
            
      END DISPLAY
 
      
      #add-point:before_more_input name="input.more_input"
      {<point name="input.more_input"/>}
      #end add-point
      
      BEFORE DIALOG
         #CALL cl_err_collect_init()      
         IF g_temp_idx > 0 THEN
            LET l_ac = g_temp_idx
            CALL DIALOG.setCurrentRow("s_detail1",l_ac)
            LET g_temp_idx = 1
         END IF
         #LET g_curr_diag = ui.DIALOG.getCurrent()
         #add-point:before_dialog name="input.before_dialog"
         {<point name="input.before_dialog"/>}
         #end add-point
         CASE g_aw
            WHEN "s_detail1"
               NEXT FIELD mhanstus
            WHEN "s_detail2"
               NEXT FIELD mhan001_2
 
         END CASE
   
      ON ACTION accept
         ACCEPT DIALOG
      
      ON ACTION cancel
         LET INT_FLAG = TRUE 
         CANCEL DIALOG
 
      ON ACTION controlr
         CALL cl_show_req_fields()
 
      ON ACTION controlf
         CALL cl_set_focus_form(ui.Interface.getRootNode()) 
              RETURNING g_fld_name,g_frm_name 
         CALL cl_fldhelp(g_frm_name,g_fld_name,g_lang) 
           
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE DIALOG
   
   END DIALOG 
    
   #新增後取消
   IF l_cmd = 'a' THEN
      #當取消或無輸入資料按確定時刪除對應資料
      IF INT_FLAG OR cl_null(g_mhan_d[g_detail_idx].mhan001) THEN
         CALL g_mhan_d.deleteElement(g_detail_idx)
         CALL g_mhan2_d.deleteElement(g_detail_idx)
 
      END IF
   END IF
   
   #修改後取消
   IF l_cmd = 'u' AND INT_FLAG THEN
      LET g_mhan_d[g_detail_idx].* = g_mhan_d_t.*
   END IF
   
   #add-point:modify段修改後 name="modify.after_input"
   {<point name="modify.after_input"/>}
   #end add-point
 
   CLOSE amhi306_bcl
   CALL s_transaction_end('Y','0')
   
END FUNCTION

