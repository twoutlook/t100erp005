   #應用 a09 樣板自動產生(Version:17)
#+ 確認碼變更 
PRIVATE FUNCTION amhm345_statechange()
   #add-point:statechange段define(客製用) name="statechange.define_customerization"
   {<point name="statechange.define_customerization" edit="c" mark="Y"/>}
   #end add-point  
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="statechange.define"
   {<point name="statechange.define"/>}
   #end add-point  
   
   #add-point:Function前置處理 name="statechange.before"
   {<point name="statechange.before"/>}
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_mhao_m.mhaosite IS NULL
      OR g_mhao_m.mhao001 IS NULL      OR g_mhao_m.mhao002 IS NULL
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN amhm345_cl USING g_enterprise,g_mhao_m.mhaosite,g_mhao_m.mhao001,g_mhao_m.mhao002
   IF STATUS THEN
      CLOSE amhm345_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN amhm345_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE amhm345_master_referesh USING g_mhao_m.mhaosite,g_mhao_m.mhao001,g_mhao_m.mhao002 INTO g_mhao_m.mhaosite, 
       g_mhao_m.mhao001,g_mhao_m.mhao002,g_mhao_m.mhao003,g_mhao_m.mhao004,g_mhao_m.mhao005,g_mhao_m.mhao006, 
       g_mhao_m.mhao007,g_mhao_m.mhao008,g_mhao_m.mhao009,g_mhao_m.mhao010,g_mhao_m.mhao011,g_mhao_m.mhao012, 
       g_mhao_m.mhaostus,g_mhao_m.mhaoownid,g_mhao_m.mhaoowndp,g_mhao_m.mhaocrtid,g_mhao_m.mhaocrtdp, 
       g_mhao_m.mhaocrtdt,g_mhao_m.mhaomodid,g_mhao_m.mhaomoddt,g_mhao_m.mhaocnfid,g_mhao_m.mhaocnfdt, 
       g_mhao_m.mhaosite_desc,g_mhao_m.mhao003_desc,g_mhao_m.mhao005_desc,g_mhao_m.mhao006_desc,g_mhao_m.mhao009_desc, 
       g_mhao_m.mhaoownid_desc,g_mhao_m.mhaoowndp_desc,g_mhao_m.mhaocrtid_desc,g_mhao_m.mhaocrtdp_desc, 
       g_mhao_m.mhaomodid_desc,g_mhao_m.mhaocnfid_desc
   
 
   #檢查是否允許此動作
   IF NOT amhm345_action_chk() THEN
      CLOSE amhm345_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_mhao_m.mhaosite,g_mhao_m.mhaosite_desc,g_mhao_m.mhao001,g_mhao_m.mhao002,g_mhao_m.mhao003, 
       g_mhao_m.mhao003_desc,g_mhao_m.mhao004,g_mhao_m.mhao005,g_mhao_m.mhao005_desc,g_mhao_m.mhao006, 
       g_mhao_m.mhao006_desc,g_mhao_m.mhao007,g_mhao_m.mhao008,g_mhao_m.mhao009,g_mhao_m.mhao009_desc, 
       g_mhao_m.mhao010,g_mhao_m.mhao011,g_mhao_m.mhao012,g_mhao_m.mhaostus,g_mhao_m.mhaoownid,g_mhao_m.mhaoownid_desc, 
       g_mhao_m.mhaoowndp,g_mhao_m.mhaoowndp_desc,g_mhao_m.mhaocrtid,g_mhao_m.mhaocrtid_desc,g_mhao_m.mhaocrtdp, 
       g_mhao_m.mhaocrtdp_desc,g_mhao_m.mhaocrtdt,g_mhao_m.mhaomodid,g_mhao_m.mhaomodid_desc,g_mhao_m.mhaomoddt, 
       g_mhao_m.mhaocnfid,g_mhao_m.mhaocnfid_desc,g_mhao_m.mhaocnfdt
 
   CASE g_mhao_m.mhaostus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/open.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/valid.png")
      WHEN "X"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/void.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   {<point name="statechange.after_refresh"/>}
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_mhao_m.mhaostus
            
            WHEN "N"
               HIDE OPTION "open"
            WHEN "Y"
               HIDE OPTION "valid"
            WHEN "X"
               HIDE OPTION "void"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      {<point name="statechange.before_menu"/>}
      #end add-point
      
      
	  
      ON ACTION open
         IF cl_auth_chk_act("open") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.open"
            {<point name="statechange.open"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION valid
         IF cl_auth_chk_act("valid") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.valid"
            {<point name="statechange.valid"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION void
         IF cl_auth_chk_act("void") THEN
            LET lc_state = "X"
            #add-point:action控制 name="statechange.void"
            {<point name="statechange.void"/>}
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      {<point name="statechange.more_control"/>}
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      AND lc_state <> "X"
      ) OR 
      g_mhao_m.mhaostus = lc_state OR cl_null(lc_state) THEN
      CLOSE amhm345_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   {<point name="statechange.b_update"/>}
   #end add-point
   
   LET g_mhao_m.mhaomodid = g_user
   LET g_mhao_m.mhaomoddt = cl_get_current()
   LET g_mhao_m.mhaostus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE mhao_t 
      SET (mhaostus,mhaomodid,mhaomoddt) 
        = (g_mhao_m.mhaostus,g_mhao_m.mhaomodid,g_mhao_m.mhaomoddt)     
    WHERE mhaoent = g_enterprise AND mhaosite = g_mhao_m.mhaosite
      AND mhao001 = g_mhao_m.mhao001      AND mhao002 = g_mhao_m.mhao002
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/open.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/valid.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/void.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE amhm345_master_referesh USING g_mhao_m.mhaosite,g_mhao_m.mhao001,g_mhao_m.mhao002 INTO g_mhao_m.mhaosite, 
          g_mhao_m.mhao001,g_mhao_m.mhao002,g_mhao_m.mhao003,g_mhao_m.mhao004,g_mhao_m.mhao005,g_mhao_m.mhao006, 
          g_mhao_m.mhao007,g_mhao_m.mhao008,g_mhao_m.mhao009,g_mhao_m.mhao010,g_mhao_m.mhao011,g_mhao_m.mhao012, 
          g_mhao_m.mhaostus,g_mhao_m.mhaoownid,g_mhao_m.mhaoowndp,g_mhao_m.mhaocrtid,g_mhao_m.mhaocrtdp, 
          g_mhao_m.mhaocrtdt,g_mhao_m.mhaomodid,g_mhao_m.mhaomoddt,g_mhao_m.mhaocnfid,g_mhao_m.mhaocnfdt, 
          g_mhao_m.mhaosite_desc,g_mhao_m.mhao003_desc,g_mhao_m.mhao005_desc,g_mhao_m.mhao006_desc,g_mhao_m.mhao009_desc, 
          g_mhao_m.mhaoownid_desc,g_mhao_m.mhaoowndp_desc,g_mhao_m.mhaocrtid_desc,g_mhao_m.mhaocrtdp_desc, 
          g_mhao_m.mhaomodid_desc,g_mhao_m.mhaocnfid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_mhao_m.mhaosite,g_mhao_m.mhaosite_desc,g_mhao_m.mhao001,g_mhao_m.mhao002,g_mhao_m.mhao003, 
          g_mhao_m.mhao003_desc,g_mhao_m.mhao004,g_mhao_m.mhao005,g_mhao_m.mhao005_desc,g_mhao_m.mhao006, 
          g_mhao_m.mhao006_desc,g_mhao_m.mhao007,g_mhao_m.mhao008,g_mhao_m.mhao009,g_mhao_m.mhao009_desc, 
          g_mhao_m.mhao010,g_mhao_m.mhao011,g_mhao_m.mhao012,g_mhao_m.mhaostus,g_mhao_m.mhaoownid,g_mhao_m.mhaoownid_desc, 
          g_mhao_m.mhaoowndp,g_mhao_m.mhaoowndp_desc,g_mhao_m.mhaocrtid,g_mhao_m.mhaocrtid_desc,g_mhao_m.mhaocrtdp, 
          g_mhao_m.mhaocrtdp_desc,g_mhao_m.mhaocrtdt,g_mhao_m.mhaomodid,g_mhao_m.mhaomodid_desc,g_mhao_m.mhaomoddt, 
          g_mhao_m.mhaocnfid,g_mhao_m.mhaocnfid_desc,g_mhao_m.mhaocnfdt
   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
   {<point name="statechange.a_update"/>}
   #end add-point
 
   #add-point:statechange段結束前 name="statechange.after"
   {<point name="statechange.after"/>}
   #end add-point  
 
   CLOSE amhm345_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL amhm345_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION
 
 
 

{</section>}

