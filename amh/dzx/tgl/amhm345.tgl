{<section id="amhm345.delete" >}
#+ 資料刪除 
PRIVATE FUNCTION amhm345_delete()
   #add-point:delete段define(客製用) name="delete.define_customerization"
   {<point name="delete.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="delete.define"
   {<point name="delete.define"/>}
   #end add-point  
   
   #add-point:Function前置處理  name="delete.pre_function"
   {<point name="delete.pre_function"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_mhao_m.mhaosite IS NULL
   OR g_mhao_m.mhao001 IS NULL
   OR g_mhao_m.mhao002 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
    
   LET g_mhaosite_t = g_mhao_m.mhaosite
   LET g_mhao001_t = g_mhao_m.mhao001
   LET g_mhao002_t = g_mhao_m.mhao002
 
   
   
 
   OPEN amhm345_cl USING g_enterprise,g_mhao_m.mhaosite,g_mhao_m.mhao001,g_mhao_m.mhao002
   IF SQLCA.SQLCODE THEN    #(ver:49)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN amhm345_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE
      LET g_errparam.popup = TRUE 
      CLOSE amhm345_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE amhm345_master_referesh USING g_mhao_m.mhaosite,g_mhao_m.mhao001,g_mhao_m.mhao002 INTO g_mhao_m.mhaosite, 
       g_mhao_m.mhao001,g_mhao_m.mhao002,g_mhao_m.mhao003,g_mhao_m.mhao004,g_mhao_m.mhao005,g_mhao_m.mhao006, 
       g_mhao_m.mhao007,g_mhao_m.mhao008,g_mhao_m.mhao009,g_mhao_m.mhao010,g_mhao_m.mhao011,g_mhao_m.mhao012, 
       g_mhao_m.mhaostus,g_mhao_m.mhaoownid,g_mhao_m.mhaoowndp,g_mhao_m.mhaocrtid,g_mhao_m.mhaocrtdp, 
       g_mhao_m.mhaocrtdt,g_mhao_m.mhaomodid,g_mhao_m.mhaomoddt,g_mhao_m.mhaocnfid,g_mhao_m.mhaocnfdt, 
       g_mhao_m.mhaosite_desc,g_mhao_m.mhao003_desc,g_mhao_m.mhao005_desc,g_mhao_m.mhao006_desc,g_mhao_m.mhao009_desc, 
       g_mhao_m.mhaoownid_desc,g_mhao_m.mhaoowndp_desc,g_mhao_m.mhaocrtid_desc,g_mhao_m.mhaocrtdp_desc, 
       g_mhao_m.mhaomodid_desc,g_mhao_m.mhaocnfid_desc
   
   
   #檢查是否允許此動作
   IF NOT amhm345_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_mhao_m_mask_o.* =  g_mhao_m.*
   CALL amhm345_mhao_t_mask()
   LET g_mhao_m_mask_n.* =  g_mhao_m.*
   
   #將最新資料顯示到畫面上
   CALL amhm345_show()
   
   IF cl_ask_delete() THEN
 
      #add-point:單頭刪除前 name="delete.head.b_delete"
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point
 
      #應用 a47 樣板自動產生(Version:4)
      #刪除相關文件
      CALL amhm345_set_pk_array()
      #add-point:相關文件刪除前 name="delete.befroe.related_document_remove"
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
 
 
      DELETE FROM mhao_t 
       WHERE mhaoent = g_enterprise AND mhaosite = g_mhao_m.mhaosite 
         AND mhao001 = g_mhao_m.mhao001 
         AND mhao002 = g_mhao_m.mhao002 
 
 
      #add-point:單頭刪除中 name="delete.head.m_delete"
      {<point name="delete.head.m_delete"/>}
      #end add-point
         
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "mhao_t:",SQLERRMESSAGE 
         LET g_errparam.code = SQLCA.SQLCODE
         LET g_errparam.popup = FALSE 
         CALL s_transaction_end('N','0')
         CALL cl_err()
      END IF
  
      
      
      #add-point:單頭刪除後 name="delete.head.a_delete"
      {<point name="delete.head.a_delete"/>}
      #end add-point
      
       
 
      #修改歷程記錄(刪除)
      LET g_log1 = util.JSON.stringify(g_mhao_m)   #(ver:49)
      IF NOT cl_log_modified_record(g_log1,'') THEN    #(ver:49)
         CLOSE amhm345_cl
         CALL s_transaction_end('N','0')
         RETURN
      END IF
      
      CLEAR FORM
      CALL amhm345_ui_browser_refresh()
      
      #確保畫面上保有資料
      IF g_browser_cnt > 0 THEN
         #CALL amhm345_browser_fill(g_wc,"")
         CALL amhm345_fetch("P")
      ELSE
         CLEAR FORM
      END IF
      CALL s_transaction_end('Y','0')
   ELSE    
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE amhm345_cl
 
   #功能已完成,通報訊息中心
   CALL amhm345_msgcentre_notify('delete')
 
   #add-point:單頭刪除完成後 name="delete.a_delete"
   {<point name="delete.a_delete"/>}
   #end add-point
 
END FUNCTION

