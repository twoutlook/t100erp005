   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE amhm345_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.mhaosite,t0.mhao001,t0.mhao002,t0.mhao003,t0.mhao004,t0.mhao005, 
       t0.mhao006,t0.mhao007,t0.mhao008,t0.mhao009,t0.mhao010,t0.mhao011,t0.mhao012,t0.mhaostus,t0.mhaoownid, 
       t0.mhaoowndp,t0.mhaocrtid,t0.mhaocrtdp,t0.mhaocrtdt,t0.mhaomodid,t0.mhaomoddt,t0.mhaocnfid,t0.mhaocnfdt, 
       t1.ooefl003 ,t2.ooefl003 ,t3.mhael023 ,t4.pmaal004 ,t5.stael003 ,t6.ooag011 ,t7.ooefl003 ,t8.ooag011 , 
