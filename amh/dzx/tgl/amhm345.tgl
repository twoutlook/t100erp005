{<section id="amhm345.modify" >}
#+ 資料修改
PRIVATE FUNCTION amhm345_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   {<point name="modify.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   {<point name="modify.define"/>}
   #end add-point
   
   #add-point:Function前置處理 name="modify.pre_function"
   {<point name="modify.pre_function"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_mhao_m.mhaosite IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_mhaosite_t = g_mhao_m.mhaosite
   LET g_mhao001_t = g_mhao_m.mhao001
   LET g_mhao002_t = g_mhao_m.mhao002
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN amhm345_cl USING g_enterprise,g_mhao_m.mhaosite,g_mhao_m.mhao001,g_mhao_m.mhao002
   IF SQLCA.SQLCODE THEN    #(ver:49)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN amhm345_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE
      LET g_errparam.popup = TRUE 
      CLOSE amhm345_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE amhm345_master_referesh USING g_mhao_m.mhaosite,g_mhao_m.mhao001,g_mhao_m.mhao002 INTO g_mhao_m.mhaosite, 
       g_mhao_m.mhao001,g_mhao_m.mhao002,g_mhao_m.mhao003,g_mhao_m.mhao004,g_mhao_m.mhao005,g_mhao_m.mhao006, 
       g_mhao_m.mhao007,g_mhao_m.mhao008,g_mhao_m.mhao009,g_mhao_m.mhao010,g_mhao_m.mhao011,g_mhao_m.mhao012, 
       g_mhao_m.mhaostus,g_mhao_m.mhaoownid,g_mhao_m.mhaoowndp,g_mhao_m.mhaocrtid,g_mhao_m.mhaocrtdp, 
       g_mhao_m.mhaocrtdt,g_mhao_m.mhaomodid,g_mhao_m.mhaomoddt,g_mhao_m.mhaocnfid,g_mhao_m.mhaocnfdt, 
       g_mhao_m.mhaosite_desc,g_mhao_m.mhao003_desc,g_mhao_m.mhao005_desc,g_mhao_m.mhao006_desc,g_mhao_m.mhao009_desc, 
       g_mhao_m.mhaoownid_desc,g_mhao_m.mhaoowndp_desc,g_mhao_m.mhaocrtid_desc,g_mhao_m.mhaocrtdp_desc, 
       g_mhao_m.mhaomodid_desc,g_mhao_m.mhaocnfid_desc
 
   #檢查是否允許此動作
   IF NOT amhm345_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_mhao_m_mask_o.* =  g_mhao_m.*
   CALL amhm345_mhao_t_mask()
   LET g_mhao_m_mask_n.* =  g_mhao_m.*
   
   
 
   #顯示資料
   CALL amhm345_show()
   
   WHILE TRUE
      LET g_mhao_m.mhaosite = g_mhaosite_t
      LET g_mhao_m.mhao001 = g_mhao001_t
      LET g_mhao_m.mhao002 = g_mhao002_t
 
      
      #寫入修改者/修改日期資訊
      LET g_mhao_m.mhaomodid = g_user 
LET g_mhao_m.mhaomoddt = cl_get_current()
LET g_mhao_m.mhaomodid_desc = cl_get_username(g_mhao_m.mhaomodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
      #end add-point
 
      #資料輸入
      CALL amhm345_input("u")     
 
      #add-point:modify段修改後 name="modify.after_input"
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         LET g_mhao_m.* = g_mhao_m_t.*
         CALL amhm345_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE mhao_t SET (mhaomodid,mhaomoddt) = (g_mhao_m.mhaomodid,g_mhao_m.mhaomoddt)
       WHERE mhaoent = g_enterprise AND mhaosite = g_mhaosite_t
         AND mhao001 = g_mhao001_t
         AND mhao002 = g_mhao002_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL amhm345_set_act_visible()
   CALL amhm345_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " mhaoent = " ||g_enterprise|| " AND",
                      " mhaosite = '", g_mhao_m.mhaosite, "' "
                      ," AND mhao001 = '", g_mhao_m.mhao001, "' "
                      ," AND mhao002 = '", g_mhao_m.mhao002, "' "
 
   #填到對應位置
   CALL amhm345_browser_fill(g_wc,"")
 
   CLOSE amhm345_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL amhm345_msgcentre_notify('modify')
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   

