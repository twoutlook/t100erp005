{<section id="amhm345.fetch" >}
#+ 指定PK後抓取單頭其他資料
PRIVATE FUNCTION amhm345_fetch(p_fl)
   #add-point:fetch段define(客製用) name="fetch.define_customerization"
   {<point name="fetch.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE p_fl       LIKE type_t.chr1
   DEFINE ls_msg     STRING
   #add-point:fetch段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="fetch.define"
   {<point name="fetch.define"/>}
   #end add-point  
   
   #add-point:Function前置處理  name="fetch.pre_function"
   {<point name="fetch.pre_function"/>}
   #end add-point
   
   #根據傳入的條件決定抓取的資料
   CASE p_fl
      WHEN "F" 
         LET g_current_idx = 1
      WHEN "P"
         IF g_current_idx > 1 THEN               
            LET g_current_idx = g_current_idx - 1
         END IF 
      WHEN "N"
         IF g_current_idx < g_header_cnt THEN
            LET g_current_idx =  g_current_idx + 1
         END IF        
      WHEN "L" 
         #LET g_current_idx = g_header_cnt        
         LET g_current_idx = g_browser.getLength()    
      WHEN "/"
         #詢問要指定的筆數
         IF (NOT g_no_ask) THEN      
            CALL cl_getmsg("fetch", g_lang) RETURNING ls_msg
            LET INT_FLAG = 0
 
            PROMPT ls_msg CLIPPED,": " FOR g_jump
               #交談指令共用ACTION
               &include "common_action.4gl"
            END PROMPT
            
            IF INT_FLAG THEN
               LET INT_FLAG = 0
               EXIT CASE  
            END IF           
         END IF
         IF g_jump > 0 THEN
            LET g_current_idx = g_jump
         END IF
         LET g_no_ask = FALSE     
   END CASE
 
   #筆數顯示
   LET g_browser_idx = g_current_idx 
   IF g_browser_cnt > 0 THEN
      DISPLAY g_browser_idx TO FORMONLY.b_index #當下筆數
      DISPLAY g_browser_cnt TO FORMONLY.b_count #總筆數
      DISPLAY g_browser_idx TO FORMONLY.h_index #當下筆數
      DISPLAY g_browser_cnt TO FORMONLY.h_count #總筆數
   ELSE
      DISPLAY '' TO FORMONLY.b_index #當下筆數
      DISPLAY '' TO FORMONLY.b_count #總筆數
      DISPLAY '' TO FORMONLY.h_index #當下筆數
      DISPLAY '' TO FORMONLY.h_count #總筆數
   END IF
   
   
   
   #避免超出browser資料筆數上限
   IF g_current_idx > g_browser.getLength() THEN
      LET g_browser_idx = g_browser.getLength()
      LET g_current_idx = g_browser.getLength() 
   END IF
   
   # 設定browse索引
   CALL g_curr_diag.setCurrentRow("s_browse", g_current_idx)
   CALL cl_navigator_setting(g_browser_idx, g_browser_cnt) 
 
   #代表沒有資料, 無需做後續資料撈取之動作
   IF g_current_idx = 0 THEN
      RETURN
   END IF
 
   #根據選定的筆數給予key欄位值
   LET g_mhao_m.mhaosite = g_browser[g_current_idx].b_mhaosite
   LET g_mhao_m.mhao001 = g_browser[g_current_idx].b_mhao001
   LET g_mhao_m.mhao002 = g_browser[g_current_idx].b_mhao002
 
                       
   #讀取單頭所有欄位資料
   EXECUTE amhm345_master_referesh USING g_mhao_m.mhaosite,g_mhao_m.mhao001,g_mhao_m.mhao002 INTO g_mhao_m.mhaosite, 
       g_mhao_m.mhao001,g_mhao_m.mhao002,g_mhao_m.mhao003,g_mhao_m.mhao004,g_mhao_m.mhao005,g_mhao_m.mhao006, 
       g_mhao_m.mhao007,g_mhao_m.mhao008,g_mhao_m.mhao009,g_mhao_m.mhao010,g_mhao_m.mhao011,g_mhao_m.mhao012, 
       g_mhao_m.mhaostus,g_mhao_m.mhaoownid,g_mhao_m.mhaoowndp,g_mhao_m.mhaocrtid,g_mhao_m.mhaocrtdp, 
       g_mhao_m.mhaocrtdt,g_mhao_m.mhaomodid,g_mhao_m.mhaomoddt,g_mhao_m.mhaocnfid,g_mhao_m.mhaocnfdt, 
       g_mhao_m.mhaosite_desc,g_mhao_m.mhao003_desc,g_mhao_m.mhao005_desc,g_mhao_m.mhao006_desc,g_mhao_m.mhao009_desc, 
       g_mhao_m.mhaoownid_desc,g_mhao_m.mhaoowndp_desc,g_mhao_m.mhaocrtid_desc,g_mhao_m.mhaocrtdp_desc, 
       g_mhao_m.mhaomodid_desc,g_mhao_m.mhaocnfid_desc
   
   #遮罩相關處理
   LET g_mhao_m_mask_o.* =  g_mhao_m.*
   CALL amhm345_mhao_t_mask()
   LET g_mhao_m_mask_n.* =  g_mhao_m.*
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL amhm345_set_act_visible()
   CALL amhm345_set_act_no_visible()
 
   #add-point:fetch段action控制 name="fetch.action_control"
   {<point name="fetch.action_control"/>}
   #end add-point  
   
   
   
   #保存單頭舊值
   LET g_mhao_m_t.* = g_mhao_m.*
   LET g_mhao_m_o.* = g_mhao_m.*
   
   LET g_data_owner = g_mhao_m.mhaoownid      
   LET g_data_dept  = g_mhao_m.mhaoowndp
   
   #重新顯示
   CALL amhm345_show()
   
 
END FUNCTION

