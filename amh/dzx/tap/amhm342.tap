  <point name="insert.default" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[      LET g_site_flag = FALSE
      CALL s_aooi500_default(g_prog,'mhaksite',g_mhak_m.mhaksite)
         RETURNING l_insert,g_mhak_m.mhaksite
      IF NOT l_insert THEN
         RETURN
      END IF
#      LET g_mhak_m.mhakunit = g_site
      CALL amhm342_mhaksite_ref()
      LET g_mhak_m.mhakunit = g_site
      #抓取法人對應會計週期參照表
      LET l_ooef017 = NULL
      LET l_glaa003 = NULL
      SELECT ooef017 INTO l_ooef017
        FROM ooef_t
       WHERE ooefent = g_enterprise
         AND ooef001 = g_mhak_m.mhaksite
      SELECT glaa003 INTO l_glaa003
        FROM glaa_t
       WHERE glaaent = g_enterprise
         AND glaacomp = l_ooef017
         AND glaa014 = 'Y'
      CALL s_get_accdate(l_glaa003,g_today,'','')
                  RETURNING l_flag,l_errno,l_glav002,l_glav005,l_sdate_s,l_sdate_e,
                            l_glav006,l_pdate_s,l_pdate_e,l_glav007,l_wdate_s,l_wdate_e
       IF l_flag = 'Y' THEN
         LET l_str1 = l_glav002 CLIPPED
         LET l_str2 = l_glav006 CLIPPED USING '&&'
         LET l_str = l_str1,l_str2
         LET  g_mhak_m.mhak001 = l_str
#      ELSE
#         INITIALIZE g_errparam TO NULL
#         LET g_errparam.code = r_errno
#         LET g_errparam.popup = TRUE
#         CALL cl_err()
#         CONTINUE FOREACH
      END IF           ]]>
