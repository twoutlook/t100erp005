  <section id="amhm342.delete" ver="2" status="" src="s" readonly="">
    <![CDATA[#+ 資料刪除 
PRIVATE FUNCTION amhm342_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define" edit="s"/>}
   #end add-point  
   #add-point:delete段define(客製用)
   {<point name="delete.define_customerization" edit="c"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_mhak_m.mhaksite IS NULL
   OR g_mhak_m.mhak001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
    
   LET g_mhaksite_t = g_mhak_m.mhaksite
   LET g_mhak001_t = g_mhak_m.mhak001
 
   
   
 
   OPEN amhm342_cl USING g_enterprise,g_mhak_m.mhaksite,g_mhak_m.mhak001
 
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN amhm342_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE amhm342_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE amhm342_master_referesh USING g_mhak_m.mhaksite,g_mhak_m.mhak001 INTO g_mhak_m.mhaksite,g_mhak_m.mhak001, 
       g_mhak_m.mhak002,g_mhak_m.mhak004,g_mhak_m.mhak006,g_mhak_m.mhak007,g_mhak_m.mhakunit,g_mhak_m.mhak003, 
       g_mhak_m.mhak005,g_mhak_m.mhak008,g_mhak_m.mhak009,g_mhak_m.mhak010,g_mhak_m.mhakstus,g_mhak_m.mhakownid, 
       g_mhak_m.mhakowndp,g_mhak_m.mhakcrtid,g_mhak_m.mhakcrtdp,g_mhak_m.mhakcrtdt,g_mhak_m.mhakmodid, 
       g_mhak_m.mhakmoddt,g_mhak_m.mhakcnfid,g_mhak_m.mhakcnfdt,g_mhak_m.mhaksite_desc,g_mhak_m.mhakownid_desc, 
       g_mhak_m.mhakowndp_desc,g_mhak_m.mhakcrtid_desc,g_mhak_m.mhakcrtdp_desc,g_mhak_m.mhakmodid_desc, 
       g_mhak_m.mhakcnfid_desc
   
   #檢查是否允許此動作
   IF NOT amhm342_action_chk() THEN
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_mhak_m_mask_o.* =  g_mhak_m.*
   CALL amhm342_mhak_t_mask()
   LET g_mhak_m_mask_n.* =  g_mhak_m.*
   
   #將最新資料顯示到畫面上
   CALL amhm342_show()
   
   IF cl_ask_delete() THEN
 
      #add-point:單頭刪除前
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point
 
      #應用 a47 樣板自動產生(Version:2)
      #刪除相關文件
      CALL amhm342_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
 
      DELETE FROM mhak_t 
       WHERE mhakent = g_enterprise AND mhaksite = g_mhak_m.mhaksite 
         AND mhak001 = g_mhak_m.mhak001 
 
 
      #add-point:單頭刪除中
      {<point name="delete.head.m_delete"/>}
      #end add-point
         
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "mhak_t" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
      END IF
  
      
      
      #add-point:單頭刪除後
      {<point name="delete.head.a_delete"/>}
      #end add-point
      
       
 
      #修改歷程記錄(刪除)
      IF NOT cl_log_modified_record('','') THEN 
         CLOSE amhm342_cl
         CALL s_transaction_end('N','0')
         RETURN
      END IF
      
      CLEAR FORM
      CALL amhm342_ui_browser_refresh()
      
      #確保畫面上保有資料
      IF g_browser_cnt > 0 THEN
         #CALL amhm342_browser_fill(g_wc,"")
         CALL amhm342_fetch("P")
      ELSE
         CLEAR FORM
      END IF
      
   END IF
 
   CLOSE amhm342_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL amhm342_msgcentre_notify('')
 
END FUNCTION
