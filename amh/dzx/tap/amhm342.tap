  <section id="amhm342.modify" ver="2" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION amhm342_modify()
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point
   #add-point:modify段define(客製用)
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_mhak_m.mhaksite IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_mhaksite_t = g_mhak_m.mhaksite
   LET g_mhak001_t = g_mhak_m.mhak001
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN amhm342_cl USING g_enterprise,g_mhak_m.mhaksite,g_mhak_m.mhak001
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN amhm342_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE amhm342_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE amhm342_master_referesh USING g_mhak_m.mhaksite,g_mhak_m.mhak001 INTO g_mhak_m.mhaksite,g_mhak_m.mhak001, 
       g_mhak_m.mhak002,g_mhak_m.mhak004,g_mhak_m.mhak006,g_mhak_m.mhak007,g_mhak_m.mhakunit,g_mhak_m.mhak003, 
       g_mhak_m.mhak005,g_mhak_m.mhak008,g_mhak_m.mhak009,g_mhak_m.mhak010,g_mhak_m.mhakstus,g_mhak_m.mhakownid, 
       g_mhak_m.mhakowndp,g_mhak_m.mhakcrtid,g_mhak_m.mhakcrtdp,g_mhak_m.mhakcrtdt,g_mhak_m.mhakmodid, 
       g_mhak_m.mhakmoddt,g_mhak_m.mhakcnfid,g_mhak_m.mhakcnfdt,g_mhak_m.mhaksite_desc,g_mhak_m.mhakownid_desc, 
       g_mhak_m.mhakowndp_desc,g_mhak_m.mhakcrtid_desc,g_mhak_m.mhakcrtdp_desc,g_mhak_m.mhakmodid_desc, 
       g_mhak_m.mhakcnfid_desc
 
   #檢查是否允許此動作
   IF NOT amhm342_action_chk() THEN
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_mhak_m_mask_o.* =  g_mhak_m.*
   CALL amhm342_mhak_t_mask()
   LET g_mhak_m_mask_n.* =  g_mhak_m.*
   
   
 
   #顯示資料
   CALL amhm342_show()
   
   WHILE TRUE
      LET g_mhak_m.mhaksite = g_mhaksite_t
      LET g_mhak_m.mhak001 = g_mhak001_t
 
      
      #寫入修改者/修改日期資訊
      LET g_mhak_m.mhakmodid = g_user 
LET g_mhak_m.mhakmoddt = cl_get_current()
LET g_mhak_m.mhakmodid_desc = cl_get_username(g_mhak_m.mhakmodid)
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
 
      #資料輸入
      CALL amhm342_input("u")     
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_mhak_m.* = g_mhak_m_t.*
         CALL amhm342_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE mhak_t SET (mhakmodid,mhakmoddt) = (g_mhak_m.mhakmodid,g_mhak_m.mhakmoddt)
       WHERE mhakent = g_enterprise AND mhaksite = g_mhaksite_t
         AND mhak001 = g_mhak001_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL amhm342_set_act_visible()
   CALL amhm342_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " mhakent = '" ||g_enterprise|| "' AND",
                      " mhaksite = '", g_mhak_m.mhaksite, "' "
                      ," AND mhak001 = '", g_mhak_m.mhak001, "' "
 
   #填到對應位置
   CALL amhm342_browser_fill(g_wc,"")
 
   CLOSE amhm342_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL amhm342_msgcentre_notify('')
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
]]>
  </section>
