  <section id="amhm342.insert" ver="4" status="" src="s" readonly="">
    <![CDATA[#+ 資料新增
PRIVATE FUNCTION amhm342_insert()
   #add-point:insert段define
   {<point name="insert.define" edit="s"/>}
   #end add-point    
   #add-point:insert段define(客製用)
   {<point name="insert.define_customerization" edit="c"/>}
   #end add-point
   
   CLEAR FORM #清畫面欄位內容
   INITIALIZE g_mhak_m.* LIKE mhak_t.*             #DEFAULT 設定
   LET g_mhaksite_t = NULL
   LET g_mhak001_t = NULL
 
   
   #add-point:insert段before
   {<point name="insert.before"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   
   WHILE TRUE
      
      #公用欄位給值
      #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_mhak_m.mhakownid = g_user
      LET g_mhak_m.mhakowndp = g_dept
      LET g_mhak_m.mhakcrtid = g_user
      LET g_mhak_m.mhakcrtdp = g_dept 
      LET g_mhak_m.mhakcrtdt = cl_get_current()
      LET g_mhak_m.mhakmodid = g_user
      LET g_mhak_m.mhakmoddt = cl_get_current()
      LET g_mhak_m.mhakstus = 'N'
 
 
 
      #append欄位給值
      
     
      #一般欄位給值
      
 
      #add-point:單頭預設值
      {<point name="insert.default"/>}
      #end add-point   
     
      #顯示狀態(stus)圖片
            #應用 a21 樣板自動產生(Version:2)
	  #根據當下狀態碼顯示圖片
      CASE g_mhak_m.mhakstus 
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/open.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/valid.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/void.png")
         
      END CASE
 
 
     
      #資料輸入
      CALL amhm342_input("a")
      
      #add-point:單頭輸入後
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG THEN
         #取消
         LET INT_FLAG = 0
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         DISPLAY g_current_cnt TO FORMONLY.h_count     #總筆數
         DISPLAY g_current_idx TO FORMONLY.h_index     #當下筆數
         INITIALIZE g_mhak_m.* TO NULL
         CALL amhm342_show()
         RETURN
      END IF
 
      LET g_rec_b = 0
      EXIT WHILE
   END WHILE
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL amhm342_set_act_visible()
   CALL amhm342_set_act_no_visible()
 
   #將新增的資料併入搜尋條件中
   LET g_state = "insert"
   
   LET g_mhaksite_t = g_mhak_m.mhaksite
   LET g_mhak001_t = g_mhak_m.mhak001
 
   
   #組合新增資料的條件
   LET g_add_browse = " mhakent = '" ||g_enterprise|| "' AND",
                      " mhaksite = '", g_mhak_m.mhaksite, "' "
                      ," AND mhak001 = '", g_mhak_m.mhak001, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL amhm342_browser_fill("","")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
 
   #撈取異動後的資料(主要是帶出reference)
   EXECUTE amhm342_master_referesh USING g_mhak_m.mhaksite,g_mhak_m.mhak001 INTO g_mhak_m.mhaksite,g_mhak_m.mhak001, 
       g_mhak_m.mhak002,g_mhak_m.mhak004,g_mhak_m.mhak006,g_mhak_m.mhak007,g_mhak_m.mhakunit,g_mhak_m.mhak003, 
       g_mhak_m.mhak005,g_mhak_m.mhak008,g_mhak_m.mhak009,g_mhak_m.mhak010,g_mhak_m.mhakstus,g_mhak_m.mhakownid, 
       g_mhak_m.mhakowndp,g_mhak_m.mhakcrtid,g_mhak_m.mhakcrtdp,g_mhak_m.mhakcrtdt,g_mhak_m.mhakmodid, 
       g_mhak_m.mhakmoddt,g_mhak_m.mhakcnfid,g_mhak_m.mhakcnfdt,g_mhak_m.mhaksite_desc,g_mhak_m.mhakownid_desc, 
       g_mhak_m.mhakowndp_desc,g_mhak_m.mhakcrtid_desc,g_mhak_m.mhakcrtdp_desc,g_mhak_m.mhakmodid_desc, 
       g_mhak_m.mhakcnfid_desc
   
   #遮罩相關處理
   LET g_mhak_m_mask_o.* =  g_mhak_m.*
   CALL amhm342_mhak_t_mask()
   LET g_mhak_m_mask_n.* =  g_mhak_m.*
   
   #將資料顯示到畫面上
   DISPLAY BY NAME g_mhak_m.mhaksite,g_mhak_m.mhaksite_desc,g_mhak_m.mhak001,g_mhak_m.mhak002,g_mhak_m.mhak004, 
       g_mhak_m.mhak006,g_mhak_m.mhak007,g_mhak_m.mhakunit,g_mhak_m.mhak003,g_mhak_m.mhak005,g_mhak_m.mhak008, 
       g_mhak_m.mhak009,g_mhak_m.mhak010,g_mhak_m.mhakstus,g_mhak_m.mhakownid,g_mhak_m.mhakownid_desc, 
       g_mhak_m.mhakowndp,g_mhak_m.mhakowndp_desc,g_mhak_m.mhakcrtid,g_mhak_m.mhakcrtid_desc,g_mhak_m.mhakcrtdp, 
       g_mhak_m.mhakcrtdp_desc,g_mhak_m.mhakcrtdt,g_mhak_m.mhakmodid,g_mhak_m.mhakmodid_desc,g_mhak_m.mhakmoddt, 
       g_mhak_m.mhakcnfid,g_mhak_m.mhakcnfid_desc,g_mhak_m.mhakcnfdt
 
   #add-point:新增結束後
   {<point name="insert.after"/>}
   #end add-point 
 
   #功能已完成,通報訊息中心
   CALL amhm342_msgcentre_notify('')
   
END FUNCTION
