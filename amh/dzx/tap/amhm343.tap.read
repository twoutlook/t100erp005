      CLOSE amhm343_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE amhm343_master_referesh USING g_mhal_m.mhalsite,g_mhal_m.mhal001,g_mhal_m.mhal002,g_mhal_m.mhal004, 
       g_mhal_m.mhal005,g_mhal_m.mhal007 INTO g_mhal_m.mhalsite,g_mhal_m.mhal001,g_mhal_m.mhal002,g_mhal_m.mhal003, 
       g_mhal_m.mhal004,g_mhal_m.mhal005,g_mhal_m.mhal006,g_mhal_m.mhal007,g_mhal_m.mhal008,g_mhal_m.mhal009, 
