  <section id="amhm343.modify" ver="2" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION amhm343_modify()
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point
   #add-point:modify段define(客製用)
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_mhal_m.mhalsite IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_mhalsite_t = g_mhal_m.mhalsite
   LET g_mhal001_t = g_mhal_m.mhal001
   LET g_mhal002_t = g_mhal_m.mhal002
   LET g_mhal004_t = g_mhal_m.mhal004
   LET g_mhal005_t = g_mhal_m.mhal005
   LET g_mhal007_t = g_mhal_m.mhal007
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN amhm343_cl USING g_enterprise,g_mhal_m.mhalsite,g_mhal_m.mhal001,g_mhal_m.mhal002,g_mhal_m.mhal004,g_mhal_m.mhal005,g_mhal_m.mhal007
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN amhm343_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE amhm343_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE amhm343_master_referesh USING g_mhal_m.mhalsite,g_mhal_m.mhal001,g_mhal_m.mhal002,g_mhal_m.mhal004, 
       g_mhal_m.mhal005,g_mhal_m.mhal007 INTO g_mhal_m.mhalsite,g_mhal_m.mhal001,g_mhal_m.mhal002,g_mhal_m.mhal003, 
       g_mhal_m.mhal004,g_mhal_m.mhal005,g_mhal_m.mhal006,g_mhal_m.mhal007,g_mhal_m.mhal008,g_mhal_m.mhal009, 
       g_mhal_m.mhal010,g_mhal_m.mhal011,g_mhal_m.mhal012,g_mhal_m.mhal013,g_mhal_m.mhal014,g_mhal_m.mhal015, 
       g_mhal_m.mhal016,g_mhal_m.mhal017,g_mhal_m.mhal018,g_mhal_m.mhal019,g_mhal_m.mhal020,g_mhal_m.mhal021, 
       g_mhal_m.mhal022,g_mhal_m.mhal023,g_mhal_m.mhal024,g_mhal_m.mhalstus,g_mhal_m.mhalownid,g_mhal_m.mhalowndp, 
       g_mhal_m.mhalcrtid,g_mhal_m.mhalcrtdp,g_mhal_m.mhalcrtdt,g_mhal_m.mhalmodid,g_mhal_m.mhalmoddt, 
       g_mhal_m.mhalcnfid,g_mhal_m.mhalcnfdt,g_mhal_m.mhalsite_desc,g_mhal_m.mhal005_desc,g_mhal_m.mhal006_desc, 
       g_mhal_m.mhal007_desc,g_mhal_m.mhal020_desc,g_mhal_m.mhalownid_desc,g_mhal_m.mhalowndp_desc,g_mhal_m.mhalcrtid_desc, 
       g_mhal_m.mhalcrtdp_desc,g_mhal_m.mhalmodid_desc,g_mhal_m.mhalcnfid_desc
 
   #檢查是否允許此動作
   IF NOT amhm343_action_chk() THEN
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_mhal_m_mask_o.* =  g_mhal_m.*
   CALL amhm343_mhal_t_mask()
   LET g_mhal_m_mask_n.* =  g_mhal_m.*
   
   
 
   #顯示資料
   CALL amhm343_show()
   
   WHILE TRUE
      LET g_mhal_m.mhalsite = g_mhalsite_t
      LET g_mhal_m.mhal001 = g_mhal001_t
      LET g_mhal_m.mhal002 = g_mhal002_t
      LET g_mhal_m.mhal004 = g_mhal004_t
      LET g_mhal_m.mhal005 = g_mhal005_t
      LET g_mhal_m.mhal007 = g_mhal007_t
 
      
      #寫入修改者/修改日期資訊
      LET g_mhal_m.mhalmodid = g_user 
LET g_mhal_m.mhalmoddt = cl_get_current()
LET g_mhal_m.mhalmodid_desc = cl_get_username(g_mhal_m.mhalmodid)
      
      #add-point:modify段修改前
