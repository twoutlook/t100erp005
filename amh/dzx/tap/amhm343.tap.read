      SELECT mhak005 INTO g_mhal_m.mhal018
        FROM mhak_t
       WHERE mhak001 = g_mhal_m.mhal001
         AND mhakent = g_enterprise
         AND mhaksite = g_mhal_m.mhalsite
      LET  g_mhal_m.mhal019 = g_mhal_m.mhal018*g_mhal_m.mhal017*g_mhal_m.mhal010
