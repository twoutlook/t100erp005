                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = 'amh-00066'
                  LET g_errparam.extend = ''
                  LET g_errparam.popup = TRUE
                  CALL cl_err()
                  LET g_mhal_m.mhal001 = g_mhal_m_t.mhal001
                  NEXT FIELD CURRENT
               END IF
               INITIALIZE l_number TO NULL
               LET l_number= l_str.subString(5,6)
