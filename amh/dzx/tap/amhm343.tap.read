   LET g_mhal_m.mhalmodid = g_user
   LET g_mhal_m.mhalmoddt = cl_get_current()
   LET g_mhal_m.mhalstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE mhal_t 
      SET (mhalstus,mhalmodid,mhalmoddt) 
        = (g_mhal_m.mhalstus,g_mhal_m.mhalmodid,g_mhal_m.mhalmoddt)     
    WHERE mhalent = g_enterprise AND mhalsite = g_mhal_m.mhalsite
      AND mhal001 = g_mhal_m.mhal001      AND mhal002 = g_mhal_m.mhal002      AND mhal004 = g_mhal_m.mhal004      AND mhal005 = g_mhal_m.mhal005      AND mhal007 = g_mhal_m.mhal007
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
