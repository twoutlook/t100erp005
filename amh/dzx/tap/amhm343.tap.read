  <point name="function.amhm343_mhalsite_ref" order="1" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################

################################################################################
PRIVATE FUNCTION amhm343_mhalsite_ref()
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_mhal_m.mhalsite
   CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_mhal_m.mhalsite_desc = '', g_rtn_fields[1] , ''
   DISPLAY BY NAME g_mhal_m.mhalsite_desc
END FUNCTION]]>
