  <section id="amhm343.main" ver="2" status="" src="s" readonly="">
    <![CDATA[#應用 a26 樣板自動產生(Version:4)
#+ 作業開始(主程式類型)
MAIN
   #add-point:main段define
   {<point name="main.define" edit="s"/>}
   #end add-point   
   #add-point:main段define(客製用)
   {<point name="main.define_customerization" edit="c"/>}
   #end add-point   
 
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("amh","")
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT mhalsite,'',mhal001,mhal002,mhal003,mhal004,mhal005,'',mhal006,'',mhal007, 
       '',mhal008,mhal009,mhal010,mhal011,mhal012,mhal013,mhal014,mhal015,mhal016,mhal017,mhal018,mhal019, 
       mhal020,'',mhal021,mhal022,mhal023,mhal024,mhalstus,mhalownid,'',mhalowndp,'',mhalcrtid,'',mhalcrtdp, 
       '',mhalcrtdt,mhalmodid,'',mhalmoddt,mhalcnfid,'',mhalcnfdt", 
                      " FROM mhal_t",
                      " WHERE mhalent= ? AND mhalsite=? AND mhal001=? AND mhal002=? AND mhal004=? AND  
                          mhal005=? AND mhal007=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE amhm343_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE t0.mhalsite,t0.mhal001,t0.mhal002,t0.mhal003,t0.mhal004,t0.mhal005,t0.mhal006, 
       t0.mhal007,t0.mhal008,t0.mhal009,t0.mhal010,t0.mhal011,t0.mhal012,t0.mhal013,t0.mhal014,t0.mhal015, 
       t0.mhal016,t0.mhal017,t0.mhal018,t0.mhal019,t0.mhal020,t0.mhal021,t0.mhal022,t0.mhal023,t0.mhal024, 
       t0.mhalstus,t0.mhalownid,t0.mhalowndp,t0.mhalcrtid,t0.mhalcrtdp,t0.mhalcrtdt,t0.mhalmodid,t0.mhalmoddt, 
       t0.mhalcnfid,t0.mhalcnfdt,t1.ooefl003 ,t2.mhael023 ,t3.ooefl003 ,t4.mhaml003 ,t5.rtaxl003 ,t6.ooag011 , 
       t7.ooefl003 ,t8.ooag011 ,t9.ooefl003 ,t10.ooag011 ,t11.ooag011",
               " FROM mhal_t t0",
                              " LEFT JOIN ooefl_t t1 ON t1.ooeflent='"||g_enterprise||"' AND t1.ooefl001=t0.mhalsite AND t1.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN mhael_t t2 ON t2.mhaelent='"||g_enterprise||"' AND t2.mhael001=t0.mhal005 AND t2.mhael022='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent='"||g_enterprise||"' AND t3.ooefl001=t0.mhal006 AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN mhaml_t t4 ON t4.mhamlent='"||g_enterprise||"' AND t4.mhaml001=t0.mhal007 AND t4.mhaml002='"||g_dlang||"' ",
               " LEFT JOIN rtaxl_t t5 ON t5.rtaxlent='"||g_enterprise||"' AND t5.rtaxl001=t0.mhal020 AND t5.rtaxl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent='"||g_enterprise||"' AND t6.ooag001=t0.mhalownid  ",
               " LEFT JOIN ooefl_t t7 ON t7.ooeflent='"||g_enterprise||"' AND t7.ooefl001=t0.mhalowndp AND t7.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t8 ON t8.ooagent='"||g_enterprise||"' AND t8.ooag001=t0.mhalcrtid  ",
               " LEFT JOIN ooefl_t t9 ON t9.ooeflent='"||g_enterprise||"' AND t9.ooefl001=t0.mhalcrtdp AND t9.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t10 ON t10.ooagent='"||g_enterprise||"' AND t10.ooag001=t0.mhalmodid  ",
               " LEFT JOIN ooag_t t11 ON t11.ooagent='"||g_enterprise||"' AND t11.ooag001=t0.mhalcnfid  ",
 
               " WHERE t0.mhalent = '" ||g_enterprise|| "' AND t0.mhalsite = ? AND t0.mhal001 = ? AND t0.mhal002 = ? AND t0.mhal004 = ? AND t0.mhal005 = ? AND t0.mhal007 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE amhm343_master_referesh FROM g_sql
 
    
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_amhm343 WITH FORM cl_ap_formpath("amh",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL amhm343_init()   
 
      #進入選單 Menu (="N")
      CALL amhm343_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_amhm343
      
   END IF 
   
   CLOSE amhm343_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
END MAIN
 
 
]]>
