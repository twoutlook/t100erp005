PRIVATE FUNCTION amhm343_reproduce()
   DEFINE l_newno     LIKE mhal_t.mhalsite 
   DEFINE l_oldno     LIKE mhal_t.mhalsite 
   DEFINE l_newno02     LIKE mhal_t.mhal001 
   DEFINE l_oldno02     LIKE mhal_t.mhal001 
   DEFINE l_newno03     LIKE mhal_t.mhal002 
   DEFINE l_oldno03     LIKE mhal_t.mhal002 
   DEFINE l_newno04     LIKE mhal_t.mhal004 
   DEFINE l_oldno04     LIKE mhal_t.mhal004 
   DEFINE l_newno05     LIKE mhal_t.mhal005 
   DEFINE l_oldno05     LIKE mhal_t.mhal005 
   DEFINE l_newno06     LIKE mhal_t.mhal007 
   DEFINE l_oldno06     LIKE mhal_t.mhal007 
 
   DEFINE l_master    RECORD LIKE mhal_t.*
   DEFINE l_cnt       LIKE type_t.num10
   #add-point:reproduce段define
   {<point name="reproduce.define" edit="s"/>}
   #end add-point   
   #add-point:reproduce段define(客製用)
   {<point name="reproduce.define_customerization" edit="c"/>}
   #end add-point
   
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF
   
   #先確定key值無遺漏
   IF g_mhal_m.mhalsite IS NULL
      OR g_mhal_m.mhal001 IS NULL
      OR g_mhal_m.mhal002 IS NULL
      OR g_mhal_m.mhal004 IS NULL
      OR g_mhal_m.mhal005 IS NULL
      OR g_mhal_m.mhal007 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
   
   #備份key值
   LET g_mhalsite_t = g_mhal_m.mhalsite
   LET g_mhal001_t = g_mhal_m.mhal001
   LET g_mhal002_t = g_mhal_m.mhal002
   LET g_mhal004_t = g_mhal_m.mhal004
   LET g_mhal005_t = g_mhal_m.mhal005
   LET g_mhal007_t = g_mhal_m.mhal007
 
   
   #清空key值
   LET g_mhal_m.mhalsite = ""
   LET g_mhal_m.mhal001 = ""
   LET g_mhal_m.mhal002 = ""
   LET g_mhal_m.mhal004 = ""
   LET g_mhal_m.mhal005 = ""
   LET g_mhal_m.mhal007 = ""
 
    
   CALL amhm343_set_entry("a")
   CALL amhm343_set_no_entry("a")
   
   #公用欄位給予預設值
   #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_mhal_m.mhalownid = g_user
      LET g_mhal_m.mhalowndp = g_dept
      LET g_mhal_m.mhalcrtid = g_user
      LET g_mhal_m.mhalcrtdp = g_dept 
      LET g_mhal_m.mhalcrtdt = cl_get_current()
      LET g_mhal_m.mhalmodid = g_user
      LET g_mhal_m.mhalmoddt = cl_get_current()
      LET g_mhal_m.mhalstus = 'N'
 
 
   
   CALL s_transaction_begin()
   
   #add-point:複製輸入前
   {<point name="reproduce.head.b_input"/>}
   #end add-point
   
   #顯示狀態(stus)圖片
         #應用 a21 樣板自動產生(Version:2)
	  #根據當下狀態碼顯示圖片
      CASE g_mhal_m.mhalstus 
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/open.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/valid.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/void.png")
         
      END CASE
 
 
   
   #清空key欄位的desc
      LET g_mhal_m.mhalsite_desc = ''
   DISPLAY BY NAME g_mhal_m.mhalsite_desc
   LET g_mhal_m.mhal005_desc = ''
   DISPLAY BY NAME g_mhal_m.mhal005_desc
   LET g_mhal_m.mhal007_desc = ''
   DISPLAY BY NAME g_mhal_m.mhal007_desc
 
   
   #資料輸入
   CALL amhm343_input("r")
   
   IF INT_FLAG THEN
      #取消
      LET INT_FLAG = 0
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = 9001 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      INITIALIZE g_mhal_m.* TO NULL
      CALL amhm343_show()
      RETURN
   END IF
   
   CALL s_transaction_begin()
   
   #add-point:單頭複製前
   {<point name="reproduce.head.b_insert" mark="Y"/>}
   #end add-point
   
   #add-point:單頭複製中
   {<point name="reproduce.head.m_insert"/>}
   #end add-point
   
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "mhal_t" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:單頭複製後
   {<point name="reproduce.head.a_insert"/>}
   #end add-point
   
   CALL s_transaction_end('Y','0')
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL amhm343_set_act_visible()
   CALL amhm343_set_act_no_visible()
 
   #將新增的資料併入搜尋條件中
   LET g_state = "insert"
   
   LET g_mhalsite_t = g_mhal_m.mhalsite
   LET g_mhal001_t = g_mhal_m.mhal001
   LET g_mhal002_t = g_mhal_m.mhal002
   LET g_mhal004_t = g_mhal_m.mhal004
   LET g_mhal005_t = g_mhal_m.mhal005
   LET g_mhal007_t = g_mhal_m.mhal007
 
   
   #組合新增資料的條件
   LET g_add_browse = " mhalent = '" ||g_enterprise|| "' AND",
                      " mhalsite = '", g_mhal_m.mhalsite, "' "
                      ," AND mhal001 = '", g_mhal_m.mhal001, "' "
                      ," AND mhal002 = '", g_mhal_m.mhal002, "' "
                      ," AND mhal004 = '", g_mhal_m.mhal004, "' "
                      ," AND mhal005 = '", g_mhal_m.mhal005, "' "
                      ," AND mhal007 = '", g_mhal_m.mhal007, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL amhm343_browser_fill("","")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   #add-point:完成複製段落後
   {<point name="reproduce.after_reproduce" />}
   #end add-point
                 
   #功能已完成,通報訊息中心
   CALL amhm343_msgcentre_notify('')
                 
END FUNCTION
]]>
