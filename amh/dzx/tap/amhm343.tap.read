  <point name="statechange.b_update" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   LET l_success = TRUE
   INITIALIZE g_errno TO NULL
   CASE
      WHEN lc_state = 'Y'
         CALL s_amhm343_conf_chf(g_mhal_m.mhalsite,g_mhal_m.mhal001,g_mhal_m.mhal002,g_mhal_m.mhal004,g_mhal_m.mhal005,g_mhal_m.mhal007,g_mhal_m.mhalstus) RETURNING l_success
         IF l_success THEN
            IF cl_ask_confirm('aim-00108') THEN
               CALL s_transaction_begin()
               CALL s_amhm343_conf_upd(g_mhal_m.mhalsite,g_mhal_m.mhal001,g_mhal_m.mhal002,g_mhal_m.mhal004,g_mhal_m.mhal005,g_mhal_m.mhal007) RETURNING l_success,g_errno
               IF NOT l_success THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = g_errno
                  LET g_errparam.extend = ''
                  LET g_errparam.popup = TRUE
                  CALL cl_err()

                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','1')
               END IF
            ELSE
               RETURN
            END IF
         ELSE
            RETURN
         END IF
      WHEN lc_state = 'X'
         CALL s_amhm343_void_chk(g_mhal_m.mhalstus) RETURNING l_success
         IF l_success THEN
            IF cl_ask_confirm('art-00039') THEN
               CALL s_transaction_begin()
               CALL s_amhm343_void_upd(g_mhal_m.mhalsite,g_mhal_m.mhal001,g_mhal_m.mhal002,g_mhal_m.mhal004,g_mhal_m.mhal005,g_mhal_m.mhal007) RETURNING l_success,g_errno
               IF NOT l_success THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = g_errno
                  LET g_errparam.extend = ''
                  LET g_errparam.popup = TRUE
                  CALL cl_err()

                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','1')
               END IF
            ELSE
               RETURN
            END IF
         ELSE
            RETURN
         END IF
      WHEN lc_state = 'N'
         CALL s_amhm343_unconf_chf(g_mhal_m.mhalstus) RETURNING l_success
         IF l_success THEN
            IF cl_ask_confirm('aim-00110') THEN
               CALL s_transaction_begin()
               CALL s_amhm343_unconf_upd(g_mhal_m.mhalsite,g_mhal_m.mhal001,g_mhal_m.mhal002,g_mhal_m.mhal004,g_mhal_m.mhal005,g_mhal_m.mhal007) RETURNING l_success,g_errno
               IF NOT l_success THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = g_errno
                  LET g_errparam.extend = ''
                  LET g_errparam.popup = TRUE
                  CALL cl_err()

                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','1')
               END IF
            ELSE
               RETURN
            END IF
         ELSE
            RETURN
         END IF
      OTHERWISE
         RETURN
   END CASE]]>
