       g_mhal_m.mhalcrtdp_desc,g_mhal_m.mhalcrtdt,g_mhal_m.mhalmodid,g_mhal_m.mhalmodid_desc,g_mhal_m.mhalmoddt, 
       g_mhal_m.mhalcnfid,g_mhal_m.mhalcnfid_desc,g_mhal_m.mhalcnfdt
 
   CASE g_mhal_m.mhalstus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/open.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/valid.png")
      WHEN "X"
