  <section id="amhm343.fetch" ver="3" status="" src="s" readonly="">
    <![CDATA[#+ 指定PK後抓取單頭其他資料
PRIVATE FUNCTION amhm343_fetch(p_fl)
   DEFINE p_fl       LIKE type_t.chr1
   DEFINE ls_msg     STRING
   #add-point:fetch段define
   {<point name="fetch.define" edit="s"/>}
   #end add-point  
   #add-point:fetch段define(客製用)
   {<point name="fetch.define_customerization" edit="c"/>}
   #end add-point
   
   #根據傳入的條件決定抓取的資料
   CASE p_fl
      WHEN "F" 
         LET g_current_idx = 1
      WHEN "P"
         IF g_current_idx > 1 THEN               
            LET g_current_idx = g_current_idx - 1
         END IF 
      WHEN "N"
         IF g_current_idx < g_header_cnt THEN
            LET g_current_idx =  g_current_idx + 1
         END IF        
      WHEN "L" 
         #LET g_current_idx = g_header_cnt        
         LET g_current_idx = g_browser.getLength()    
      WHEN "/"
         #詢問要指定的筆數
         IF (NOT g_no_ask) THEN      
            CALL cl_getmsg("fetch", g_lang) RETURNING ls_msg
            LET INT_FLAG = 0
 
            PROMPT ls_msg CLIPPED,": " FOR g_jump
               #交談指令共用ACTION
               &include "common_action.4gl"
            END PROMPT
            
            IF INT_FLAG THEN
               LET INT_FLAG = 0
               EXIT CASE  
            END IF           
         END IF
         IF g_jump > 0 THEN
            LET g_current_idx = g_jump
         END IF
         LET g_no_ask = FALSE     
   END CASE
 
   #筆數顯示
   LET g_browser_idx = g_current_idx 
   IF g_browser_cnt > 0 THEN
      DISPLAY g_browser_idx TO FORMONLY.b_index #當下筆數
      DISPLAY g_browser_cnt TO FORMONLY.b_count #總筆數
      DISPLAY g_browser_idx TO FORMONLY.h_index #當下筆數
      DISPLAY g_browser_cnt TO FORMONLY.h_count #總筆數
   ELSE
      DISPLAY '' TO FORMONLY.b_index #當下筆數
      DISPLAY '' TO FORMONLY.b_count #總筆數
      DISPLAY '' TO FORMONLY.h_index #當下筆數
      DISPLAY '' TO FORMONLY.h_count #總筆數
   END IF
   
   
   
   #避免超出browser資料筆數上限
   IF g_current_idx > g_browser.getLength() THEN
      LET g_current_idx = g_browser.getLength()
   END IF
   
   # 設定browse索引
   CALL g_curr_diag.setCurrentRow("s_browse", g_current_idx)
   CALL cl_navigator_setting(g_browser_idx, g_browser_cnt) 
 
   #代表沒有資料, 無需做後續資料撈取之動作
   IF g_current_idx = 0 THEN
      RETURN
   END IF
 
   #根據選定的筆數給予key欄位值
   LET g_mhal_m.mhalsite = g_browser[g_current_idx].b_mhalsite
   LET g_mhal_m.mhal001 = g_browser[g_current_idx].b_mhal001
   LET g_mhal_m.mhal002 = g_browser[g_current_idx].b_mhal002
   LET g_mhal_m.mhal004 = g_browser[g_current_idx].b_mhal004
   LET g_mhal_m.mhal005 = g_browser[g_current_idx].b_mhal005
   LET g_mhal_m.mhal007 = g_browser[g_current_idx].b_mhal007
 
                       
   #讀取單頭所有欄位資料
   EXECUTE amhm343_master_referesh USING g_mhal_m.mhalsite,g_mhal_m.mhal001,g_mhal_m.mhal002,g_mhal_m.mhal004, 
       g_mhal_m.mhal005,g_mhal_m.mhal007 INTO g_mhal_m.mhalsite,g_mhal_m.mhal001,g_mhal_m.mhal002,g_mhal_m.mhal003, 
       g_mhal_m.mhal004,g_mhal_m.mhal005,g_mhal_m.mhal006,g_mhal_m.mhal007,g_mhal_m.mhal008,g_mhal_m.mhal009, 
       g_mhal_m.mhal010,g_mhal_m.mhal011,g_mhal_m.mhal012,g_mhal_m.mhal013,g_mhal_m.mhal014,g_mhal_m.mhal015, 
       g_mhal_m.mhal016,g_mhal_m.mhal017,g_mhal_m.mhal018,g_mhal_m.mhal019,g_mhal_m.mhal020,g_mhal_m.mhal021, 
       g_mhal_m.mhal022,g_mhal_m.mhal023,g_mhal_m.mhal024,g_mhal_m.mhalstus,g_mhal_m.mhalownid,g_mhal_m.mhalowndp, 
       g_mhal_m.mhalcrtid,g_mhal_m.mhalcrtdp,g_mhal_m.mhalcrtdt,g_mhal_m.mhalmodid,g_mhal_m.mhalmoddt, 
       g_mhal_m.mhalcnfid,g_mhal_m.mhalcnfdt,g_mhal_m.mhalsite_desc,g_mhal_m.mhal005_desc,g_mhal_m.mhal006_desc, 
       g_mhal_m.mhal007_desc,g_mhal_m.mhal020_desc,g_mhal_m.mhalownid_desc,g_mhal_m.mhalowndp_desc,g_mhal_m.mhalcrtid_desc, 
       g_mhal_m.mhalcrtdp_desc,g_mhal_m.mhalmodid_desc,g_mhal_m.mhalcnfid_desc
   
   #遮罩相關處理
   LET g_mhal_m_mask_o.* =  g_mhal_m.*
   CALL amhm343_mhal_t_mask()
   LET g_mhal_m_mask_n.* =  g_mhal_m.*
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL amhm343_set_act_visible()
   CALL amhm343_set_act_no_visible()
 
   #add-point:fetch段action控制
   {<point name="fetch.action_control"/>}
   #end add-point  
   
   
   
   #保存單頭舊值
   LET g_mhal_m_t.* = g_mhal_m.*
   LET g_mhal_m_o.* = g_mhal_m.*
   
   LET g_data_owner = g_mhal_m.mhalownid      
   LET g_data_dept  = g_mhal_m.mhalowndp
   
   #重新顯示
   CALL amhm343_show()
   
 
END FUNCTION
]]>
