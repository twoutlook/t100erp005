       g_mhal_m.mhal016,g_mhal_m.mhal017,g_mhal_m.mhal018,g_mhal_m.mhal019,g_mhal_m.mhal020,g_mhal_m.mhal021, 
       g_mhal_m.mhal022,g_mhal_m.mhal023,g_mhal_m.mhal024,g_mhal_m.mhalstus,g_mhal_m.mhalownid,g_mhal_m.mhalowndp, 
       g_mhal_m.mhalcrtid,g_mhal_m.mhalcrtdp,g_mhal_m.mhalcrtdt,g_mhal_m.mhalmodid,g_mhal_m.mhalmoddt, 
       g_mhal_m.mhalcnfid,g_mhal_m.mhalcnfdt,g_mhal_m.mhalsite_desc,g_mhal_m.mhal005_desc,g_mhal_m.mhal006_desc, 
       g_mhal_m.mhal007_desc,g_mhal_m.mhal020_desc,g_mhal_m.mhalownid_desc,g_mhal_m.mhalowndp_desc,g_mhal_m.mhalcrtid_desc, 
       g_mhal_m.mhalcrtdp_desc,g_mhal_m.mhalmodid_desc,g_mhal_m.mhalcnfid_desc
 
   #檢查是否允許此動作
