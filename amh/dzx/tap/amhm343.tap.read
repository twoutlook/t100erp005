       g_mhal_m.mhalcrtid,g_mhal_m.mhalcrtdp,g_mhal_m.mhalcrtdt,g_mhal_m.mhalmodid,g_mhal_m.mhalmoddt, 
       g_mhal_m.mhalcnfid,g_mhal_m.mhalcnfdt,g_mhal_m.mhalsite_desc,g_mhal_m.mhal005_desc,g_mhal_m.mhal006_desc, 
