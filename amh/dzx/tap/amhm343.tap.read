         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/void.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE amhm343_master_referesh USING g_mhal_m.mhalsite,g_mhal_m.mhal001,g_mhal_m.mhal002,g_mhal_m.mhal004, 
          g_mhal_m.mhal005,g_mhal_m.mhal007 INTO g_mhal_m.mhalsite,g_mhal_m.mhal001,g_mhal_m.mhal002, 
          g_mhal_m.mhal003,g_mhal_m.mhal004,g_mhal_m.mhal005,g_mhal_m.mhal006,g_mhal_m.mhal007,g_mhal_m.mhal008, 
          g_mhal_m.mhal009,g_mhal_m.mhal010,g_mhal_m.mhal011,g_mhal_m.mhal012,g_mhal_m.mhal013,g_mhal_m.mhal014, 
          g_mhal_m.mhal015,g_mhal_m.mhal016,g_mhal_m.mhal017,g_mhal_m.mhal018,g_mhal_m.mhal019,g_mhal_m.mhal020, 
          g_mhal_m.mhal021,g_mhal_m.mhal022,g_mhal_m.mhal023,g_mhal_m.mhal024,g_mhal_m.mhalstus,g_mhal_m.mhalownid, 
          g_mhal_m.mhalowndp,g_mhal_m.mhalcrtid,g_mhal_m.mhalcrtdp,g_mhal_m.mhalcrtdt,g_mhal_m.mhalmodid, 
          g_mhal_m.mhalmoddt,g_mhal_m.mhalcnfid,g_mhal_m.mhalcnfdt,g_mhal_m.mhalsite_desc,g_mhal_m.mhal005_desc, 
          g_mhal_m.mhal006_desc,g_mhal_m.mhal007_desc,g_mhal_m.mhal020_desc,g_mhal_m.mhalownid_desc, 
          g_mhal_m.mhalowndp_desc,g_mhal_m.mhalcrtid_desc,g_mhal_m.mhalcrtdp_desc,g_mhal_m.mhalmodid_desc, 
          g_mhal_m.mhalcnfid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_mhal_m.mhalsite,g_mhal_m.mhalsite_desc,g_mhal_m.mhal001,g_mhal_m.mhal002,g_mhal_m.mhal003, 
          g_mhal_m.mhal004,g_mhal_m.mhal005,g_mhal_m.mhal005_desc,g_mhal_m.mhal006,g_mhal_m.mhal006_desc, 
          g_mhal_m.mhal007,g_mhal_m.mhal007_desc,g_mhal_m.mhal008,g_mhal_m.mhal009,g_mhal_m.mhal010, 
          g_mhal_m.mhal011,g_mhal_m.mhal012,g_mhal_m.mhal013,g_mhal_m.mhal014,g_mhal_m.mhal015,g_mhal_m.mhal016, 
          g_mhal_m.mhal017,g_mhal_m.mhal018,g_mhal_m.mhal019,g_mhal_m.mhal020,g_mhal_m.mhal020_desc, 
          g_mhal_m.mhal021,g_mhal_m.mhal022,g_mhal_m.mhal023,g_mhal_m.mhal024,g_mhal_m.mhalstus,g_mhal_m.mhalownid, 
          g_mhal_m.mhalownid_desc,g_mhal_m.mhalowndp,g_mhal_m.mhalowndp_desc,g_mhal_m.mhalcrtid,g_mhal_m.mhalcrtid_desc, 
          g_mhal_m.mhalcrtdp,g_mhal_m.mhalcrtdp_desc,g_mhal_m.mhalcrtdt,g_mhal_m.mhalmodid,g_mhal_m.mhalmodid_desc, 
          g_mhal_m.mhalmoddt,g_mhal_m.mhalcnfid,g_mhal_m.mhalcnfid_desc,g_mhal_m.mhalcnfdt
   END IF
 
   #add-point:stus修改後
   {<point name="statechange.a_update"/>}
   #end add-point
 
   #add-point:statechange段結束前
   {<point name="statechange.after"/>}
   #end add-point  
 
   CLOSE amhm343_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL amhm343_msgcentre_notify(lc_state)
   
END FUNCTION
 
 
]]>
