  <point name="function.amhm343_mhal005_init" order="4" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################

################################################################################
PRIVATE FUNCTION amhm343_mhal005_init()
DEFINE l_mhae010       LIKE mhae_t.mhae010
DEFINE l_rtax006       LIKE rtax_t.rtax006
    #查出部门编号 经营大类
    SELECT mhae004,mhae012 INTO g_mhal_m.mhal006,g_mhal_m.mhal020
     FROM mhae_t
    WHERE mhae001 = g_mhal_m.mhal005
      AND mhaeent = g_enterprise
    CALL amhm343_mhal020_ref()    
    CALL amhm343_mhal006_ref() 
    #查出进场时间 撤柜时间
    SELECT stea017,stea018 INTO g_mhal_m.mhal008,g_mhal_m.mhal009
      FROM stea_t
     WHERE stea001 = g_mhal_m.mhal005
       AND steaent = g_enterprise
    #查出经营小类
   SELECT mhae010 INTO l_mhae010
     FROM mhae_t
    WHERE mhae001 = g_mhal_m.mhal005
      AND mhaeent = g_enterprise
   #查出一级品类   
   SELECT rtax006 INTO l_rtax006
     FROM rtax_t
    WHERE rtax001 = l_mhae010
      AND rtaxent = g_enterprise    
    #查出标准经营时间
    SELECT mhaj007 INTO g_mhal_m.mhal016
     FROM mhaj_t
    WHERE mhaj001 = g_mhal_m.mhal001
      AND mhajent = g_enterprise
      AND mhajsite = g_mhal_m.mhalsite
      AND mhaj002 = l_rtax006
      AND mhajstus = 'Y'
     # 查出实际经营时间
    SELECT mhaj007 INTO g_mhal_m.mhal017
     FROM mhaj_t
    WHERE mhaj001 = g_mhal_m.mhal001
      AND mhajent = g_enterprise
      AND mhajsite = g_mhal_m.mhalsite
      AND mhaj002 = l_rtax006
      AND mhajstus = 'Y'      
      
END FUNCTION]]>
  </point>
  <point name="function.amhm343_mhal007_init" order="5" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################

################################################################################
PRIVATE FUNCTION amhm343_mhal007_init()
DEFINE l_mham003 LIKE mham_t.mham003
DEFINE l_mham004 LIKE mham_t.mham004
DEFINE l_mham008 LIKE mham_t.mham008
    #查出功率   数量   计费系数 
   IF g_mhal_m.mhal002 = '2' THEN
      SELECT mham003,mham004,mham008 INTO l_mham003,l_mham004,l_mham008
        FROM mham_t
       WHERE mham001 = g_mhal_m.mhal007
         AND mhament = g_enterprise
      LET  g_mhal_m.mhal010 = l_mham003*l_mham004*l_mham008/1000
   ELSE
      LET  g_mhal_m.mhal010 = g_mhal_m.mhal015/g_mhal_m.mhal017
   END IF
      
END FUNCTION]]>
  </point>
