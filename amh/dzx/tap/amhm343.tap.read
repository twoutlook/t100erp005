         AFTER INPUT
            #若點選cancel則離開dialog
            IF INT_FLAG THEN
               EXIT DIALOG
            END IF
            
            #錯誤訊息統整顯示
            #CALL cl_err_collect_show()
            #CALL cl_showmsg()
  
            IF p_cmd <> "u" THEN
               #當p_cmd不為u代表為新增/複製
               LET l_count = 1  
 
               #確定新增的資料不存在(不重複)
               SELECT COUNT(*) INTO l_count FROM mhal_t
                WHERE mhalent = g_enterprise AND mhalsite = g_mhal_m.mhalsite
                  AND mhal001 = g_mhal_m.mhal001
                  AND mhal002 = g_mhal_m.mhal002
                  AND mhal004 = g_mhal_m.mhal004
                  AND mhal005 = g_mhal_m.mhal005
                  AND mhal007 = g_mhal_m.mhal007
 
               IF l_count = 0 THEN
               
                  #add-point:單頭新增前
                  {<point name="input.head.b_insert" mark="Y"/>}
