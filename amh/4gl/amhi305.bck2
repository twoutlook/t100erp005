{<section id="amhi305.modify" >}
#+ 資料修改
PRIVATE FUNCTION amhi305_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   
   #end add-point
   DEFINE  l_cmd                  LIKE type_t.chr1
   DEFINE  l_ac_t                 LIKE type_t.num10               #未取消的ARRAY CNT 
   DEFINE  l_n                    LIKE type_t.num10               #檢查重複用  
   DEFINE  l_cnt                  LIKE type_t.num10               #檢查重複用  
   DEFINE  l_lock_sw              LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert         LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete         LIKE type_t.num5                #可刪除否  
   DEFINE  l_count                LIKE type_t.num10
   DEFINE  l_i                    LIKE type_t.num10
   DEFINE  ls_return              STRING
   DEFINE  l_var_keys             DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys           DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                 DYNAMIC ARRAY OF STRING
   DEFINE  l_fields               DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak         DYNAMIC ARRAY OF STRING
   DEFINE  li_reproduce           LIKE type_t.num10
   DEFINE  li_reproduce_target    LIKE type_t.num10
   DEFINE  lb_reproduce           BOOLEAN
   DEFINE  l_insert               BOOLEAN
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   DEFINE  l_errno               LIKE type_t.chr10
   DEFINE  l_success             LIKE type_t.num5
   DEFINE p_cmd   LIKE type_t.chr1 
   #end add-point 
   
   #add-point:Function前置處理  name="modify.pre_function"
   
   #end add-point
   
   LET g_action_choice = ""
   
   LET g_qryparam.state = "i"
 
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   
   #add-point:modify開始前 name="modify.define_sql"
   
   #end add-point
   
   LET INT_FLAG = FALSE
   LET lb_reproduce = FALSE
   LET l_insert = FALSE
 
   #關閉被遮罩相關欄位輸入, 無法確定USER是否會需要輸入此欄位
   #因此先行關閉, 若有需要可於下方add-point中自行開啟
   CALL cl_mask_set_no_entry()
 
   #add-point:modify段修改前 name="modify.before_input"
   
   #end add-point
 
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #Page1 預設值產生於此處
      INPUT ARRAY g_mham_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_detail_cnt,WITHOUT DEFAULTS, #MAXCOUNT = g_max_rec,
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
 
         #應用 a43 樣板自動產生(Version:4)
         ON ACTION update_item
            LET g_action_choice="update_item"
            IF cl_auth_chk_act("update_item") THEN
               
               #add-point:ON ACTION update_item name="input.detail_input.page1.update_item"
               IF NOT cl_null(g_mham_d[l_ac].mham001) THEN
                  CALL n_mhaml(g_mham_d[l_ac].mham001)
                  INITIALIZE g_ref_fields TO NULL
                  LET g_ref_fields[1] = g_mham_d[l_ac].mham001
                  CALL ap_ref_array2(g_ref_fields," SELECT mhaml003,mhaml004 FROM mhaml_t WHERE mhamlent = '"||g_enterprise||"' AND mhaml001 = ?  AND mhaml002 = '"||g_dlang||"'","") RETURNING g_rtn_fields
                  LET g_mham_d[l_ac].mhaml003 = g_rtn_fields[1]
                  LET g_mham_d[l_ac].mhaml004 = g_rtn_fields[2]
                  DISPLAY BY NAME g_mham_d[l_ac].mhaml003,g_mham_d[l_ac].mhaml004
               END IF
               #END add-point
            END IF
 
 
 
 
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_mham_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL amhi305_b_fill(g_wc2)
            LET g_detail_cnt = g_mham_d.getLength()
         
         BEFORE ROW
            #add-point:modify段before row name="input.body.before_row2"
          
            #end add-point  
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_cmd = ''
            LET l_ac_t = l_ac 
            LET l_ac = g_detail_idx
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            DISPLAY g_mham_d.getLength() TO FORMONLY.cnt
         
            CALL s_transaction_begin()
            LET g_detail_cnt = g_mham_d.getLength()
            
            IF g_detail_cnt >= l_ac 
               AND g_mham_d[l_ac].mham001 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_mham_d_t.* = g_mham_d[l_ac].*  #BACKUP
               LET g_mham_d_o.* = g_mham_d[l_ac].*  #BACKUP
               IF NOT amhi305_lock_b("mham_t") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH amhi305_bcl INTO g_mham_d[l_ac].mhamstus,g_mham_d[l_ac].mham001,g_mham_d[l_ac].mham002, 
                      g_mham_d[l_ac].mham003,g_mham_d[l_ac].mham004,g_mham_d[l_ac].mham005,g_mham_d[l_ac].mham006, 
                      g_mham_d[l_ac].mham007,g_mham_d[l_ac].mham008,g_mham_d[l_ac].mham009,g_mham_d[l_ac].mham010, 
                      g_mham_d[l_ac].mham011,g_mham_d[l_ac].mham012,g_mham_d[l_ac].mham013,g_mham2_d[l_ac].mham001, 
                      g_mham2_d[l_ac].mhamownid,g_mham2_d[l_ac].mhamowndp,g_mham2_d[l_ac].mhamcrtid, 
                      g_mham2_d[l_ac].mhamcrtdp,g_mham2_d[l_ac].mhamcrtdt,g_mham2_d[l_ac].mhammodid, 
                      g_mham2_d[l_ac].mhammoddt,g_mham2_d[l_ac].mhamcnfid,g_mham2_d[l_ac].mhamcnfdt
                  IF SQLCA.sqlcode THEN
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = g_mham_d_t.mham001,":",SQLERRMESSAGE  
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     LET l_lock_sw = "Y"
                  END IF
                  
                  #遮罩相關處理
