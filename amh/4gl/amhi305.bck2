               CALL amhi305_insert_b('mham_t',gs_keys,"'1'")
                           
               #add-point:單身新增後 name="input.body.a_insert"
               
               #end add-point
            ELSE    
               INITIALIZE g_mham_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
