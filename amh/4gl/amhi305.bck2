            ELSE
               LET l_cmd='a'
            END IF
            CALL amhi305_set_entry_b(l_cmd)
            CALL amhi305_set_no_entry_b(l_cmd)
            #add-point:modify段before row name="input.body.before_row"
            CALL amhi305_set_entry_b("a")
            CALL amhi305_set_no_entry_b("a")
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
LET g_detail_multi_table_t.mhaml001 = g_mham_d[l_ac].mham001
LET g_detail_multi_table_t.mhaml002 = g_dlang
LET g_detail_multi_table_t.mhaml003 = g_mham_d[l_ac].mhaml003
LET g_detail_multi_table_t.mhaml004 = g_mham_d[l_ac].mhaml004
 
 
            #其他table進行lock
            
            INITIALIZE l_var_keys TO NULL 
            INITIALIZE l_field_keys TO NULL 
            LET l_field_keys[01] = 'mhamlent'
            LET l_var_keys[01] = g_enterprise
            LET l_field_keys[02] = 'mhaml001'
            LET l_var_keys[02] = g_mham_d[l_ac].mham001
            LET l_field_keys[03] = 'mhaml002'
            LET l_var_keys[03] = g_dlang
            IF NOT cl_multitable_lock(l_var_keys,l_field_keys,'mhaml_t') THEN
               RETURN 
            END IF 
 
 
        
         BEFORE INSERT
            
            LET l_insert = TRUE
            IF s_transaction_chk("N",0) THEN
               CALL s_transaction_begin()
            END IF
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_mham_d_t.* TO NULL
            INITIALIZE g_mham_d_o.* TO NULL
            INITIALIZE g_mham_d[l_ac].* TO NULL 
