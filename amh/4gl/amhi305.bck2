                   g_mham_d[l_ac].mham006,g_mham_d[l_ac].mham007,g_mham_d[l_ac].mham008,g_mham_d[l_ac].mham009, 
                   g_mham_d[l_ac].mham010,g_mham_d[l_ac].mham011,g_mham_d[l_ac].mham012,g_mham_d[l_ac].mham013, 
                   g_mham2_d[l_ac].mhamownid,g_mham2_d[l_ac].mhamowndp,g_mham2_d[l_ac].mhamcrtid,g_mham2_d[l_ac].mhamcrtdp, 
                   g_mham2_d[l_ac].mhamcrtdt,g_mham2_d[l_ac].mhammodid,g_mham2_d[l_ac].mhammoddt,g_mham2_d[l_ac].mhamcnfid, 
                   g_mham2_d[l_ac].mhamcnfdt)
                WHERE mhament = g_enterprise AND
                  mham001 = g_mham_d_t.mham001 #項次   
 
                  
               #add-point:單身修改中 name="input.body.m_update"
               
               #end add-point
                  
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL s_transaction_end('N','0')
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "mham_t" 
                     LET g_errparam.code   = "std-00009" 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                  WHEN SQLCA.sqlcode #其他錯誤
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "mham_t:",SQLERRMESSAGE 
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL s_transaction_end('N','0')
                     CALL cl_err()
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_mham_d[g_detail_idx].mham001
               LET gs_keys_bak[1] = g_mham_d_t.mham001
               CALL amhi305_update_b('mham_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                              INITIALIZE l_var_keys TO NULL 
         INITIALIZE l_field_keys TO NULL 
         INITIALIZE l_vars TO NULL 
         INITIALIZE l_fields TO NULL 
         INITIALIZE l_var_keys_bak TO NULL 
         IF g_mham_d[l_ac].mham001 = g_detail_multi_table_t.mhaml001 AND
         g_mham_d[l_ac].mhaml003 = g_detail_multi_table_t.mhaml003 AND
         g_mham_d[l_ac].mhaml004 = g_detail_multi_table_t.mhaml004 THEN
         ELSE 
            LET l_var_keys[01] = g_enterprise
            LET l_field_keys[01] = 'mhamlent'
            LET l_var_keys_bak[01] = g_enterprise
            LET l_var_keys[02] = g_mham_d[l_ac].mham001
            LET l_field_keys[02] = 'mhaml001'
            LET l_var_keys_bak[02] = g_detail_multi_table_t.mhaml001
            LET l_var_keys[03] = g_dlang
            LET l_field_keys[03] = 'mhaml002'
            LET l_var_keys_bak[03] = g_detail_multi_table_t.mhaml002
            LET l_vars[01] = g_mham_d[l_ac].mhaml003
            LET l_fields[01] = 'mhaml003'
            LET l_vars[02] = g_mham_d[l_ac].mhaml004
            LET l_fields[02] = 'mhaml004'
            CALL cl_multitable(l_var_keys,l_field_keys,l_vars,l_fields,l_var_keys_bak,'mhaml_t')
         END IF 
 
                     #修改歷程記錄(修改)
                     LET g_log1 = util.JSON.stringify(g_mham_d_t)
                     LET g_log2 = util.JSON.stringify(g_mham_d[l_ac])
                     IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                        CALL s_transaction_end('N','0')
                     END IF
               END CASE
               
               #將遮罩欄位進行遮蔽
               CALL amhi305_mham_t_mask_restore('restore_mask_n')
               
               #add-point:單身修改後 name="input.body.a_update"
               
               #end add-point
 
            END IF
            
         AFTER ROW
            CALL amhi305_unlock_b("mham_t")
            IF INT_FLAG THEN
               CALL s_transaction_end('N','0')
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               IF l_cmd = 'u' THEN
                  LET g_mham_d[l_ac].* = g_mham_d_t.*
               END IF
               #add-point:單身after row name="input.body.a_close"
               
               #end add-point
            ELSE
               CALL s_transaction_end('Y','0')
            END IF
            #其他table進行unlock
            CALL cl_multitable_unlock()
             #add-point:單身after row name="input.body.a_row"
            
            #end add-point
            
         AFTER INPUT
            #add-point:單身input後 name="input.body.a_input"
            
            #end add-point
            #錯誤訊息統整顯示
            #CALL cl_err_collect_show()
            #CALL cl_showmsg()
            
         ON ACTION controlo   
            IF l_insert THEN
               LET li_reproduce = l_ac_t
               LET li_reproduce_target = l_ac
               LET g_mham_d[li_reproduce_target].* = g_mham_d[li_reproduce].*
               LET g_mham2_d[li_reproduce_target].* = g_mham2_d[li_reproduce].*
 
               LET g_mham_d[li_reproduce_target].mham001 = NULL
 
            ELSE
               CALL FGL_SET_ARR_CURR(g_mham_d.getLength()+1)
               LET lb_reproduce = TRUE
               LET li_reproduce = l_ac
               LET li_reproduce_target = g_mham_d.getLength()+1
            END IF
            
      END INPUT
      
 
      
      DISPLAY ARRAY g_mham2_d TO s_detail2.*
         ATTRIBUTES(COUNT=g_detail_cnt)  
      
         BEFORE DISPLAY 
            CALL amhi305_b_fill(g_wc2)
            CALL FGL_SET_ARR_CURR(g_detail_idx)
      
         BEFORE ROW
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail2")
            LET l_ac = g_detail_idx
            LET g_temp_idx = l_ac
            DISPLAY g_detail_idx TO FORMONLY.idx
            CALL cl_show_fld_cont() 
            
         #add-point:page2自定義行為 name="input.body2.action"
         
         #end add-point
            
      END DISPLAY
 
      
      #add-point:before_more_input name="input.more_input"
      
      #end add-point
      
      BEFORE DIALOG
         #CALL cl_err_collect_init()      
         IF g_temp_idx > 0 THEN
            LET l_ac = g_temp_idx
            CALL DIALOG.setCurrentRow("s_detail1",l_ac)
            LET g_temp_idx = 1
         END IF
         #LET g_curr_diag = ui.DIALOG.getCurrent()
         #add-point:before_dialog name="input.before_dialog"
         
         #end add-point
         CASE g_aw
            WHEN "s_detail1"
               NEXT FIELD mhamstus
            WHEN "s_detail2"
               NEXT FIELD mham001_2
 
         END CASE
   
      ON ACTION accept
         ACCEPT DIALOG
      
      ON ACTION cancel
         LET INT_FLAG = TRUE 
         CANCEL DIALOG
 
      ON ACTION controlr
         CALL cl_show_req_fields()
 
      ON ACTION controlf
         CALL cl_set_focus_form(ui.Interface.getRootNode()) 
              RETURNING g_fld_name,g_frm_name 
         CALL cl_fldhelp(g_frm_name,g_fld_name,g_lang) 
           
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE DIALOG
   
   END DIALOG 
    
   #新增後取消
   IF l_cmd = 'a' THEN
      #當取消或無輸入資料按確定時刪除對應資料
      IF INT_FLAG OR cl_null(g_mham_d[g_detail_idx].mham001) THEN
         CALL g_mham_d.deleteElement(g_detail_idx)
         CALL g_mham2_d.deleteElement(g_detail_idx)
 
      END IF
   END IF
   
   #修改後取消
   IF l_cmd = 'u' AND INT_FLAG THEN
      LET g_mham_d[g_detail_idx].* = g_mham_d_t.*
   END IF
   
   #add-point:modify段修改後 name="modify.after_input"
   
   #end add-point
 
   CLOSE amhi305_bcl
   CALL s_transaction_end('Y','0')
   
END FUNCTION
 
