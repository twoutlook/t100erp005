      LET g_mham2_d[l_ac].mhamownid = g_user
      LET g_mham2_d[l_ac].mhamowndp = g_dept
      LET g_mham2_d[l_ac].mhamcrtid = g_user
      LET g_mham2_d[l_ac].mhamcrtdp = g_dept 
