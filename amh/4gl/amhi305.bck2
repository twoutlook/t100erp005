            #END add-point
 
 
         #應用 a02 樣板自動產生(Version:2)
         AFTER FIELD mhamstus
            
            #add-point:AFTER FIELD mhamstus name="input.a.page1.mhamstus"
            
            #END add-point
            
 
 
         #應用 a04 樣板自動產生(Version:3)
         ON CHANGE mhamstus
            #add-point:ON CHANGE mhamstus name="input.g.page1.mhamstus"
            
            #END add-point 
 
 
         #應用 a01 樣板自動產生(Version:2)
         BEFORE FIELD mham001
            #add-point:BEFORE FIELD mham001 name="input.b.page1.mham001"
            
            #END add-point
 
 
         #應用 a02 樣板自動產生(Version:2)
         AFTER FIELD mham001
            
            #add-point:AFTER FIELD mham001 name="input.a.page1.mham001"
            #應用 a05 樣板自動產生(Version:2)
            #確認資料無重複
            IF  g_mham_d[g_detail_idx].mham001 IS NOT NULL THEN 
               IF l_cmd = 'a' OR ( l_cmd = 'u' AND (g_mham_d[g_detail_idx].mham001 != g_mham_d_t.mham001)) THEN 
                  IF NOT ap_chk_notDup("","SELECT COUNT(*) FROM mham_t WHERE "||"mhament = '" ||g_enterprise|| "' AND "||"mham001 = '"||g_mham_d[g_detail_idx].mham001 ||"'",'std-00004',0) THEN 
                     NEXT FIELD CURRENT
                  END IF
               END IF
            END IF


            #END add-point
            
 
 
         #應用 a04 樣板自動產生(Version:3)
         ON CHANGE mham001
