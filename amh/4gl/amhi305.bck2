{<section id="amhi305.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION amhi305_b_fill(p_wc2)              #BODY FILL UP
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   
   #end add-point
   DEFINE p_wc2    STRING
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   
   #end add-point
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前 name="b_fill.sql_before"
   
   #end add-point
 
   LET g_sql = "SELECT  DISTINCT t0.mhamstus,t0.mham001,t0.mham002,t0.mham003,t0.mham004,t0.mham005, 
       t0.mham006,t0.mham007,t0.mham008,t0.mham009,t0.mham010,t0.mham011,t0.mham012,t0.mham013,t0.mham001, 
       t0.mhamownid,t0.mhamowndp,t0.mhamcrtid,t0.mhamcrtdp,t0.mhamcrtdt,t0.mhammodid,t0.mhammoddt,t0.mhamcnfid, 
       t0.mhamcnfdt ,t1.ooefl003 ,t2.mhael023 ,t3.ooefl003 ,t4.rtaxl003 ,t5.ooag011 ,t6.ooefl003 ,t7.ooag011 , 
       t8.ooefl003 ,t9.ooag011 ,t10.ooag011 FROM mham_t t0",
               " LEFT JOIN mhaml_t ON mhamlent = "||g_enterprise||" AND mham001 = mhaml001 AND mhaml002 = '",g_dlang,"'",
                              " LEFT JOIN ooefl_t t1 ON t1.ooeflent="||g_enterprise||" AND t1.ooefl001=t0.mham005 AND t1.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN mhael_t t2 ON t2.mhaelent="||g_enterprise||" AND t2.mhael001=t0.mham006 AND t2.mhael022='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent="||g_enterprise||" AND t3.ooefl001=t0.mham007 AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN rtaxl_t t4 ON t4.rtaxlent="||g_enterprise||" AND t4.rtaxl001=t0.mham010 AND t4.rtaxl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=t0.mhamownid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent="||g_enterprise||" AND t6.ooefl001=t0.mhamowndp AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent="||g_enterprise||" AND t7.ooag001=t0.mhamcrtid  ",
               " LEFT JOIN ooefl_t t8 ON t8.ooeflent="||g_enterprise||" AND t8.ooefl001=t0.mhamcrtdp AND t8.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t9 ON t9.ooagent="||g_enterprise||" AND t9.ooag001=t0.mhammodid  ",
               " LEFT JOIN ooag_t t10 ON t10.ooagent="||g_enterprise||" AND t10.ooag001=t0.mhamcnfid  ",
 
               " WHERE t0.mhament= ?  AND  1=1 AND (", p_wc2, ") " 
   #add-point:b_fill段sql wc name="b_fill.sql_wc"
   
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("mham_t"),
                      " ORDER BY t0.mham001"
   
   #add-point:b_fill段sql之後 name="b_fill.sql_after"
   
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"mham_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE amhi305_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR amhi305_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_mham_d.clear()
   CALL g_mham2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_mham_d[l_ac].mhamstus,g_mham_d[l_ac].mham001,g_mham_d[l_ac].mham002,g_mham_d[l_ac].mham003, 
       g_mham_d[l_ac].mham004,g_mham_d[l_ac].mham005,g_mham_d[l_ac].mham006,g_mham_d[l_ac].mham007,g_mham_d[l_ac].mham008, 
       g_mham_d[l_ac].mham009,g_mham_d[l_ac].mham010,g_mham_d[l_ac].mham011,g_mham_d[l_ac].mham012,g_mham_d[l_ac].mham013, 
       g_mham2_d[l_ac].mham001,g_mham2_d[l_ac].mhamownid,g_mham2_d[l_ac].mhamowndp,g_mham2_d[l_ac].mhamcrtid, 
       g_mham2_d[l_ac].mhamcrtdp,g_mham2_d[l_ac].mhamcrtdt,g_mham2_d[l_ac].mhammodid,g_mham2_d[l_ac].mhammoddt, 
       g_mham2_d[l_ac].mhamcnfid,g_mham2_d[l_ac].mhamcnfdt,g_mham_d[l_ac].mham005_desc,g_mham_d[l_ac].mham006_desc, 
       g_mham_d[l_ac].mham007_desc,g_mham_d[l_ac].mham010_desc,g_mham2_d[l_ac].mhamownid_desc,g_mham2_d[l_ac].mhamowndp_desc, 
       g_mham2_d[l_ac].mhamcrtid_desc,g_mham2_d[l_ac].mhamcrtdp_desc,g_mham2_d[l_ac].mhammodid_desc, 
       g_mham2_d[l_ac].mhamcnfid_desc
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充 name="b_fill.fill"
      
      #end add-point
      
      CALL amhi305_detail_show()      
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
 
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   LET g_error_show = 0
   
 
   
   CALL g_mham_d.deleteElement(g_mham_d.getLength())   
   CALL g_mham2_d.deleteElement(g_mham2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_mham_d.getLength()
      LET g_mham2_d[l_ac].mham001 = g_mham_d[l_ac].mham001 
 
      #add-point:b_fill段key值相關欄位 name="b_fill.keys.fill"
      
      #end add-point
   END FOR
   
   IF g_cnt > g_mham_d.getLength() THEN
      LET l_ac = g_mham_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_mham_d.getLength()
      LET g_mham_d_mask_o[l_ac].* =  g_mham_d[l_ac].*
      CALL amhi305_mham_t_mask()
      LET g_mham_d_mask_n[l_ac].* =  g_mham_d[l_ac].*
   END FOR
   
   LET g_mham2_d_mask_o.* =  g_mham2_d.*
   FOR l_ac = 1 TO g_mham2_d.getLength()
      LET g_mham2_d_mask_o[l_ac].* =  g_mham2_d[l_ac].*
      CALL amhi305_mham_t_mask()
      LET g_mham2_d_mask_n[l_ac].* =  g_mham2_d[l_ac].*
   END FOR
 
   
   LET l_ac = g_cnt
 
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_mham_d.getLength()
   DISPLAY g_detail_idx TO FORMONLY.idx
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE amhi305_pb
   
END FUNCTION
 
