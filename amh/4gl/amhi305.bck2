#+ QBE資料查詢
PRIVATE FUNCTION amhi305_query()
   #add-point:query段define(客製用) name="query.define_customerization"
   
   #end add-point
   DEFINE ls_wc      LIKE type_t.chr500
   DEFINE ls_return  STRING
   DEFINE ls_result  STRING 
   #add-point:query段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="query.define"
   
   #end add-point 
   
   #add-point:Function前置處理  name="query.pre_function"
   
   #end add-point
   
   LET INT_FLAG = 0
   CLEAR FORM
   CALL g_mham_d.clear()
   
   LET g_qryparam.state = "c"
   
   #wc備份
   LET ls_wc = g_wc2
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      CONSTRUCT g_wc2 ON mhamstus,mham001,mhaml003,mhaml004,mham002,mham003,mham004,mham005,mham006, 
          mham007,mham008,mham009,mham010,mham011,mham012,mham013,mhamownid,mhamowndp,mhamcrtid,mhamcrtdp, 
          mhamcrtdt,mhammodid,mhammoddt,mhamcnfid,mhamcnfdt 
 
         FROM s_detail1[1].mhamstus,s_detail1[1].mham001,s_detail1[1].mhaml003,s_detail1[1].mhaml004, 
             s_detail1[1].mham002,s_detail1[1].mham003,s_detail1[1].mham004,s_detail1[1].mham005,s_detail1[1].mham006, 
             s_detail1[1].mham007,s_detail1[1].mham008,s_detail1[1].mham009,s_detail1[1].mham010,s_detail1[1].mham011, 
             s_detail1[1].mham012,s_detail1[1].mham013,s_detail2[1].mhamownid,s_detail2[1].mhamowndp, 
             s_detail2[1].mhamcrtid,s_detail2[1].mhamcrtdp,s_detail2[1].mhamcrtdt,s_detail2[1].mhammodid, 
             s_detail2[1].mhammoddt,s_detail2[1].mhamcnfid,s_detail2[1].mhamcnfdt 
      
         #應用 a11 樣板自動產生(Version:3)
         #共用欄位查詢處理  
         ##----<<mhamcrtdt>>----
         AFTER FIELD mhamcrtdt
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
 
         #----<<mhammoddt>>----
         AFTER FIELD mhammoddt
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
         
         #----<<mhamcnfdt>>----
         AFTER FIELD mhamcnfdt
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
