       g_mhaf_m.mhafcrtid,g_mhaf_m.mhafcrtid_desc,g_mhaf_m.mhafcrtdp,g_mhaf_m.mhafcrtdp_desc,g_mhaf_m.mhafcrtdt, 
       g_mhaf_m.mhafmodid,g_mhaf_m.mhafmodid_desc,g_mhaf_m.mhafmoddt,g_mhaf_m.mhafcnfid,g_mhaf_m.mhafcnfid_desc, 
       g_mhaf_m.mhafcnfdt
 
