         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE mhaf_t SET (mhafmodid,mhafmoddt) = (g_mhaf_m.mhafmodid,g_mhaf_m.mhafmoddt)
       WHERE mhafent = g_enterprise AND mhaf001 = g_mhaf001_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL amhm310_set_act_visible()
   CALL amhm310_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " mhafent = '" ||g_enterprise|| "' AND",
                      " mhaf001 = '", g_mhaf_m.mhaf001, "' "
 
   #填到對應位置
   CALL amhm310_browser_fill(g_wc,"")
 
   CLOSE amhm310_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL amhm310_msgcentre_notify('modify')
   
   LET g_worksheet_hidden = 0
   
