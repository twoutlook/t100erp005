PRIVATE FUNCTION amhm310_upd(p_mhafsite,p_mhaf001,p_mhaf002,p_mhaf005,p_mhaf012,p_docno)

DEFINE p_mhafsite   LIKE mhaf_t.mhafsite 
DEFINE p_mhaf001    LIKE mhaf_t.mhaf001
DEFINE p_mhaf002    LIKE mhaf_t.mhaf002
DEFINE p_mhaf005    LIKE mhaf_t.mhaf005
DEFINE p_mhaf012    LIKE mhaf_t.mhaf012
DEFINE p_docno      LIKE mhaf_t.mhaf014
DEFINE r_success    LIKE type_t.num5
DEFINE l_sql        STRING

       LET r_success = TRUE 
       
       LET l_sql="UPDATE mhaf_t SET mhaf014 = '",p_docno,"' ",
                 #"            WHERE mhafsite = '",p_mhafsite,"'",   #160905-00003#6 20160905 mark by beckxie
                 "            WHERE mhafent = ",g_enterprise,       #160905-00003#6 20160905 add by beckxie
                 "              AND mhafsite = '",p_mhafsite,"'",   #160905-00003#6 20160905 add by beckxie
                 "              AND mhaf001 = '",p_mhaf001,"'",
                 "              AND mhaf002 = '",p_mhaf002,"'",
                 "              AND mhaf005 = '",p_mhaf005,"'",
                 "              AND mhaf012 = '",p_mhaf012,"'"
                 
                 
       PREPARE amhm310_upd1 FROM l_sql
       EXECUTE amhm310_upd1
       
       IF SQLCA.sqlcode OR SQLCA.sqlerrd[3]<=0 THEN
          INITIALIZE g_errparam TO NULL 
          LET g_errparam.code = SQLCA.sqlcode
          LET g_errparam.extend ="upd mhaf_t"
          LET g_errparam.popup = TRUE
          CALL cl_err()
          LET r_success = FALSE
       END IF 

       RETURN r_success


END FUNCTION

