# Modify.........:
################################################################################
PRIVATE FUNCTION amhm310_chk_mhaf003()
DEFINE l_n      LIKE type_t.num5

   #判斷是否在職狀態
   SELECT COUNT(*) INTO l_n
     FROM mhaf_t
    WHERE mhafent = g_enterprise
      AND mhaf003 = g_mhaf_m.mhaf003
      AND mhaf011 = '1'
      AND mhafstus = 'Y'
   IF l_n > 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.extend = g_mhaf_m.mhaf003
      LET g_errparam.code   = 'amh-00031'
      LET g_errparam.popup  = TRUE
      CALL cl_err()
      RETURN FALSE
   END IF
   #判斷是否納入黑名單
   SELECT COUNT(*) INTO l_n
     FROM mhag_t
    WHERE mhagent = g_enterprise
      AND mhag002 = g_mhaf_m.mhaf003
      AND mhag007 = 'Y'
      AND mhagstus = 'Y'
   IF l_n > 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.extend = g_mhaf_m.mhaf003
      LET g_errparam.code   = 'amh-00032'
      LET g_errparam.popup  = TRUE
      CALL cl_err()
      RETURN FALSE
   END IF
   
   RETURN TRUE
END FUNCTION

