   END IF
   
   #顯示最新的資料
   EXECUTE amhm310_master_referesh USING g_mhaf_m.mhaf001 INTO g_mhaf_m.mhafsite,g_mhaf_m.mhaf001,g_mhaf_m.mhaf002, 
       g_mhaf_m.mhaf003,g_mhaf_m.mhaf004,g_mhaf_m.mhaf005,g_mhaf_m.mhaf006,g_mhaf_m.mhaf017,g_mhaf_m.mhaf007, 
       g_mhaf_m.mhaf008,g_mhaf_m.mhaf009,g_mhaf_m.mhaf010,g_mhaf_m.mhaf011,g_mhaf_m.mhaf018,g_mhaf_m.mhaf012,g_mhaf_m.mhaf019,g_mhaf_m.mhaf013, 
       g_mhaf_m.mhaf014,g_mhaf_m.mhaf015,g_mhaf_m.mhaf016,g_mhaf_m.mhafunit,g_mhaf_m.mhafstus,g_mhaf_m.mhafownid, 
       g_mhaf_m.mhafowndp,g_mhaf_m.mhafcrtid,g_mhaf_m.mhafcrtdp,g_mhaf_m.mhafcrtdt,g_mhaf_m.mhafmodid, 
       g_mhaf_m.mhafmoddt,g_mhaf_m.mhafcnfid,g_mhaf_m.mhafcnfdt,g_mhaf_m.mhafsite_desc,g_mhaf_m.mhaf004_desc, 
       g_mhaf_m.mhaf019_desc,g_mhaf_m.mhaf013_desc,g_mhaf_m.mhafownid_desc,g_mhaf_m.mhafowndp_desc,g_mhaf_m.mhafcrtid_desc, 
       g_mhaf_m.mhafcrtdp_desc,g_mhaf_m.mhafmodid_desc,g_mhaf_m.mhafcnfid_desc
 
   #檢查是否允許此動作
   IF NOT amhm310_action_chk() THEN
      CLOSE amhm310_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_mhaf_m.mhafsite,g_mhaf_m.mhafsite_desc,g_mhaf_m.mhaf001,g_mhaf_m.mhaf002,g_mhaf_m.mhaf003, 
       g_mhaf_m.mhaf004,g_mhaf_m.mhaf004_desc,g_mhaf_m.mhaf005,g_mhaf_m.mhaf006,g_mhaf_m.mhaf017,g_mhaf_m.mhaf007, 
