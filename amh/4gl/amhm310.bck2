       g_mhaf_m.mhaf008,g_mhaf_m.mhaf009,g_mhaf_m.mhaf010,g_mhaf_m.mhaf011,g_mhaf_m.mhaf012,g_mhaf_m.mhaf012_desc, 
       g_mhaf_m.mhaf018,g_mhaf_m.mhaf019,g_mhaf_m.mhaf019_desc,g_mhaf_m.mhaf013,g_mhaf_m.mhaf013_desc,g_mhaf_m.mhaf014,g_mhaf_m.mhaf015,g_mhaf_m.mhaf016,g_mhaf_m.mhafunit, 
       g_mhaf_m.mhafstus,g_mhaf_m.mhafownid,g_mhaf_m.mhafownid_desc,g_mhaf_m.mhafowndp,g_mhaf_m.mhafowndp_desc, 
       g_mhaf_m.mhafcrtid,g_mhaf_m.mhafcrtid_desc,g_mhaf_m.mhafcrtdp,g_mhaf_m.mhafcrtdp_desc,g_mhaf_m.mhafcrtdt, 
       g_mhaf_m.mhafmodid,g_mhaf_m.mhafmodid_desc,g_mhaf_m.mhafmoddt,g_mhaf_m.mhafcnfid,g_mhaf_m.mhafcnfid_desc, 
       g_mhaf_m.mhafcnfdt
   
   CALL cl_set_head_visible("","YES")  
   
