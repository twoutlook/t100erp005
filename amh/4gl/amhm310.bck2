{<section id="amhm310.browser_fill" >}
#應用 a29 樣板自動產生(Version:7)
#+ 瀏覽頁簽資料填充(一般單檔)
PRIVATE FUNCTION amhm310_browser_fill(p_wc,ps_page_action) 
   DEFINE p_wc              STRING
   DEFINE ls_wc             STRING
   DEFINE ps_page_action    STRING
   DEFINE l_searchcol       STRING
   DEFINE l_sql             STRING
   DEFINE l_sql_rank        STRING
   #add-point:browser_fill段define
   DEFINE l_where           STRING
   #end add-point
   #add-point:browser_fill段define

   #end add-point
   
   LET l_searchcol = "mhaf001"
 
   LET p_wc = p_wc.trim() #當查詢按下Q時 按下放棄 g_wc = "  " 所以要清掉空白
   IF cl_null(p_wc) THEN  #p_wc 查詢條件
      LET p_wc = " 1=1 " 
   END IF
   #add-point:browser_fill段wc控制
   CALL s_aooi500_sql_where(g_prog,'mhafsite') RETURNING l_where
   LET p_wc = p_wc," AND ",l_where
   #end add-point
 
   LET g_sql = " SELECT COUNT(*) FROM mhaf_t ",
               "  ",
               "  ",
               " WHERE mhafent = '" ||g_enterprise|| "' AND ", 
               p_wc CLIPPED, cl_sql_add_filter("mhaf_t")
                
   #add-point:browser_fill段cnt_sql

   #end add-point
                
   PREPARE header_cnt_pre FROM g_sql
   EXECUTE header_cnt_pre INTO g_browser_cnt
   FREE header_cnt_pre 
   
   #若超過最大顯示筆數
   IF g_browser_cnt > g_max_browse THEN
      IF g_error_show = 1 THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = g_browser_cnt 
         LET g_errparam.code   = 9035
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
      END IF
   END IF
   
   LET g_error_show = 0
   
   IF ps_page_action = "F" OR
      ps_page_action = "P"  OR
      ps_page_action = "N"  OR
      ps_page_action = "L"  THEN
      LET g_page_action = ps_page_action
   END IF
   
   IF cl_null(g_add_browse) THEN
      #清除畫面
      CLEAR FORM
      INITIALIZE g_mhaf_m.* TO NULL
      CALL g_browser.clear()
      LET g_cnt = 1
      LET ls_wc = p_wc
   ELSE
      LET ls_wc = g_add_browse
      LET g_cnt = g_current_idx
   END IF
   
   LET g_sql = " SELECT t0.mhafstus,t0.mhafsite,t0.mhaf001,t0.mhaf002,t0.mhaf003,t0.mhaf004,t0.mhaf005, 
       t0.mhaf006,t0.mhaf007,t0.mhaf008,t0.mhaf009,t0.mhaf010,t0.mhaf011,t0.mhaf018,t0.mhaf012,t0.mhaf013,t0.mhaf014, 
       t0.mhaf015,t0.mhaf017",
               " FROM mhaf_t t0 ",
               "  ",
               
               " WHERE t0.mhafent = '" ||g_enterprise|| "' AND ", ls_wc, cl_sql_add_filter("mhaf_t")
   #add-point:browser_fill段fill_wc

   #end add-point 
   LET g_sql = g_sql, " ORDER BY ",l_searchcol," ",g_order
   #add-point:browser_fill段before_pre

   #end add-point                    
 
   #LET g_sql = cl_sql_add_tabid(g_sql,"mhaf_t")             #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE browse_pre FROM g_sql
   DECLARE browse_cur CURSOR FOR browse_pre
 
   #CALL g_browser.clear()
   #LET g_cnt = 1
   FOREACH browse_cur INTO g_browser[g_cnt].b_statepic,g_browser[g_cnt].b_mhafsite,g_browser[g_cnt].b_mhaf001, 
       g_browser[g_cnt].b_mhaf002,g_browser[g_cnt].b_mhaf003,g_browser[g_cnt].b_mhaf004,g_browser[g_cnt].b_mhaf005, 
       g_browser[g_cnt].b_mhaf006,g_browser[g_cnt].b_mhaf007,g_browser[g_cnt].b_mhaf008,g_browser[g_cnt].b_mhaf009, 
       g_browser[g_cnt].b_mhaf010,g_browser[g_cnt].b_mhaf011,g_browser[g_cnt].b_mhaf018,g_browser[g_cnt].b_mhaf012,g_browser[g_cnt].b_mhaf013, 
       g_browser[g_cnt].b_mhaf014,g_browser[g_cnt].b_mhaf015,g_browser[g_cnt].b_mhaf017
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "foreach:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      
      
      #add-point:browser_fill段reference

      #end add-point
      
      #遮罩相關處理
      CALL amhm310_browser_mask()
      
            #應用 a24 樣板自動產生(Version:2)
      #browser顯示圖片
      CASE g_browser[g_cnt].b_statepic
         WHEN "N"
            LET g_browser[g_cnt].b_statepic = "stus/16/open.png"
         WHEN "Y"
            LET g_browser[g_cnt].b_statepic = "stus/16/valid.png"
         WHEN "X"
            LET g_browser[g_cnt].b_statepic = "stus/16/void.png"
         
      END CASE
 
 
      LET g_cnt = g_cnt + 1
      IF g_cnt > g_max_rec THEN
         EXIT FOREACH
      END IF
   END FOREACH
 
   #清空g_add_browse, 並指定指標位置
   IF NOT cl_null(g_add_browse) THEN
      LET g_add_browse = ""
      CALL g_curr_diag.setCurrentRow("s_browse",g_current_idx)
   END IF
   
   IF cl_null(g_browser[g_cnt].b_mhaf001) THEN
      CALL g_browser.deleteElement(g_cnt)
   END IF
   
   LET g_header_cnt = g_browser.getLength()
   LET g_current_cnt = g_browser.getLength()
   LET g_browser_cnt = g_browser.getLength()
   LET g_rec_b = g_browser.getLength()
   LET g_cnt = 0
   DISPLAY g_browser_cnt TO FORMONLY.b_count
   DISPLAY g_browser_cnt TO FORMONLY.h_count
   
   #CALL amhm310_fetch("") 
   
   FREE browse_pre
   
   #若無資料則關閉相關功能
   IF g_browser_cnt = 0 THEN
      CALL cl_set_act_visible("statechange,modify,delete,reproduce,mainhidden", FALSE)
      CALL cl_navigator_setting(0,0)
   ELSE
      CALL cl_set_act_visible("mainhidden", TRUE)
   END IF
   
   #add-point:browser_fill段結束前
   CALL amhm310_set_act_visible()
   CALL amhm310_set_act_no_visible()
   #end add-point   
   
END FUNCTION
 
 
 
