               NEXT FIELD CURRENT
            ELSE
               CALL s_aooi500_chk(g_prog,'mhafsite',g_mhaf_m.mhafsite,g_mhaf_m.mhafsite) RETURNING l_success,l_errno
               IF NOT l_success THEN
                  INITIALIZE g_errparam TO NULL
