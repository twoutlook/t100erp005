                  LET g_errparam.extend = "" 
                  LET g_errparam.code   = SQLCA.sqlcode 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
         ELSE
            CALL s_transaction_end('N','0')
            RETURN
         END IF
      WHEN lc_state = 'X'
         IF NOT cl_ask_confirm('art-00039') THEN
            CALL s_transaction_end('N','0')
            RETURN
         END IF
      OTHERWISE
         CALL s_transaction_end('N','0')   #160816-00068#7
         RETURN
   END CASE
   #end add-point
   
   LET g_mhaf_m.mhafmodid = g_user
   LET g_mhaf_m.mhafmoddt = cl_get_current()
   LET g_mhaf_m.mhafstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE mhaf_t 
      SET (mhafstus,mhafmodid,mhafmoddt) 
        = (g_mhaf_m.mhafstus,g_mhaf_m.mhafmodid,g_mhaf_m.mhafmoddt)     
    WHERE mhafent = g_enterprise AND mhaf001 = g_mhaf_m.mhaf001
 
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
