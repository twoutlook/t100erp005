################################################################################
PRIVATE FUNCTION amhm310_ins_stba()
DEFINE l_stba   RECORD LIKE stba_t.*
DEFINE l_stbb   RECORD LIKE stbb_t.*
DEFINE l_doctype      LIKE type_t.chr10
DEFINE l_success      LIKE type_t.num5
DEFINE r_success      LIKE type_t.num5  
DEFINE r_docno        LIKE type_t.num5
DEFINE l_ooef017      LIKE ooef_t.ooef017
DEFINE l_glaa003      LIKE glaa_t.glaa003
DEFINE l_flag         LIKE type_t.chr1
DEFINE l_errno        LIKE type_t.chr100 
DEFINE l_glav005      LIKE glav_t.glav005
DEFINE l_sdate_s      LIKE type_t.chr500
DEFINE l_sdate_e     LIKE type_t.chr500
DEFINE l_pdate_s      LIKE type_t.chr500
DEFINE l_pdate_e      LIKE type_t.chr500
DEFINE l_glav007      LIKE glav_t.glav007
DEFINE l_wdate_s      LIKE type_t.chr500
DEFINE l_wdate_e      LIKE type_t.chr500
DEFINE l_glav002_1    LIKE glav_t.glav002
DEFINE l_glav002      LIKE glav_t.glav002
DEFINE l_glav006_1    LIKE glav_t.glav006
DEFINE l_glav006      LIKE glav_t.glav006
             
   LET r_success=TRUE
  
   #取astt322预设单别
   CALL s_arti200_get_def_doc_type(g_mhaf_m.mhafsite,'astt322','1') RETURNING l_success,l_doctype
   IF NOT l_success THEN
      LET r_success = FALSE
      RETURN r_success
   END IF
         
   #自动编号
   CALL s_aooi200_gen_docno(g_mhaf_m.mhafsite,l_doctype,g_today,'astt322') RETURNING l_success,l_stba.stbadocno
   IF NOT l_success THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'apm-00003'
      LET g_errparam.extend = l_doctype
      LET g_errparam.popup = TRUE
      CALL cl_err()
      LET r_success = FALSE
      RETURN r_success,r_docno
   END IF
   #产生单头
   LET l_stba.stbaent   = g_enterprise        #企业编号    
   LET l_stba.stbasite  = g_mhaf_m.mhafsite   #营运据点    
   LET l_stba.stbaunit  = g_mhaf_m.mhafsite   #应用组织    
   #LET l_stba.stbadocno = ''                  #单据编号   #mark by geza 20150906  
   LET l_stba.stbadocdt = g_today             #单据日期    
  #LET l_stba.stba001   = ''                  #结算中心    
  #LET l_stba.stba002   = ''                  #供应商编号  
   LET l_stba.stba003   = '1'                 #经营方式    
   LET l_stba.stba004   = ''                  #结算方式    
   LET l_stba.stba005   = ''                  #结算类型    
   LET l_stba.stba006   = '17'                #来源类型    
   LET l_stba.stba007   = g_mhaf_m.mhaf014    #来源单号    
   LET l_stba.stba008   = g_user              #人员        
   LET l_stba.stba009   = g_dept              #部门        
   LET l_stba.stbastus  = 'N'                 #状态码      
   LET l_stba.stbaownid = g_user              #资料所有者  
   LET l_stba.stbaowndp = g_dept              #资料所有部门
   LET l_stba.stbacrtid = g_user              #资料建立者  
   LET l_stba.stbacrtdp = g_dept              #资料建立部门
   LET l_stba.stbacrtdt = g_today             #资料创建日  
   LET l_stba.stbamodid = g_user              #资料修改者  
   LET l_stba.stbamoddt = cl_get_current()    #最近修改日  
   LET l_stba.stbacnfid = g_user              #资料确认者  
   LET l_stba.stbacnfdt = ''                  #数据确认日  
   #LET l_stba.stba010   = ''                  #合同编号   
   #抓合同编号
   SELECT stfa001 INTO  l_stba.stba010
     FROM stfa_t
    WHERE stfaent = g_enterprsie
      AND stfa005 = g_mhaf_m.mhaf012
      AND stfastus = 'Y'      
   LET l_stba.stba011   = ''                  #币别        
   LET l_stba.stba012   = ''                  #税别        
   LET l_stba.stba013   = g_mhaf_m.mhaf012    #专柜编号    
   LET l_stba.stba014   = '3'                 #费用类型    #add by geza 20150906 
   LET l_stba.stba015   = '1'                  #交款状态    
   LET l_stba.stba000   = 'astt322'                 #程序编号     #add by geza 20150906
   LET l_stba.stba016   = g_mhaf_m.mhaf002    #交款人      
#合同编号,税别,结算方式，结算类型
   SELECT stfa001,stfa033,stfa036,stfa037
     INTO l_stba.stba010,l_stba.stba012,l_stba.stba004,l_stba.stba005
     FROM stfa_t
    WHERE stfaent=g_enterprise
      AND stfa005=g_mhaf_m.mhaf012
#币别   
   SELECT stfa032 INTO l_stba.stba011
     FROM stfa_t
    WHERE stfaent=g_enterprise
      AND stfa001=l_stba.stba010


   INSERT INTO stba_t values l_stba.*
   IF SQLCA.sqlcode OR SQLCA.sqlerrd[3]<=0 THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend ="ins stba_t"
      LET g_errparam.popup = TRUE
      CALL cl_err()
      LET r_success = FALSE
   END IF 
  
  #产生单身
    LET l_stbb.stbbent=g_enterprise                 #add by geza 20150906  
    LET l_stbb.stbbdocno=l_stba.stbadocno            #单据编号  
    LET l_stbb.stbbseq  ='1'                           #项次      
    LET l_stbb.stbb001  =''                            #费用编号  
    LET l_stbb.stbb002  =l_stba.stba011              #币别      
    LET l_stbb.stbb003  =l_stba.stba012              #税别      
    LET l_stbb.stbb004  =''                          #价款类别  
    LET l_stbb.stbb005  =''                          #起始日期    
    LET l_stbb.stbb006  =''                          #截止日期    
    LET l_stbb.stbb007  =''                          #结算会计期
    LET l_stbb.stbb008  =''                          #财务会计期
    LET l_stbb.stbb009  =''                          #费用金额  
    #LET l_stbb.stbb010  =''                          #承担对象  #mark by geza 20150906
    LET l_stbb.stbb010  ='1'                          #承担对象  #add by geza 20150906
    #LET l_stbb.stbb011  =''                          #所属品类  #mark by geza 20150831
    #add by geza 20150831(S)
    #所属品类
    SELECT stfa051 INTO  l_stbb.stbb011    
      FROM stfa_t
     WHERE stfaent = g_enterprsie
       AND stfa001 = l_stba.stba010
       AND stfastus = 'Y'
    #add by geza 20150831(E)   
    LET l_stbb.stbb011  =''                          #所属品类  #mark by geza 20150831
    LET l_stbb.stbb012  =g_dept                      #所属部门  
    #LET l_stbb.stbb013  =''                         #结算对象  #mark by geza 20150906
    LET l_stbb.stbb013  ='1'                         #结算对象  #add by geza 20150906
    LET l_stbb.stbb014  =''                          #财务会计期别
    LET l_stbb.stbb015  =''                          #纳入结算单否
    LET l_stbb.stbb016  =''                          #票扣否
    LET l_stbb.stbb017  =''                          #备注
    #LET l_stbb.stbb018  =''                          #结算账期  #mark by geza 20150831
   
      
    #费用编号 
    SELECT stav002 INTO l_stbb.stbb001
      FROM stav_t
     WHERE stavent=g_enterprise
       AND stav001='amhm310'     
    #价款类别
    SELECT stae006 INTO l_stbb.stbb004
      FROM stae_t
     WHERE staeent=g_enterprise
       AND stae001=l_stbb001
    #费用金额
    SELECT stfq008 INTO l_stbb.stbb009
      FROM stfq_t
     WHERE stfqent=g_enterprise
       AND stfq003=g_mhaf_m.mhaf012
    #mark by geza 20150831(S)
    #结算账期
#   SELECT stfjseq INTO l_stbb.stbb018
#    FROM stfj_t
#   WHERE stfjent=g_enterprise
#     AND stfj001=l_stba.stba010
    #mark by geza 20150831(E)  
     #票扣否,纳入结算单否
   SELECT stfb019,stfb018 INTO l_stbb.stbb016,l_stbb.stbb015
     FROM stfb_t
    WHERE stfbent=g_enterprise
      AND stfb001=l_stba.stba010
      
   #查询資料  ---会计期别
    #抓取法人對應會計週期參照表
      LET l_ooef017 = NULL
      LET l_glaa003 = NULL     
      
     #法人     
     SELECT ooef017 INTO l_ooef017           
        FROM ooef_t
       WHERE ooefent = g_enterprise
         AND ooef001 = g_site
         
     #依据法人+运营组织+主账套--------会计周期参照表号
     SELECT glaa003 INTO l_glaa003
       FROM glaa_t
      WHERE glaaent = g_enterprise
        AND glaacomp = l_ooef017
        AND glaa014 = 'Y'
         
      #取得會計週期資料
      CALL s_get_accdate(l_glaa003,g_today,'','')
         RETURNING l_flag,l_errno,l_glav002,l_glav005,l_sdate_s,l_sdate_e,
                   l_glav006,l_pdate_s,l_pdate_e,l_glav007,l_wdate_s,l_wdate_e
      IF l_flag = 'Y' THEN
         LET l_glav002_1 = l_glav002           #年度glav002,会计期glav006
         LET l_glav006_1 = l_glav006 - 1       #取上一会计期       
         IF l_glav006_1 = 0 THEN               #若为第一个会计期，则去上一年会计期别最大的（即上一年最后一期）
            LET l_glav002_1 = l_glav002 - 1
            SELECT DISTINCT MAX(glav006) INTO l_glav006_1
              FROM glav_t
             WHERE glavent = g_enterprise
               AND glav001 = l_glaa003
               AND glav002 = l_glav002_1
         END IF        
         #抓取当前日期上一会计期的起讫日期l_pdate_s和l_pdate_e
         CALL s_get_accdate(l_glaa003,'',l_glav002_1,l_glav006_1)
            RETURNING l_flag,l_errno,l_glav002,l_glav005,l_sdate_s,l_sdate_e,
                      l_glav006,l_pdate_s,l_pdate_e,l_glav007,l_wdate_s,l_wdate_e                      
         LET l_stbb.stbb005 = l_pdate_s
         LET l_stbb.stbb006 = l_pdate_e
      END IF
      #add by geza 20150831(S)
      IF l_stbb.stbb005 IS NULL THEN
         LET l_stbb.stbb005 = g_today
      END IF
      IF l_stbb.stbb006 IS NULL THEN
         LET l_stbb.stbb006 = g_today
      END IF            
      #根据开始日期结束日期抓取结算账期和结算日期
      CALL s_settle_date_get_stbb019(l_stba.stba010,l_stbb.stbb005,l_stbb.stbb006,'2') 
         RETURNING  l_stbb.stbb018,l_stbb.stbb019  
      #add by geza 20150831(E)
      
      
      
    INSERT INTO stbb_t values l_stbb.*  
    IF SQLCA.sqlcode OR SQLCA.sqlerrd[3]<=0 THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend ="ins stbb_t"
      LET g_errparam.popup = TRUE
      CALL cl_err()
      LET r_success = FALSE
    END IF 
    
    RETURN r_success,l_stba.stbadocno   #add by geza 20150906

END FUNCTION

