               LET g_errparam.extend = "" 
               LET g_errparam.code   = 'amh-00044' 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               RETURN
            END IF
         END IF
         #已經交款，不可取消確認
         IF NOT cl_null(g_mhaf_m.mhaf014) THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "" 
            LET g_errparam.code   = 'amh-00063' 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            CALL s_transaction_end('N','0')
            RETURN
         END IF
         IF cl_ask_confirm('aim-00110') THEN
            IF g_mhaf_m.mhaf005 = '1' OR g_mhaf_m.mhaf005 = '3' THEN
               UPDATE mhaf_t SET mhaf011 = '0',
                                 mhafcnfid = '',
                                 mhafcnfdt = ''                                 
                WHERE mhafent = g_enterprise
                  AND mhaf001 = g_mhaf_m.mhaf001
