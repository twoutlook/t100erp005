   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF
   
   #先確定key值無遺漏
   IF g_mhaf_m.mhaf001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
   
   #備份key值
   LET g_mhaf001_t = g_mhaf_m.mhaf001
 
   
   #清空key值
   LET g_mhaf_m.mhaf001 = ""
 
    
   CALL amhm310_set_entry("a")
   CALL amhm310_set_no_entry("a")
   
   #公用欄位給予預設值
   #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_mhaf_m.mhafownid = g_user
      LET g_mhaf_m.mhafowndp = g_dept
      LET g_mhaf_m.mhafcrtid = g_user
      LET g_mhaf_m.mhafcrtdp = g_dept 
      LET g_mhaf_m.mhafcrtdt = cl_get_current()
      LET g_mhaf_m.mhafmodid = g_user
      LET g_mhaf_m.mhafmoddt = cl_get_current()
      LET g_mhaf_m.mhafstus = 'N'
 
 
   
   CALL s_transaction_begin()
   
   #add-point:複製輸入前
   LET g_site_flag = FALSE
   CALL s_aooi500_default(g_prog,'mhafsite',g_mhaf_m.mhafsite)
      RETURNING l_insert,g_mhaf_m.mhafsite
   IF NOT l_insert THEN
      RETURN
   END IF
   LET g_mhaf_m.mhafunit = g_mhaf_m.mhafsite      
   LET g_mhaf_m.mhaf002 = ""
   LET g_mhaf_m.mhaf003 = ""
   LET g_mhaf_m.mhaf006 = ""
   LET g_mhaf_m.mhaf007 = g_today
   LET g_mhaf_m.mhaf008 = ""
   LET g_mhaf_m.mhaf009 = "0"
   LET g_mhaf_m.mhaf010 = "0"
   LET g_mhaf_m.mhaf011 = "0"
   LET g_mhaf_m.mhaf014 = ""
   LET g_mhaf_m.mhaf015 = ""
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_mhaf_m.mhafsite
   CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_mhaf_m.mhafsite_desc = '', g_rtn_fields[1] , ''
   DISPLAY BY NAME g_mhaf_m.mhafsite_desc
   #end add-point
   
   #顯示狀態(stus)圖片
         #應用 a21 樣板自動產生(Version:2)
	  #根據當下狀態碼顯示圖片
      CASE g_mhaf_m.mhafstus 
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/open.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/valid.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/void.png")
         
      END CASE
 
 
   
   #清空key欄位的desc
   
   
   #資料輸入
   CALL amhm310_input("r")
   
   IF INT_FLAG THEN
      #取消
      LET INT_FLAG = 0
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = 9001 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      INITIALIZE g_mhaf_m.* TO NULL
      CALL amhm310_show()
      RETURN
   END IF
   
   CALL s_transaction_begin()
   
   #add-point:單頭複製前
   
   #end add-point
   
   #add-point:單頭複製中
   
   #end add-point
   
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "mhaf_t" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:單頭複製後
   
   #end add-point
   
   CALL s_transaction_end('Y','0')
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL amhm310_set_act_visible()
   CALL amhm310_set_act_no_visible()
 
   #將新增的資料併入搜尋條件中
   LET g_state = "insert"
   
   LET g_mhaf001_t = g_mhaf_m.mhaf001
 
   
   #組合新增資料的條件
   LET g_add_browse = " mhafent = '" ||g_enterprise|| "' AND",
                      " mhaf001 = '", g_mhaf_m.mhaf001, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL amhm310_browser_fill("","")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   #add-point:完成複製段落後
   
   #end add-point
                 
   #功能已完成,通報訊息中心
   CALL amhm310_msgcentre_notify('reproduce')
                 
