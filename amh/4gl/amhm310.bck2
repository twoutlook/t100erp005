       g_mhaf_m.mhafmoddt,g_mhaf_m.mhafcnfid,g_mhaf_m.mhafcnfdt,g_mhaf_m.mhafsite_desc,g_mhaf_m.mhaf004_desc, 
       g_mhaf_m.mhaf012_desc,g_mhaf_m.mhaf013_desc,g_mhaf_m.mhafownid_desc,g_mhaf_m.mhafowndp_desc,g_mhaf_m.mhafcrtid_desc, 
       g_mhaf_m.mhafcrtdp_desc,g_mhaf_m.mhafmodid_desc,g_mhaf_m.mhafcnfid_desc
 
   #檢查是否允許此動作
   IF NOT amhm310_action_chk() THEN
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_mhaf_m_mask_o.* =  g_mhaf_m.*
   CALL amhm310_mhaf_t_mask()
   LET g_mhaf_m_mask_n.* =  g_mhaf_m.*
   
   
 
