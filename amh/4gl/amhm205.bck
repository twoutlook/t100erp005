   #保存單頭舊值
   LET g_mhbe_m_t.* = g_mhbe_m.*
   LET g_mhbe_m_o.* = g_mhbe_m.*
   
   LET g_data_owner = g_mhbe_m.mhbeownid      
   LET g_data_dept  = g_mhbe_m.mhbeowndp
   
