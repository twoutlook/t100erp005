{<section id="amhm205.state_change" >}
   #應用 a09 樣板自動產生(Version:17)
#+ 確認碼變更 
PRIVATE FUNCTION amhm205_statechange()
   #add-point:statechange段define(客製用) name="statechange.define_customerization"
   
   #end add-point  
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="statechange.define"
   DEFINE l_success      LIKE type_t.num5
   #end add-point  
   
   #add-point:Function前置處理 name="statechange.before"
   RETURN
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_mhbe_m.mhbe001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN amhm205_cl USING g_enterprise,g_mhbe_m.mhbe001
   IF STATUS THEN
      CLOSE amhm205_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN amhm205_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE amhm205_master_referesh USING g_mhbe_m.mhbe001 INTO g_mhbe_m.mhbesite,g_mhbe_m.mhbe001,g_mhbe_m.mhbe002, 
       g_mhbe_m.mhbe003,g_mhbe_m.mhbe004,g_mhbe_m.mhbe005,g_mhbe_m.mhbe006,g_mhbe_m.mhbe007,g_mhbe_m.mhbe008, 
       g_mhbe_m.mhbe009,g_mhbe_m.mhbe010,g_mhbe_m.mhbe017,g_mhbe_m.mhbe011,g_mhbe_m.mhbe012,g_mhbe_m.mhbe015, 
       g_mhbe_m.mhbestus,g_mhbe_m.mhbeownid,g_mhbe_m.mhbeowndp,g_mhbe_m.mhbecrtid,g_mhbe_m.mhbecrtdp, 
       g_mhbe_m.mhbecrtdt,g_mhbe_m.mhbemodid,g_mhbe_m.mhbemoddt,g_mhbe_m.mhbecnfid,g_mhbe_m.mhbecnfdt, 
       g_mhbe_m.mhbesite_desc,g_mhbe_m.mhbe003_desc,g_mhbe_m.mhbe004_desc,g_mhbe_m.mhbe005_desc,g_mhbe_m.mhbe009_desc, 
       g_mhbe_m.mhbe010_desc,g_mhbe_m.mhbe017_desc,g_mhbe_m.mhbeownid_desc,g_mhbe_m.mhbeowndp_desc,g_mhbe_m.mhbecrtid_desc, 
       g_mhbe_m.mhbecrtdp_desc,g_mhbe_m.mhbemodid_desc,g_mhbe_m.mhbecnfid_desc
   
 
   #檢查是否允許此動作
   IF NOT amhm205_action_chk() THEN
      CLOSE amhm205_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_mhbe_m.mhbesite,g_mhbe_m.mhbesite_desc,g_mhbe_m.mhbe001,g_mhbe_m.mhbe002,g_mhbe_m.mhbel003, 
       g_mhbe_m.mhbel004,g_mhbe_m.mhbe003,g_mhbe_m.mhbe003_desc,g_mhbe_m.mhbe004,g_mhbe_m.mhbe004_desc, 
       g_mhbe_m.mhbe005,g_mhbe_m.mhbe005_desc,g_mhbe_m.mhbe006,g_mhbe_m.mhbe007,g_mhbe_m.mhbe008,g_mhbe_m.mhbe009, 
       g_mhbe_m.mhbe009_desc,g_mhbe_m.mhbe010,g_mhbe_m.mhbe010_desc,g_mhbe_m.mhbe017,g_mhbe_m.mhbe017_desc, 
       g_mhbe_m.mhbe011,g_mhbe_m.mhbe012,g_mhbe_m.mhbe015,g_mhbe_m.mhbestus,g_mhbe_m.mhbeownid,g_mhbe_m.mhbeownid_desc, 
       g_mhbe_m.mhbeowndp,g_mhbe_m.mhbeowndp_desc,g_mhbe_m.mhbecrtid,g_mhbe_m.mhbecrtid_desc,g_mhbe_m.mhbecrtdp, 
       g_mhbe_m.mhbecrtdp_desc,g_mhbe_m.mhbecrtdt,g_mhbe_m.mhbemodid,g_mhbe_m.mhbemodid_desc,g_mhbe_m.mhbemoddt, 
       g_mhbe_m.mhbecnfid,g_mhbe_m.mhbecnfid_desc,g_mhbe_m.mhbecnfdt
 
   CASE g_mhbe_m.mhbestus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/open.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/valid.png")
      WHEN "X"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/void.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_mhbe_m.mhbestus
            
            WHEN "N"
               HIDE OPTION "open"
            WHEN "Y"
               HIDE OPTION "valid"
            WHEN "X"
               HIDE OPTION "void"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      CASE g_mhbe_m.mhbestus
            WHEN "X"
               HIDE OPTION "valid"
               
            WHEN "Y"
               HIDE OPTION "void"
         END CASE
      #end add-point
      
      
	  
      ON ACTION open
         IF cl_auth_chk_act("open") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.open"
            
            #end add-point
         END IF
         EXIT MENU
      ON ACTION valid
         IF cl_auth_chk_act("valid") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.valid"
            
            #end add-point
         END IF
         EXIT MENU
      ON ACTION void
         IF cl_auth_chk_act("void") THEN
            LET lc_state = "X"
            #add-point:action控制 name="statechange.void"
            
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      AND lc_state <> "X"
      ) OR 
      g_mhbe_m.mhbestus = lc_state OR cl_null(lc_state) THEN
      CLOSE amhm205_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   CALL cl_err_collect_init()
   LET l_success = TRUE
   CASE 
      WHEN lc_state = 'Y' AND g_mhbe_m.mhbestus = 'N'  
         CALL s_amhm205_conf_chk(g_mhbe_m.mhbe001) RETURNING l_success
         IF l_success THEN
            IF cl_ask_confirm('aim-00108') THEN
               CALL s_transaction_begin()
               CALL s_amhm205_conf_upd(g_mhbe_m.mhbe001) RETURNING l_success
               IF NOT l_success THEN
                  CALL cl_err_collect_show()
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL cl_err_collect_show()
                  CALL s_transaction_end('Y','1')
               END IF
            ELSE
               CALL cl_err_collect_show()
               CALL s_transaction_end('N','0')   #160816-00068#08 by 08209 add
               RETURN            
            END IF
         ELSE
            CALL cl_err_collect_show()
            CALL s_transaction_end('N','0')   #160816-00068#08 by 08209 add
            RETURN            
         END IF         
      WHEN lc_state = 'X' AND g_mhbe_m.mhbestus = 'N'  
         CALL s_amhm205_void_chk(g_mhbe_m.mhbe001) RETURNING l_success
         IF l_success THEN
            IF cl_ask_confirm('art-00039') THEN
               CALL s_transaction_begin()
               CALL s_amhm205_void_upd(g_mhbe_m.mhbe001) RETURNING l_success
               IF NOT l_success THEN
                  CALL cl_err_collect_show()
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL cl_err_collect_show()
                  CALL s_transaction_end('Y','1')
               END IF
            ELSE
               CALL cl_err_collect_show()
               CALL s_transaction_end('N','0')   #160816-00068#08 by 08209 add
               RETURN
            END IF
         ELSE
            CALL cl_err_collect_show()
            CALL s_transaction_end('N','0')   #160816-00068#08 by 08209 add
            RETURN    
         END IF
      WHEN lc_state = 'N' AND g_mhbe_m.mhbestus = 'Y'  
         CALL s_amhm205_unconf_chk(g_mhbe_m.mhbe001) RETURNING l_success
         IF l_success THEN
            IF cl_ask_confirm('aim-00110') THEN
               CALL s_transaction_begin()
               CALL s_amhm205_unconf_upd(g_mhbe_m.mhbe001) RETURNING l_success
               IF NOT l_success THEN
                  CALL cl_err_collect_show()
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL cl_err_collect_show()
                  CALL s_transaction_end('Y','1')
               END IF
            ELSE
               CALL cl_err_collect_show()
               CALL s_transaction_end('N','0')   #160816-00068#08 by 08209 add
               RETURN
            END IF
         ELSE
            CALL cl_err_collect_show()
            CALL s_transaction_end('N','0')   #160816-00068#08 by 08209 add
            RETURN    
         END IF
      WHEN lc_state = 'N' AND g_mhbe_m.mhbestus = 'X'
         CALL s_amhm205_valid_chk(g_mhbe_m.mhbe001) RETURNING l_success
         IF l_success THEN
            IF cl_ask_confirm('art-00038') THEN
               CALL s_transaction_begin()
               CALL s_amhm205_valid_upd(g_mhbe_m.mhbe001) RETURNING l_success
               IF NOT l_success THEN
                  CALL cl_err_collect_show()
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL cl_err_collect_show()
                  CALL s_transaction_end('Y','1')
               END IF
            ELSE
               CALL cl_err_collect_show()
               CALL s_transaction_end('N','0')   #160816-00068#08 by 08209 add
               RETURN
            END IF
         ELSE
            CALL cl_err_collect_show()
            CALL s_transaction_end('N','0')   #160816-00068#08 by 08209 add
            RETURN    
         END IF         
      OTHERWISE
         CALL cl_err_collect_show()
         CALL s_transaction_end('N','0')   #160816-00068#08 by 08209 add
         RETURN
   END CASE   
   #end add-point
   
   LET g_mhbe_m.mhbemodid = g_user
   LET g_mhbe_m.mhbemoddt = cl_get_current()
   LET g_mhbe_m.mhbestus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE mhbe_t 
      SET (mhbestus,mhbemodid,mhbemoddt) 
        = (g_mhbe_m.mhbestus,g_mhbe_m.mhbemodid,g_mhbe_m.mhbemoddt)     
    WHERE mhbeent = g_enterprise AND mhbe001 = g_mhbe_m.mhbe001
 
    
   IF SQLCA.sqlcode THEN
