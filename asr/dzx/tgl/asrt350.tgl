   #檢查是否允許此動作
   IF NOT asrt350_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_srba_m_mask_o.* =  g_srba_m.*
   CALL asrt350_srba_t_mask()
   LET g_srba_m_mask_n.* =  g_srba_m.*
   
   
   
   #add-point:modify段show之前 name="modify.before_show"
