          g_sffb_d[l_ac].sffb023,g_sffb_d[l_ac].sffb021,g_sffb_d[l_ac].sffb022,g_sffb_d[l_ac].sffb026, 
          g_sffb_d[l_ac].sffbstus,g_sffb_d[l_ac].sffb015,g_sffb_d[l_ac].sffb004,g_sffb_d[l_ac].sffb003, 
