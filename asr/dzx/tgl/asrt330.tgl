                  g_sffb_d[g_detail_idx].sffb017,g_sffb_d[g_detail_idx].sffb018,g_sffb_d[g_detail_idx].sffb019, 
                  g_sffb_d[g_detail_idx].sffb020,g_sffb_d[g_detail_idx].sffb012,g_sffb_d[g_detail_idx].sffb013, 
