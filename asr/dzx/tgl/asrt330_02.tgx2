   LET g_forupd_sql = "SELECT sffbdocno,'',sffbseq,'','','',sffb027,sffb028,sffb007,'',sffb008,sffb017, 
       sffb018,sffb019,sffb020 FROM sffb_t WHERE sffbent= ? AND sffbdocno=? AND sffbseq=? FOR UPDATE" 

