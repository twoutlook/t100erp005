  <section id="asrt340_04.b_fill" ver="4" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION asrt340_04_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2           STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE sfedsite,sfeddocno,sfed004,sfed001,'','',sfed002,sfed003 FROM sfed_t", 

               "",
               " WHERE sfedent= ? AND 1=1 AND ", p_wc2 
    
   LET g_sql = g_sql, #,cl_get_extra_cond('zzuser', 'zzgrup') 
                      " ORDER BY sfed_t.sfeddocno,sfed_t.sfed004"
  
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
  
   PREPARE asrt340_04_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR asrt340_04_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_sfed_d.clear()
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_sfed_d[l_ac].sfedsite,g_sfed_d[l_ac].sfeddocno,g_sfed_d[l_ac].sfed004, 
       g_sfed_d[l_ac].sfed001,g_sfed_d[l_ac].sfed001_desc1,g_sfed_d[l_ac].sfed001_desc2,g_sfed_d[l_ac].sfed002, 
       g_sfed_d[l_ac].sfed003
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL asrt340_04_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec THEN
      IF g_error_show = 1 THEN
         CALL cl_err("sfed_t",9035,1)
      END IF
   END IF 
   LET g_error_show = 0
   
 
  
   
   CALL g_sfed_d.deleteElement(g_sfed_d.getLength())   
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_sfed_d.getLength()
 
   END FOR
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = l_ac - 1
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE asrt340_04_pb
   
END FUNCTION
]]>
  </section>
