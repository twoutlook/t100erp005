  <section id="asrt340_04.modify" ver="5" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION asrt340_04_modify()
   DEFINE  l_cmd                  LIKE type_t.chr1
   DEFINE  l_ac_t                 LIKE type_t.num5                #未取消的ARRAY CNT 
   DEFINE  l_n                    LIKE type_t.num5                #檢查重複用  
   DEFINE  l_cnt                  LIKE type_t.num5                #檢查重複用  
   DEFINE  l_lock_sw              LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert         LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete         LIKE type_t.num5                #可刪除否  
   DEFINE  l_count                LIKE type_t.num5
   DEFINE  l_i                    LIKE type_t.num5
   DEFINE  ls_return              STRING
   DEFINE  l_var_keys             DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys           DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                 DYNAMIC ARRAY OF STRING
   DEFINE  l_fields               DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak         DYNAMIC ARRAY OF STRING
   DEFINE  li_reproduce           LIKE type_t.num5
   DEFINE  li_reproduce_target    LIKE type_t.num5
   DEFINE  lb_reproduce           BOOLEAN
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point 
   
   LET g_action_choice = ""
   
   LET g_qryparam.state = "i"
 
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
 
   #add-point:modify段define_sql
   {<point name="modify.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT sfedsite,sfeddocno,sfed004,sfed001,'','',sfed002,sfed003 FROM sfed_t WHERE  
       sfedent=? AND sfeddocno=? AND sfed004=? FOR UPDATE"
   #add-point:modify段define_sql
   {<point name="modify.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   DECLARE asrt340_04_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR
   
 
   
   LET INT_FLAG = FALSE
   LET lb_reproduce = FALSE
 
   #add-point:modify段修改前
   {<point name="modify.before_input"/>}
   #end add-point
 
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #Page1 預設值產生於此處
      INPUT ARRAY g_sfed_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_detail_cnt,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = FALSE, 
                  DELETE ROW = FALSE,
                  APPEND ROW = FALSE)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_sfed_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL asrt340_04_b_fill(g_wc2)
            LET g_detail_cnt = g_sfed_d.getLength()
         
         BEFORE ROW
            #add-point:modify段before row
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            DISPLAY g_sfed_d.getLength() TO FORMONLY.cnt
         
            CALL s_transaction_begin()
            LET g_detail_cnt = g_sfed_d.getLength()
            
            IF g_detail_cnt >= l_ac 
               AND g_sfed_d[l_ac].sfeddocno IS NOT NULL
               AND g_sfed_d[l_ac].sfed004 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_sfed_d_t.* = g_sfed_d[l_ac].*  #BACKUP
               IF NOT asrt340_04_lock_b("sfed_t") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH asrt340_04_bcl INTO g_sfed_d[l_ac].sfedsite,g_sfed_d[l_ac].sfeddocno,g_sfed_d[l_ac].sfed004, 
                      g_sfed_d[l_ac].sfed001,g_sfed_d[l_ac].sfed001_desc1,g_sfed_d[l_ac].sfed001_desc2, 
                      g_sfed_d[l_ac].sfed002,g_sfed_d[l_ac].sfed003
                  IF SQLCA.sqlcode THEN
                     CALL cl_err(g_sfed_d_t.sfeddocno,SQLCA.sqlcode,1)
                     LET l_lock_sw = "Y"
                  END IF
                  
                  CALL asrt340_04_detail_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
        
         BEFORE INSERT
            
            CALL s_transaction_begin()
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_sfed_d_t.* TO NULL
            INITIALIZE g_sfed_d[l_ac].* TO NULL 
            
            LET g_sfed_d_t.* = g_sfed_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL asrt340_04_set_entry_b("a")
            CALL asrt340_04_set_no_entry_b("a")
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_sfed_d[li_reproduce_target].* = g_sfed_d[li_reproduce].*
 
               LET g_sfed_d[g_sfed_d.getLength()].sfeddocno = NULL
               LET g_sfed_d[g_sfed_d.getLength()].sfed004 = NULL
 
            END IF
            #公用欄位給值(單身)
            
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM sfed_t 
             WHERE sfedent = g_enterprise AND sfeddocno = g_sfed_d[l_ac].sfeddocno
                                       AND sfed004 = g_sfed_d[l_ac].sfed004
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前
               {<point name="input.body.b_insert"/>}
               #end add-point
            
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_sfed_d[g_detail_idx].sfeddocno
               LET gs_keys[2] = g_sfed_d[g_detail_idx].sfed004
               CALL asrt340_04_insert_b('sfed_t',gs_keys,"'1'")
                           
               #add-point:單身新增後
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_sfed_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("sfed_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL asrt340_04_b_fill(g_wc2)
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert2"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR 'INSERT O.K'
               LET g_detail_cnt = g_detail_cnt + 1
               
               LET g_wc2 = g_wc2, " OR (sfeddocno = '", g_sfed_d[l_ac].sfeddocno, "' "
                                  ," AND sfed004 = '", g_sfed_d[l_ac].sfed004, "' "
 
                                  ,")"
            END IF                
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_sfed_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_sfed_d.deleteElement(l_ac)
               NEXT FIELD sfeddocno
            END IF
            IF g_sfed_d[l_ac].sfeddocno IS NOT NULL
               AND g_sfed_d_t.sfed004 IS NOT NULL
 
               THEN     
            
               #add-point:單身刪除ask前
               {<point name="input.body.b_delete_ask"/>}
               #end add-point   
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete" mark="Y"/>}
               #end add-point   
               
               DELETE FROM sfed_t
                WHERE sfedent = g_enterprise AND 
                      sfeddocno = g_sfed_d_t.sfeddocno
                      AND sfed004 = g_sfed_d_t.sfed004
 
                      
               #add-point:單身刪除中
               {<point name="input.body.m_delete"/>}
               #end add-point  
                      
               IF SQLCA.sqlcode THEN
                  CALL cl_err("sfed_t",SQLCA.sqlcode,1)
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               ELSE
                  LET g_detail_cnt = g_detail_cnt-1
                  
                  #add-point:單身刪除後
                  {<point name="input.body.a_delete"/>}
                  #end add-point
                  CALL s_transaction_end('Y','0')
               END IF 
               CLOSE asrt340_04_bcl
               LET l_count = g_sfed_d.getLength()
            END IF 
                           INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_sfed_d[g_detail_idx].sfeddocno
               LET gs_keys[2] = g_sfed_d[g_detail_idx].sfed004
 
              
         AFTER DELETE 
            #add-point:單身刪除後2
            {<point name="input.body.after_delete"/>}
            #end add-point
                           CALL asrt340_04_delete_b('sfed_t',gs_keys,"'1'")
 
         #---------------------<  Detail: page1  >---------------------
         #----<<sfedsite>>----
         #此段落由子樣板a01產生
         BEFORE FIELD sfedsite
            #add-point:BEFORE FIELD sfedsite
            {<point name="input.b.page1.sfedsite" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD sfedsite
            
            #add-point:AFTER FIELD sfedsite
            {<point name="input.a.page1.sfedsite" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE sfedsite
            #add-point:ON CHANGE sfedsite
            {<point name="input.g.page1.sfedsite" />}
            #END add-point
 
         #----<<sfeddocno>>----
         #此段落由子樣板a01產生
         BEFORE FIELD sfeddocno
            #add-point:BEFORE FIELD sfeddocno
            {<point name="input.b.page1.sfeddocno" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD sfeddocno
            
            #add-point:AFTER FIELD sfeddocno
            {<point name="input.a.page1.sfeddocno" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE sfeddocno
            #add-point:ON CHANGE sfeddocno
            {<point name="input.g.page1.sfeddocno" />}
            #END add-point
 
         #----<<sfed004>>----
         #此段落由子樣板a01產生
         BEFORE FIELD sfed004
            #add-point:BEFORE FIELD sfed004
            {<point name="input.b.page1.sfed004" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD sfed004
            
            #add-point:AFTER FIELD sfed004
            {<point name="input.a.page1.sfed004" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE sfed004
            #add-point:ON CHANGE sfed004
            {<point name="input.g.page1.sfed004" />}
            #END add-point
 
         #----<<sfed001>>----
         #此段落由子樣板a02產生
         AFTER FIELD sfed001
            
            #add-point:AFTER FIELD sfed001
            {<point name="input.a.page1.sfed001" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD sfed001
            #add-point:BEFORE FIELD sfed001
            {<point name="input.b.page1.sfed001" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE sfed001
            #add-point:ON CHANGE sfed001
            {<point name="input.g.page1.sfed001" />}
            #END add-point
 
         #----<<sfed001_desc1>>----
         #----<<sfed001_desc2>>----
         #----<<sfed002>>----
         #此段落由子樣板a01產生
         BEFORE FIELD sfed002
            #add-point:BEFORE FIELD sfed002
            {<point name="input.b.page1.sfed002" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD sfed002
            
            #add-point:AFTER FIELD sfed002
            {<point name="input.a.page1.sfed002" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE sfed002
            #add-point:ON CHANGE sfed002
            {<point name="input.g.page1.sfed002" />}
            #END add-point
 
         #----<<sfed003>>----
         #此段落由子樣板a01產生
         BEFORE FIELD sfed003
            #add-point:BEFORE FIELD sfed003
            {<point name="input.b.page1.sfed003" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD sfed003
            
            #add-point:AFTER FIELD sfed003
            {<point name="input.a.page1.sfed003" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE sfed003
            #add-point:ON CHANGE sfed003
            {<point name="input.g.page1.sfed003" />}
            #END add-point
 
 
         #---------------------<  Detail: page1  >---------------------
         #----<<sfedsite>>----
         #Ctrlp:input.c.page1.sfedsite
         ON ACTION controlp INFIELD sfedsite
            #add-point:ON ACTION controlp INFIELD sfedsite
            {<point name="input.c.page1.sfedsite" />}
            #END add-point
 
         #----<<sfeddocno>>----
         #Ctrlp:input.c.page1.sfeddocno
         ON ACTION controlp INFIELD sfeddocno
            #add-point:ON ACTION controlp INFIELD sfeddocno
            {<point name="input.c.page1.sfeddocno" />}
            #END add-point
 
         #----<<sfed004>>----
         #Ctrlp:input.c.page1.sfed004
         ON ACTION controlp INFIELD sfed004
            #add-point:ON ACTION controlp INFIELD sfed004
            {<point name="input.c.page1.sfed004" />}
            #END add-point
 
         #----<<sfed001>>----
         #Ctrlp:input.c.page1.sfed001
         ON ACTION controlp INFIELD sfed001
            #add-point:ON ACTION controlp INFIELD sfed001
            {<point name="input.c.page1.sfed001" />}
            #END add-point
 
         #----<<sfed001_desc1>>----
         #----<<sfed001_desc2>>----
         #----<<sfed002>>----
         #Ctrlp:input.c.page1.sfed002
         ON ACTION controlp INFIELD sfed002
            #add-point:ON ACTION controlp INFIELD sfed002
            {<point name="input.c.page1.sfed002" />}
            #END add-point
 
         #----<<sfed003>>----
         #Ctrlp:input.c.page1.sfed003
         ON ACTION controlp INFIELD sfed003
            #add-point:ON ACTION controlp INFIELD sfed003
            {<point name="input.c.page1.sfed003" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_sfed_d[l_ac].* = g_sfed_d_t.*
               CLOSE asrt340_04_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_sfed_d[l_ac].sfeddocno,-263,1)
               LET g_sfed_d[l_ac].* = g_sfed_d_t.*
            ELSE
               
               #寫入修改者/修改日期資訊(單身)
               
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               UPDATE sfed_t SET (sfedsite,sfeddocno,sfed004,sfed001,sfed002,sfed003) = (g_sfed_d[l_ac].sfedsite, 
                   g_sfed_d[l_ac].sfeddocno,g_sfed_d[l_ac].sfed004,g_sfed_d[l_ac].sfed001,g_sfed_d[l_ac].sfed002, 
                   g_sfed_d[l_ac].sfed003)
                WHERE sfedent = g_enterprise AND
                  sfeddocno = g_sfed_d_t.sfeddocno #項次   
                  AND sfed004 = g_sfed_d_t.sfed004  
 
                  
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
                  
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("sfed_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                    WHEN SQLCA.sqlcode #其他錯誤
                     CALL cl_err("sfed_t",SQLCA.sqlcode,1)  
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_sfed_d[g_detail_idx].sfeddocno
               LET gs_keys_bak[1] = g_sfed_d_t.sfeddocno
               LET gs_keys[2] = g_sfed_d[g_detail_idx].sfed004
               LET gs_keys_bak[2] = g_sfed_d_t.sfed004
               CALL asrt340_04_update_b('sfed_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
               END CASE
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            CALL asrt340_04_unlock_b("sfed_t")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock
            
             #add-point:單身after row
            {<point name="input.body.a_row"/>}
            #end add-point
            
         AFTER INPUT
            #add-point:單身input後
            {<point name="input.body.a_input"/>}
            #end add-point
            
         ON ACTION controlo   
            CALL FGL_SET_ARR_CURR(g_sfed_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_sfed_d.getLength()+1
            
      END INPUT
      
 
      
 
      
      #add-point:before_more_input
      {<point name="input.more_input"/>}
      #end add-point
      
      BEFORE DIALOG 
         IF g_temp_idx > 0 THEN
            LET l_ac = g_temp_idx
            CALL DIALOG.setCurrentRow("s_detail1",l_ac)
            LET g_temp_idx = 1
         END IF
         LET g_curr_diag = ui.DIALOG.getCurrent()
         #add-point:before_dialog
         {<point name="input.before_dialog"/>}
         #end add-point
         CASE g_aw
            WHEN "s_detail1"
               NEXT FIELD sfedsite
 
         END CASE
   
      ON ACTION accept
         ACCEPT DIALOG
      
      ON ACTION cancel
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION controlr
         CALL cl_show_req_fields()
 
      ON ACTION controlf
         CALL cl_set_focus_form(ui.Interface.getRootNode()) 
              RETURNING g_fld_name,g_frm_name 
         CALL cl_fldhelp(g_frm_name,g_fld_name,g_lang) 
           
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE DIALOG
   
   END DIALOG 
   
   #add-point:modify段修改後
   {<point name="modify.after_input"/>}
   #end add-point
 
   CLOSE asrt340_04_bcl
   CALL s_transaction_end('Y','0')
   
END FUNCTION
]]>
