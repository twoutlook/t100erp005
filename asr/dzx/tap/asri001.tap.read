                  #add-point:單頭新增中
                  {<point name="input.head.m_insert"/>}
                  #end add-point
                  
                  IF SQLCA.sqlcode THEN
                     CALL cl_err("srza_t",SQLCA.sqlcode,1)  
                     CONTINUE DIALOG
                  END IF
                  
                  
                  
                  #資料多語言用-增/改
                  
                  
                  #add-point:單頭新增後
                  {<point name="input.head.a_insert"/>}
                  #end add-point
                  
                  CALL s_transaction_end('Y','0')
               ELSE
                  CALL cl_err( "g_srza_m.srza001", "std-00006", 0 )
                  CALL s_transaction_end('N','0')
               END IF 
            ELSE
               #add-point:單頭修改前
               {<point name="input.head.b_update" mark="Y"/>}
               #end add-point
               UPDATE srza_t SET (srza001,srza002,srza012,srzastus,srza003,srza004,srza006,srza007,srza008, 
                   srza009,srza010,srza005,srza011,srzaownid,srzaowndp,srzacrtid,srzacrtdp,srzacrtdt) = (g_srza_m.srza001, 
                   g_srza_m.srza002,g_srza_m.srza012,g_srza_m.srzastus,g_srza_m.srza003,g_srza_m.srza004, 
                   g_srza_m.srza006,g_srza_m.srza007,g_srza_m.srza008,g_srza_m.srza009,g_srza_m.srza010, 
                   g_srza_m.srza005,g_srza_m.srza011,g_srza_m.srzaownid,g_srza_m.srzaowndp,g_srza_m.srzacrtid, 
                   g_srza_m.srzacrtdp,g_srza_m.srzacrtdt)
                WHERE srzaent = g_enterprise AND srzasite = g_site AND srza001 = g_srza001_t #
 
               #add-point:單頭修改中
               {<point name="input.head.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("srza_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                  WHEN SQLCA.sqlcode #其他錯誤
                     CALL cl_err("srza_t",SQLCA.sqlcode,1)  
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                     
                     #資料多語言用-增/改
                     
                     #add-point:單頭修改後
                     {<point name="input.head.a_update"/>}
                     #end add-point
                     LET g_log1 = util.JSON.stringify(g_srza_m_t)
                     LET g_log2 = util.JSON.stringify(g_srza_m)
                     IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                        CALL s_transaction_end('N','0')
                     ELSE
                        CALL s_transaction_end('Y','0')
                     END IF
               END CASE
            END IF
           #controlp
      END INPUT
          
      #add-point:input段more input 
      {<point name="input.more_input"/>}
      #end add-point
          
      ON ACTION controlf
         CALL cl_set_focus_form(ui.Interface.getRootNode()) RETURNING g_fld_name,g_frm_name
         CALL cl_fldhelp(g_frm_name, g_fld_name, g_lang)
 
