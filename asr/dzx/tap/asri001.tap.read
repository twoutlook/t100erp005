  <section id="asri001.delete" ver="7" status="" src="s">
    <![CDATA[#+ 資料刪除 
PRIVATE FUNCTION asri001_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point  
   
   IF g_srza_m.srza001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   CALL asri001_show()
   
   CALL s_transaction_begin()
    
   LET g_srza001_t = g_srza_m.srza001
 
   
   
 
   OPEN asri001_cl USING g_enterprise, g_site,
                           g_srza_m.srza001
 
   IF STATUS THEN
      CALL cl_err("OPEN asri001_cl:", STATUS, 0)
      CLOSE asri001_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE asri001_master_referesh USING g_srza_m.srza001 INTO g_srza_m.srza001,g_srza_m.srza002,g_srza_m.srza012, 
       g_srza_m.srzastus,g_srza_m.srza003,g_srza_m.srza004,g_srza_m.srza006,g_srza_m.srza007,g_srza_m.srza008, 
       g_srza_m.srza009,g_srza_m.srza010,g_srza_m.srza005,g_srza_m.srza011,g_srza_m.srzaownid,g_srza_m.srzaowndp, 
       g_srza_m.srzacrtid,g_srza_m.srzacrtdp,g_srza_m.srzacrtdt,g_srza_m.srzamodid,g_srza_m.srzamoddt, 
       g_srza_m.srza012_desc,g_srza_m.srzaownid_desc,g_srza_m.srzaowndp_desc,g_srza_m.srzacrtid_desc, 
       g_srza_m.srzacrtdp_desc,g_srza_m.srzamodid_desc
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_srza_m.srza001,SQLCA.sqlcode,0)
      RETURN
   END IF
   
   IF cl_ask_delete() THEN
 
      #add-point:單頭刪除前
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point
 
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL asri001_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
      DELETE FROM srza_t 
       WHERE srzaent = g_enterprise AND srzasite = g_site AND srza001 = g_srza_m.srza001 
 
 
      #add-point:單頭刪除中
      {<point name="delete.head.m_delete"/>}
      #end add-point
         
      IF SQLCA.sqlcode THEN
         CALL cl_err("srza_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
  
      
      
      #add-point:單頭刪除後
      {<point name="delete.head.a_delete"/>}
      #end add-point
      
          
      CLEAR FORM
      CALL asri001_ui_browser_refresh()
      IF g_browser_cnt > 0 THEN
         CALL asri001_browser_fill(g_wc,"")
         CALL asri001_fetch("P")
      ELSE
         CALL asri001_browser_fill(" 1=1 ","F")
      END IF
      
   END IF
 
   CLOSE asri001_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_srza_m.srza001,"D")
 
END FUNCTION
]]>
