  <section id="asri001.modify" ver="4" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION asri001_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point
   
   IF g_srza_m.srza001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF 
   
   EXECUTE asri001_master_referesh USING g_srza_m.srza001 INTO g_srza_m.srza001,g_srza_m.srza002,g_srza_m.srza012, 
       g_srza_m.srzastus,g_srza_m.srza003,g_srza_m.srza004,g_srza_m.srza006,g_srza_m.srza007,g_srza_m.srza008, 
       g_srza_m.srza009,g_srza_m.srza010,g_srza_m.srza005,g_srza_m.srza011,g_srza_m.srzaownid,g_srza_m.srzaowndp, 
       g_srza_m.srzacrtid,g_srza_m.srzacrtdp,g_srza_m.srzacrtdt,g_srza_m.srzamodid,g_srza_m.srzamoddt, 
       g_srza_m.srza012_desc,g_srza_m.srzaownid_desc,g_srza_m.srzaowndp_desc,g_srza_m.srzacrtid_desc, 
       g_srza_m.srzacrtdp_desc,g_srza_m.srzamodid_desc
 
   ERROR ""
  
   LET g_srza001_t = g_srza_m.srza001
 
   
   CALL s_transaction_begin()
   
   OPEN asri001_cl USING g_enterprise, g_site,g_srza_m.srza001
   IF STATUS THEN
      CALL cl_err("OPEN asri001_cl:", STATUS, 1)
      CLOSE asri001_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE asri001_master_referesh USING g_srza_m.srza001 INTO g_srza_m.srza001,g_srza_m.srza002,g_srza_m.srza012, 
       g_srza_m.srzastus,g_srza_m.srza003,g_srza_m.srza004,g_srza_m.srza006,g_srza_m.srza007,g_srza_m.srza008, 
       g_srza_m.srza009,g_srza_m.srza010,g_srza_m.srza005,g_srza_m.srza011,g_srza_m.srzaownid,g_srza_m.srzaowndp, 
       g_srza_m.srzacrtid,g_srza_m.srzacrtdp,g_srza_m.srzacrtdt,g_srza_m.srzamodid,g_srza_m.srzamoddt, 
       g_srza_m.srza012_desc,g_srza_m.srzaownid_desc,g_srza_m.srzaowndp_desc,g_srza_m.srzacrtid_desc, 
       g_srza_m.srzacrtdp_desc,g_srza_m.srzamodid_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err("srza_t",SQLCA.sqlcode,0)
      CLOSE asri001_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
 
   CALL asri001_show()
   
   WHILE TRUE
      LET g_srza_m.srza001 = g_srza001_t
 
      
      #寫入修改者/修改日期資訊
      LET g_srza_m.srzamodid = g_user 
LET g_srza_m.srzamoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
 
      CALL asri001_input("u")     #欄位更改
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_srza_m.* = g_srza_m_t.*
         CALL asri001_show()
         CALL cl_err("",9001,0)
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE srza_t SET (srzamodid,srzamoddt) = (g_srza_m.srzamodid,g_srza_m.srzamoddt)
       WHERE srzaent = g_enterprise AND srzasite = g_site AND srza001 = g_srza001_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_srza_m.srza001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE asri001_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_srza_m.srza001,"U")
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
]]>
