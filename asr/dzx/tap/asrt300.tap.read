            CALL asrt300_idx_chk()
            
         ON ACTION previous
            CALL asrt300_fetch('P')
            LET g_current_row = g_current_idx
            LET g_curr_diag = ui.DIALOG.getCurrent()
            CALL asrt300_idx_chk()
            
         ON ACTION jump
