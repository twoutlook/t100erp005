            END IF
 
 
         #+ 此段落由子樣板a43產生
         ON ACTION process
            LET g_action_choice="process"
            IF cl_auth_chk_act("process") THEN
               
               #add-point:ON ACTION process
               {<point name="menu.process" />}
               #END add-point
               EXIT DIALOG
            END IF
 
 
         
         #+ 此段落由子樣板a46產生
         #新增相關文件
         ON ACTION related_document
            CALL asrt300_set_pk_array()
