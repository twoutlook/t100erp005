  <section id="asrt300.ui_dialog" ver="19" status="" src="s">
    <![CDATA[#+ 功能選單
PRIVATE FUNCTION asrt300_ui_dialog()
   DEFINE li_idx    LIKE type_t.num5
   DEFINE ls_wc     STRING
   DEFINE lb_first  BOOLEAN
   DEFINE la_param  RECORD
             prog   STRING,
             param  DYNAMIC ARRAY OF STRING
                    END RECORD
