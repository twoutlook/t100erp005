PUBLIC FUNCTION aapi350_apad002_chk(p_ac)
   DEFINE p_ac        LIKE type_t.num10
   DEFINE l_count     LIKE type_t.num10  # 錯誤數目     
   DEFINE r_success   BOOLEAN
   DEFINE r_errno     LIKE gzze_t.gzze001    
   LET r_success = TRUE
   LET r_errno =''
   
   LET l_count = ''
   SELECT count (*) INTO l_count 
     FROM  apad_t 
     WHERE apadent = g_enterprise
       AND apad002 = g_apad_d[p_ac].apad002
       AND apad001 <> g_apac_m.apac001
       AND apad005 = 'Y'
       AND apad006 IS NULL
    
    IF cl_null(l_count) THEN 
       LET l_count = 0
    END IF
    
    IF l_count > 0 THEN
       LET r_success = FALSE
       LET r_errno ='aap-00084'
    END IF
    
   RETURN r_success,r_errno
          
 
END FUNCTION

