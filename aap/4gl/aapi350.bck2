# Descriptions...: 狀態確認有效改無效檢核
# Memo...........:
# Usage..........: aapi350_unactive_upd()
################################################################################
PUBLIC FUNCTION aapi350_unactive_upd()
   
   DEFINE r_success BOOLEAN
      LET r_success = TRUE
   
   UPDATE apad_t
      SET apad_t.apad005 = 'N'
    WHERE apad_t.apadent = g_enterprise AND apad_t.apad001 = g_apac_m.apac001
    
    IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()
 
      LET r_success = FALSE
      RETURN r_success   
    END IF      
    
     UPDATE apad_t
      SET apad_t.apad006 = g_today
    WHERE apad_t.apadent = g_enterprise AND apad_t.apad001 = g_apac_m.apac001 
      AND apad_t.apad006 IS NULL
   
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()
 
      LET r_success = FALSE      
      RETURN r_success   
   END IF
   RETURN r_success
        
END FUNCTION
