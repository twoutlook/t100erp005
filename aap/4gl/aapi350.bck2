{<section id="aapi350.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION aapi350_b_fill()
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   
   #end add-point     
   DEFINE p_wc2      STRING
   DEFINE li_idx     LIKE type_t.num10
   #add-point:b_fill段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   
   #end add-point     
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   #清空第一階單身
   CALL g_apad_d.clear()
 
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   
   #end add-point
   
   #判斷是否填充
   IF aapi350_fill_chk(1) THEN
      #切換上下筆時不重組SQL
      IF (g_action_choice = "query" OR cl_null(g_action_choice))
      #add-point:b_fill段long_sql_if name="b_fill.long_sql_if"
      
      #end add-point
      THEN
         LET g_sql = "SELECT  DISTINCT apad002,apad003,apad004,apad007,apad005,apad006,apadsite  FROM apad_t", 
                
                     " INNER JOIN apac_t ON apacent = " ||g_enterprise|| " AND apac001 = apad001 ",
 
                     #"",
                     
                     "",
                     #下層單身所需的join條件
 
                     
                     " WHERE apadent=? AND apad001=?"
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before name="b_fill.body.fill_sql"
       # LET g_sql = g_sql CLIPPED," AND (apad002 IN(SELECT UNIQUE nmll001 FROM nmll_t WHERE nmllent = ",g_enterprise," AND nmll002 = '",g_user,"')
         LET g_sql = g_sql CLIPPED," AND (apad002 IN(",g_sql_bank,")
                                      OR  TRIM(apad002) IS NULL)"   #160122-00001#11
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料#160122-00001#11
         #end add-point
         IF NOT cl_null(g_wc2_table1) THEN
            LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY apad_t.apad002"
         
         #add-point:單身填充控制 name="b_fill.sql"
 
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE aapi350_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR aapi350_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_apac_m.apac001
                                               
      FOREACH b_fill_cs INTO g_apad_d[l_ac].apad002,g_apad_d[l_ac].apad003,g_apad_d[l_ac].apad004,g_apad_d[l_ac].apad007, 
          g_apad_d[l_ac].apad005,g_apad_d[l_ac].apad006,g_apad_d[l_ac].apadsite
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充 name="b_fill.fill"
         
         CALL aapi350_nmaal003_ref()     #帳戶銀行                                       
         CALL aapi350_nmas003_ref()      #幣別                         
         CALL aapi350_nmaa003_ref()      #編碼類型
         CALL aapi350_nmaa003_desc_ref() #編碼類型說明            
         CALL aapi350_nmaa004_ref()      #開戶銀行編碼
         CALL aapi350_nmaa004_desc_ref() #開戶銀行說明
         CALL aapi350_nmas004_ref()      #POS收銀帳戶否
         CALL aapi350_apad003_apad004_ref() #幣別取位
         CALL aapi350_apad007_desc()     #160518-00024#15
   
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF
            EXIT FOREACH
         END IF
         
         LET l_ac = l_ac + 1
      END FOREACH
      LET g_error_show = 0
   
   END IF
    
 
   
   #add-point:browser_fill段其他table處理 name="browser_fill.other_fill"
   
   #end add-point
   
   CALL g_apad_d.deleteElement(g_apad_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE aapi350_pb
 
   
   LET li_idx = l_ac
   
   #遮罩相關處理
   FOR l_ac = 1 TO g_apad_d.getLength()
      LET g_apad_d_mask_o[l_ac].* =  g_apad_d[l_ac].*
      CALL aapi350_apad_t_mask()
      LET g_apad_d_mask_n[l_ac].* =  g_apad_d[l_ac].*
   END FOR
   
 
   
   LET l_ac = li_idx
   
   CALL cl_ap_performance_next_end()
 
END FUNCTION
 
