PRIVATE FUNCTION aapi350_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   
   #end add-point    
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   DEFINE l_wc2_table1          STRING
 
 
   #add-point:modify段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   
   #end add-point    
   
   #add-point:Function前置處理  name="modify.pre_function"
   
   #end add-point
   
   #保存單頭舊值
   LET g_apac_m_t.* = g_apac_m.*
   LET g_apac_m_o.* = g_apac_m.*
   
   IF g_apac_m.apac001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_apac001_t = g_apac_m.apac001
 
   CALL s_transaction_begin()
   
   OPEN aapi350_cl USING g_enterprise,g_apac_m.apac001
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aapi350_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CLOSE aapi350_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aapi350_master_referesh USING g_apac_m.apac001 INTO g_apac_m.apac001,g_apac_m.apacstus,g_apac_m.apac002, 
       g_apac_m.apac007,g_apac_m.apac006,g_apac_m.apacsite,g_apac_m.apac003,g_apac_m.apac004,g_apac_m.apac005, 
       g_apac_m.apacownid,g_apac_m.apacowndp,g_apac_m.apaccrtid,g_apac_m.apaccrtdp,g_apac_m.apaccrtdt, 
       g_apac_m.apacmodid,g_apac_m.apacmoddt,g_apac_m.apacownid_desc,g_apac_m.apacowndp_desc,g_apac_m.apaccrtid_desc, 
       g_apac_m.apaccrtdp_desc,g_apac_m.apacmodid_desc
   
   #檢查是否允許此動作
   IF NOT aapi350_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
