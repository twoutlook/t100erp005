{<section id="aapi350.state_change" >}
   #應用 a09 樣板自動產生(Version:17)
#+ 確認碼變更 
PRIVATE FUNCTION aapi350_statechange()
   #add-point:statechange段define(客製用) name="statechange.define_customerization"
   
   #end add-point  
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="statechange.define"
   
   #end add-point  
   
   #add-point:Function前置處理 name="statechange.before"
   
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_apac_m.apac001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN aapi350_cl USING g_enterprise,g_apac_m.apac001
   IF STATUS THEN
      CLOSE aapi350_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aapi350_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE aapi350_master_referesh USING g_apac_m.apac001 INTO g_apac_m.apac001,g_apac_m.apacstus,g_apac_m.apac002, 
       g_apac_m.apac007,g_apac_m.apac006,g_apac_m.apacsite,g_apac_m.apac003,g_apac_m.apac004,g_apac_m.apac005, 
       g_apac_m.apacownid,g_apac_m.apacowndp,g_apac_m.apaccrtid,g_apac_m.apaccrtdp,g_apac_m.apaccrtdt, 
       g_apac_m.apacmodid,g_apac_m.apacmoddt,g_apac_m.apacownid_desc,g_apac_m.apacowndp_desc,g_apac_m.apaccrtid_desc, 
       g_apac_m.apaccrtdp_desc,g_apac_m.apacmodid_desc
   
 
   #檢查是否允許此動作
   IF NOT aapi350_action_chk() THEN
      CLOSE aapi350_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_apac_m.apac001,g_apac_m.apacl003,g_apac_m.apacstus,g_apac_m.apac002,g_apac_m.apac007, 
       g_apac_m.apac006,g_apac_m.apacsite,g_apac_m.apacsite_desc,g_apac_m.apac003,g_apac_m.apac003_desc, 
       g_apac_m.apac004,g_apac_m.apac004_desc,g_apac_m.apac005,g_apac_m.apac005_desc,g_apac_m.apacownid, 
       g_apac_m.apacownid_desc,g_apac_m.apacowndp,g_apac_m.apacowndp_desc,g_apac_m.apaccrtid,g_apac_m.apaccrtid_desc, 
       g_apac_m.apaccrtdp,g_apac_m.apaccrtdp_desc,g_apac_m.apaccrtdt,g_apac_m.apacmodid,g_apac_m.apacmodid_desc, 
       g_apac_m.apacmoddt
 
   CASE g_apac_m.apacstus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_apac_m.apacstus
            
            WHEN "N"
               HIDE OPTION "inactive"
            WHEN "Y"
               HIDE OPTION "active"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      
      #end add-point
      
      
	  
      ON ACTION inactive
         IF cl_auth_chk_act("inactive") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.inactive"
 
            #end add-point
         END IF
         EXIT MENU
      ON ACTION active
         IF cl_auth_chk_act("active") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.active"
        
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      ) OR 
      g_apac_m.apacstus = lc_state OR cl_null(lc_state) THEN
      CLOSE aapi350_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   IF lc_state = "N" THEN      
      CALL s_transaction_begin()
      IF NOT cl_ask_confirm('aap-00042') THEN
         CALL s_transaction_end('N','0')   #160816-00068#14 by 08209 add
         RETURN
      ELSE
         IF NOT aapi350_unactive_upd() THEN
            CALL s_transaction_end('N','0')
            RETURN
         ELSE
            CALL s_transaction_end('Y','0')
         END IF        
      END IF
   END IF
   
   IF lc_state = "Y" THEN
      CALL s_transaction_begin() 
      IF NOT cl_ask_confirm('aap-00043') THEN 
         CALL s_transaction_end('N','0')   #160816-00068#14 by 08209 add
         RETURN
      ELSE
         IF NOT aapi350_active_upd() THEN
            CALL s_transaction_end('N','0')
            RETURN
         ELSE
            CALL s_transaction_end('Y','0')
         END IF        
      END IF
   END IF
   #end add-point
   
   LET g_apac_m.apacmodid = g_user
   LET g_apac_m.apacmoddt = cl_get_current()
   LET g_apac_m.apacstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE apac_t 
      SET (apacstus,apacmodid,apacmoddt) 
        = (g_apac_m.apacstus,g_apac_m.apacmodid,g_apac_m.apacmoddt)     
    WHERE apacent = g_enterprise AND apac001 = g_apac_m.apac001
 
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE aapi350_master_referesh USING g_apac_m.apac001 INTO g_apac_m.apac001,g_apac_m.apacstus, 
          g_apac_m.apac002,g_apac_m.apac007,g_apac_m.apac006,g_apac_m.apacsite,g_apac_m.apac003,g_apac_m.apac004, 
          g_apac_m.apac005,g_apac_m.apacownid,g_apac_m.apacowndp,g_apac_m.apaccrtid,g_apac_m.apaccrtdp, 
          g_apac_m.apaccrtdt,g_apac_m.apacmodid,g_apac_m.apacmoddt,g_apac_m.apacownid_desc,g_apac_m.apacowndp_desc, 
          g_apac_m.apaccrtid_desc,g_apac_m.apaccrtdp_desc,g_apac_m.apacmodid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_apac_m.apac001,g_apac_m.apacl003,g_apac_m.apacstus,g_apac_m.apac002,g_apac_m.apac007, 
          g_apac_m.apac006,g_apac_m.apacsite,g_apac_m.apacsite_desc,g_apac_m.apac003,g_apac_m.apac003_desc, 
          g_apac_m.apac004,g_apac_m.apac004_desc,g_apac_m.apac005,g_apac_m.apac005_desc,g_apac_m.apacownid, 
          g_apac_m.apacownid_desc,g_apac_m.apacowndp,g_apac_m.apacowndp_desc,g_apac_m.apaccrtid,g_apac_m.apaccrtid_desc, 
          g_apac_m.apaccrtdp,g_apac_m.apaccrtdp_desc,g_apac_m.apaccrtdt,g_apac_m.apacmodid,g_apac_m.apacmodid_desc, 
          g_apac_m.apacmoddt
   END IF
 
