################################################################################
PUBLIC FUNCTION aapi350_apaddel_chk(p_ac)
   DEFINE p_ac        LIKE type_t.num10
   DEFINE l_count     LIKE type_t.num10  # 錯誤數目     
   DEFINE r_success   BOOLEAN
   DEFINE r_errno     LIKE gzze_t.gzze001    
   LET r_success = TRUE
   LET r_errno =''
   
   LET l_count = ''
   SELECT COUNT(*) INTO l_count
     FROM apce_t
    WHERE apceent  = g_enterprise
      AND apce008  = g_apad_d[p_ac].apad002
      
      
   IF cl_null(l_count) THEN 
      LET l_count = 0
   END IF
    
    IF l_count > 0 THEN
       LET r_success = FALSE
       LET r_errno ='aap-00088'
    END IF
    
   RETURN r_success,r_errno

END FUNCTION

################################################################################
# Descriptions...: 須檢核（ＡＰ沖帳明細apce_t.apce008付款帳戶), 
#                  已有記錄者，提示訊息且不可刪除，只允許作失效
# Memo...........:
# Usage..........:aapi350_apacdel_chk()
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PUBLIC FUNCTION aapi350_apacdel_chk()
   DEFINE l_clist       RECORD
          apad002   LIKE apad_t.apad002
      END RECORD      
   DEFINE p_ac        LIKE type_t.num10
   DEFINE l_cnt       LIKE type_t.num10 
   DEFINE l_sql       STRING
   DEFINE l_count     LIKE type_t.num10  # 錯誤數目     
   DEFINE r_success   BOOLEAN 
   LET r_success = TRUE 
  
   LET l_sql = "SELECT apad002 ",
               "  FROM  apad_t",
               " WHERE apadent = ",g_enterprise," AND apad001 = '",g_apac_m.apac001,"' "
               
   LET l_cnt = g_apad_d.getLength() #單身總筆數
   CALL cl_err_collect_init() 
   PREPARE apce_pre FROM l_sql
   DECLARE apce_cur CURSOR FOR apce_pre 
      FOREACH apce_cur INTO l_clist.apad002
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = ''
            LET g_errparam.popup = TRUE
            CALL cl_err()

            EXIT FOREACH
         END IF                       
     
         SELECT COUNT(*) INTO l_count
           FROM apce_t
          WHERE apceent  = g_enterprise
            AND apce008  = l_clist.apad002            
          IF cl_null(l_count) THEN 
             LET l_count = 0
          END IF        
          IF l_count > 0 THEN 
             INITIALIZE g_errparam TO NULL 
             LET g_errparam.extend = '' 
             LET g_errparam.code   = 'aap-00088' 
             LET g_errparam.popup  = TRUE 
             CALL cl_err()          
#             CALL cl_errmsg('apad002',l_clist.apad002,'','aap-00088',1)
             LET r_success = FALSE
          END IF        
      END FOREACH
   CALL cl_err_collect_show()
   RETURN r_success

  
END FUNCTION

