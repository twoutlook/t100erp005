                    LET la_param.param[6] = 'N'
               ELSE               
                  LET la_param.param[6] = g_xrea_d[l_ac].xrea100
               END IF
               LET la_param.param[7] = g_xrea_d[l_ac].xrea009
               LET ls_js = util.JSON.stringify( la_param )
