      LET g_detail_idx = 1
      LET g_detail_idx2 = 1
      CALL aapq910_b_fill()
   ELSE
      CALL aapq910_query()
   END IF
   
   WHILE TRUE
 
      IF g_action_choice = "logistics" THEN
         #清除畫面及相關資料
         CLEAR FORM
         CALL g_xrea_d.clear()
 
         LET g_wc  = " 1=2"
         LET g_wc2 = " 1=1"
