       WHERE apgkent = g_enterprise
         AND apgkdocno = g_apgl_m.apgl023
         AND apgk013 = g_apgl_m.apgl007
      IF cl_null(l_count) THEN LET l_count = 0 END IF
      IF l_count > 0 THEN
         CALL cl_set_comp_entry('apgl007',FALSE)            
      END IF
   END IF
  
   #end add-point 
 
