            IF NOT cl_null(g_apgl_m.apgl004) THEN #申請單帶出道單單號              
               LET l_count = 0
               SELECT COUNT(*) INTO l_count
                 FROM apgk_t WHERE apgkent = g_enterprise
                  AND apgkcomp = g_apgl_m.apglcomp AND apgk003 = g_apgl_m.apgl003   AND apgkstus = 'Y'            
               IF l_count = 1 THEN       #如果只有一筆 帶出到單單號 
                  SELECT apgkdocno,apgk013 INTO g_apgl_m.apgl023,g_apgl_m.apgl007 #到單單號/裝運單號
