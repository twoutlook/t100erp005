PRIVATE FUNCTION aapt550_tax_ins_b2(p_ac)
DEFINE p_ac         LIKE type_t.num10
DEFINE l_sfin4012   LIKE type_t.chr80
DEFINE l_ld         LIKE glaa_t.glaald
DEFINE l_dummy2     LIKE type_t.num20_6
DEFINE l_dummy3     LIKE type_t.num20_6
DEFINE l_glaa001    LIKE glaa_t.glaa001
DEFINE l_oodb004    LIKE oodb_t.oodb004
DEFINE l_oodb011    LIKE oodb_t.oodb011
DEFINE l_apcb105    LIKE apcb_t.apcb105
DEFINE ls_js         STRING
DEFINE lc_param      RECORD
         apca004     LIKE apca_t.apca004
                 END RECORD  

   LET l_ld = NULL
   SELECT glaald INTO l_ld FROM glaa_t
    WHERE   glaaent = g_enterprise
      AND glaacomp = g_apgl_m.apglcomp
      AND glaa014 = 'Y'
   
   #匯率修改產生s_tax
   IF NOT cl_null(g_apgm2_d[p_ac].apgc006) AND NOT cl_null(g_apgm2_d[p_ac].apgc100)
      AND NOT cl_null(g_apgm2_d[p_ac].apgc101) AND NOT cl_null(l_ld)
      THEN
      CALL s_tax_chk(g_apgm2_d[p_ac].apgcorga, g_apgm2_d[p_ac].apgc006) 
         RETURNING g_sub_success,l_oodb004,g_apgm2_d[p_ac].l_apgc006_2,g_apgm2_d[p_ac].l_apgc006_1,l_oodb011
      IF g_apgm2_d[p_ac].l_apgc006_1 = 'Y' THEN
         LET l_apcb105 = g_apgm2_d[p_ac].apgc105
      ELSE
         LET l_apcb105 = g_apgm2_d[p_ac].apgc103
      END IF
      
       CALL s_tax_ins(g_apgl_m.apgldocno,g_apgm2_d[p_ac].apgcseq,'0',g_apgm2_d[p_ac].apgcorga,l_apcb105,
                     g_apgm2_d[p_ac].apgc006,1,g_apgm2_d[p_ac].apgc100,g_apgm2_d[p_ac].apgc101,l_ld,1,1)
           RETURNING g_apgm2_d[p_ac].apgc103,g_apgm2_d[p_ac].apgc104,g_apgm2_d[p_ac].apgc105,
                     g_apgm2_d[p_ac].apgc113,g_apgm2_d[p_ac].apgc114,g_apgm2_d[p_ac].apgc115,
                     l_dummy2,l_dummy2,l_dummy2,
                     l_dummy3,l_dummy3,l_dummy3
   END IF
   DISPLAY BY NAME g_apgm2_d[p_ac].apgc006
END FUNCTION

