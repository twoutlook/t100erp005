# Descriptions...: 裝運單相關資訊
# Memo...........:
# Usage..........: CALL aapt550_apgi_ref(p_apgidocno)
# Date & Author..: 2016/03/14 By Hans
# Modify.........:
################################################################################
PRIVATE FUNCTION aapt550_apgi_ref(p_apgidocno)
DEFINE p_apgidocno LIKE apgi_t.apgidocno


      SELECT DISTINCT apgi005,apgi012,apgi013, #運送方式/起運日期/預計抵達日期
                      apgi009,apgi008,apgi014, #裝運公司/船名航次/領櫃地點
                      apgi015,apgi016,apgi017  #領櫃代號/交櫃地點/櫃量                     
        INTO g_apgl_m.apgl008,g_apgl_m.apgl012,g_apgl_m.apgl013,
             g_apgl_m.apgl014,g_apgl_m.apgl015,g_apgl_m.apgl017,
             g_apgl_m.apgl018,g_apgl_m.apgl019,g_apgl_m.apgl020
        FROM apgi_t
       WHERE apgient = g_enterprise AND apgidocno = p_apgidocno
         AND apgicomp = g_apgl_m.apglcomp
   IF NOT cl_null(g_apgl_m.apgl008) THEN
      CALL s_desc_get_acc_desc('263',g_apgl_m.apgl008) RETURNING g_apgl_m.apgl008_desc
   END IF
         
   DISPLAY BY NAME  g_apgl_m.apgl008,g_apgl_m.apgl012,g_apgl_m.apgl013,
                    g_apgl_m.apgl014,g_apgl_m.apgl015,g_apgl_m.apgl017,
                    g_apgl_m.apgl018,g_apgl_m.apgl019,g_apgl_m.apgl020,
                    g_apgl_m.apgl008_desc
END FUNCTION

################################################################################
# Descriptions...: 費用單身匯率重計算
# Memo...........:
# Usage..........: CALL aapt550_curr_recount_b2(l_ac)
# Date & Author..: 2016/03/16 By Hans
# Modify.........:
################################################################################
PRIVATE FUNCTION aapt550_curr_recount_b2(p_ac)
 DEFINE p_ac         LIKE type_t.num10
   DEFINE l_sfin4012   LIKE type_t.chr80
   DEFINE l_ld         LIKE glaa_t.glaald
   DEFINE l_dummy2     LIKE type_t.num20_6
   DEFINE l_dummy3     LIKE type_t.num20_6
   DEFINE l_glaa001    LIKE glaa_t.glaa001
   DEFINE l_oodb004    LIKE oodb_t.oodb004
   DEFINE l_oodb005    LIKE oodb_t.oodb005
   DEFINE l_oodb006    LIKE oodb_t.oodb006
   DEFINE l_oodb011    LIKE oodb_t.oodb011
   DEFINE l_apcb105    LIKE apcb_t.apcb105
   DEFINE ls_js         STRING
   DEFINE lc_param      RECORD
            apca004     LIKE apca_t.apca004
                    END RECORD
   
   
   LET l_ld = NULL LET l_glaa001 = NULL
   SELECT glaald ,glaa001
     INTO l_ld ,l_glaa001
     FROM glaa_t
    WHERE glaaent = g_enterprise
      AND glaacomp = g_apgl_m.apglcomp
      AND glaa014 = 'Y'
   IF cl_null(p_ac) OR p_ac <=0 THEN RETURN END IF
   
   #有輸入交易帳戶時,取銀存支出匯率來源
   IF NOT cl_null(g_apgm2_d[l_ac].apgc014) THEN
      CALL cl_get_para(g_enterprise,g_apgl_m.apglcomp,'S-FIN-4012') RETURNING l_sfin4012
   ELSE
      LET l_sfin4012 = ''
   END IF
   
   #160824-00049#3-----s
   IF g_apgm2_d[l_ac].apgc100 = l_glaa001 THEN
      LET g_apgm2_d[l_ac].apgc101 = 1
   ELSE
      IF g_apgm2_d[l_ac].apgc100 <> g_apgl_m.apgl100 THEN   
   #160824-00049#3-----e   
         IF l_sfin4012  = '23' THEN
            #銀行日平均匯率
            CALL s_anm_get_exrate(l_ld,g_apgl_m.apglcomp,g_apgm2_d[l_ac].apgc014,g_apgm2_d[l_ac].apgc100,l_glaa001,g_apgl_m.apgldocdt) RETURNING g_apgm2_d[l_ac].apgc101
            #160824-00049#3-----s
            IF cl_null(g_apgm2_d[l_ac].apgc101)THEN
               LET g_apgm2_d[l_ac].apgc101 = 1
               INITIALIZE g_errparam.* TO NULL
               LET g_errparam.extend = ''
               LET g_errparam.code = 'aap-00587'
               LET g_errparam.popup = TRUE
               CALL cl_err()              
            END IF
            #160824-00049#3-----e
         ELSE
            LET lc_param.apca004 = g_apgl_m.apgl002
            LET ls_js = util.JSON.stringify(lc_param)
            CALL s_fin_get_curr_rate(g_apgl_m.apglcomp,l_ld,g_apgl_m.apgldocdt,g_apgm2_d[l_ac].apgc100,ls_js)
                 RETURNING g_apgm2_d[l_ac].apgc101,l_dummy2,l_dummy3
         END IF   
   #160824-00049#3-----s
      ELSE
         LET g_apgm2_d[l_ac].apgc101 = g_apgl_m.apgl101
      END IF
   END IF
   #160824-00049#3-----e  
   
   CALL aapt550_tax_ins_b2(p_ac)
END FUNCTION

