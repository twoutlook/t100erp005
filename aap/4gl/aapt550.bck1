################################################################################
PRIVATE FUNCTION aapt550_apgl007_chk(p_apgidocno)
DEFINE l_count       LIKE type_t.num5
DEFINE p_apgidocno   LIKE apgi_t.apgidocno
DEFINE r_success     LIKE type_t.num5
DEFINE r_errno       LIKE gzze_t.gzze001
DEFINE l_apgi003     LIKE apgi_t.apgi003
DEFINE l_apgistus    LIKE apgi_t.apgistus


   LET r_success = TRUE 
   #檢核裝運單號是否存在   
   SELECT apgi003,apgistus INTO l_apgi003,l_apgistus
     FROM apgi_t WHERE apgidocno = p_apgidocno   
      AND apgicomp = g_apgl_m.apglcomp     
      AND apgient = g_enterprise
      
   CASE
      WHEN SQLCA.SQLCODE = 100
         LET r_success = FALSE
         LET r_errno = 'aap-00502'    
      WHEN l_apgi003 <> g_apgl_m.apgl003
         LET r_success = FALSE
         LET r_errno = 'aap-00533' 
      WHEN l_apgistus <> 'Y' 
         LET r_success = FALSE
         LET r_errno = 'aap-00523'
   END CASE
   
  
   RETURN r_success,r_errno



END FUNCTION

