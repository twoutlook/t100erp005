      CALL cl_set_comp_entry('apgl004',FALSE)
   END IF
  
   SELECT isai008 INTO l_isai008
     FROM isai_t,ooef_t WHERE isaient = ooefent AND isaient = g_enterprise                   
      AND isai001 = ooef019 AND ooef001 = g_apgl_m.apglcomp

   IF l_isai008 <> 'TW' THEN
      CALL cl_set_comp_required('apgl009',FALSE)
   END IF 
   #申請單之/裝運單號是否存在
   IF NOT cl_null(g_apgl_m.apgl007) THEN
      LET l_count = 0
