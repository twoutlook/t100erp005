################################################################################
PRIVATE FUNCTION aapt550_ins_apgm()
DEFINE l_i           LIKE type_t.num5
DEFINE l_sql         STRING
DEFINE l_apgm105_sum LIKE apgm_t.apgm105
DEFINE l_apgm115_sum LIKE apgm_t.apgm115
DEFINE l_ld          LIKE glaa_t.glaald
DEFINE l_glaa001     LIKE glaa_t.glaa001
DEFINE l_apga041     LIKE apga_t.apga041
DEFINE l_flag        LIKE type_t.chr1


   LET l_ld = NULL   LET l_glaa001 = NULL
   SELECT glaald ,glaa001
     INTO l_ld ,l_glaa001
     FROM glaa_t
    WHERE glaaent = g_enterprise
      AND glaacomp = g_apgl_m.apglcomp
      AND glaa014 = 'Y'              
        
  LET l_i = 1
  LET l_apgm105_sum = 0
  LET l_apgm115_sum = 0
  SELECT apga041 INTO l_apga041                                               #160428-00001#5多幣別採購否
    FROM apga_t WHERE apgaent = g_enterprise AND apgadocno = g_apgl_m.apgl004 #160428-00001#5多幣別採購否
  IF cl_null(g_apgl_m.apgl007) THEN #裝運單空白，由申請單帶出   
     LET l_flag = l_apga041
     LET l_sql = " SELECT apgbseq,apgborga,apgb001,apgb002, ",
                 "        apgb003,(apgb008-apgb010),apgb009,apgb006,   ",   #albireo 160628  補逗點
                 "        apgb100,apgb101                   ", #160428-00001#5 單身增加幣別/匯率
                 "  FROM apgb_t                             ",
                 " WHERE apgbent = '",g_enterprise,"'       ",
                 "   AND apgbdocno = '",g_apgl_m.apgl004,"' ",
                 "   AND apgbcomp = '",g_apgl_m.apglcomp,"' ",
                 "   AND (apgb008-apgb010) > 0              " 
     PREPARE aapt550_abgm_ins_prep01 FROM l_sql
     DECLARE aapt550_abgm_ins_curs01 CURSOR FOR aapt550_abgm_ins_prep01
     FOREACH aapt550_abgm_ins_curs01 INTO  g_apgm_d[l_i].apgmseq2,g_apgm_d[l_i].apgmorga,
                                           g_apgm_d[l_i].apgm001,g_apgm_d[l_i].apgm002,
                                           g_apgm_d[l_i].apgm003,g_apgm_d[l_i].apgm004,
                                           g_apgm_d[l_i].apgm005,g_apgm_d[l_i].apgm006,
                                           g_apgm_d[l_i].apgm100,g_apgm_d[l_i].apgm101 #160428-00001#5單身增加幣別/匯率
                                                   
                                                               
        SELECT MAX(apgmseq)+1 INTO g_apgm_d[l_i].apgmseq 
          FROM apgm_t
         WHERE apgment = g_enterprise
           AND apgmcomp = g_apgl_m.apglcomp
           AND apgmdocno = g_apgl_m.apgldocno
        IF cl_null(g_apgm_d[l_i].apgmseq) THEN LET g_apgm_d[l_i].apgmseq = 1 END IF
        LET g_apgm_d[l_i].apgm105 = g_apgm_d[l_i].apgm005 * g_apgm_d[l_i].apgm004
        #取位(原幣)
        CALL s_curr_round_ld('1',l_ld,l_glaa001,g_apgm_d[l_i].apgm105 ,2) 
           RETURNING g_sub_success,g_errno,g_apgm_d[l_i].apgm105
        #160428-00001#5---s---
        LET g_apgm_d[l_i].apgm115 =g_apgm_d[l_i].apgm105 * g_apgm_d[l_i].apgm101 
        #取位(本幣)
        CALL s_curr_round_ld('1',l_ld,l_glaa001,g_apgm_d[l_i].apgm115 ,2) 
           RETURNING g_sub_success,g_errno,g_apgm_d[l_i].apgm115
        #160428-00001#5---e---
        INSERT INTO apgm_t
                (apgment,apgmcomp,apgmdocno,apgmseq,
                 apgmseq2,apgmorga,apgm001,apgm002,
                 apgm003,apgm006,apgm005,apgm004,apgm105,
                 apgm100,apgm101,apgm115)                    #160428-00001#5 單身增加幣別/匯率
        VALUES(g_enterprise,g_apgl_m.apglcomp,g_apgl_m.apgldocno,g_apgm_d[l_i].apgmseq
             ,g_apgm_d[l_i].apgmseq2,g_apgm_d[l_i].apgmorga,g_apgm_d[l_i].apgm001, 
              g_apgm_d[l_i].apgm002,g_apgm_d[l_i].apgm003,g_apgm_d[l_i].apgm006, 
              g_apgm_d[l_i].apgm005,g_apgm_d[l_i].apgm004,g_apgm_d[l_i].apgm105,
              g_apgm_d[l_i].apgm100,g_apgm_d[l_i].apgm101,g_apgm_d[l_i].apgm115) #160428-00001#5 單身增加幣別/匯率/本幣
       LET l_i = l_i +1 
     END FOREACH  
   ELSE  
     LET l_sql = " SELECT apgjseq2,apgjorga,apgj001,apgj002,       ",
                 "        apgj003,apgj005,apgj004,                 ",
                 "        apgj100,apgj101                          ",
                 "   FROM apgj_t                                   ",
                 "  WHERE apgjent   = '",g_enterprise,"'           ",
                 "    AND apgjdocno = '",g_apgl_m.apgl007,"'       ", #裝運單號
                 "    AND apgjcomp  = '",g_apgl_m.apglcomp,"'      "
     PREPARE aapt550_abgm_ins_prep02 FROM l_sql
     DECLARE aapt550_abgm_ins_curs02 CURSOR FOR aapt550_abgm_ins_prep02
     FOREACH aapt550_abgm_ins_curs02 INTO  g_apgm_d[l_i].apgmseq2,g_apgm_d[l_i].apgmorga,
                                           g_apgm_d[l_i].apgm001,g_apgm_d[l_i].apgm002,
                                           g_apgm_d[l_i].apgm003,g_apgm_d[l_i].apgm004,
                                           g_apgm_d[l_i].apgm005,
                                           g_apgm_d[l_i].apgm100,g_apgm_d[l_i].apgm101 #160428-00001#5 單身增加幣別/匯率/本幣
   
                                                               
        SELECT MAX(apgmseq)+1 INTO g_apgm_d[l_i].apgmseq 
          FROM apgm_t
         WHERE apgment = g_enterprise
           AND apgmcomp = g_apgl_m.apglcomp
           AND apgmdocno = g_apgl_m.apgldocno
        IF cl_null(g_apgm_d[l_i].apgmseq) THEN LET g_apgm_d[l_i].apgmseq = 1 END IF        
        
        LET g_apgm_d[l_i].apgm105 = g_apgm_d[l_i].apgm005 * g_apgm_d[l_i].apgm004
        #取位(原幣)
        CALL s_curr_round_ld('1',l_ld,l_glaa001,g_apgm_d[l_i].apgm105 ,2) 
           RETURNING g_sub_success,g_errno,g_apgm_d[l_i].apgm105
        #160428-00001#5---s---
        LET g_apgm_d[l_i].apgm115 =g_apgm_d[l_i].apgm105 * g_apgm_d[l_i].apgm101 
        #取位(本幣)
        CALL s_curr_round_ld('1',l_ld,l_glaa001,g_apgm_d[l_i].apgm115 ,2) 
           RETURNING g_sub_success,g_errno,g_apgm_d[l_i].apgm115           
        #稅別/申請單上面的稅別
        LET g_apgm_d[l_i].apgm006 = ''
        SELECT apgb006 INTO g_apgm_d[l_i].apgm006 FROM apgb_t
         WHERE apgbent = g_enterprise AND apgbdocno = g_apgl_m.apgl004
           AND apgbseq = g_apgm_d[l_i].apgmseq2
        
        INSERT INTO apgm_t
                   (apgment,apgmcomp,apgmdocno,apgmseq,
                    apgmseq2,apgmorga,apgm001,apgm002,
                     apgm003,apgm006,apgm005,apgm004,apgm105,
                     apgm100,apgm101,apgm115)
                     
         VALUES(g_enterprise,g_apgl_m.apglcomp,g_apgl_m.apgldocno,g_apgm_d[l_i].apgmseq
                ,g_apgm_d[l_i].apgmseq2,g_apgm_d[l_i].apgmorga,g_apgm_d[l_i].apgm001, 
                 g_apgm_d[l_i].apgm002,g_apgm_d[l_i].apgm003,g_apgm_d[l_i].apgm006, 
                 g_apgm_d[l_i].apgm005,g_apgm_d[l_i].apgm004,g_apgm_d[l_i].apgm105,
                 g_apgm_d[l_i].apgm100,g_apgm_d[l_i].apgm101,g_apgm_d[l_i].apgm115)
         LET l_i = l_i +1    
      END FOREACH
   END IF     
   CALL g_apgm_d.deleteElement(g_apgm_d.getLength())
   #單身原幣金額回寫單頭
   CALL aapt550_apgm_recont() #160428-00001#5 增加本幣回寫    
                         
END FUNCTION

################################################################################
# Descriptions...: 檢查裝運單號是否存在
