################################################################################
# Descriptions...: 法人欄位檢核
# Memo...........: 檢核存在有效;檢核是否為法人組織;user有無權限使用此組織
# Usage..........: CALL aapt550_apglcomp_chk(p_apglcomp)
#                  RETURNING g_sub_success,g_errno
# Input parameter: p_apglcomp     法人
# Return code....: r_success
#                : r_errno
# Date & Author..: 2016/03/07 By Hans
# Modify.........:
################################################################################
PRIVATE FUNCTION aapt550_apglcomp_chk(p_apglcomp)
DEFINE p_apglcomp   LIKE apgl_t.apglcomp
DEFINE r_success    LIKE type_t.num5
DEFINE r_errno      LIKE gzze_t.gzze001
DEFINE l_ooef       RECORD
                    ooef003   LIKE ooef_t.ooef003,
                    ooefstus  LIKE ooef_t.ooefstus
                    END RECORD
DEFINE l_count      LIKE type_t.num10
DEFINE l_sql        STRING
   
   LET r_success = TRUE
   LET r_errno   = ''
   
   INITIALIZE l_ooef.* TO NULL
   SELECT ooef003,ooefstus 
     INTO l_ooef.*
     FROM ooef_t
    WHERE ooefent = g_enterprise
      AND ooef001 = p_apglcomp
      
   CASE
      WHEN SQLCA.SQLCODE = 100
         LET r_success = FALSE
         LET r_errno   = 'wss-00231'
      WHEN l_ooef.ooefstus <> 'Y'
         LET r_success = FALSE
         LET r_errno   = 'wss-00231'
      WHEN l_ooef.ooef003  <> 'Y'
         LET r_success = FALSE
         LET r_errno   = 'aap-00011'
   END CASE
   IF NOT r_success THEN RETURN r_success,r_errno END IF
   
   #USER azzi800權限
   #kris提示法人控卡若不存在azzi800設定則不可使用
   #此時不需展組織
   #orga才需展組織
   #albireo 160909 #160824-00049#1-----s
   LET l_count = NULL
   LET l_sql = "SELECT COUNT(*) FROM ooef_t WHERE ooefent = ",g_enterprise," ",
               "   AND ooef001 = '",p_apglcomp,"' ",
               "   AND ooef001 IN ",g_wc_cs_comp CLIPPED
   PREPARE sel_ooefp11 FROM l_sql
   EXECUTE sel_ooefp11 INTO l_count
   IF cl_null(l_count)THEN LET l_count = 0 END IF
   #albireo 160909 #160824-00049#1-----e
   
   IF l_count = 0 THEN
      LET r_success = FALSE
      LET r_errno   = 'ais-00228'
   END IF
   
   RETURN r_success,r_errno
END FUNCTION

################################################################################
# Descriptions...: 申請單號檢核
# Memo...........:
# Usage..........: CALL aapt550_apgl004_chk(p_apgadocno)
# Date & Author..: 2016/03/08 By Hans
# Modify.........:
################################################################################
PRIVATE FUNCTION aapt550_apgl_chk(p_apgadocno,p_apga001)
DEFINE p_apgadocno LIKE apga_t.apgadocno
DEFINE p_apga001   LIKE apga_t.apga001
DEFINE r_success   LIKE type_t.num5
DEFINE r_errno     LIKE gzze_t.gzze001
DEFINE l_count     LIKE type_t.num5
DEFINE l_apgastus  LIKE apga_t.apgastus
DEFINE l_apga004   LIKE apga_t.apga004
DEFINE l_apgacomp  LIKE apga_t.apgacomp

   LET r_success = TRUE 
   #檢核 申請單號是否存在
   IF NOT cl_null(p_apgadocno) THEN
      LET l_apgastus ='' 
      SELECT apgastus INTO l_apgastus
        FROM apga_t WHERE apgadocno = p_apgadocno
         AND apgaent = g_enterprise AND apga029 = 'N'
         AND apgastus = 'Y'               
      CASE
         WHEN SQLCA.SQLCODE = 100
            LET r_success = FALSE
            LET r_errno = 'aap-00511'
            RETURN r_success,r_errno
         WHEN l_apgastus <> 'Y' 
            LET r_success = FALSE
            LET r_errno = 'aap-00512'
            RETURN r_success,r_errno
      END CASE
   END IF  
   #檢核L/C是否存在
   IF NOT cl_null(p_apga001) THEN
      LET l_apgastus =''
      SELECT apgastus INTO l_apgastus
       FROM apga_t WHERE apga001 = p_apga001
        AND apgaent = g_enterprise  AND apga029 = 'N'             
      CASE
         WHEN SQLCA.SQLCODE = 100
            LET r_success = FALSE
            LET r_errno = 'aap-00448'
            RETURN r_success,r_errno
         WHEN l_apgastus <> 'Y' 
            LET r_success = FALSE
            LET r_errno = 'aap-00534'
            RETURN r_success,r_errno
      END CASE
       #檢核 供應商是否與aapt510相同
      IF NOT cl_null(g_apgl_m.apgl002) THEN
         LET l_apga004 = ''
         SELECT apga004 INTO l_apga004
          FROM apga_t WHERE apga001 = p_apga001 AND apga029 = 'N'
           AND apgaent = g_enterprise    
           
         CASE
            WHEN l_apga004 <>  g_apgl_m.apgl002  
               LET r_success = FALSE
               LET r_errno  = 'aap-00532' 
               RETURN r_success,r_errno            
         END CASE
      END IF
      IF NOT cl_null(g_apgl_m.apglcomp) THEN
         LET l_apgacomp = ''
         SELECT apgacomp INTO l_apgacomp
          FROM apga_t WHERE apga001 = p_apga001 AND apga029 = 'N'
           AND apgaent = g_enterprise
         CASE
            WHEN l_apgacomp <>  g_apgl_m.apglcomp
               LET r_success = FALSE
               LET r_errno  = 'aap-00547' 
               RETURN r_success,r_errno            
         END CASE
           
      END IF
   END IF
   
  
   
   IF NOT cl_null(p_apgadocno) AND NOT cl_null(p_apga001) THEN
      LET l_apgastus =''
      SELECT apgastus INTO l_apgastus
       FROM apga_t WHERE apga001 = p_apga001 AND apga029 = 'N'
        AND apgadocno = p_apgadocno
        AND apgaent = g_enterprise
        
      CASE
         WHEN l_apgastus <> 'Y' 
            LET r_success = FALSE
            LET r_errno = 'aap-00512'
            RETURN r_success,r_errno
      END CASE
   END IF

   RETURN r_success,r_errno

END FUNCTION

