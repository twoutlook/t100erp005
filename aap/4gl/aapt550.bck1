       g_apgl_m.apgl003,g_apgl_m.apgl004,g_apgl_m.apgl030,g_apgl_m.apglstus,g_apgl_m.apglownid,g_apgl_m.apglowndp, 
       g_apgl_m.apglcrtid,g_apgl_m.apglcrtdp,g_apgl_m.apglcrtdt,g_apgl_m.apglmodid,g_apgl_m.apglmoddt, 
       g_apgl_m.apglcnfid,g_apgl_m.apglcnfdt,g_apgl_m.apgl023,g_apgl_m.apgl022,g_apgl_m.apgl100,g_apgl_m.apgl101, 
       g_apgl_m.apgl103,g_apgl_m.apgl113,g_apgl_m.apgl029,g_apgl_m.apgl024,g_apgl_m.apgl025,g_apgl_m.apgl026, 
       g_apgl_m.apgl027,g_apgl_m.apgl028,g_apgl_m.apgl007,g_apgl_m.apgl008,g_apgl_m.apgl009,g_apgl_m.apgl010, 
       g_apgl_m.apgl011,g_apgl_m.apgl012,g_apgl_m.apgl013,g_apgl_m.apgl014,g_apgl_m.apgl015,g_apgl_m.apgl016, 
       g_apgl_m.apgl017,g_apgl_m.apgl018,g_apgl_m.apgl019,g_apgl_m.apgl020,g_apgl_m.apgl021,g_apgl_m.apglownid_desc, 
