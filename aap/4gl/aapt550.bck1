   CALL s_transaction_begin()
   
   OPEN aapt550_cl USING g_enterprise,g_apgl_m.apglcomp,g_apgl_m.apgldocno
   IF STATUS THEN
      CLOSE aapt550_cl
      CALL s_transaction_end('N','0')
