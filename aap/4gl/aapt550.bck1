              SELECT apga004 INTO g_apgl_m.apgl002
                FROM apga_t WHERE apga001 = g_apgl_m.apgl003
                 AND apgaent = g_enterprise
               CALL s_desc_get_trading_partner_abbr_desc(g_apgl_m.apgl002) RETURNING g_apgl_m.apgl002_desc
               DISPLAY BY NAME g_apgl_m.apgl002,g_apgl_m.apgl002_desc
