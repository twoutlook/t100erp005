{<section id="aapt110_01.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION aapt110_01_b_fill(p_wc2)              #BODY FILL UP
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   
   #end add-point
   DEFINE p_wc2    STRING
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   DEFINE l_str    LIKE type_t.chr1
   LET p_wc2 = "apba005 = '",g_pmdtdocno,"'"
   IF g_pmdtseq <> 0 THEN
      LET p_wc2 = p_wc2 ," AND apba006 = ",g_pmdtseq
   END IF  
   #end add-point
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前 name="b_fill.sql_before"
   LET g_sql = " SELECT apbb009,apbb010,apbb012,apbb013,apbb014,",
               "        apbb015",
               "   FROM apba_t,apbb_t ",
               "  WHERE apbaent = apbbent AND apbadocno = apbbdocno ",
               "    AND apbaent = ",g_enterprise," AND apba005 = ? AND apba006 = ?"
   PREPARE aapt110_01_p_apbb FROM g_sql
   DECLARE aapt110_01_c_apbb CURSOR FOR aapt110_01_p_apbb
   #end add-point
 
   LET g_sql = "SELECT  DISTINCT t0.apbaseq,t0.apba005,t0.apba006,t0.apba007,t0.apba008,t0.apbadocno, 
       t0.apba010,t0.apba014,t0.apba103,t0.apba104,t0.apba105,t0.apba113,t0.apba114,t0.apba115  FROM apba_t t0", 
 
               "",
               
               " WHERE t0.apbaent= ?  AND  1=1 AND (", p_wc2, ") " 
   #add-point:b_fill段sql wc name="b_fill.sql_wc"
   
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("apba_t"),
                      " ORDER BY t0.apbadocno,t0.apbaseq"
   
   #add-point:b_fill段sql之後 name="b_fill.sql_after"
   #因有正負數需求因此重寫b_fill
   LET g_sql = "SELECT  UNIQUE t0.apbaseq,t0.apba005,t0.apba006,apba007,t0.apba008,t0.apbadocno,t0.apba010*apba012, 
                         t0.apba014,
                         t0.apba103*apba012,t0.apba104*apba012,t0.apba105*apba012,
                         t0.apba113*apba012,t0.apba114*apba012,t0.apba115*apba012
                    FROM apba_t t0",                
               " WHERE t0.apbaent= ?  AND  1=1 AND (", p_wc2, ") " 
   #add-point:b_fill段sql wc

   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("apba_t"),
                      " ORDER BY t0.apba006"
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"apba_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE aapt110_01_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aapt110_01_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_apba_d.clear()
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_apba_d[l_ac].apbaseq,g_apba_d[l_ac].apba005,g_apba_d[l_ac].apba006,g_apba_d[l_ac].apba007, 
       g_apba_d[l_ac].apba008,g_apba_d[l_ac].apbadocno,g_apba_d[l_ac].apba010,g_apba_d[l_ac].apba014, 
       g_apba_d[l_ac].apba103,g_apba_d[l_ac].apba104,g_apba_d[l_ac].apba105,g_apba_d[l_ac].apba113,g_apba_d[l_ac].apba114, 
       g_apba_d[l_ac].apba115
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充 name="b_fill.fill"
      OPEN aapt110_01_c_apbb USING g_apba_d[l_ac].apba005,g_apba_d[l_ac].apba006 
      FETCH aapt110_01_c_apbb INTO g_apba_d[l_ac].apbb009,g_apba_d[l_ac].apbb010,g_apba_d[l_ac].apbb012,g_apba_d[l_ac].apbb013,g_apba_d[l_ac].apbb014,
            g_apba_d[l_ac].apbb015
      SELECT pmdt006 INTO g_apba_d[l_ac].apba007
        FROM pmdt_t
       WHERE pmdtent = g_enterprise
         AND pmdtdocno = g_apba_d[l_ac].apba005 AND pmdtseq = g_apba_d[l_ac].apba006 
      CALL s_desc_get_item_desc(g_apba_d[l_ac].apba007) RETURNING g_apba_d[l_ac].apba008,l_str 
      #160602-00023#1----begin
      CASE
          WHEN NOT cl_null(g_apba_d[l_ac].apba008) AND NOT cl_null(l_str)
               LET g_apba_d[l_ac].apba008 = g_apba_d[l_ac].apba008,"/",l_str 
          WHEN NOT cl_null(g_apba_d[l_ac].apba008)
               LET g_apba_d[l_ac].apba008 = g_apba_d[l_ac].apba008
          WHEN NOT cl_null(l_str)
               LET g_apba_d[l_ac].apba008 = l_str 
      END CASE      
      #160602-00023#1----end      
      #end add-point
      
      CALL aapt110_01_detail_show()      
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
 
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   LET g_error_show = 0
   
 
   
   CALL g_apba_d.deleteElement(g_apba_d.getLength())   
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_apba_d.getLength()
 
      #add-point:b_fill段key值相關欄位 name="b_fill.keys.fill"
      
      #end add-point
   END FOR
   
   IF g_cnt > g_apba_d.getLength() THEN
      LET l_ac = g_apba_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_apba_d.getLength()
      LET g_apba_d_mask_o[l_ac].* =  g_apba_d[l_ac].*
      CALL aapt110_01_apba_t_mask()
      LET g_apba_d_mask_n[l_ac].* =  g_apba_d[l_ac].*
   END FOR
   
 
   
   LET l_ac = g_cnt
 
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   LET g_sql = " SELECT apbbdocno,apbb009,apbb010,apbb014,",
               "        SUM(apba103*apba012),SUM(apba104*apba012),SUM(apba105*apba012),",
               "        SUM(apba113*apba012),SUM(apba114*apba012),SUM(apba115*apba012) ",
               "   FROM apba_t,apbb_t ",
               "  WHERE apbaent = apbbent AND apbadocno = apbbdocno ",
               "    AND apbaent = ",g_enterprise," AND apba005 = ?",
               "  GROUP BY apbbdocno,apbb009,apbb010,apbb014"               
   LET g_sql = cl_sql_add_mask(g_sql)
   PREPARE aapt110_01_pb1 FROM g_sql
   DECLARE b_fill_curs1 CURSOR FOR aapt110_01_pb1
   
   CALL g_apba_d2.clear()
   LET g_cnt = l_ac
   LET l_ac = 1    
   OPEN b_fill_curs1 USING g_pmdtdocno
   FOREACH b_fill_curs1 INTO g_apba_d2[l_ac].apbbdocno,g_apba_d2[l_ac].apbb009,g_apba_d2[l_ac].apbb010,g_apba_d2[l_ac].apbb014, 
       g_apba_d2[l_ac].apba103,g_apba_d2[l_ac].apba104,g_apba_d2[l_ac].apba105,
       g_apba_d2[l_ac].apba113,g_apba_d2[l_ac].apba114,g_apba_d2[l_ac].apba115
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF

      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
 
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   LET g_error_show = 0
   
   CALL g_apba_d2.deleteElement(g_apba_d2.getLength())   

   IF g_cnt > g_apba_d2.getLength() THEN
      LET l_ac = g_apba_d2.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_apba_d.getLength()
   DISPLAY g_detail_idx TO FORMONLY.idx
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE aapt110_01_pb
   
END FUNCTION
 
