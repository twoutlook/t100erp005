{<section id="aapt590.fetch" >}
#+ 指定PK後抓取單頭其他資料
PRIVATE FUNCTION aapt590_fetch(p_flag)
   #add-point:fetch段define(客製用) name="fetch.define_customerization"
   
   #end add-point    
   DEFINE p_flag     LIKE type_t.chr1
   DEFINE ls_msg     STRING
   #add-point:fetch段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="fetch.define"
   
   #end add-point    
   
   #add-point:Function前置處理  name="fetch.pre_function"
   
   #end add-point
   
   IF g_browser_cnt = 0 THEN
      RETURN
   END IF
 
   #清空第二階單身
 
   
   CALL cl_ap_performance_next_start()
   CASE p_flag
      WHEN 'F' 
         LET g_current_idx = 1
      WHEN 'L'  
         LET g_current_idx = g_browser.getLength()              
      WHEN 'P'
         IF g_current_idx > 1 THEN               
            LET g_current_idx = g_current_idx - 1
         END IF 
      WHEN 'N'
         IF g_current_idx < g_header_cnt THEN
            LET g_current_idx =  g_current_idx + 1
         END IF        
      WHEN '/'
         IF (NOT g_no_ask) THEN    
            CALL cl_set_act_visible("accept,cancel", TRUE)    
            CALL cl_getmsg('fetch',g_lang) RETURNING ls_msg
            LET INT_FLAG = 0
 
            PROMPT ls_msg CLIPPED,':' FOR g_jump
               #交談指令共用ACTION
               &include "common_action.4gl" 
            END PROMPT
 
            CALL cl_set_act_visible("accept,cancel", FALSE)    
            IF INT_FLAG THEN
                LET INT_FLAG = 0
                EXIT CASE  
            END IF           
         END IF
         
         IF g_jump > 0 AND g_jump <= g_browser.getLength() THEN
             LET g_current_idx = g_jump
         END IF
         LET g_no_ask = FALSE  
   END CASE 
 
   CALL g_curr_diag.setCurrentRow("s_browse", g_current_idx) #設定browse 索引
   
   LET g_current_row = g_current_idx
   LET g_detail_cnt = g_header_cnt                  
   
   #單身總筆數顯示
   IF g_detail_cnt > 0 THEN
      #若單身有資料時, idx至少為1
      IF g_detail_idx <= 0 THEN
         LET g_detail_idx = 1
      END IF
      DISPLAY g_detail_idx TO FORMONLY.idx  
   ELSE
      LET g_detail_idx = 0
      DISPLAY '' TO FORMONLY.idx    
   END IF
   
   #瀏覽頁筆數顯示
   LET g_browser_idx = g_pagestart+g_current_idx-1
   DISPLAY g_browser_idx TO FORMONLY.b_index   #當下筆數
   DISPLAY g_browser_idx TO FORMONLY.h_index   #當下筆數
   
   CALL cl_navigator_setting( g_current_idx, g_browser_cnt )
 
   #代表沒有資料
   IF g_current_idx = 0 OR g_browser.getLength() = 0 THEN
      RETURN
   END IF
   
   #避免超出browser資料筆數上限
   IF g_current_idx > g_browser.getLength() THEN
      LET g_browser_idx = g_browser.getLength()
      LET g_current_idx = g_browser.getLength()
   END IF
   
   LET g_apgn_m.apgncomp = g_browser[g_current_idx].b_apgncomp
   LET g_apgn_m.apgndocno = g_browser[g_current_idx].b_apgndocno
 
   
   #重讀DB,因TEMP有不被更新特性
   EXECUTE aapt590_master_referesh USING g_apgn_m.apgncomp,g_apgn_m.apgndocno INTO g_apgn_m.apgncomp, 
       g_apgn_m.apgn001,g_apgn_m.apgndocno,g_apgn_m.apgndocdt,g_apgn_m.apgn013,g_apgn_m.apgn003,g_apgn_m.apgnstus, 
       g_apgn_m.apgnownid,g_apgn_m.apgnowndp,g_apgn_m.apgncrtid,g_apgn_m.apgncrtdp,g_apgn_m.apgncrtdt, 
       g_apgn_m.apgnmodid,g_apgn_m.apgnmoddt,g_apgn_m.apgncnfid,g_apgn_m.apgncnfdt,g_apgn_m.apgn100, 
       g_apgn_m.apgn103,g_apgn_m.apgn104,g_apgn_m.apgn106,g_apgn_m.apgn108,g_apgn_m.apgn109,g_apgn_m.apgn102, 
       g_apgn_m.apgn012,g_apgn_m.apgn101,g_apgn_m.apgn118,g_apgn_m.apgn119,g_apgn_m.apgn112,g_apgn_m.apgn005, 
       g_apgn_m.apgn004,g_apgn_m.apgn105,g_apgn_m.apgn107,g_apgn_m.apgn002,g_apgn_m.apgn006,g_apgn_m.apgn007, 
       g_apgn_m.apgn008,g_apgn_m.apgn009,g_apgn_m.apgn010,g_apgn_m.apgn011,g_apgn_m.apgncomp_desc,g_apgn_m.apgn001_desc, 
       g_apgn_m.apgnownid_desc,g_apgn_m.apgnowndp_desc,g_apgn_m.apgncrtid_desc,g_apgn_m.apgncrtdp_desc, 
       g_apgn_m.apgnmodid_desc,g_apgn_m.apgncnfid_desc,g_apgn_m.apgn005_desc,g_apgn_m.apgn006_desc,g_apgn_m.apgn007_desc, 
       g_apgn_m.apgn008_desc,g_apgn_m.apgn009_desc,g_apgn_m.apgn010_desc,g_apgn_m.apgn011_desc
   
   #遮罩相關處理
   LET g_apgn_m_mask_o.* =  g_apgn_m.*
   CALL aapt590_apgn_t_mask()
   LET g_apgn_m_mask_n.* =  g_apgn_m.*
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL aapt590_set_act_visible()   
   CALL aapt590_set_act_no_visible()
   
   #add-point:fetch段action控制 name="fetch.action_control"
   
   #end add-point  
   
   
   
   #add-point:fetch結束前 name="fetch.after"
   #160428-00001#7-----s
   SELECT apga104,apga108,apga102 INTO g_apgn_m.l_apga104,   g_apgn_m.l_apga108 ,   g_apgn_m.l_apga102 
     FROM apga_t
    WHERE apgaent = g_enterprise
      AND apgacomp = g_apgn_m.apgncomp
      AND apgadocno = g_apgn_m.apgn013
   IF cl_null(g_apgn_m.l_apga104)THEN LET g_apgn_m.l_apga104 = 0 END IF
   IF cl_null(g_apgn_m.l_apga108)THEN LET g_apgn_m.l_apga108 = 0 END IF
   IF cl_null(g_apgn_m.l_apga102)THEN LET g_apgn_m.l_apga102 = 0 END IF
   
   LET g_apgn_m.l_apgn102 = g_apgn_m.apgn102
   LET g_apgn_m.l_apgn108 = g_apgn_m.apgn108
   LET g_apgn_m.l_apgn109 = g_apgn_m.apgn109
   
   SELECT glaa001 INTO g_apgn_m.l_glaa001 FROM glaa_t
    WHERE glaaent = g_enterprise
      AND glaacomp = g_apgn_m.apgncomp
