      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN aapt590_cl USING g_enterprise,g_apgn_m.apgncomp,g_apgn_m.apgndocno
   IF STATUS THEN
      CLOSE aapt590_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aapt590_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE aapt590_master_referesh USING g_apgn_m.apgncomp,g_apgn_m.apgndocno INTO g_apgn_m.apgncomp, 
       g_apgn_m.apgn001,g_apgn_m.apgndocno,g_apgn_m.apgndocdt,g_apgn_m.apgn013,g_apgn_m.apgn003,g_apgn_m.apgnstus, 
       g_apgn_m.apgnownid,g_apgn_m.apgnowndp,g_apgn_m.apgncrtid,g_apgn_m.apgncrtdp,g_apgn_m.apgncrtdt, 
       g_apgn_m.apgnmodid,g_apgn_m.apgnmoddt,g_apgn_m.apgncnfid,g_apgn_m.apgncnfdt,g_apgn_m.apgn100, 
       g_apgn_m.apgn103,g_apgn_m.apgn104,g_apgn_m.apgn106,g_apgn_m.apgn108,g_apgn_m.apgn109,g_apgn_m.apgn102, 
       g_apgn_m.apgn012,g_apgn_m.apgn101,g_apgn_m.apgn118,g_apgn_m.apgn119,g_apgn_m.apgn112,g_apgn_m.apgn005, 
       g_apgn_m.apgn004,g_apgn_m.apgn105,g_apgn_m.apgn107,g_apgn_m.apgn002,g_apgn_m.apgn006,g_apgn_m.apgn007, 
       g_apgn_m.apgn008,g_apgn_m.apgn009,g_apgn_m.apgn010,g_apgn_m.apgn011,g_apgn_m.apgncomp_desc,g_apgn_m.apgn001_desc, 
       g_apgn_m.apgnownid_desc,g_apgn_m.apgnowndp_desc,g_apgn_m.apgncrtid_desc,g_apgn_m.apgncrtdp_desc, 
       g_apgn_m.apgnmodid_desc,g_apgn_m.apgncnfid_desc,g_apgn_m.apgn005_desc,g_apgn_m.apgn006_desc,g_apgn_m.apgn007_desc, 
       g_apgn_m.apgn008_desc,g_apgn_m.apgn009_desc,g_apgn_m.apgn010_desc,g_apgn_m.apgn011_desc
   
 
   #檢查是否允許此動作
   IF NOT aapt590_action_chk() THEN
      CLOSE aapt590_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_apgn_m.apgncomp,g_apgn_m.apgncomp_desc,g_apgn_m.apgn001,g_apgn_m.apgn001_desc,g_apgn_m.apga006, 
       g_apgn_m.apgndocno,g_apgn_m.apgndocdt,g_apgn_m.apgn013,g_apgn_m.apga002,g_apgn_m.apga004,g_apgn_m.apga004_desc, 
       g_apgn_m.apga001,g_apgn_m.apgn003,g_apgn_m.apgnstus,g_apgn_m.apgnownid,g_apgn_m.apgnownid_desc, 
       g_apgn_m.apgnowndp,g_apgn_m.apgnowndp_desc,g_apgn_m.apgncrtid,g_apgn_m.apgncrtid_desc,g_apgn_m.apgncrtdp, 
       g_apgn_m.apgncrtdp_desc,g_apgn_m.apgncrtdt,g_apgn_m.apgnmodid,g_apgn_m.apgnmodid_desc,g_apgn_m.apgnmoddt, 
       g_apgn_m.apgncnfid,g_apgn_m.apgncnfid_desc,g_apgn_m.apgncnfdt,g_apgn_m.apga010,g_apgn_m.apga007, 
       g_apgn_m.apga007_desc,g_apgn_m.apgn100,g_apgn_m.apgn103,g_apgn_m.l_apga104,g_apgn_m.l_apga108, 
       g_apgn_m.l_apga102,g_apgn_m.apgn104,g_apgn_m.l_apgn104_amt,g_apgn_m.apgn106,g_apgn_m.apgn108, 
       g_apgn_m.apgn109,g_apgn_m.apgn102,g_apgn_m.apgn012,g_apgn_m.l_glaa001,g_apgn_m.apgn101,g_apgn_m.l_dummy2, 
       g_apgn_m.l_dummy1,g_apgn_m.l_dummy3,g_apgn_m.apgn118,g_apgn_m.apgn119,g_apgn_m.apgn112,g_apgn_m.apga003, 
       g_apgn_m.apgn005,g_apgn_m.apgn005_desc,g_apgn_m.apgk006,g_apgn_m.apgn004,g_apgn_m.apgn105,g_apgn_m.apgn107, 
       g_apgn_m.apgn002,g_apgn_m.l_apgn108,g_apgn_m.apgn006,g_apgn_m.apgn006_desc,g_apgn_m.apgn007,g_apgn_m.apgn007_desc, 
       g_apgn_m.l_apgn109,g_apgn_m.apgn008,g_apgn_m.apgn008_desc,g_apgn_m.apgn009,g_apgn_m.apgn009_desc, 
       g_apgn_m.l_apgn102,g_apgn_m.apgn010,g_apgn_m.apgn010_desc,g_apgn_m.apgn011,g_apgn_m.apgn011_desc 
 
 
   CASE g_apgn_m.apgnstus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
      WHEN "X"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_apgn_m.apgnstus
            
            WHEN "N"
               HIDE OPTION "unconfirmed"
            WHEN "Y"
               HIDE OPTION "confirmed"
            WHEN "X"
               HIDE OPTION "invalid"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      CALL cl_set_act_visible("signing,withdraw",FALSE)
      CALL cl_set_act_visible("unconfirmed,invalid,confirmed",TRUE)
      CALL cl_set_act_visible("closed",FALSE)

      CASE g_apgn_m.apgnstus
         WHEN "N"  
            CALL cl_set_act_visible("unconfirmed,hold",FALSE)
            #需提交至BPM時，則顯示「提交」功能並隱藏「確認」功能
            IF cl_bpm_chk() THEN
                CALL cl_set_act_visible("signing",TRUE)
                CALL cl_set_act_visible("confirmed",FALSE)
            END IF
 
         WHEN "R"   #保留修改的功能(如作廢)，隱藏其他應用功能(如: 確認、未確認、留置、過帳…)
            CALL cl_set_act_visible("confirmed,unconfirmed",FALSE)

         WHEN "D"   #保留修改的功能(如作廢)，隱藏其他應用功能(如: 確認、未確認、留置、過帳…)
            CALL cl_set_act_visible("confirmed,unconfirmed",FALSE) 
          
         WHEN "X"
            CALL s_transaction_end('N','0')      #150401-00001#13
            RETURN

         WHEN "Y"
            CALL cl_set_act_visible("invalid,confirmed",FALSE)
         
         WHEN "W"    #只能顯示抽單;其餘應用功能皆隱藏
             CALL cl_set_act_visible("withdraw",TRUE)  
             CALL cl_set_act_visible("unconfirmed,invalid,confirmed",FALSE)
         
         WHEN "A"     #只能顯示確認; 其餘應用功能皆隱藏
             CALL cl_set_act_visible("confirmed ",TRUE)  
             CALL cl_set_act_visible("unconfirmed,invalid",FALSE)
      END CASE
      #end add-point
      
      
	  
      ON ACTION unconfirmed
         IF cl_auth_chk_act("unconfirmed") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.unconfirmed"
            
            #end add-point
         END IF
         EXIT MENU
      ON ACTION confirmed
         IF cl_auth_chk_act("confirmed") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.confirmed"
            
            #end add-point
         END IF
         EXIT MENU
      ON ACTION invalid
         IF cl_auth_chk_act("invalid") THEN
            LET lc_state = "X"
            #add-point:action控制 name="statechange.invalid"
            
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      AND lc_state <> "X"
      ) OR 
      g_apgn_m.apgnstus = lc_state OR cl_null(lc_state) THEN
      CLOSE aapt590_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   #確認
   IF lc_state = 'Y' THEN
      IF g_apgn_m.apgnstus = 'N' THEN
         #160428-00001#7 mark-----s
         #IF g_apgn_m.apgn104 + g_apgn_m.apgn108 + g_apgn_m.apgn109 > 0 THEN
         #   CALL aapt590_02(g_apgn_m.apgncomp,g_apgn_m.apgndocno) RETURNING g_sub_success,l_str_apgn
         #   IF NOT g_sub_success THEN
         #      CALL cl_err_collect_show()
         #      RETURN
         #   END IF
         #END IF
         #160428-00001#7 mark-----e
         CALL cl_err_collect_init()
         CALL s_aapt590_conf_chk(g_apgn_m.apgncomp,g_apgn_m.apgndocno) RETURNING g_sub_success
         IF NOT g_sub_success THEN
            CALL s_transaction_end('N','0')
            CALL cl_err_collect_show()
            RETURN
         ELSE
            IF NOT cl_ask_confirm('aim-00108') THEN   #是否執行確認？
               CALL s_transaction_end('N','0')
               CALL cl_err_collect_show()
               RETURN
            ELSE
               CALL s_transaction_begin()
               CALL s_aapt590_conf_upd(g_apgn_m.apgncomp,g_apgn_m.apgndocno,l_str_apgn) RETURNING g_sub_success
               IF NOT g_sub_success THEN
                  CALL s_transaction_end('N','0')
                  CALL cl_err_collect_show()
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','0')
                  CALL cl_err_collect_show()
               END IF
            END IF
         END IF
      END IF  
   END IF  
   #取消確認
   IF lc_state = 'N' THEN
      CALL cl_err_collect_init()
      CALL s_aapt590_unconf_chk(g_apgn_m.apgncomp,g_apgn_m.apgndocno) RETURNING g_sub_success
      IF NOT g_sub_success THEN
         CALL s_transaction_end('N','0')
         CALL cl_err_collect_show()
         RETURN
      ELSE
         IF NOT cl_ask_confirm('aim-00110') THEN   #是否執行取消確認？
            CALL s_transaction_end('N','0')
            CALL cl_err_collect_show()
            RETURN
         ELSE
            CALL s_transaction_begin()
            CALL s_aapt590_unconf_upd(g_apgn_m.apgncomp,g_apgn_m.apgndocno) RETURNING g_sub_success
            IF NOT g_sub_success THEN
               CALL s_transaction_end('N','0')
               CALL cl_err_collect_show()
               RETURN
            ELSE
               CALL s_transaction_end('Y','0')
               CALL cl_err_collect_show()
            END IF
         END IF
      END IF
   END IF
   #作廢
   IF lc_state = 'X' THEN
      CALL cl_err_collect_init()
      CALL s_aapt590_void_chk(g_apgn_m.apgncomp,g_apgn_m.apgndocno) RETURNING g_sub_success
      IF NOT g_sub_success THEN
         CALL s_transaction_end('N','0')           #150401-00001#13
         CALL cl_err_collect_show()
         RETURN
      ELSE
         IF NOT cl_ask_confirm('aim-00109') THEN   #是否執行作廢？
            CALL s_transaction_end('N','0')        #150401-00001#13
            CALL cl_err_collect_show()
            RETURN
         ELSE
            CALL s_transaction_end('Y','0')
            CALL cl_err_collect_show()
         END IF
      END IF
   END IF  
   #end add-point
   
   LET g_apgn_m.apgnmodid = g_user
   LET g_apgn_m.apgnmoddt = cl_get_current()
   LET g_apgn_m.apgnstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE apgn_t 
      SET (apgnstus,apgnmodid,apgnmoddt) 
        = (g_apgn_m.apgnstus,g_apgn_m.apgnmodid,g_apgn_m.apgnmoddt)     
    WHERE apgnent = g_enterprise AND apgncomp = g_apgn_m.apgncomp
      AND apgndocno = g_apgn_m.apgndocno
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE aapt590_master_referesh USING g_apgn_m.apgncomp,g_apgn_m.apgndocno INTO g_apgn_m.apgncomp, 
          g_apgn_m.apgn001,g_apgn_m.apgndocno,g_apgn_m.apgndocdt,g_apgn_m.apgn013,g_apgn_m.apgn003,g_apgn_m.apgnstus, 
          g_apgn_m.apgnownid,g_apgn_m.apgnowndp,g_apgn_m.apgncrtid,g_apgn_m.apgncrtdp,g_apgn_m.apgncrtdt, 
          g_apgn_m.apgnmodid,g_apgn_m.apgnmoddt,g_apgn_m.apgncnfid,g_apgn_m.apgncnfdt,g_apgn_m.apgn100, 
          g_apgn_m.apgn103,g_apgn_m.apgn104,g_apgn_m.apgn106,g_apgn_m.apgn108,g_apgn_m.apgn109,g_apgn_m.apgn102, 
