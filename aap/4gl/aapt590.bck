################################################################################
# Descriptions...: 檢查單別
# Memo...........:
# Date & Author..: 16/04/08 By 02097
# Modify.........:
################################################################################
PRIVATE FUNCTION aapt590_chk_slip(p_slip)
DEFINE p_slip        LIKE apgn_t.apgndocno
DEFINE r_success     LIKE type_t.num10
DEFINE r_errno       LIKE gzze_t.gzze001
DEFINE l_ap_slip     LIKE apgn_t.apgndocno
DEFINE l_chr         LIKE type_t.chr1
DEFINE l_com_0001    LIKE type_t.num5
DEFINE l_slip        STRING
DEFINE l_len         LIKE type_t.num5

   WHENEVER ERROR CONTINUE  
   LET r_success = TRUE
   
   CALL s_aooi200_fin_get_slip(p_slip) RETURNING g_sub_success,l_ap_slip
   CALL s_fin_get_doc_para(g_glaald,g_apgn_m.apgncomp,l_ap_slip,'D-FIN-0030') RETURNING l_chr
   IF l_chr = 'N' THEN
      LET r_success = FALSE
      LET r_errno = 'aap-00334'
      RETURN r_success,r_errno
   END IF
   CALL cl_get_para(g_enterprise,'','E-COM-0001') RETURNING l_com_0001
   LET l_slip = l_ap_slip
   LET l_len  = l_slip.getLength()
   IF l_len <> l_com_0001 THEN
      LET r_success = FALSE
      LET r_errno = 'aoo-00292'
      RETURN r_success,r_errno
   END IF
   RETURN r_success,r_errno
END FUNCTION

################################################################################
# Descriptions...: 法人INFO
# Memo...........:
# Modify.........:
################################################################################
PRIVATE FUNCTION aapt590_get_comp_info()
DEFINE l_isai002     LIKE isai_t.isai002
DEFINE l_glaa005     LIKE glaa_t.glaa005
   IF cl_null(g_apgn_m.apgncomp) THEN RETURN END IF
   CALL s_fin_get_major_ld(g_apgn_m.apgncomp) RETURNING g_glaald
   CALL s_ld_sel_glaa(g_glaald,'glaa001|glaa004|glaa005|glaa024')
        RETURNING g_sub_success,g_glaa001,g_glaa004,g_glaa005,g_glaa024
   #取得該法人的稅區
   CALL s_tax_get_ooef019(g_apgn_m.apgncomp) RETURNING g_sub_success,g_ooef019
   
   CALL cl_set_comp_visible('apgc008',FALSE)
   SELECT isai002 INTO l_isai002
     FROM isai_t
    WHERE isaient = g_enterprise AND isai001 = g_ooef019            
   IF l_isai002 = "2" THEN
      CALL cl_set_comp_visible('apgc008',TRUE)
   END IF
   #160428-00001#7-----s

   #自備款
   SELECT glab006,glab007 INTO g_apgn_m.apgn006,g_apgn_m.apgn007
     FROM glab_t
    WHERE glab002 = '3117' AND glab001 = '15' AND glab003 = '8504_37' AND glabld = g_glaald
      AND glabent = g_enterprise
   #保證金
   SELECT glab006,glab007 INTO g_apgn_m.apgn008,g_apgn_m.apgn009
     FROM glab_t
    WHERE glab002 = '3117' AND glab001 = '15' AND glab003 = '8504_34' AND glabld = g_glaald
      AND glabent = g_enterprise
    #預付款
   SELECT glab006,glab007 INTO g_apgn_m.apgn010,g_apgn_m.apgn011
     FROM glab_t
    WHERE glab002 = '3117' AND glab001 = '15' AND glab003 = '8504_38' AND glabld = g_glaald
      AND glabent = g_enterprise
    
   
   CALL s_desc_get_nmajl003_desc(g_apgn_m.apgn006) RETURNING g_apgn_m.apgn006_desc
   CALL s_desc_get_nmail004_desc(g_glaa005,g_apgn_m.apgn007) RETURNING g_apgn_m.apgn007_desc
   CALL s_desc_get_nmajl003_desc(g_apgn_m.apgn008) RETURNING g_apgn_m.apgn008_desc
   CALL s_desc_get_nmail004_desc(g_glaa005,g_apgn_m.apgn009) RETURNING g_apgn_m.apgn009_desc
   CALL s_desc_get_nmajl003_desc(g_apgn_m.apgn010) RETURNING g_apgn_m.apgn010_desc
   CALL s_desc_get_nmail004_desc(g_glaa005,g_apgn_m.apgn011) RETURNING g_apgn_m.apgn011_desc
    
   DISPLAY BY NAME g_apgn_m.apgn006_desc,g_apgn_m.apgn007_desc,g_apgn_m.apgn008_desc,
                   g_apgn_m.apgn009_desc,g_apgn_m.apgn010_desc,g_apgn_m.apgn011_desc
   #160428-00001#7-----e
END FUNCTION

################################################################################
# Descriptions...: 申請單INFO
# Memo...........:
# Modify.........:
################################################################################
PRIVATE FUNCTION aapt590_get_apgndocno_info(p_cmd)
DEFINE p_cmd         LIKE type_t.chr1
DEFINE l_apga026     LIKE apga_t.apga026
DEFINE l_apga100     LIKE apga_t.apga100
DEFINE l_apga103     LIKE apga_t.apga103  #開狀金額
DEFINE l_apga104     LIKE apga_t.apga104  #自備款
DEFINE l_apga108     LIKE apga_t.apga108  #保證金
DEFINE l_apgk010     LIKE apgk_t.apgk010
DEFINE l_apgk103     LIKE apgk_t.apgk103  #到單金額
DEFINE l_apgk104     LIKE apgk_t.apgk104  #自備款
DEFINE l_apgl103     LIKE apgl_t.apgl103  #到貨金額
DEFINE ls_js         STRING
DEFINE lc_param      RECORD
         apca004     LIKE apca_t.apca004,
         ooib001     LIKE ooib_t.ooib001
                 END RECORD
DEFINE l_no_use_var  LIKE type_t.chr1
DEFINE l_cnt         LIKE type_t.num5
#160428-00001#7-----s
DEFINE l_apga113     LIKE apga_t.apga113
DEFINE l_apga114     LIKE apga_t.apga114
DEFINE l_apgk113     LIKE apgk_t.apgk113
DEFINE l_apgk114     LIKE apgk_t.apgk114
DEFINE l_apga118     LIKE apga_t.apga118
#160428-00001#7-----e

   #IF cl_null(g_apgn_m.apgndocno) THEN RETURN END IF   #160428-00001#7 mark
   IF cl_null(g_apgn_m.apgn013)THEN RETURN END IF       #160428-00001#7
   
   #(aapt510)                        
   SELECT apga001,apga002,apga003,apga004,
          apga006,apga007,apga010,apga026,
          apga100,apga103,apga104,apga108,
          apga113,apga114,apga118       #160428-00001#7 add
     INTO g_apgn_m.apga001,g_apgn_m.apga002,g_apgn_m.apga003,g_apgn_m.apga004,
          g_apgn_m.apga006,g_apgn_m.apga007,g_apgn_m.apga010,l_apga026,
          l_apga100       ,l_apga103       ,l_apga104       ,l_apga108,
          l_apga113       ,l_apga114       ,l_apga118    #160428-00001#7 add
     FROM apga_t
    WHERE apgaent  = g_enterprise
      AND apgacomp = g_apgn_m.apgncomp 
      #AND apgadocno = g_apgn_m.apgndocno   #160428-00001#7 mark
      AND apgadocno = g_apgn_m.apgn013      #160428-00001#7
      
   #160824-00049#6-----s
   IF cl_null(l_apga103)THEN LET l_apga103 = 0 END IF
   IF cl_null(l_apga104)THEN LET l_apga104 = 0 END IF
   IF cl_null(l_apga108)THEN LET l_apga108 = 0 END IF
   IF cl_null(l_apga113)THEN LET l_apga113 = 0 END IF
   IF cl_null(l_apga114)THEN LET l_apga114 = 0 END IF
   IF cl_null(l_apga118)THEN LET l_apga118 = 0 END IF
   #160824-00049#6-----e
      
   CALL s_desc_get_nmabl003(g_apgn_m.apga007) RETURNING g_apgn_m.apga007_desc
   #(aapt540)
   SELECT apgk103,apgk104,apgk006,apgk010
     INTO l_apgk103,l_apgk104,g_apgn_m.apgk006,l_apgk010,
          l_apgk113,l_apgk114           #160428-00001#7 add
     FROM apgk_t
    WHERE apgkent = g_enterprise 
      AND apgkcomp= g_apgn_m.apgncomp 
      #AND apgk005 = g_apgn_m.apgndocno   #160428-00001#7 mark
      AND apgk005 = g_apgn_m.apgn013      #160428-00001#7 add
      AND apgkstus= 'Y'

   #160824-00049#6-----s
   IF cl_null(l_apgk103)THEN LET l_apgk103 = 0 END IF
   IF cl_null(l_apgk104)THEN LET l_apgk104 = 0 END IF
   IF cl_null(l_apgk113)THEN LET l_apgk113 = 0 END IF
   IF cl_null(l_apgk114)THEN LET l_apgk114 = 0 END IF
   #160824-00049#6-----e

   #(aapt550)
   SELECT apgl103
     INTO l_apgl103
     FROM apgl_t
    WHERE apglent = g_enterprise 
      AND apglcomp= g_apgn_m.apgncomp 
      #AND apgl005 = g_apgn_m.apgndocno   #160428-00001#7 mark
      AND apgl005 = g_apgn_m.apgn013      #160428-00001#7 add
      AND apglstus= 'Y'
   LET g_apgn_m.l_apgn104_amt = l_apga103 - l_apgk103
   IF p_cmd = 'i' THEN
      LET g_apgn_m.apgn100 = l_apga100
      IF g_glaa001 = g_apgn_m.apgn100 THEN
         LET g_apgn_m.apgn101 = 1
      ELSE
         LET lc_param.apca004 = g_apgn_m.apga004
         LET ls_js = util.JSON.stringify(lc_param)
         CALL s_fin_get_curr_rate(g_apgn_m.apgncomp,g_glaald,g_apgn_m.apgndocdt,g_apgn_m.apgn100,ls_js)
              RETURNING g_apgn_m.apgn101,l_no_use_var,l_no_use_var      
      END IF
      LET g_apgn_m.apgn103 = l_apga103
      LET g_apgn_m.apgn104 = l_apgk103
      LET g_apgn_m.apgn105 = l_apgl103
      #融資到帳金額:條件為申請單號存在於 aapt540 and 融資到帳單號 is not null  
      IF NOT cl_null(l_apgk010) THEN
         LET g_apgn_m.apgn106 = l_apgk010
      END IF
      IF cl_null(g_apgn_m.apgn106) THEN
         LET g_apgn_m.apgn106 = 0
      END IF
      #if 自備款於開狀時支付= Y then 取 aapt510 金額 else 取 aapt540
      IF l_apga026 = 'Y' THEN
         LET g_apgn_m.apgn107 = l_apga104
      ELSE
         LET g_apgn_m.apgn107 = l_apgk104
      END IF
      LET g_apgn_m.apgn109 = l_apga108
      LET g_apgn_m.apgn119 = l_apga118    #160428-00001#7 add
      CALL aapt590_set_entry('')
      CALL aapt590_set_no_entry('')
   END IF
   
   CALL s_desc_get_trading_partner_abbr_desc(g_apgn_m.apga004) RETURNING g_apgn_m.apga004_desc
   DISPLAY BY NAME g_apgn_m.apga001,g_apgn_m.apga002,g_apgn_m.apga003,g_apgn_m.apga004,g_apgn_m.apga004_desc,
                   g_apgn_m.apga006,g_apgn_m.apga007,g_apgn_m.apga010,
                   g_apgn_m.apgn100,g_apgn_m.apgn103,g_apgn_m.apgn104,g_apgn_m.apgn105,
                   g_apgn_m.apgn106,g_apgn_m.apgn107,g_apgn_m.apgn109
   DISPLAY BY NAME g_apgn_m.apgk006,g_apgn_m.l_apgn104_amt
   DISPLAY BY NAME g_apgn_m.apgn119    #160428-00001#7 add
END FUNCTION

################################################################################
# Descriptions...: 取得單身(apgc)匯率
# Memo...........:
# Date & Author..: 16/04/11 By 02097
# Modify.........:
################################################################################
PRIVATE FUNCTION aapt590_get_apgc_rate()
DEFINE l_sfin4012       LIKE type_t.chr2
DEFINE l_no_use_var     LIKE type_t.chr1
DEFINE ls_js            STRING
DEFINE lc_param         RECORD
         apca004        LIKE apca_t.apca004
                    END RECORD
   
   IF g_apgn_m.apgn100 = g_apgc_d[l_ac].apgc100 THEN  #若與主檔同幣別則帶主檔之匯率,else另取
      LET g_apgc_d[l_ac].apgc101 = g_apgn_m.apgn101
   ELSE      
      #有輸入交易帳戶時,取銀存支出匯率來源
      IF NOT cl_null(g_apgc_d[l_ac].apgc014) THEN
         CALL cl_get_para(g_enterprise,g_apgn_m.apgncomp,'S-FIN-4012') RETURNING l_sfin4012
      ELSE
         LET l_sfin4012 = ''
      END IF
            
      IF l_sfin4012  = '23' THEN
         #銀行日平均匯率
         CALL s_anm_get_exrate(g_glaald,g_apgn_m.apgncomp,g_apgc_d[l_ac].apgc014,g_apgc_d[l_ac].apgc100,g_glaa001,g_apgn_m.apga003) RETURNING g_apgc_d[l_ac].apgc101
         #160824-00049#6-----s
         IF cl_null(g_apgc_d[l_ac].apgc101)THEN
            LET g_apgc_d[l_ac].apgc101 = 1
            INITIALIZE g_errparam.* TO NULL
            LET g_errparam.extend = ''
            LET g_errparam.code = 'aap-00587'
            LET g_errparam.popup = TRUE
            CALL cl_err()              
         END IF
         #160824-00049#6-----e
      ELSE
         LET lc_param.apca004 = g_apgn_m.apga004
         LET ls_js = util.JSON.stringify(lc_param)
         CALL s_fin_get_curr_rate(g_apgn_m.apgncomp,g_glaald,g_apgn_m.apga003,g_apgc_d[l_ac].apgc100,ls_js)
              RETURNING g_apgc_d[l_ac].apgc101,l_no_use_var,l_no_use_var
      END IF   
      
   END IF
END FUNCTION

################################################################################
# Descriptions...: 計算金額
# Memo...........:
# Date & Author..: 16/04/11 By 02097
# Modify.........:
################################################################################
PRIVATE FUNCTION aapt590_calc_apgc_money(p_oodb005)
DEFINE p_oodb005     LIKE oodb_t.oodb005
DEFINE l_apgc103     LIKE apgc_t.apgc103     #計稅基礎
DEFINE l_case        LIKE type_t.chr1

   IF cl_null(g_apgc_d[l_ac].apgc006) OR cl_null(g_apgc_d[l_ac].apgc100) OR cl_null(g_apgc_d[l_ac].apgc101) THEN
      LET g_apgc_d[l_ac].apgc103 = 0
      LET g_apgc_d[l_ac].apgc104 = 0
      LET g_apgc_d[l_ac].apgc105 = 0
      LET g_apgc_d[l_ac].apgc113 = 0
      LET g_apgc_d[l_ac].apgc114 = 0
      LET g_apgc_d[l_ac].apgc115 = 0
      RETURN
   END IF
   IF NOT cl_null(g_apgc_d[l_ac].apgc006) AND cl_null(p_oodb005) THEN
      #重新取p_oodb005
   END IF
   IF cl_null(p_oodb005) THEN
      LET g_apgc_d[l_ac].apgc103 = 0
      LET g_apgc_d[l_ac].apgc104 = 0
      LET g_apgc_d[l_ac].apgc105 = 0
      LET g_apgc_d[l_ac].apgc113 = 0
      LET g_apgc_d[l_ac].apgc114 = 0
      LET g_apgc_d[l_ac].apgc115 = 0
      RETURN
   END IF
   LET l_case = 'N'
   CASE
      WHEN INFIELD(apgc006)
         LET l_case = 'Y'
            
      WHEN INFIELD(apgc103)
         IF p_oodb005 = 'N' THEN   #未稅的計價基礎 應重推金額
            LET l_case = 'Y'
         ELSE
            LET g_apgc_d[l_ac].apgc104 = g_apgc_d[l_ac].apgc105 - g_apgc_d[l_ac].apgc103
         END IF
         
      WHEN INFIELD(apgc104)
         IF p_oodb005 = 'N' THEN
            LET g_apgc_d[l_ac].apgc105 = g_apgc_d[l_ac].apgc103 + g_apgc_d[l_ac].apgc104
         ELSE
            LET g_apgc_d[l_ac].apgc103 = g_apgc_d[l_ac].apgc105 - g_apgc_d[l_ac].apgc104            
         END IF
         
      WHEN INFIELD(apgc105)
         IF p_oodb005 = 'N' THEN
            LET g_apgc_d[l_ac].apgc104 = g_apgc_d[l_ac].apgc105 - g_apgc_d[l_ac].apgc103
         ELSE
            LET l_case = 'Y'
         END IF
      
      WHEN INFIELD(apgc113)
         IF p_oodb005 = 'N' THEN
            LET g_apgc_d[l_ac].apgc115 = g_apgc_d[l_ac].apgc113 + g_apgc_d[l_ac].apgc114 
         ELSE
            LET g_apgc_d[l_ac].apgc114 = g_apgc_d[l_ac].apgc115 - g_apgc_d[l_ac].apgc113            
         END IF
         
      WHEN INFIELD(apgc114)
         IF p_oodb005 = 'N' THEN
            LET g_apgc_d[l_ac].apgc115 = g_apgc_d[l_ac].apgc113 + g_apgc_d[l_ac].apgc114
         ELSE
            LET g_apgc_d[l_ac].apgc113 = g_apgc_d[l_ac].apgc115 - g_apgc_d[l_ac].apgc114            
         END IF
         
      WHEN INFIELD(apgc115)
         IF p_oodb005 = 'N' THEN
            LET g_apgc_d[l_ac].apgc113 = g_apgc_d[l_ac].apgc115 - g_apgc_d[l_ac].apgc114        
         ELSE
            LET g_apgc_d[l_ac].apgc114 = g_apgc_d[l_ac].apgc115 - g_apgc_d[l_ac].apgc113 
         END IF
   END CASE         
   
   IF l_case = 'Y' THEN
      LET l_apgc103 = 0
      IF p_oodb005 = 'N' THEN                   #未稅
         LET l_apgc103 = g_apgc_d[l_ac].apgc103
      ELSE
         LET l_apgc103 = g_apgc_d[l_ac].apgc105
      END IF
      IF cl_null(l_apgc103) THEN LET l_apgc103 = 0 END IF
                      #營運據點/p_oodb002:稅別/p_money:計稅基礎/p_num:數量/p_curr:幣別/p_rate:匯率
      CALL s_tax_count(g_apgn_m.apgncomp,g_apgc_d[l_ac].apgc006,l_apgc103,1,g_apgc_d[l_ac].apgc100,g_apgc_d[l_ac].apgc101)
           RETURNING g_apgc_d[l_ac].apgc103,g_apgc_d[l_ac].apgc104,g_apgc_d[l_ac].apgc105
                    ,g_apgc_d[l_ac].apgc113,g_apgc_d[l_ac].apgc114,g_apgc_d[l_ac].apgc115
   END IF
   
END FUNCTION

