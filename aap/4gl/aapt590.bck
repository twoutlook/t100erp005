         PREPARE aapt590_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR aapt590_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
   #  OPEN b_fill_cs USING g_enterprise,g_apgn_m.apgncomp,g_apgn_m.apgndocno   #(ver:78)
                                               
      FOREACH b_fill_cs USING g_enterprise,g_apgn_m.apgncomp,g_apgn_m.apgndocno INTO g_apgc_d[l_ac].apgc900, 
          g_apgc_d[l_ac].apgcseq,g_apgc_d[l_ac].apgcorga,g_apgc_d[l_ac].apgc001,g_apgc_d[l_ac].apgc014, 
          g_apgc_d[l_ac].apgc100,g_apgc_d[l_ac].apgc101,g_apgc_d[l_ac].apgc006,g_apgc_d[l_ac].apgc007, 
          g_apgc_d[l_ac].apgc008,g_apgc_d[l_ac].apgc009,g_apgc_d[l_ac].apgc010,g_apgc_d[l_ac].apgc011, 
