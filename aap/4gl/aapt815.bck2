   END IF
   
   #160225-00001#1--(S)
   IF NOT cl_null(g_apea_m.apeadocno) THEN
      SELECT glaald INTO l_ld FROM glaa_t
       WHERE glaaent = g_enterprise
         AND glaacomp = g_apea_m.apeacomp
