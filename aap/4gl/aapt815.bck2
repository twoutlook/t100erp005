              = 
             (ps_keys[1],ps_keys[2]
              ,g_apeb_d[g_detail_idx].apeborga,g_apeb_d[g_detail_idx].apeb023,g_apeb_d[g_detail_idx].apeb003, 
                  g_apeb_d[g_detail_idx].apeb004,g_apeb_d[g_detail_idx].apeb100,g_apeb_d[g_detail_idx].apeb108, 
                  g_apeb_d[g_detail_idx].apeb101,g_apeb_d[g_detail_idx].apeb118,g_apeb_d[g_detail_idx].apeb006, 
                  g_apeb_d[g_detail_idx].apeb024,g_apeb_d[g_detail_idx].apeb025,g_apeb_d[g_detail_idx].apeb015, 
                  g_apeb_d[g_detail_idx].apeb031) 
         WHERE apebent = g_enterprise AND apebdocno = ps_keys_bak[1] AND apebseq = ps_keys_bak[2]
      #add-point:update_b段修改中 name="update_b.m_update"
      
      #end add-point   
