      LET l_flag = 'N'
      IF l_tran_flag = 'Y' THEN 
         CALL s_transaction_begin()
      END IF
   #end add-point
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
{</section>}
 
{<section id="aapt815.input.head" >}
      #單頭段
      INPUT BY NAME g_apea_m.apeasite,g_apea_m.apea003,g_apea_m.apeadocdt,g_apea_m.apeacomp,g_apea_m.apeald, 
          g_apea_m.apeadocno,g_apea_m.apea001,g_apea_m.apea019,g_apea_m.apea020,g_apea_m.apea025,g_apea_m.apea026, 
          g_apea_m.apea013,g_apea_m.apea027,g_apea_m.apeastus 
         ATTRIBUTE(WITHOUT DEFAULTS)
         
         #自訂ACTION(master_input)
         
     
         BEFORE INPUT
            IF s_transaction_chk("N",0) THEN
               CALL s_transaction_begin()
            END IF
            OPEN aapt815_cl USING g_enterprise,g_apea_m.apeadocno
            IF STATUS THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "OPEN aapt815_cl:" 
               LET g_errparam.code   = STATUS 
               LET g_errparam.popup  = TRUE 
               CLOSE aapt815_cl
               CALL s_transaction_end('N','0')
               CALL cl_err()
               RETURN
            END IF
            
            IF l_cmd_t = 'r' THEN
               
            END IF
            #因應離開單頭後已寫入資料庫, 若重新回到單頭則視為修改
            #因此需於此處開啟/關閉欄位
            CALL aapt815_set_entry(p_cmd)
            #add-point:資料輸入前 name="input.m.before_input"
 
            #end add-point
            CALL aapt815_set_no_entry(p_cmd)
    
                  #應用 a02 樣板自動產生(Version:2)
         AFTER FIELD apeasite
            
            #add-point:AFTER FIELD apeasite name="input.a.apeasite"
            LET g_apea_m.apeasite_desc = '' #160822-00008#2
            IF NOT cl_null(g_apea_m.apeasite) THEN
               #IF p_cmd = 'a' OR (p_cmd = 'u' AND (g_apea_m.apeasite != g_apea_m_t.apeasite OR g_apea_m_t.apeasite IS NULL )) THEN  #160822-00008#2 Mark
               IF g_apea_m.apeasite != g_apea_m_o.apeasite OR cl_null(g_apea_m_o.apeasite) THEN  #160822-00008#2
                  #160509-00004#90 add ------
                  #以目前的資料重展結構,避免[帳套]有值時會比對錯誤,在s_fin_account_center_with_ld_chk做勾稽時會依據這個結構做是否有符合的帳套
                  CALL s_fin_account_center_sons_query('3',g_apea_m.apeasite,g_apea_m.apeadocdt,'1')
                  #如果帳務中心不同 且帳套有值 先依據現在的帳務中心跟帳套勾稽一次---(S)---
                  #避免USER 在帳務中心跟帳套卡住走不了 增加對帳套有資料的處理
                  IF g_apea_m_t.apeasite != g_apea_m.apeasite AND NOT cl_null(g_apea_m.apeald) THEN   
                     CALL s_fin_account_center_with_ld_chk(g_apea_m.apeasite,g_apea_m.apeald,g_user,'3','N','',g_apea_m.apeadocdt) RETURNING g_sub_success,g_errno
                     IF NOT g_sub_success THEN
                        #勾稽失敗:目前的帳套不在這個帳務中心下 因此預設值給帳務中心的主帳套
                        CALL s_fin_orga_get_comp_ld(g_apea_m.apeasite) RETURNING g_sub_success,g_errno,g_apea_m.apeacomp,g_apea_m.apeald
                        #判斷這個主帳套使用者是否有權限
                        CALL s_fin_account_center_with_ld_chk(g_apea_m.apeasite,g_apea_m.apeald,g_user,'3','N','',g_apea_m.apeadocdt) RETURNING g_sub_success,g_errno
                     END IF
                     #判斷完成後 若勾稽失敗則回復舊值
                     IF NOT g_sub_success THEN
                        INITIALIZE g_errparam TO NULL
                        LET g_errparam.code = g_errno
                        LET g_errparam.extend = ''
                        LET g_errparam.popup = TRUE
                        CALL cl_err()
                        #LET g_apea_m.apeasite = g_apea_m_t.apeasite #160822-00008#2 Mark
                        LET g_apea_m.apeasite = g_apea_m_o.apeasite #160822-00008#2
                        CALL s_desc_get_department_desc(g_apea_m.apeasite) RETURNING g_apea_m.apeasite_desc
                        DISPLAY BY NAME g_apea_m.apeasite_desc
                        NEXT FIELD CURRENT
                     END IF
                  END IF
                  #如果帳務中心不同 且帳套有值 先依據現在的帳務中心跟帳套勾稽一次---(E)---
                  #160509-00004#90 add end---
                  CALL s_fin_account_center_with_ld_chk(g_apea_m.apeasite,g_apea_m.apeald,g_user,'3','N','',g_apea_m.apeadocdt) RETURNING g_sub_success,g_errno #160509-00004#90 fix
                  IF NOT g_sub_success THEN
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code = g_errno
                     LET g_errparam.extend = ''
                     LET g_errparam.popup = TRUE
                     CALL cl_err()
                     LET g_apea_m.apeasite = g_site
                     LET g_apea_m.apeasite_desc = s_desc_get_department_desc(g_apea_m.apeasite)
                     DISPLAY BY NAME g_apea_m.apeasite,g_apea_m.apeasite_desc
                     NEXT FIELD apeasite
                  END IF
                  LET g_apea_m.apeasite_desc = s_desc_get_department_desc(g_apea_m.apeasite)
                  DISPLAY BY NAME g_apea_m.apeasite_desc
                  #營運據點取得帳別與法人
                  CALL s_fin_orga_get_comp_ld(g_apea_m.apeasite) RETURNING g_sub_success,g_errno,g_apea_m.apeacomp,g_apea_m.apeald
                  #匯率/單據
                  CALL s_ld_sel_glaa(g_apea_m.apeald,'glaa001|glaa024') RETURNING g_sub_success,g_glaa001,g_glaa024
                  #160509-00004#90 add ------
                  #依據正確的資料再重展一次結構
                  CALL s_fin_account_center_sons_query('3',g_apea_m.apeasite,g_apea_m.apeadocdt,'1')
                  CALL s_fin_account_center_sons_str() RETURNING g_wc_apeasite
                  CALL s_fin_get_wc_str(g_wc_apeasite) RETURNING g_wc_apeasite
                  #160509-00004#90 add end---
