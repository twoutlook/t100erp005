   LET g_apca_m.sum_apcc139  = g_apca_m.sum_apcc139  - g_apca_m.apca137
   #150401-00001--(E)
   #150325 add ------
   DISPLAY BY NAME g_apca_m.sum_apcc119,g_apca_m.sum_apcc119_1,g_apca_m.sum_apcc129,g_apca_m.sum_apcc139,
