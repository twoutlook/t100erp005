      LET g_errparam.popup  = TRUE 
      CLOSE aapt831_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aapt831_master_referesh USING g_apca_m.apcald,g_apca_m.apcadocno INTO g_apca_m.apcasite,g_apca_m.apcald, 
       g_apca_m.apcacomp,g_apca_m.apcadocno,g_apca_m.apcadocdt,g_apca_m.apca014,g_apca_m.apca015,g_apca_m.apca057, 
