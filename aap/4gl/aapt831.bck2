################################################################################
# Descriptions...: 針對單一筆單身 update 不存在單身的欄位
# Memo...........:
# Usage..........: CALL aapt831_apcb_upd(p_ac)
#                  RETURN r_errno
# Input parameter: p_ac
# Return code....: r_errno
# Date & Author..: 14/03/24 By albireo
# Modify.........:
################################################################################
PUBLIC FUNCTION aapt831_apcb_upd(p_ac)
   DEFINE p_ac    LIKE type_t.num10
   DEFINE r_errno LIKE gzze_t.gzze001
   
   #單身其他幣別欄位資料
   DEFINE l_apcb  RECORD 
                  apcb103   LIKE apcb_t.apcb103,
                  apcb104   LIKE apcb_t.apcb104,
                  apcb106   LIKE apcb_t.apcb106,
                  apcb108   LIKE apcb_t.apcb108,
                  apcb113   LIKE apcb_t.apcb113,
                  apcb114   LIKE apcb_t.apcb114,
                  apcb116   LIKE apcb_t.apcb116,
                  apcb118   LIKE apcb_t.apcb118,
                  apcb123   LIKE apcb_t.apcb124,
                  apcb126   LIKE apcb_t.apcb126,
                  apcb128   LIKE apcb_t.apcb128,
                  apcb133   LIKE apcb_t.apcb133,
                  apcb134   LIKE apcb_t.apcb134,
                  apcb136   LIKE apcb_t.apcb136,
                  apcb138   LIKE apcb_t.apcb138,
                  apcb121   LIKE apcb_t.apcb121,
                  apcb131   LIKE apcb_t.apcb131,
                  apcb125   LIKE apcb_t.apcb125,
                  apcb135   LIKE apcb_t.apcb135,
                  apcb124   LIKE apcb_t.apcb124,
                  apcb107   LIKE apcb_t.apcb107,
                  apcb117   LIKE apcb_t.apcb117,
                  apcb127   LIKE apcb_t.apcb127,
                  apcb137   LIKE apcb_t.apcb137,
                  apcb029   LIKE apcb_t.apcb029,
                  apcb022   LIKE apcb_t.apcb022
                  END RECORD
                  
   DEFINE l_glaa  RECORD
                  glaa015   LIKE glaa_t.glaa015,
                  glaa017   LIKE glaa_t.glaa017,
                  glaa019   LIKE glaa_t.glaa019,
                  glaa021   LIKE glaa_t.glaa021,
                  glaa018   LIKE glaa_t.glaa018,
                  glaa026   LIKE glaa_t.glaa026
                  END RECORD
   DEFINE l_curr  LIKE glaa_t.glaa001
   DEFINE l_ooaj  RECORD
                  ooaj0042   LIKE ooaj_t.ooaj004,
                  ooaj0043   LIKE ooaj_t.ooaj004
                  END RECORD
                  
   DEFINE l_ooef014          LIKE ooef_t.ooef014   #匯率參照表
 
   LET r_errno = NULL 
   IF cl_null(p_ac) OR p_ac <= 0 THEN RETURN r_errno END IF
   
   INITIALIZE l_apcb.* TO NULL
   #交易原幣
   LET l_apcb.apcb103 = g_apcb_d[p_ac].apcb105
   LET l_apcb.apcb104 = 0 
   LET l_apcb.apcb106 = 0
   LET l_apcb.apcb107 = 0
   LET l_apcb.apcb108 = g_apcb_d[p_ac].apcb105
   
   #本幣
   LET l_apcb.apcb113 = g_apcb_d[p_ac].apcb115
   LET l_apcb.apcb114 = 0 
   LET l_apcb.apcb116 = 0 
   LET l_apcb.apcb117 = 0
   LET l_apcb.apcb118 = g_apcb_d[p_ac].apcb115
   
   #其他幣別資料
   INITIALIZE l_glaa.* TO NULL
   SELECT glaa015,glaa017,glaa019,glaa021,glaa018,glaa026
     INTO l_glaa.*
     FROM glaa_t
    WHERE glaaent = g_enterprise
      AND glaald  = g_apca_m.apcald
      
   #匯率設定
   #匯率變更使用s_curr_round取位 141006-s
   #LET l_ooef014 = NULL
   #SELECT ooef014 INTO l_ooef014 FROM ooef_t
   # WHERE ooefent = g_enterprise
   #   AND ooef001 = g_apca_m.apcacomp
   #   
   #INITIALIZE l_ooaj.* TO NULL
   #CALL s_curr_sel_ooaj004(l_ooef014,g_apca_m.apca120) RETURNING l_ooaj.ooaj0042   
   #CALL s_curr_sel_ooaj004(l_ooef014,g_apca_m.apca130) RETURNING l_ooaj.ooaj0043
   #匯率變更使用s_curr_round取位 141006-e
   #其他本位幣二
   IF l_glaa.glaa015 = 'Y' THEN
      LET l_apcb.apcb121 = 1
      IF l_glaa.glaa017 = '1' THEN
         #換算基準:交易幣
         LET l_curr = g_apcb_d[p_ac].apcb100
         LET l_apcb.apcb125 = g_apcb_d[p_ac].apcb105
      ELSE
         #換算基準:帳別本幣
         LET l_curr = g_apca_m.glaa001
         LET l_apcb.apcb125 = g_apcb_d[p_ac].apcb115
      END IF
      CALL s_aooi160_get_exrate('2',g_apca_m.apcald,g_apca_m.apcadocdt,l_curr,g_apca_m.apca120,0,l_glaa.glaa018)
           RETURNING l_apcb.apcb121
      LET l_apcb.apcb125 = l_apcb.apcb125 * l_apcb.apcb121      
      #CALL s_num_round('1',l_apcb.apcb125,l_ooaj.ooaj0042) RETURNING l_apcb.apcb125
      IF cl_null(l_apcb.apcb125)THEN LET l_apcb.apcb125 = 0 END IF
      CALL s_curr_round_ld('1',g_apca_m.apcald,g_apca_m.apca120,l_apcb.apcb125,2) RETURNING g_sub_success,g_errno,l_apcb.apcb125
   END IF
   IF cl_null(l_apcb.apcb125) THEN LET l_apcb.apcb125 = 0 END IF
   #其他本位幣三
   IF l_glaa.glaa019 = 'Y' THEN
      LET l_apcb.apcb131 = 1
      IF l_glaa.glaa021 = '1' THEN
         #換算基準:交易幣
         LET l_curr = g_apcb_d[p_ac].apcb100
         LET l_apcb.apcb135 = g_apcb_d[p_ac].apcb105
      ELSE
         #換算基準:帳別本幣
         LET l_curr = g_apca_m.glaa001
         LET l_apcb.apcb135 = g_apcb_d[p_ac].apcb115
      END IF
      CALL s_aooi160_get_exrate('2',g_apca_m.apcald,g_apca_m.apcadocdt,l_curr,g_apca_m.apca120,0,l_glaa.glaa018)
           RETURNING l_apcb.apcb131
      LET l_apcb.apcb135 = l_apcb.apcb135 * l_apcb.apcb131
      #CALL s_num_round('1',l_apcb.apcb135,l_ooaj.ooaj0043) RETURNING l_apcb.apcb135      
      IF cl_null(l_apcb.apcb135)THEN LET l_apcb.apcb135 = 0 END IF      
      CALL s_curr_round_ld('1',g_apca_m.apcald,g_apca_m.apca130,l_apcb.apcb135,2) RETURNING g_sub_success,g_errno,l_apcb.apcb135
   END IF
   IF cl_null(l_apcb.apcb135) THEN LET l_apcb.apcb135 = 0 END IF
   
   #第二幣
   LET l_apcb.apcb123 = l_apcb.apcb125
   LET l_apcb.apcb124 = 0 
   LET l_apcb.apcb126 = 0 
   LET l_apcb.apcb127 = 0
   LET l_apcb.apcb128 = l_apcb.apcb125
   
   #第三幣
   LET l_apcb.apcb133 = l_apcb.apcb135
   LET l_apcb.apcb134 = 0
   LET l_apcb.apcb136 = 0
   LET l_apcb.apcb137 = 0
   LET l_apcb.apcb138 = l_apcb.apcb135
   
   #隱藏的會科
   IF NOT cl_null(g_apca_m.apca035)THEN
      LET l_apcb.apcb029 = g_apca_m.apca035
   END IF 
   
   LET l_apcb.apcb022 = 1
   
   UPDATE apcb_t 
             SET (apcb103,apcb104,apcb106,apcb108,apcb113,apcb114,
                  apcb116,apcb118,apcb123,apcb124,apcb126,apcb128,apcb133,
                  apcb134,apcb136,apcb138,apcb121,apcb131,apcb125,
                  apcb135,apcb107,apcb117,apcb127,apcb137,apcb022)
             = (l_apcb.apcb103,l_apcb.apcb104,l_apcb.apcb106,l_apcb.apcb108,
                l_apcb.apcb113,l_apcb.apcb114,l_apcb.apcb116,l_apcb.apcb118,
                l_apcb.apcb123,l_apcb.apcb124,l_apcb.apcb126,l_apcb.apcb128,
                l_apcb.apcb133,l_apcb.apcb134,l_apcb.apcb136,l_apcb.apcb138,
                l_apcb.apcb121,l_apcb.apcb131,l_apcb.apcb125,l_apcb.apcb135,
                l_apcb.apcb107,l_apcb.apcb117,l_apcb.apcb127,l_apcb.apcb137,l_apcb.apcb022)
    #WHERE apcbld    = g_apca_m.apcald   #160905-00002#4 mark
    WHERE apcbent   = g_enterprise       #160905-00002#4 add
      AND apcbld    = g_apca_m.apcald    #160905-00002#4 add
      AND apcbdocno = g_apca_m.apcadocno
      AND apcbseq   = g_apcb_d[p_ac].apcbseq

   IF SQLCA.SQLCODE THEN
      LET r_errno = SQLCA.SQLCODE
   END IF
   
   RETURN r_errno
END FUNCTION

