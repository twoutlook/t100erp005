################################################################################
# Descriptions...: 多帳期產生1
# Memo...........:
# Usage..........: CALL aapt831_ins_apcc()
# Input parameter: 
# Return code....: 
# Date & Author..: 14/03/27 By albireo
# Modify.........:
################################################################################
PUBLIC FUNCTION aapt831_ins_apcc()
   DEFINE l_diff    LIKE type_t.num20_6
   DEFINE l_success LIKE type_t.num5
   DEFINE l_str     STRING
   DEFINE l_count   LIKE type_t.num10      #計算是否有單身    有單身才產生多帳期
   DEFINE l_amount              RECORD
             apcb103         LIKE apcb_t.apcb103,
             apcb113         LIKE apcb_t.apcb113,
             apcb123         LIKE apcb_t.apcb123,
             apcb133         LIKE apcb_t.apcb133,
             xrcd104         LIKE xrcd_t.xrcd104,
             xrcd114         LIKE xrcd_t.xrcd114,
             xrcd124         LIKE xrcd_t.xrcd124,
             xrcd134         LIKE xrcd_t.xrcd134,
             apca106         LIKE apca_t.apca106,
             apca116         LIKE apca_t.apca116,
             apca126         LIKE apca_t.apca126,
             apca136         LIKE apca_t.apca136,
             apca107         LIKE apca_t.apca107,   #NO USE
             apca117         LIKE apca_t.apca117,   #NO USE
             apca127         LIKE apca_t.apca127,   #NO USE
             apca137         LIKE apca_t.apca137,   #NO USE
             apca108         LIKE apca_t.apca108,
             apca118         LIKE apca_t.apca118,
             apca128         LIKE apca_t.apca128,
             apca138         LIKE apca_t.apca138
                         END RECORD

   IF cl_null(g_apca_m.apcadocno)THEN RETURN END IF
   LET l_count = NULL
   SELECT COUNT(*) INTO l_count FROM apcb_t
    WHERE apcbent = g_enterprise
      AND apcbld  = g_apca_m.apcald
      AND apcbdocno  = g_apca_m.apcadocno
   IF cl_null(l_count)THEN LET l_count = 0 END IF
   IF l_count = 0 THEN RETURN END IF

   #判斷是否單身金額與多帳期金額是否一致
    LET l_diff = 0
    #若多帳期有資料,考慮交易幣 匯率/金額 若有差異就重新產生多帳期
    #以ABS取差異的絕對值 避免 差異被net掉
    SELECT SUM(diff) INTO l_diff
      FROM (
            SELECT apcbseq,(ABS(apcb102 - CASE WHEN apcc101 IS NULL THEN 0 ELSE apcc101 END)+         #交易原幣單價(匯率)
                            ABS(apcb105 - CASE WHEN apcc105 IS NULL THEN 0 ELSE apcc105 END)+         #交易原幣含稅
                            ABS(apcb115 - CASE WHEN apcc115 IS NULL THEN 0 ELSE apcc115 END)+         #本幣含稅
                            ABS(apcb121 - CASE WHEN apcc121 IS NULL THEN 0 ELSE apcc121 END)+         #本幣二匯率
                            ABS(apcb125 - CASE WHEN apcc125 IS NULL THEN 0 ELSE apcc125 END)+         #本幣二含稅
                            ABS(apcb131 - CASE WHEN apcc131 IS NULL THEN 0 ELSE apcc131 END)+         #本幣三匯率
                            ABS(apcb135 - CASE WHEN apcc135 IS NULL THEN 0 ELSE apcc135 END)) as diff #本幣三含稅
              FROM apcb_t LEFT OUTER JOIN apcc_t ON apcbent = apccent AND apcbld = apccld
                                                AND apcbseq = apccseq AND apcbdocno =apccdocno 
             WHERE apcbent = g_enterprise
               AND apcbld  = g_apca_m.apcald
               AND apcbdocno = g_apca_m.apcadocno
            )
    #取絕對值後 並不會有負數,在此只是以防萬一   

    #先強制產生多帳期   #albireo-s
    LET l_diff = 1
    IF l_diff > 0 THEN
      #CALL s_transaction_begin()            
      #albireo 141117------------------------------------s
      INITIALIZE l_amount.* TO NULL
      CALL s_aap_clac_bill_master(g_apca_m.apcald,g_apca_m.apcadocno) RETURNING l_amount.*
      
      LET g_apca_m.apcamoddt = cl_get_current()
      UPDATE apca_t SET (apca103,apca104,apca106,apca107,apca108,
                         apca113,apca114,apca116,apca117,apca118,
                         apca123,apca124,apca126,apca127,apca128,
