################################################################################
# Descriptions...: 檢核理由碼有沒有再帳套+分類碼裡
# Memo...........: #161014-00017#1
# Usage..........: CALL aapt831_ld_oocq001_chk(l_apcb014)
#                  RETURNING 無
# Input parameter: 無
# Return code....: 無
# Date & Author..: 161019 By 08171
# Modify.........:
################################################################################
PRIVATE FUNCTION aapt831_ld_oocq001_chk(l_apcb014)
   DEFINE r_success LIKE type_t.num5
   DEFINE r_errno   LIKE gzze_t.gzze001
   DEFINE l_cnt     LIKE type_t.num5
   DEFINE l_apcb014 LIKE apcb_t.apcb014
   
   LET r_success = TRUE
   
   SELECT COUNT(1) INTO l_cnt
     FROM oocq_t LEFT OUTER JOIN oocql_t ON oocq001 = oocql001 AND oocq002 = oocql002 AND oocqent = oocqlent AND oocql003 = g_dlang
    WHERE oocqent = g_enterprise AND oocq001 = '3212'
      AND oocqstus = 'Y'
      AND oocq002 IN( SELECT glab003 FROM glab_t WHERE glabld = g_apca_m.apcald AND glab002 = '3212' AND glab001 = '11' AND glabent = g_enterprise)
      AND oocq002 = l_apcb014
   
   IF l_cnt = 0 THEN
      LET r_success = FALSE
      LET r_errno   = 'sub-00294'
   END IF
   RETURN r_success,r_errno
END FUNCTION

 
