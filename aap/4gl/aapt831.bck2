   LET g_apca_m.net4          = g_apca_m.apca138 - g_apca_m.sum_apcc139
   #150401-00001--(S)
   #141106-00010#23-----s
   SELECT apca117 INTO g_apca_m.apca117 FROM apca_t
    WHERE apcaent = g_enterprise
      AND apcald = g_apca_m.apcald
      AND apcadocno = g_apca_m.apcadocno
   IF cl_null(g_apca_m.apca117)THEN LET g_apca_m.apca117 = 0 END IF
   #141106-00010#23-----e
   LET g_apca_m.sum_apcc119  = g_apca_m.sum_apcc119  - g_apca_m.apca117
   LET g_apca_m.sum_apcc119_1= g_apca_m.sum_apcc119_1- g_apca_m.apca117
