################################################################################
# Descriptions...: 沖帳維護後影響多帳期金額
# Memo...........:
# Usage..........: CALL aapt831_apce_upd_apcc()
# Input parameter: 
# Return code....:
# Date & Author..: 14/03/27 By albireo
# Modify.........:
################################################################################
PUBLIC FUNCTION aapt831_apce_upd_apcc()
  DEFINE l_sql   STRING
   DEFINE l_apce  RECORD
                  apce002   LIKE apce_t.apce002,    #類型
                  apce100   LIKE apce_t.apce100,    #幣別
                  apce109   LIKE apce_t.apce109,
                  apce119   LIKE apce_t.apce119,
                  apce129   LIKE apce_t.apce129,
                  apce139   LIKE apce_t.apce139
                  END RECORD 
   DEFINE l_apcc  RECORD 
                  apccseq   LIKE apcc_t.apccseq,
                  apcc107   LIKE apcc_t.apcc107,
                  apcc117   LIKE apcc_t.apcc117,
                  apcc127   LIKE apcc_t.apcc127,
                  apcc137   LIKE apcc_t.apcc137
                  END RECORD  
                  
   DEFINE l_count LIKE type_t.num10 
   #需建立在apcc多帳期產生後的行為
   LET l_count = NULL
   SELECT COUNT(*) INTO l_count FROM apcc_t
    WHERE apccent   = g_enterprise
      AND apccld    = g_apca_m.apcald
      AND apccdocno = g_apca_m.apcadocno 
   IF cl_null(l_count) THEN LET l_count = 0 END IF
   IF l_count <= 0 THEN RETURN END IF

   #apce資料只限定41,30,17
   LET l_sql = "SELECT apce002,apce100,apce109,apce119,apce129,apce139 ",
               "  FROM apce_t ",
               " WHERE apceent = '",g_enterprise,"' ",
               "   AND apceld = '",g_apca_m.apcald,"' ",
               "   AND apcedocno = '",g_apca_m.apcadocno,"' ",
               "   AND apce002 IN ('41','30','17')"
   PREPARE sel_aapt831_apcep3 FROM l_sql
   DECLARE sel_aapt831_apcec3 CURSOR FOR sel_aapt831_apcep3

   INITIALIZE l_apce.* TO NULL
   INITIALIZE l_apcc.* TO NULL
   LET l_apcc.apcc107 = 0 
   LET l_apcc.apcc117 = 0
   LET l_apcc.apcc127 = 0
   LET l_apcc.apcc137 = 0
   FOREACH sel_aapt831_apcec3 INTO l_apce.*
      IF SQLCA.SQLCODE THEN INITIALIZE g_errparam TO NULL
  LET g_errparam.code = SQLCA.SQLCODE
  LET g_errparam.extend = 'FOREACH:sel_aapt831_apcec1'
  LET g_errparam.popup = TRUE
  CALL cl_err()
 END IF
      
      #判斷如果是17 塞-數
      IF l_apce.apce002 = '17' THEN
         LET l_apce.apce109 = l_apce.apce109 * (-1)
         LET l_apce.apce119 = l_apce.apce119 * (-1)
         LET l_apce.apce129 = l_apce.apce129 * (-1)
         LET l_apce.apce139 = l_apce.apce139 * (-1)
      END IF
      #其他塞正
      LET l_apcc.apcc107 = 0
      LET l_apcc.apcc117 = l_apcc.apcc117 + l_apce.apce119
      LET l_apcc.apcc127 = l_apcc.apcc127 + l_apce.apce129
      LET l_apcc.apcc137 = l_apcc.apcc137 + l_apce.apce139
   END FOREACH
   
   SELECT MIN(apccseq) INTO l_apcc.apccseq FROM apcc_t
    WHERE apccent = g_enterprise
      AND apccld  = g_apca_m.apcald
      AND apccdocno = g_apca_m.apcadocno

   UPDATE apcc_t 
      SET (apcc107,apcc117,apcc127,apcc137)
      =   (l_apcc.apcc107,l_apcc.apcc117,l_apcc.apcc127,l_apcc.apcc137)
    WHERE apccent = g_enterprise
      AND apccld  = g_apca_m.apcald
      AND apccdocno = g_apca_m.apcadocno
      AND apccseq   = l_apcc.apccseq
      AND apcc001   = 1

   IF SQLCA.SQLCODE THEN INITIALIZE g_errparam TO NULL
  LET g_errparam.code = SQLCA.SQLCODE
  LET g_errparam.extend = 'upd apcc_t'
  LET g_errparam.popup = TRUE
  CALL cl_err()
 END IF
END FUNCTION

################################################################################
# Descriptions...: 留置
# Memo...........:
# Usage..........: CALL aapt831_lien(p_flag)
# Input parameter: p_flag         TRUE留置/FLASE取消
# Return code....:
# Date & Author..: 14/03/27 By albireo
# Modify.........:
################################################################################
PUBLIC FUNCTION aapt831_lien(p_flag)
   DEFINE p_flag    LIKE type_t.num5
   DEFINE l_apca044 LIKE apca_t.apca044

   #應先查出單據
   IF cl_null(g_apca_m.apcald) OR cl_null(g_apca_m.apcadocno) THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = "std-00003"
      LET g_errparam.extend = ""
      LET g_errparam.popup = FALSE
      CALL cl_err()

      RETURN
   END IF
   LET g_apca_m_t.* = g_apca_m.*
   CALL cl_set_comp_entry("apca041,apca042,apca043,apca044,apca045",TRUE)
   INPUT BY NAME g_apca_m.apca041,g_apca_m.apca042,
                 g_apca_m.apca043,g_apca_m.apca044,g_apca_m.apca045
      ATTRIBUTE(WITHOUT DEFAULTS)
      BEFORE INPUT
         #留置狀態碼:Y留置,N取消留置
         IF p_flag THEN
            SELECT sum(apcc108) INTO l_apca044
              FROM apcc_t
             WHERE apccent  = g_enterprise AND apccld = g_apca_m.apcald
              AND apccdocno = g_apca_m.apcadocno
            IF cl_null(l_apca044) THEN LET l_apca044 = 0 END IF
            IF l_apca044 = 0 THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 'aap-00036'
               LET g_errparam.extend = ''
               LET g_errparam.popup = TRUE
               CALL cl_err()

               NEXT FIELD CURRENT
            END IF
            LET g_apca_m.apca040 = 'Y'
            DISPLAY BY NAME g_apca_m.apca040
            CALL cl_set_comp_entry("apca043",FALSE)
            LET g_apca_m.apca042 = g_today
            DISPLAY BY NAME g_apca_m.apca042
            IF cl_null (g_apca_m.apca044) THEN LET g_apca_m.apca044 = l_apca044 END IF
            LET g_apca_m.apca043 = ''
            DISPLAY BY NAME g_apca_m.apca043
         ELSE
            LET g_apca_m.apca040 = 'N'
            DISPLAY BY NAME g_apca_m.apca040
            LET g_apca_m.apca044 = 0
            DISPLAY BY NAME g_apca_m.apca044
            CALL cl_set_comp_entry("apca041,apca042,apca044,apca045",FALSE)
            CALL cl_set_comp_entry("apca043",TRUE)
            LET g_apca_m.apca043 = g_today
            DISPLAY BY NAME g_apca_m.apca043
         END IF

      AFTER FIELD apca044     #原幣金額
         IF l_apca044 < g_apca_m.apca044 THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = 'aap-00037'
            LET g_errparam.extend = ''
            LET g_errparam.popup = TRUE
            CALL cl_err()

            NEXT FIELD CURRENT
         END IF
        
      AFTER FIELD apca043     #留置解除日期
         IF NOT cl_null(g_apca_m.apca043) AND g_apca_m.apca043 < g_today THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = 'aap-00035'
            LET g_errparam.extend = ''
            LET g_errparam.popup = TRUE
            CALL cl_err()

            NEXT FIELD CURRENT
         END IF

      AFTER FIELD apca041     #理由碼
         IF NOT cl_null(g_apca_m.apca041) THEN
            IF g_apca_m.apca041 != g_apca_m_t.apca041 OR g_apca_m_t.apca041 IS NULL THEN
               IF NOT s_azzi650_chk_exist('298',g_apca_m.apca041) THEN
                  CALL s_desc_get_acc_desc('298',g_apca_m.apca041) RETURNING g_apca_m.apca041_desc
                  DISPLAY BY NAME g_apca_m.apca041_desc
                  NEXT FIELD CURRENT
               END IF
            END IF
         END IF
         CALL s_desc_get_acc_desc('298',g_apca_m.apca041) RETURNING g_apca_m.apca041_desc
         DISPLAY BY NAME g_apca_m.apca041_desc  

      ON ACTION controlp INFIELD apca041
         #開窗i段
	     INITIALIZE g_qryparam.* TO NULL
         LET g_qryparam.state = 'i'
	     LET g_qryparam.reqry = FALSE
         LET g_qryparam.default1 = g_apca_m.apca041     #給予default值
         
         LET g_qryparam.arg1 = "298"
         CALL q_oocq002()                              
         LET g_apca_m.apca041 = g_qryparam.return1      
         DISPLAY g_apca_m.apca041 TO apca041            
         CALL s_desc_get_acc_desc('298',g_apca_m.apca041) RETURNING g_apca_m.apca041_desc
         DISPLAY BY NAME g_apca_m.apca041_desc 
         NEXT FIELD apca041                            
         
      AFTER INPUT
         LET g_apca_m.apcamodid = g_user 
         LET g_apca_m.apcamoddt = cl_get_current()
         CALL s_transaction_begin()
         UPDATE apca_t 
            SET apca040   = g_apca_m.apca040, apca041 = g_apca_m.apca041,
                apca042   = g_apca_m.apca042, apca044 = g_apca_m.apca044,
                apca045   = g_apca_m.apca045, apca043 = g_apca_m.apca043,
                apcamodid = g_user          , apcamoddt = g_apca_m.apcamoddt
          WHERE apcaent   = g_enterprise AND apcald = g_apca_m.apcald
            AND apcadocno = g_apca_m.apcadocno
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = "std-00009"
               LET g_errparam.extend = "apca_t"
               LET g_errparam.popup = TRUE
               CALL cl_err()

               CALL s_transaction_end('N','0')
            WHEN SQLCA.sqlcode #其他錯誤
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = SQLCA.sqlcode
               LET g_errparam.extend = "apca_t"
               LET g_errparam.popup = TRUE
               CALL cl_err()
  
               CALL s_transaction_end('N','0')
            OTHERWISE
               CALL s_transaction_end('Y','0')
               DISPLAY BY NAME g_apca_m.apcamodid,g_apca_m.apcamoddt
         END CASE
   END INPUT
   IF INT_FLAG THEN
      LET INT_FLAG = 0
      LET g_apca_m.* = g_apca_m_t.*
      CALL aapt831_show()
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 9001
      LET g_errparam.extend = ''
      LET g_errparam.popup = FALSE
      CALL cl_err()

   END IF

   CALL cl_set_comp_entry("apca041,apca042,apca043,apca044,apca045",FALSE)
END FUNCTION

