################################################################################
# Descriptions...: 單身幣別預設匯率
# Memo...........:
# Usage..........: CALL aapt831_apcb100_carry(p_ac)
# Input parameter: 
# Return code....: 
# Date & Author..: 14/03/19 By albireo
# Modify.........:
################################################################################
PUBLIC FUNCTION aapt831_apcb100_carry(p_ac)
   DEFINE p_ac      LIKE type_t.num10
   DEFINE l_ooab002 LIKE ooab_t.ooab002
   DEFINE l_sfin3009 LIKE type_t.chr1
   
   IF cl_null(p_ac) OR p_ac <= 0 THEN　RETURN END IF
   CALL s_ld_sel_glaa(g_apca_m.apcald,'glaa001')
        RETURNING  g_sub_success,g_apca_m.glaa001 
        
   #151012-00014#2-----s
   #改依aoos020(S-FIN-3009)設定,帶入日/月平均匯率預設值
   #取單身交易原幣對本幣的匯率運算方式及取得的匯率
   CALL cl_get_para(g_enterprise,g_apca_m.apcacomp,'S-BAS-0014') RETURNING l_ooab002
   #--(MARK)-s
   #CALL s_aooi160_get_exrate('2',g_apca_m.apcald,g_apca_m.apcadocdt,g_apcb_d[p_ac].apcb100,g_apca_m.glaa001,0,l_ooab002)
   #   RETURNING g_apcb_d[p_ac].apcb102  
   #--(MARK)-e
   
   CALL cl_get_para(g_enterprise,g_apca_m.apcacomp,'S-FIN-3009') RETURNING l_sfin3009
   CASE l_sfin3009
      WHEN '1'  #依立帳日匯率(aooi160)
          CALL s_aooi160_get_exrate('2',g_apca_m.apcald,g_apca_m.apcadocdt,g_apcb_d[p_ac].apcb100,g_apca_m.glaa001,0,l_ooab002)
               RETURNING g_apcb_d[p_ac].apcb102
      WHEN '2'  #依立帳日月平均匯率(aooi170)
          CALL s_aooi160_get_exrate_avg('2',g_apca_m.apcald,g_apca_m.apcadocdt,g_apcb_d[p_ac].apcb100,g_apca_m.glaa001,0,l_ooab002)
               RETURNING g_sub_success,g_errno,g_apcb_d[p_ac].apcb102
   END CASE
   #151012-00014#2-----e

END FUNCTION
