   LET g_apca_m.sum_apcc129   = l_apcc.apcc129
   LET g_apca_m.sum_apcc139   = l_apcc.apcc139
   LET g_apca_m.apca113_1     = g_apca_m.apca113
   LET g_apca_m.apca116_1     = g_apca_m.apca116           #albireo 150225 140806-00012#6 
