################################################################################
# Descriptions...: 繳款單身輸入檢核(輸入當下)
# Memo...........: 和預支待抵單身相互控卡
# Date & Author..: 141118 By albireo 
# Modify.........:
################################################################################
PUBLIC FUNCTION aapt831_source_doc_chk_b2(p_ac,p_field)
   DEFINE p_ac        LIKE type_t.num10
   DEFINE p_field     LIKE type_t.chr10
   DEFINE r_success   LIKE type_t.num5
   DEFINE r_errno     LIKE gzze_t.gzze001
   DEFINE l_count     LIKE type_t.num10
   DEFINE l_sourcesum LIKE type_t.num20_6
   DEFINE l_targetsum LIKE type_t.num20_6
   
   LET r_success = TRUE
   LET r_errno   = ''
   IF cl_null(p_ac) OR p_ac <= 0 THEN RETURN r_success,r_errno END IF
   
   IF p_field = 'apce100_b2' THEN
      #預支待抵單身
      #與所有預支待抵原單apcb的原幣去比
      #
      LET l_count = NULL
      SELECT COUNT(*) INTO l_count FROM apcb_t
       WHERE apcbent = g_enterprise
         AND apcbld  = g_apca_m.apcald
         AND apcb100 = g_apcb2_d[p_ac].apce100_b2
         AND apcbdocno IN (SELECT apce003 FROM apce_t
                            WHERE apceent = g_enterprise
                              AND apceld  = g_apca_m.apcald
                              AND apcedocno  = g_apca_m.apcadocno
                              AND apce002 <> '17')
                              
      IF cl_null(l_count)THEN LET l_count = 0 END IF
      IF l_count = 0 THEN
         #幣別不存在
         #必須存在於預付單身
         LET r_success = FALSE
         LET r_errno = 'aap-00260'
      END IF
   END IF
   
   IF p_field = 'apce109_b2' THEN
      #規格
      #與所有預支待抵原單apcb的原幣去比
      #可沖抵原幣金額 < (現在此筆輸入的原幣金額+其他相同幣別的原幣金額)
      LET l_sourcesum = NULL
      SELECT SUM(apcb105) INTO l_sourcesum FROM apcb_t
       WHERE apcbent = g_enterprise
         AND apcbld  = g_apca_m.apcald
   #      AND apcb100 = g_apcb2_d[p_ac].apce100_b2
         AND apcbdocno IN (SELECT apce003 FROM apce_t
                            WHERE apceent = g_enterprise
                              AND apceld  = g_apca_m.apcald
                              AND apcedocno  = g_apca_m.apcadocno
                              AND apce002 <> '17')                              
      IF cl_null(l_sourcesum)THEN LET l_sourcesum = 0 END IF
      
      LET l_targetsum = NULL
      SELECT SUM(apce109) INTO l_targetsum FROM apce_t
       WHERE apceent = g_enterprise
         AND apceld  = g_apca_m.apcald
         AND apcedocno = g_apca_m.apcadocno
         AND apce002 = '17'
    #     AND apce100 = g_apcb2_d[p_ac].apce100_b2
         AND apceseq <> g_apcb2_d[p_ac].apceseq_b2
      IF cl_null(l_targetsum)THEN LET l_targetsum = 0 END IF
      
      CASE
         WHEN l_sourcesum = 0 
            LET r_success = FALSE
            LET r_errno   = 'aap-00262'
         WHEN l_sourcesum < (l_targetsum + g_apcb2_d[p_ac].apce109_b2)
            LET r_success = FALSE
            LET r_errno   = 'aap-00261'
      END CASE
   END IF
   
   RETURN r_success,r_errno
END FUNCTION

