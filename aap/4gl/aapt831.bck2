PUBLIC FUNCTION aapt831_source_doc_chk_b1(p_ac)
   DEFINE p_ac   LIKE type_t.num5
   DEFINE r_success LIKE type_t.num5
   DEFINE r_errno   LIKE gzze_t.gzze001
   DEFINE l_count   LIKE type_t.num5
   DEFINE l_apca001 LIKE apca_t.apca001
   DEFINE l_stus    LIKE apca_t.apcastus
   DEFINE l_apcc109 LIKE type_t.num20_6
   #albireo 150616-00002#1-----s
   DEFINE l_apca018 LIKE apca_t.apca018   #待抵單來源預支單
   #albireo 150616-00002#1-----e
   #150622-00005#1-----s
   DEFINE l_apca004 LIKE apca_t.apca004   #對象
   DEFINE l_apca014 LIKE apca_t.apca014   #業務人員
   #150622-00005#1-----e
   
   LET r_success = TRUE
   LET r_errno  = ''
   IF cl_null(p_ac) OR p_ac <= 0 THEN RETURN r_success,r_errno END IF
   
   #單號先檢核
   IF NOT cl_null(g_apcb1_d[p_ac].apce003_b1)THEN
      LET l_apca001 = NULL
      LET l_stus    = NULL
      LET l_apca018 = NULL
      SELECT apca001,apcastus,apca018
        INTO l_apca001,l_stus,l_apca018
        FROM apca_t
       WHERE apcaent = g_enterprise
         AND apcald  = g_apca_m.apcald
         AND apcadocno = g_apcb1_d[l_ac].apce003_b1
      CASE
         WHEN SQLCA.SQLCODE = 100
            #
            LET r_errno = 'aap-00159'
            LET r_success = FALSE
         #albireo #150602-00036#1 -----s
         WHEN l_apca001 NOT MATCHES '2[136]'   #不為員工預支待抵 or AAPT310   150622-00005#1
            LET r_success = FALSE
            LET r_errno = 'aap-00367'
         #albireo #150602-00036#1 -----e
         
         #150622-00005#1-----s
         #aapt310 必須符合 EMPL+員工
         WHEN l_apca001 MATCHES '2[13]' AND l_apca004 <> 'EMPL'
            LET r_success = FALSE
            LET r_errno = 'aap-00375'
         WHEN l_apca001 MATCHES '2[13]' AND l_apca014 <> g_apca_m.apca014
            LET r_success = FALSE
            LET r_errno = 'aap-00375'            
         #150622-00005#1-----e
         WHEN l_stus <> 'Y'
            LET r_errno = 'aap-00162'
            LET r_success = FALSE
      END CASE      
      
      #albireo 150616-00002#1-----s
      IF r_success THEN
         #來源預支單必須已經沖抵完畢
         LET l_apcc109 = NULL
         SELECT SUM(apcc108-apcc109) INTO l_apcc109 FROM apcc_t
          WHERE apccent = g_enterprise
            AND apccdocno = l_apca018
         IF cl_null(l_apcc109)THEN LET l_apcc109 = 0 END IF
         IF l_apcc109 <> 0 THEN
            LET r_errno = 'aap-00374'
            LET r_success = FALSE         
         END IF
      END IF
      #albireo 150616-00002#1-----e
   ELSE
      RETURN r_success,r_errno
   END IF
   IF NOT r_success THEN
      RETURN r_success,r_errno
   END IF
   
   #檢核金額
   IF NOT cl_null(g_apcb1_d[p_ac].apce004_b1)THEN
      LET l_apcc109 = NULL
      SELECT (apcc108-apcc109) INTO l_apcc109 FROM apcc_t
       WHERE apccent = g_enterprise
         AND apccld  = g_apca_m.apcald
         AND apccdocno = g_apcb1_d[l_ac].apce003_b1
         AND apccseq   = g_apcb1_d[l_ac].apce004_b1
         AND apcc001   = g_apcb1_d[l_ac].apce005_b1
      IF cl_null(l_apcc109)THEN LET l_apcc109 = 0 END IF
      CASE
         WHEN SQLCA.SQLCODE = 100
            #
            LET r_errno = 'aap-00163'
            LET r_success = FALSE
         WHEN l_apcc109 = 0 
            LET r_errno = 'aap-00273'
            LET r_success = FALSE
      END CASE      
      
      #albireo 150626-----s
      #比照直接沖銷檢核金額
      IF r_success THEN
         CALL s_aapt420_apcc_used_chk(g_apcb1_d[l_ac].apce002_b1,g_apca_m.apcald,g_apca_m.apcald,g_apcb1_d[l_ac].apce003_b1,
                                      g_apcb1_d[l_ac].apce004_b1,g_apcb1_d[l_ac].apce005_b1,
                                      l_apcc109,g_apca_m.apcadocno,g_apcb1_d[l_ac].apceseq_b1,'1','0')
                           RETURNING g_sub_success,g_errno
         IF NOT g_sub_success THEN
            LET r_success = FALSE
            LET r_errno = g_errno
         END IF
      END IF
      #albireo 150626-----e
   ELSE
      RETURN r_success,r_errno
   END IF
   
   RETURN r_success,r_errno
END FUNCTION

