################################################################################
# Descriptions...: 單身1來源組織檢查
# Memo...........:
# Usage..........: CALL aapt831_apcborga_chk(p_ac)
#                  RETURNING r_errno
# Input parameter: p_ac           
# Return code....: r_errno    
# Date & Author..: 14/03/17 By albireo
# Modify.........:
################################################################################
PUBLIC FUNCTION aapt831_apcborga_chk(p_ac)
   DEFINE p_ac    LIKE type_t.num10
   DEFINE r_errno LIKE gzze_t.gzze001
   DEFINE l_ooef  RECORD 
                  ooef017   LIKE ooef_t.ooef017,
                  ooefstus  LIKE ooef_t.ooefstus
                  END RECORD
   
   LET r_errno = NULL
   IF cl_null(p_ac) OR p_ac <= 0 THEN RETURN r_errno END IF
   
   INITIALIZE l_ooef.* TO NULL
   SELECT ooef017,ooefstus INTO l_ooef.* FROM ooef_t
    WHERE ooefent = g_enterprise
      AND ooef001 = g_apcb_d[p_ac].apcborga
      
   CASE
      WHEN SQLCA.SQLCODE = 100
         LET r_errno = 'aoo-00094'
      WHEN l_ooef.ooefstus = 'N'
         #LET r_errno = 'aoo-00095' #160318-00005#3 mark
         LET r_errno = 'sub-01302'  #160318-00005#3 add
      WHEN l_ooef.ooef017 <> g_apca_m.apcacomp
         LET r_errno = 'aap-00034'
   END CASE
   
   IF cl_null(r_errno)THEN
      #是否為帳務中心管理的組織檢查
      CALL s_fin_account_center_chk(g_apca_m.apcasite,g_apcb_d[p_ac].apcborga,'3',g_apca_m.apcadocdt)
           RETURNING g_sub_success,r_errno
      IF NOT cl_null(r_errno)THEN RETURN r_errno END IF
   END IF
 
   RETURN r_errno
   
END FUNCTION
