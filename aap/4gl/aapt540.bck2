   EXECUTE aapt540_master_referesh USING g_apgk_m.apgkcomp,g_apgk_m.apgkdocno INTO g_apgk_m.apgkcomp, 
       g_apgk_m.apgk004,g_apgk_m.apgk002,g_apgk_m.apgkdocno,g_apgk_m.apgkdocdt,g_apgk_m.apgk001,g_apgk_m.apgk003, 
