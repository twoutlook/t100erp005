                      ," AND apgkdocno = '", g_apgk_m.apgkdocno, "' "
 
   #填到對應位置
   CALL aapt540_browser_fill("")
 
   CLOSE aapt540_cl
   
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aapt540_msgcentre_notify('modify')
 
END FUNCTION 
 
