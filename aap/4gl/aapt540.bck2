       g_apgk_m.apgkcnfdt,g_apgk_m.apgkpstid,g_apgk_m.apgkpstdt,g_apgk_m.apgk100,g_apgk_m.apgk101,g_apgk_m.apgk014, 
       g_apgk_m.apgk013,g_apgk_m.apgk006,g_apgk_m.apgk103,g_apgk_m.apgk008,g_apgk_m.apgk007,g_apgk_m.apgk009, 
       g_apgk_m.apgk010,g_apgk_m.apgk019,g_apgk_m.apgk104,g_apgk_m.apgk011,g_apgk_m.apgk016,g_apgk_m.apgk017, 
       g_apgk_m.apgk018,g_apgk_m.apgk012,g_apgk_m.apgk114,g_apgk_m.apgk113,g_apgk_m.apgkcomp_desc,g_apgk_m.apgk004_desc, 
       g_apgk_m.apgk001_desc,g_apgk_m.apgkownid_desc,g_apgk_m.apgkowndp_desc,g_apgk_m.apgkcrtid_desc, 
       g_apgk_m.apgkcrtdp_desc,g_apgk_m.apgkmodid_desc,g_apgk_m.apgkcnfid_desc,g_apgk_m.apgkpstid_desc, 
       g_apgk_m.apgk017_desc,g_apgk_m.apgk018_desc
   
   #檢查是否允許此動作
   IF NOT aapt540_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_apgk_m_mask_o.* =  g_apgk_m.*
   CALL aapt540_apgk_t_mask()
   LET g_apgk_m_mask_n.* =  g_apgk_m.*
   
   
   
   #add-point:modify段show之前 name="modify.before_show"
   
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
 
   
   CALL aapt540_show()
   #add-point:modify段show之後 name="modify.after_show"
   
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
 
 
    
   WHILE TRUE
      LET g_apgkcomp_t = g_apgk_m.apgkcomp
      LET g_apgkdocno_t = g_apgk_m.apgkdocno
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_apgk_m.apgkmodid = g_user 
LET g_apgk_m.apgkmoddt = cl_get_current()
LET g_apgk_m.apgkmodid_desc = cl_get_username(g_apgk_m.apgkmodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      LET g_master_commit = "N"
      CALL aapt540_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後 name="modify.after_input"
      
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE apgk_t SET (apgkmodid,apgkmoddt) = (g_apgk_m.apgkmodid,g_apgk_m.apgkmoddt)
          WHERE apgkent = g_enterprise AND apgkcomp = g_apgkcomp_t
            AND apgkdocno = g_apgkdocno_t
 
      END IF
    
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         #若單頭無commit則還原
         IF g_master_commit = "N" THEN
            LET g_apgk_m.* = g_apgk_m_t.*
            CALL aapt540_show()
         END IF
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_apgk_m.apgkcomp != g_apgk_m_t.apgkcomp
      OR g_apgk_m.apgkdocno != g_apgk_m_t.apgkdocno
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前 name="modify.body.b_fk_update"
         
         #end add-point
         
         #更新單身key值
         UPDATE apgc_t SET apgccomp = g_apgk_m.apgkcomp
                                       ,apgcdocno = g_apgk_m.apgkdocno
 
          WHERE apgcent = g_enterprise AND apgccomp = g_apgk_m_t.apgkcomp
            AND apgcdocno = g_apgk_m_t.apgkdocno
 
            
         #add-point:單身fk修改中 name="modify.body.m_fk_update"
         
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            #   INITIALIZE g_errparam TO NULL 
            #   LET g_errparam.extend = "apgc_t" 
            #   LET g_errparam.code   = "std-00009" 
            #   LET g_errparam.popup  = TRUE 
            #   CALL cl_err()
            #   CALL s_transaction_end('N','0')
            #   CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "apgc_t:",SQLERRMESSAGE 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL s_transaction_end('N','0')
               CALL cl_err()
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後 name="modify.body.a_fk_update"
         
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL aapt540_set_act_visible()   
   CALL aapt540_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " apgkent = " ||g_enterprise|| " AND",
