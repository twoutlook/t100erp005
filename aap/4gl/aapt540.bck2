      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_apgkcomp_t = g_apgk_m.apgkcomp
   LET g_apgkdocno_t = g_apgk_m.apgkdocno
 
   CALL s_transaction_begin()
   
   OPEN aapt540_cl USING g_enterprise,g_apgk_m.apgkcomp,g_apgk_m.apgkdocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aapt540_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CLOSE aapt540_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
