PRIVATE FUNCTION aapt540_curr_recount_b2(p_ac)
   DEFINE p_ac         LIKE type_t.num10
   DEFINE l_sfin4012   LIKE type_t.chr80
   DEFINE l_ld         LIKE glaa_t.glaald
   DEFINE l_dummy2     LIKE type_t.num20_6
   DEFINE l_dummy3     LIKE type_t.num20_6
   DEFINE l_glaa001    LIKE glaa_t.glaa001
   DEFINE l_oodb004    LIKE oodb_t.oodb004
   DEFINE l_oodb005    LIKE oodb_t.oodb005
   DEFINE l_oodb006    LIKE oodb_t.oodb006
   DEFINE l_oodb011    LIKE oodb_t.oodb011
   DEFINE l_apcb105    LIKE apcb_t.apcb105
   DEFINE ls_js         STRING
   DEFINE lc_param      RECORD
            apca004     LIKE apca_t.apca004
                    END RECORD
   
   
   LET l_ld = NULL LET l_glaa001 = NULL
   SELECT glaald ,glaa001
     INTO l_ld ,l_glaa001
     FROM glaa_t
    WHERE glaaent = g_enterprise
      AND glaacomp = g_apgk_m.apgkcomp
      AND glaa014 = 'Y'
   IF cl_null(p_ac) OR p_ac <=0 THEN RETURN END IF
   
   #有輸入交易帳戶時,取銀存支出匯率來源
   IF NOT cl_null(g_apgc_d[l_ac].apgc014) THEN
      CALL cl_get_para(g_enterprise,g_apgk_m.apgkcomp,'S-FIN-4012') RETURNING l_sfin4012
   ELSE
      LET l_sfin4012 = ''
   END IF

   #160824-00049#4-----s
   IF g_apgc_d[l_ac].apgc100 = l_glaa001 THEN
      LET g_apgc_d[l_ac].apgc101 = 1
   ELSE
      IF g_apgc_d[l_ac].apgc100 <> g_apgk_m.apgk100 THEN   
   #160824-00049#4-----e
         IF l_sfin4012  = '23' THEN
            #銀行日平均匯率
            CALL s_anm_get_exrate(l_ld,g_apgk_m.apgkcomp,g_apgc_d[l_ac].apgc014,g_apgc_d[l_ac].apgc100,l_glaa001,g_apgk_m.apgkdocdt) RETURNING g_apgc_d[l_ac].apgc101
            #160824-00049#4-----s
            IF cl_null(g_apgc_d[l_ac].apgc101)THEN
               LET g_apgc_d[l_ac].apgc101 = 1
               INITIALIZE g_errparam.* TO NULL
               LET g_errparam.extend = ''
               LET g_errparam.code = 'aap-00587'
               LET g_errparam.popup = TRUE
               CALL cl_err()              
            END IF
            #160824-00049#4-----e
         ELSE
            LET lc_param.apca004 = g_apgk_m.apgk001
            LET ls_js = util.JSON.stringify(lc_param)
            CALL s_fin_get_curr_rate(g_apgk_m.apgkcomp,l_ld,g_apgk_m.apgkdocdt,g_apgc_d[l_ac].apgc100,ls_js)
                 RETURNING g_apgc_d[l_ac].apgc101,l_dummy2,l_dummy3
         END IF   
   #160824-00049#4-----s
      ELSE
         LET g_apgc_d[l_ac].apgc101 = g_apgk_m.apgk101
      END IF
   END IF
   #160824-00049#4-----e   
   
   CALL aapt540_tax_ins_b2(p_ac)
END FUNCTION

PRIVATE FUNCTION aapt540_tax_ins_b2(p_ac)
  DEFINE p_ac         LIKE type_t.num10
   DEFINE l_sfin4012   LIKE type_t.chr80
   DEFINE l_ld         LIKE glaa_t.glaald
   DEFINE l_dummy2     LIKE type_t.num20_6
   DEFINE l_dummy3     LIKE type_t.num20_6
   DEFINE l_glaa001    LIKE glaa_t.glaa001
   DEFINE l_oodb004    LIKE oodb_t.oodb004
   DEFINE l_oodb005    LIKE oodb_t.oodb005
   DEFINE l_oodb006    LIKE oodb_t.oodb006
   DEFINE l_oodb011    LIKE oodb_t.oodb011
   DEFINE l_apcb105    LIKE apcb_t.apcb105
   DEFINE ls_js         STRING
   DEFINE lc_param      RECORD
            apca004     LIKE apca_t.apca004
                    END RECORD  

   LET l_ld = NULL
   SELECT glaald INTO l_ld FROM glaa_t
    WHERE glaaent = g_enterprise
      AND glaacomp = g_apgk_m.apgkcomp
      AND glaa014 = 'Y'
   
   #匯率修改產生s_tax
   IF NOT cl_null(g_apgc_d[p_ac].apgc006) AND NOT cl_null(g_apgc_d[p_ac].apgc100)
      AND NOT cl_null(g_apgc_d[p_ac].apgc101) AND NOT cl_null(l_ld)
      THEN
      CALL s_tax_chk(g_apgc_d[p_ac].apgcorga, g_apgc_d[p_ac].apgc006) RETURNING g_sub_success,l_oodb004,l_oodb005,l_oodb006,l_oodb011
      IF l_oodb005 = 'Y' THEN
         LET l_apcb105 = g_apgc_d[p_ac].apgc105
      ELSE
         LET l_apcb105 = g_apgc_d[p_ac].apgc103
      END IF
      CALL s_tax_ins(g_apgk_m.apgkdocno,g_apgc_d[p_ac].apgcseq,'0',g_apgc_d[p_ac].apgcorga,l_apcb105,
                     g_apgc_d[p_ac].apgc006,1,g_apgc_d[p_ac].apgc100,g_apgc_d[p_ac].apgc101,l_ld,1,1)
           RETURNING g_apgc_d[p_ac].apgc103,g_apgc_d[p_ac].apgc104,g_apgc_d[p_ac].apgc105,
                     g_apgc_d[p_ac].apgc113,g_apgc_d[p_ac].apgc114,g_apgc_d[p_ac].apgc115,
                     l_dummy2,l_dummy2,l_dummy2,
                     l_dummy3,l_dummy3,l_dummy3
   END IF
   DISPLAY BY NAME g_apgc_d[p_ac].apgc006
END FUNCTION

