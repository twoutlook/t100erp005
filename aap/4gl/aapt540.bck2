################################################################################
PRIVATE FUNCTION aapt540_apgk005003_chk()
DEFINE r_success   LIKE type_t.num5
   DEFINE r_errno     LIKE gzze_t.gzze001
   DEFINE l_apga      RECORD
                      apgastus   LIKE apga_t.apgastus,
                      apga029    LIKE apga_t.apga029,
                      apga002    LIKE apga_t.apga002 #,                   
                      END RECORD
   DEFINE l_count     LIKE type_t.num10
   DEFINE l_count2    LIKE type_t.num10
   
   LET r_success = TRUE
   LET r_errno = ''
   
   IF NOT cl_null(g_apgk_m.apgk005)THEN
      #申請單號
      LET l_count2 = NULL
      SELECT COUNT(*) INTO l_count2 FROM apgk_t
       WHERE apgkent = g_enterprise
         AND apgkcomp = g_apgk_m.apgkcomp
         AND apgk005 = g_apgk_m.apgk005
         AND apgkdocno <> COALESCE(g_apgk_m.apgkdocno,' ')
         AND apgkstus = 'N'
      IF cl_null(l_count2)THEN LET l_count2 = 0 END IF
     
      LET l_count = NULL
      SELECT COUNT(*) INTO l_count FROM apgd_t
       WHERE apgdent = g_enterprise
         AND apgdcomp = g_apgk_m.apgkcomp
         AND apgddocno = g_apgk_m.apgk005
         AND apgdstus  = 'N'
      IF cl_null(l_count)THEN LET l_count = 0 END IF
      
      INITIALIZE l_apga.* TO NULL
      SELECT apgastus,apga029,apga002 INTO l_apga.*
        FROM apga_t
       WHERE apgaent = g_enterprise
         AND apgacomp = g_apgk_m.apgkcomp
         AND apgadocno = g_apgk_m.apgk005
         
      CASE
         WHEN SQLCA.SQLCODE = 100         #apga不存在
            LET r_success = FALSE 
            LET r_errno   = 'aap-00511'  
         WHEN l_apga.apgastus <> 'Y'      #申請單未確認
            LET r_success = FALSE
            LET r_errno   = 'aap-00512'
         WHEN l_apga.apga029 = 'Y'        #申請單已結案
            LET r_success = FALSE
            LET r_errno   = 'aap-00513'
         WHEN l_count > 0                 #申請單尚有未確認的變更單
            LET r_success = FALSE
            LET r_errno   = 'aap-00514'
         WHEN l_count2 > 0               #申請單有另外未確認的裝運通知單
            LET r_success = FALSE
            LET r_errno = 'aap-00515'
      END CASE
   END IF
   
   IF NOT r_success THEN
      RETURN r_success,r_errno
   END IF
   
   IF NOT cl_null(g_apgk_m.apgk003)THEN
      LET l_count = NULL
      SELECT COUNT(*) INTO l_count FROM apga_t
       WHERE apgaent = g_enterprise
         AND apga001 = g_apgk_m.apgk003
         AND apgastus = 'Y'
      IF cl_null(l_count)THEN LET l_count = 0 END IF
      IF l_count = 0 THEN      #LC NO 不存在確認申請單中
         LET r_success = FALSE
         LET r_errno = 'aap-00516'
      END IF
   END IF
   IF NOT r_success THEN
      RETURN r_success,r_errno
   END IF
   
   IF NOT cl_null(g_apgk_m.apgk005) AND NOT cl_null(g_apgk_m.apgk003)THEN
      #同時控卡aapt520存在N的否   有也是代表aapt510 未ready
      #160318 kris aapt530卡aapt510 ready否  aapt540 aapt550也卡aapt510ready否 因為卡不住
      
      #aapt510存在並確認且與lc_no可對上
      LET l_count = NULL
      SELECT COUNT(*) INTO l_count FROM apga_t
       WHERE apgacomp  = g_apgk_m.apgkcomp
         AND apgaent   = g_enterprise
         AND apgadocno = g_apgk_m.apgk005
         AND apga001   = g_apgk_m.apgk003
      IF cl_null(l_count)THEN LET l_count = 0 END IF
      
      IF l_count = 0 THEN
         LET r_success = FALSE
         LET r_errno = 'aap-00517'
      END IF
      
   END IF
   
   RETURN r_success,r_errno
END FUNCTION

