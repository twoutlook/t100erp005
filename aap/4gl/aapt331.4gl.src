################################################################################
# Descriptions...: 單身幣別預設匯率
# Memo...........:
# Usage..........: CALL aapt331_apcb100_carry(p_apcb100)
# Input parameter: p_apcb100 交易幣別
#                : 
# Return code....: l_apcb102 匯率
#                : 
# Date & Author..: #161124-00062#1 20161128 By catmoon47
# Modify.........:
################################################################################
PRIVATE FUNCTION aapt331_apcb100_carry(p_apcb100)
   DEFINE p_apcb100  LIKE apcb_t.apcb100
   
   DEFINE l_ooab002  LIKE ooab_t.ooab002
   DEFINE l_sfin3009 LIKE type_t.chr1
   DEFINE l_apcb102  LIKE apcb_t.apcb102
   
   CALL s_ld_sel_glaa(g_apca_m.apcald,'glaa001')
        RETURNING  g_sub_success,g_apca_m.glaa001 
        
   #改依aoos020(S-FIN-3009)設定,帶入日/月平均匯率預設值
   #取單身交易原幣對本幣的匯率運算方式及取得的匯率
   CALL cl_get_para(g_enterprise,g_apca_m.apcacomp,'S-BAS-0014') RETURNING l_ooab002   
   CALL cl_get_para(g_enterprise,g_apca_m.apcacomp,'S-FIN-3009') RETURNING l_sfin3009
   CASE l_sfin3009
      WHEN '1'  #依立帳日匯率(aooi160)
          CALL s_aooi160_get_exrate('2',g_apca_m.apcald,g_apca_m.apcadocdt,p_apcb100,g_apca_m.glaa001,0,l_ooab002)
               RETURNING l_apcb102
      WHEN '2'  #依立帳日月平均匯率(aooi170)
          CALL s_aooi160_get_exrate_avg('2',g_apca_m.apcald,g_apca_m.apcadocdt,p_apcb100,g_apca_m.glaa001,0,l_ooab002)
               RETURNING g_sub_success,g_errno,l_apcb102
   END CASE
   
   RETURN l_apcb102
   
END FUNCTION

################################################################################
# Descriptions...: 付款條件的相關帶值
# Memo...........:
# Usage..........: CALL aapt331_apca008_carry()
# Input parameter: 
# Date & Author..: 2016/12/06 By dorishsu 161202-00039#1
# Modify.........:
################################################################################
PUBLIC FUNCTION aapt331_apca008_carry()
   DEFINE l_ooib  RECORD
                  ooib025   LIKE ooib_t.ooib025,    #多帳期條件
                  ooib005   LIKE ooib_t.ooib005     #繳款優惠條件
                  END RECORD
   #付款條件帶出多帳期條件繳款條件
   INITIALIZE l_ooib.* TO NULL
   SELECT ooib025,ooib005 INTO l_ooib.ooib025,l_ooib.ooib005 FROM ooib_t
    WHERE ooibent = g_enterprise
      AND ooib002 = g_apca_m.apca008

   IF NOT cl_null(g_apca_m.apca008) AND NOT cl_null(g_apca_m.apca004)
      AND NOT cl_null(g_apca_m.apcacomp) AND NOT cl_null(g_apca_m.apcadocdt)THEN

      #應付款日 票到期日
      CALL s_fin_date_ap_fee(g_apca_m.apcacomp,g_apca_m.apca004,g_apca_m.apca008,g_apca_m.apcadocdt,
         g_apca_m.apcadocdt,g_apca_m.apcadocdt,'') RETURNING g_sub_success,g_apca_m.apca009,g_apca_m.apca010
   END IF
   DISPLAY BY NAME g_apca_m.apca009,g_apca_m.apca010
END FUNCTION

 
