################################################################################
# Descriptions...: 設定本位幣金額
# Memo...........:
# Usage..........: CALL aapt331_curr_info_master()
# Date & Author..: 2014/03/19 By Belle
# Modify.........:
################################################################################
PRIVATE FUNCTION aapt331_curr_info_master()
  
   #apca_t:
   SELECT apca103,apca113,apca123,apca133,
          apca106,apca116,apca126,apca136,
          apca108,apca118,apca128,apca138,
          apca103,apca113,
          apca106,apca116,
          apca108,apca118
     INTO g_apca_m.apca103,g_apca_m.apca113,g_apca_m.apca123,g_apca_m.apca133,
          g_apca_m.apca106,g_apca_m.apca116,g_apca_m.apcc126,g_apca_m.apcc136,
          g_apca_m.apca108,g_apca_m.apca118,g_apca_m.apcc128,g_apca_m.apcc138,
          g_apca_m.apca1031,g_apca_m.apca1131,
          g_apca_m.apca1061,g_apca_m.apcc1161,
          g_apca_m.apca1081,g_apca_m.apcc1181
     FROM apca_t
    WHERE apcaent = g_enterprise AND apcald = g_apca_m.apcald
      AND apcadocno = g_apca_m.apcadocno
  
   #apcc_t:
   SELECT SUM(apcc109*apcc006),
          SUM((apcc119*apcc006)+apcc113),
          SUM((apcc129*apcc006)+apcc123),
          SUM((apcc139*apcc006)+apcc133)
     INTO g_apca_m.apcc109,g_apca_m.apcc119,g_apca_m.apcc129,g_apca_m.apcc139
     FROM apcc_t
    WHERE apccent = g_enterprise AND apccld = g_apca_m.apcald
      AND apccdocno = g_apca_m.apcadocno
   
   IF cl_null(g_apca_m.apcc1161) THEN LET g_apca_m.apcc1161= 0 END IF
   IF cl_null(g_apca_m.apcc126)  THEN LET g_apca_m.apcc126 = 0 END IF
   IF cl_null(g_apca_m.apcc136)  THEN LET g_apca_m.apcc136 = 0 END IF
   IF cl_null(g_apca_m.apcc1181) THEN LET g_apca_m.apcc1181= 0 END IF
   IF cl_null(g_apca_m.apcc128)  THEN LET g_apca_m.apcc128 = 0 END IF
   IF cl_null(g_apca_m.apcc138)  THEN LET g_apca_m.apcc138 = 0 END IF
   IF cl_null(g_apca_m.apcc109)  THEN LET g_apca_m.apcc109 = 0 END IF
   IF cl_null(g_apca_m.apcc119)  THEN LET g_apca_m.apcc119 = 0 END IF
   IF cl_null(g_apca_m.apcc129)  THEN LET g_apca_m.apcc129 = 0 END IF
   IF cl_null(g_apca_m.apcc139)  THEN LET g_apca_m.apcc139 = 0 END IF

   #calc:
   LET g_apca_m.apca1081 = g_apca_m.apca108
   LET g_apca_m.apcc1181 = g_apca_m.apca118
   LET g_apca_m.apcc1091 = g_apca_m.apcc109
   LET g_apca_m.apcc1191 = g_apca_m.apcc119
   LET g_apca_m.net   = g_apca_m.apca108 - g_apca_m.apcc109
   LET g_apca_m.net1  = g_apca_m.apca118 - g_apca_m.apcc119
   
   LET g_apca_m.net01 = g_apca_m.apca108 - g_apca_m.apcc109
   LET g_apca_m.net11 = g_apca_m.apcc1181- g_apca_m.apcc1191
   LET g_apca_m.net2  = g_apca_m.apcc128 - g_apca_m.apcc129
   LET g_apca_m.net3  = g_apca_m.apcc138 - g_apca_m.apcc139
   
   DISPLAY BY NAME g_apca_m.apca103 ,g_apca_m.apca113 ,g_apca_m.apca123,g_apca_m.apca133,
                   g_apca_m.apca106 ,g_apca_m.apca116 ,
                   g_apca_m.apca108 ,g_apca_m.apca118 ,g_apca_m.apcc128,g_apca_m.apcc138,
                   g_apca_m.apcc109 ,g_apca_m.apcc119 ,g_apca_m.apcc129,g_apca_m.apcc139,
                   g_apca_m.apca1031,g_apca_m.apca1131,
                   g_apca_m.apca1061,
                   g_apca_m.apca1081,g_apca_m.apcc1181,
                   g_apca_m.apcc1091,g_apca_m.apcc1191,
                   g_apca_m.net,g_apca_m.net1,g_apca_m.net01,g_apca_m.net11,g_apca_m.net2,g_apca_m.net3
                   
 
END FUNCTION
################################################################################
# Descriptions...: 本位幣頁籤控制
# Memo...........:
# Usage..........: CALL aapt331_curr_show(p_ld)
# Input parameter: p_ld           帳別
# Return code....: void
# Date & Author..: 2014/03/19 By Belle
# Modify.........:
################################################################################
PRIVATE FUNCTION aapt331_curr_page(p_ld)
DEFINE p_ld        LIKE apca_t.apcald
DEFINE l_gzzd005   LIKE gzzd_t.gzzd005
DEFINE l_str       STRING
   
   CALL s_ld_sel_glaa(p_ld,'glaacomp|glaa001|glaa004|glaa015|glaa016|glaa017|glaa019|glaa020|glaa021|glaa024|glaa121')
        RETURNING g_sub_success,g_glaa.*
   LET g_apca_m.glaa001 = g_glaa.glaa001
   LET g_apca_m.apca120 = g_glaa.glaa016
   LET g_apca_m.apca130 = g_glaa.glaa020

   DISPLAY BY NAME g_apca_m.glaa001

   #帳別本幣的資料顯示  
   IF cl_null(g_apca_m.apca116) THEN LET g_apca_m.apca116 = 0 END IF
   LET g_apca_m.apca118 = g_apca_m.apca113 - g_apca_m.apca116
   IF cl_null(g_apca_m.apca118) THEN LET g_apca_m.apca118 = 0 END IF
   IF cl_null(g_apca_m.apcc119) THEN LET g_apca_m.apcc119 = 0 END IF
   LET g_apca_m.apca1131 = g_apca_m.apca113
   LET g_apca_m.apcc1181 = g_apca_m.apca113 - g_apca_m.apca116
   LET g_apca_m.apcc1191 = g_apca_m.apcc119
   LET g_apca_m.net     = g_apca_m.apca118 - g_apca_m.apcc119
   LET g_apca_m.net1    = g_apca_m.net
   DISPLAY BY NAME g_apca_m.apca113,
                   g_apca_m.apca1131,g_apca_m.apcc1181,g_apca_m.apcc1191,
                   g_apca_m.net,g_apca_m.net1
   
   SELECT SUM(apcc128),SUM(apcc129),
          SUM(apcc138),SUM(apcc139)
     INTO g_apca_m.apcc128,g_apca_m.apcc129,
          g_apca_m.apcc138,g_apca_m.apcc139
     FROM apcc_t
    WHERE apccent = g_enterprise AND apccld = g_apca_m.apcald
      AND apccdocno = g_apca_m.apcadocno  
   
   IF g_glaa.glaa015 = 'N' AND  g_glaa.glaa019 = 'N' THEN
      CALL cl_set_comp_visible('page2',FALSE)
      CALL cl_set_comp_visible('page6',FALSE)
   ELSE
      CALL cl_set_comp_visible('page2',TRUE)
      CALL cl_set_comp_visible('page6',TRUE)
      ##
      #azzi902要設定
      SELECT gzzd005 INTO l_gzzd005 FROM gzzd_t
       WHERE gzzd001  = g_prog  AND gzzd002  = g_lang
         AND gzzd003  = 'apcb115_b2' #多語
         AND gzzdstus = 'Y'
      LET l_str = l_gzzd005,"-",g_apca_m.glaa001
      CALL cl_set_comp_att_text('apcb115_b2',l_str)
      #B1.啟用本位幣二
      IF g_glaa.glaa015 = 'Y' THEN
         CALL cl_set_comp_visible('lbl_glaa015',TRUE)
         LET g_apca_m.net2 = g_apca_m.apcc128- g_apca_m.apcc129
         SELECT gzzd005 INTO l_gzzd005 FROM gzzd_t
          WHERE gzzd001  = g_prog  AND gzzd002  = g_lang
            AND gzzd003  = 'apcb125_b2' #多語
            AND gzzdstus = 'Y'
         LET l_str = l_gzzd005,"-",g_apca_m.apca120
         CALL cl_set_comp_att_text('apcb125_b2',l_str)
         DISPLAY BY NAME g_apca_m.apca120,g_apca_m.apca123,g_apca_m.apcc128,g_apca_m.apcc129,g_apca_m.net2
      ELSE
         CALL cl_set_comp_visible('lbl_glaa015',FALSE)
      END IF
      #B2.啟用本位幣三
