   #end add-point
   
   LET g_update = TRUE   
   
   #判斷key是否有改變
   LET lb_chk = TRUE
   FOR li_idx = 1 TO ps_keys.getLength()
      IF ps_keys[li_idx] <> ps_keys_bak[li_idx] THEN
         LET lb_chk = FALSE
         EXIT FOR
      END IF
   END FOR
   
   #不需要做處理
   IF lb_chk THEN
      RETURN
   END IF
   
   #判斷是否是同一群組的table
   LET ls_group = "'1',"
   IF ls_group.getIndexOf(ps_page,1) > 0 AND ps_table <> "apcb_t" THEN
      #add-point:update_b段修改前 name="update_b.before_update"
      CALL s_curr_round_ld('1',g_apca_m.apcald,g_apca_m.apca100,g_apcb_d[g_detail_idx].apcb105,2) RETURNING g_sub_success,g_errno,g_apcb_d[g_detail_idx].apcb105
      CALL s_curr_round_ld('1',g_apca_m.apcald,g_glaa.glaa001,g_apcb_d[g_detail_idx].apcb115,2) RETURNING g_sub_success,g_errno,g_apcb_d[g_detail_idx].apcb115
      CALL s_curr_round_ld('1',g_apca_m.apcald,g_glaa.glaa016,g_apcb_d[g_detail_idx].apcb125,2) RETURNING g_sub_success,g_errno,g_apcb_d[g_detail_idx].apcb125
      CALL s_curr_round_ld('1',g_apca_m.apcald,g_glaa.glaa020,g_apcb_d[g_detail_idx].apcb135,2) RETURNING g_sub_success,g_errno,g_apcb_d[g_detail_idx].apcb135            
      #end add-point 
      
      #將遮罩欄位還原
      CALL aapt331_apcb_t_mask_restore('restore_mask_o')
               
      UPDATE apcb_t 
         SET (apcbld,apcbdocno,
              apcbseq
              ,apcborga,apcb014,apcb005,apcb100,apcb105,apcb102,apcb115,apcb008,apcb009,apcb030,apcb047,apcb021,apcb029,apcb017,apcb010,apcb121,apcb125,apcb131,apcb135,apcb004,apcb002,apcb003,apcb023,apcb011,apcblegl,apcb012,apcb018,apcb015,apcb016,apcb024,apcb028,apcb051) 
              = 
             (ps_keys[1],ps_keys[2],ps_keys[3]
              ,g_apcb_d[g_detail_idx].apcborga,g_apcb_d[g_detail_idx].apcb014,g_apcb_d[g_detail_idx].apcb005, 
                  g_apcb_d[g_detail_idx].apcb100,g_apcb_d[g_detail_idx].apcb105,g_apcb_d[g_detail_idx].apcb102, 
                  g_apcb_d[g_detail_idx].apcb115,g_apcb_d[g_detail_idx].apcb008,g_apcb_d[g_detail_idx].apcb009, 
                  g_apcb_d[g_detail_idx].apcb030,g_apcb_d[g_detail_idx].apcb047,g_apcb_d[g_detail_idx].apcb021, 
                  g_apcb_d[g_detail_idx].apcb029,g_apcb_d[g_detail_idx].apcb017,g_apcb_d[g_detail_idx].apcb010, 
                  g_apcb_d[g_detail_idx].apcb121,g_apcb_d[g_detail_idx].apcb125,g_apcb_d[g_detail_idx].apcb131, 
                  g_apcb_d[g_detail_idx].apcb135,g_apcb_d[g_detail_idx].apcb004,g_apcb_d[g_detail_idx].apcb002, 
                  g_apcb_d[g_detail_idx].apcb003,g_apcb_d[g_detail_idx].apcb023,g_apcb_d[g_detail_idx].apcb011, 
                  g_apcb_d[g_detail_idx].apcblegl,g_apcb_d[g_detail_idx].apcb012,g_apcb_d[g_detail_idx].apcb018, 
                  g_apcb_d[g_detail_idx].apcb015,g_apcb_d[g_detail_idx].apcb016,g_apcb_d[g_detail_idx].apcb024, 
                  g_apcb_d[g_detail_idx].apcb028,g_apcb_d[g_detail_idx].apcb051) 
         WHERE apcbent = g_enterprise AND apcbld = ps_keys_bak[1] AND apcbdocno = ps_keys_bak[2] AND apcbseq = ps_keys_bak[3]
      #add-point:update_b段修改中 name="update_b.m_update"
                              
      #end add-point   
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "apcb_t" 
            LET g_errparam.code = "std-00009" 
            LET g_errparam.popup = TRUE 
            CALL s_transaction_end('N','0')
            CALL cl_err()
            
         WHEN SQLCA.SQLCODE #其他錯誤
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "apcb_t:",SQLERRMESSAGE 
            LET g_errparam.code = SQLCA.SQLCODE 
            LET g_errparam.popup = TRUE 
            CALL s_transaction_end('N','0')
            CALL cl_err()
            
         OTHERWISE
 
      END CASE
      
      #將遮罩欄位進行遮蔽
      CALL aapt331_apcb_t_mask_restore('restore_mask_n')
               
      #add-point:update_b段修改後 name="update_b.after_update"
                        
      #end add-point  
   END IF
   
   #子表處理
   IF ls_group.getIndexOf(ps_page,1) > 0 THEN
      
   END IF
   
   
 
   
 
   
   #add-point:update_b段other name="update_b.other"
               
   #end add-point  
   
