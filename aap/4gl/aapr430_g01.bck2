                  "       (CASE WHEN (SELECT ooefl003 FROM ooefl_t WHERE ooeflent = apdaent AND ooefl001 = apdasite AND ooefl002 = '",g_dlang,"') IS NULL THEN apdasite",
                  "             ELSE (SELECT apdasite||'.'||ooefl003 FROM ooefl_t WHERE ooeflent = apdaent AND ooefl001 = apdasite AND ooefl002 = '",g_dlang,"') END ),",
