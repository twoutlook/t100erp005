            #END add-point
 
 
         #應用 a02 樣板自動產生(Version:2)
         AFTER FIELD curr
            
            #add-point:AFTER FIELD curr name="input.a.page1.curr"
            
            #END add-point
            
 
 
         #應用 a04 樣板自動產生(Version:3)
         ON CHANGE curr
            #add-point:ON CHANGE curr name="input.g.page1.curr"
            
