         #應用 a01 樣板自動產生(Version:2)
         BEFORE FIELD apfh103
            #add-point:BEFORE FIELD apfh103 name="input.b.page1.apfh103"
            
            #END add-point
 
 
         #應用 a02 樣板自動產生(Version:2)
         AFTER FIELD apfh103
            
            #add-point:AFTER FIELD apfh103 name="input.a.page1.apfh103"
