PRIVATE FUNCTION aapt520_fomula_h(p_fomula)
   DEFINE p_fomula   LIKE type_t.chr10
   DEFINE l_ld       LIKE glaa_t.glaald
   DEFINE l_glaa001  LIKE glaa_t.glaa001
   DEFINE l_apga104  LIKE apga_t.apga104
   
   LET l_ld = NULL
   LET l_glaa001 = NULL
   SELECT glaald,glaa001 INTO l_ld,l_glaa001
     FROM glaa_t
    WHERE glaaent = g_enterprise
      AND glaacomp = g_apgd_m.apgdcomp
      AND glaa014 = 'Y'
      
   LET l_apga104 = NULL
   SELECT apga104 INTO l_apga104 FROM apga_t
    WHERE apgaent = g_enterprise
      AND apgacomp = g_apgd_m.apgdcomp
      AND apgadocno = g_apgd_m.apgddocno
   IF cl_null(l_apga104)THEN LET l_apga104 = 0 END IF
   
   CASE
      WHEN p_fomula = 'A'     
         #104=103*015/100
         
         
         IF cl_null(g_apgd_m.apgd103)THEN LET g_apgd_m.apgd103 = 0 END IF
         IF cl_null(g_apgd_m.apgd015)THEN LET g_apgd_m.apgd015 = 0 END IF         
         LET g_apgd_m.apgd104 = g_apgd_m.apgd103 * g_apgd_m.apgd015 / 100
         #原幣取位
         CALL s_curr_round_ld('1',l_ld,g_apgd_m.apgd100,g_apgd_m.apgd104,2) RETURNING g_sub_success,g_errno,g_apgd_m.apgd104
         
         #變更單 自備款計算後小於原單 則還是用原單 其他照常
         IF g_apgd_m.apgd104 < l_apga104 THEN
            LET g_apgd_m.apgd104 = l_apga104
            INITIALIZE g_errparam.* TO NULL
            LET g_errparam.code = 'aap-00508'
            LET g_errparam.popup = TRUE
            CALL cl_err()
         END IF
         
      WHEN p_fomula = 'B'
         #105=103-104
         IF cl_null(g_apgd_m.apgd103)THEN LET g_apgd_m.apgd103 = 0 END IF
         IF cl_null(g_apgd_m.apgd104)THEN LET g_apgd_m.apgd104 = 0 END IF           
         LET g_apgd_m.apgd105 = g_apgd_m.apgd103 - g_apgd_m.apgd104
      WHEN p_fomula = 'C'
         #113=103*101
         IF cl_null(g_apgd_m.apgd103)THEN LET g_apgd_m.apgd103 = 0 END IF
         IF cl_null(g_apgd_m.apgd101)THEN LET g_apgd_m.apgd101 = 0 END IF  
         LET g_apgd_m.apgd113 = g_apgd_m.apgd103 * g_apgd_m.apgd101
         #本幣取位
         CALL s_curr_round_ld('1',l_ld,l_glaa001,g_apgd_m.apgd113,2) RETURNING g_sub_success,g_errno,g_apgd_m.apgd113
      WHEN p_fomula = 'D'
         #114=104*101
         IF cl_null(g_apgd_m.apgd104)THEN LET g_apgd_m.apgd104 = 0 END IF
         IF cl_null(g_apgd_m.apgd101)THEN LET g_apgd_m.apgd101 = 0 END IF  
         LET g_apgd_m.apgd114 = g_apgd_m.apgd104 * g_apgd_m.apgd101
         #本幣取位
         CALL s_curr_round_ld('1',l_ld,l_glaa001,g_apgd_m.apgd114,2) RETURNING g_sub_success,g_errno,g_apgd_m.apgd114
      WHEN p_fomula = 'E'
         #115=105*101
         IF cl_null(g_apgd_m.apgd105)THEN LET g_apgd_m.apgd105 = 0 END IF
         IF cl_null(g_apgd_m.apgd101)THEN LET g_apgd_m.apgd101 = 0 END IF  
         LET g_apgd_m.apgd115 = g_apgd_m.apgd105 * g_apgd_m.apgd101
         #本幣取位
         CALL s_curr_round_ld('1',l_ld,l_glaa001,g_apgd_m.apgd115,2) RETURNING g_sub_success,g_errno,g_apgd_m.apgd115
      #160428-00001#2-----s
      WHEN p_fomula = 'F'
        #118=108*101
         IF cl_null(g_apgd_m.apgd108)THEN LET g_apgd_m.apgd108 = 0 END IF
         IF cl_null(g_apgd_m.apgd101)THEN LET g_apgd_m.apgd101 = 0 END IF  
         LET g_apgd_m.apgd118 = g_apgd_m.apgd108 * g_apgd_m.apgd101
         #本幣取位
         CALL s_curr_round_ld('1',l_ld,l_glaa001,g_apgd_m.apgd118,2) RETURNING g_sub_success,g_errno,g_apgd_m.apgd118
      WHEN p_fomula = 'G'
        #112=102*101
         IF cl_null(g_apgd_m.apgd102)THEN LET g_apgd_m.apgd102 = 0 END IF
         IF cl_null(g_apgd_m.apgd101)THEN LET g_apgd_m.apgd101 = 0 END IF  
         LET g_apgd_m.apgd112 = g_apgd_m.apgd102 * g_apgd_m.apgd101
         #本幣取位
         CALL s_curr_round_ld('1',l_ld,l_glaa001,g_apgd_m.apgd112,2) RETURNING g_sub_success,g_errno,g_apgd_m.apgd112
      #160428-00001#2-----e
   END CASE
   CALL aapt520_to_o_h()
END FUNCTION

