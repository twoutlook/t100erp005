            LET g_current_row = g_current_idx
            LET g_curr_diag = ui.DIALOG.getCurrent()
            CALL aapt520_idx_chk()
            
         ON ACTION last
            LET g_action_choice = "fetch"
            CALL aapt520_fetch('L')
            LET g_current_row = g_current_idx
