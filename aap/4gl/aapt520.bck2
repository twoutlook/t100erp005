PRIVATE FUNCTION aapt520_apge001_002_carry(p_ac)
   DEFINE p_ac   LIKE type_t.num10
   DEFINE l_source      DYNAMIC ARRAY OF RECORD
         chr         LIKE type_t.chr500,
         dat         LIKE type_t.dat
                 END RECORD
   DEFINE l_ld   LIKE glaa_t.glaald
   DEFINE l_pmdn047   LIKE pmdn_t.pmdn047
   DEFINE l_pmdn007   LIKE pmdn_t.pmdn007
   DEFINE l_glaa001   LIKE glaa_t.glaa001
   DEFINE l_oodb004   LIKE oodb_t.oodb004
   DEFINE l_apca013   LIKE apca_t.apca013
   DEFINE l_apca012   LIKE apca_t.apca012
   DEFINE l_oodb011   LIKE oodb_t.oodb011
   #160428-00001#2-----s
   DEFINE l_pmdl015   LIKE pmdl_t.pmdl015
   DEFINE l_pmdl016   LIKE pmdl_t.pmdl016   
   DEFINE r_success  LIKE type_t.num5         
   DEFINE r_errno    LIKE gzze_t.gzze001      
   #160428-00001#2-----e
   
# 貨號           apge003   不可空白  
# 品名規格       apge004   不可空白,依貨號帶出預設值       
# 單位           apge005   不可空白，檢核同採購單 apmt500    
# 稅別           apge006   若有採購單則帶採購單稅別,不可輸入 else 可稅入含稅稅別 and 下一筆帶上一筆稅別當預設 
# 含稅否         apge007   串 oodb                                  
# 採購數量       apge008   >0                                                
# 原幣含稅單價   apge009   若有採購單 and 含稅單價則帶出採購單之單價, 不可修改,
#                           採購單為未稅單價, 則轉換成含稅單價, 採購單含稅金額/數量= 含稅單價, 若無採購單則可input
# 原幣含稅金額   apge105   若有採購單則帶出採購單之原幣含稅金額, 不可修改, 若無則採購數量　*　單價                                 
# 本幣含稅金額   apge115   原幣金額*匯率 
      LET l_ld = NULL   LET l_glaa001 = NULL
      
      LET r_success = TRUE   LET r_errno = NULL    #160428-00001#2 add
      
      SELECT glaald ,glaa001
        INTO l_ld ,l_glaa001
        FROM glaa_t
       WHERE glaaent = g_enterprise
         AND glaacomp = g_apgd_m.apgdcomp
         AND glaa014 = 'Y'

      LET l_pmdn047 = NULL
      LET l_pmdn007 = NULL
      SELECT pmdn047 ,pmdn007
        INTO l_pmdn047 ,l_pmdn007
        FROM pmdn_t
       WHERE pmdnent = g_enterprise
         AND pmdndocno = g_apge_d[p_ac].apge001
         AND pmdnseq   = g_apge_d[p_ac].apge002
        
      IF cl_null(l_pmdn047)THEN LET l_pmdn047 = 0 END IF
      
      #160428-00001#2-----s
      LET l_pmdl015 = NULL
      LET l_pmdl016 = NULL
      SELECT pmdl015,pmdl016 
        INTO l_pmdl015,l_pmdl016
        FROM pmdl_t 
       WHERE pmdlent = g_enterprise
         AND pmdldocno = g_apge_d[p_ac].apge001  
       #檢查幣別是大於兩種
      CALL aapt520_curry_chk(l_pmdl015) RETURNING r_success,r_errno
      IF NOT r_success THEN     
         RETURN r_success,r_errno       
      END IF      
      #160428-00001#2-----e
      
      CALL l_source.clear()
      CALL s_aapt300_source_doc_carry_b('10',g_apge_d[p_ac].apge001,g_apge_d[p_ac].apge002,'','')
           RETURNING g_sub_success,g_errno,l_source
      LET g_apge_d[p_ac].apge100  = l_pmdl015         #幣別       #160428-00001#2
      LET g_apge_d[p_ac].apge101  = l_pmdl016         #匯率       #160428-00001#2           
      LET g_apge_d[p_ac].apge003  = l_source[4].chr   #產品編號
      LET g_apge_d[p_ac].apge004  = l_source[5].chr   #品名規格
      LET g_apge_d[p_ac].apge005  = l_source[6].chr   #單位
      LET g_apge_d[p_ac].apge006  = l_source[20].chr  #稅別
                                                      #含稅否
      LET g_apge_d[p_ac].apge008  = l_pmdn007         #數量
      LET g_apge_d[p_ac].apge105  = l_pmdn047         #pmdn047   #原幣含稅金額
      LET g_apge_d[p_ac].apge009  = l_pmdn047 / g_apge_d[p_ac].apge008   #pmdn047 / 數量 =原幣含稅單價
      #取位(原幣)
      CALL s_curr_round_ld('1',l_ld,g_apgd_m.apgd100,g_apge_d[p_ac].apge009,1) RETURNING g_sub_success,g_errno,g_apge_d[l_ac].apge009
                  
      #本幣含稅金額
      #LET g_apge_d[p_ac].apge115 = g_apge_d[p_ac].apge105 * g_apgd_m.apgd101   #160428-00001#2 mark
      LET g_apge_d[p_ac].apge115 = g_apge_d[p_ac].apge105 * g_apge_d[p_ac].apge101   #160428-00001#2
      #取位(本幣)
      CALL s_curr_round_ld('1',l_ld,l_glaa001,g_apge_d[p_ac].apge115,2) RETURNING g_sub_success,g_errno,g_apge_d[l_ac].apge115 
      CALL s_tax_chk(g_apgd_m.apgdcomp,g_apge_d[p_ac].apge006) RETURNING g_sub_success,l_oodb004,l_apca013,l_apca012,l_oodb011
      LET g_apge_d[p_ac].apge007 = l_apca013
      
      RETURN r_success,r_errno   #160428-00001#2 
END FUNCTION

