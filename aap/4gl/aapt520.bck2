# Descriptions...: 依單身有採購變更單的採購單號重產
# Memo...........:
# Date & Author..: 160726 By albireo
# Modify.........:
################################################################################
PRIVATE FUNCTION aapt520_reins_apge()
   DEFINE l_sql   STRING
   DEFINE l_idx   LIKE type_t.num10
   DEFINE l_apge  RECORD LIKE apge_t.*
   DEFINE l_apgear type_g_apge_d
   DEFINE l_apge001 LIKE apge_t.apge001
   DEFINE l_count   LIKE type_t.num10
   
   DEFINE l_source      DYNAMIC ARRAY OF RECORD
         chr         LIKE type_t.chr500,
         dat         LIKE type_t.dat
                 END RECORD
   DEFINE l_ld   LIKE glaa_t.glaald
   DEFINE l_pmdn047   LIKE pmdn_t.pmdn047
   DEFINE l_pmdn007   LIKE pmdn_t.pmdn007
   DEFINE l_glaa001   LIKE glaa_t.glaa001
   DEFINE l_oodb004   LIKE oodb_t.oodb004
   DEFINE l_apca013   LIKE apca_t.apca013
   DEFINE l_apca012   LIKE apca_t.apca012
   DEFINE l_oodb011   LIKE oodb_t.oodb011
   DEFINE l_pmdl015   LIKE pmdl_t.pmdl015
   DEFINE l_pmdl016   LIKE pmdl_t.pmdl016   
   DEFINE r_success  LIKE type_t.num5         
   DEFINE r_errno    LIKE gzze_t.gzze001      

   LET r_success = TRUE
   
   #先抓出單身採購單
   LET l_sql = "SELECT DISTINCT apge001 FROM apge_t ",
               " WHERE apgeent = ",g_enterprise," ",
               "   AND apgedocno = '",g_apgd_m.apgddocno,"' ",
               "   AND apgecomp  = '",g_apgd_m.apgdcomp,"' ",
               "   AND apge900   = ",g_apgd_m.apgd900," "
   PREPARE sel_apgep1 FROM l_sql
   DECLARE sel_apgec1 CURSOR FOR sel_apgep1               
     
   LET l_sql = "SELECT 0,'','',pmdndocno,pmdnseq,'','','','', ",   
               "       '','','','','','','','','','','' ",
               "  FROM pmdn_t ",
               " WHERE pmdnent = ",g_enterprise," ",
               "   AND pmdndocno = ? "
   PREPARE sel_pmdnp1 FROM l_sql
   DECLARE sel_pmdnc1 CURSOR FOR sel_pmdnp1
   
   FOREACH sel_apgec1 INTO l_apge001
      IF SQLCA.SQLCODE THEN EXIT FOREACH END IF
      LET l_count = NULL
      SELECT COUNT(*) INTO l_count FROM pmee_t 
       WHERE pmeeent = g_enterprise AND pmee902 >= g_apgd_m.apgd003
         AND pmeedocno = l_apge001
         AND pmeestus = 'Y'         
      IF cl_null(l_count)THEN LET l_count = 0 END IF
      
      IF l_count > 0 THEN
         LET l_pmdl015 = NULL
         LET l_pmdl016 = NULL
         SELECT pmdl015,pmdl016 
           INTO l_pmdl015,l_pmdl016
           FROM pmdl_t 
          WHERE pmdlent = g_enterprise
            AND pmdldocno = l_apge001
         #檢查幣別是大於兩種
         CALL aapt520_curry_chk(l_pmdl015) RETURNING g_sub_success,g_errno
         IF NOT r_success THEN     
            CONTINUE FOREACH
         END IF 
      
         DELETE FROM apge_t 
          WHERE apgeent = g_enterprise
            AND apgecomp = g_apgd_m.apgdcomp
            AND apgedocno = g_apgd_m.apgddocno
            AND apge900 = g_apgd_m.apgd900
            AND apge001 = l_apge001
         FOREACH sel_pmdnc1 USING l_apge001 INTO l_apgear.*
            IF SQLCA.SQLCODE THEN EXIT FOREACH END IF

            SELECT glaald ,glaa001
              INTO l_ld ,l_glaa001
              FROM glaa_t
             WHERE glaaent = g_enterprise
               AND glaacomp = g_apgd_m.apgdcomp
               AND glaa014 = 'Y'
            
            LET l_pmdn047 = NULL
            LET l_pmdn007 = NULL
            SELECT pmdn047 ,pmdn007
              INTO l_pmdn047 ,l_pmdn007
              FROM pmdn_t
             WHERE pmdnent = g_enterprise
               AND pmdndocno = l_apgear.apge001
               AND pmdnseq   = l_apgear.apge002
              
            IF cl_null(l_pmdn047)THEN LET l_pmdn047 = 0 END IF
            
            CALL l_source.clear()
            CALL s_aapt300_source_doc_carry_b('10',l_apgear.apge001,l_apgear.apge002,'','')
                 RETURNING g_sub_success,g_errno,l_source
            LET l_apgear.apge100  = l_pmdl015         #幣別       
            LET l_apgear.apge101  = l_pmdl016         #匯率                  
            LET l_apgear.apge003  = l_source[4].chr   #產品編號
            LET l_apgear.apge004  = l_source[5].chr   #品名規格
            LET l_apgear.apge005  = l_source[6].chr   #單位
            LET l_apgear.apge006  = l_source[20].chr  #稅別
                                                      #含稅否
            LET l_apgear.apge008  = l_pmdn007         #數量
            LET l_apgear.apge105  = l_pmdn047         #pmdn047   #原幣含稅金額
            LET l_apgear.apge009  = l_pmdn047 / l_apgear.apge008   #pmdn047 / 數量 =原幣含稅單價
            #取位(原幣)
            CALL s_curr_round_ld('1',l_ld,g_apgd_m.apgd100,l_apgear.apge009,1) RETURNING g_sub_success,g_errno,l_apgear.apge009                        
            #本幣含稅金額
            LET l_apgear.apge115 = l_apgear.apge105 * l_apgear.apge101   
            #取位(本幣)
            CALL s_curr_round_ld('1',l_ld,l_glaa001,l_apgear.apge115,2) RETURNING g_sub_success,g_errno,l_apgear.apge115 
            CALL s_tax_chk(g_apgd_m.apgdcomp,l_apgear.apge006) RETURNING g_sub_success,l_oodb004,l_apca013,l_apca012,l_oodb011
            LET l_apgear.apge007 = l_apca013
            
            SELECT MAX(apgeseq)+1 INTO l_apgear.apgeseq
              FROM apge_t
             WHERE apgeent = g_enterprise
               AND apgecomp = g_apgd_m.apgdcomp
               AND apgedocno = g_apgd_m.apgddocno
               AND apge900   = g_apgd_m.apgd900
            IF cl_null(l_apgear.apgeseq)THEN
               LET l_apgear.apgeseq = 1 
            END IF

            INITIALIZE l_apge.* TO NULL
            LET l_apge.apgeent = g_enterprise
            LET l_apge.apgecomp = g_apgd_m.apgdcomp
            LET l_apge.apgedocno = g_apgd_m.apgddocno
            LET l_apge.apgeseq = l_apgear.apgeseq
            LET l_apge.apge900 = g_apgd_m.apgd900
            LET l_apge.apgeorga = g_apgd_m.apgdcomp
            LET l_apge.apge001 = l_apgear.apge001
            LET l_apge.apge002 = l_apgear.apge002
            LET l_apge.apge003 = l_apgear.apge003
            LET l_apge.apge004 = l_apgear.apge004
            LET l_apge.apge005 = l_apgear.apge005
            LET l_apge.apge006 = l_apgear.apge006
            LET l_apge.apge007 = l_apgear.apge007
            LET l_apge.apge008 = l_apgear.apge008
            LET l_apge.apge009 = l_apgear.apge009
            LET l_apge.apge105 = l_apgear.apge105
            LET l_apge.apge115 = l_apgear.apge115
            LET l_apge.apge100 = l_apgear.apge100
            LET l_apge.apge101 = l_apgear.apge101
            INSERT INTO apge_t VALUES(l_apge.*)      
         END FOREACH
      END IF
   END FOREACH
   
   RETURN r_success
END FUNCTION

