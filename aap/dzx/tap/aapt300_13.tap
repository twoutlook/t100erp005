  <section id="aapt300_13.delete" ver="6" status="" src="s">
<![CDATA[#+ 資料刪除 
PRIVATE FUNCTION aapt300_13_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point  
   
   IF g_apca_m.apcald IS NULL
   OR g_apca_m.apcadocno IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   CALL aapt300_13_show()
   
   CALL s_transaction_begin()
    
   LET g_apcald_t = g_apca_m.apcald
   LET g_apcadocno_t = g_apca_m.apcadocno
 
   
   
 
   OPEN aapt300_13_cl USING g_enterprise,
                           g_apca_m.apcald
                           ,g_apca_m.apcadocno
 
   IF STATUS THEN
      CALL cl_err("OPEN aapt300_13_cl:", STATUS, 0)
      CLOSE aapt300_13_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   FETCH aapt300_13_cl INTO g_apca_m.apca040,g_apca_m.apca042,g_apca_m.apca041,g_apca_m.apca041_desc, 
       g_apca_m.apca044,g_apca_m.apcald,g_apca_m.apca045,g_apca_m.apcadocno,g_apca_m.apca043
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_apca_m.apcald,SQLCA.sqlcode,0)
      RETURN
   END IF
   
   IF cl_ask_delete() THEN
 
      #add-point:單頭刪除前
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point
 
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL aapt300_13_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
      DELETE FROM apca_t 
       WHERE apcaent = g_enterprise AND apcald = g_apca_m.apcald 
         AND apcadocno = g_apca_m.apcadocno 
 
 
      #add-point:單頭刪除中
      {<point name="delete.head.m_delete"/>}
      #end add-point
         
      IF SQLCA.sqlcode THEN
         CALL cl_err("apca_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
  
      
      
      #add-point:單頭刪除後
      {<point name="delete.head.a_delete"/>}
      #end add-point
      
          
      
      CLEAR FORM
      CALL aapt300_13_ui_browser_refresh()
      IF g_browser_cnt > 0 THEN
         CALL aapt300_13_fetch("P")
      ELSE
         CALL aapt300_13_browser_fill(" 1=1 ","F")
      END IF
      
   END IF
 
   CLOSE aapt300_13_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_apca_m.apcald,"D")
 
END FUNCTION
]]>
