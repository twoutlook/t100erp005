  <point name="construct.c.apcadocno" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[            ##############################################
            #資料範圍:            
            #確認
            #帳套帳務中心範圍符合
            #無傳票
            #多帳期未沖且還沒做重評價
            #不為系統產生            
            ##############################################
            INITIALIZE g_qryparam.* TO NULL
