            LET g_qryparam.reqry = FALSE
            LET g_qryparam.where = " apcastus = 'Y' ",
                                   " AND apcald = '",g_master.apcald,"' ",
                                   " AND apcasite IN ",g_wc_apcasite," ",
                                   " AND apca038 IS NULL ",
                                   " AND NOT EXISTS(SELECT 1 FROM apcc_t WHERE apccld = apcald AND apccent = apcaent AND apccdocno = apcadocno ",
                                   "                                       AND (apcc109 <> 0 OR apcc119 <> 0 OR apcc103 <> 0)) ",
                                   " AND apca017 <> '1' " 
            CALL q_apcadocno_12()                    
            DISPLAY g_qryparam.return1 TO apcadocno  
            NEXT FIELD apcadocno                     ]]>
