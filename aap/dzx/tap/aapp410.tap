  <point name="function.aapp410_b_fill2" cite_std="N" status="u" ver="1" src="s" new="Y" order="1" mark_hard="N">
<![CDATA[
################################################################################
# Descriptions...: 付款信息單身填充
# Memo...........:
# Usage..........: CALL aapp410_b_fill2(p_ac)
# Input parameter: 
# Date & Author..: 14/06/06 By albireo
# Modify.........:
################################################################################
PUBLIC FUNCTION aapp410_b_fill2(p_ac)
DEFINE p_ac   LIKE type_t.num10
   DEFINE l_index    LIKE type_t.num10

   IF cl_null(p_ac) OR p_ac <= 0 THEN RETURN END IF

   LET g_sql = " SELECT nmck023,nmck011,nmck012,nmck100,nmck103,nmck025,",
               "        nmck004,'',nmckstus,nmck005,'',nmck014,nmck015,'' ",
               "   FROM nmck_t,nmcl_t ", 
               "  WHERE nmckent = ",g_enterprise," ",
               "    AND nmckent = nmclent ",
               "    AND nmckcomp = nmclcomp ",
               "    AND nmckdocno = nmcldocno ",        
               "    AND nmcl001 = '",g_detail_d[p_ac].b_apcedocno,"' ",
               "    AND nmcl002 = '",g_detail_d[p_ac].b_apceseq,"' ",
               "    AND nmck023 IN ('10','20','30') "   #現金+匯款及票據
   PREPARE aapp410_sel2 FROM g_sql
   DECLARE b_fill_curs2 CURSOR FOR aapp410_sel2
   
   CALL g_detail2_d.clear()
 
   LET l_index = 1   
   FOREACH b_fill_curs2 INTO g_detail2_d[l_index].* 
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
      
      #抓銀行名稱
      SELECT nmaal003 INTO g_detail2_d[l_index].b_nmck004_desc 
        FROM nmas_t
        LEFT OUTER JOIN nmaal_t ON nmasent = nmaalent AND nmas001 = nmaal001 AND nmaal002 = g_dlang
        LEFT JOIN nmaa_t  ON nmasent = nmaaent  AND nmas001 = nmaa001
       WHERE nmaaent = g_enterprise
         AND nmas002 = g_detail2_d[l_index].b_nmck004

      #抓付款對象
      LET g_detail2_d[l_index].b_nmck005_desc = s_desc_get_trading_partner_abbr_desc(g_detail2_d[l_index].b_nmck005)

      #付款資訊
      IF cl_null(g_detail2_d[l_index].b_nmck014)THEN
         LET g_detail2_d[l_index].b_nmck015_desc = g_detail2_d[l_index].b_nmck015
      ELSE
         LET g_detail2_d[l_index].b_nmck015_desc = g_detail2_d[l_index].b_nmck015 CLIPPED,
                                                 "/",g_detail2_d[l_index].b_nmck014 CLIPPED
      END IF

      LET l_index = l_index + 1
      IF l_index > g_max_rec THEN
         IF g_error_show = 1 THEN
            CALL cl_err( "", 9035, 1 )
         END IF
         EXIT FOREACH
      END IF
      
   END FOREACH
   LET g_error_show = 0
   CALL g_detail2_d.deleteElement(g_detail2_d.getLength())
   
   CLOSE b_fill_curs2
   FREE aapp410_sel2
END FUNCTION]]>
</point>
  <point name="function.aapp410_apdacomp_carry" cite_std="N" status="" ver="1" src="s" new="Y" order="2" mark_hard="N">
<![CDATA[
################################################################################
# Descriptions...: 法人帶出主帳套及法人說明主帳套說明
# Memo...........:
# Usage..........: CALL aapp410_apdacomp_carry()
# Date & Author..: 14/06/06 By albireo
# Modify.........:
################################################################################
PUBLIC FUNCTION aapp410_apdacomp_carry()
   LET tm.apdacomp_desc = s_desc_get_department_desc(tm.apdacomp)
   SELECT glaald INTO tm.apdald FROM glaa_t
    WHERE glaaent = g_enterprise
      AND glaacomp = tm.apdacomp
      AND glaa014 = 'Y'
   LET tm.apdald_desc = s_desc_get_ld_desc(tm.apdald)
   IF cl_null(tm.apdald_desc)THEN
      LET tm.apdald_desc = tm.apdald
   ELSE
      LET tm.apdald_desc = tm.apdald,' (',tm.apdald_desc,') '
   END IF
   DISPLAY BY NAME tm.apdacomp_desc,tm.apdacomp,tm.apdald_desc
END FUNCTION]]>
</point>
  <point name="function.aapp410_payornot_chk" cite_std="N" status="u" ver="1" src="s" new="Y" order="3" mark_hard="N">
<![CDATA[
################################################################################
# Descriptions...: 請款單身勾選時或執行批次時的確認
# Memo...........:
# Usage..........: CALL aapp410_payornot_chk(p_ac)
#                  RETURNING r_success,r_errno
# Input parameter: 
# Date & Author..: 14/06/06 By albireo
# Modify.........:
################################################################################
PUBLIC FUNCTION aapp410_payornot_chk(p_ac)
   DEFINE p_ac        LIKE type_t.num10
   DEFINE r_errno     LIKE gzze_t.gzze001
   DEFINE r_success   LIKE type_t.num5
   DEFINE l_count     LIKE type_t.num10     #匯款資料檢查不符合
   DEFINE l_count2    LIKE type_t.num10     #票據資料檢查有問題
   DEFINE l_apce109   LIKE apce_t.apce109   #請款金額
   DEFINE l_nmckcount LIKE type_t.num20_6   #單一種類金額 
   DEFINE l_nmcksum   LIKE type_t.num20_6   #總金額(現金匯款票據)

   LET r_success = TRUE
   LET r_errno   = ''
   IF cl_null(p_ac) OR p_ac <= 0 THEN RETURN r_success,r_errno END IF

   #檢查1  分鏡規格CheckBoxRole1
   #apce009='Y'，須為己轉出納資料者
   LET l_count = 0 
   SELECT COUNT(*) INTO l_count FROM apce_t
    WHERE apceent   = g_enterprise
      AND apceld    = tm.apdald
      AND apcedocno = g_detail_d[p_ac].b_apcedocno
      AND apceseq   = g_detail_d[p_ac].b_apceseq
      AND apce009 <> 'Y'
   IF cl_null(l_count)THEN LET l_count = 0 END IF
   IF l_count > 0 THEN
      LET r_success = FALSE
      LET r_errno   = 'aap-00104'
   END IF
   
   IF NOT r_success THEN RETURN r_success,r_errno END IF

   #檢查2a 此apce單身所屬的付款資料為匯款 或現金者，必須已壓上匯款日期
   LET l_count = 0 
   SELECT COUNT(*) INTO l_count FROM nmck_t,nmcl_t
    WHERE nmckent = nmclent
      AND nmckdocno = nmcldocno
      AND nmckcomp  = nmclcomp
      AND nmckent = g_enterprise
      AND nmcldocno = g_detail_d[p_ac].b_apcedocno
      AND nmcl002   = g_detail_d[p_ac].b_apceseq
      AND nmck023   IN('10','20')    #匯款+現金
      AND nmck012 IS NULL 
   IF cl_null(l_count)THEN LET l_count = 0 END IF

   #檢查2b 此apce單身所屬付款資料為票據者，必需票據狀態為開立1
   LET l_count2 = 0 
   SELECT COUNT(*) INTO l_count2 FROM nmck_t,nmcl_t
    WHERE nmckent = nmclent
      AND nmckdocno = nmcldocno
      AND nmckcomp  = nmclcomp
      AND nmckent = g_enterprise
      AND nmcldocno = g_detail_d[p_ac].b_apcedocno
      AND nmcl002   = g_detail_d[p_ac].b_apceseq
      AND nmck023   = '30'    #票據
      AND nmck026  <> '1'
   IF cl_null(l_count2)THEN LET l_count2 = 0 END IF 

   CASE
      WHEN l_count > 0 AND l_count2 > 0 
         LET r_success = FALSE
         LET r_errno   = 'aap-00085'
      WHEN l_count > 0 
         LET r_success = FALSE
         LET r_errno   = 'aap-00086'
      WHEN l_count2 > 0 
         LET r_success = FALSE
         LET r_errno   = 'aap-00087'
   END CASE
   
   IF NOT r_success THEN RETURN r_success,r_errno END IF
   
   #檢查3 分鏡規格CheckBoxRole1
   #轉出納付款資料，支付金額合計=請款金額，且單據狀態碼皆為已確認
   #nmck_t 實際出納款資料現金 電匯 票據確認後金額，必須等於apce 請款單選取得那筆金額，
   #       幣別不會有不同的狀況，以原幣平檢核                  

   #請款資料
   LET l_apce109 = NULL
   SELECT apce109 INTO l_apce109 FROM apce_t
    WHERE apceent = g_enterprise
      AND apceld  = tm.apdald
      AND apcedocno = g_detail_d[p_ac].b_apcedocno
      AND apceseq   = g_detail_d[p_ac].b_apceseq
   IF cl_null(l_apce109)THEN LET l_apce109 = 0 END IF
   
   LET l_nmcksum = 0 
   #實際納款資料(票據 匯款)
   LET l_nmckcount = NULL
   SELECT SUM(nmck103) INTO l_nmckcount FROM nmck_t,nmcl_t
    WHERE nmckent = nmclent
      AND nmckdocno = nmcldocno
      AND nmckcomp  = nmclcomp
      AND nmckent = g_enterprise
      AND nmcldocno = g_detail_d[p_ac].b_apcedocno
      AND nmcl002   = g_detail_d[p_ac].b_apceseq
      AND ((nmck023   = '30' AND nmck026  <> '1')   #票據
          OR (nmck023 = '20' AND nmck012 IS NULL))  #匯款
      AND nmckstus = 'Y'
   IF cl_null(l_nmckcount)THEN LET l_nmckcount = 0 END IF
   LET l_nmcksum = l_nmcksum + l_nmckcount
   
   #現金部分待補
   #nmbb albireo
   
   IF l_apce109 <> l_nmcksum THEN
      LET r_success = FALSE
      LET r_errno   = 'aap-00105'      
   END IF
   
  
   RETURN r_success,r_errno
END FUNCTION]]>
</point>
  <point name="function.aapp410_do_process_chk" cite_std="N" status="" ver="1" src="s" new="Y" order="4" mark_hard="N">
<![CDATA[
################################################################################
# Descriptions...: 檢查選取的資料
# Memo...........:
# Usage..........: CALL aapp410_do_process_chk()
# Input parameter: 
# Date & Author..: 14/06/06 By albireo
# Modify.........:
################################################################################
PUBLIC FUNCTION aapp410_do_process_chk()
DEFINE l_i    LIKE type_t.num10
   
   FOR l_i = 1 TO g_detail_d.getLength()
      IF g_detail_d[l_i].sel = 'Y' THEN
         CALL aapp410_payornot_chk(l_i) RETURNING g_success1,g_errno
         IF NOT cl_null(g_errno)THEN
            LET g_aapp410msg = g_detail_d[l_i].b_apcedocno CLIPPED," / ",g_detail_d[l_i].b_apceseq USING "<<<<<<<<<<"
            CALL cl_errmsg('apdadocno,apdaseq',g_aapp410msg,'',g_errno,1)
            LET g_detail_d[l_i].sel = 'N'
            CONTINUE FOR
         END IF
      END IF 
   END FOR
END FUNCTION]]>
</point>
  <point name="function.aapp410_do_process" cite_std="N" status="u" ver="1" src="s" new="Y" order="5" mark_hard="N">
<![CDATA[
################################################################################
# Descriptions...: 執行請款單身批次結案
# Memo...........:
# Usage..........: CALL aapp410_do_process()
# Date & Author..: 14/06/06 By albireo
# Modify.........:
################################################################################
PUBLIC FUNCTION aapp410_do_process()
DEFINE l_count   LIKE type_t.num10
   DEFINE l_i       LIKE type_t.num10

   FOR l_i = 1 TO g_detail_d.getLength()
      IF g_detail_d[l_i].sel = 'Y' THEN
         CALL s_transaction_begin()
         UPDATE apce_t SET apce033 = g_user,
                           apce034 = TODAY
          WHERE apceent = g_enterprise
            AND apceld  = tm.apdald
            AND apcedocno = g_detail_d[l_i].b_apcedocno
            AND apceseq   = g_detail_d[l_i].b_apceseq
         IF SQLCA.SQLCODE THEN
            LET g_aapp410msg = g_detail_d[l_i].b_apcedocno CLIPPED," / ",g_detail_d[l_i].b_apceseq USING "<<<<<<<<<<"
            CALL cl_errmsg('apdadocno,apdaseq',g_aapp410msg,'upd_apce',SQLCA.SQLCODE,1)
            CALL s_transaction_end('N','0')
         ELSE
            CALL s_transaction_end('Y','0')
         END IF
      END IF
   END FOR
END FUNCTION]]>
</point>
