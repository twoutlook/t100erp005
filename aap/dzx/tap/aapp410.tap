  <point name="ui_dialog.more_displayarray" cite_std="N" status="u" ver="1" src="s" new="N" mark_hard="N">
<![CDATA[         DISPLAY ARRAY g_detail_d TO s_detail1.* ATTRIBUTE(COUNT=g_detail_cnt)
            BEFORE ROW 
               LET l_ac = DIALOG.getCurrentRow("s_detail1")
               CALL aapp410_b_fill2(l_ac) 
            ON ACTION choice_one
               FOR li_idx = 1 TO g_detail_d.getLength()
                  IF DIALOG.isRowSelected("s_detail1", li_idx) THEN
                     IF g_detail_d[li_idx].sel = 'N' THEN
                        CALL aapp410_payornot_chk(li_idx) RETURNING g_success1,g_errno
                        IF NOT cl_null(g_errno)THEN
                           CALL cl_err('',g_errno,1)
                           CONTINUE FOR
                        END IF
                        LET g_detail_d[li_idx].sel = "Y"
                     ELSE
                        LET g_detail_d[li_idx].sel = "N"
                     END IF
                  END IF
