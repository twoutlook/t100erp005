  #IF NOT g_success1 OR cl_null(g_glaald) THEN
      CALL s_fin_ld_carry('',g_user) RETURNING g_success1,tm.apdald,tm.apdacomp,g_errno
      CALL s_fin_account_center_with_ld_chk(tm.apdasite,tm.apdald,g_user,2,'N','') RETURNING g_success1,g_errno
  #END IF
   LET tm.apdald_desc = s_desc_get_ld_desc(tm.apdald)
