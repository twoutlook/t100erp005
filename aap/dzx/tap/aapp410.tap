  <point name="b_fill.sql_before" cite_std="N" status="u" ver="1" src="s" new="N" mark_hard="N">
<![CDATA[   LET ls_wc = tm.wc CLIPPED
   IF cl_null(ls_wc)THEN LET ls_wc = " 1=1" END IF 
    
   #分鏡規格s_apce
   #。同　帳務中心　及　主帳套別之請款單
	#。只取沖銷類型＝’１０付款’
	#。扣帳核銷日期apce034 IS NULL
	#。已確認之請款單據apdaconf='Y'
	#。符合　ＱＢＥ條件範圍
	# apce006 IN 10 20 30 (SCC8310)       
   LET g_sql  = "SELECT 'N',apcedocno,apceseq,apce006,apce100,apce109,apce032,apce038,apce009 ",
                "  FROM apce_t,apda_t ",
                " WHERE apceent = ? ",
                "   AND apdaent = apceent ",
                "   AND apdadocno = apcedocno ",
                "   AND apdald    = apceld ",
                "   AND apdastus = 'Y' ",           
                "   AND apce034 IS NULL ",
                "   AND apce002 = '10' ",
                "   AND apdaconf = 'Y' ",
                "   AND apdacomp = '",tm.apdacomp,"'",
                "   AND apdasite = '",tm.apdasite,"'",
                "   AND apce006 IN ('10','20','30') ",
                "   AND ",ls_wc CLIPPED]]>
</point>
