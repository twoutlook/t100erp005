  <section id="aapi050.input.body" ver="6" status="" src="s">
    <![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_apaa_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂單身ACTION
         
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_apaa_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL aapi050_b_fill(g_wc2) 
            IF g_rec_b != 0 THEN
               CALL fgl_set_arr_curr(l_ac)
            END IF
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN aapi050_cl USING g_enterprise,
                                               g_apaa_m.apaasite
 
                                               
               IF STATUS THEN
                  CALL cl_err("OPEN aapi050_cl:", STATUS, 1)
                  CLOSE aapi050_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_apaa_d[l_ac].apaa001 IS NOT NULL
               AND g_apaa_d[l_ac].apaa002 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_apaa_d_t.* = g_apaa_d[l_ac].*  #BACKUP
               LET g_apaa_d_o.* = g_apaa_d[l_ac].*  #BACKUP
               CALL aapi050_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL aapi050_set_no_entry_b(l_cmd)
               OPEN aapi050_bcl USING g_enterprise,g_apaa_m.apaasite,
 
                                                g_apaa_d_t.apaa001
                                                ,g_apaa_d_t.apaa002
 
               IF STATUS THEN
                  CALL cl_err("OPEN aapi050_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aapi050_bcl INTO g_apaa_d[l_ac].apaastus,g_apaa_d[l_ac].apaa001,g_apaa_d[l_ac].apaa002, 
                      g_apaa_d[l_ac].apaa003,g_apaa_d[l_ac].apaa004,g_apaa_d[l_ac].apaa005,g_apaa_d[l_ac].apaa006, 
                      g_apaa_d[l_ac].apaa007,g_apaa_d[l_ac].apaa008,g_apaa_d[l_ac].apaa009,g_apaa2_d[l_ac].apaa001, 
                      g_apaa2_d[l_ac].apaa002,g_apaa2_d[l_ac].apaaownid,g_apaa2_d[l_ac].apaaowndp,g_apaa2_d[l_ac].apaacrtid, 
                      g_apaa2_d[l_ac].apaacrtdp,g_apaa2_d[l_ac].apaacrtdt,g_apaa2_d[l_ac].apaamodid, 
                      g_apaa2_d[l_ac].apaamoddt
                  IF SQLCA.sqlcode THEN
                      CALL cl_err(g_apaa_d_t.apaa001,SQLCA.sqlcode,1)
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL aapi050_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
        
         BEFORE INSERT
            
            INITIALIZE g_apaa_d_t.* TO NULL
            INITIALIZE g_apaa_d_o.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_apaa_d[l_ac].* TO NULL
            
            #公用欄位預設值
            #此段落由子樣板a14產生    
      LET g_apaa2_d[l_ac].apaaownid = g_user
      LET g_apaa2_d[l_ac].apaaowndp = g_dept
      LET g_apaa2_d[l_ac].apaacrtid = g_user
      LET g_apaa2_d[l_ac].apaacrtdp = g_dept 
      LET g_apaa2_d[l_ac].apaacrtdt = cl_get_current()
      LET g_apaa2_d[l_ac].apaamodid = ""
      LET g_apaa2_d[l_ac].apaamoddt = ""
      LET g_apaa_d[l_ac].apaastus = ""
 
  
            
            #一般欄位預設值
                  LET g_apaa_d[l_ac].apaastus = "Y"
      LET g_apaa_d[l_ac].apaa002 = "1"
      LET g_apaa_d[l_ac].apaa003 = "Y"
      LET g_apaa_d[l_ac].apaa004 = "1"
      LET g_apaa_d[l_ac].apaa005 = "0"
      LET g_apaa_d[l_ac].apaa006 = "0"
      LET g_apaa_d[l_ac].apaa007 = "0"
      LET g_apaa_d[l_ac].apaa008 = "0"
      LET g_apaa_d[l_ac].apaa009 = "1"
 
            
            
            LET g_apaa_d_t.* = g_apaa_d[l_ac].*     #新輸入資料
            LET g_apaa_d_o.* = g_apaa_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL aapi050_set_entry_b(l_cmd)
            #add-point:set_entry_b後
            {<point name="input.body.before_insert.set_entry_b"/>}
            #end add-point
            CALL aapi050_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_apaa_d[li_reproduce_target].* = g_apaa_d[li_reproduce].*
               LET g_apaa2_d[li_reproduce_target].* = g_apaa2_d[li_reproduce].*
 
               LET g_apaa_d[g_apaa_d.getLength()].apaa001 = NULL
               LET g_apaa_d[g_apaa_d.getLength()].apaa002 = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
 
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM apaa_t 
             WHERE apaaent = g_enterprise AND apaasite = g_apaa_m.apaasite
 
               AND apaa001 = g_apaa_d[l_ac].apaa001
               AND apaa002 = g_apaa_d[l_ac].apaa002
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               CALL s_transaction_begin()
               #add-point:單身新增前
               {<point name="input.body.b_insert" mark="Y"/>}
               #end add-point
               INSERT INTO apaa_t
                           (apaaent,
                            apaasite,
                            apaa001,apaa002
                            ,apaastus,apaa003,apaa004,apaa005,apaa006,apaa007,apaa008,apaa009,apaaownid,apaaowndp,apaacrtid,apaacrtdp,apaacrtdt,apaamodid,apaamoddt) 
                     VALUES(g_enterprise,
                            g_apaa_m.apaasite,
                            g_apaa_d[l_ac].apaa001,g_apaa_d[l_ac].apaa002
                            ,g_apaa_d[l_ac].apaastus,g_apaa_d[l_ac].apaa003,g_apaa_d[l_ac].apaa004,g_apaa_d[l_ac].apaa005, 
                                g_apaa_d[l_ac].apaa006,g_apaa_d[l_ac].apaa007,g_apaa_d[l_ac].apaa008, 
                                g_apaa_d[l_ac].apaa009,g_apaa2_d[l_ac].apaaownid,g_apaa2_d[l_ac].apaaowndp, 
                                g_apaa2_d[l_ac].apaacrtid,g_apaa2_d[l_ac].apaacrtdp,g_apaa2_d[l_ac].apaacrtdt, 
                                g_apaa2_d[l_ac].apaamodid,g_apaa2_d[l_ac].apaamoddt)
               #add-point:單身新增中
               {<point name="input.body.m_insert"/>}
               #end add-point
               LET p_cmd = 'u'
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_apaa_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("apaa_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR "INSERT O.K"
               LET g_rec_b=g_rec_b+1
               DISPLAY g_rec_b TO FORMONLY.cnt
            END IF
            
            #add-point:單身新增後
            {<point name="input.body.after_insert"/>}
            #end add-point
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_apaa_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_apaa_d.deleteElement(l_ac)
               NEXT FIELD apaa001
            END IF
            IF g_apaa_d_t.apaa001 IS NOT NULL
               AND g_apaa_d_t.apaa002 IS NOT NULL
 
               THEN
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete"/>}
               #end add-point
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               IF aapi050_before_delete() THEN 
                  CALL s_transaction_end('Y','0')
               ELSE 
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               END IF 
               CLOSE aapi050_bcl
               LET l_count = g_apaa_d.getLength()
            END IF 
            
            #add-point:單身刪除後
            {<point name="input.body.a_delete"/>}
            #end add-point
              
         AFTER DELETE 
            #add-point:單身AFTER DELETE 
            {<point name="input.body.after_delete"/>}
            #end add-point
 
                  #此段落由子樣板a01產生
         BEFORE FIELD apaastus
            #add-point:BEFORE FIELD apaastus
            {<point name="input.b.page1.apaastus" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD apaastus
            
            #add-point:AFTER FIELD apaastus
            {<point name="input.a.page1.apaastus" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE apaastus
            #add-point:ON CHANGE apaastus
            {<point name="input.g.page1.apaastus" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD apaa001
            
            #add-point:AFTER FIELD apaa001
            {<point name="input.a.page1.apaa001" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD apaa001
            #add-point:BEFORE FIELD apaa001
            {<point name="input.b.page1.apaa001" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE apaa001
            #add-point:ON CHANGE apaa001
            {<point name="input.g.page1.apaa001" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD apaa002
            #add-point:BEFORE FIELD apaa002
            {<point name="input.b.page1.apaa002" />}
            #END add-point
 
