  <section id="aapi050.detail_reproduce" ver="2" status="" src="s">
    <![CDATA[#+ 單身自動複製
PRIVATE FUNCTION aapi050_detail_reproduce()
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE apaa_t.*
 
 
   #add-point:delete段define
   {<point name="detail_reproduce.define"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE aapi050_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE aapi050_detail AS ",
                "SELECT * FROM apaa_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO aapi050_detail SELECT * FROM apaa_t 
                                         WHERE apaaent = g_enterprise AND apaasite = g_apaasite_t
 
   
   #將key修正為調整後   
   UPDATE aapi050_detail 
      #更新key欄位
      SET apaasite = g_apaa_m.apaasite
 
      #更新共用欄位
      #此段落由子樣板a13產生
       , apaaownid = g_user
       , apaaowndp = g_dept
       , apaacrtid = g_user
       , apaacrtdp = g_dept 
       , apaacrtdt = ld_date
       , apaamodid = "" 
       , apaamoddt = "" 
      #, apaastus = "Y"
 
 
                                       
  
   #將資料塞回原table   
   INSERT INTO apaa_t SELECT * FROM aapi050_detail
   
   IF SQLCA.sqlcode THEN
      CALL cl_err("reproduce",SQLCA.sqlcode,1)
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE aapi050_detail
   
   #add-point:單身複製後1
   {<point name="detail_reproduce.body.table1.a_insert"/>}
   #end add-point
 
 
   
 
   
   #多語言複製段落
   
   
   CALL s_transaction_end('Y','0')
   
   #已新增完, 調整資料內容(修改時使用)
   LET g_apaasite_t = g_apaa_m.apaasite
 
   
   DROP TABLE aapi050_detail
   
END FUNCTION
]]>
