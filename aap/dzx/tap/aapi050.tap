  <section id="aapi050.b_fill" ver="6" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aapi050_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_apaa_d.clear()
   CALL g_apaa2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE apaastus,apaa001,apaa002,apaa003,apaa004,apaa005,apaa006,apaa007,apaa008, 
       apaa009,apaa001,apaa002,apaaownid,apaaowndp,apaacrtid,apaacrtdp,apaacrtdt,apaamodid,apaamoddt, 
       t1.pmaal004 ,t2.ooag011 ,t3.ooefl003 ,t4.ooag011 ,t5.ooefl003 ,t6.ooag011 FROM apaa_t",   
               "",
               
                              " LEFT JOIN pmaal_t t1 ON t1.pmaalent='"||g_enterprise||"' AND t1.pmaal001=apaa001 AND t1.pmaal002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t2 ON t2.ooagent='"||g_enterprise||"' AND t2.ooag001=apaaownid  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent='"||g_enterprise||"' AND t3.ooefl001=apaaowndp AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t4 ON t4.ooagent='"||g_enterprise||"' AND t4.ooag001=apaacrtid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=apaacrtdp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent='"||g_enterprise||"' AND t6.ooag001=apaamodid  ",
 
               " WHERE apaaent= ? AND apaasite=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("apaa_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF aapi050_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY apaa_t.apaa001,apaa_t.apaa002"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE aapi050_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR aapi050_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_apaa_m.apaasite
                                               
      FOREACH b_fill_cs INTO g_apaa_d[l_ac].apaastus,g_apaa_d[l_ac].apaa001,g_apaa_d[l_ac].apaa002,g_apaa_d[l_ac].apaa003, 
          g_apaa_d[l_ac].apaa004,g_apaa_d[l_ac].apaa005,g_apaa_d[l_ac].apaa006,g_apaa_d[l_ac].apaa007, 
          g_apaa_d[l_ac].apaa008,g_apaa_d[l_ac].apaa009,g_apaa2_d[l_ac].apaa001,g_apaa2_d[l_ac].apaa002, 
          g_apaa2_d[l_ac].apaaownid,g_apaa2_d[l_ac].apaaowndp,g_apaa2_d[l_ac].apaacrtid,g_apaa2_d[l_ac].apaacrtdp, 
          g_apaa2_d[l_ac].apaacrtdt,g_apaa2_d[l_ac].apaamodid,g_apaa2_d[l_ac].apaamoddt,g_apaa_d[l_ac].apaa001_desc, 
          g_apaa2_d[l_ac].apaaownid_desc,g_apaa2_d[l_ac].apaaowndp_desc,g_apaa2_d[l_ac].apaacrtid_desc, 
          g_apaa2_d[l_ac].apaacrtdp_desc,g_apaa2_d[l_ac].apaamodid_desc
                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_apaa2_d[l_ac].apaaownid_desc = cl_get_username(g_apaa2_d[l_ac].apaaownid)
      #LET g_apaa2_d[l_ac].apaaowndp_desc = cl_get_deptname(g_apaa2_d[l_ac].apaaowndp)
      #LET g_apaa2_d[l_ac].apaacrtid_desc = cl_get_username(g_apaa2_d[l_ac].apaacrtid)
      #LET g_apaa2_d[l_ac].apaacrtdp_desc = cl_get_deptname(g_apaa2_d[l_ac].apaacrtdp)
      #LET g_apaa2_d[l_ac].apaamodid_desc = cl_get_username(g_apaa2_d[l_ac].apaamodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_apaa_d.deleteElement(g_apaa_d.getLength())
      CALL g_apaa2_d.deleteElement(g_apaa2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE aapi050_pb   
   
END FUNCTION
]]>
  </section>
