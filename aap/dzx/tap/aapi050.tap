            END IF
 
 
         #+ 此段落由子樣板a43產生
         ON ACTION open_apmm100_02
            LET g_action_choice="open_apmm100_02"
            IF cl_auth_chk_act("open_apmm100_02") THEN
               
               #add-point:ON ACTION open_apmm100_02
               {<point name="menu.open_apmm100_02" />}
