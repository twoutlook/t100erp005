   LET g_sql = " SELECT UNIQUE apaasite,t1.ooefl003",
               " FROM apaa_t",
                              " LEFT JOIN ooefl_t t1 ON t1.ooeflent='"||g_enterprise||"' AND t1.ooefl001=apaasite AND t1.ooefl002='"||g_dlang||"' ",
 
               " WHERE apaaent = '" ||g_enterprise|| "' AND apaasite = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE aapi050_master_referesh FROM g_sql
 
 
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_aapi050 WITH FORM cl_ap_formpath("aap",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL aapi050_init()   
 
      #進入選單 Menu (="N")
      CALL aapi050_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_aapi050
      
   END IF 
   
   CLOSE aapi050_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
   
END MAIN
 
 
]]>
  </section>
  <section id="aapi050.modify" ver="4" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aapi050_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_apaa_m.apaasite IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE aapi050_master_referesh USING g_apaa_m.apaasite INTO g_apaa_m.apaasite,g_apaa_m.apaasite_desc 

 
   ERROR ""
  
   LET g_apaasite_t = g_apaa_m.apaasite
 
   CALL s_transaction_begin()
   
   OPEN aapi050_cl USING g_enterprise,g_apaa_m.apaasite
 
   IF STATUS THEN
      CALL cl_err("OPEN aapi050_cl:", STATUS, 1)
      CLOSE aapi050_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aapi050_cl INTO g_apaa_m.apaasite,g_apaa_m.apaasite_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_apaa_m.apaasite,SQLCA.sqlcode,0)
      CLOSE aapi050_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL aapi050_show()
   WHILE TRUE
      LET g_apaasite_t = g_apaa_m.apaasite
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL aapi050_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_apaa_m.* = g_apaa_m_t.*
         CALL aapi050_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_apaa_m.apaasite != g_apaasite_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE apaa_t SET apaasite = g_apaa_m.apaasite
 
          WHERE apaaent = g_enterprise AND apaasite = g_apaasite_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("apaa_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("apaa_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_apaa_m.apaasite,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE aapi050_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_apaa_m.apaasite,'U')
 
   CALL aapi050_b_fill("1=1")
   
END FUNCTION   
]]>
  </section>
