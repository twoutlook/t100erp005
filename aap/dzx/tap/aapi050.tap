            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
         
         #----<<apaacnfdt>>----
         
         #----<<apaapstdt>>----
 
 
           
         #單身一般欄位開窗相關處理
                  #此段落由子樣板a01產生
         BEFORE FIELD apaastus
            #add-point:BEFORE FIELD apaastus
            {<point name="construct.b.page1.apaastus" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD apaastus
            
            #add-point:AFTER FIELD apaastus
            {<point name="construct.a.page1.apaastus" />}
            #END add-point
            
 
         #Ctrlp:construct.c.page1.apaastus
         ON ACTION controlp INFIELD apaastus
            #add-point:ON ACTION controlp INFIELD apaastus
            {<point name="construct.c.page1.apaastus" />}
            #END add-point
 
         #Ctrlp:construct.c.page1.apaa001
         ON ACTION controlp INFIELD apaa001
            #add-point:ON ACTION controlp INFIELD apaa001
            {<point name="construct.c.page1.apaa001" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD apaa001
            #add-point:BEFORE FIELD apaa001
            {<point name="construct.b.page1.apaa001" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD apaa001
            
            #add-point:AFTER FIELD apaa001
            {<point name="construct.a.page1.apaa001" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD apaa002
            #add-point:BEFORE FIELD apaa002
            {<point name="construct.b.page1.apaa002" />}
            #END add-point
 
