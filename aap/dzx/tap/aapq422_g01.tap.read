#           LET g_sql = " SELECT apce100,(SUM(CASE apce015 WHEN 'C' THEN apce109*-1 ELSE 0 END) + SUM(CASE apce015 WHEN 'D' THEN apce109 ELSE 0 END)) ",
#                       "   FROM apce_t                          ",
