#                       "    AND apceent   = '",sr1.apdaent,"'   ",
#                       "    AND apceld    = '",sr1.apdald,"'    ",
#                       "    GROUP BY apce100                    ",
#                       "    ORDER BY apce100                    " 
                       
           LET g_sql = " SELECT stbe008,SUM(stbe011 * stbe016)    ",
                       "   FROM stbe_t,apcb_t                     ",
                       "  WHERE stbeent = '",g_enterprise,"'      ",
                       "    AND stbedocno = '",sr1.l_stbedocno,"' ",
                       "    AND apcbld = '",sr1.apdald,"'         ",
                       "    AND apcbdocno = '",sr1.apce003,"'     ",   
                       "    AND stbeent = apcbent                 ",
                       "    AND stbedocno = apcb008               ",
                       "    AND stbe002 = apcb002                 ",
                       "    AND stbe003 = apcb003                 ",
                       "    GROUP BY stbe008                      ",
                       "    ORDER BY stbe008                      " 
                       
           LET l_sql_cnt = " SELECT COUNT(*) FROM (",g_sql,")"
           PREPARE aapq422_g01_subrep05_cnt_pre FROM l_sql_cnt          
           EXECUTE aapq422_g01_subrep05_cnt_pre INTO l_count
           FREE aapq422_g01_subrep05_cnt_pre            
           IF l_count > 0 THEN 
              LET l_subrep05_show = 'Y' 
           END IF  
           LET l_count  = 1
