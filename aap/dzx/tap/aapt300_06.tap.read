  <point name="input.pre_input" order="" ver="3" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   CALL cl_set_combo_scc('apca001','8502') 
   LET g_apcadocno = p_apcadocno
   LET g_apcald    = p_apcald
   INITIALIZE g_glaa.* TO NULL   
   INITIALIZE l_apca.* TO NULL
   
   CALL s_ld_sel_glaa(g_apcald,'ALL') RETURNING g_glaa.*
   
   SELECT * INTO l_apca.* FROM apca_t
    WHERE apcaent = g_enterprise
      AND apcald  = g_apcald  AND apcadocno = g_apcadocno 
   
   CALL aapt300_06_b_fill1()
   LET l_sql = "SELECT apcfdocno,apcfld,apcfseq,apcfseq2,",
               "            '',apcf008,apcf009,apcf021,     '',apcf007,",
               "       apcf101,apcf103,apcf104,apcf105,apcf102,",
               "       apcf113,apcf114,apcf115,apcf010,apcf020,",
               "       apcf022,apcf023,apcf111,apcf122,apcf123,",
               "       apcf124,apcf125,apcf126,apcf127,",
               "       apcf132,apcf133,apcf134,apcf135,apcf136,",
               "       apcf137,apcf001,apcf002,",
               "       apcf023,apcf111,apcf122,apcf123,apcf124,apcf125,apcf126,apcf127,",
               "       apcf132,apcf133,apcf134,apcf135,apcf136,apcf137,apcf001,apcf002,apcforga,",
               "       apcf026,apcf027,apcf028,apcf029,apcf030,apcf031,apcf032,apcf033,apcf034,",
               "       apcf035,apcf036,apcf037,apcf038,apcf039,apcf040,apcf041,apcf042,apcf043,",
               "       apcf044,apcf045,apcf046,apcf047,apcf048 ",
               "  FROM apcf_t ",
               " WHERE apcfent = ",g_enterprise," ",
               "   AND apcfld  = '",g_apcald,"' AND apcfdocno = '",g_apcadocno,"' ",
               "   AND apcfseq = ? AND apcfseq2 = ? FOR UPDATE"
   LET l_sql = cl_sql_forupd(l_sql)                #轉換不同資料庫語法
   LET l_sql = cl_sql_add_mask(l_sql)              #遮蔽特定資料
   DECLARE aapt300_06_cl CURSOR FROM l_sql
   
   CALL aapt300_06_set_entry_b1(l_cmd)
   CALL aapt300_06_set_no_entry_b1(l_cmd)
   
 
   IF l_apca.apcastus MATCHES '[YX]' THEN
      CALL aapt300_06_display()
      CLOSE WINDOW w_aapt300_06 
