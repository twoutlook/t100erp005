  <point name="input.before_input" order="" ver="3" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[            SELECT apcadocdt INTO l_apcadocdt
              FROM apca_t
             WHERE apcaent = g_enterprise AND apcald = g_apcald
               AND apcadocno=g_apcadocno
               
         BEFORE ROW
            LET l_insert = FALSE
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET g_detail_idx = l_ac
            
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
         
            CALL s_transaction_begin()
            OPEN aapt300_06_cl USING g_apcf_d[l_ac].apcfseq,g_apcf_d[l_ac].apcfseq2
            IF STATUS THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "OPEN aapt300_06_cl:" 
               LET g_errparam.code   = STATUS 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
 
               CLOSE aapt300_06_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            
            LET g_rec_b = g_apcf_d.getLength()
            
            IF g_rec_b >= l_ac AND g_apcf_d[l_ac].apcfseq2 IS NOT NULL THEN
               LET l_cmd='u'
               LET g_apcf_d_t.* = g_apcf_d[l_ac].*  #BACKUP
               LET g_apcf_d_o.* = g_apcf_d[l_ac].*  #BACKUP
               CALL aapt300_06_set_entry_b1(l_cmd)
               CALL aapt300_06_set_no_entry_b1(l_cmd)
               FETCH aapt300_06_cl INTO g_apcf_d[l_ac].*
               
               IF SQLCA.sqlcode THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = g_apcf_d_t.apcfseq2 
                  LET g_errparam.code   = SQLCA.sqlcode 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  LET l_lock_sw = "Y"
               END IF
               SELECT apca001 INTO g_apcf_d[l_ac].apca001
                 FROM apca_t
                WHERE apcaent = g_enterprise
                  AND apcald  = g_apcald AND apcadocno = g_apcf_d[l_ac].apcf008
               LET g_apcf_d[l_ac].l_apcf021_desc = s_desc_get_account_desc(g_apcald,g_apcf_d[l_ac].apcf021)
            ELSE
               LET l_cmd='a'
            END IF


         BEFORE INSERT
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_apcf_d[l_ac].* TO NULL 
            INITIALIZE g_apcf_d_t.* TO NULL 
            INITIALIZE g_apcf_d_o.* TO NULL 
            LET g_apcf_d[l_ac].apcfdocno = g_apcadocno
            LET g_apcf_d[l_ac].apcfld    = g_apcald
            
            SELECT MAX(apcfseq2) + 1 INTO g_apcf_d[l_ac].apcfseq2
              FROM apcf_t
             WHERE apcfent = g_enterprise
               AND apcfld  = p_apcald
               AND apcfdocno = p_apcadocno
            IF cl_null(g_apcf_d[l_ac].apcfseq2)THEN LET g_apcf_d[l_ac].apcfseq2 = 1 END IF
            LET g_apcf_d[l_ac].apcf008 = 'DIFF'
            CALL cl_get_para(g_enterprise,g_glaa.glaacomp,'S-FIN-2011') RETURNING l_sfin2011
            IF l_sfin2011 = 'Y' THEN #不迴轉
               CALL s_fin_get_account(g_apcald,'13',l_apca.apca007,'8504_22') RETURNING g_sub_success,g_apcf_d[l_ac].apcf025,g_errno
            ELSE
               CALL s_fin_get_account(g_apcald,'13',l_apca.apca007,'8504_21') RETURNING g_sub_success,g_apcf_d[l_ac].apcf024,g_errno
            END IF
            LET g_apcf_d_t.* = g_apcf_d[l_ac].*
            LET g_apcf_d_o.* = g_apcf_d[l_ac].*
            
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
            LET l_cnt = 0
            SELECT count(*) INTO l_cnt
              FROM apcf_t
             WHERE apcfent = g_enterprise
               AND apcfld  = g_apcald AND apcfdocno = g_apcadocno
               AND apcf008 = g_apcf_d[l_ac].apcf008 AND apcf009 = g_apcf_d[l_ac].apcf009
               AND apcfseq <>g_apcf_d[l_ac].apcfseq AND apcfseq2<>g_apcf_d[l_ac].apcfseq2
            IF l_cnt = 0 THEN
               INSERT INTO apcf_t(apcfent,apcfld,apcforga,apcfdocno,apcfseq,apcfseq2,
                               apcf001,apcf002,apcf007,apcf008,apcf009,
                               apcf010,apcf021,apcf022,apcf023,
                               apcf101,apcf102,apcf103,apcf104,
                               apcf105,apcf113,apcf114,apcf115,
                               apcf026,apcf027,apcf028,apcf029,apcf030,
                               apcf031,apcf032,apcf033,apcf034,apcf035,
                               apcf036,apcf037,apcf038,apcf039,apcf040,
                               apcf041,apcf042,apcf043,apcf044,apcf045,
                               apcf046,apcf047,apcf048
                               )
                       VALUES(g_enterprise,g_apcald,g_apcf_d[l_ac].apcforga,g_apcadocno,g_apcf_d[l_ac].apcfseq,g_apcf_d[l_ac].apcfseq2,
                              'aapt300'   ,'01'     ,g_apcf_d[l_ac].apcf007,g_apcf_d[l_ac].apcf008,g_apcf_d[l_ac].apcf009,
                              0                     ,g_apcf_d[l_ac].apcf021,g_apcf_d[l_ac].apcf022,g_apcf_d[l_ac].apcf023,
                              g_apcf_d[l_ac].apcf101,g_apcf_d[l_ac].apcf102,g_apcf_d[l_ac].apcf103,g_apcf_d[l_ac].apcf104,
                              g_apcf_d[l_ac].apcf105,g_apcf_d[l_ac].apcf113,g_apcf_d[l_ac].apcf114,g_apcf_d[l_ac].apcf115,
                              g_apcf_d[l_ac].apcf026,g_apcf_d[l_ac].apcf027,g_apcf_d[l_ac].apcf028,g_apcf_d[l_ac].apcf029,g_apcf_d[l_ac].apcf030,
                              g_apcf_d[l_ac].apcf031,g_apcf_d[l_ac].apcf032,g_apcf_d[l_ac].apcf033,g_apcf_d[l_ac].apcf034,g_apcf_d[l_ac].apcf035,
                              g_apcf_d[l_ac].apcf036,g_apcf_d[l_ac].apcf037,g_apcf_d[l_ac].apcf038,g_apcf_d[l_ac].apcf039,g_apcf_d[l_ac].apcf040,
                              g_apcf_d[l_ac].apcf041,g_apcf_d[l_ac].apcf042,g_apcf_d[l_ac].apcf043,g_apcf_d[l_ac].apcf044,g_apcf_d[l_ac].apcf045,
                              g_apcf_d[l_ac].apcf046,g_apcf_d[l_ac].apcf047,g_apcf_d[l_ac].apcf048)
            ELSE
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = 'INSERT' 
               LET g_errparam.code   = "std-00006" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
 
               INITIALIZE g_apcf_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
            IF SQLCA.SQLcode  THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "apcf_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               CALL aapt300_06_b_fill1()
               CALL s_transaction_end('Y','0')
               LET g_rec_b = g_rec_b + 1
            END IF
            
         ON ROW CHANGE
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
 
               LET INT_FLAG = 0
               LET g_apcf_d[l_ac].* = g_apcf_d_t.*
               CLOSE aapt300_06_cl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF

            IF l_lock_sw = 'Y' THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = g_apcf_d[l_ac].apcfseq 
               LET g_errparam.code   = -263 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
