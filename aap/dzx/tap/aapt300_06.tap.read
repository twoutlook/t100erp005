PRIVATE FUNCTION aapt300_06_set_apcf008_detail()
DEFINE l_cmd         LIKE type_t.chr1
DEFINE l_apca        RECORD LIKE apca_t.*
DEFINE l_apcc        RECORD LIKE apcc_t.*
