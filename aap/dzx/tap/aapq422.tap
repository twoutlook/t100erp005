   LET ls_sql_rank = "SELECT UNIQUE '',apcedocno,apceld,0,apcbdocno,apcbseq,apceorga,'','','','', ",
                     "               '','','','','','','','','','','','','' ",
                     "  FROM ( ",
                     "SELECT UNIQUE apcedocno,apceld,apcbdocno,apcbseq,apceorga ",
                     "  FROM apda_t,apce_t,apcb_t ",
                     " WHERE apceent=? AND apdaent = apceent ",
                     "   AND apdald = apceld ",
                     "   AND apdadocno = apcedocno ",
                     "   AND apce001 = 'aapt420'",
                     "   AND apcbent = apceent ",
                     "   AND apcbld = apceld ",
                     "   AND apcbdocno = apce003 ",
                     "   AND ", ls_wc,
                     "       )"
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("apda_t,apce_t"),
                     " ORDER BY apce_t.apcedocno,apce_t.apceseq"]]>
  </point>
  <point name="b_fill.sql_after" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   LET g_sql = "SELECT '',apcedocno,apceld,0,apcbdocno,apcbseq,apceorga,'','','','','','','','', 
       '','','','','','','','',''",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page]]>
