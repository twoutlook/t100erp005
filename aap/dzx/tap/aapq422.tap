  <point name="b_fill.rank_sql_after" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   LET ls_wc = ls_wc," AND ",g_wc_1
   
   #LET ls_sql_rank = "SELECT  UNIQUE '',apcedocno,apceld,apceseq,apce003,'',apceorga,'','','','', 
   #                                  '','','','','','','','','','','','',''  ,
