  <section id="aapq422.b_fill" ver="5" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aapq422_b_fill()
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準
   {<point name="b_fill.define" edit="s"/>}
   #end add-point
   #add-point:b_fill段define-客製
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   LET ls_sql_rank = "SELECT  UNIQUE '',apcedocno,apceld,apceseq,apce003,apce004,apceorga,'','','','', 
       '','','','','','','','','','','','',''  ,DENSE_RANK() OVER( ORDER BY apce_t.apceseq) AS RANK FROM apce_t", 

 
 
                     "",
                     " WHERE apceent=? AND apceld=? AND apcedocno=? AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("apce_t"),
                     " ORDER BY apce_t.apceseq"
 
   #add-point:b_fill段rank_sql_after
   {<point name="b_fill.rank_sql_after"/>}
   #end add-point
 
   LET g_sql = "SELECT COUNT(*) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT '',apcedocno,apceld,apceseq,apce003,apce004,apceorga,'','','','','','','','', 
       '','','','','','','','',''",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE aapq422_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aapq422_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_apce_d.clear()
 
   #add-point:陣列清空
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1   
 
   FOREACH b_fill_curs INTO g_apce_d[l_ac].sel,g_apce_d[l_ac].apcedocno,g_apce_d[l_ac].apceld,g_apce_d[l_ac].apceseq, 
       g_apce_d[l_ac].apce003,g_apce_d[l_ac].apce004,g_apce_d[l_ac].apceorga,g_apce_d[l_ac].apceorga_desc, 
       g_apce_d[l_ac].apcb002,g_apce_d[l_ac].apcb003,g_apce_d[l_ac].apcb004,g_apce_d[l_ac].apcb004_desc, 
       g_apce_d[l_ac].stbe005,g_apce_d[l_ac].stbe005_desc,g_apce_d[l_ac].stbe006,g_apce_d[l_ac].stbe007, 
       g_apce_d[l_ac].stbe008,g_apce_d[l_ac].stbe008_desc,g_apce_d[l_ac].stbe009,g_apce_d[l_ac].stbe009_desc, 
       g_apce_d[l_ac].stbe011,g_apce_d[l_ac].stbe016,g_apce_d[l_ac].stbe020,g_apce_d[l_ac].stbe020_desc 

      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #LET g_apce_d[l_ac].statepic = cl_get_actipic(g_apce_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL aapq422_detail_show("'1'")      
 
      CALL aapq422_apce_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
   LET g_error_show = 0
   
 
   
   CALL g_apce_d.deleteElement(g_apce_d.getLength())   
 
   #add-point:陣列長度調整
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = g_apce_d.getLength()
#  DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE aapq422_pb
 
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL aapq422_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL aapq422_detail_action_trans()
 
   IF g_apce_d.getLength() > 0 THEN
      LET l_ac = 1
      CALL aapq422_fetch()
   END IF
   
      CALL aapq422_filter_show('apcedocno','b_apcedocno')
   CALL aapq422_filter_show('apceld','b_apceld')
   CALL aapq422_filter_show('apceseq','b_apceseq')
   CALL aapq422_filter_show('apce003','b_apce003')
   CALL aapq422_filter_show('apce004','b_apce004')
   CALL aapq422_filter_show('apceorga','b_apceorga')
   CALL aapq422_filter_show('apcb002','b_apcb002')
   CALL aapq422_filter_show('apcb003','b_apcb003')
   CALL aapq422_filter_show('apcb004','b_apcb004')
   CALL aapq422_filter_show('stbe005','b_stbe005')
   CALL aapq422_filter_show('stbe006','b_stbe006')
   CALL aapq422_filter_show('stbe007','b_stbe007')
   CALL aapq422_filter_show('stbe008','b_stbe008')
   CALL aapq422_filter_show('stbe009','b_stbe009')
   CALL aapq422_filter_show('stbe011','b_stbe011')
   CALL aapq422_filter_show('stbe016','b_stbe016')
   CALL aapq422_filter_show('stbe020','b_stbe020')
 
 
   #add-point:b_fill段結束前
   {<point name="b_fill.after"/>}
   #end add-point
 
END FUNCTION
]]>
  </section>
