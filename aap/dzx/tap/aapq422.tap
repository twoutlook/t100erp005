   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_aapq422 WITH FORM cl_ap_formpath("aap",g_code)
   
      #瀏覽頁簽資料初始化
