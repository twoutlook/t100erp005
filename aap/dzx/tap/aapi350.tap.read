            #add-point:cs段before_construct
            {<point name="cs.head.before_construct"/>}
            #end add-point 
            
         #公用欄位開窗相關處理
         #此段落由子樣板a11產生
         ##----<<apaccrtdt>>----
         AFTER FIELD apaccrtdt
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
         
         #----<<apacmoddt>>----
         AFTER FIELD apacmoddt
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
         
         #----<<apaccnfdt>>----
         
         #----<<apacpstdt>>----
 
 
            
         #一般欄位開窗相關處理    
                  #Ctrlp:construct.c.apac001
         ON ACTION controlp INFIELD apac001
            #add-point:ON ACTION controlp INFIELD apac001
            {<point name="construct.c.apac001" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD apac001
            #add-point:BEFORE FIELD apac001
            {<point name="construct.b.apac001" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD apac001
            
            #add-point:AFTER FIELD apac001
            {<point name="construct.a.apac001" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD apacl003
            #add-point:BEFORE FIELD apacl003
            {<point name="construct.b.apacl003" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD apacl003
            
            #add-point:AFTER FIELD apacl003
            {<point name="construct.a.apacl003" />}
            #END add-point
            
 
         #Ctrlp:construct.c.apacl003
         ON ACTION controlp INFIELD apacl003
            #add-point:ON ACTION controlp INFIELD apacl003
            {<point name="construct.c.apacl003" />}
            #END add-point
 
