  <point name="function.aapi350_unactive_upd" order="5" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 狀態確認有效改無效檢核
# Memo...........:
# Usage..........: aapi350_unactive_upd()
################################################################################
PUBLIC FUNCTION aapi350_unactive_upd()
   
   DEFINE r_success BOOLEAN
      LET r_success = TRUE
   
   UPDATE apad_t
      SET apad_t.apad005 = 'N'
    WHERE apad_t.apadent = g_enterprise AND apad_t.apad001 = g_apac_m.apac001
    
    IF SQLCA.sqlcode THEN
      CALL cl_err('',SQLCA.sqlcode,1) 
      LET r_success = FALSE
      RETURN r_success   
    END IF      
    
     UPDATE apad_t
      SET apad_t.apad006 = g_today
    WHERE apad_t.apadent = g_enterprise AND apad_t.apad001 = g_apac_m.apac001 
      AND apad_t.apad006 IS NULL
   
   IF SQLCA.sqlcode THEN
      CALL cl_err('',SQLCA.sqlcode,1) 
      LET r_success = FALSE      
      RETURN r_success   
   END IF
   RETURN r_success
        
END FUNCTION]]>
