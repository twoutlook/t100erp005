  <point name="function.aapi350_active_upd" order="7" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 狀態確認無效改有效檢核
# Memo...........:
# Usage..........: aapi350_active_upd()
################################################################################
PUBLIC FUNCTION aapi350_active_upd()
  
   DEFINE clist       RECORD
          b_apad002   LIKE apad_t.apad002
      END RECORD
   DEFINE l_sql       STRING
   DEFINE l_sql2      STRING 
   DEFINE l_i         INTEGER
   DEFINE l_nmasstus  LIKE type_t.num5
   DEFINE r_success   BOOLEAN
   
   LET r_success = TRUE
   LET l_sql = "SELECT apad002",
               " FROM apad_t LEFT JOIN apac_t ON apad001 = apac001 AND apadent = apacent",
               " WHERE apac001 = '",g_apac_m.apac001,"' AND apacent = '",g_enterprise,"' "
               
   PREPARE aap_pre FROM l_sql
   DECLARE aap_cur CURSOR FOR aap_pre
   
   LET l_i = 1
   FOREACH aap_cur INTO clist.b_apad002  
      IF SQLCA.sqlcode THEN
         CALL cl_err('',SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF  
      
      SELECT nmaastus INTO l_nmasstus
        FROM apad_t,nmas_t,nmaa_t
       WHERE apadent = nmasent
         AND apad002 = nmas002
         AND nmas001 = nmaa001
         AND nmasent = nmaaent
         AND apadent = g_enterprise
         AND apad002 = clist.b_apad002  
 
      IF SQLCA.sqlcode = 100 THEN
         CALL cl_err('',SQLCA.sqlcode,1)
         LET r_success = FALSE      
         RETURN r_success 
      END IF       

      IF NOT l_nmasstus = 'Y' THEN
         CALL cl_err('','apm-00332',1)
         LET r_success = FALSE
         RETURN r_success                           
      END IF                 
   END FOREACH
   RETURN r_success
   
END FUNCTION]]>
  </point>
