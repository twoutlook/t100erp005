  <section id="aapi350.main" ver="18" status="" src="s">
    <![CDATA[#+ 此段落由子樣板a26產生
#OPTIONS SHORT CIRCUIT
#+ 作業開始 
MAIN
   #add-point:main段define
   {<point name="main.define"/>}
   #end add-point   
 
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("aap","")
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT apac001,'',apacstus,apac002,apac007,apac006,apacsite,'',apac003,'',apac004, 
       '',apac005,'',apacownid,'',apacowndp,'',apaccrtid,'',apaccrtdp,'',apaccrtdt,apacmodid,'',apacmoddt", 
        
                      " FROM apac_t",
                      " WHERE apacent= ? AND apac001=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE aapi350_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE apac001,apacstus,apac002,apac007,apac006,apacsite,apac003,apac004,apac005, 
       apacownid,apacowndp,apaccrtid,apaccrtdp,apaccrtdt,apacmodid,apacmoddt,t1.ooag011 ,t2.ooefl003 , 
       t3.ooag011 ,t4.ooefl003 ,t5.ooag011",
               " FROM apac_t",
                              " LEFT JOIN ooag_t t1 ON t1.ooagent='"||g_enterprise||"' AND t1.ooag001=apacownid  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent='"||g_enterprise||"' AND t2.ooefl001=apacowndp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent='"||g_enterprise||"' AND t3.ooag001=apaccrtid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=apaccrtdp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent='"||g_enterprise||"' AND t5.ooag001=apacmodid  ",
 
               " WHERE apacent = '" ||g_enterprise|| "' AND apac001 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE aapi350_master_referesh FROM g_sql
 
 
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_aapi350 WITH FORM cl_ap_formpath("aap",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL aapi350_init()   
 
      #進入選單 Menu (="N")
      CALL aapi350_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_aapi350
      
   END IF 
   
   CLOSE aapi350_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
   
END MAIN
 
 
]]>
  </section>
  <section id="aapi350.modify" ver="14" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aapi350_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_apac_m.apac001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   ERROR ""
  
   LET g_apac001_t = g_apac_m.apac001
 
   CALL s_transaction_begin()
   
   OPEN aapi350_cl USING g_enterprise,g_apac_m.apac001
   IF STATUS THEN
      CALL cl_err("OPEN aapi350_cl:", STATUS, 1)
      CLOSE aapi350_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aapi350_master_referesh USING g_apac_m.apac001 INTO g_apac_m.apac001,g_apac_m.apacstus,g_apac_m.apac002, 
       g_apac_m.apac007,g_apac_m.apac006,g_apac_m.apacsite,g_apac_m.apac003,g_apac_m.apac004,g_apac_m.apac005, 
       g_apac_m.apacownid,g_apac_m.apacowndp,g_apac_m.apaccrtid,g_apac_m.apaccrtdp,g_apac_m.apaccrtdt, 
       g_apac_m.apacmodid,g_apac_m.apacmoddt,g_apac_m.apacownid_desc,g_apac_m.apacowndp_desc,g_apac_m.apaccrtid_desc, 
       g_apac_m.apaccrtdp_desc,g_apac_m.apacmodid_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_apac_m.apac001,SQLCA.sqlcode,0)
      CLOSE aapi350_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   CALL aapi350_show()
   WHILE TRUE
      LET g_apac001_t = g_apac_m.apac001
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_apac_m.apacmodid = g_user 
LET g_apac_m.apacmoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      CALL aapi350_input("u")
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_apac_m.* = g_apac_m_t.*
         CALL aapi350_show()
         CALL cl_err('',9001,0)
         CALL s_transaction_end('N','0')
         EXIT WHILE
      END IF 
      
      #若有modid跟moddt則進行update
      UPDATE apac_t SET (apacmodid,apacmoddt) = (g_apac_m.apacmodid,g_apac_m.apacmoddt)
       WHERE apacent = g_enterprise AND apac001 = g_apac001_t
 
                  
      #若單頭key欄位有變更
      IF g_apac_m.apac001 != g_apac001_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE apad_t SET apad001 = g_apac_m.apac001
 
          WHERE apadent = g_enterprise AND apad001 = g_apac001_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("apad_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("apad_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_apac_m.apac001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE aapi350_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_apac_m.apac001,'U')
 
END FUNCTION   
]]>
  </section>
