   #add-point:cs段開始前
   {<point name="cs.before_construct"/>}
   #end add-point 
   
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
      
      #單頭
      CONSTRUCT BY NAME g_wc ON apac001,apacl003,apacstus,apac002,apac007,apac006,apacsite,apacsite_desc, 
          apac003,apac003_desc,apac004,apac004_desc,apac005,apac005_desc,apacownid,apacowndp,apaccrtid, 
          apaccrtdp,apaccrtdt,apacmodid,apacmoddt
 
