  <point name="rep.a_group.l_order.before" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[           #aapt110小計
           #收款單
           LET g_sql = "     SELECT apbb051,apeadocdt,apeadocno,'',apeb100,0,",  
                  "                 SUM(apeb109) ",
                  "            FROM apea_t,apeb_t ",
                  "            LEFT JOIN apbb_t ON apbbent = apebent AND apbbdocno = apeb003 ",
                  "           WHERE apeaent = apebent ",
                  "             AND apeadocno = apebdocno ",
                  "             AND apeaent = ",g_enterprise,
                  "             AND apbbcomp = '",sr1.apbbcomp,"' ",
                  "             AND apbb002 = '",sr1.apbb002,"' ",
                  "             AND ",tm.wc CLIPPED,
                  "             AND apeastus = 'Y' ",
                  "             AND apeadocdt BETWEEN '",tm.a2,"' AND '",tm.a3,"' ",
                  "           GROUP BY apeadocdt,apeadocno,apeb100,apbb051 "
 
           LET l_cnt = 0
           LET l_sub_sql = ""
           LET l_subrep06_show ="N"
           LET l_sub_sql = "SELECT COUNT(1) FROM (",g_sql,")"
           PREPARE aapr110_g01_repcur06_cnt_pre FROM l_sub_sql
           EXECUTE aapr110_g01_repcur06_cnt_pre INTO l_cnt
           IF l_cnt > 0 THEN
              LET l_subrep06_show ="Y"
           END IF
     
           PRINTX l_subrep06_show
           START REPORT aapr110_g01_subrep06
           DECLARE aapr110_g01_repcur06 CURSOR FROM g_sql
           FOREACH aapr110_g01_repcur06 INTO sr6.*
              IF STATUS THEN
                 INITIALIZE g_errparam TO NULL
                 LET g_errparam.extend = "aapr110_g01_repcur06:"
                 LET g_errparam.code   = SQLCA.sqlcode
                 LET g_errparam.popup  = FALSE
                 CALL cl_err()
                 EXIT FOREACH
              END IF
              #收款核銷
              LET sr6.isag001 = s_desc_gzcbl004_desc('9743','A') CLIPPED
              #人員名稱
              IF NOT cl_null(sr6.l_apbb051) THEN
                 LET sr6.l_apbb051 = sr6.l_apbb051 CLIPPED,".",s_desc_get_person_desc(sr6.l_apbb051) CLIPPED
              END IF

              OUTPUT TO REPORT aapr110_g01_subrep06(sr6.*)
              END FOREACH
           FINISH REPORT aapr110_g01_subrep06
           
           #期間合計
           LET g_sql = "SELECT '',SUM(A),SUM(B),apbb014",
                       "   FROM (",
                        "          SELECT '1' AS C,apbb051,apbbdocdt,apbbdocno,apbb014,SUM(apba105*apba012) AS A,",
                        "                 0 AS B,apbbent,apbb004,apbbcomp,apbb002,",
                        "                 apba004 AS isag001 ",
                        "            FROM apbb_t ",
                        "            LEFT JOIN apba_t ON apbaent = apbbent AND apbadocno = apbbdocno ",
                        "           WHERE apbbent=",g_enterprise,
                        "             AND apbbstus ='Y'",
                       "              AND apbbcomp = '",sr1.apbbcomp,"'",
                       "              AND apbb002 = '",sr1.apbb002,"'",
                        "             AND ",tm.wc CLIPPED,
                        "             AND apbbdocdt BETWEEN '",tm.a2,"' AND '",tm.a3,"'",
                        "           GROUP BY apbb051,apbbdocdt,apbbdocno,apbb014,apbbent,apbb004,apbbcomp,apbb002,apba004 ",
                        "          UNION ",
                        "          SELECT '2',apbb051,apeadocdt,apeadocno,apeb100,0,",
                        "                 SUM(apeb109),apeaent,apeasite,apbbcomp,apbb002,",
                        "                 apeb012 ",
                        "            FROM apea_t,apeb_t ",
                        "                        LEFT JOIN apbb_t ON apbbent = apebent AND apbbdocno = apeb003 ",
                        "           WHERE apeaent = apebent ",
                        "             AND apeadocno = apebdocno ",
                        "             AND apeaent = ",g_enterprise,
                        "             AND apbbcomp = '",sr1.apbbcomp,"'",
                        "             AND apbb002 = '",sr1.apbb002,"'",
                        "             AND ",tm.wc CLIPPED,
                        "             AND apeadocdt BETWEEN '",tm.a2,"' AND '",tm.a3,"' ",
                        "             AND apeastus = 'Y' ",
                        "           GROUP BY apbb051,apeadocdt,apeadocno,apeb100,apeaent,apeasite,apbbcomp,apbb002,apeb012",
                        "        ) ",
                        " GROUP BY apbb014 "

           LET l_cnt = 0
           LET l_sub_sql = ""
           LET l_subrep10_show ="N"
           LET l_sub_sql = "SELECT COUNT(1) FROM (",g_sql,")"
           PREPARE aapr110_g01_repcur07_cnt_pre FROM l_sub_sql
           EXECUTE aapr110_g01_repcur07_cnt_pre INTO l_cnt
           IF l_cnt > 0 THEN
              LET l_subrep07_show ="Y"
           END IF
           PRINTX l_subrep07_show
           START REPORT aapr110_g01_subrep07
           DECLARE aapr110_g01_repcur07 CURSOR FROM g_sql
           LET l_flag = 1
           FOREACH aapr110_g01_repcur07 INTO sr7.*
              IF STATUS THEN
                 INITIALIZE g_errparam TO NULL
                 LET g_errparam.extend = "aapr110_g01_repcur07:"
                 LET g_errparam.code   = SQLCA.sqlcode
                 LET g_errparam.popup  = FALSE
                 CALL cl_err()
                 EXIT FOREACH
              END IF
              #合計名稱
              IF l_flag = 1 THEN
                 CALL s_azzi902_get_gzzd('aapq930','sum01') RETURNING sr7.isag001,l_comment
              ELSE
                 LET sr7.isag001 = ''
              END IF
              LET l_flag = l_flag + 1
              OUTPUT TO REPORT aapr110_g01_subrep07(sr7.*)
              END FOREACH
           FINISH REPORT aapr110_g01_subrep07
           
           #本期應收款
           LET g_sql = "SELECT '',SUM(A)-SUM(B),apbb014",
                        "   FROM (",
                        "          SELECT '1' AS C,apbb051,apbbdocdt,apbbdocno,apbb014,SUM(apba105*apba012) AS A,",
                        "                 0 AS B,apbbent,apbb004,apbbcomp,apbb002,", 
                        "                 apba004 AS isag001 ",
                        "            FROM apbb_t ",
                        "            LEFT JOIN apba_t ON apbaent = apbbent AND apbadocno = apbbdocno ",
                        "           WHERE apbbent=",g_enterprise,
                        "             AND apbbstus ='Y'",
                        "              AND apbbcomp = '",sr1.apbbcomp,"'",
                        "              AND apbb002 = '",sr1.apbb002,"'",
                        "             AND apbbdocdt <= '",tm.a3,"'",
                        "             AND ",tm.wc CLIPPED,
                        "           GROUP BY apbb051,apbbdocdt,apbbdocno,apbb014,apbbent,apbb004,apbbcomp,apbb002,apba004 ",                  
                        "          UNION ",
                        "          SELECT '2',apbb051,apeadocdt,apeadocno,apeb100,0,",
                        "                 SUM(apeb109),apeaent,apeasite,apbbcomp,apbb002,",
                        "                 apeb012 ",
                        "            FROM apea_t,apeb_t ",
                        "              LEFT JOIN apbb_t ON apbbent = apebent AND apbbdocno = apeb003 ",
                        "           WHERE apeaent = apebent ",
                        "             AND apeadocno = apebdocno ",
                        "             AND apeaent = ",g_enterprise,
                        "             AND apbbcomp = '",sr1.apbbcomp,"'",
                        "             AND apbb002 = '",sr1.apbb002,"'",
                        "             AND apeadocdt <= '",tm.a3,"' ",
                        "             AND apeastus = 'Y' ",
                        "             AND ",tm.wc CLIPPED,
                        "           GROUP BY apbb051,apeadocdt,apeadocno,apeb100,apeaent,apeasite,apbbcomp,apbb002,apeb012",                  
                        "        ) ",
                        " GROUP BY apbb014 "
           
           LET l_cnt = 0
           LET l_sub_sql = ""
           LET l_subrep08_show ="N"
           LET l_sub_sql = "SELECT COUNT(1) FROM (",g_sql,")"
           PREPARE aapr110_g01_repcur08_cnt_pre FROM l_sub_sql
           EXECUTE aapr110_g01_repcur08_cnt_pre INTO l_cnt
           IF l_cnt > 0 THEN
              LET l_subrep08_show ="Y"
           END IF
           PRINTX l_subrep08_show
           START REPORT aapr110_g01_subrep08
           DECLARE aapr110_g01_repcur08 CURSOR FROM g_sql
           LET l_flag = 1
           FOREACH aapr110_g01_repcur08 INTO sr8.*
              IF STATUS THEN
                 INITIALIZE g_errparam TO NULL
                 LET g_errparam.extend = "aapr110_g01_repcur08:"
                 LET g_errparam.code   = SQLCA.sqlcode
                 LET g_errparam.popup  = FALSE
                 CALL cl_err()
                 EXIT FOREACH
              END IF
              #本期應收款名稱
              IF l_flag = 1 THEN
                 #CALL s_azzi902_get_gzzd('aisr350','lbl_isag001') RETURNING sr8.isag001,l_comment
                 CALL cl_getmsg('aap-00329',g_lang) RETURNING sr8.isag001
              ELSE
                 LET sr8.isag001 = ''
              END IF
              LET l_flag = l_flag + 1
              OUTPUT TO REPORT aapr110_g01_subrep08(sr8.*)
              END FOREACH
           FINISH REPORT aapr110_g01_subrep08
           
           #備註
           LET sr1.l_memo = cl_getmsg("ais-00181",g_lang) CLIPPED,"\r\n"
           LET sr1.l_memo = sr1.l_memo,cl_getmsg("ais-00182",g_lang) CLIPPED,"\r\n\r\n"
           LET sr1.l_memo = sr1.l_memo,cl_getmsg("ais-00184",g_lang) CLIPPED,"\r\n\r\n"
           LET sr1.l_memo = sr1.l_memo,cl_getmsg("ais-00185",g_lang) CLIPPED
           PRINTX sr1.l_memo
]]>
  </point>
