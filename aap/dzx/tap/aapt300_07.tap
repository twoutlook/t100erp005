  <section id="aapt300_07.modify" ver="1" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aapt300_07_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point
   
   IF g_apca_m.apcald IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF 
   
    SELECT UNIQUE apcadocno,apcald,apca008,apca009,apca010,apca054
 INTO g_apca_m.apcadocno,g_apca_m.apcald,g_apca_m.apca008,g_apca_m.apca009,g_apca_m.apca010,g_apca_m.apca054 

 FROM apca_t
 WHERE apcaent = g_enterprise AND apcald = g_apca_m.apcald AND apcadocno = g_apca_m.apcadocno
 
   ERROR ""
  
   LET g_apcald_t = g_apca_m.apcald
   LET g_apcadocno_t = g_apca_m.apcadocno
 
 
   
   CALL s_transaction_begin()
   
   OPEN aapt300_07_cl USING g_enterprise,g_apca_m.apcald,g_apca_m.apcadocno
   IF STATUS THEN
      CALL cl_err("OPEN aapt300_07_cl:", STATUS, 1)
      CLOSE aapt300_07_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aapt300_07_cl INTO g_apca_m.apcadocno,g_apca_m.apcald,g_apca_m.apca008,g_apca_m.apca008_desc, 
       g_apca_m.apca009,g_apca_m.apca010,g_apca_m.apca054,g_apca_m.apca054_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err("apca_t",SQLCA.sqlcode,0)
      CLOSE aapt300_07_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
 
   CALL aapt300_07_show()
   
   WHILE TRUE
      LET g_apca_m.apcald = g_apcald_t
      LET g_apca_m.apcadocno = g_apcadocno_t
 
 
      
      #寫入修改者/修改日期資訊
      
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
 
      CALL aapt300_07_input("u")     #欄位更改
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_apca_m.* = g_apca_m_t.*
         CALL aapt300_07_show()
         CALL cl_err("",9001,0)
         EXIT WHILE
      END IF
 
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_apca_m.apcald,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE aapt300_07_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_apca_m.apcald,"U")
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
