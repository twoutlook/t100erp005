  <point name="function.aapt310_source_doc_chk" order="9" ver="5" cite_std="N" new="Y" ch="" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 採購單參考單號+序次檢核
# Memo...........: 修正的地方日後要搬入s_aapt300_source_doc_chk
# Date & Author..: 141021 By albireo
# Modify.........:
################################################################################
PRIVATE FUNCTION aapt310_source_doc_chk(p_apcacomp,p_apcasite,p_cust,p_prog,p_docno,p_seq,p_docdt,p_apcald)
DEFINE p_apcacomp   LIKE apca_t.apcacomp
DEFINE p_apcasite   LIKE apca_t.apcasite
DEFINE p_cust       LIKE pmaa_t.pmaa001
DEFINE p_prog       LIKE oocq_t.oocq002
DEFINE p_docno      LIKE type_t.chr50
DEFINE p_seq        LIKE type_t.num10
DEFINE l_stus       LIKE type_t.chr1      #單據狀態
DEFINE l_cnt        LIKE type_t.num10
DEFINE l_site       LIKE ooef_t.ooef001   #單據營運據點
DEFINE r_success    LIKE type_t.num5
DEFINE r_errno      LIKE gzze_t.gzze001
DEFINE l_cust       LIKE pmaa_t.pmaa001   #來源單據交易對象
DEFINE p_docdt      LIKE type_t.dat       #日期         單據日期 
DEFINE l_docdt      LIKE type_t.dat       #來源單據日期   
DEFINE l_pmds000    LIKE pmds_t.pmds000  
#DEFINE l_pmds084    LIKE pmds_t.pmds084     #用於排除多角貿易    採購還沒開欄位
DEFINE p_apcald    LIKE type_t.chr1          #用於主帳套 第二 第三帳套的判定
DEFINE l_pmdt       RECORD
                    pmdt024   LIKE pmdt_t.pmdt024,
                    pmdt056   LIKE pmdt_t.pmdt056,
                    pmdt057   LIKE pmdt_t.pmdt057,
                    pmdt058   LIKE pmdt_t.pmdt058
                    END RECORD
DEFINE l_s_fin_0001 LIKE glaa_t.glaald   
DEFINE l_s_fin_0002 LIKE glaa_t.glaald    
DEFINE l_main_ld    LIKE glaa_t.glaald   
DEFINE l_pmdm       RECORD    
                    pmdm005   LIKE pmdm_t.pmdm005,
                    pmdm008   LIKE pmdm_t.pmdm008,
                    pmdm010   LIKE pmdm_t.pmdm010,
                    pmdm012   LIKE pmdm_t.pmdm012
                    END RECORD
DEFINE l_sql        STRING

   LET r_success = TRUE
   LET r_errno   = ''
   IF cl_null(p_apcacomp) OR cl_null(p_apcasite) 
      OR cl_null(p_cust) OR cl_null(p_prog)
      OR cl_null(p_docno) OR cl_null(p_seq) THEN
      RETURN r_success, r_errno
   END IF
   
 
   #抓取主副一副二帳套
   LET l_s_fin_0001 = NULL
   LET l_s_fin_0002 = NULL
   LET l_main_ld    = NULL
   SELECT glaald INTO l_main_ld FROM glaa_t
    WHERE glaaent  = g_enterprise
      AND glaacomp = p_apcacomp

   CALL cl_get_para(g_enterprise,p_apcacomp,'S-FIN-0001') RETURNING l_s_fin_0001
   CALL cl_get_para(g_enterprise,p_apcacomp,'S-FIN-0002') RETURNING l_s_fin_0002   
   
   CASE
      WHEN p_prog = '10'   #'apmt500'    #採購單
         #檢查pmdl
         #檢查單號存在否
         LET l_stus = NULL
         LET l_site = NULL
         LET l_cust = NULL
         LET l_docdt = NULL
         SELECT pmdlstus,pmdlsite,pmdl021,pmdldocdt
           INTO l_stus,l_site,l_cust,l_docdt
           FROM pmdl_t
          WHERE pmdlent   = g_enterprise
            AND pmdldocno = p_docno
         CASE
            WHEN SQLCA.SQLCODE = 100
               LET r_errno = 'aap-00051'#無此單據
               LET r_success = FALSE
            WHEN l_stus <> 'Y'          #不為已確認
               LET r_errno = 'aap-00074'
               LET r_success = FALSE
            WHEN NOT cl_null(l_site)    #檢查單據營運中心是否為法人及帳務中心管轄
               CALL s_fin_apcborga_chk(p_apcasite,p_apcacomp,l_site,p_docdt,'3')
                   RETURNING r_success,r_errno
            WHEN NOT cl_null(p_cust)                  
               IF l_cust <> p_cust THEN
                  LET r_errno = 'aap-00067'
                  LET r_success = FALSE              
               END IF 
            OTHERWISE
         END CASE         
         
         IF NOT cl_null(r_errno)THEN RETURN r_success,r_errno END IF

         #單身最重要的是金額平衡
         INITIALIZE l_pmdm.* TO NULL
         ##################################################################################################
         #SELECT SUM(pmdm005),SUM(pmdm008),SUM(pmdm010),SUM(pmdm012)      
         #  INTO l_pmdm.*
         #  FROM pmdm_t
         # WHERE pmdment = g_enterprise
         #   AND pmdmdocno = p_docno
         #這段修正一下配合序次可輸入
         IF cl_null(p_seq)THEN
            #沒輸入序次用SUM
            LET l_sql = " SELECT SUM(pmdm005),SUM(pmdm008),SUM(pmdm010),SUM(pmdm012) ",
                        "   FROM pmdm_t ",
                        "  WHERE pmdment = ",g_enterprise," ",
                        "    AND pmdmdocno ='",p_docno,"' "
         ELSE
            #有輸入序次用單筆
            LET l_sql = " SELECT pmdm005,pmdm008,pmdm010,pmdm012 ",
                        "   FROM pmdm_t ",
                        "  WHERE pmdment = ",g_enterprise," ",
                        "    AND pmdmdocno ='",p_docno,"' ",
                        "    AND pmdm001   = ",p_seq," "
         END IF
         PREPARE s_aapt300_selpmdmp FROM l_sql
         EXECUTE s_aapt300_selpmdmp INTO l_pmdm.*         
         ##################################################################################################
         IF cl_null(l_pmdm.pmdm005)THEN LET l_pmdm.pmdm005 = 0 END IF
         IF cl_null(l_pmdm.pmdm008)THEN LET l_pmdm.pmdm008 = 0 END IF
         IF cl_null(l_pmdm.pmdm010)THEN LET l_pmdm.pmdm010 = 0 END IF
         IF cl_null(l_pmdm.pmdm012)THEN LET l_pmdm.pmdm012 = 0 END IF              

         CASE
            WHEN SQLCA.SQLCODE = 100
               LET r_success = FALSE
               LET r_errno = 'aap-00068'
            WHEN p_apcald = l_main_ld AND l_pmdm.pmdm005 <= l_pmdm.pmdm008       #主帳套時 金額<=立帳金額時錯 
               LET r_success = FALSE
               LET r_errno = 'aap-00069'
            WHEN p_apcald = l_s_fin_0001 AND l_pmdm.pmdm005 <= l_pmdm.pmdm010    #副一帳套 金額<=立帳金額時錯
               LET r_success = FALSE
               LET r_errno = 'aap-00070'
            WHEN p_apcald = l_s_fin_0002 AND l_pmdm.pmdm005 <= l_pmdm.pmdm012    #副二帳套 計價金額<=立帳金額時錯
               LET r_success = FALSE
               LET r_errno = 'aap-00071'
            OTHERWISE
         END CASE

      OTHERWISE
   END CASE
   RETURN r_success,r_errno
END FUNCTION]]>
  </point>
