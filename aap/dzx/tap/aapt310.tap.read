            chr    LIKE type_t.chr500,
            dat    LIKE type_t.dat
               END RECORD
   
   IF NOT g_apca_m.apca001 = '13' THEN LET p_flag = 'N' END IF
   LET l_cnt = 0
   #來源單據取值
   CALL l_source.clear()
   CALL s_aapt300_source_doc_carry_b(g_apcb_d[p_ac].apcb001,g_apcb_d[p_ac].apcb002,g_apcb_d[p_ac].apcb003,g_apca_m.apcadocno,g_apca_m.apcald)
