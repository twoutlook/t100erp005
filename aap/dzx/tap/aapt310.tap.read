################################################################################
PRIVATE FUNCTION aapt310_get_source_b(p_ac,p_flag)
DEFINE p_ac        LIKE type_t.num5
DEFINE p_flag      LIKE apcb_t.apcb023
DEFINE l_apca001   LIKE apca_t.apca001
DEFINE l_oodb004   LIKE oodb_t.oodb004  #稅別名稱
