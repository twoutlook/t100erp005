   LET g_apcald_t = g_apca_m.apcald
   LET g_apcadocno_t = g_apca_m.apcadocno
 
   CALL s_transaction_begin()
   
   OPEN aapt310_cl USING g_enterprise,g_apca_m.apcald,g_apca_m.apcadocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aapt310_cl:" 
