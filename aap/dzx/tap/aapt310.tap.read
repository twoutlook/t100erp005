       g_apca_m.apcacrtid,g_apca_m.apcacrtid_desc,g_apca_m.apcacrtdp,g_apca_m.apcacrtdp_desc,g_apca_m.apcacrtdt, 
       g_apca_m.apcamodid,g_apca_m.apcamodid_desc,g_apca_m.apcamoddt,g_apca_m.apcacnfid,g_apca_m.apcacnfid_desc, 
