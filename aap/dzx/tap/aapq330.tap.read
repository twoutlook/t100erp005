#單身 type 宣告
PRIVATE TYPE type_g_apca_d RECORD
       
       sel LIKE type_t.chr500, 
   apca004 LIKE type_t.chr80, 
   txt LIKE type_t.chr80, 
   val LIKE type_t.chr80, 
   val2 LIKE type_t.chr80, 
   apca100 LIKE apca_t.apca100, 
