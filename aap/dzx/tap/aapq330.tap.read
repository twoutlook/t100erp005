  <section id="aapq330.filter" ver="28" status="" src="s">
    <![CDATA[#+ 此段落由子樣板qs13產生
#+ filter段相關程式段
#+ filter過濾功能
PRIVATE FUNCTION aapq330_filter()
   {<Local define>}
   {</Local define>}
   #add-point:filter段define
   {<point name="filter.define"/>}
   #end add-point
 
   LET INT_FLAG = 0
 
   LET g_qryparam.state = 'c'
   LET g_detail_idx  = 1
   LET g_detail_idx2 = 1
 
   LET g_wc_filter_t = g_wc_filter
   LET g_wc_t = g_wc
 
   CALL gfrm_curr.setFieldHidden("formonly.sel", TRUE)
   CALL gfrm_curr.setFieldHidden("formonly.b_statepic", TRUE)
 
   
 
   LET g_wc = cl_replace_str(g_wc, g_wc_filter, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   #+ 此段落由子樣板qs08產生
   #+ filter段DIALOG段的組成
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON apca100,apcc105,apcc133,apcc134,apcc135,apcc136,apcc137,apcc118,apcc138, 
          apcc115,apcc139
                          FROM s_detail1[1].b_apca100,s_detail1[1].b_apcc105,s_detail1[1].b_apcc133, 
                              s_detail1[1].b_apcc134,s_detail1[1].b_apcc135,s_detail1[1].b_apcc136,s_detail1[1].b_apcc137, 
                              s_detail1[1].b_apcc118,s_detail1[1].b_apcc138,s_detail1[1].b_apcc115,s_detail1[1].b_apcc139 

 
         BEFORE CONSTRUCT
            CALL cl_qbe_init()
                     DISPLAY aapq330_filter_parser('apca100') TO s_detail1[1].b_apca100
            DISPLAY aapq330_filter_parser('apcc105') TO s_detail1[1].b_apcc105
            DISPLAY aapq330_filter_parser('apcc133') TO s_detail1[1].b_apcc133
            DISPLAY aapq330_filter_parser('apcc134') TO s_detail1[1].b_apcc134
            DISPLAY aapq330_filter_parser('apcc135') TO s_detail1[1].b_apcc135
            DISPLAY aapq330_filter_parser('apcc136') TO s_detail1[1].b_apcc136
            DISPLAY aapq330_filter_parser('apcc137') TO s_detail1[1].b_apcc137
            DISPLAY aapq330_filter_parser('apcc118') TO s_detail1[1].b_apcc118
            DISPLAY aapq330_filter_parser('apcc138') TO s_detail1[1].b_apcc138
            DISPLAY aapq330_filter_parser('apcc115') TO s_detail1[1].b_apcc115
            DISPLAY aapq330_filter_parser('apcc139') TO s_detail1[1].b_apcc139
 
 
      END CONSTRUCT
 
      #add-point:filter段add_cs
      {<point name="filter.add_cs"/>}
      #end add-point
 
      BEFORE DIALOG
         #add-point:filter段b_dialog
         {<point name="filter.b_dialog"/>}
         #end add-point
 
      ON ACTION accept
         ACCEPT DIALOG
 
      ON ACTION cancel
         LET INT_FLAG = 1
         EXIT DIALOG
 
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE DIALOG
 
   END DIALOG
 
 
 
   
 
   IF NOT INT_FLAG THEN
      LET g_wc_filter = g_wc_filter, " "
   ELSE
      LET g_wc_filter = g_wc_filter_t
   END IF
 
      CALL aapq330_filter_show('apca100','b_apca100')
