  <section id="aapq330.b_fill" ver="50" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aapq330_b_fill()
   {<Local define>}
   DEFINE ls_wc           STRING
   DEFINE l_pid           LIKE type_t.chr50
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_detail_idx  = 1
   LET g_detail_idx2 = 1
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
 
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
 
   CALL g_apca_d.clear()
 
   #add-point:陣列清空
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   LET l_ac = 1
   ERROR "Searching!"
 
   # b_fill段sql組成及FOREACH撰寫
   #+ 此段落由子樣板qs04產生
   #+ b_fill段資料取得(包含sql組成及FOREACH段撰寫)
   LET g_sql = "SELECT  UNIQUE '','','','','',apca100,'','','','','','','','','','' FROM apca_t",
 
 
               "",
               " WHERE apcaent= ? AND 1=1 AND ", ls_wc
 
   LET g_sql = g_sql, " ORDER BY apca_t.apcald,apca_t.apcadocno"
 
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   PREPARE aapq330_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aapq330_pb
 
   OPEN b_fill_curs USING g_enterprise
 
   FOREACH b_fill_curs INTO g_apca_d[l_ac].sel,g_apca_d[l_ac].apca004,g_apca_d[l_ac].txt,g_apca_d[l_ac].val, 
       g_apca_d[l_ac].val2,g_apca_d[l_ac].apca100,g_apca_d[l_ac].apcc105,g_apca_d[l_ac].apcc133,g_apca_d[l_ac].apcc134, 
       g_apca_d[l_ac].apcc135,g_apca_d[l_ac].apcc136,g_apca_d[l_ac].apcc137,g_apca_d[l_ac].apcc118,g_apca_d[l_ac].apcc138, 
       g_apca_d[l_ac].apcc115,g_apca_d[l_ac].apcc139
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
 
      
 
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL aapq330_detail_show("'1'")
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            CALL cl_err( "", 9035, 1 )
         END IF
         EXIT FOREACH
      END IF
 
   END FOREACH
 
 
 
 
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   CALL g_apca_d.deleteElement(g_apca_d.getLength())
 
   #add-point:陣列長度調整
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
 
   LET g_error_show = 0
 
   LET g_detail_cnt = l_ac - 1
   DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
 
   #+ 此段落由子樣板qs06產生
   #+ b_fill段CURSOR釋放
   CLOSE b_fill_curs
   FREE aapq330_pb
 
 
 
 
   LET l_ac = 1
   CALL aapq330_b_fill2()
 
      CALL aapq330_filter_show('apca100','b_apca100')
   CALL aapq330_filter_show('apcc105','b_apcc105')
   CALL aapq330_filter_show('apcc133','b_apcc133')
   CALL aapq330_filter_show('apcc134','b_apcc134')
   CALL aapq330_filter_show('apcc135','b_apcc135')
   CALL aapq330_filter_show('apcc136','b_apcc136')
   CALL aapq330_filter_show('apcc137','b_apcc137')
   CALL aapq330_filter_show('apcc118','b_apcc118')
   CALL aapq330_filter_show('apcc138','b_apcc138')
   CALL aapq330_filter_show('apcc115','b_apcc115')
   CALL aapq330_filter_show('apcc139','b_apcc139')
 
 
END FUNCTION
]]>
