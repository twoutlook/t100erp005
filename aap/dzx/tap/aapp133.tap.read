  <point name="input.a.slip1" order="" ver="2" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[            #帳款單別
            LET g_master.l_slip1_desc = ''
            DISPLAY BY NAME g_master.l_slip1_desc
            IF NOT cl_null(g_master.sel1) AND NOT cl_null(g_master.slip1)THEN
               IF g_master.sel1 = '3' THEN
                  #入庫
                  CALL s_fin_slip_chk(g_master.slip1,'aapt300',g_master.apcald,'D-FIN-3001') RETURNING g_sub_success,g_errno
               ELSE
                  #倉退
                  CALL s_fin_slip_chk(g_master.slip1,'aapt340',g_master.apcald,'D-FIN-3001') RETURNING g_sub_success,g_errno
               END IF
               IF NOT g_sub_success THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = g_errno
                  LET g_errparam.extend = ''
                  LET g_errparam.popup = TRUE
                  CALL cl_err()
                  NEXT FIELD CURRENT
               END IF              
            END IF
            CALL s_aooi200_fin_get_slip_desc(g_master.slip1)RETURNING g_master.l_slip1_desc
            DISPLAY BY NAME g_master.l_slip1_desc]]>
  </point>
  <point name="input.c.apca007" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[            #帳款類型
