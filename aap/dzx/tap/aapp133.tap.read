  <point name="construct.c.pmdtdocno" order="" ver="3" cite_std="N" new="N" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[            #開窗c段
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c'
            LET g_qryparam.reqry = FALSE
            CALL s_fin_get_ld_type(g_master.apcald) RETURNING g_qryparam.arg1
            IF NOT g_qryparam.arg1 = 0 THEN
               #150702-00001#1-----s
#               #150309apo--(s)
#               IF g_master.sel1 = '3' THEN   #入庫
#                  LET g_qryparam.where = " pmds000 IN ('3','4','6','12','13','17','18','20') "   #150629-00038#1 add 17,18,20
#               ELSE                          #倉退
#                  LET g_qryparam.where = " pmds000 IN ('7','14','15','19') "   #150629-00038#1 add 19
#               END IF               
#               #150309apo--(e)      

               IF g_master.sel1 = '3' THEN   #入庫
                  LET g_qryparam.where = " pmds000 IN ",l_pmds000_str2 CLIPPED  
               ELSE                          #倉退
                  LET g_qryparam.where = " pmds000 IN ",l_pmds000_str3 CLIPPED
               END IF      
               #150702-00001#1-----e               
               #LET g_qryparam.where = "     pmds000 IN ('3','4','6','7','12','13','14','15') ",   #150309apo mark   #pmds000:SCC-2060排除收貨單  
               LET g_qryparam.where = g_qryparam.where CLIPPED,                                    #150309apo
                                      " AND pmdtsite IN ",g_wc_apcasite," ",
                                      " AND pmdsstus = 'S' ",                      #限制狀態為已過帳
                                      " AND pmdt084 = 'Y' "                       #排除"多角貿易"型態的單子    


               #稅別有輸入時,則加入稅別為 QBE條件                                   
               IF NOT cl_null(g_master.apca011) THEN
                  LET g_qryparam.where = g_qryparam.where," AND pmds033 ='",g_master.apca011,"'"
               END IF            
               #日期有輸入時,加入qbe條件
               IF NOT cl_null(g_master.apcadocdt) THEN
                  LET g_qryparam.where = g_qryparam.where," AND pmds001 BETWEEN '",g_pdate_s,"' AND '",g_master.apcadocdt,"' "
               END IF
               CALL q_pmdtdocno_7()                           #呼叫開窗
               DISPLAY g_qryparam.return1 TO pmdtdocno  #顯示到畫面上
            ELSE
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 'aap-00301'
               LET g_errparam.extend = ''
               LET g_errparam.popup = TRUE
               CALL cl_err()               
            END IF            
            NEXT FIELD pmdtdocno]]>
