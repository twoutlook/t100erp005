         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()
         RETURN
      END IF
   END IF
   #彙總條件不為2:入庫單號時,立帳日期一定要輸入
   IF g_master.sel2 <> '2' THEN
      IF cl_null(g_master.apcadocdt) THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'aap-00333'
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()
         RETURN         
      END IF
   END IF]]>
  </point>
  <point name="process.process" order="" ver="3" cite_std="N" new="N" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[
   LET l_wc2 = ''
   IF g_master.chk_21 = 'Y' THEN LET l_wc2 = l_wc2 ,",'21'" END IF   #訂金待抵       #對應41:應付款單
   IF g_master.chk_22 = 'Y' THEN LET l_wc2 = l_wc2 ,",'22'" END IF   #倉退待抵       #對應41:應付款單
   IF g_master.chk_23 = 'Y' THEN LET l_wc2 = l_wc2 ,",'23'" END IF   #預付待抵       #對應41:應付款單
   IF g_master.chk_24 = 'Y' THEN LET l_wc2 = l_wc2 ,",'24'" END IF   #沖銷溢付待抵   #對應41:應付款單
   IF g_master.chk_25 = 'Y' THEN LET l_wc2 = l_wc2 ,",'25'" END IF   #押金待底       #對應42:應付待抵款單
   IF g_master.chk_29 = 'Y' THEN LET l_wc2 = l_wc2 ,",'29'" END IF   #其他待抵       #對應41:應付款單
   IF NOT cl_null(l_wc2) THEN 
      LET l_wc2 = s_chr_replace(l_wc2,',',' ',1)   #把第一個,以空白取代
   END IF
   CALL cl_err_collect_init()
   CALL s_fin_get_ld_type(g_master.apcald) RETURNING l_ld_type
   IF l_ld_type = 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'aap-00301'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()   
      CALL cl_err_collect_show()
      RETURN
   END IF  

   LET l_wc =  g_master.wc CLIPPED
   IF cl_null(l_wc)THEN LET l_wc = ' 1=1' END IF
   LET l_wc = l_wc CLIPPED, " AND pmdssite IN ",g_wc_apcasite
   CALL s_ld_sel_glaa(g_master.apcald,'glaa001|glaa025|glaacomp') RETURNING  g_sub_success,l_glaa001,l_glaa025,l_apcacomp
   CALL s_transaction_begin()   
   #1.寫入TEMP
   DELETE FROM s_aapp131_tmp_g
   LET l_field[1].f11 = "pmdt056"   LET l_field[2].f11 = "pmdt057"   LET l_field[3].f11 = "pmdt058"   #請款數量
  
   #150702-00001#1-----s
   LET l_pmds000_str1 = s_aap_get_acc_str('2060',"gzcb003 = 'Y'")
   LET l_pmds000_str1 = s_fin_get_wc_str(l_pmds000_str1)
  
   LET l_pmds000_str2 = s_aap_get_acc_str('2060',"gzcb003 = 'Y' AND gzcb004 = 1 ")
   LET l_pmds000_str2 = s_fin_get_wc_str(l_pmds000_str2)  
  
   LET l_pmds000_str3 = s_aap_get_acc_str('2060',"gzcb003 = 'Y' AND gzcb004 = -1 ")
   LET l_pmds000_str3 = s_fin_get_wc_str(l_pmds000_str3)  
   #150702-00001#1-----e
  
   #抓取資料條件和q_pmdtdocno_4盡量一致
   #可選取的資料才會跟預期的資料相同
   #可立暫估數量 = 計價數量 - 已立帳數量
   IF g_wc2 = " 1=1" THEN  #表示不需要join採購單欄位
      LET ls_sql ="SELECT pmdsent,pmdsdocdt,pmds000,pmds007,pmds008,pmds031,pmds033,pmds037,pmds038, ",
                  "       pmdtsite,pmdtdocno,pmdtseq,pmdt001,pmdt006,pmdt019,pmdt024, ",
                  "       pmdldocno,pmdl002,pmdl003,pmdt024,'','','','','' ", 
                  "  FROM (",
                  "       SELECT pmdsent,pmdsdocdt,pmds000,pmds007,pmds008,pmds031,pmds033,pmds037,pmds038, ",
                  "              pmdtsite,pmdtdocno,pmdtseq,pmdt001,pmdt006,pmdt019, ",
                  "              COALESCE(pmdt024,0) - COALESCE(",l_field[l_ld_type].f11,",0) pmdt024, ",
                  "              pmdldocno,pmdl002,pmdl003,pmds001 ",   #150210apo add pmds001
                  "         FROM pmds_t,pmdt_t LEFT OUTER JOIN pmdl_t ON pmdlent = pmdtent AND pmdldocno = pmdt001 ",
                  "        WHERE pmdsent = pmdtent AND pmdt084 ='Y' AND pmdsent = '",g_enterprise,"' ", 
                  "          AND pmdsdocno = pmdtdocno ",
                  #"          AND pmds000 IN ('3','4','6','7','12','13','14','15','17','18','19','20') ", #150702-00001#1 mark  #150629-00038#1 add 17~20     #pmds000:SCC-2060
                  "          AND pmds000 IN ",l_pmds000_str1 CLIPPED,   #150702-00001#1 add
                  "          AND pmdsstus = 'S' ",                         #只抓入庫/且過帳
                  "          AND ",l_wc CLIPPED,") ",
                  "  WHERE pmdt024 > 0 "            
   ELSE
      LET ls_sql ="SELECT pmdsent,pmdsdocdt,pmds000,pmds007,pmds008,pmds031,pmds033,pmds037,pmds038, ",
                  "       pmdtsite,pmdtdocno,pmdtseq,pmdt001,pmdt006,pmdt019,pmdt024, ",
                  "       pmdldocno,pmdl002,pmdl003,pmdt024,'','','','','' ", 
                  "  FROM (",
                  "       SELECT pmdsent,pmdsdocdt,pmds000,pmds007,pmds008,pmds031,pmds033,pmds037,pmds038, ",
                  "              pmdtsite,pmdtdocno,pmdtseq,pmdt001,pmdt006,pmdt019, ",
                  "              COALESCE(pmdt024,0) - COALESCE(",l_field[l_ld_type].f11,",0) pmdt024, ",
                  "              pmdldocno,pmdl002,pmdl003,pmds001 ",   #150210apo add pmds001
                  "         FROM pmds_t,pmdt_t,pmdl_t  ",
                  "        WHERE pmdsent = pmdtent AND pmdt084 ='Y' AND pmdsent = ", g_enterprise,
                  "          AND pmdsdocno = pmdtdocno AND pmdldocno = pmdt001",
                  #"          AND pmdsstus = 'S' AND pmds000 IN ('3','6','7','12','13','14','15','17','18','19','20') ",  #150702-00001#1 mark  #150629-00038#1  add 17~20   #pmds000:SCC-2060 只抓入庫/且過帳
                  "          AND pmdsstus = 'S' AND pmds000 IN ",l_pmds000_str1 CLIPPED,  #150702-00001#1 add
                  
                  "          AND ",l_wc CLIPPED, " AND ",g_wc2 CLIPPED,
                  "       ) ",
                  "  WHERE pmdt024 > 0 "    
   END IF
   #150702-00001#1-----s
#   IF g_master.sel1 = '3' THEN   #入庫
#      LET ls_sql = ls_sql," AND pmds000 IN ('3','4','6','12','13','17','18','20') "  #150629-00038#1 add 17,18,20
#   ELSE                          #倉退
#      LET ls_sql = ls_sql," AND pmds000 IN ('7','14','15','19') "   #150629-00038#1 add 19
#   END IF
   IF g_master.sel1 = '3' THEN   #入庫
      LET ls_sql = ls_sql," AND pmds000 IN ",l_pmds000_str2 CLIPPED
   ELSE                          #倉退
      LET ls_sql = ls_sql," AND pmds000 IN ",l_pmds000_str3 CLIPPED
   END IF
   #150702-00001#1-----e
   #150210--(s)
   IF NOT cl_null(g_master.apcadocdt) THEN
      LET ls_sql = ls_sql," AND pmds001 BETWEEN '",g_pdate_s,"' AND '",g_master.apcadocdt,"' "
   END IF
   #150210--(e)
   PREPARE aapp133_sel_p FROM ls_sql
   DECLARE aapp133_sel_c CURSOR FOR aapp133_sel_p
   ##########################################################################################
   #一些會取代掉的預設值會在這邊給入
   #若符合不可產生帳款單的單據，會在此利用CONTINUE FOREACH 跳過INSERT TEMP的部分
   #拆單條件若無值會給一個空白或定值以免SQL失敗少抓到資料
   ##########################################################################################   
   FOREACH aapp133_sel_c INTO l_tmp.*
      IF SQLCA.SQLCODE THEN EXIT FOREACH END IF
      #入庫單所屬法人與帳務中心所屬法人不同者剔除
      CALL s_fin_orga_get_comp_ld(l_tmp.pmdtsite) RETURNING g_sub_success,g_errno,l_pmdtcomp,l_pmdtld
      IF l_pmdtcomp <> l_apcacomp THEN
         CONTINUE FOREACH
      END IF
      #帳款類別
      IF NOT cl_null(g_master.apca007) THEN
         LET l_tmp.apca007 = g_master.apca007
      ELSE
         #取對象別預設(若無資料給空白)
         CALL s_apmm101_sel_pmab(l_apcacomp,l_tmp.pmds007,'pmab055') RETURNING g_sub_success,g_errno,l_tmp.apca007
      END IF
      #立帳日期(沒輸入立帳日期表示由入庫日期當立帳日)
      IF NOT cl_null(g_master.apcadocdt) THEN
         LET l_tmp.pmdsdocdt = g_master.apcadocdt
      END IF      
      #稅別
      IF NOT cl_null(g_master.apca011)THEN
         #若稅別有輸入，表示限定篩選條件，單身資料須為同稅別者
         IF l_tmp.pmds033 <> g_master.apca011 THEN
            CONTINUE FOREACH
         END IF
      END IF      
      #付款條件編號
      IF NOT cl_null(g_master.apca008) THEN
         LET l_tmp.pmds031 = g_master.apca008   
      END IF
      
      CASE g_master.rate1
           WHEN '2'  #依立帳日匯率(aooi160)
               CALL s_aooi160_get_exrate('2',g_master.apcald,l_tmp.pmdsdocdt,l_tmp.pmds037,l_glaa001,0,l_glaa025)
                    RETURNING l_tmp.pmds038
           WHEN '3'  #依立帳日月平均匯率(aooi170)
               CALL s_aooi160_get_exrate_avg('2',g_master.apcald,g_master.apcadocdt,l_tmp.pmds037,l_glaa001,0,l_glaa025)
                    RETURNING g_sub_success,g_errno,l_tmp.pmds038
      END CASE
      
      ####不產生帳款的檢查(1:稅別 2:付款條件 3:原單無匯率)
      #1.稅別
      IF cl_null(l_tmp.pmds033) THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'asf-00228'        
         LET g_errparam.extend = l_tmp.pmdtdocno
         LET g_errparam.popup = TRUE
         CALL cl_err()      
         CONTINUE FOREACH
      END IF      
      #2.付款條件:如果無付款條件,將影響多帳期產生
      IF cl_null(l_tmp.pmds031)THEN                
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'apm-00530'         
         LET g_errparam.extend = l_tmp.pmdtdocno
         LET g_errparam.popup = TRUE
         CALL cl_err()      
         CONTINUE FOREACH
      END IF  
      #3.無匯率資料      
      IF cl_null(l_tmp.pmds038) THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'sub-00064'         
         LET g_errparam.extend = l_tmp.pmdtdocno
         LET g_errparam.popup = TRUE
         CALL cl_err()
         CONTINUE FOREACH
      END IF
      ####處理拆單條件
      CASE
         WHEN g_master.sel2 = '1'            #150106-00011#4
            LET l_tmp.cond1 = l_tmp.pmds007  #150106-00011#4
         WHEN g_master.sel2 = '2'
            LET l_tmp.cond1 = l_tmp.pmdtdocno
         WHEN g_master.sel2 = '3'
            LET l_tmp.cond1 = l_tmp.pmdldocno
         WHEN g_master.sel2 = '4'
            LET l_tmp.cond1 = l_tmp.pmdl002
         WHEN g_master.sel2 = '5'
            LET l_tmp.cond1 = l_tmp.pmdl003
         WHEN g_master.sel2 = '6'
            LET l_tmp.cond1 = l_tmp.pmdt006
      END CASE      
      INSERT INTO s_aapp131_tmp_g VALUES(l_tmp.*)
   END FOREACH
   CALL s_transaction_end('Y',0)   #150126-00012#1 
   #計算迴圈筆數
   SELECT count(*) INTO l_cnt
     FROM s_aapp131_tmp_g
   IF l_cnt = 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'asf-00230'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()
      #CALL s_transaction_end('N',0)   #150126-00012#1 mark
   ELSE
      #2.產生單據
      LET lc_param_apca.type = '3'
      LET lc_param_apca.sel1 = g_master.sel1
      LET lc_param_apca.sel2 = g_master.sel2
      LET lc_param_apca.slip = g_master.slip1
      LET lc_param_apca.site = g_master.apcasite
      LET lc_param_apca.ld   = g_master.apcald
      LET lc_param_apca.wc2  = l_wc2
      LET lc_param_apca.bgjob= g_bgjob
      LET ls_js = util.JSON.stringify(lc_param_apca)
     #CALL s_aapp131_ins_apca('3',g_master.sel1,g_master.sel2,g_master.slip1,g_master.apcasite,g_master.apcald,l_wc2,g_bgjob)
      CALL s_aapp131_ins_apca(ls_js) RETURNING g_sub_success,l_strno,l_endno
      IF g_sub_success THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'asf-00251'
         LET g_errparam.replace[1] = l_strno
         LET g_errparam.replace[2] = l_endno
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()
         #CALL s_transaction_end('Y',0)   #150126-00012#1 mark
      ELSE
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'aap-00187'   #單據產生失敗
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()   
         #CALL s_transaction_end('N',0)   #150126-00012#1 mark
         DISPLAY '' ,0 TO stagenow,stagecomplete
      END IF   
   END IF
   
   CALL cl_err_collect_show()]]>
  </point>
