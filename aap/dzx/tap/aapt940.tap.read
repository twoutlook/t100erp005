   CLOSE aapt940_cl
   
   CALL aapt940_idx_chk()
   
   #撈取異動後的資料(主要是帶出reference)
   EXECUTE aapt940_master_referesh USING g_xrej_m.xrejdocno INTO g_xrej_m.xrejsite,g_xrej_m.xrejld,g_xrej_m.xrej004, 
