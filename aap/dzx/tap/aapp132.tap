  <point name="process.pre_process" order="" ver="4" cite_std="N" new="N" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[   IF g_bgjob = "Y" OR g_bgjob = "T" THEN
      LET g_master.wc        = lc_param.wc
      LET g_master.apcasite  = lc_param.apcasite
      LET g_master.apcald    = lc_param.apcald
      LET g_master.apca008   = lc_param.apca008
      LET g_master.rate1     = lc_param.rate1
      LET g_master.sel1      = lc_param.sel1
      LET g_master.sel2      = lc_param.sel2
      LET g_master.slip1     = lc_param.slip1
      LET g_master.apca007   = lc_param.apca007
      LET g_master.apcadocdt = lc_param.apcadocdt
      LET g_master.chk_21    = lc_param.chk_21
      LET g_master.chk_22    = lc_param.chk_22
      LET g_master.chk_23    = lc_param.chk_23
      LET g_master.chk_24    = lc_param.chk_24
      LET g_master.chk_25    = lc_param.chk_25
      LET g_master.chk_29    = lc_param.chk_29
      CALL s_ld_sel_glaa(g_master.apcald,'glaa003|glaa024') RETURNING  g_sub_success,g_glaa003,g_glaa024
   END IF
   #檢核單別是否該帳套可以使用
   IF NOT cl_null(g_master.sel1) AND NOT cl_null(g_master.slip1)THEN
      IF g_master.sel1 = '2' THEN
         #藍字發票
         CALL s_fin_slip_chk(g_master.slip1,'aapt300',g_master.apcald,'D-FIN-3001') RETURNING g_sub_success,g_errno
      ELSE
         #紅字發票
         CALL s_fin_slip_chk(g_master.slip1,'aapt340',g_master.apcald,'D-FIN-3001') RETURNING g_sub_success,g_errno
      END IF
      IF NOT g_sub_success THEN
         LET g_master.slip1 = ''
         LET g_master.l_slip1_desc = ''
         DISPLAY BY NAME g_master.slip1,g_master.l_slip1_desc
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = g_errno
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()
         RETURN
      END IF              
   END IF
   #150210apo--(s)
   #彙總條件不為7:發票收票單號時,立帳日期一定要輸入
   IF g_master.sel2 <> '7' THEN
      IF cl_null(g_master.apcadocdt) THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'aap-00333'
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()
         RETURN         
      END IF
   END IF   
   #150210apo--(e)]]>
  </point>
  <point name="process.process" order="" ver="4" cite_std="N" new="N" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[   CALL s_get_accdate(g_glaa003,g_master.apcadocdt,'','')
   RETURNING g_sub_success,g_errno,l_glav002,l_glav005,l_sdate_s,l_sdate_e,
             l_glav006,g_pdate_s,g_pdate_e,l_glav007,l_wdate_s,l_wdate_e   
   IF g_master.sel1 = '2' THEN
      LET l_apbb050 = '1'  #發票
   ELSE
      LET l_apbb050 = '2'  #折讓單
   END IF
   LET l_wc2 = ''
   IF g_master.chk_21 = 'Y' THEN LET l_wc2 = l_wc2 ,",'21'" END IF   #訂金待抵       #對應41:應付款單
   IF g_master.chk_22 = 'Y' THEN LET l_wc2 = l_wc2 ,",'22'" END IF   #倉退待抵       #對應41:應付款單
   IF g_master.chk_23 = 'Y' THEN LET l_wc2 = l_wc2 ,",'23'" END IF   #預付待抵       #對應41:應付款單
   IF g_master.chk_24 = 'Y' THEN LET l_wc2 = l_wc2 ,",'24'" END IF   #沖銷溢付待抵   #對應41:應付款單
   IF g_master.chk_25 = 'Y' THEN LET l_wc2 = l_wc2 ,",'25'" END IF   #押金待底       #對應42:應付待抵款單
   IF g_master.chk_29 = 'Y' THEN LET l_wc2 = l_wc2 ,",'29'" END IF   #其他待抵       #對應41:應付款單
   IF NOT cl_null(l_wc2) THEN 
      LET l_wc2 = s_chr_replace(l_wc2,',',' ',1)   #把第一個,以空白取代
   END IF
   LET l_field[1].cond1 = 'apbb002'    #交易對象
   LET l_field[4].cond1 = 'apbb051'    #採購人員
   LET l_field[5].cond1 = 'apbb052'    #採購部門
   LET l_field[7].cond1 = 'apbbdocno'  #收票單號
   LET l_field[1].f1 = "pmdt056"   LET l_field[2].f1 = "pmdt057"  LET l_field[3].f1 = "pmdt058"   #已請款數量
   
   CALL cl_err_collect_init()
   CALL s_fin_get_ld_type(g_master.apcald) RETURNING l_ld_type
   IF l_ld_type = 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'aap-00301'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()   
      CALL cl_err_collect_show()
      RETURN
   END IF
   LET ls_sql = " SELECT pmdt024 - ",l_field[l_ld_type].f1,
                "   FROM pmdt_t",
                " WHERE pmdtent = ? AND pmdtdocno = ? AND pmdtseq =  ? "
   PREPARE aapp132_get_pmdt024_p FROM ls_sql
   CALL s_ld_sel_glaa(g_master.apcald,'glaa025|glaacomp') RETURNING  g_sub_success,l_glaa025,l_apcacomp
   
   CALL s_transaction_begin()
   #1.寫入TEMP
   DELETE FROM s_aapp131_tmp_g
   LET ls_sql = "SELECT apbaent ,apbb011,apba004,apbb002,     '',",                        #ent/發票日期/來源作業/交易對象/''
                "       apbb054 ,apbb012,apbb014,apbb015,",                                #付款條件/稅別/幣別/匯率
                "       apbaorga,apbbdocno,apbaseq,     '',     '',",                      #營運組織/收票單號/收票項次/''/''
                "       apba009 ,apba010,     '',apbb051,apbb052,",                        #入庫單位/發票數量/''/採購人員/採購部門
                "       ''      ,apbb050,     '',",l_field[g_master.sel2].cond1,",",       #''/發票性質/帳款類型/彙總條件
                "       apba005 ,apba006",                                                 #交易單號/交易項次
                "  FROM apba_t,apbb_t",
                " WHERE apbaent = apbbent AND apbadocno = apbbdocno AND apbbstus = 'Y'",
                "   AND apbaent = ",g_enterprise," AND ",g_master.wc,
                "   AND apbb050 ='",l_apbb050,"'   AND apbb004 = '",g_master.apcasite,"'",
                "   AND apbbdocno NOT IN (SELECT DISTINCT apbadocno FROM apba_t WHERE apbaent =",g_enterprise," AND apba004 = '10')",  #採購單不列入
                "   AND apbbdocno NOT IN (SELECT DISTINCT apcb049 FROM apca_t,apcb_t WHERE apcaent = apcbent AND apcastus <> 'X' AND apcb049 IS NOT NULL",
                "                            AND apcbent =",g_enterprise," AND apcadocno = apcbdocno AND apcbld = '",g_master.apcald,"')", #已在t300發票立帳者的不列入(排除作廢)
                " AND apbbdocdt BETWEEN '",g_pdate_s,"' AND '",g_master.apcadocdt,"' "

   PREPARE aapp132_sel_p FROM ls_sql
   DECLARE aapp132_sel_c CURSOR FOR aapp132_sel_p
   FOREACH aapp132_sel_c INTO l_tmp.*
      IF SQLCA.SQLCODE THEN EXIT FOREACH END IF
      #入庫單所屬法人與帳務中心所屬法人不同者剔除
      CALL s_fin_orga_get_comp_ld(l_tmp.pmdtsite) RETURNING g_sub_success,g_errno,l_pmdtcomp,l_pmdtld
      IF l_pmdtcomp <> l_apcacomp THEN
         CONTINUE FOREACH
      END IF            
      #借用此暫存檔欄位pmds000,放置apba004
      IF l_tmp.pmds000 MATCHES '2[01]' OR l_tmp.pmds000 = '11' THEN
         EXECUTE aapp132_get_pmdt024_p USING g_enterprise,l_tmp.apba005,l_tmp.apba006 INTO l_pmdt024
         IF cl_null(l_pmdt024) THEN LET l_pmdt024 = 0 END IF
         IF  l_pmdt024 < l_tmp.pmdt024 THEN
            LET l_tmp.apcb007 = l_pmdt024
         ELSE
            LET l_tmp.apcb007 = l_tmp.pmdt024
         END IF
      ELSE
         LET l_tmp.apcb007 = l_tmp.pmdt024
      END IF

      #帳款類別
      IF NOT cl_null(g_master.apca007) THEN
         LET l_tmp.apca007 = g_master.apca007
      ELSE
         #取對象別預設(若無資料給空白)
         CALL s_apmm101_sel_pmab(l_apcacomp,l_tmp.pmds007,'pmab055') RETURNING g_sub_success,g_errno,l_tmp.apca007
      END IF
      #立帳日期(沒輸入立帳日期表示由發票日期當立帳日)
      IF NOT cl_null(g_master.apcadocdt) THEN
         LET l_tmp.pmdsdocdt = g_master.apcadocdt
      END IF
      #付款條件編號
      IF NOT cl_null(g_master.apca008) THEN
         LET l_tmp.pmds031 = g_master.apca008   
      END IF
      
      CASE g_master.rate1
           WHEN '2'  #依立帳日匯率(aooi160)
               CALL s_aooi160_get_exrate('2',g_master.apcald,l_tmp.pmdsdocdt,l_tmp.pmds037,l_glaa001,0,l_glaa025)
                    RETURNING l_tmp.pmds038
           WHEN '3'  #依立帳日月平均匯率(aooi170)
               CALL s_aooi160_get_exrate_avg('2',g_master.apcald,g_master.apcadocdt,l_tmp.pmds037,l_glaa001,0,l_glaa025)
                    RETURNING g_sub_success,g_errno,l_tmp.pmds038
      END CASE
      IF cl_null(l_tmp.pmds038) THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'sub-00064'         #找不到有效的匯率資料
         LET g_errparam.extend = l_tmp.pmdtdocno
         LET g_errparam.popup = TRUE
         CALL cl_err()
         CONTINUE FOREACH
      END IF
      INSERT INTO s_aapp131_tmp_g VALUES(l_tmp.*)
   END FOREACH
   CALL s_transaction_end('Y',0)   #150126-00012#1 
   #計算迴圈筆數
   SELECT count(*) INTO l_cnt
     FROM s_aapp131_tmp_g
   IF l_cnt = 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'asf-00230'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()
      #CALL s_transaction_end('N',0)   #150126-00012#1 mark
   ELSE
      #2.產生單據
      LET lc_param_apca.type = '2'
      LET lc_param_apca.sel1 = g_master.sel1
      LET lc_param_apca.sel2 = g_master.sel2
      LET lc_param_apca.slip = g_master.slip1
      LET lc_param_apca.site = g_master.apcasite
      LET lc_param_apca.ld   = g_master.apcald
      LET lc_param_apca.wc2  = l_wc2
      LET lc_param_apca.bgjob= g_bgjob
      LET ls_js = util.JSON.stringify(lc_param_apca)
     #CALL s_aapp131_ins_apca('2',g_master.sel1,g_master.sel2,g_master.slip1,g_master.apcasite,g_master.apcald,l_wc2,g_bgjob)
      CALL s_aapp131_ins_apca(ls_js) RETURNING g_sub_success,l_strno,l_endno
      IF g_sub_success THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'asf-00251'
         LET g_errparam.replace[1] = l_strno
         LET g_errparam.replace[2] = l_endno
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()
         #CALL s_transaction_end('Y',0)   #150126-00012#1 mark
      ELSE
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'aap-00187'   #單據產生失敗
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()   
         #CALL s_transaction_end('N',0)   #150126-00012#1 mark
         DISPLAY '' ,0 TO stagenow,stagecomplete
      END IF   
   END IF
   
   CALL cl_err_collect_show()]]>
  </point>
