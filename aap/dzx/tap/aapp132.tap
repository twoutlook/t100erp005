  <point name="function.aapp132_qbe_clear" order="1" ver="4" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 清空&給預設 #150127-00007#1
# Memo...........:
# Usage..........: CALL aapp132_qbe_clear()
# Date & Author..: 2015/02/24 By Reanna
# Modify.........:
################################################################################
PRIVATE FUNCTION aapp132_qbe_clear()
   
   INITIALIZE g_master.* TO NULL
   
   #150331-00006#4 add ------
   IF NOT cl_null(g_argv[1]) AND NOT cl_null(g_argv[2]) THEN
      LET g_master.apcasite  = g_argv[1]
      LET g_master.apbbdocno = g_argv[2]
      LET g_master.wc = "apbbdocno='",g_master.apbbdocno,"'"
      DISPLAY g_master.apbbdocno TO apbbdocno
      
      #該帳務中心與帳別無法勾稽到,以登入人員g_user取得預設帳別/法人
      CALL s_fin_orga_get_comp_ld(g_master.apcasite) RETURNING g_sub_success,g_errno,l_apcacomp,g_master.apcald   
      #若取不出資料,則不預設帳別
      IF NOT g_sub_success THEN
         LET g_master.apcald   = ''
      ELSE
         CALL s_ld_sel_glaa(g_master.apcald,'glaa003|glaa024') RETURNING  g_sub_success,g_glaa003,g_glaa024
         LET g_master.l_apcald_desc  = s_desc_get_ld_desc(g_master.apcald)
      END IF
      CALL s_fin_account_center_sons_query('3',g_master.apcasite,g_today,'1')
      CALL s_fin_account_center_ld_str() RETURNING g_wc_apcald
      CALL s_fin_get_wc_str(g_wc_apcald) RETURNING g_wc_apcald
      LET g_master.l_apcasite_desc= s_desc_get_department_desc(g_master.apcasite)
      LET g_master_o.apcasite = g_master.apcasite

   ELSE
   #150331-00006#4 add end---
      CALL s_aap_get_default_apcasite('','','') RETURNING g_sub_success,g_errno,g_master.apcasite,g_master.apcald,l_apcacomp
      IF g_sub_success THEN 
         CALL s_fin_account_center_sons_query('3',g_master.apcasite,g_today,'1')
         CALL s_fin_account_center_ld_str() RETURNING g_wc_apcald
         CALL s_fin_get_wc_str(g_wc_apcald) RETURNING g_wc_apcald
         CALL s_ld_sel_glaa(g_master.apcald,'glaa003|glaa024') RETURNING  g_sub_success,g_glaa003,g_glaa024
      
         LET g_master.l_apcald_desc  = s_desc_get_ld_desc(g_master.apcald)
         LET g_master.l_apcasite_desc= s_desc_get_department_desc(g_master.apcasite)
         LET g_master_o.apcasite = g_master.apcasite
      END IF
   END IF #150331-00006#4 add
   
   LET g_master.sel1   = '2'
   LET g_master.sel2   = '1'
   CALL cl_set_comp_required("apcadocdt",TRUE)  #150303apo
   LET g_master.rate1  = '1'
   LET g_master.apcadocdt= ''
   LET g_master.chk_21 = 'N'
   LET g_master.chk_22 = 'N'
   LET g_master.chk_23 = 'N'
   LET g_master.chk_24 = 'N'
   LET g_master.chk_25 = 'N'
   LET g_master.chk_29 = 'N'

   DISPLAY BY NAME g_master.apcald  ,g_master.l_apcald_desc,
                   g_master.apcasite,g_master.l_apcasite_desc
   
END FUNCTION]]>
  </point>
