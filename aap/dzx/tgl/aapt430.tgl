      LET g_apda_m.l_sumcost = "0"
      LET g_apda_m.l_sumdiscount = "0"
      LET g_apda_m.l_sumprepay = "0"
 
  
      #add-point:單頭預設值 name="insert.default"
      {<point name="insert.default"/>}
