{<section id="aapt430.ui_dialog" >}
#+ 功能選單
PRIVATE FUNCTION aapt430_ui_dialog()
   #add-point:ui_dialog段define(客製用) name="ui_dialog.define_customerization"
   {<point name="ui_dialog.define_customerization" edit="c" mark="Y"/>}
   #end add-point
