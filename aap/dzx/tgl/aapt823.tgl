       WHERE apdeent = g_enterprise AND
             apdeld = g_apda_m.apdald AND apdedocno = g_apda_m.apdadocno
      #add-point:單身刪除中 name="delete.body.m_delete2"
      {<point name="delete.body.m_delete2"/>}
