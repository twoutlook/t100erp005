       xrem002,xrem005,xremstus,xremownid,'',xremowndp,'',xremcrtid,'',xremcrtdp,'',xremcrtdt,xremmodid, 
       '',xremmoddt,xremcnfid,'',xremcnfdt", 
                      " FROM xrem_t",
                      " WHERE xrement= ? AND xremdocno=? FOR UPDATE"
