{<section id="asfp501.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION asfp501_b_fill(p_wc2)              #BODY FILL UP
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   
   #end add-point
   DEFINE p_wc2            STRING
   DEFINE ls_owndept_list  STRING  #(ver:35) add
   DEFINE ls_ownuser_list  STRING  #(ver:35) add
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   DEFINE l_success      LIKE type_t.num5
   DEFINE l_slip         STRING
   #end add-point
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前 name="b_fill.sql_before"
   
   #end add-point
 
   LET g_sql = "SELECT  DISTINCT t0.sfaadocno,t0.sfaa003,t0.sfaa057,t0.sfaa010,t0.sfaa020,t0.sfaastus, 
       t0.sfaa012,t0.sfaa013,t0.sfaa009,t0.sfaa017,t0.sfaa002,t0.sfaa046,t0.sfaa047,t0.sfaa048 ,t2.imaal003 , 
       t3.oocal003 ,t4.pmaal004 ,t5.ooefl003 ,t6.ooag011 FROM sfaa_t t0",
               "",
                              " LEFT JOIN imaal_t t2 ON t2.imaalent="||g_enterprise||" AND t2.imaal001=t0.sfaa010 AND t2.imaal002='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t3 ON t3.oocalent="||g_enterprise||" AND t3.oocal001=t0.sfaa013 AND t3.oocal002='"||g_dlang||"' ",
               " LEFT JOIN pmaal_t t4 ON t4.pmaalent="||g_enterprise||" AND t4.pmaal001=t0.sfaa009 AND t4.pmaal002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent="||g_enterprise||" AND t5.ooefl001=t0.sfaa017 AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent="||g_enterprise||" AND t6.ooag001=t0.sfaa002  ",
 
               " WHERE t0.sfaaent= ?  AND  1=1 AND (", p_wc2, ") "
 
   #(ver:35) ---add start---
   
   #(ver:35) --- add end ---
 
   #add-point:b_fill段sql wc name="b_fill.sql_wc"
   LET g_sql = g_sql," AND sfaastus = 'C' AND sfaasite = '",g_site,"'"
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("sfaa_t"),
                      " ORDER BY t0.sfaadocno"
   
   #add-point:b_fill段sql之後 name="b_fill.sql_after"
   
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"sfaa_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE asfp501_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR asfp501_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_sfaa_d.clear()
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_sfaa_d[l_ac].sfaadocno,g_sfaa_d[l_ac].sfaa003,g_sfaa_d[l_ac].sfaa057,g_sfaa_d[l_ac].sfaa010, 
       g_sfaa_d[l_ac].sfaa020,g_sfaa_d[l_ac].sfaastus,g_sfaa_d[l_ac].sfaa012,g_sfaa_d[l_ac].sfaa013, 
       g_sfaa_d[l_ac].sfaa009,g_sfaa_d[l_ac].sfaa017,g_sfaa_d[l_ac].sfaa002,g_sfaa_d[l_ac].sfaa046,g_sfaa_d[l_ac].sfaa047, 
       g_sfaa_d[l_ac].sfaa048,g_sfaa_d[l_ac].sfaa010_desc,g_sfaa_d[l_ac].sfaa013_desc,g_sfaa_d[l_ac].sfaa009_desc, 
       g_sfaa_d[l_ac].sfaa017_desc,g_sfaa_d[l_ac].sfaa002_desc
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH b_fill_curs:",SQLERRMESSAGE 
         LET g_errparam.code = SQLCA.SQLCODE
         LET g_errparam.popup = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充 name="b_fill.fill"
      LET g_sfaa_d[l_ac].sel = 'N'
      CALL s_aooi200_get_slip(g_sfaa_d[l_ac].sfaadocno) RETURNING l_success,l_slip
      CALL s_aooi200_get_slip_desc(l_slip) RETURNING g_sfaa_d[l_ac].sfaadocno_desc
      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_sfaa_d[l_ac].sfaa010
      CALL ap_ref_array2(g_ref_fields,"SELECT imaal004 FROM imaal_t WHERE imaalent='"||g_enterprise||"' AND imaal001=? AND imaal002='"||g_dlang||"'","") RETURNING g_rtn_fields
      LET g_sfaa_d[l_ac].sfaa010_desc_1 = '', g_rtn_fields[1] , ''
      #end add-point
      
      CALL asfp501_detail_show()      
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
 
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   LET g_error_show = 0
   
 
   
   CALL g_sfaa_d.deleteElement(g_sfaa_d.getLength())   
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_sfaa_d.getLength()
 
      #add-point:b_fill段key值相關欄位 name="b_fill.keys.fill"
      
      #end add-point
   END FOR
   
   IF g_cnt > g_sfaa_d.getLength() THEN
      LET l_ac = g_sfaa_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_sfaa_d.getLength()
      LET g_sfaa_d_mask_o[l_ac].* =  g_sfaa_d[l_ac].*
      CALL asfp501_sfaa_t_mask()
      LET g_sfaa_d_mask_n[l_ac].* =  g_sfaa_d[l_ac].*
   END FOR
   
 
   
   LET l_ac = g_cnt
 
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_sfaa_d.getLength()
   DISPLAY g_detail_idx TO FORMONLY.idx
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE asfp501_pb
   
END FUNCTION
 
