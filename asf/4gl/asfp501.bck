################################################################################
# Descriptions...: 工单取消结案
# Memo...........:
# Usage..........: CALL asfp501_cancel_closed()
# Date & Author..: 14/10/20 By wuxja
# Modify.........:
################################################################################
PRIVATE FUNCTION asfp501_cancel_closed()
DEFINE l_n               LIKE type_t.num5
DEFINE l_i               LIKE type_t.num5
DEFINE l_flag            LIKE type_t.chr1
DEFINE l_success         LIKE type_t.num5    #160129-00002#5 add
DEFINE l_doc_type        LIKE ooba_t.ooba002 #160129-00002#5 add

   CALL cl_showmsg_init()
   LET l_n = 0
   LET l_flag = 'N'
   
   FOR l_i = 1 TO g_sfaa_d.getLength()
      IF g_sfaa_d[l_i].sel = 'Y' THEN
         LET l_flag = 'Y'
         CALL s_transaction_begin()
         IF NOT asfp501_cancel_closed_upd(g_sfaa_d[l_i].sfaadocno) THEN 
            CALL s_transaction_end("N","0")
            LET l_n = l_n + 1
         ELSE            
           #151014 Sarah mod -----(S)
           #CALL s_transaction_end("Y","0")
           #委外工單取消結案,要把對應的委外採購單也取消結案
            IF g_sfaa_d[l_i].sfaa057 ='2' AND NOT asfp501_PO_cancel_closed_upd(g_sfaa_d[l_i].sfaadocno) THEN
               CALL s_transaction_end("N","0")
               LET l_n = l_n + 1
            ELSE         
               #160129-00002#5-S
               CALL s_aooi200_get_slip(g_sfaa_d[l_i].sfaadocno) RETURNING l_success,l_doc_type
               #161116-00008#4-S
               #IF cl_get_para(g_enterprise,g_site,'S-SYS-0003') = 'Y' AND cl_get_doc_para(g_enterprise,g_site,l_doc_type,'D-MFG-0079') = 'Y' THEN  
               IF cl_get_para(g_enterprise,g_site,'S-SYS-0003') = 'Y' THEN  
                  IF cl_aws_prod_para(g_enterprise,"mes-0001") = 'Y' THEN
                     IF s_aws_mes2_wo_status_process(g_sfaa_d[l_i].sfaadocno,'3') THEN
                        CALL cl_ask_end1()
                     ELSE       
                        CALL cl_err()     
                        CALL s_transaction_end('N','0')
                        RETURN
                     END IF
                  ELSE
                     IF cl_get_doc_para(g_enterprise,g_site,l_doc_type,'D-MFG-0079') = 'Y' THEN  
               #161116-00008#4-E                  
                        IF NOT (s_aws_mes_ins('asfp501','undo_close', g_sfaa_d[l_i].sfaadocno ) = 1) THEN    
                           CALL cl_err()       
                           CALL s_transaction_end('N','0')
                           RETURN
                        ELSE
                           CALL cl_ask_end1() 
                        END IF  
               #161116-00008#4-S
                     END IF   
                  END IF 
               #161116-00008#4-E    
               END IF
               #160129-00002#5-E  
               CALL s_transaction_end("Y","0")
            END IF
           #151014 Sarah mod -----(E)
         END IF
      END IF
   END FOR
   
   IF l_n = 0 THEN
      IF l_flag = 'N' THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.extend = ""
         LET g_errparam.code   = "asf-00443"
         LET g_errparam.popup  = TRUE
         CALL cl_err()
      END IF
      IF l_flag = 'Y' THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.extend = ""
         LET g_errparam.code   = "asf-00442"
         LET g_errparam.popup  = TRUE
         CALL cl_err()
      END IF
   ELSE
      CALL cl_showmsg()
   END IF
  
END FUNCTION

#取消结案前check及update
PRIVATE FUNCTION asfp501_cancel_closed_upd(p_sfaadocno)
DEFINE p_sfaadocno              LIKE sfaa_t.sfaadocno
DEFINE r_success                LIKE type_t.chr1
DEFINE l_n                      LIKE type_t.num5
   
   LET r_success = FALSE
   
   SELECT COUNT(*) INTO l_n FROM sfaa_t 
    WHERE sfaaent = g_enterprise AND sfaasite = g_site
      AND sfaadocno = p_sfaadocno AND sfaastus = 'C'
   IF l_n = 0 THEN
      CALL cl_errmsg('',p_sfaadocno,'','asf-00441',1)
      RETURN r_success
   END IF
   
   UPDATE sfaa_t SET sfaa047 = NULL,sfaastus = 'F',sfaa065 = '0'
    WHERE sfaaent = g_enterprise AND sfaasite = g_site
      AND sfaadocno = p_sfaadocno
   IF SQLCA.sqlcode THEN
      CALL cl_errmsg('update sfaa_t',p_sfaadocno,'',SQLCA.sqlcode,1)
      RETURN r_success
   END IF
   
   #2015/09/21 by stellar add ----- (S)
   #取消結案時，若來源是獨立需求，則更新獨立需求的已耗數量
   IF NOT asfp501_upd_psab006(p_sfaadocno) THEN
      LET r_success = FALSE
      RETURN r_success
   END IF
   #2015/09/21 by stellar add ----- (E)
   
   #add--151207-00017#2 By shiun--(S)
   #取消結案時，若來源是MPS計劃，則更新MPS計劃的已開工單量
   IF NOT asfp501_upd_psad006(p_sfaadocno) THEN
      LET r_success = FALSE
      RETURN r_success
   END IF
   #add--151207-00017#2 By shiun--(E)
   LET r_success = TRUE
   RETURN r_success
    
END FUNCTION

################################################################################
# Descriptions...: 取消結案時，若是獨立需求，則更新以號數量
# Memo...........:
# Usage..........: CALL asfp501_upd_psab006(p_sfaadocno)
#                  RETURNING r_success
# Input parameter: p_sfaadocno    工單單號
# Return code....: r_success      TRUE/FALSE
# Date & Author..: 2015/09/21 By stellar
# Modify.........:
################################################################################
PRIVATE FUNCTION asfp501_upd_psab006(p_sfaadocno)
DEFINE p_sfaadocno       LIKE sfaa_t.sfaadocno
DEFINE r_success         LIKE type_t.num5
DEFINE l_count           LIKE type_t.num5
DEFINE l_psab004         LIKE psab_t.psab004
DEFINE l_psab006         LIKE psab_t.psab006
DEFINE l_psabdocno       LIKE psab_t.psabdocno
DEFINE l_psabseq         LIKE psab_t.psabseq
DEFINE l_sfaa005         LIKE sfaa_t.sfaa005
DEFINE l_sfaa006         LIKE sfaa_t.sfaa006
DEFINE l_sfaa007         LIKE sfaa_t.sfaa007
DEFINE l_sfaa010         LIKE sfaa_t.sfaa010
DEFINE l_sfaa012         LIKE sfaa_t.sfaa012
DEFINE l_sfaa013         LIKE sfaa_t.sfaa013
DEFINE l_sfaa022         LIKE sfaa_t.sfaa022
DEFINE l_sfaa023         LIKE sfaa_t.sfaa023
DEFINE l_sfaa050         LIKE sfaa_t.sfaa050
DEFINE l_success         LIKE type_t.num5

   LET r_success = TRUE
   
   IF cl_null(p_sfaadocno) THEN
      RETURN r_success
   END IF
   
   SELECT sfaa005,sfaa006,sfaa007,sfaa010,sfaa012,sfaa013,sfaa022,sfaa023,sfaa050
     INTO l_sfaa005,l_sfaa006,l_sfaa007,l_sfaa010,l_sfaa012,l_sfaa013,l_sfaa022,l_sfaa023,l_sfaa050
     FROM sfaa_t
    WHERE sfaaent = g_enterprise
      AND sfaadocno = p_sfaadocno
   
   IF l_sfaa005 = '6' THEN
      LET l_psabdocno = l_sfaa006
      LET l_psabseq = l_sfaa007
   ELSE
      LET l_psabdocno = l_sfaa022
      LET l_psabseq = l_sfaa023
   END IF
   
   IF cl_null(l_psabdocno) OR cl_null(l_psabseq) THEN
      RETURN r_success
   END IF
   
   #檢查是否有獨立需求單
   LET l_count = 0
   SELECT COUNT(*) INTO l_count 
     FROM psab_t
    WHERE psabent = g_enterprise
      AND psabdocno = l_psabdocno
      AND psabseq = l_psabseq
      AND psab001 = l_sfaa010
   IF cl_null(l_count) OR l_count <= 0 THEN
      RETURN r_success
   END IF
   
   #抓取已耗需求數量
   LET l_psab006 = 0
   SELECT psab004,psab006 INTO l_psab004,l_psab006
     FROM psab_t
    WHERE psabent = g_enterprise
      AND psabdocno = l_psabdocno
      AND psabseq = l_psabseq
      AND psab001 = l_sfaa010
   IF cl_null(l_psab006) THEN LET l_psab006 = 0 END IF
   
   #單位換算                   
   CALL s_aooi250_convert_qty(l_sfaa010,l_sfaa013,l_psab004,l_sfaa050)
        RETURNING l_success,l_sfaa050
        
   #單位換算                   
   CALL s_aooi250_convert_qty(l_sfaa010,l_sfaa013,l_psab004,l_sfaa012)
        RETURNING l_success,l_sfaa012
   
   #已耗需求數量=原已耗需求數量-欲減少的已耗需求數量+欲增加的已耗需求數量
   LET l_psab006 = l_psab006 - l_sfaa050 + l_sfaa012
   
   #更新已耗需求數量
   UPDATE psab_t SET psab006 = l_psab006
    WHERE psabent = g_enterprise
      AND psabdocno = l_psabdocno
      AND psabseq = l_psabseq
      AND psab001 = l_sfaa010
   IF SQLCA.sqlcode OR SQLCA.sqlerrd[3] = 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend = 'upd psab006'
      LET g_errparam.popup = TRUE
      
      CALL cl_err() 
      LET r_success = FALSE
      RETURN r_success
   END IF
   
   RETURN r_success
END FUNCTION

################################################################################
# Descriptions...: 委外採購單取消結案
# Memo...........:
# Usage..........: CALL asfp501_PO_cancel_closed_upd(p_sfaadocno)
#                  RETURNING r_success
# Input parameter: p_sfaadocno 工單編號
# Return code....: r_success   狀態
# Date & Author..: 15/10/14 By Sarah
# Modify.........:
################################################################################
PRIVATE FUNCTION asfp501_PO_cancel_closed_upd(p_sfaadocno)
DEFINE p_sfaadocno              LIKE sfaa_t.sfaadocno
DEFINE r_success                LIKE type_t.chr1
DEFINE l_sql                    STRING
DEFINE l_pmdndocno              LIKE pmdn_t.pmdndocno   #採購單號
DEFINE l_pmdnseq                LIKE pmdn_t.pmdnseq     #採購項次
DEFINE l_pmdlsite               LIKE pmdl_t.pmdlsite    #營運據點

   LET r_success = TRUE
   LET l_pmdndocno = ''
   LET l_pmdnseq = ''
   LET l_pmdlsite = ''

   #抓取委外採購單號,項次,營運據點   
   DECLARE asfp501_sel_po_cs CURSOR FOR
      SELECT UNIQUE pmdldocno,pmdnseq,pmdlsite
        FROM pmdl_t,pmdn_t,pmdp_t
       WHERE pmdlent   = pmdpent   AND pmdlent   = pmdnent
         AND pmdldocno = pmdpdocno AND pmdldocno = pmdndocno
         AND pmdnseq   = pmdpseq
         AND pmdl005   = '2'           #委外採購
         AND pmdl007   = '4'           #資料來源=4.一般工单
         AND pmdp003   = p_sfaadocno   #工單單號
         AND pmdlstus <>'X'            #剔除作廢單據   
   FOREACH asfp501_sel_po_cs INTO l_pmdndocno,l_pmdnseq,l_pmdlsite
       #單身狀態取消結案
       IF NOT s_apmp510_pmdn045_unclosed(l_pmdndocno,l_pmdnseq,l_pmdlsite) THEN
          LET r_success = FALSE
       END IF
   END FOREACH
   
   IF NOT cl_null(l_pmdndocno) AND NOT cl_null(l_pmdlsite) THEN
      #物流結案 => 裡面已經有包含將採購單的狀態碼回復為Y的處理
      IF NOT s_apmp510_pmdl047_unclosed(l_pmdndocno,l_pmdlsite) THEN
         LET r_success = FALSE
      END IF
   END IF

   RETURN r_success
   
END FUNCTION
################################################################################
# Descriptions...: 取消結案時，若是MPS計劃，則更新已開工單量
# Memo...........:
# Usage..........: CALL asfp501_upd_psad006(p_sfaadocno)
#                  RETURNING r_success
# Input parameter: p_sfaadocno    工單單號
# Return code....: r_success      TRUE/FALSE
# Date & Author..: 2016/01/05 By shiun
# Modify.........:
################################################################################
PRIVATE FUNCTION asfp501_upd_psad006(p_sfaadocno)
DEFINE p_sfaadocno       LIKE sfaa_t.sfaadocno
DEFINE r_success         LIKE type_t.num5
DEFINE l_count           LIKE type_t.num5
DEFINE l_psad004         LIKE psad_t.psad004
DEFINE l_psad006         LIKE psad_t.psad006
DEFINE l_psaddocno       LIKE psad_t.psaddocno
DEFINE l_psadseq         LIKE psad_t.psadseq
DEFINE l_sfaa005         LIKE sfaa_t.sfaa005
DEFINE l_sfaa006         LIKE sfaa_t.sfaa006
DEFINE l_sfaa007         LIKE sfaa_t.sfaa007
DEFINE l_sfaa010         LIKE sfaa_t.sfaa010
DEFINE l_sfaa012         LIKE sfaa_t.sfaa012
DEFINE l_sfaa013         LIKE sfaa_t.sfaa013
DEFINE l_sfaa022         LIKE sfaa_t.sfaa022
DEFINE l_sfaa023         LIKE sfaa_t.sfaa023
DEFINE l_sfaa050         LIKE sfaa_t.sfaa050
DEFINE l_success         LIKE type_t.num5

   LET r_success = TRUE
   
   IF cl_null(p_sfaadocno) THEN
      RETURN r_success
   END IF
   
   SELECT sfaa005,sfaa006,sfaa007,sfaa010,sfaa012,sfaa013,sfaa022,sfaa023,sfaa050
     INTO l_sfaa005,l_sfaa006,l_sfaa007,l_sfaa010,l_sfaa012,l_sfaa013,l_sfaa022,l_sfaa023,l_sfaa050
     FROM sfaa_t
    WHERE sfaaent = g_enterprise
      AND sfaadocno = p_sfaadocno
   
   IF l_sfaa005 = '7' THEN
      LET l_psaddocno = l_sfaa006
      LET l_psadseq = l_sfaa007
   ELSE
      LET l_psaddocno = l_sfaa022
      LET l_psadseq = l_sfaa023
   END IF
   
   IF cl_null(l_psaddocno) OR cl_null(l_psadseq) THEN
      RETURN r_success
   END IF
   
   #檢查是否有MPS計劃單
   LET l_count = 0
   SELECT COUNT(*) INTO l_count 
     FROM psad_t
    WHERE psadent = g_enterprise
      AND psaddocno = l_psaddocno
      AND psadseq = l_psadseq
      AND psad001 = l_sfaa010
   IF cl_null(l_count) OR l_count <= 0 THEN
      RETURN r_success
   END IF
   
   #抓取已開工單量
   LET l_psad006 = 0
   SELECT psad004,psad006 INTO l_psad004,l_psad006
     FROM psad_t
    WHERE psadent = g_enterprise
      AND psaddocno = l_psaddocno
      AND psadseq = l_psadseq
      AND psad001 = l_sfaa010
   IF cl_null(l_psad006) THEN LET l_psad006 = 0 END IF
   
   #單位換算                   
   CALL s_aooi250_convert_qty(l_sfaa010,l_sfaa013,l_psad004,l_sfaa050)
        RETURNING l_success,l_sfaa050
        
   #單位換算                   
   CALL s_aooi250_convert_qty(l_sfaa010,l_sfaa013,l_psad004,l_sfaa012)
        RETURNING l_success,l_sfaa012
   
   #已開工單量=原已開工單量-欲減少的已開工單量+欲增加的已開工單量
   LET l_psad006 = l_psad006 - l_sfaa050 + l_sfaa012
   
   #更新已開工單量
   UPDATE psad_t SET psad006 = l_psad006
    WHERE psadent = g_enterprise
      AND psaddocno = l_psaddocno
      AND psadseq = l_psadseq
      AND psad001 = l_sfaa010
   IF SQLCA.sqlcode OR SQLCA.sqlerrd[3] = 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend = 'upd psad006'
      LET g_errparam.popup = TRUE
      
      CALL cl_err() 
      LET r_success = FALSE
      RETURN r_success
   END IF
   
   RETURN r_success
END FUNCTION

 
