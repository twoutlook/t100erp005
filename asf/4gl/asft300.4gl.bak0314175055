               #160318-00025#4--add--end
               IF NOT cl_chk_exist("v_ooca001") THEN
                  LET g_sfba_d[l_ac].sfba014 = g_sfba_d_t.sfba014
                  INITIALIZE g_ref_fields TO NULL
