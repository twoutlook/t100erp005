            IF NOT cl_ap_chk_Range(g_sfba_d[l_ac].sfba022,"0","0","","","azz-00087",1) THEN
               LET g_sfba_d[l_ac].sfba006 = g_sfba_d_t.sfba006
               NEXT FIELD sfba022
