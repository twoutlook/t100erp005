PRIVATE FUNCTION asft300_b_desc()
DEFINE  l_success        LIKE type_t.num5
#160122-00003#1-s
#DEFINE  l_sql            STRING
#DEFINE  l_inag           RECORD LIKE inag_t.*
#DEFINE  l_sfba013        LIKE sfba_t.sfba013
