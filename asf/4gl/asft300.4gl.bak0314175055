            IF cl_null(g_sfba_d[l_ac].sfba016) THEN 
               LET g_sfba_d[l_ac].sfba016 = 0
            END IF 
            IF NOT cl_null(g_sfba_d[l_ac].sfba013) THEN
               #单位取位
