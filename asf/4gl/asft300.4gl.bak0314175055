            LET g_sfba_d[l_ac].number1 = g_sfba_d[l_ac].sfba013 - g_sfba_d[l_ac].sfba015 - g_sfba_d[l_ac].sfba016
            IF g_sfba_d[l_ac].number1 < 0 THEN
