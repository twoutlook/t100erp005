            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c'
            LET g_qryparam.reqry = FALSE
            CALL q_ooag001()
            DISPLAY g_qryparam.return1 TO sfaa002
            NEXT FIELD sfaa002

         #Ctrlp:construct.c.sfaa007
