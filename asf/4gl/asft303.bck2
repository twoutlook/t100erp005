    WHERE sfqaent = g_enterprise
      AND sfqasite = g_site
      AND sfqadocno = g_sfqa_m.sfqadocno
   IF (g_action_choice="modify" OR g_action_choice="delete" OR g_action_choice="modify_detail")  THEN
     LET g_errno = NULL
     CASE g_sfqa_m.sfqastus
        WHEN 'W'
           LET g_errno = 'sub-01347'
        WHEN 'X'
           LET g_errno = 'sub-00229'
        WHEN 'Y'
           LET g_errno = 'sub-00178'

     END CASE

     IF NOT cl_null(g_errno) THEN
        INITIALIZE g_errparam TO NULL
        LET g_errparam.code = g_errno
        LET g_errparam.extend = g_sfqa_m.sfqadocno
        LET g_errparam.popup = TRUE
        CALL cl_err()
        LET g_errno = NULL
        CALL asft303_set_act_visible()
        CALL asft303_set_act_no_visible()
        CALL asft303_show()
        RETURN FALSE
     END IF
   END IF      
   #160818-00017#37-e

   #end add-point
      
   RETURN TRUE
   
