#新增时开窗返回多个工单单号时，拆分显示于画面
PRIVATE FUNCTION asft303_b_string(p_string)
DEFINE p_string            STRING
DEFINE l_string            STRING
DEFINE l_sfqb001           LIKE sfqb_t.sfqb001
DEFINE l_n                 LIKE type_t.num5

   LET l_string = p_string
   WHILE l_string.getIndexOf("|",1) > 0
      LET l_sfqb001 = l_string.substring(1,l_string.getIndexOf("|",1)-1)
      SELECT COUNT(*) INTO l_n FROM sfqb_t
       WHERE sfqbent = g_enterprise AND sfqbdocno = g_sfqa_m.sfqadocno
         AND sfqb001 = l_sfqb001
      IF l_n = 0 THEN
         INSERT INTO sfqb_t(sfqbent,sfqbsite,sfqbdocno,sfqb001)
                     VALUES(g_enterprise,g_site,g_sfqa_m.sfqadocno,l_sfqb001)
      END IF
      LET l_string = l_string.substring(l_string.getIndexOf("|",1)+1,l_string.getLength())
   END WHILE
   
   LET l_sfqb001 = l_string
   SELECT COUNT(*) INTO l_n FROM sfqb_t
    WHERE sfqbent = g_enterprise AND sfqbdocno = g_sfqa_m.sfqadocno
      AND sfqb001 = l_sfqb001
   IF l_n = 0 THEN
      INSERT INTO sfqb_t(sfqbent,sfqbsite,sfqbdocno,sfqb001)
                  VALUES(g_enterprise,g_site,g_sfqa_m.sfqadocno,l_sfqb001)
   END IF
   
        
END FUNCTION

 
