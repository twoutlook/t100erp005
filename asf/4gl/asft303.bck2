#根據工單單號帶出單身相關資料
PRIVATE FUNCTION asft303_b_show(p_sfqb001)
DEFINE p_sfqb001                LIKE sfqb_t.sfqb001
DEFINE l_date1                  LIKE type_t.dat
DEFINE l_date2                  LIKE type_t.dat
DEFINE l_date3                  LIKE type_t.dat

   IF cl_null(p_sfqb001) THEN
      RETURN
   END IF
   
   SELECT sfaadocno,sfaa010,'','',sfaa012,sfaa013,'',sfaadocdt,sfaa019,sfaa020,sfaa002,'',sfaastus,
          sfaa049,sfaa050,sfaa051,sfaa055,sfaa056 INTO g_sfqb_d[l_ac].* FROM sfaa_t
    WHERE sfaaent = g_enterprise AND sfaadocno = p_sfqb001
    
   SELECT sfaadocdt,sfaa019,sfaa020 INTO l_date1,l_date2,l_date3 FROM sfaa_t
    WHERE sfaaent = g_enterprise AND sfaadocno = p_sfqb001
   LET g_sfqb_d[l_ac].sfaadocdt = l_date1
   LET g_sfqb_d[l_ac].sfaa019 = l_date2
   LET g_sfqb_d[l_ac].sfaa020 = l_date3
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_sfqb_d[l_ac].sfaa010
