   #end add-point
 
   
 
   #讀入ref值
   IF ps_page.getIndexOf("'1'",1) > 0 THEN
      #帶出公用欄位reference值page1
      
 
      #add-point:show段單身reference name="detail_show.body.reference"
      
      IF cl_null(g_detail[l_ac].progress) THEN  
         IF ((g_detail[l_ac].sfaa050 + g_detail[l_ac].sfaa051) < g_detail[l_ac].sfaa012) AND
               (g_detail[l_ac].sfaa020 < g_today) THEN
             LET g_detail[l_ac].progress = '2'
         ELSE    
             LET g_detail[l_ac].progress = '1' 
         END IF
            
         IF ((g_detail[l_ac].sfaa050 + g_detail[l_ac].sfaa051) >= g_detail[l_ac].sfaa012) AND
              (g_detail[l_ac].sfaa046 > g_detail[l_ac].sfaa020 OR g_today > g_detail[l_ac].sfaa020) THEN
            LET g_detail[l_ac].progress = '2'
         ELSE   
            LET g_detail[l_ac].progress = '1' 
         END IF      
      END IF 
            
      IF g_detail[l_ac].sfaastus = 'F' THEN
         LET l_sfdc_cnt = 0
         SELECT COUNT(*) INTO l_sfdc_cnt
           FROM sfdc_t
          WHERE sfdcent = g_enterprise
            AND sfdc001 = g_detail[l_ac].sfaadocno
            
         LET l_sfec_cnt = 0
         SELECT COUNT(*) INTO l_sfec_cnt
           FROM sfec_t
          WHERE sfecent = g_enterprise
            AND sfec001 = g_detail[l_ac].sfaadocno
            
         #委外採購入庫
         LET l_pmdt_cnt = 0
         SELECT COUNT(*) INTO l_pmdt_cnt
           FROM pmdt_t
          WHERE pmdtent= g_enterprise
            AND pmdt001=(SELECT pmdldocno FROM pmdl_t
                          WHERE pmdlent = g_enterprise
                            AND pmdl008 = g_detail[l_ac].sfaadocno)
         LET l_sfec_cnt = l_sfec_cnt + l_pmdt_cnt
        
         LET l_num = 0
         SELECT SUM(CASE
                    WHEN COALESCE(sfba013,0) - COALESCE(sfba015,0) - COALESCE(sfba016,0) < 0
                      THEN 0
                    ELSE
                      COALESCE(sfba013,0) - COALESCE(sfba015,0) - COALESCE(sfba016,0)
                 END)
           INTO l_num
           FROM sfba_t
          WHERE sfbaent = g_enterprise
            AND sfbadocno = g_detail[g_detail_idx].sfaadocno
         CASE
            #發放：無發料單、入庫單
            WHEN l_sfdc_cnt = 0 AND l_sfec_cnt = 0
               LET g_detail[l_ac].stus = 'F'
            
            #部分發料：有發料單 AND 備料有欠料
            WHEN l_sfdc_cnt >= 1 AND l_num >= 1
               LET g_detail[l_ac].stus = 'PF'
               
            #全部發料：有發料單 AND 備料無欠料 AND 無入庫單
           #WHEN l_sfdc_cnt >= 1 AND l_num = 0                      #151014 Sarah mark
            WHEN l_sfdc_cnt >= 1 AND l_num = 0 AND l_sfec_cnt = 0   #151014 Sarah mod
               LET g_detail[l_ac].stus = 'AF'
               
            #部分入庫：有入庫單 AND 入庫數量 < 生產數量
            WHEN l_sfec_cnt >= 1 AND
                 (g_detail[l_ac].sfaa050 + g_detail[l_ac].sfaa051) < g_detail[l_ac].sfaa012
               LET g_detail[l_ac].stus = 'PS'
            #全部入庫：有入庫單 AND 入庫數量 >= 生產數量
            WHEN l_sfec_cnt >= 1 AND
                 (g_detail[l_ac].sfaa050 + g_detail[l_ac].sfaa051) >= g_detail[l_ac].sfaa012
               LET g_detail[l_ac].stus = 'AS'
         END CASE
      ELSE
