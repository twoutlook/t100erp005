PRIVATE FUNCTION asfq009_has_child(p_sfaadocno,p_bf_sfaadocno)
DEFINE   p_sfaadocno    LIKE sfaa_t.sfaadocno
DEFINE   p_bf_sfaadocno LIKE sfaa_t.sfaadocno
DEFINE   li_cnt         LIKE type_t.num5
