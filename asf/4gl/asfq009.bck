# Descriptions...: 寻找该母工单的子工单
# Memo...........:
# Usage..........: CALL asfq009_get_child(p_sfaadocno,p_bf_sfaadocno)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By lixh
# Modify.........:
################################################################################
PRIVATE FUNCTION asfq009_get_child(p_sfaadocno,p_bf_sfaadocno)
DEFINE   p_sfaadocno    LIKE sfaa_t.sfaadocno
DEFINE   p_bf_sfaadocno LIKE sfaa_t.sfaadocno
DEFINE   l_sql          STRING
DEFINE   l_cnt          LIKE type_t.num5
DEFINE   l_i            LIKE type_t.num5
DEFINE   l_n            LIKE type_t.num5
DEFINE   r_sfaa         DYNAMIC ARRAY OF RECORD
                  sfaa021      LIKE sfaa_t.sfaa021,
                  sfaadocno    LIKE sfaa_t.sfaa021,
                  sfaa025      LIKE sfaa_t.sfaa025
         END RECORD 

#   LET l_sql = " SELECT '','','','','','','','','',sfaadocno,sfaadocdt,sfaa003,sfaa010,t1.imaal003,t1.imaal004,sfaa019,sfaa020,sfaa002,t2.ooag011,
#                        sfaa068,t3.ooefl003,sfaa012,sfaa049,sfaa071,sfaa050,sfaa051,sfaa021,sfaa025,sfaa013,sfaa046,sfaastus,sfaa011 FROM sfaa_t ",
#               #語系
#               "   LEFT JOIN imaal_t t1 ON t1.imaalent = '"||g_enterprise||"' AND t1.imaal001 = sfaa010 AND t1.imaal002 = '"||g_dlang||"' ",
#               "   LEFT JOIN ooag_t t2 ON t2.ooagent = '"||g_enterprise||"' AND t2.ooag001 = sfaa002 ",
#               "   LEFT JOIN ooefl_t t3 ON t3.ooeflent = '"||g_enterprise||"' AND t3.ooefl001 = sfaa068 AND t3.ooefl002 = '"||g_dlang||"' ",
#               "  WHERE sfaaent = ",g_enterprise,
#               "    AND sfaadocno = ? "
#               
#   LET l_sql = cl_sql_add_mask(l_sql)              #遮蔽特定資料
#   PREPARE asfq009_get_child_pre FROM l_sql
#   DECLARE asfq009_get_child_cs CURSOR FOR asfq009_get_child_pre 
      
   LET l_sql = " SELECT sfaa021,sfaadocno,sfaa025 FROM sfaa_t ", 
               "  WHERE sfaaent = ",g_enterprise,
               "    AND sfaa021 = '",p_sfaadocno,"'",
               "    AND sfaa025 = '",p_bf_sfaadocno,"'"   
   LET l_sql = cl_sql_add_mask(l_sql)              #遮蔽特定資料
   PREPARE asfq009_get_child_pre FROM l_sql
   DECLARE asfq009_get_child_cs CURSOR FOR asfq009_get_child_pre 
   
   LET l_i = 1
   CALL r_sfaa.clear()
   #在FOREACH中直接使用遞迴,資料會錯亂,所以先將資料放到陣列後,在FOR迴圈處理
   FOREACH asfq009_get_child_cs INTO r_sfaa[l_i].sfaa021,r_sfaa[l_i].sfaadocno,r_sfaa[l_i].sfaa025
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF   
      LET l_i = l_i + 1
   END FOREACH   
   CALL r_sfaa.deleteelement(l_i)  #刪除FOREACH最後新增的空白列
   LET l_i = l_i - 1
   IF l_i >0 THEN
      FOR l_n = 1 TO l_i
         SELECT '','','','','','','','','',sfaadocno,sfaadocdt,sfaa003,sfaa010,t1.imaal003,t1.imaal004,sfaa019,sfaa020,sfaa002,t2.ooag011,
                                 sfaa068,t3.ooefl003,sfaa012,sfaa049,sfaa071,sfaa050,sfaa051,sfaa021,sfaa025,sfaa013,sfaa046,sfaastus,sfaa011 
           INTO l_sfaa2.name,l_sfaa2.pid,l_sfaa2.id,l_sfaa2.exp,l_sfaa2.isnode,l_sfaa2.isExp,l_sfaa2.expcode, 
                l_sfaa2.progress,l_sfaa2.stus,l_sfaa2.sfaadocno,l_sfaa2.sfaadocdt,l_sfaa2.sfaa003,l_sfaa2.sfaa010,
                l_sfaa2.sfaa010_desc,l_sfaa2.sfaa010_desc_1,l_sfaa2.sfaa019,l_sfaa2.sfaa020,l_sfaa2.sfaa002,l_sfaa2.sfaa002_desc,
                l_sfaa2.sfaa068,l_sfaa2.sfaa068_desc,l_sfaa2.sfaa012,l_sfaa2.sfaa049,l_sfaa2.sfaa071,l_sfaa2.sfaa050, 
                l_sfaa2.sfaa051,l_sfaa2.sfaa021,l_sfaa2.sfaa025,l_sfaa2.sfaa013,l_sfaa2.sfaa046,l_sfaa2.sfaastus,l_sfaa2.sfaa011 
           FROM sfaa_t           
           LEFT JOIN imaal_t t1 ON t1.imaalent = g_enterprise AND t1.imaal001 = sfaa010 AND t1.imaal002 = g_dlang
           LEFT JOIN ooag_t t2 ON t2.ooagent = g_enterprise AND t2.ooag001 = sfaa002 
           LEFT JOIN ooefl_t t3 ON t3.ooeflent = g_enterprise AND t3.ooefl001 = sfaa068 AND t3.ooefl002 = g_dlang
          WHERE sfaaent = g_enterprise
            AND sfaadocno = r_sfaa[l_n].sfaadocno      

         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
    
         END IF
         LET l_cnt = 0
         SELECT COUNT(*) INTO l_cnt FROM asfq009_sfaa_tmp
          WHERE sfaadocno = l_sfaa2.sfaadocno
         IF cl_null(l_cnt) THEN LET l_cnt = 0 END IF  
         IF l_cnt = 0 THEN  #写入临时表
            INSERT INTO asfq009_sfaa_tmp(name,pid,id,exp,isnode,isExp,expcode, 
                                         progress,stus,sfaadocno,sfaadocdt,sfaa003,sfaa010,
                                         sfaa010_desc,sfaa010_desc_1,sfaa019,sfaa020,sfaa002,sfaa002_desc,
                                         sfaa068,sfaa068_desc,sfaa012,sfaa049,sfaa071,sfaa050, 
                                         sfaa051,sfaa021,sfaa025,sfaa013,sfaa046,sfaastus,sfaa011)
                 VALUES (l_sfaa2.name,l_sfaa2.pid,l_sfaa2.id,l_sfaa2.exp,l_sfaa2.isnode,l_sfaa2.isExp,l_sfaa2.expcode, 
                         l_sfaa2.progress,l_sfaa2.stus,l_sfaa2.sfaadocno,l_sfaa2.sfaadocdt,l_sfaa2.sfaa003,l_sfaa2.sfaa010,
                         l_sfaa2.sfaa010_desc,l_sfaa2.sfaa010_desc_1,l_sfaa2.sfaa019,l_sfaa2.sfaa020,l_sfaa2.sfaa002,l_sfaa2.sfaa002_desc,
                         l_sfaa2.sfaa068,l_sfaa2.sfaa068_desc,l_sfaa2.sfaa012,l_sfaa2.sfaa049,l_sfaa2.sfaa071,l_sfaa2.sfaa050, 
                         l_sfaa2.sfaa051,l_sfaa2.sfaa021,l_sfaa2.sfaa025,l_sfaa2.sfaa013,l_sfaa2.sfaa046,l_sfaa2.sfaastus,l_sfaa2.sfaa011)      
         END IF
         LET l_cnt = 0
         SELECT COUNT(*) INTO l_cnt FROM sfaa_t
          WHERE sfaaent = g_enterprise
            AND sfaa025 = l_sfaa2.sfaadocno   
         IF cl_null(l_cnt) THEN LET l_cnt = 0 END IF
         IF l_cnt > 0 THEN         
            CALL asfq009_get_child(p_sfaadocno,l_sfaa2.sfaadocno)
         END IF   
      END FOR
   END IF    
END FUNCTION

