################################################################################
# Descriptions...: 寻找子工单
# Memo...........:
# Usage..........: CALL asfq009_tree_expand_2(p_id,p_sfaadocno,p_bf_sfaadocno)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By lixh
# Modify.........:
################################################################################
PRIVATE FUNCTION asfq009_tree_expand_2(p_id,p_sfaadocno,p_bf_sfaadocno)
DEFINE  p_id           LIKE type_t.num5
DEFINE  p_sfaadocno    LIKE sfaa_t.sfaadocno
DEFINE  p_bf_sfaadocno LIKE sfaa_t.sfaadocno
DEFINE  l_sql          STRING
DEFINE  li_ac          LIKE type_t.num5 
DEFINE  l_cnt          LIKE type_t.num5
DEFINE  l_gzze003      LIKE gzze_t.gzze003
DEFINE  l_sfaa_4         DYNAMIC ARRAY OF RECORD
                   sfaa021      LIKE sfaa_t.sfaa021,
                   sfaadocno    LIKE sfaa_t.sfaa021,
                   sfaa025      LIKE sfaa_t.sfaa025
         END RECORD 
        
   CALL l_sfaa_4.clear()
   CALL g_sfaa.clear()
   #展子工单
   CALL asfq009_tree_single_desc(l_ac,p_sfaadocno,p_bf_sfaadocno)    
   #                                  母工單       前工單   
   
   FOR li_ac = 1 TO g_sfaa.getlength()
       LET l_sfaa_4[li_ac].* = g_sfaa[li_ac].*
   END FOR   
   LET l_cnt = p_id   
   #LET l_ac = l_ac + 1
   FOR li_ac = 1 TO g_sfaa.getlength()
      LET l_ac = l_ac + 1
      EXECUTE b_fill_curs6 USING l_sfaa_4[li_ac].sfaadocno
                            INTO g_detail[l_ac].*                            
      #LET g_detail[l_ac].pid  = l_cnt
      CALL asfq009_detail_show("'1'")
      LET l_gzze003 = cl_getmsg('asf-00758',g_dlang)
      LET g_detail[l_ac].name = l_gzze003,l_sfaa_4[li_ac].sfaadocno        
      LET g_detail[l_ac].pid = g_detail[l_cnt].id
      LET g_detail[l_ac].id  = g_detail[l_ac].pid,'.', li_ac USING "<<<"
      LET g_detail[l_ac].exp  = TRUE
      LET g_detail[l_ac].isExp  = TRUE
      LET g_detail[l_ac].expcode  = g_detail[l_cnt].expcode + 1
      LET g_detail[l_ac].isnode = asfq009_has_child(p_sfaadocno,l_sfaa_4[li_ac].sfaadocno)   #是否有子节点  
      IF g_detail[l_ac].isnode THEN 
         CALL asfq009_tree_expand_2(l_ac,p_sfaadocno,l_sfaa_4[li_ac].sfaadocno)
      END IF
   END FOR   

END FUNCTION

 
