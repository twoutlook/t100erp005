               EXECUTE p350_02_pmdo_prep USING l_sfeb_02.pmdt001,l_sfeb_02.sfeb004,l_sfeb_02.sfeb005 INTO l_sfeb_02.pmdt002,l_sfeb_02.pmdt003,l_sfeb_02.pmdt004,l_sfeb_02.pmdt036   #160812-00005#1 pmdt007 mode pmdt036 
