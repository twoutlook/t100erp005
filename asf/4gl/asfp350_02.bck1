     FOREACH p350_02_curs1 USING l_sfeb_01.sfeb004,l_sfeb_01.sfeb005 INTO l_sfeb001,l_sfeb023,l_pmdt001,l_pmdt002,l_pmdt003,l_pmdt004
         LET l_sfeb_02.sfebseq = l_sfeb_01.sfebseq
