      LET g_errparam.extend = ''
      LET g_errparam.code   = 'asf-00230'  #無資料處理!
      LET g_errparam.popup  = TRUE
      CALL cl_err()   
      RETURN FALSE
   ELSE 
      RETURN TRUE   
   END IF

END FUNCTION

