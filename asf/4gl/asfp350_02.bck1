                 #LET l_sum2 = 0
                 #IF NOT cl_null(l_sfeb_01.sfeb010) AND NOT cl_null(l_sfec010) THEN
                 #   CALL s_aooi250_convert_qty(l_sfeb_01.sfeb004,l_sfec010,l_sfeb_01.sfeb010,l_sfec011)
                 #     RETURNING l_success,l_sum2
                 #   IF NOT l_success THEN
