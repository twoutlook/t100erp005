                  LET g_errparam.extend = "sfib_t"
                  LET g_errparam.popup = TRUE
                  CALL cl_err()

                  LET r_success = FALSE
                  RETURN r_success
               END IF
            ELSE
               #無維護群組
               #更新上站作業+上站制程序對應下站程序+下站制程序為本資料的本站程序+本站制程序
               UPDATE sfib_t SET sfib007=l_sfib.sfib001,sfib008=l_sfib.sfib002
