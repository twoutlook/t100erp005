# Memo...........:
# Usage..........: CALL asft800_sfkb_desc(p_ac)
# Input parameter: 
# Return code....: 
# Date & Author..: 
# Modify.........: 160908-00047#1
################################################################################
PRIVATE FUNCTION asft800_sfkb_desc(p_ac)
DEFINE p_ac         LIKE type_t.num5
DEFINE l_sfaa013    LIKE sfaa_t.sfaa013
DEFINE l_sfab007    LIKE sfab_t.sfab007
DEFINE l_sql        STRING
DEFINE l_xmdd001    LIKE xmdd_t.xmdd001
DEFINE l_success    LIKE type_t.num5

   #訂單
   IF g_sfkg2_d[p_ac].sfkb001 ='2' THEN
      IF NOT cl_null(g_sfkg2_d[p_ac].sfkb002) AND NOT cl_null(g_sfkg2_d[p_ac].sfkb003) AND NOT cl_null(g_sfkg2_d[p_ac].sfkb004) AND NOT cl_null(g_sfkg2_d[p_ac].sfkb005) THEN
         SELECT xmdd001,xmdd011,xmdd006,xmdd004,xmda004,pmaal004,xmda003,ooefl003,xmda002,ooag011
           INTO l_xmdd001,g_sfkg2_d[p_ac].a0,g_sfkg2_d[p_ac].a1,g_sfkg2_d[p_ac].a2,
                g_sfkg2_d[p_ac].a6,g_sfkg2_d[p_ac].a6_desc,
                g_sfkg2_d[p_ac].a7,g_sfkg2_d[p_ac].a7_desc,
                g_sfkg2_d[p_ac].a8,g_sfkg2_d[p_ac].a8_desc
           FROM xmdd_t,xmda_t
           LEFT JOIN pmaal_t ON pmaalent=xmdaent AND pmaal001=pmaal004 AND pmaal002=g_dlang
           LEFT JOIN ooefl_t ON ooeflent=xmdaent AND ooefl001=xmda003 AND ooefl002=g_dlang
           LEFT JOIN ooag_t ON ooagent=xmdaent AND ooag001=xmda002
          WHERE xmdaent=xmddent AND xmdadocno=xmdddocno
            AND xmddent=g_enterprise AND xmddsite=g_site AND xmdd001=g_sfka_m.sfka010
            AND xmdddocno=g_sfkg2_d[p_ac].sfkb002 AND xmddseq=g_sfkg2_d[p_ac].sfkb003
            AND xmddseq1=g_sfkg2_d[p_ac].sfkb004 AND xmddseq2=g_sfkg2_d[p_ac].sfkb005
         
         LET g_sfkg2_d[p_ac].a4=g_sfka_m.sfka013
         CALL s_aooi250_convert_qty(l_xmdd001,g_sfkg2_d[p_ac].a2,g_sfkg2_d[p_ac].a4,g_sfkg2_d[p_ac].a1) 
              RETURNING l_success,g_sfkg2_d[p_ac].a3
         IF NOT l_success THEN
            LET g_sfkg2_d[p_ac].a3 = g_sfkg2_d[p_ac].a1
         END IF
         
         #已开工单数量
         LET l_sql = " SELECT sfaa013,sfab007 FROM sfaa_t,sfab_t ",
                     "  WHERE sfaaent=sfabent AND sfaadocno=sfabdocno",
                     "    AND sfaaent=",g_enterprise," AND sfaasite='",g_site,"'",
                     "    AND sfaa010='",g_sfka_m.sfka010,"' AND sfaastus != 'X'",
                     "    AND sfab002='",g_sfkg2_d[p_ac].sfkb002,"' AND sfab003=",g_sfkg2_d[p_ac].sfkb003,
                     "    AND sfab004=",g_sfkg2_d[p_ac].sfkb004," AND sfab005=",g_sfkg2_d[p_ac].sfkb005
         PREPARE asft800_qty2_pre FROM l_sql
         DECLARE asft800_qty2_cs CURSOR FOR asft800_qty2_pre
         LET g_sfkg2_d[p_ac].a5 = 0
         FOREACH asft800_qty2_cs INTO l_sfaa013,l_sfab007
            IF SQLCA.sqlcode THEN
               EXIT FOREACH
            END IF
            CALL s_aooi250_convert_qty(g_sfka_m.sfka010,l_sfaa013,g_sfkg2_d[p_ac].a4,l_sfab007) 
                 RETURNING l_success,l_sfab007
            LET g_sfkg2_d[p_ac].a5 = g_sfkg2_d[p_ac].a5 + l_sfab007
         END FOREACH
     END IF
  END IF
END FUNCTION
