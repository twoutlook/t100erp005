# Descriptions...: 同項次的部位+作業代號+作業序+發料料號為相同,不同項次不可以有同樣的部位+作業代號+作業序+發料料號
# Memo...........:
# Usage..........: CALL asft800_sfkgseq_2(p_cmd)
#                  RETURNING r_success
# Input parameter: p_cmd       狀態
# Return code....: r_success   TRUE/FALSE
# Date & Author..: 2017/01/16 By shiun   #160908-00025#1
# Modify.........: 
################################################################################
PRIVATE FUNCTION asft800_sfkgseq_2(p_cmd)
DEFINE p_cmd     LIKE type_t.chr1
DEFINE l_n       LIKE type_t.num5
DEFINE l_n1      LIKE type_t.num5

  IF cl_null(g_sfkg_d[g_detail_idx].sfkgseq) THEN
     RETURN TRUE
  END IF 
  IF g_sfkg_d[g_detail_idx].sfkg002 IS NULL THEN
     RETURN TRUE
  END IF 
  IF g_sfkg_d[g_detail_idx].sfkg003 IS NULL THEN
     RETURN TRUE
  END IF
  IF g_sfkg_d[g_detail_idx].sfkg004 IS NULL THEN
     RETURN TRUE
  END IF
  IF g_sfkg_d[g_detail_idx].sfkg006 IS NULL THEN
     RETURN TRUE
  END IF
  SELECT COUNT(1) INTO l_n FROM sfkg_t WHERE sfkgent = g_enterprise AND sfkgsite = g_site AND sfkgdocno = g_sfka_m.sfkadocno
     AND sfkg900 = g_sfka_m.sfka900 AND sfkgseq = g_sfkg_d[g_detail_idx].sfkgseq
  IF (l_n > 1 AND p_cmd = 'u') OR (l_n > 0 AND p_cmd = 'a') THEN
     SELECT COUNT(1) INTO l_n1 FROM sfkg_t WHERE sfkgent = g_enterprise AND sfkgsite = g_site 
        AND sfkgdocno = g_sfka_m.sfkadocno  AND sfkg900 = g_sfka_m.sfka900
        AND sfkgseq = g_sfkg_d[g_detail_idx].sfkgseq AND sfkg002 = g_sfkg_d[g_detail_idx].sfkg002 AND sfkg003 = g_sfkg_d[g_detail_idx].sfkg003
        AND sfkg004 = g_sfkg_d[g_detail_idx].sfkg004 AND sfkg006 = g_sfkg_d[g_detail_idx].sfkg006
     IF l_n1 = 0 THEN
        INITIALIZE g_errparam TO NULL
        LET g_errparam.code = 'asf-00827'
        LET g_errparam.extend = ''
        LET g_errparam.popup = TRUE
        CALL cl_err()

        RETURN FALSE
     END IF
  ELSE
     SELECT COUNT(1) INTO l_n1 FROM sfkg_t WHERE sfkgent = g_enterprise AND sfkgsite = g_site 
        AND sfkgdocno = g_sfka_m.sfkadocno  AND sfkg900 = g_sfka_m.sfka900
        AND sfkg002 = g_sfkg_d[g_detail_idx].sfkg002 AND sfkg003 = g_sfkg_d[g_detail_idx].sfkg003
        AND sfkg004 = g_sfkg_d[g_detail_idx].sfkg004 AND sfkg006 = g_sfkg_d[g_detail_idx].sfkg006
     IF l_n1 > 0 THEN
        INITIALIZE g_errparam TO NULL
        LET g_errparam.code = 'asf-00827'
        LET g_errparam.extend = ''
        LET g_errparam.popup = TRUE
        CALL cl_err()

        RETURN FALSE
     END IF
  END IF
  RETURN TRUE
END FUNCTION

 
