#      AND sfcadocno = g_sfka_m.sfkadocno
#   IF l_sum > 1 THEN
#      INITIALIZE g_errparam TO NULL
#      LET g_errparam.code = 'asf-00726'
#      LET g_errparam.extend = g_sfka_m.sfkadocno
#      LET g_errparam.popup = TRUE
#      CALL cl_err()
#      RETURN FALSE
#   END IF
#   #160511-00021#1-e
#161109-00039#1-e mark

   RETURN TRUE
