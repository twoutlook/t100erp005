################################################################################
# Descriptions...: 工单单号检查
# Memo...........:
# Usage..........: CALL asft800_sfkadocno_chk()
#                  RETURNING TRUE/FALSE
# Input parameter: 
# Return code....: 
# Date & Author..: 
# Modify.........: 160908-00047#1
################################################################################
PRIVATE FUNCTION asft800_sfkadocno_chk()
DEFINE l_sfaa  RECORD
    sfaadocno  LIKE sfaa_t.sfaadocno,
    sfaa003    LIKE sfaa_t.sfaa003,  #160727-00025#2
    sfaa047    LIKE sfaa_t.sfaa047,
    sfaa048    LIKE sfaa_t.sfaa048
           END RECORD
DEFINE l_n          LIKE type_t.num5

   INITIALIZE l_sfaa.* TO NULL
   SELECT sfaadocno,sfaa003,sfaa047,sfaa048  #160727-00025#2 add sfaa003
     INTO l_sfaa.sfaadocno,l_sfaa.sfaa003,l_sfaa.sfaa047,l_sfaa.sfaa048  #160727-00025#2 add sfaa003
     FROM sfaa_t
    WHERE sfaaent = g_enterprise
      AND sfaadocno = g_sfka_m.sfkadocno
      AND sfaastus NOT IN ('X','N')
      AND sfaasite = g_site  #160907-00058#1

   #是否存在工单
   IF cl_null(l_sfaa.sfaadocno) THEN
      INITIALIZE g_errparam TO NULL
      #160727-00025#2-s
      IF g_argv[01] = '4' THEN
         #不存在符合條件的模具工單！
         LET g_errparam.code = 'asf-00775'
      ELSE
      #160727-00025#2-e
         #不存在符合條件的工單！
         LET g_errparam.code = 'asf-00002'
      END IF  #160727-00025#2
      LET g_errparam.extend = g_sfka_m.sfkadocno
      LET g_errparam.popup = TRUE
      CALL cl_err()
      RETURN FALSE
   END IF
   
   #此工單已存在生管結案日或者成本結案日，不可變更
   IF NOT cl_null(l_sfaa.sfaa047) OR NOT cl_null(l_sfaa.sfaa048) THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'asf-00293'
      LET g_errparam.extend = g_sfka_m.sfkadocno
      LET g_errparam.popup = TRUE
      CALL cl_err()
      RETURN FALSE
   END IF
   
   #此工單已存在未確認的工單變更單
   LET l_n = 0
   SELECT COUNT(1) INTO l_n FROM sfka_t
