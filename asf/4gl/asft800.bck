################################################################################
# Descriptions...: 依據傳入的參數而:
#                  1. 計算同項次其項序不為0的數據筆數
#                  2. 加總同項次其項序不為0的總應發數
# Memo...........: #160120-00020#1 --- add
# Usage..........: CALL asft800_sfkg_cnt(p_type)
#                  RETURNING r_n1
#                            r_n2
# Input parameter: p_type   決定計算或加總
# Return code....: r_n1     回傳計算數值
#                  r_n2     回傳加總數值
# Date & Author..: 2016/01/29 By Ann_Huang
# Modify.........:
################################################################################
PRIVATE FUNCTION asft800_sfkg_cnt(p_type)
DEFINE p_type     LIKE type_t.chr10
DEFINE r_n1       LIKE type_t.num5
DEFINE r_n2       LIKE sfkg_t.sfkg013

   LET r_n1 = 0
   LET r_n2 = 0
   
   IF p_type ='count' THEN     #計算同項次其項序不為0的數據筆數
      SELECT COUNT(1) INTO r_n1
      FROM sfkg_t
      WHERE  sfkgent = g_enterprise
         AND sfkgsite = g_site
         AND sfkgdocno = g_sfka_m.sfkadocno
         AND sfkgseq = g_sfkg_d[l_ac].sfkgseq
         AND sfkgseq1 > 0
         
      IF cl_null(r_n1) THEN   LET r_n1 = 0  END IF
      RETURN r_n1
   END IF

   #若有取替代資料時，主要備料的QPA 應由  (主要備料的標準應發+ 所有替代料的總應發 ) 來計算，否則後續在算套數時會有問題 
   IF p_type ='sum' THEN               
      SELECT SUM(sfkg013/sfkg022) INTO r_n2
      FROM sfkg_t 
         INNER JOIN sfka_t
            ON sfkgent = sfkaent  and sfkgsite = sfkasite  and sfkgdocno = sfkadocno  and sfkg900 = sfka900
      WHERE   sfkgent = g_enterprise
          AND sfkgsite = g_site
          AND sfkgdocno = g_sfka_m.sfkadocno
          AND sfkg900 = g_sfka_m.sfka900     #變更序
          AND sfkgseq = g_sfkg_d[l_ac].sfkgseq
          AND sfkgseq1 > 0
          
      IF cl_null(r_n2) THEN   LET r_n2 = 0  END IF
      RETURN r_n2
   END IF

END FUNCTION

################################################################################
# Descriptions...: 檢查該項次中發料料號,是否已發料過
# Memo...........: 
# Usage..........: CALL asft800_sfkg006_chk(p_type,p_cmd)
#                  RETURNING TRUE/FALSE
# Input parameter: p_type   決定檢查BOM料號或發料料號
#                   1. 檢查BOM料號  (存在sfdc)
#                   2. 檢查發料料號 (存在sfdd)
# Return code....: 
# Date & Author..: #160216-00007#1 By Ann_Huang
# Modify.........: #160908-00047#1 add p_cmd
################################################################################
PRIVATE FUNCTION asft800_sfkg006_chk(p_type,p_cmd)
DEFINE p_type       LIKE type_t.chr1
DEFINE p_cmd        LIKE type_t.chr1
DEFINE l_n          LIKE type_t.num5
DEFINE l_item       LIKE sfkg_t.sfkg006
DEFINE l_success    LIKE type_t.num5
DEFINE l_slip       LIKE ooba_t.ooba002
DEFINE l_para       LIKE type_t.chr1
DEFINE l_bmba       DYNAMIC ARRAY OF RECORD
    bmba001         LIKE bmba_t.bmba001,     #bom相关资料都可以通过回传的key值抓取
    bmba002         LIKE bmba_t.bmba002,
    bmba003         LIKE bmba_t.bmba003,
    bmba004         LIKE bmba_t.bmba004,
    bmba005         DATETIME YEAR TO SECOND,
    bmba007         LIKE bmba_t.bmba007,
    bmba008         LIKE bmba_t.bmba008,
    bmba035         LIKE bmba_t.bmba035,     #保稅否 
    l_bmba011       LIKE bmba_t.bmba011,     #QPA 分子，对应于原始的主件料号
    l_bmba012       LIKE bmba_t.bmba012,     #QPA 分母，对应于原始的主件料号
    l_inam002       LIKE inam_t.inam002      #元件对应特征
                END RECORD

   LET l_n = 0
   CASE p_type
      WHEN '1'  #BOM 料號
         IF NOT asft800_sfkgseq(p_cmd) THEN
            RETURN FALSE
         END IF
         #按工單單號+項次,找尋該BOM料號是否已經發料 扣除作廢且申請數量>0
         SELECT COUNT(sfdc004) INTO l_n
           FROM sfda_t,sfdc_t
          WHERE sfdcent = sfdaent
            AND sfdcsite = sfdasite
            AND sfdcdocno = sfdadocno
            AND sfdcent = g_enterprise
            AND (sfdastus <> 'X' AND sfdc007 >0)  #扣除作廢且申請數量>0
            AND sfdc001 = g_sfka_m.sfkadocno      #工單單號
            AND sfdc002 = g_sfkg_d[l_ac].sfkgseq  #工單-項次
            AND sfdc004 = g_sfkg_d_t.sfkg005      #舊值BOM料號
         IF l_n > 0 THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = 'asf-00318'
            LET g_errparam.extend = g_sfkg_d_t.sfkg005
            LET g_errparam.popup = TRUE
            CALL cl_err()
            RETURN FALSE
         END IF
         LET l_item = g_sfkg_d[l_ac].sfkg005
      WHEN '2'  #發料料號
         #160908-00025#1-s-add
         IF NOT asft800_sfkgseq_2(p_cmd) THEN
            RETURN FALSE
         END IF
         #160908-00025#1-e-add
         #按工單單號+項次,找尋該料號是否已經發料 扣除作廢且數量>0
         SELECT COUNT(sfdd001) INTO l_n
           FROM sfda_t,sfdc_t,sfdd_t
          WHERE sfdcent = sfdaent
            AND sfdcsite = sfdasite
            AND sfdcdocno = sfdadocno
            AND sfdcent = g_enterprise
            AND (sfdastus <> 'X' AND sfdd007 >0)  #扣除作廢且數量>0
            AND sfdc001 = g_sfka_m.sfkadocno      #工單單號
            AND sfddent = sfdcent
            AND sfdddocno = sfdcdocno
            AND sfddseq   = sfdcseq
            AND sfdc002 = g_sfkg_d[l_ac].sfkgseq  #工單-項次
            AND sfdd001 = g_sfkg_d_t.sfkg006      #舊值料號
         IF l_n > 0 THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = 'asf-00318'
            LET g_errparam.extend = g_sfkg_d_t.sfkg006
            LET g_errparam.popup = TRUE
            CALL cl_err()
            RETURN FALSE
         END IF
         LET l_item = g_sfkg_d[l_ac].sfkg006
   END CASE
   
   INITIALIZE g_chkparam.* TO NULL
   LET g_chkparam.arg1 = l_item
   IF NOT cl_chk_exist("v_imaa001") THEN
      RETURN FALSE
   END IF
   IF NOT cl_chk_exist("v_imaf001_1") THEN
      RETURN FALSE
   END IF
   
   CALL s_aooi200_get_slip(g_sfka_m.sfkadocno) RETURNING l_success,l_slip
   LET l_para = cl_get_doc_para(g_enterprise,g_site,l_slip,'D-MFG-0024')
   IF l_para != 2 THEN
      CALL s_asft300_02_bom(0,g_sfka_m.sfka010,g_sfka_m.sfka011,g_sfka_m.sfka013,1,1,g_sfka_m.sfka015,'Y',l_item,g_sfkg_d[l_ac].sfkg021,'',g_sfkg_d[l_ac].sfkg033)
           RETURNING l_bmba
      IF l_bmba.getLength() = 0 THEN
         CASE l_para
            WHEN '1'  #单别对应参数设置为1时，不存在bom时报错重新输入
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 'asf-00334'
               LET g_errparam.extend = l_item
               LET g_errparam.popup = TRUE
               CALL cl_err()
               RETURN FALSE
            WHEN '3'  #单别对应参数设置为3时，不存在bom时警告
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 'asf-00335'
               LET g_errparam.extend = l_item
               LET g_errparam.popup = TRUE
               CALL cl_err()
         END CASE
      END IF
   END IF
   
   RETURN TRUE
END FUNCTION

