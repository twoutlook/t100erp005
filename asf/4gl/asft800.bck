################################################################################
# Descriptions...: 新增取替代後，要推算原發料數量的總應發數量
# Memo...........: #160905-00022#2
# Usage..........: CALL asft800_calc_rep_sub_sfba013()
#                     RETURNING l_bmea011,l_bmea012,l_bmea016,r_sfkg013
# Input parameter: 
# Return code....: l_bmea011   取替代量
#                : l_bmea012   元件底數
#                : l_bmea016   替代方式(1.部分取替代 2.完全取替代)
#                : r_sfkg013   計算後總應發數量
# Date & Author..: 16/09/06 By Sarah
# Modify.........: 
################################################################################
PRIVATE FUNCTION asft800_calc_rep_sub_sfba013()
DEFINE l_flag                LIKE type_t.chr1
DEFINE l_n                   LIKE type_t.num5
DEFINE l_n1                  LIKE type_t.num5
DEFINE l_bmea011             LIKE bmea_t.bmea011
DEFINE l_bmea012             LIKE bmea_t.bmea012
DEFINE l_bmea014             LIKE bmea_t.bmea014
DEFINE l_bmea016             LIKE bmea_t.bmea016
DEFINE l_bmea017             LIKE bmea_t.bmea017
DEFINE l_sfkg013             LIKE sfkg_t.sfkg013
DEFINE r_sfkg013             LIKE sfkg_t.sfkg013

   LET l_sfkg013 = 0
   LET r_sfkg013 = 0
   LET l_bmea016 = ''
   LET l_bmea011 = 1
   LET l_bmea012 = 1
#底下程式架構從asft300_02.4gl中複製過來的
   #先判断是否是全主件替代
   LET l_flag = 'N'  #全主件替代判断标识位
   LET l_n = 0
   SELECT COUNT(1) INTO l_n
     FROM bmea_t
    WHERE bmeaent = g_enterprise AND bmeasite = g_site 
      AND bmea001 = 'ALL' AND bmea003 = g_sfkg_d[l_ac].sfkg005 AND bmea008 = g_sfkg_d[l_ac].sfkg006
   IF l_n = 0 THEN
      LET l_flag = 'Y'
      LET l_n = 0
      SELECT COUNT(1) INTO l_n
        FROM bmea_t
       WHERE bmeaent = g_enterprise AND bmeasite = g_site
         AND bmea001 = g_sfkg_d[l_ac].sfkg001 AND bmea002 = g_sfka_m.sfka011
         AND bmea003 = g_sfkg_d[l_ac].sfkg005 AND bmea004 = g_sfkg_d[l_ac].sfkg002
         AND bmea005 = g_sfkg_d[l_ac].sfkg003 AND bmea006 = g_sfkg_d[l_ac].sfkg004
         AND bmea008 = g_sfkg_d[l_ac].sfkg006
         AND bmea019 = g_sfkg_d[l_ac].sfkg021
   END IF
   IF l_n > 1 THEN
      SELECT bmea011,bmea012,bmea014,bmea016,bmea017 INTO l_bmea011,l_bmea012,l_bmea014,l_bmea016,l_bmea017
        FROM bmea_t
       WHERE bmeaent = g_enterprise AND bmeasite = g_site
         AND bmea001 = g_sfkg_d[l_ac].sfkg001 AND bmea002 = g_sfka_m.sfka011
         AND bmea003 = g_sfkg_d[l_ac].sfkg005 AND bmea004 = g_sfkg_d[l_ac].sfkg002
         AND bmea005 = g_sfkg_d[l_ac].sfkg003 AND bmea006 = g_sfkg_d[l_ac].sfkg004
         AND bmea008 = g_sfkg_d[l_ac].sfkg006 AND bmea007 = '2'
         AND bmea019 = g_sfkg_d[l_ac].sfkg021
      #替代料有限定客戶,需检查
      IF l_bmea014 = 'Y' AND NOT cl_null(g_sfka_m.sfka009) THEN
         LET l_n1 = 0
         SELECT COUNT(1) INTO l_n1 FROM bmeb_t
          WHERE bmebent = g_enterprise AND bmebsite = g_site
            AND bmeb001 = g_sfkg_d[l_ac].sfkg001 AND bmeb002 = g_sfka_m.sfka011
            AND bmeb003 = g_sfkg_d[l_ac].sfkg005 AND bmeb004 = g_sfkg_d[l_ac].sfkg002
            AND bmeb005 = g_sfkg_d[l_ac].sfkg003 AND bmeb006 = g_sfkg_d[l_ac].sfkg004
            AND bmeb008 = g_sfkg_d[l_ac].sfkg006 AND bmeb007 = '2' AND bmeb009 = g_sfka_m.sfka009
            AND bmeb010 = g_sfkg_d[l_ac].sfkg021
         IF l_n1 = 0 THEN
            RETURN l_bmea011,l_bmea012,l_bmea016,r_sfkg013
         END IF
      END IF
   ELSE
      IF l_flag = 'N' THEN
         SELECT bmea011,bmea012,bmea014,bmea016,bmea017 INTO l_bmea011,l_bmea012,l_bmea014,l_bmea016,l_bmea017
           FROM bmea_t
          WHERE bmeaent = g_enterprise AND bmeasite = g_site
            AND bmea001 = 'ALL' AND bmea003 = g_sfkg_d[l_ac].sfkg005
            AND bmea008 = g_sfkg_d[l_ac].sfkg006 AND bmea019 = g_sfkg_d[l_ac].sfkg021
         #替代料有限定客戶,需检查
         IF l_bmea014 = 'Y' AND NOT cl_null(g_sfka_m.sfka009) THEN
            LET l_n1 = 0
            SELECT COUNT(1) INTO l_n1
              FROM bmeb_t
             WHERE bmebent = g_enterprise AND bmebsite = g_site 
               AND bmeb001 = 'ALL' AND bmeb003 = g_sfkg_d[l_ac].sfkg005 
               AND bmeb008 = g_sfkg_d[l_ac].sfkg006  AND bmeb009 = g_sfka_m.sfka009
               AND bmeb010 = g_sfkg_d[l_ac].sfkg021
            IF l_n1 = 0 THEN
               RETURN l_bmea011,l_bmea012,l_bmea016,r_sfkg013
            END IF
         END IF
      ELSE
         SELECT bmea011,bmea012,bmea014,bmea016,bmea017 INTO l_bmea011,l_bmea012,l_bmea014,l_bmea016,l_bmea017
           FROM bmea_t
          WHERE bmeaent = g_enterprise AND bmeasite = g_site
            AND bmea001 = g_sfkg_d[l_ac].sfkg001 AND bmea002 = g_sfka_m.sfka011
            AND bmea003 = g_sfkg_d[l_ac].sfkg005 AND bmea004 = g_sfkg_d[l_ac].sfkg002
            AND bmea005 = g_sfkg_d[l_ac].sfkg003 AND bmea006 = g_sfkg_d[l_ac].sfkg004
            AND bmea008 = g_sfkg_d[l_ac].sfkg006
            AND bmea019 = g_sfkg_d[l_ac].sfkg021
         #替代料有限定客戶,需检查
         IF l_bmea014 = 'Y' AND NOT cl_null(g_sfka_m.sfka009) THEN
            LET l_n1 = 0
            SELECT COUNT(1) INTO l_n1
              FROM bmeb_t
             WHERE bmebent = g_enterprise AND bmebsite = g_site
               AND bmeb001 = g_sfkg_d[l_ac].sfkg001 AND bmeb002 = g_sfka_m.sfka011
               AND bmeb003 = g_sfkg_d[l_ac].sfkg005 AND bmeb004 = g_sfkg_d[l_ac].sfkg002
               AND bmeb005 = g_sfkg_d[l_ac].sfkg003 AND bmeb006 = g_sfkg_d[l_ac].sfkg004
               AND bmeb008 = g_sfkg_d[l_ac].sfkg006 AND bmeb009 = g_sfka_m.sfka009
               AND bmeb010 = g_sfkg_d[l_ac].sfkg021
            IF l_n1 = 0 THEN
               RETURN l_bmea011,l_bmea012,l_bmea016,r_sfkg013
            END IF
         END IF
      END IF
   END IF

   #抓取原本BOM發料料號那筆的總應發數量
   SELECT sfba013 INTO l_sfkg013
     FROM sfba_t
    WHERE sfbaent = g_enterprise AND sfbaite = g_site
      AND sfbadocno = g_sfka_m.sfkadocno
      AND sfbaseq = g_sfkg_d[l_ac].sfkgseq
      AND sfbaseq1 = 0   
   
   #替代料為完全替代時，應要控制全部的應發量都要轉換為替代料數量
   IF l_bmea016 = '2' THEN
      LET r_sfkg013 = l_sfkg013
   END IF

   #替代料为部分替代时，替代料的數量不可超过上限比率
   IF l_bmea016 = '1' THEN
      LET r_sfkg013 = l_sfkg013 * l_bmea017 / 100
   END IF

   RETURN l_bmea011,l_bmea012,l_bmea016,r_sfkg013
END FUNCTION

################################################################################
# Descriptions...: 新增取替代後，要推算原發料數量的總應發數量
# Memo...........: 
# Usage..........: CALL asft800_upd_sfkg023()
#                     RETURNING TRUE/FALSE
# Input parameter: 
# Return code....: 
# Date & Author..: #160905-00022#2 By Whitney
# Modify.........: 
################################################################################
PRIVATE FUNCTION asft800_upd_sfkg023()
DEFINE l_sfkg010    LIKE sfkg_t.sfkg010
DEFINE l_sfkg011    LIKE sfkg_t.sfkg011
DEFINE l_sfkg013    LIKE sfkg_t.sfkg013
DEFINE l_sfkg023    LIKE sfkg_t.sfkg023
DEFINE l_sfba013    LIKE sfba_t.sfba013

   LET l_sfkg013 = 0
   LET l_sfkg023 = 0
   SELECT sfkg013,sfkg023 INTO l_sfkg013,l_sfkg023 FROM sfkg_t
    WHERE sfkgent = g_enterprise AND sfkgsite = g_site AND sfkgdocno = g_sfka_m.sfkadocno
      AND sfkgseq = g_sfkg_d[g_detail_idx].sfkgseq AND sfkgseq1 = 0 AND sfkg900 = g_sfka_m.sfka900
   #重算QPA分子分母
   CALL asft800_sfkg023_qpa(l_sfkg023) RETURNING l_sfkg010,l_sfkg011
   #取替代
   IF g_sfkg_d[g_detail_idx].sfkgseq1 > 0 THEN
      LET l_sfkg013 = 0
      SELECT SUM(sfkg013/sfkg022) INTO l_sfkg013 FROM sfkg_t
       WHERE sfkgent = g_enterprise AND sfkgsite = g_site AND sfkgdocno = g_sfka_m.sfkadocno
         AND sfkgseq = g_sfkg_d[g_detail_idx].sfkgseq AND sfkg900 = g_sfka_m.sfka900
         AND sfkgseq1 <> 0 AND sfkgseq1 <> g_sfkg_d[g_detail_idx].sfkgseq1
      IF cl_null(l_sfkg013) THEN LET l_sfkg013 = 0 END IF
      UPDATE sfkg_t
         SET sfkg013 = sfkg023 + sfkg024 - l_sfkg013 - g_sfkg_d[g_detail_idx].sfkg013 / g_sfkg_d[g_detail_idx].sfkg022,
             sfkg010 = l_sfkg010,
             sfkg011 = l_sfkg011,
             sfkg901 = '2'
       WHERE sfkgent = g_enterprise AND sfkgsite = g_site AND sfkgdocno = g_sfka_m.sfkadocno
         AND sfkgseq = g_sfkg_d[g_detail_idx].sfkgseq AND sfkgseq1 = 0 AND sfkg900 = g_sfka_m.sfka900
      IF SQLCA.SQLcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "UPDATE sfkg_t",SQLERRMESSAGE
         LET g_errparam.popup = TRUE
         CALL cl_err()
         RETURN FALSE
      END IF
      LET l_sfba013 = 0
      SELECT sfba013 INTO l_sfba013  FROM sfba_t 
       WHERE sfbaent = g_enterprise AND sfbasite = g_site AND sfbadocno = g_sfka_m.sfkadocno
         AND sfbaseq = g_sfkg_d[g_detail_idx].sfkgseq AND sfbaseq1 = 0
      IF (l_sfkg013 IS NOT NULL AND (l_sfkg013 != l_sfba013 OR l_sfba013 IS NULL)) OR
         (l_sfkg013 IS NULL AND l_sfba013 IS NOT NULL) THEN
      #其說明的SQL
         LET g_sfkh007 = ""
         IF NOT s_asft800_ins_sfkh(g_sfka_m.sfkadocno,g_sfkg_d[g_detail_idx].sfkgseq,0,g_sfka_m.sfka900,'sfba013',g_sfkh003,l_sfba013,l_sfkg013,g_sfkh007) THEN
         END IF
      ELSE
         IF NOT s_asft800_del_sfkh(g_sfka_m.sfkadocno,g_sfkg_d[g_detail_idx].sfkgseq,0,g_sfka_m.sfka900,'sfba013') THEN
         END IF
      END IF
   ELSE
      LET l_sfkg013 = 0
      SELECT SUM(sfkg013/sfkg022) INTO l_sfkg013 FROM sfkg_t
       WHERE sfkgent = g_enterprise AND sfkgsite = g_site AND sfkgdocno = g_sfka_m.sfkadocno
         AND sfkgseq = g_sfkg_d[g_detail_idx].sfkgseq AND sfkgseq1 <> 0 AND sfkg900 = g_sfka_m.sfka900
      IF cl_null(l_sfkg013) THEN LET l_sfkg013 = 0 END IF
      UPDATE sfkg_t
         SET sfkg013 = sfkg023 + sfkg024 - l_sfkg013,
             sfkg010 = l_sfkg010,
             sfkg011 = l_sfkg011
             #sfkg901 = '2'      #170214-00040#1
       WHERE sfkgent = g_enterprise AND sfkgsite = g_site AND sfkgdocno = g_sfka_m.sfkadocno
         AND sfkgseq = g_sfkg_d[g_detail_idx].sfkgseq AND sfkgseq1 = 0 AND sfkg900 = g_sfka_m.sfka900
      IF SQLCA.SQLcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "UPDATE sfkg_t",SQLERRMESSAGE
         LET g_errparam.popup = TRUE
         CALL cl_err()
         RETURN FALSE
      END IF
   END IF

   RETURN TRUE
END FUNCTION

