      IF NOT cl_null(g_no_entry) THEN
         CALL cl_set_comp_entry(g_no_entry,FALSE)
      END IF
      #add-point:set_no_entry段欄位控制 name="set_no_entry.field_control"
      
      #end add-point 
   END IF 
   
   IF p_cmd = 'u' THEN  #docno,ld欄位確認是絕對關閉
      CALL cl_set_comp_entry("sfkadocno",FALSE)
   END IF 
 
#  IF p_cmd = 'u' THEN  #docdt欄位依照設定關閉(FALSE則為設定不同意修正) #(ver:78)
      IF NOT cl_chk_update_docdt() THEN
         CALL cl_set_comp_entry("",FALSE)
      END IF
#  END IF 
   
   #add-point:set_no_entry段欄位控制後 name="set_no_entry.after_control"
   #160908-00047#1-s
   #有單身資料時，工單來源、來源單號項次項序分批序、工單來源頁籤，限制不可修改，以免已存在的單身資料與單頭不合
   LET l_n = 0
   SELECT COUNT(1) INTO l_n FROM sfkg_t
    WHERE sfkgent=g_enterprise AND sfkgsite=g_site AND sfkgdocno=g_sfka_m.sfkadocno
   IF l_n > 0 THEN
      CALL cl_set_comp_entry("sfka005,sfka006,sfka007,sfka008,sfka063,l_sfka007",FALSE)
   END IF
