   #170103-00063#1-s
   #IF NOT cl_null(g_sfkg_d[l_ac].sfkg006) THEN
   IF NOT cl_null(g_sfkg_d[l_ac].sfkg006) AND g_sfkg_d[l_ac].sfkg023 <> 0 THEN
   #170103-00063#1-e
      #建议应发数量
      CALL s_asft300_03(g_sfkg_d[l_ac].sfkg006,g_sfkg_d[l_ac].sfkg013) RETURNING l_success,l_sfkg013
      IF l_success THEN
         LET g_sfkg_d[l_ac].sfkg013=l_sfkg013
