#同项次的部位+作业编号+制程序+BOM料号为相同,不同项次不可以有同样的部位+作业编号+制程序+BOM料号
PRIVATE FUNCTION asft800_sfkgseq(p_cmd)
DEFINE p_cmd     LIKE type_t.chr1
DEFINE l_n       LIKE type_t.num5
DEFINE l_n1      LIKE type_t.num5

  IF cl_null(g_sfkg_d[g_detail_idx].sfkgseq) THEN
     RETURN TRUE
  END IF 
  IF g_sfkg_d[g_detail_idx].sfkg002 IS NULL THEN
     RETURN TRUE
  END IF 
  IF g_sfkg_d[g_detail_idx].sfkg003 IS NULL THEN
     RETURN TRUE
  END IF
  IF g_sfkg_d[g_detail_idx].sfkg004 IS NULL THEN
     RETURN TRUE
  END IF
  IF g_sfkg_d[g_detail_idx].sfkg005 IS NULL THEN
     RETURN TRUE
  END IF
  SELECT COUNT(1) INTO l_n FROM sfkg_t WHERE sfkgent = g_enterprise AND sfkgsite = g_site AND sfkgdocno = g_sfka_m.sfkadocno
     AND sfkg900 = g_sfka_m.sfka900 AND sfkgseq = g_sfkg_d[g_detail_idx].sfkgseq
  IF (l_n > 1 AND p_cmd = 'u') OR (l_n > 0 AND p_cmd = 'a') THEN
     SELECT COUNT(1) INTO l_n1 FROM sfkg_t WHERE sfkgent = g_enterprise AND sfkgsite = g_site 
        AND sfkgdocno = g_sfka_m.sfkadocno  AND sfkg900 = g_sfka_m.sfka900
        AND sfkgseq = g_sfkg_d[g_detail_idx].sfkgseq AND sfkg002 = g_sfkg_d[g_detail_idx].sfkg002 AND sfkg003 = g_sfkg_d[g_detail_idx].sfkg003
        AND sfkg004 = g_sfkg_d[g_detail_idx].sfkg004 AND sfkg005 = g_sfkg_d[g_detail_idx].sfkg005
     IF l_n1 = 0 THEN
        INITIALIZE g_errparam TO NULL
        LET g_errparam.code = 'asf-00070'
        LET g_errparam.extend = ''
        LET g_errparam.popup = TRUE
        CALL cl_err()

        RETURN FALSE
     END IF
  ELSE
     SELECT COUNT(1) INTO l_n1 FROM sfkg_t WHERE sfkgent = g_enterprise AND sfkgsite = g_site 
        AND sfkgdocno = g_sfka_m.sfkadocno  AND sfkg900 = g_sfka_m.sfka900
        AND sfkg002 = g_sfkg_d[g_detail_idx].sfkg002 AND sfkg003 = g_sfkg_d[g_detail_idx].sfkg003
        AND sfkg004 = g_sfkg_d[g_detail_idx].sfkg004 AND sfkg005 = g_sfkg_d[g_detail_idx].sfkg005
     IF l_n1 > 0 THEN
        INITIALIZE g_errparam TO NULL
        LET g_errparam.code = 'asf-00070'
        LET g_errparam.extend = ''
        LET g_errparam.popup = TRUE
        CALL cl_err()

        RETURN FALSE
     END IF
  END IF
  RETURN TRUE
END FUNCTION

#重新计算QPA
PRIVATE FUNCTION asft800_sfkg023_qpa(p_sfkg023)
DEFINE p_sfkg023         LIKE sfkg_t.sfkg023
DEFINE r_sfkg010         LIKE sfkg_t.sfkg010
DEFINE r_sfkg011         LIKE sfkg_t.sfkg011
DEFINE l_num1            LIKE sfaa_t.sfaa012
DEFINE l_num2            LIKE sfaa_t.sfaa012
DEFINE l_num1_t          LIKE type_t.num20
DEFINE l_num2_t          LIKE type_t.num20
DEFINE l_j               LIKE type_t.num10
DEFINE l_k               LIKE type_t.num10

   LET l_num1 = g_sfka_m.sfka012
   LET l_num2 = p_sfkg023
   LET l_num1_t = l_num1
   LET l_num2_t = l_num2
   WHILE TRUE
     #存在小数情况           
     IF l_num1 = l_num1_t AND l_num2 = l_num2_t THEN
        EXIT WHILE
     ELSE
        LET l_num1 = l_num1 * 10
        LET l_num1_t = l_num1
        LET l_num2 = l_num2 * 10
        LET l_num2_t = l_num2
        CONTINUE WHILE
     END IF
   END WHILE
      
   #QPA分子分母约分计算
   IF l_num1 < l_num2 THEN
      LET l_j = l_num1_t
   ELSE
      LET l_j = l_num2_t
   END IF
   FOR l_k = l_j TO 1 STEP -1
      IF (l_num1_t MOD l_k = 0) AND (l_num2_t MOD l_k = 0) THEN
         EXIT FOR
      END IF
   END FOR
   
   LET r_sfkg010 = l_num2_t / l_k
   LET r_sfkg011 = l_num1_t / l_k
   #170103-00063#1-s
   IF r_sfkg010 = 0 AND r_sfkg011 = 0 THEN
      LET r_sfkg011 = 1
   END IF
   #170103-00063#1-e
   RETURN r_sfkg010,r_sfkg011
END FUNCTION

