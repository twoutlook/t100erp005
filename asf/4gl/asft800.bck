PRIVATE FUNCTION asft800_gen()

   #變更序=採購變更單的變更序最大值+1
   SELECT MAX(sfka900)+1 INTO g_sfka_m.sfka900
     FROM sfka_t
    WHERE sfkaent = g_enterprise
      AND sfkadocno = g_sfka_m.sfkadocno
   IF cl_null(g_sfka_m.sfka900) OR g_sfka_m.sfka900 = 0 THEN
      LET g_sfka_m.sfka900 = 1
   END IF
   
   #变更单单头资料，sfka_t
   IF NOT asft800_gen_sfka() THEN
      RETURN FALSE
   END IF
   
   #变更单工单来源页签资料，sfkb
   IF NOT asft800_gen_sfkb() THEN
      RETURN FALSE
   END IF
   
   #变更单工单生產料號明細页签资料，sfkc
   IF NOT asft800_gen_sfkc() THEN
      RETURN FALSE
   END IF
   
   #变更单工单商品序号页签资料，sfkf
   IF NOT asft800_gen_sfkf() THEN
      RETURN FALSE
   END IF
   
   #变更单工单備料檔页签资料，sfkg
   IF NOT asft800_gen_sfkg() THEN
      RETURN FALSE
   END IF
   
   #160727-00025#2-s
   IF g_argv[01] ='4' THEN
      #变更单工单模具页签资料，sfki
      IF NOT asft800_gen_sfki() THEN
         RETURN FALSE
      END IF
   END IF
   #160727-00025#2-e
   
   RETURN TRUE
END FUNCTION

