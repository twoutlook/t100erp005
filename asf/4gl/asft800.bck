      CALL cl_set_comp_entry("sfka005,sfka006,sfka007,sfka008,sfka063,l_sfka007",FALSE)
   END IF
   #當來源單號或母工單的來源單號為訂單時，自動帶出訂單的客戶，且不能修改。若是其他的單據則自行輸入
   IF g_sfka_m.sfka005 = '2' THEN
      CALL cl_set_comp_entry("sfka009",FALSE)
   END IF
   IF g_sfka_m.sfka005 = '4' THEN
      LET l_sfaa005 = ''
      SELECT sfaa005 INTO l_sfaa005 FROM sfaa_t
       WHERE sfaaent=g_enterprise AND sfaasite=g_site AND sfaadocno=g_sfka_m.sfka021
      IF l_sfaa005 = '2' THEN
         CALL cl_set_comp_entry("sfka009",FALSE)
      END IF
   END IF
   IF g_sfka_m.sfka005 NOT MATCHES '[01]' THEN
      CALL cl_set_comp_entry('sfka010,sfka011',FALSE)
   END IF
   IF g_sfka_m.sfka005 = '1' THEN
      CALL cl_set_comp_entry("sfka012",FALSE)
   END IF
   IF g_sfka_m.sfka057 != '2' THEN
      CALL cl_set_comp_entry("sfka018",FALSE)
   END IF
   IF g_sfka_m.sfka003 != '5' THEN
      CALL cl_set_comp_entry("sfka025",FALSE)
   END IF
   IF cl_null(g_sfka_m.sfka060) THEN
      CALL cl_set_comp_entry('sfka058',FALSE)
   END IF
   CALL cl_set_comp_entry("sfka900",FALSE)
   #160908-00047#1-e
   
   #end add-point 
 
END FUNCTION
 
