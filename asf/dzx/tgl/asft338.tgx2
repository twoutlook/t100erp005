      #end add-point 
      
      #將遮罩欄位還原
      CALL asft338_sffd_t_mask_restore('restore_mask_o')
               
      UPDATE sffd_t 
         SET (sffddocno,
              sffdseq1,sffd001
              ,sffd002,sffd003) 
