         UPDATE sfia_t SET (sfiamodid,sfiamoddt) = (g_sfia_m.sfiamodid,g_sfia_m.sfiamoddt)
          WHERE sfiaent = g_enterprise AND sfiadocno = g_sfiadocno_t
 
      END IF
    
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
