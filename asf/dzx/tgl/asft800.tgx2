   {<point name="browser_fill.before_browser_fill"/>}
   #end add-point
   
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   LET l_wc  = g_wc.trim() 
   LET l_wc2 = g_wc2.trim()
 
   #add-point:browser_fill,foreach前 name="browser_fill.before_foreach"
   {<point name="browser_fill.before_foreach"/>}
   #end add-point
   
   IF g_wc2 <> " 1=1" THEN
      #單身有輸入搜尋條件                      
      LET l_sub_sql = " SELECT DISTINCT sfkadocno,sfka900 ",
                      " FROM sfka_t ",
                      " ",
                      " LEFT JOIN sfkg_t ON sfkgent = sfkaent AND sfkadocno = sfkgdocno AND sfka900 = sfkg900 ", "  ",
                      #add-point:browser_fill段sql(sfkg_t1) name="browser_fill.cnt.join.}"
                      {<point name="browser_fill.cnt.join.}"/>}
                      #end add-point
                      " LEFT JOIN sfkb_t ON sfkbent = sfkaent AND sfkadocno = sfkbdocno AND sfka900 = sfkb900", "  ",
                      #add-point:browser_fill段sql(sfkb_t1) name="browser_fill.cnt.join.sfkb_t1"
                      {<point name="browser_fill.cnt.join.sfkb_t1"/>}
                      #end add-point
 
                      " LEFT JOIN sfkc_t ON sfkcent = sfkaent AND sfkadocno = sfkcdocno AND sfka900 = sfkc900", "  ",
                      #add-point:browser_fill段sql(sfkc_t1) name="browser_fill.cnt.join.sfkc_t1"
                      {<point name="browser_fill.cnt.join.sfkc_t1"/>}
                      #end add-point
 
                      " LEFT JOIN sfkf_t ON sfkfent = sfkaent AND sfkadocno = sfkfdocno AND sfka900 = sfkf900", "  ",
                      #add-point:browser_fill段sql(sfkf_t1) name="browser_fill.cnt.join.sfkf_t1"
                      {<point name="browser_fill.cnt.join.sfkf_t1"/>}
                      #end add-point
 
 
 
                      " ", 
                      " ", 
                      " ",                      
 
                      " ",                      
 
                      " ",                      
 
 
 
