  <field cite_std="N" name="sfaa_t.sfaa005" ver="1" src="s" column="sfaa005" table="sfaa_t" attribute="" type="" req="Y" i_zoom="" c_zoom="" chk_ref="" items="4009" default="0" max_compare="" max="" min_compare="" min="" can_edit="Y" can_query="Y" widget="ComboBox" status="">
    <![CDATA[工單來源：
