  <field cite_std="N" name="sfaa_t.sfaa002" ver="1" src="s" column="sfaa002" table="sfaa_t" attribute="" type="" req="Y" i_zoom="q_ooag001_2" c_zoom="q_ooag00_2" chk_ref="" items="" default="" max_compare="" max="" min_compare="" min="" can_edit="Y" can_query="Y" widget="ButtonEdit" status="">
    <![CDATA[生管人員：
