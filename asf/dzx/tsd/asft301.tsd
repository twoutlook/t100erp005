  <field cite_std="N" name="sfcb_t.sfcb012" ver="1" src="s" column="sfcb012" table="sfcb_t" attribute="C001" type="varchar2(1)" req="N" i_zoom="" c_zoom="" chk_ref="" items="" default="N" max="" min="" can_edit="Y" can_query="Y" widget="CheckBox" status="">
    <![CDATA[允許委外：
