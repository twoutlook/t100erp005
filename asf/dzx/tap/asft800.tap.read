    <![CDATA[
#工单来源检查
PRIVATE FUNCTION asft800_sfkb002_chk()
DEFINE l_sql         STRING
DEFINE l_n           LIKE type_t.num5


  #當等於訂單、計畫等，檢查是否存在來源資料裡
  IF NOT cl_null(g_sfkg2_d[l_ac].sfkb002) THEN
     #订单
     IF g_sfkg2_d[l_ac].sfkb001 = '2' THEN
        LET l_sql = "SELECT COUNT(*) FROM xmda_t,xmdd_t WHERE xmdaent=xmddent AND xmdadocno=xmdddocno ",
                    "   AND xmddent='",g_enterprise,"' AND xmddsite='",g_site,"'",
                    "   AND xmdddocno='",g_sfkg2_d[l_ac].sfkb002,"' AND xmdd001='",g_sfka_m.sfka010,"'",
                    "   AND xmdastus = 'Y'"
        
        IF NOT cl_null(g_sfkg2_d[l_ac].sfkb003) THEN
           LET l_sql = l_sql," AND xmddseq=", g_sfkg2_d[l_ac].sfkb003
        END IF
        IF NOT cl_null(g_sfkg2_d[l_ac].sfkb004) THEN
           LET l_sql = l_sql," AND xmddseq1=", g_sfkg2_d[l_ac].sfkb004
        END IF
        IF NOT cl_null(g_sfkg2_d[l_ac].sfkb005) THEN
           LET l_sql = l_sql," AND xmddseq2=", g_sfkg2_d[l_ac].sfkb005
        END IF        
        PREPARE asft800_sfkb002_pre1 FROM l_sql
        EXECUTE asft800_sfkb002_pre1 INTO l_n
     END IF 

     IF l_n = 0 THEN
        INITIALIZE g_errparam TO NULL 
        LET g_errparam.extend = g_sfkg2_d[l_ac].sfkb002
        LET g_errparam.code   = "asf-00154" 
        LET g_errparam.popup  = TRUE 
        CALL cl_err()
        RETURN FALSE
     END IF
  ELSE
     RETURN FALSE
  END IF
  
  CALL asft800_sfkb_desc(l_ac)
  IF g_sfkg2_d[l_ac].a3 - g_sfkg2_d[l_ac].a5 <= 0 THEN
     INITIALIZE g_errparam TO NULL 
     LET g_errparam.extend = g_sfkg2_d[l_ac].sfkb002
     LET g_errparam.code   = "asf-00336" 
     LET g_errparam.popup  = TRUE 
     CALL cl_err()
     RETURN FALSE
  END IF
  LET g_sfkg2_d[l_ac].sfkb007 = g_sfkg2_d[l_ac].a3 - g_sfkg2_d[l_ac].a5
  DISPLAY BY NAME g_sfkg2_d[l_ac].*
  RETURN TRUE
  
END FUNCTION]]>
