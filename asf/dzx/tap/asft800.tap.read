    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION asft800_b_fill()
   #add-point:b_fill段define(客製用)
   {<point name="b_fill.define_customerization" edit="c" mark="Y"/>}
   #end add-point     
   DEFINE p_wc2      STRING
