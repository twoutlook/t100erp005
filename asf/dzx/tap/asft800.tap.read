   #end add-point
   
   #清空第一階單身
   CALL g_sfkg_d.clear()
   CALL g_sfkg2_d.clear()
   CALL g_sfkg3_d.clear()
   CALL g_sfkg4_d.clear()
   CALL g_sfkg5_d.clear()
   CALL g_sfkg6_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF asft800_fill_chk(1) THEN
      #切換上下筆時不重組SQL
      IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
         LET g_sql = "SELECT  UNIQUE sfkgseq,sfkgseq1,sfkg002,sfkg003,sfkg004,sfkg005,sfkg022,sfkg006, 
             sfkg021,sfkg007,sfkg008,sfkg009,sfkg010,sfkg011,sfkg012,sfkg023,sfkg024,sfkg013,sfkg014, 
             sfkg015,sfkg016,sfkg025,sfkg017,sfkg018,sfkg019,sfkg020,sfkg029,sfkg030,sfkg028,sfkg001, 
             sfkg026,sfkg027,sfkg901,sfkg904,sfkg905,sfkgsite ,t1.oocql004 ,t2.oocql004 ,t3.imaal003 , 
             t4.imaal003 ,t5.imecl005 ,t6.oocal003 ,t7.inaa002 ,t8.inab003 ,t9.imaal003 ,t10.oocql004 FROM sfkg_t", 
                
                     " INNER JOIN sfka_t ON sfkadocno = sfkgdocno ",
                     " AND sfka900 = sfkg900 ",
 
                     #"",
                     
                     "",
                                    " LEFT JOIN oocql_t t1 ON t1.oocqlent='"||g_enterprise||"' AND t1.oocql001='215' AND t1.oocql002=sfkg002 AND t1.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t2 ON t2.oocqlent='"||g_enterprise||"' AND t2.oocql001='221' AND t2.oocql002=sfkg003 AND t2.oocql003='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t3 ON t3.imaalent='"||g_enterprise||"' AND t3.imaal001=sfkg005 AND t3.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t4 ON t4.imaalent='"||g_enterprise||"' AND t4.imaal001=sfkg006 AND t4.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imecl_t t5 ON t5.imeclent='"||g_enterprise||"' AND t5.imecl003=sfkg021 AND t5.imecl004='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t6 ON t6.oocalent='"||g_enterprise||"' AND t6.oocal001=sfkg014 AND t6.oocal002='"||g_dlang||"' ",
               " LEFT JOIN inaa_t t7 ON t7.inaaent='"||g_enterprise||"' AND t7.inaasite=sfkgsite AND t7.inaa001=sfkg019  ",
               " LEFT JOIN inab_t t8 ON t8.inabent='"||g_enterprise||"' AND t8.inab001=sfkg019 AND t8.inab002=sfkg020  ",
               " LEFT JOIN imaal_t t9 ON t9.imaalent='"||g_enterprise||"' AND t9.imaal001=sfkg001 AND t9.imaal002='"||g_dlang||"' ",
