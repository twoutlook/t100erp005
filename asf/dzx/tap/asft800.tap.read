  <point name="function.asft800_sfkg023_qpa" order="46" ver="3" cite_std="N" new="Y" ch="" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
#重新计算QPA
PRIVATE FUNCTION asft800_sfkg023_qpa(p_sfkg023)
DEFINE p_sfkg023         LIKE sfkg_t.sfkg023
DEFINE r_sfkg010         LIKE sfkg_t.sfkg010
DEFINE r_sfkg011         LIKE sfkg_t.sfkg011
DEFINE l_num1            LIKE sfaa_t.sfaa012
DEFINE l_num2            LIKE sfaa_t.sfaa012
DEFINE l_num1_t          LIKE type_t.num20
DEFINE l_num2_t          LIKE type_t.num20
DEFINE l_j               LIKE type_t.num10
DEFINE l_k               LIKE type_t.num10

   LET l_num1 = g_sfka_m.sfka012
   LET l_num2 = p_sfkg023
   LET l_num1_t = l_num1
   LET l_num2_t = l_num2
   WHILE TRUE
     #存在小数情况           
     IF l_num1 = l_num1_t AND l_num2 = l_num2_t THEN
        EXIT WHILE
     ELSE
        LET l_num1 = l_num1 * 10
        LET l_num1_t = l_num1
        LET l_num2 = l_num2 * 10
        LET l_num2_t = l_num2
        CONTINUE WHILE
     END IF
   END WHILE
      
   #QPA分子分母约分计算
   IF l_num1 < l_num2 THEN
      LET l_j = l_num1_t
   ELSE
      LET l_j = l_num2_t
   END IF
   FOR l_k = l_j TO 1 STEP -1
      IF (l_num1_t MOD l_k = 0) AND (l_num2_t MOD l_k = 0) THEN
         EXIT FOR
      END IF
   END FOR
   
   LET r_sfkg010 = l_num2_t / l_k
   LET r_sfkg011 = l_num1_t / l_k
   RETURN r_sfkg010,r_sfkg011
END FUNCTION]]>
