    <![CDATA[
#订单时chk数量不可大于剩余可生产数量,
#不可小於已入庫量（已入庫合格量+已入庫不合格量）
PRIVATE FUNCTION asft800_sfka012_chk()
DEFINE l_sql         STRING
DEFINE l_xmda007     LIKE xmda_t.xmda007
DEFINE l_xmda008     LIKE xmda_t.xmda008
DEFINE l_xmdd        RECORD LIKE xmdd_t.*
DEFINE l_success     LIKE type_t.num5
DEFINE l_rate        LIKE type_t.num5
DEFINE l_sum         LIKE sfka_t.sfka012
DEFINE l_num         LIKE sfka_t.sfka012
DEFINE l_num1        LIKE sfka_t.sfka012
DEFINE l_xmdddocno   LIKE xmdd_t.xmdddocno
DEFINE l_xmddseq     LIKE xmdd_t.xmddseq
DEFINE l_xmddseq1    LIKE xmdd_t.xmddseq1
DEFINE l_xmddseq2    LIKE xmdd_t.xmddseq2
DEFINE l_sfaa012     LIKE sfaa_t.sfaa012
DEFINE l_sfke001_sum LIKE sfke_t.sfke001

  #订单时chk数量不可大于剩余可生产数量
  IF g_sfka_m.sfka005 ='2' THEN
     IF NOT cl_null(g_sfka_m.sfka006) AND NOT cl_null(g_sfka_m.sfka007) AND NOT cl_null(g_sfka_m.sfka008) AND NOT cl_null(g_sfka_m.sfka063) THEN
        SELECT xmda007,xmda008,xmdd_t.* INTO l_xmda007,l_xmda008,l_xmdd.* FROM xmda_t,xmdd_t 
         WHERE xmdaent=xmddent AND xmdadocno=xmdddocno
           AND xmddent=g_enterprise AND xmddsite=g_site
           AND xmdddocno=g_sfka_m.sfka006 AND xmddseq=g_sfka_m.sfka007
           AND xmddseq1=g_sfka_m.sfka008 AND xmddseq2=g_sfka_m.sfka063       
        
        #此笔工单原始数量
        SELECT sfaa012 INTO l_sfaa012 FROM sfaa_t WHERE sfaaent=g_enterprise AND sfaasite=g_site AND sfaadocno=g_sfka_m.sfkadocno
        IF cl_null(l_sfaa012) THEN
           LET l_sfaa012 = 0
        END IF
        
       #CALL s_aimi190_get_convert(g_sfka_m.sfka010,l_xmdd.xmdd006,g_sfka_m.sfka013) RETURNING l_success,l_rate
       #IF l_success THEN
       #   LET l_xmdd.xmdd006 = l_xmdd.xmdd006 * l_rate
       #END IF
        CALL s_aooi250_convert_qty(g_sfka_m.sfka010,l_xmdd.xmdd004,g_sfka_m.sfka013,l_xmdd.xmdd006) 
             RETURNING l_success,l_xmdd.xmdd006
        #已开工单量计算
        CALL asft800_sfka012_chk_qty(g_sfka_m.sfka006,g_sfka_m.sfka007,g_sfka_m.sfka008,g_sfka_m.sfka063,g_sfka_m.sfka013) RETURNING l_sum
            
        IF g_sfka_m.sfka012 > l_xmdd.xmdd006 - l_sum + l_sfaa012 THEN
           INITIALIZE g_errparam TO NULL
           LET g_errparam.code = 'asf-00337'
           LET g_errparam.extend = ''
           LET g_errparam.popup = TRUE
           CALL cl_err()

           RETURN FALSE
        END IF
        #预先订单对应已开工单数量也纳入计算
        IF l_xmda007 = '2' AND NOT cl_null(l_xmda008) THEN
           LET l_sql = "SELECT xmdddocno,xmddseq,xmddseq1,xmddseq2 FROM xmdd_t,xmda_t WHERE xmdaent=xmddent AND xmdasite=xmddsite AND xmdadocno=xmdddocno",
                      "   AND xmddent=",g_enterprise," AND xmddsite='",g_site,"' AND xmdadocno='",l_xmda008,"'",
                      "   AND xmdd001='",g_sfka_m.sfka010,"' AND xmdastus='Y'"
           PREPARE asft800_01_pre_2 FROM l_sql
           DECLARE asft800_01_cs_2 CURSOR FOR asft800_01_pre_2
           LET l_num = 0
           FOREACH asft800_01_cs_2 INTO l_xmdddocno,l_xmddseq,l_xmddseq1,l_xmddseq2
              IF SQLCA.sqlcode THEN
                 INITIALIZE g_errparam TO NULL
                 LET g_errparam.code = SQLCA.sqlcode
                 LET g_errparam.extend = "FOREACH:"
                 LET g_errparam.popup = TRUE
                 CALL cl_err()

                 EXIT FOREACH
              END IF
              CALL asft800_sfka012_chk_qty(l_xmdddocno,l_xmddseq,l_xmddseq1,l_xmddseq2,g_sfka_m.sfka013) RETURNING l_num1
              LET l_num = l_num + l_num1
           END FOREACH
           IF g_sfka_m.sfka012 > l_xmdd.xmdd006 - l_sum + l_sfaa012 - l_num THEN
              INITIALIZE g_errparam TO NULL
           LET g_errparam.code = 'asf-00337'
           LET g_errparam.extend = ''
           LET g_errparam.popup = TRUE
           CALL cl_err()

              RETURN FALSE
           END IF
        END IF  
     END IF
  END IF
  
  #不可小於已入庫量（已入庫合格量+已入庫不合格量）
  IF NOT cl_null(g_sfka_m.sfka050) AND NOT cl_null(g_sfka_m.sfka051) THEN
     IF g_sfka_m.sfka012 < g_sfka_m.sfka050 + g_sfka_m.sfka051 THEN
        INITIALIZE g_errparam TO NULL
        LET g_errparam.code = 'asf-00378'
        LET g_errparam.extend = ''
        LET g_errparam.popup = TRUE
        CALL cl_err()
        RETURN FALSE
     END IF
  END IF
  
  #不可小于计划完工日期数量加总
  SELECT SUM(sfke001) INTO l_sfke001_sum FROM sfke_t 
   WHERE sfkeent = g_enterprise AND sfkesite = g_site 
     AND sfkedocno = p_sfkadocno AND sfke900 = p_sfka900
  IF g_sfka_m.sfka012 < l_sfke001_sum THEN
     INITIALIZE g_errparam TO NULL
     LET g_errparam.code = 'asf-00164'
     LET g_errparam.extend = ''
     LET g_errparam.popup = TRUE
     CALL cl_err()
     RETURN FALSE
  END IF
  RETURN TRUE
END FUNCTION]]>
  </point>
