               IF NOT cl_null(g_sfka_m.sfka006) AND NOT cl_null(g_sfka_m.sfka007) AND NOT cl_null(g_sfka_m.sfka008) AND NOT cl_null(g_sfka_m.sfka063)
               AND (cl_null(l_sfaa006) OR cl_null(l_sfaa007) OR cl_null(l_sfaa008) OR cl_null(l_sfaa063)) THEN
               #mod zhangllc 151102 --end
                  DELETE FROM sfkb_t WHERE sfkbent=g_enterprise AND sfkbsite=g_site AND sfkbdocno=g_sfka_m.sfkadocno AND sfkb900=g_sfka_m.sfka900
                  INSERT INTO sfkb_t(sfkbent,sfkbsite,sfkbdocno,sfkbseq,sfkb001,sfkb002,sfkb003,sfkb004,sfkb005,sfkb006,sfkb007,sfkb900,sfkb901,sfkb902,sfkb905,sfkb906)
