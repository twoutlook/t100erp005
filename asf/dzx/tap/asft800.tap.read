  <point name="statechange.before_menu" order="" ver="9" cite_std="N" new="Y" ch="" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[      CALL cl_set_act_visible("unconfirmed,invalid,confirmed",TRUE)
      #提交和抽單一開始先無條件關
      CALL cl_set_act_visible("signing,withdraw",FALSE)
      CASE g_sfka_m.sfkastus
         WHEN "N"
            CALL cl_set_act_visible("unconfirmed,closed",FALSE)
            #需提交至BPM時，則顯示「提交」功能並隱藏「確認」功能
            IF cl_bpm_chk() THEN
               CALL cl_set_act_visible("signing",TRUE)
               CALL cl_set_act_visible("confirmed",FALSE)
            END IF
         #MOD BY zhujing 2015-6-16 应该根据状态码隐藏posted 和unposted以及withdraw
         #保留修改的功能(如作廢)，隱藏其他應用功能
         WHEN "R"
            CALL cl_set_act_visible("confirmed,unconfirmed",FALSE)
            HIDE OPTION "posted"
            HIDE OPTION "unposted" 
            HIDE OPTION "withdraw"   
         WHEN "D"
            CALL cl_set_act_visible("confirmed,unconfirmed",FALSE)
            HIDE OPTION "posted"
            HIDE OPTION "unposted" 
            HIDE OPTION "withdraw"   
         WHEN "X"
            CALL cl_set_act_visible("unconfirmed,invalid,confirmed",FALSE)
            
         WHEN "Y"
            CALL cl_set_act_visible("invalid,confirmed",FALSE)
            
         #只能顯示抽單;其餘應用功能皆隱藏
         WHEN "W"
            CALL cl_set_act_visible("withdraw",TRUE)
            CALL cl_set_act_visible("unconfirmed,invalid,confirmed",FALSE)
            HIDE OPTION "posted"
            HIDE OPTION "unposted"   
         WHEN "A"     #只能顯示確認; 其餘應用功能皆隱藏
            CALL cl_set_act_visible("confirmed ",TRUE)
            CALL cl_set_act_visible("unconfirmed,invalid",FALSE)
            HIDE OPTION "posted"
            HIDE OPTION "unposted" 
            HIDE OPTION "withdraw"   
      END CASE]]>
  </point>
  <point name="statechange.confirmed" order="" ver="3" cite_std="N" new="Y" ch="" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[         CALL s_transaction_begin()
         IF NOT s_asft800_confirm_chk(g_sfka_m.sfkadocno,g_sfka_m.sfka900) THEN
            CALL s_transaction_end('N','0')
            RETURN
         ELSE
            IF NOT cl_ask_confirm('aim-00108') THEN
               CALL s_transaction_end('N','0')
               RETURN
            ELSE
               IF NOT s_asft800_confirm_upd(g_sfka_m.sfkadocno,g_sfka_m.sfka900) THEN
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','0')
               END IF
            END IF
         END IF]]>
