    <![CDATA[
#来源单号检查
PRIVATE FUNCTION asft800_sfka006_chk()
DEFINE l_sql         STRING
DEFINE l_sql1        STRING
DEFINE l_sql2        STRING
DEFINE l_n           LIKE type_t.num5
DEFINE l_xmda004     LIKE xmda_t.xmda004
DEFINE l_xmdd        RECORD LIKE xmdd_t.*
DEFINE l_success     LIKE type_t.num5
DEFINE l_sfka007     STRING
DEFINE l_sfka008     STRING
DEFINE l_sfka063     STRING

  #當等於訂單、計畫等，檢查是否存在來源資料裡
  IF NOT cl_null(g_sfka_m.sfka006) THEN
     #订单
     IF g_sfka_m.sfka005 = '2' THEN
        LET l_sql1 = "SELECT COUNT(*) "
        LET l_sql2 = "SELECT xmddseq,xmddseq1,xmddseq2 "
        LET l_sql = " FROM xmdd_t WHERE xmddent='",g_enterprise,"' AND xmddsite='",g_site,"'",
                    "   AND xmdddocno='",g_sfka_m.sfka006,"' AND xmdd001='",g_sfka_m.sfka010,"'"
        IF NOT cl_null(g_sfka_m.sfka007) THEN
           LET l_sql = l_sql," AND xmddseq=", g_sfka_m.sfka007
        END IF
        IF NOT cl_null(g_sfka_m.sfka008) THEN
           LET l_sql = l_sql," AND xmddseq1=", g_sfka_m.sfka008
        END IF
        IF NOT cl_null(g_sfka_m.sfka063) THEN
           LET l_sql = l_sql," AND xmddseq2=", g_sfka_m.sfka063
        END IF
        LET l_sql1 = l_sql1,l_sql        
        PREPARE asft800_sfka006_pre1 FROM l_sql1
        EXECUTE asft800_sfka006_pre1 INTO l_n

        IF l_n = 0 THEN
           INITIALIZE g_errparam TO NULL
           LET g_errparam.code = 'asf-00154'
           LET g_errparam.extend = g_sfka_m.sfka006
           LET g_errparam.popup = TRUE
           CALL cl_err()       
           RETURN FALSE
        END IF
        
        IF l_n = 1 THEN
           LET l_sql2 = l_sql2,l_sql
           PREPARE asft800_sfka006_pre2 FROM l_sql2
           EXECUTE asft800_sfka006_pre2 INTO g_sfka_m.sfka007,g_sfka_m.sfka008,g_sfka_m.sfka063
        ELSE
           #开窗选择
           INITIALIZE g_qryparam.* TO NULL
           LET g_qryparam.state = 'i'
			  LET g_qryparam.reqry = FALSE
           LET g_qryparam.where = " xmdddocno = '",g_sfka_m.sfka006,"' AND xmdd001='",g_sfka_m.sfka010,"'"
           IF NOT cl_null(g_sfka_m.sfka007) THEN
              LET g_qryparam.where = g_qryparam.where," AND xmddseq = ",g_sfka_m.sfka007
           END IF
           IF NOT cl_null(g_sfka_m.sfka008) THEN
              LET g_qryparam.where = g_qryparam.where," AND xmddseq1 = ",g_sfka_m.sfka008
           END IF
           IF NOT cl_null(g_sfka_m.sfka063) THEN
              LET g_qryparam.where = g_qryparam.where," AND xmddseq2 = ",g_sfka_m.sfka063
           END IF
           CALL q_xmdddocno()                                #呼叫開窗        
           LET g_sfka_m.sfka006 = g_qryparam.return1              #將開窗取得的值回傳到變數
           LET g_sfka_m.sfka007 = g_qryparam.return2
           LET g_sfka_m.sfka008 = g_qryparam.return3
           LET g_sfka_m.sfka063 = g_qryparam.return4
        END IF   
        LET l_sfka007 = g_sfka_m.sfka007
        LET l_sfka008 = g_sfka_m.sfka008
        LET l_sfka063 = g_sfka_m.sfka063
        LET g_sfka_m.l_sfka007 = l_sfka007 CLIPPED,"-",l_sfka008 CLIPPED,"-",l_sfka063 CLIPPED
        DISPLAY BY NAME g_sfka_m.sfka007,g_sfka_m.sfka008,g_sfka_m.sfka063,g_sfka_m.l_sfka007
     END IF
  ELSE
     RETURN FALSE
  END IF

  RETURN TRUE
END FUNCTION]]>
