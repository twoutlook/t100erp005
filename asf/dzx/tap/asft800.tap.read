#根据工单单号带出工单全部资料
PRIVATE FUNCTION asft800_gen()
DEFINE r_success              LIKE type_t.num5

   LET r_success = FALSE
   #变更单单头资料，sfka_t
   IF NOT asft800_gen_sfka() THEN
      RETURN r_success
   END IF
   
   #变更单工单来源页签资料，sfkb
   IF NOT asft800_gen_sfkb() THEN
      RETURN r_success
   END IF
   
   #变更单工单生產料號明細页签资料，sfkc
   IF NOT asft800_gen_sfkc() THEN
      RETURN r_success
   END IF
   
   #变更单工单特征页签资料，sfkd
   IF NOT asft800_gen_sfkd() THEN
      RETURN r_success
   END IF
   
   #变更单工单计划完工日期页签资料，sfke
   IF NOT asft800_gen_sfke() THEN
      RETURN r_success
   END IF
   
   #变更单工单商品序号页签资料，sfkf
   IF NOT asft800_gen_sfkf() THEN
      RETURN r_success
   END IF
   
   #变更单工单備料檔页签资料，sfkg
   IF NOT asft800_gen_sfkg() THEN
      RETURN r_success
   END IF
     
   LET r_success = TRUE
   RETURN r_success
   
END FUNCTION]]>
