  <point name="function.asft800_sfkadocno_chk" order="1" ver="3" cite_std="N" new="Y" ch="" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
#工单单号检查
PRIVATE FUNCTION asft800_sfkadocno_chk()
DEFINE r_success         LIKE type_t.num5
DEFINE l_sfaadocno       LIKE sfaa_t.sfaadocno
DEFINE l_sfaa047         LIKE sfaa_t.sfaa047
DEFINE l_sfaa048         LIKE sfaa_t.sfaa048
DEFINE l_n               LIKE type_t.num5

   LET r_success = FALSE
   IF cl_null(g_sfka_m.sfkadocno) THEN
      RETURN r_success
   END IF

   SELECT sfaadocno,sfaa047,sfaa048 INTO l_sfaadocno,l_sfaa047,l_sfaa048 FROM sfaa_t
    WHERE sfaaent=g_enterprise AND sfaadocno=g_sfka_m.sfkadocno
      AND sfaastus NOT IN('X','N')
   #是否存在工单
   IF cl_null(l_sfaadocno) THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'asf-00002'
      LET g_errparam.extend = g_sfka_m.sfkadocno
      LET g_errparam.popup = TRUE
      CALL cl_err()

      RETURN r_success
   END IF
   #是否已有生管结案日或者成本结案日
   IF NOT cl_null(l_sfaa047) OR NOT cl_null(l_sfaa048) THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'asf-00293'
      LET g_errparam.extend = g_sfka_m.sfkadocno
      LET g_errparam.popup = TRUE
      CALL cl_err()

      RETURN r_success
   END IF
   
   #同一张工单不可有多张未确认的变更单
   SELECT COUNT(*) INTO l_n FROM sfka_t 
    WHERE sfkaent=g_enterprise AND sfkadocno=g_sfka_m.sfkadocno
      AND sfkastus='N'
   IF l_n > 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'asf-00294'
      LET g_errparam.extend = g_sfka_m.sfkadocno
      LET g_errparam.popup = TRUE
      CALL cl_err()

      RETURN r_success
   END IF
   
   #變更序=採購變更單的變更序最大值+1
   SELECT MAX(sfka900)+1 INTO g_sfka_m.sfka900 FROM sfka_t
    WHERE sfkaent = g_enterprise
      AND sfkadocno = g_sfka_m.sfkadocno
   IF cl_null(g_sfka_m.sfka900) OR g_sfka_m.sfka900 = 0 THEN
      LET g_sfka_m.sfka900 = 1
   END IF
   
   LET r_success = TRUE
   RETURN r_success
END FUNCTION]]>
  </point>
