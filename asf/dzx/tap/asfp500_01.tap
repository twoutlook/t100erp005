  <section id="asfp500_01.b_fill" ver="1" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION asfp500_01_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2           STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE '','','','','','','','','','','','' FROM gzza_t",
               "",
               " WHERE 1=1 AND ", p_wc2
    
   LET g_sql = g_sql, " ORDER BY gzza_t.gzza001"
  
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
  
   PREPARE asfp500_01_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR asfp500_01_pb
   
   OPEN b_fill_curs
 
   CALL g_gzza_d.clear()
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_gzza_d[l_ac].prog,g_gzza_d[l_ac].prog_desc,g_gzza_d[l_ac].docno,g_gzza_d[l_ac].stus, 
       g_gzza_d[l_ac].crtid,g_gzza_d[l_ac].crtid_desc,g_gzza_d[l_ac].crtdp,g_gzza_d[l_ac].crtdp_desc, 
       g_gzza_d[l_ac].open,g_gzza_d[l_ac].post,g_gzza_d[l_ac].errno,g_gzza_d[l_ac].errno_desc
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL asfp500_01_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec AND g_error_show = 1 THEN
      CALL cl_err("gzza_t",9035,1)
   END IF 
   LET g_error_show = 0
   
 
  
   
   CALL g_gzza_d.deleteElement(g_gzza_d.getLength())   
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_gzza_d.getLength()
 
   END FOR
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = l_ac - 1
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE asfp500_01_pb
   
END FUNCTION
]]>
  </section>
