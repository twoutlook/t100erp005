  <section id="asfq360.fetch" ver="9" status="" src="s">
    <![CDATA[#+ 單身陣列填充2
PRIVATE FUNCTION asfq360_fetch()
   {<Local define>}
   DEFINE li_ac           LIKE type_t.num5
   {</Local define>}
   #add-point:fetch段define
   {<point name="fetch.define"/>}
   #end add-point
   
   CALL g_sfla2_d.clear()
 
   CALL g_sfla3_d.clear()
 
 
   #add-point:陣列清空
   {<point name="fetch.array_clear" />}
   #end add-point
   
