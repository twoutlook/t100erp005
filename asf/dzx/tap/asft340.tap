  <point name="function.asft340_chk_sfea004" order="30" ver="13" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 检查 PBI sfea004 值的合法性
# Memo...........:
# Usage..........: CALL asft340_chk_sfea004()
#                       RETURNING r_success
# Input parameter: NULL
# Return code....: r_success 检查通过否标识位
# Date & Author..: 2014-12-26 By Carrier
# Modify.........:
################################################################################
PRIVATE FUNCTION asft340_chk_sfea004()
   DEFINE r_success      LIKE type_t.num5
   DEFINE l_sfqastus     LIKE sfqa_t.sfqastus
   DEFINE l_success      LIKE type_t.num5
   DEFINE l_errno        LIKE gzze_t.gzze001
   DEFINE l_cnt1         LIKE type_t.num5
   DEFINE l_cnt2         LIKE type_t.num5
   
   LET r_success = FALSE
   
   SELECT sfqastus INTO l_sfqastus FROM sfqa_t
    WHERE sfqaent   = g_enterprise
      AND sfqadocno = g_sfea_m.sfea004
      
   CASE
      WHEN SQLCA.sqlcode = 100  LET l_errno = 'asf-00470'   #无此PBI编号
      WHEN l_sfqastus <> 'Y'    LET l_errno = 'sub-00180'   #此笔资料未审核
      OTHERWISE                 LET l_errno = SQLCA.SQLCODE USING '-----'
   END CASE

   #l_cnt1满足PBI及工单一致的笔数
   SELECT COUNT(*) INTO l_cnt1 FROM sfeb_t,sfqb_t
    WHERE sfebent   = sfqbent AND sfebent = g_enterprise
      AND sfebdocno = g_sfea_m.sfeadocno
      AND sfqbdocno = g_sfea_m.sfea004
      AND sfqb001   = sfeb001
   IF cl_null(l_cnt1) THEN LET l_cnt1 = 0 END IF
      
   #l_cnt2 sfeb_t的总笔数
   SELECT COUNT(*) INTO l_cnt2 FROM sfeb_t
    WHERE sfebent   = g_enterprise
      AND sfebdocno = g_sfea_m.sfeadocno
   IF cl_null(l_cnt2) THEN LET l_cnt2 = 0 END IF
   
   IF l_cnt1 <> l_cnt2 THEN
      #单身已有资料，与此PBI不匹配
      LET l_errno = 'asf-00472'
   END IF
   
   IF NOT cl_null(l_errno) THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = l_errno
      LET g_errparam.extend = g_sfea_m.sfea004
      LET g_errparam.popup = TRUE
      CALL cl_err()
      RETURN r_success
   END IF

   LET r_success = TRUE
   RETURN r_success

   
END FUNCTION]]>
  </point>
  <point name="function.asft340_gen_detail_by_pbi" order="31" ver="17" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 按PBI单号产生单身
# Memo...........:
# Usage..........: CALL asft340_gen_detail_by_pbi()
#                       RETURNING r_success
# Input parameter: NULL
# Return code....: r_success      成功否标识符
# Date & Author..: 2015-01-04 By Carrier
# Modify.........:
################################################################################
PUBLIC FUNCTION asft340_gen_detail_by_pbi()
   DEFINE r_success         LIKE type_t.num5
   DEFINE l_cnt             LIKE type_t.num5
   DEFINE l_sfqb001         LIKE sfqb_t.sfqb001  #工單單號
   DEFINE l_sql             STRING
   DEFINE l_sfeb            RECORD LIKE sfeb_t.*
   DEFINE l_sfac001         LIKE sfac_t.sfac001
   DEFINE l_sfac002         LIKE sfac_t.sfac002
   DEFINE l_sfac004         LIKE sfac_t.sfac004
   DEFINE l_sfac006         LIKE sfac_t.sfac006
   DEFINE l_qty1            LIKE sfeb_t.sfeb009 
   DEFINE l_success         LIKE type_t.num5
   DEFINE l_sfec            RECORD LIKE sfec_t.*
   
   LET r_success = TRUE

   IF g_sfea_m.sfea004 IS NULL THEN
      LET r_success = TRUE
      RETURN r_success
   END IF
   
   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt FROM sfeb_t
    WHERE sfebent   = g_enterprise
      AND sfebdocno = g_sfea_m.sfeadocno
         
   IF g_sfea_m_t.sfea004 IS NULL OR g_sfea_m_t.sfea004 <> g_sfea_m.sfea004 OR l_cnt = 0 THEN
      IF NOT cl_ask_confirm('asf-00473') THEN  #是否由【PBI编号】自动产生单身资料？
         LET r_success = TRUE
         RETURN r_success
      END IF   

      IF l_cnt > 0 THEN
         #是否删除单身资料?
         IF cl_ask_confirm('asf-00667') THEN
            DELETE FROM sfeb_t
             WHERE sfebent   = g_enterprise
               AND sfebdocno = g_sfea_m.sfeadocno

            IF SQLCA.sqlcode THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = SQLCA.sqlcode
               LET g_errparam.extend = 'DELETE sfeb'
               LET g_errparam.popup = TRUE
               CALL cl_err()
               LET r_success = FALSE
               RETURN r_success
            END IF
   
            DELETE FROM sfec_t
             WHERE sfecent   = g_enterprise
               AND sfecdocno = g_sfea_m.sfeadocno

            IF SQLCA.sqlcode THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = SQLCA.sqlcode
               LET g_errparam.extend = 'DELETE sfec'
               LET g_errparam.popup = TRUE
               CALL cl_err()
               LET r_success = FALSE
               RETURN r_success
            END IF

            DELETE FROM inao_t
             WHERE inaoent = g_enterprise AND inaosite = g_site AND
                   inaodocno = g_sfea_m.sfeadocno
               AND (inao000 = '1' OR inao000 = '2')
               
            IF SQLCA.sqlcode THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = SQLCA.sqlcode
               LET g_errparam.extend = 'DELETE inao'
               LET g_errparam.popup = TRUE
               CALL cl_err()
               LET r_success = FALSE
               RETURN r_success
            END IF
            
         ELSE
            LET r_success = TRUE
            RETURN r_success         
         END IF
      END IF
   ELSE
      LET r_success = TRUE
      RETURN r_success
   END IF

   #取RUN CARD号
   LET g_sql = " SELECT sfcb001 FROM sfcb_t ",
               "  WHERE sfcbent   = ",g_enterprise,
               "    AND sfcbdocno = ? ",
               " ORDER BY sfcb001 "
   PREPARE asft340_get_run_card_p1 FROM g_sql      
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code   = SQLCA.sqlcode
      LET g_errparam.extend = "PREPARE asft340_get_run_card_p1"
      LET g_errparam.popup  = TRUE
      CALL cl_err()
      LET r_success = FALSE
      RETURN r_success
   END IF

   DECLARE asft340_get_run_card_cs1 SCROLL CURSOR FOR asft340_get_run_card_p1
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code   = SQLCA.sqlcode
      LET g_errparam.extend = "DECLARE asft340_get_run_card_cs1"
      LET g_errparam.popup  = TRUE
      CALL cl_err()
      LET r_success = FALSE
      RETURN r_success
   END IF
         
   LET l_sql = "SELECT sfqb001,sfac001,sfac002,sfac004,sfac006 FROM sfqb_t,sfac_t ",
               " WHERE sfqbent   = ",g_enterprise,
               "   AND sfqbdocno = '",g_sfea_m.sfea004,"' ",
               "   AND sfacent   = sfqbent ",
               "   AND sfqb001   = sfacdocno ",
               "   AND sfac002 IN ('1','3') "
   PREPARE asft340_gen_b_sfea004_p FROM l_sql
   DECLARE asft340_gen_b_sfea004_c CURSOR FOR asft340_gen_b_sfea004_p
   
   CALL cl_err_collect_init()

   LET l_cnt = 1   
   FOREACH asft340_gen_b_sfea004_c INTO l_sfqb001,l_sfac001,l_sfac002,l_sfac004,l_sfac006
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = 'foreach asft340_gen_b_sfea004_c err'
         LET g_errparam.popup = TRUE
         CALL cl_err()
         LET r_success = FALSE
         EXIT FOREACH
      END IF
      
      #检查是否存在工单中，且为发放状态
      IF NOT s_asft300_chk_stus(l_sfqb001,'F') THEN
         LET r_success = FALSE        
         CONTINUE FOREACH
      END IF
       

      INITIALIZE l_sfeb.* TO NULL
      LET l_sfeb.sfebent   = g_enterprise        #企业编号
      LET l_sfeb.sfebsite  = g_sfea_m.sfeasite   #营运据点
      LET l_sfeb.sfebdocno = g_sfea_m.sfeadocno  #完工入库单号
      #项次
      SELECT MAX(sfebseq) + 1 INTO l_sfeb.sfebseq FROM sfeb_t
       WHERE sfebent   = g_enterprise
         AND sfebdocno = g_sfea_m.sfeadocno
      IF cl_null(l_sfeb.sfebseq) THEN
         LET l_sfeb.sfebseq = 1
      END IF
            
      LET l_sfeb.sfeb001   = l_sfqb001           #工单单号
      #FQC
      CALL asft340_set_sfeb002('',l_sfeb.sfeb001,l_sfeb.sfeb003)
           RETURNING l_sfeb.sfeb002
  
      LET l_sfeb.sfeb003   = l_sfac002           #入库类型
      LET l_sfeb.sfeb004   = l_sfac001           #料件编号
      LET l_sfeb.sfeb005   = l_sfac006           #特征
      IF l_sfeb.sfeb005 IS NULL THEN
         LET l_sfeb.sfeb005 = ' '
      END IF
      LET l_sfeb.sfeb006   = ''                  #包装容器
      LET l_sfeb.sfeb007   = l_sfac004           #单位
      
      #RUN CARD
      #取工单的第一笔RUN CARD号
      OPEN asft340_get_run_card_cs1 USING l_sfeb.sfeb001
      FETCH FIRST asft340_get_run_card_cs1  INTO l_sfeb.sfeb026       
      IF cl_null(l_sfeb.sfeb026) THEN
         LET l_sfeb.sfeb026 = 0
      END IF
      
      CASE l_sfeb.sfeb003
           WHEN '1'  #一般
                CALL s_asft340_avariable_store_in_qty_1('1',l_sfeb.sfeb001,l_sfeb.sfeb026,
                                                        l_sfeb.sfeb004,l_sfeb.sfeb005,
                                                        l_sfeb.sfeb007,g_sfea_m.sfeadocdt,0,0)
                     RETURNING l_qty1
   
           WHEN '3'  #多产出主件                  
                CALL s_asft340_avariable_store_in_qty_2('1',l_sfeb.sfeb001,l_sfeb.sfeb026,
                                                        l_sfeb.sfeb004,
                                                        l_sfeb.sfeb007,g_sfea_m.sfeadocdt,0,0)             
                     RETURNING l_qty1                  
                     
      END CASE 
      #剩余的数量 
      IF cl_null(l_qty1) THEN LET l_qty1 = 0 END IF
         
      LET l_sfeb.sfeb008   = l_qty1              #申请数量
      
      IF l_sfeb.sfeb008 <=0 THEN
         #工单 %1 料件 %2 可入库数量为0,此笔数据不能插入单身
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'asf-00475'
         LET g_errparam.extend = ''
         LET g_errparam.replace[1] = l_sfeb.sfeb001
         LET g_errparam.replace[2] = l_sfeb.sfeb004
         LET g_errparam.popup = FALSE
         CALL cl_err()      
         CONTINUE FOREACH
      END IF
      ##实际数量
      IF l_sfeb.sfeb002 = 'N' THEN
         LET l_sfeb.sfeb009 = l_sfeb.sfeb008
         LET l_sfeb.sfeb027 = l_sfeb.sfeb008
      ELSE
         LET l_sfeb.sfeb009 = 0
         LET l_sfeb.sfeb027 = 0
      END IF
      #参考单位     
      CALL asft340_set_sfeb010(l_sfeb.sfeb004)  
           RETURNING l_sfeb.sfeb010    
           
      #申请参考数量/#实际参考数量
      #若没有参考单位时,参考数量DEFAULT NULL
      IF cl_null(l_sfeb.sfeb010) THEN
         LET l_sfeb.sfeb011 = NULL
         LET l_sfeb.sfeb012 = NULL
      ELSE
         CALL s_aooi250_convert_qty(l_sfeb.sfeb004,l_sfeb.sfeb007,l_sfeb.sfeb010,l_sfeb.sfeb008)
              RETURNING l_success,l_sfeb.sfeb011
         IF NOT l_success THEN
            LET l_sfeb.sfeb011 = l_sfeb.sfeb008
         END IF  
         
         IF l_sfeb.sfeb002 = 'N' THEN
            LET l_sfeb.sfeb012 = l_sfeb.sfeb011
         ELSE
            LET l_sfeb.sfeb012 = 0
         END IF
      END IF
      #指定库位/指定储位/指定批号
      CALL s_asft340_set_warehouses(l_sfeb.sfeb001,l_sfeb.sfeb004,'','','')  
           RETURNING l_sfeb.sfeb013,l_sfeb.sfeb014,l_sfeb.sfeb015                

      LET l_sfeb.sfeb016   = ' '                #库存管理特征
      
      #专案代号/WBS/活动代号
      SELECT sfaa028,sfaa029,sfaa030 INTO l_sfeb.sfeb017,l_sfeb.sfeb018,l_sfeb.sfeb019 FROM sfaa_t
       WHERE sfaaent = g_enterprise
         AND sfaadocno = l_sfeb.sfeb001
               
      LET l_sfeb.sfeb020   = ' '               #理由码
      
      #库存有效日期
      CALL s_aini010_calculate_effdt(g_sfea_m.sfeasite,l_sfeb.sfeb004,l_sfeb.sfeb005,l_sfeb.sfeb015,g_sfea_m.sfeadocdt)
           RETURNING l_sfeb.sfeb021      
      LET l_sfeb.sfeb022   = ' '               #库存备注
      LET l_sfeb.sfeb023   = ''                #生产料号
      LET l_sfeb.sfeb024   = ''                #生产料号BOM特性
      LET l_sfeb.sfeb025   = ''                #生产料号特征
    
      INSERT INTO sfeb_t VALUES l_sfeb.*
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = 'insert sfeb'
         LET g_errparam.popup = FALSE
         CALL cl_err()
         LET r_success = FALSE
         CONTINUE FOREACH
      END IF
      
      IF l_sfeb.sfeb002 = 'Y' THEN
         CONTINUE FOREACH
      END IF
      
      LET l_sfec.sfecent   = g_enterprise                     #企業代碼
      LET l_sfec.sfecsite  = g_sfea_m.sfeasite                #營運據點
      LET l_sfec.sfecdocno = g_sfea_m.sfeadocno       #單號
      LET l_sfec.sfecseq   = l_sfeb.sfebseq           #項次
      LET l_sfec.sfecseq1  = 1                        #項次1
      LET l_sfec.sfec001   = l_sfeb.sfeb001           #工單單號
      LET l_sfec.sfec002   = ''                       #FQC單號
      LET l_sfec.sfec003   = ''                       #判定項次
      LET l_sfec.sfec004   = l_sfeb.sfeb003           #入庫類型
      LET l_sfec.sfec005   = l_sfeb.sfeb004           #料件編號
      LET l_sfec.sfec006   = l_sfeb.sfeb005           #特徵
      LET l_sfec.sfec007   = l_sfeb.sfeb006           #包裝容器
      LET l_sfec.sfec008   = l_sfeb.sfeb007           #單位
      LET l_sfec.sfec009   = l_sfeb.sfeb008           #數量
      LET l_sfec.sfec010   = l_sfeb.sfeb010           #參考單位
      LET l_sfec.sfec011   = l_sfeb.sfeb011           #參考數量
      LET l_sfec.sfec012   = l_sfeb.sfeb013           #庫位
      LET l_sfec.sfec013   = l_sfeb.sfeb014           #儲位
      LET l_sfec.sfec014   = l_sfeb.sfeb015           #批號
      LET l_sfec.sfec015   = l_sfeb.sfeb016           #庫存管理特徵
      LET l_sfec.sfec016   = l_sfeb.sfeb021           #有效日期
      LET l_sfec.sfec017   = l_sfeb.sfeb022           #庫存備註
      LET l_sfec.sfec021   = l_sfeb.sfeb026           #RUN CARD
      #add by lixiang 2015/06/26---s---
      LET l_sfec.sfec022   = l_sfeb.sfeb017           #專案編號
      LET l_sfec.sfec023   = l_sfeb.sfeb018           #WBS
      LET l_sfec.sfec024   = l_sfeb.sfeb019           #活動編號
      #add by lixiang 2015/06/26---e---
      
      INSERT INTO sfec_t VALUES(l_sfec.*)
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = 'insert sfec'
         LET g_errparam.popup = FALSE
         CALL cl_err()
         LET r_success = FALSE
         CONTINUE FOREACH
      END IF

   END FOREACH
   FREE asft340_gen_b_sfea004_p
   FREE asft340_get_run_card_p1
   
   CALL cl_err_collect_show()
   
