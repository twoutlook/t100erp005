  <point name="function.asft340_sfeb001_reference" order="1" ver="13" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 取工单相关字段值
# Memo...........:
# Usage..........: CALL asft340_sfeb001_reference()
#                  RETURNING NULL
# Input parameter: p_p_sfaadocno      工单单号
# Return code....: NULL
# Date & Author..: 2013/12/09 By Carrier
# Modify.........:
################################################################################
PRIVATE FUNCTION asft340_sfeb001_reference()
   DEFINE l_sfaa           RECORD LIKE sfaa_t.*
   DEFINE l_str            LIKE type_t.chr10
 
   
   SELECT * INTO l_sfaa.* FROM sfaa_t
    WHERE sfaaent   = g_enterprise
      AND sfaasite  = g_site
      AND sfaadocno = g_sfeb_d[l_ac].sfeb001

   #1.sfeb002  -FQC否
   CALL asft340_set_sfeb002(l_sfaa.sfaa044,g_sfeb_d[l_ac].sfeb001,g_sfeb_d[l_ac].sfeb003)
        RETURNING g_sfeb_d[l_ac].sfeb002 
   
   #2.sfeb003  -入库类型 设置默认值
   CALL asft340_set_sfeb003('a',l_sfaa.sfaa003) 
        RETURNING l_str
        
   #3.sfeb004  -料件编号
   CALL asft340_set_sfeb004()
   
   #4.sfeb005  -特征值
   CALL asft340_set_sfeb005()
   
   #5.sfeb007 - 单位
   CALL asft340_set_sfeb007()
   
   #6.sfeb008 - 申请数量
   CALL asft340_set_sfeb008()
   
   #7.sfeb010 - 参考单位
   CALL asft340_set_sfeb010(g_sfeb_d[l_ac].sfeb004)  
        RETURNING g_sfeb_d[l_ac].sfeb010
   
   #8.sfeb011 - 参考数量
   CALL asft340_set_sfeb011()
   
   #9.sfeb013/sfeb014/sfeb015 - 仓/批/批
   CALL s_asft340_set_warehouses(g_sfeb_d[l_ac].sfeb001,g_sfeb_d[l_ac].sfeb004,l_sfaa.sfaa034,l_sfaa.sfaa035,l_sfaa.sfaa059)  
        RETURNING g_sfeb_d[l_ac].sfeb013,g_sfeb_d[l_ac].sfeb014,g_sfeb_d[l_ac].sfeb015

   #10.sfeb017 - 专案代号
   CALL asft340_set_sfeb017(l_sfaa.sfaa028)
   
   #11.sfeb018 - WBS
   CALL asft340_set_sfeb018(l_sfaa.sfaa029)   
   
   #12.sfeb018 - 活动代号
   CALL asft340_set_sfeb019(l_sfaa.sfaa030)  

   #13.sfeb026 - RUN CARD
   CALL asft340_set_sfeb026()
   
   #置有效日期
   CALL asft340_set_sfeb021()  
   
END FUNCTION]]>
  </point>
  <point name="function.asft340_set_sfeb002" order="2" ver="14" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: FQC sfeb002的默认值设定                                      
# Memo...........:                                                              
# Usage..........: CALL asft340_set_sfeb002(p_sfaa044,p_sfeb001,p_sfeb003)      
#                  RETURNING NULL                                               
# Input parameter: p_sfaa044        FQC                                         
#                : p_sfeb001        工单编号                                    
#                : p_sfeb003        入库类型                                    
# Return code....: r_sfeb002        FQC否                                       
# Date & Author..: 2015-01-03 By Carrier                                        
# Modify.........:                                                              
################################################################################
PRIVATE FUNCTION asft340_set_sfeb002(p_sfaa044,p_sfeb001,p_sfeb003)
   DEFINE p_sfaa044     LIKE sfaa_t.sfaa044      #FQC否      
   DEFINE p_sfeb001     LIKE sfeb_t.sfeb001
   DEFINE p_sfeb003     LIKE sfeb_t.sfeb003        
   DEFINE r_sfeb002     LIKE sfeb_t.sfeb002                     
   DEFINE l_str         LIKE ooab_t.ooab002      #参数值                                  
   DEFINE l_doc_type    LIKE ooba_t.ooba002                                               
   DEFINE l_success     LIKE type_t.num5                                                  
   DEFINE l_ooef004     LIKE ooef_t.ooef004                                               
                          
   LET r_sfeb002 = 'N'                                                                
#  IF cl_null(p_sfaa044) THEN                                                             
      SELECT sfaa044 INTO p_sfaa044 FROM sfaa_t                                           
       WHERE sfaaent   = g_enterprise                                                     
         AND sfaasite  = g_sfea_m.sfeasite                                                
         AND sfaadocno = p_sfeb001                                           
#  END IF                                                                                 
   LET r_sfeb002 = p_sfaa044                                                 
                                                                                          
   #5.副产品/回收料                                                                       
   IF p_sfeb003 = '5' THEN                                                   
      #若参数 "副产品/回收料是否FQC" = 'N'时,sfeb002 = 'N';若参数值为'Y'时,取sfaa044      
      CALL s_aooi200_get_slip(p_sfeb001)                                     
           RETURNING l_success,l_doc_type                                                 
      SELECT ooef004 INTO l_ooef004 FROM ooef_t                                           
       WHERE ooefent = g_enterprise                                                       
         AND ooef001 = g_sfea_m.sfeasite                                                  
      LET l_str = cl_get_doc_para(g_enterprise,g_sfea_m.sfeasite,l_doc_type,'D-MFG-0051') 
      IF l_str = 'N' THEN                                                                 
         LET r_sfeb002 = 'N'                                                 
      END IF                                                                              
   END IF  
   
   RETURN r_sfeb002      
END FUNCTION]]>
  </point>
