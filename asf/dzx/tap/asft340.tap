                END IF         
           WHEN '3'   #拆件式工单
                LET l_default = '4'   #拆件式 
                LET r_valid = '4'
      END CASE
   END IF
   
   #若p_cmd = 'a' 代表需要预设sfeb003的值
   IF p_cmd = 'a' THEN
      LET g_sfeb_d[l_ac].sfeb003 = l_default
   END IF
   RETURN r_valid


