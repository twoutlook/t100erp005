#         IF l_cnt > 0 THEN
#            #多产出主件时,只能挑 3.多产出主件 5.副产品/回收料
#            CALL l_cbo.RemoveItem('1')  #一般
#            CALL l_cbo.RemoveItem('2')  #联产品
#            CALL l_cbo.RemoveItem('4')  #拆件式
#         ELSE
#            #非多产出主件时,只能挑 1.一般 2.联产品 5.副产品
#            CALL l_cbo.RemoveItem('3')  #多产出主件
#            CALL l_cbo.RemoveItem('4')  #拆件式            
