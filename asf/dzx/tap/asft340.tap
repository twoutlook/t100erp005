   {<point name="input.after_set_entry"/>}
   #end add-point
   CALL asft340_set_no_entry(p_cmd)
 
   DISPLAY BY NAME g_sfea_m.sfeadocno,g_sfea_m.sfeasite,g_sfea_m.sfeadocdt,g_sfea_m.sfea001,g_sfea_m.sfea002, 
       g_sfea_m.sfea003,g_sfea_m.sfeastus,g_sfea_m.sfea004,g_sfea_m.sfea005
   
   LET lb_reproduce = FALSE
   
   #關閉被遮罩相關欄位輸入, 無法確定USER是否會需要輸入此欄位
   #因此先行關閉, 若有需要可於下方add-point中自行開啟
   CALL cl_mask_set_no_entry()
   
   #add-point:資料輸入前
