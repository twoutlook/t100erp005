  <point name="statechange.posted" order="" ver="16" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[            IF NOT cl_ask_confirm('sub-00360') THEN
               RETURN
            END IF
                     
            #过帐前状态检查
            CALL cl_err_collect_init() #汇总错误讯息初始化            
            CALL s_asft340_post_chk(g_sfea_m.sfeadocno)
                 RETURNING l_success
            IF NOT l_success THEN
               CALL cl_err_collect_show()           
               RETURN
            END IF
            #过帐日期录入
            CALL s_asft340_post_input(g_sfea_m.sfeadocno)
                 RETURNING l_success,g_sfea_m.sfea001
            IF NOT l_success THEN
               LET INT_FLAG = 0
               RETURN
            END IF         
            CALL s_transaction_begin()     
            CALL cl_err_collect_init() #汇总错误讯息初始化            
            CALL s_asft340_post_upd(g_sfea_m.sfeadocno,g_sfea_m.sfea001)
                 RETURNING l_success
            IF NOT l_success THEN
               CALL cl_err_collect_show()     
               CALL s_transaction_end('N',0)
            ELSE
