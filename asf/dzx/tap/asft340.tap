  <section id="asft340.b_fill" ver="31" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION asft340_b_fill()
   DEFINE p_wc2      STRING
   DEFINE li_idx     LIKE type_t.num10
   #add-point:b_fill段define
   {<point name="b_fill.define" edit="s"/>}
   #end add-point     
   #add-point:b_fill段define(客製用)
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point     
 
   CALL g_sfeb_d.clear()    #g_sfeb_d 單頭及單身 
   CALL g_sfeb3_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF asft340_fill_chk(1) THEN
      #切換上下筆時不重組SQL
      IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
         LET g_sql = "SELECT  UNIQUE sfebseq,sfeb001,sfeb026,sfeb002,sfeb003,sfeb004,sfeb005,sfeb006, 
             sfeb007,sfeb008,sfeb027,sfeb009,sfeb010,sfeb011,sfeb012,sfeb013,sfeb014,sfeb015,sfeb016, 
             sfeb017,sfeb018,sfeb019,sfeb020,sfeb021,sfeb022,sfebsite ,t1.imaal003 ,t2.imaal004 ,t3.inaa002 , 
             t4.inab003 ,t5.pjbal003 ,t6.pjbbl004 ,t7.oocql004 FROM sfeb_t",   
                     " INNER JOIN sfea_t ON sfeadocno = sfebdocno ",
 
                     #"",
                     
                     "",
                                    " LEFT JOIN imaal_t t1 ON t1.imaalent='"||g_enterprise||"' AND t1.imaal001=sfeb004 AND t1.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t2 ON t2.imaalent='"||g_enterprise||"' AND t2.imaal001=sfeb004 AND t2.imaal002='"||g_dlang||"' ",
               " LEFT JOIN inaa_t t3 ON t3.inaaent='"||g_enterprise||"' AND t3.inaasite=sfebsite AND t3.inaa001=sfeb013  ",
               " LEFT JOIN inab_t t4 ON t4.inabent='"||g_enterprise||"' AND t4.inabsite=sfebsite AND t4.inab001=sfeb013 AND t4.inab002=sfeb014  ",
               " LEFT JOIN pjbal_t t5 ON t5.pjbalent='"||g_enterprise||"' AND t5.pjbal001=sfeb017 AND t5.pjbal002='"||g_dlang||"' ",
               " LEFT JOIN pjbbl_t t6 ON t6.pjbblent='"||g_enterprise||"' AND t6.pjbbl001=sfeb017 AND t6.pjbbl002=sfeb018 AND t6.pjbbl003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t7 ON t7.oocqlent='"||g_enterprise||"' AND t7.oocql001='225' AND t7.oocql002=sfeb020 AND t7.oocql003='"||g_dlang||"' ",
 
                     " WHERE sfebent=? AND sfebdocno=?"
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before
         {<point name="b_fill.body.fill_sql"/>}
         #end add-point
         IF NOT cl_null(g_wc2_table1) THEN
            LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY sfeb_t.sfebseq"
         
         #add-point:單身填充控制
         {<point name="b_fill.sql"/>}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE asft340_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR asft340_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_sfea_m.sfeadocno
                                               
      FOREACH b_fill_cs INTO g_sfeb_d[l_ac].sfebseq,g_sfeb_d[l_ac].sfeb001,g_sfeb_d[l_ac].sfeb026,g_sfeb_d[l_ac].sfeb002, 
          g_sfeb_d[l_ac].sfeb003,g_sfeb_d[l_ac].sfeb004,g_sfeb_d[l_ac].sfeb005,g_sfeb_d[l_ac].sfeb006, 
          g_sfeb_d[l_ac].sfeb007,g_sfeb_d[l_ac].sfeb008,g_sfeb_d[l_ac].sfeb027,g_sfeb_d[l_ac].sfeb009, 
          g_sfeb_d[l_ac].sfeb010,g_sfeb_d[l_ac].sfeb011,g_sfeb_d[l_ac].sfeb012,g_sfeb_d[l_ac].sfeb013, 
          g_sfeb_d[l_ac].sfeb014,g_sfeb_d[l_ac].sfeb015,g_sfeb_d[l_ac].sfeb016,g_sfeb_d[l_ac].sfeb017, 
          g_sfeb_d[l_ac].sfeb018,g_sfeb_d[l_ac].sfeb019,g_sfeb_d[l_ac].sfeb020,g_sfeb_d[l_ac].sfeb021, 
          g_sfeb_d[l_ac].sfeb022,g_sfeb_d[l_ac].sfebsite,g_sfeb_d[l_ac].sfeb004_desc,g_sfeb_d[l_ac].sfeb004_desc_desc, 
          g_sfeb_d[l_ac].sfeb013_desc,g_sfeb_d[l_ac].sfeb014_desc,g_sfeb_d[l_ac].sfeb017_desc,g_sfeb_d[l_ac].sfeb018_desc, 
          g_sfeb_d[l_ac].sfeb020_desc
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF
            EXIT FOREACH
         END IF
         
         LET l_ac = l_ac + 1
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
   #判斷是否填充
   IF asft340_fill_chk(2) THEN
      IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
         LET g_sql = "SELECT  UNIQUE sfecseq,sfecseq1,sfec001,sfec021,sfec002,sfec003,sfec004,sfec005, 
             sfec006,sfec007,sfec008,sfec009,sfec010,sfec011,sfec012,sfec013,sfec014,sfec015,sfec022, 
             sfec023,sfec024,sfec016,sfec017,sfecsite ,t8.qcbc002 ,t9.imaal003 ,t10.imaal004 ,t11.inaa002 , 
             t12.inab003 FROM sfec_t",   
                     " INNER JOIN sfea_t ON sfeadocno = sfecdocno ",
 
                     "",
                     
                                    " LEFT JOIN qcbc_t t8 ON t8.qcbcent='"||g_enterprise||"' AND t8.qcbcsite=sfecsite AND t8.qcbcdocno=sfec002 AND t8.qcbcseq=sfec003  ",
               " LEFT JOIN imaal_t t9 ON t9.imaalent='"||g_enterprise||"' AND t9.imaal001=sfec005 AND t9.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t10 ON t10.imaalent='"||g_enterprise||"' AND t10.imaal001=sfec005 AND t10.imaal002='"||g_dlang||"' ",
               " LEFT JOIN inaa_t t11 ON t11.inaaent='"||g_enterprise||"' AND t11.inaasite=sfecsite AND t11.inaa001=sfec012  ",
               " LEFT JOIN inab_t t12 ON t12.inabent='"||g_enterprise||"' AND t12.inabsite=sfecsite AND t12.inab001=sfec012 AND t12.inab002=sfec013  ",
 
                     " WHERE sfecent=? AND sfecdocno=?"   
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before
         {<point name="b_fill.body2.fill_sql"/>}
         #end add-point
         IF NOT cl_null(g_wc2_table2) THEN
            LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY sfec_t.sfecseq,sfec_t.sfecseq1"
         
         #add-point:單身填充控制
         {<point name="b_fill.sql2"/>}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE asft340_pb2 FROM g_sql
         DECLARE b_fill_cs2 CURSOR FOR asft340_pb2
      END IF
    
      LET l_ac = 1
      
      OPEN b_fill_cs2 USING g_enterprise,g_sfea_m.sfeadocno
                                               
      FOREACH b_fill_cs2 INTO g_sfeb3_d[l_ac].sfecseq,g_sfeb3_d[l_ac].sfecseq1,g_sfeb3_d[l_ac].sfec001, 
          g_sfeb3_d[l_ac].sfec021,g_sfeb3_d[l_ac].sfec002,g_sfeb3_d[l_ac].sfec003,g_sfeb3_d[l_ac].sfec004, 
          g_sfeb3_d[l_ac].sfec005,g_sfeb3_d[l_ac].sfec006,g_sfeb3_d[l_ac].sfec007,g_sfeb3_d[l_ac].sfec008, 
          g_sfeb3_d[l_ac].sfec009,g_sfeb3_d[l_ac].sfec010,g_sfeb3_d[l_ac].sfec011,g_sfeb3_d[l_ac].sfec012, 
          g_sfeb3_d[l_ac].sfec013,g_sfeb3_d[l_ac].sfec014,g_sfeb3_d[l_ac].sfec015,g_sfeb3_d[l_ac].sfec022, 
          g_sfeb3_d[l_ac].sfec023,g_sfeb3_d[l_ac].sfec024,g_sfeb3_d[l_ac].sfec016,g_sfeb3_d[l_ac].sfec017, 
          g_sfeb3_d[l_ac].sfecsite,g_sfeb3_d[l_ac].sfec003_desc,g_sfeb3_d[l_ac].sfec005_desc,g_sfeb3_d[l_ac].sfec005_desc_desc, 
          g_sfeb3_d[l_ac].sfec012_desc,g_sfeb3_d[l_ac].sfec013_desc
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill2.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
         
      END FOREACH
   END IF
 
 
   
   #add-point:browser_fill段其他table處理
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_sfeb_d.deleteElement(g_sfeb_d.getLength())
   CALL g_sfeb3_d.deleteElement(g_sfeb3_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE asft340_pb
   FREE asft340_pb2
 
 
   
   LET li_idx = l_ac
   
   #遮罩相關處理
   FOR l_ac = 1 TO g_sfeb_d.getLength()
      LET g_sfeb_d_mask_o[l_ac].* =  g_sfeb_d[l_ac].*
      CALL asft340_sfeb_t_mask()
      LET g_sfeb_d_mask_n[l_ac].* =  g_sfeb_d[l_ac].*
   END FOR
   
   LET g_sfeb3_d_mask_o.* =  g_sfeb3_d.*
   FOR l_ac = 1 TO g_sfeb3_d.getLength()
      LET g_sfeb3_d_mask_o[l_ac].* =  g_sfeb3_d[l_ac].*
      CALL asft340_sfec_t_mask()
      LET g_sfeb3_d_mask_n[l_ac].* =  g_sfeb3_d[l_ac].*
   END FOR
 
   
   LET l_ac = li_idx
   
   CALL cl_ap_performance_next_end()
 
END FUNCTION
]]>
  </section>
