  <point name="function.asft340_chk_sfac" order="32" ver="16" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 检查sfac资料的有效性
# Memo...........:
# Usage..........: CALL asft340_chk_sfac()
#                  RETURNING r_success
# Input parameter: 
# Return code....: r_success      检查通过否标识位
# Date & Author..: 2015-04-14 By Carrier
# Modify.........:
################################################################################
PRIVATE FUNCTION asft340_chk_sfac()
   DEFINE r_success      LIKE type_t.num5
   DEFINE l_cnt          LIKE type_t.num5
   DEFINE l_success      LIKE type_t.num5
      
   LET r_success = FALSE

   #检查工单+类型+料号+特征是否出现多笔,若是报错
   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt FROM sfac_t
    WHERE sfacent = g_enterprise
      AND sfacdocno = g_sfeb_d[l_ac].sfeb001
      AND sfac001 = g_sfeb_d[l_ac].sfeb004
      AND sfac002 = g_sfeb_d[l_ac].sfeb003
      AND sfac006 = g_sfeb_d[l_ac].sfeb005
   IF cl_null(l_cnt) THEN LET l_cnt = 0 END IF
   IF l_cnt > 1 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'asf-00686'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      LET g_errparam.replace[1] =  g_sfeb_d[l_ac].sfeb001
      LET g_errparam.replace[2] =  g_sfeb_d[l_ac].sfeb003 
      LET g_errparam.replace[3] =  g_sfeb_d[l_ac].sfeb004
      LET g_errparam.replace[4] =  g_sfeb_d[l_ac].sfeb005
      CALL cl_err()
      RETURN r_success      
   END IF
                          
   LET r_success = TRUE   
   RETURN r_success       
END FUNCTION]]>
