           DECLARE asfr310_g04_repcur03 CURSOR FROM g_sql
           FOREACH asfr310_g04_repcur03 INTO sr2.*
              IF STATUS THEN CALL cl_err('asfr310_g04_repcur03:',STATUS,1) EXIT FOREACH END IF
              #add-point:rep.sub03.foreach
              {<point name="rep.sub03.foreach"/>}
              #end add-point:rep.sub03.foreach
              OUTPUT TO REPORT asfr310_g04_subrep03(sr2.*)
              END FOREACH
           FINISH REPORT asfr310_g04_subrep03
           #add-point:rep.sub03.after
           {<point name="rep.sub03.after"/>}
           #end add-point:rep.sub03.after
 
 
 
            #add-point:rep.everyrow.after
            {<point name="rep.everyrow.after"/>}
            #end add-point:rep.everyrow.after        
 
        #讀取afterGrup子樣板+子報表樣板
        AFTER GROUP OF sr1.sfdadocno
 
           #add-point:rep.a_group.sfdadocno.before
           {<point name="rep.a_group.sfdadocno.before"/>}
           #end add-point:
 
           #add-point:rep.sub04.before
