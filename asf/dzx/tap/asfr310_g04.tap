           LET g_sql = " SELECT ooff013 FROM ooff_t WHERE ooffstus='Y' and ooff001='7' AND ooff012='1' AND ooff002 = '", 
                sr1.sfdaent CLIPPED ,"'"
 
           LET l_cnt = 0
