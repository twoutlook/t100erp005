  <section id="asft330_02.b_fill" ver="5" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION asft330_02_b_fill()
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_sffd_d.clear()    #g_sffd_d 單頭及單身 
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF asft330_02_fill_chk(1) THEN
   
      LET g_sql = "SELECT  UNIQUE sffdseq1,sffd001,sffd002,sffd003,sffdsite ,t1.oocql004 FROM sffd_t", 
             
                  " INNER JOIN sffb_t ON sffbdocno = sffddocno ",
                  " AND sffbseq = sffdseq ",
 
                  #"",
                  
                  "",
                                 " LEFT JOIN oocql_t t1 ON t1.oocqlent='"||g_enterprise||"' AND t1.oocql001='1053' AND t1.oocql002=sffd001 AND t1.oocql003='"||g_dlang||"' ",
 
                  " WHERE"
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      #add-point:b_fill段sql_before
      {<point name="b_fill.body.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table1) THEN
         LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
      END IF
      
      #子單身的WC
      
      
      LET g_sql = g_sql, " ORDER BY sffd_t.sffdseq1,sffd_t.sffd001"
      
      #add-point:單身填充控制
      {<point name="b_fill.sql"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE asft330_02_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR asft330_02_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_sffb_m.sffbdocno,g_sffb_m.sffbseq
                                               
      FOREACH b_fill_cs INTO g_sffd_d[l_ac].sffdseq1,g_sffd_d[l_ac].sffd001,g_sffd_d[l_ac].sffd002,g_sffd_d[l_ac].sffd003, 
          g_sffd_d[l_ac].sffdsite,g_sffd_d[l_ac].sffd001_desc
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               CALL cl_err( '', 9035, 1)
            END IF
            EXIT FOREACH
         END IF
         
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
 
   
   #add-point:browser_fill段其他table處理
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_sffd_d.deleteElement(g_sffd_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE asft330_02_pb
 
   
END FUNCTION
]]>
  </section>
