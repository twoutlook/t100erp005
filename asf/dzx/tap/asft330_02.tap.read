  <section id="asft330_02.modify" ver="7" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION asft330_02_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_sffb_m.sffbdocno IS NULL
   OR g_sffb_m.sffbseq IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   ERROR ""
  
   LET g_sffbdocno_t = g_sffb_m.sffbdocno
   LET g_sffbseq_t = g_sffb_m.sffbseq
 
   CALL s_transaction_begin()
   
   OPEN asft330_02_cl USING g_enterprise,g_sffb_m.sffbdocno,g_sffb_m.sffbseq
   IF STATUS THEN
      CALL cl_err("OPEN asft330_02_cl:", STATUS, 1)
      CLOSE asft330_02_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE asft330_02_master_referesh USING g_sffb_m.sffbdocno,g_sffb_m.sffbseq INTO g_sffb_m.sffbdocno, 
       g_sffb_m.sffbseq,g_sffb_m.sffb005,g_sffb_m.sffb006,g_sffb_m.sffb007,g_sffb_m.sffb008,g_sffb_m.sffb017, 
       g_sffb_m.sffb018,g_sffb_m.sffb019,g_sffb_m.sffb020,g_sffb_m.sffbdocno_desc,g_sffb_m.sffb007_desc 

 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_sffb_m.sffbdocno,SQLCA.sqlcode,0)
      CLOSE asft330_02_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   CALL asft330_02_show()
   WHILE TRUE
      LET g_sffbdocno_t = g_sffb_m.sffbdocno
      LET g_sffbseq_t = g_sffb_m.sffbseq
 
      
      #寫入修改者/修改日期資訊(單頭)
      
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      CALL asft330_02_input("u")
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_sffb_m.* = g_sffb_m_t.*
         CALL asft330_02_show()
         CALL cl_err('',9001,0)
         CALL s_transaction_end('N','0')
         EXIT WHILE
      END IF 
      
      #若有modid跟moddt則進行update
 
                  
      #若單頭key欄位有變更
      IF g_sffb_m.sffbdocno != g_sffbdocno_t 
      OR g_sffb_m.sffbseq != g_sffbseq_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE sffd_t SET sffddocno = g_sffb_m.sffbdocno
                                       ,sffdseq = g_sffb_m.sffbseq
 
          WHERE sffdent = g_enterprise AND sffddocno = g_sffbdocno_t
            AND sffdseq = g_sffbseq_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("sffd_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("sffd_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_sffb_m.sffbdocno,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE asft330_02_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_sffb_m.sffbdocno,'U')
 
END FUNCTION   
]]>
