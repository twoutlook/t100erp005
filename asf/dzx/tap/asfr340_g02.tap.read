  <section id="asfr340_g02.rep_data" ver="3" status="" src="s">
    <![CDATA[PRIVATE FUNCTION asfr340_g02_rep_data()
   DEFINE HANDLER         om.SaxDocumentHandler
   DEFINE l_i             INTEGER
 
    #判斷是否有報表資料，若回彈出訊息視窗
    IF sr.getLength() = 0 THEN
       CALL cl_err_msg(NULL , "adz-00285", '', 1)
       RETURN 
    END IF
    #CALL cl_gr_init_apr()                          #t100先不用
    WHILE TRUE
       CALL cl_gr_init_pageheader()               
       LET handler = cl_gr_handler()
       IF handler IS NOT NULL THEN
          START REPORT asfr340_g02_rep TO XML HANDLER handler
          FOR l_i = 1 TO sr.getLength()
             OUTPUT TO REPORT asfr340_g02_rep(sr[l_i].*)
          END FOR
          FINISH REPORT asfr340_g02_rep
       END IF
       IF INT_FLAG = TRUE THEN
          LET INT_FLAG = FALSE
          EXIT WHILE
       END IF
    END WHILE
    CALL cl_gr_close_report()
END FUNCTION
]]>
  </section>
  <section id="asfr340_g02.sel_prep" ver="6" status="" src="s">
    <![CDATA[#+ 選單功能實際執行處
PRIVATE FUNCTION asfr340_g02_sel_prep()
   #add-point:sel_prep段define
   {<point name="sel_prep.define"/>}
   #end add-point
 
   #add-point:sel_prep before
   {<point name="sel_prep.before" mark="Y"/>}
   #end add-point
   
   #add-point:sel_prep g_select
   {<point name="sel_prep.g_select" mark="Y"/>}
   #end add-point
   LET g_select = " SELECT sfea001,sfea002,sfea003,sfea004,sfea005,sfeadocdt,sfeadocno,sfeaent,sfeasite, 
       sfeastus,sfeb001,sfeb002,sfeb003,sfeb004,sfeb005,sfeb006,sfeb007,sfeb008,sfeb009,sfeb010,sfeb011, 
       sfeb012,sfeb013,sfeb014,sfeb015,sfeb016,sfeb017,sfeb018,sfeb019,sfeb020,sfeb021,sfeb022,sfeb026, 
       sfebseq,sfebsite,ooefl_t.ooefl003,x.imaal_t_imaal003,trim(sfea003)||'.'||trim(ooefl_t.ooefl003), 
       x.imaal_t_imaal004,''"
 
   #add-point:sel_prep g_from
   {<point name="sel_prep.g_from" mark="Y"/>}
   #end add-point
    LET g_from = " FROM sfea_t LEFT OUTER JOIN ooefl_t ON ooefl_t.ooefl001 = sfea_t.sfea003 AND ooefl_t.ooeflent = sfea_t.sfeaent AND ooefl_t.ooefl002 = '" , 
        g_dlang,"'" ," LEFT OUTER JOIN ( SELECT sfeb_t.*,imaal_t.imaal003 imaal_t_imaal003,imaal_t.imaal004 imaal_t_imaal004 FROM sfeb_t             LEFT OUTER JOIN imaal_t ON imaal_t.imaal001 = sfeb_t.sfeb004 AND imaal_t.imaalent = sfeb_t.sfebent AND imaal_t.imaal002 = '" , 
        g_dlang,"'" ," ) x  ON sfea_t.sfeaent = x.sfebent AND sfea_t.sfeadocno = x.sfebdocno"
 
   #add-point:sel_prep g_where
   {<point name="sel_prep.g_where" mark="Y"/>}
   #end add-point
    LET g_where = " WHERE " ,tm.wc CLIPPED 
 
   #add-point:sel_prep g_order
   {<point name="sel_prep.g_order" mark="Y"/>}
   #end add-point
    LET g_order = " ORDER BY sfeadocno,sfebseq"
 
   #add-point:sel_prep.sql.before
   {<point name="sel_prep.sql.before mark="Y""/>}
   #end add-point:sel_prep.sql.before
   LET g_where = g_where ,cl_sql_add_filter("sfea_t")   #資料過濾功能
   LET g_sql = g_select CLIPPED ," ",g_from CLIPPED ," ",g_where CLIPPED ," ",g_order CLIPPED
   LET g_sql = cl_sql_add_mask(g_sql)    #遮蔽特定資料, 若寫至add-point也需複製此段 
 
   #add-point:sel_prep.sql.after
   {<point name="sel_prep.sql.after mark="Y""/>}
   #end add-point
   PREPARE asfr340_g02_prepare FROM g_sql
   IF STATUS THEN
      CALL cl_err('prepare:',STATUS,1)
      LET g_rep_success = 'N'    
   END IF
   DECLARE asfr340_g02_curs CURSOR FOR asfr340_g02_prepare
 
END FUNCTION
]]>
  </section>
