  <point name="rep.everyrow.afterrow" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[            #子報表06 入庫明細
            START REPORT asfr340_g02_subrep06

            IF NOT cl_null(sr1.sfebseq) AND tm.a1 = "Y" THEN  
               LET g_sql = "SELECT sfecent,sfecsite,sfecdocno,sfecseq1,sfec002,sfec003,sfec009,sfec011,sfec012,sfec013,sfec014,sfec015 ",                            
                           "  FROM sfec_t ",
                           " WHERE sfecdocno = '",sr1.sfeadocno CLIPPED,"'",
                           "   AND sfecent   = '",sr1.sfeaent   CLIPPED,"'",
                           "   AND sfecseq   = '",sr1.sfebseq   CLIPPED,"'"                           
                                                     
               DECLARE asfr340_g02_repcur06 CURSOR FROM g_sql
               FOREACH asfr340_g02_repcur06 INTO sr4.* 
               #倉庫編號帶說明            
                SELECT inaa002 INTO sr4.l_inaa002
                  FROM inaa_t
                 WHERE inaaent = sr4.sfecent
                   AND inaasite = sr4.sfecsite
                   AND inaa001 = sr4.sfec012               
                LET sr4.l_sfec012_inaa002 = sr4.sfec012,".",sr4.l_inaa002
                
                #FQC單號/項次            
                LET sr4.l_sfec002_sfec003 = sr4.sfec002 || "/" || sr4.sfec003
                
                #判定結果說明   
                SELECT qcbc002 INTO sr4.l_qcbc002
                 FROM  qcbc_t
                 WHERE qcbcdocno = sr4.sfec002
                   AND qcbcseq = sr4.sfec003                  
                   AND qcbcent = sr4.sfecent
                   AND qcbcsite = sr4.sfecsite 
                   
                SELECT qcaol004 INTO sr4.l_qcaol004
                  FROM qcaol_t
                 WHERE qcaolent = sr4.sfecent
                   AND qcaol002 = sr4.l_qcbc002 
                   
                   #庫存特徵
                    If cl_null(sr4.sfec015) THEN
                       LET sr4.l_sfec015_show = "N" 
                    ELSE
                       LET sr4.l_sfec015_show = "Y"
                    END IF
                    
                  OUTPUT TO REPORT asfr340_g02_subrep06(sr4.*)
               END FOREACH
            END IF               
            FINISH REPORT asfr340_g02_subrep06

            #子報表05 批序號管理
            START REPORT asfr340_g02_subrep05
            IF NOT cl_null(sr1.sfebseq) THEN
               LET g_sql = "SELECT inaodocno,inaoseq1,inao008,inao009,inao010,inao012 ",
                           "  FROM inao_t ",
                           " WHERE inaodocno = '",sr1.sfeadocno CLIPPED,"'",
                           "   AND inaoent   = '",sr1.sfeaent   CLIPPED,"'",
                           "   AND inaoseq   = '",sr1.sfebseq   CLIPPED,"'",                          
                           "   AND inao008 IS NOT NULL  ",
                           "   AND inao009 IS NOT NULL  " 
                                  
               DECLARE asfr340_g02_repcur05 CURSOR FROM g_sql
               FOREACH asfr340_g02_repcur05 INTO sr3.*                              
                  OUTPUT TO REPORT asfr340_g02_subrep05(sr3.*)
               END FOREACH
            END IF               
            FINISH REPORT asfr340_g02_subrep05]]>
  </point>
