{<section id="afci200.modify" >}
#+ 資料修改
PRIVATE FUNCTION afci200_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   {<point name="modify.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   {<point name="modify.define"/>}
   #end add-point
   
   #add-point:Function前置處理 name="modify.pre_function"
   {<point name="modify.pre_function"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_fcaa_m.fcaa001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_fcaa001_t = g_fcaa_m.fcaa001
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN afci200_cl USING g_enterprise,g_fcaa_m.fcaa001
   IF SQLCA.SQLCODE THEN    #(ver:49)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN afci200_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE
      LET g_errparam.popup = TRUE 
      CLOSE afci200_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE afci200_master_referesh USING g_fcaa_m.fcaa001 INTO g_fcaa_m.fcaa001,g_fcaa_m.fcaa002,g_fcaa_m.fcaa021, 
       g_fcaa_m.fcaa022,g_fcaa_m.fcaa023,g_fcaa_m.fcaa011,g_fcaa_m.fcaa012,g_fcaa_m.fcaa031,g_fcaa_m.fcaaent, 
       g_fcaa_m.fcaaownid,g_fcaa_m.fcaaowndp,g_fcaa_m.fcaacrtid,g_fcaa_m.fcaacrtdp,g_fcaa_m.fcaacrtdt, 
       g_fcaa_m.fcaamodid,g_fcaa_m.fcaamoddt,g_fcaa_m.fcaaownid_desc,g_fcaa_m.fcaaowndp_desc,g_fcaa_m.fcaacrtid_desc, 
       g_fcaa_m.fcaacrtdp_desc,g_fcaa_m.fcaamodid_desc
 
   #檢查是否允許此動作
   IF NOT afci200_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_fcaa_m_mask_o.* =  g_fcaa_m.*
   CALL afci200_fcaa_t_mask()
   LET g_fcaa_m_mask_n.* =  g_fcaa_m.*
   
   
 
   #顯示資料
   CALL afci200_show()
   
   WHILE TRUE
      LET g_fcaa_m.fcaa001 = g_fcaa001_t
 
      
      #寫入修改者/修改日期資訊
      LET g_fcaa_m.fcaamodid = g_user 
LET g_fcaa_m.fcaamoddt = cl_get_current()
LET g_fcaa_m.fcaamodid_desc = cl_get_username(g_fcaa_m.fcaamodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
      #end add-point
 
      #資料輸入
      CALL afci200_input("u")     
 
      #add-point:modify段修改後 name="modify.after_input"
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         LET g_fcaa_m.* = g_fcaa_m_t.*
         CALL afci200_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE fcaa_t SET (fcaamodid,fcaamoddt) = (g_fcaa_m.fcaamodid,g_fcaa_m.fcaamoddt)
       WHERE fcaaent = g_enterprise AND fcaa001 = g_fcaa001_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL afci200_set_act_visible()
   CALL afci200_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " fcaaent = " ||g_enterprise|| " AND",
                      " fcaa001 = '", g_fcaa_m.fcaa001, "' "
 
   #填到對應位置
   CALL afci200_browser_fill(g_wc,"")
 
   CLOSE afci200_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
