  <point name="function.afci200_default_ins" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 執行程式fcaa_t沒有資料　自動新增一筆資料
# Memo...........:
# Usage..........: CALL afci200_default_ins()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/04/15 By pomelo
# Modify.........:
################################################################################
PUBLIC FUNCTION afci200_default_ins()
DEFINE l_fcaa          RECORD
       fcaaent         LIKE fcaa_t.fcaaent,
       fcaa001         LIKE fcaa_t.fcaa001,
       fcaa002         LIKE fcaa_t.fcaa002,
       fcaa021         LIKE fcaa_t.fcaa021,
       fcaa022         LIKE fcaa_t.fcaa022,
       fcaa023         LIKE fcaa_t.fcaa023,
       fcaa011         LIKE fcaa_t.fcaa011,
       fcaa012         LIKE fcaa_t.fcaa012,
       fcaa031         LIKE fcaa_t.fcaa031,
       fcaaownid       LIKE fcaa_t.fcaaownid,
       fcaaowndp       LIKE fcaa_t.fcaaowndp,
       fcaacrtid       LIKE fcaa_t.fcaacrtid,
       fcaacrtdp       LIKE fcaa_t.fcaacrtdp,
       fcaacrtdt       DATETIME YEAR TO SECOND
                       END RECORD
                       
   INITIALIZE l_fcaa.* TO NULL
   LET l_fcaa.fcaaent   = g_enterprise
   LET l_fcaa.fcaa001   = '1'
   LET l_fcaa.fcaa002   = 100
   LET l_fcaa.fcaa011   = '1'
   LET l_fcaa.fcaa012   = 7
   LET l_fcaa.fcaa021   = '1'
   LET l_fcaa.fcaa022   = ''
   LET l_fcaa.fcaa023   = ''
   LET l_fcaa.fcaa031   = 0.5
   LET l_fcaa.fcaaownid = g_user
   LET l_fcaa.fcaaowndp = g_dept
   LET l_fcaa.fcaacrtid = g_user
   LET l_fcaa.fcaacrtdp = g_dept 
   LET l_fcaa.fcaacrtdt = cl_get_current()
   
   CALL s_transaction_begin()
   INSERT INTO fcaa_t(fcaaent,  fcaa001,  fcaa002,  fcaa011,  fcaa012,
                      fcaa021,  fcaa022,  fcaa023,  fcaa031,  fcaaownid,
                      fcaaowndp,fcaacrtid,fcaacrtdp,fcaacrtdt)
      VALUES(l_fcaa.fcaaent, l_fcaa.fcaa001, l_fcaa.fcaa002, l_fcaa.fcaa011, l_fcaa.fcaa012,
             l_fcaa.fcaa021, l_fcaa.fcaa022, l_fcaa.fcaa023, l_fcaa.fcaa031, l_fcaa.fcaaownid,
             l_fcaa.fcaaowndp, l_fcaa.fcaacrtid, l_fcaa.fcaacrtdp, l_fcaa.fcaacrtdt)
   IF SQLCA.sqlcode THEN
      CALL cl_err('Ins fcaa_t',SQLCA.sqlcode,1)
      CALL s_transaction_end('N','0')
   ELSE
      CALL s_transaction_end('Y','0')
   END IF
END FUNCTION]]>
  </point>
  <point name="function.afci200_chk_fcaa023" order="2" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 輸入的權重期數應與移期數N(fcaa022)相同
# Memo...........:
# Usage..........: CALL afci200_chk_fcaa023()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/04/15 By pomelo
# Modify.........:
################################################################################
PUBLIC FUNCTION afci200_chk_fcaa023()
DEFINE tok          base.StringTokenizer
DEFINE l_str        STRING
DEFINE l_num        STRING
DEFINE l_sum        LIKE type_t.num5
DEFINE l_cnt        LIKE type_t.num5
DEFINE l_success    LIKE type_t.num5

   LET g_errno = ''
   LET l_cnt = 0
   LET l_sum = 0

   LET tok = base.StringTokenizer.create(g_fcaa_m.fcaa023,",")
   WHILE tok.hasMoreTokens()
      LET l_num = tok.nextToken()
      LET l_success = 0
      #判斷輸入的字是否為數值
      CALL cl_chk_num(l_num,'N') RETURNING l_success
      IF NOT l_success THEN
         #輸入的字串必須由數值[0~9]或半形逗號[,]組成！;請重新輸入！
         LET g_errno = 'afc-00001'
         RETURN
      END IF
      LET l_sum = l_sum + l_num
      LET l_cnt = l_cnt + 1
   END WHILE
   
   IF l_cnt <> g_fcaa_m.fcaa022 THEN
      #輸入的權重數量必須與移期數N相同！;請重新輸入！
      LET g_errno = 'afc-00002'
      RETURN
   END IF
   
   IF l_sum <> 100 THEN
      #輸入的權重加總起來必須等於100！;請重新輸入！
      LET g_errno = 'afc-00003'
      RETURN
   END IF
END FUNCTION]]>
