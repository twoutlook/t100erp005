                  END IF
                  
                  
                  
                  #資料多語言用-增/改
                  
                  
                  #add-point:單頭新增後
                  {<point name="input.head.a_insert"/>}
                  #end add-point
                  
                  CALL s_transaction_end('Y','0')
               ELSE
                  CALL cl_err( "g_fcaa_m.fcaa001", "std-00006", 0 )
                  CALL s_transaction_end('N','0')
               END IF 
            ELSE
               #add-point:單頭修改前
               {<point name="input.head.b_update" mark="Y"/>}
               #end add-point
               UPDATE fcaa_t SET (fcaa001,fcaa002,fcaa021,fcaa022,fcaa023,fcaa011,fcaa012,fcaa031,fcaaent, 
                   fcaaownid,fcaaowndp,fcaacrtid,fcaacrtdp,fcaacrtdt,fcaamodid,fcaamoddt) = (g_fcaa_m.fcaa001, 
