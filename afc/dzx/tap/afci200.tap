               LET l_count = 1  
 
               SELECT COUNT(*) INTO l_count FROM fcaa_t
                WHERE fcaaent = g_enterprise AND fcaa001 = g_fcaa_m.fcaa001
 
               IF l_count = 0 THEN
               
                  #add-point:單頭新增前
                  {<point name="input.head.b_insert" mark="Y"/>}
                  #end add-point
               
                  INSERT INTO fcaa_t (fcaaent,fcaa001,fcaa002,fcaa021,fcaa022,fcaa023,fcaa011,fcaa012, 
                      fcaa031,fcaaent,fcaaownid,fcaaowndp,fcaacrtid,fcaacrtdp,fcaacrtdt,fcaamodid,fcaamoddt) 

                  VALUES (g_enterprise,g_fcaa_m.fcaa001,g_fcaa_m.fcaa002,g_fcaa_m.fcaa021,g_fcaa_m.fcaa022, 
