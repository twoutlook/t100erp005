  <section id="afci200.modify" ver="3" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION afci200_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point
   
   IF g_fcaa_m.fcaa001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF 
   
   EXECUTE afci200_master_referesh USING g_fcaa_m.fcaa001 INTO g_fcaa_m.fcaa001,g_fcaa_m.fcaa002,g_fcaa_m.fcaa021, 
       g_fcaa_m.fcaa022,g_fcaa_m.fcaa023,g_fcaa_m.fcaa011,g_fcaa_m.fcaa012,g_fcaa_m.fcaa031,g_fcaa_m.fcaaent, 
       g_fcaa_m.fcaaownid,g_fcaa_m.fcaaowndp,g_fcaa_m.fcaacrtid,g_fcaa_m.fcaacrtdp,g_fcaa_m.fcaacrtdt, 
       g_fcaa_m.fcaamodid,g_fcaa_m.fcaamoddt
 
   ERROR ""
  
   LET g_fcaa001_t = g_fcaa_m.fcaa001
 
   
   CALL s_transaction_begin()
   
   OPEN afci200_cl USING g_enterprise,g_fcaa_m.fcaa001
   IF STATUS THEN
      CALL cl_err("OPEN afci200_cl:", STATUS, 1)
      CLOSE afci200_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH afci200_cl INTO g_fcaa_m.fcaa001,g_fcaa_m.fcaa002,g_fcaa_m.fcaa021,g_fcaa_m.fcaa022,g_fcaa_m.fcaa023, 
       g_fcaa_m.fcaa011,g_fcaa_m.fcaa012,g_fcaa_m.fcaa031,g_fcaa_m.fcaaent,g_fcaa_m.fcaaownid,g_fcaa_m.fcaaownid_desc, 
       g_fcaa_m.fcaaowndp,g_fcaa_m.fcaaowndp_desc,g_fcaa_m.fcaacrtid,g_fcaa_m.fcaacrtid_desc,g_fcaa_m.fcaacrtdp, 
       g_fcaa_m.fcaacrtdp_desc,g_fcaa_m.fcaacrtdt,g_fcaa_m.fcaamodid,g_fcaa_m.fcaamodid_desc,g_fcaa_m.fcaamoddt 

 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err("fcaa_t",SQLCA.sqlcode,0)
      CLOSE afci200_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
 
   CALL afci200_show()
   
   WHILE TRUE
      LET g_fcaa_m.fcaa001 = g_fcaa001_t
 
      
      #寫入修改者/修改日期資訊
      LET g_fcaa_m.fcaamodid = g_user 
LET g_fcaa_m.fcaamoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
 
      CALL afci200_input("u")     #欄位更改
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_fcaa_m.* = g_fcaa_m_t.*
         CALL afci200_show()
         CALL cl_err("",9001,0)
         EXIT WHILE
      END IF
 
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_fcaa_m.fcaa001,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE afci200_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_fcaa_m.fcaa001,"U")
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
]]>
