         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE apmt801_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR apmt801_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
   #  OPEN b_fill_cs USING g_enterprise,g_pmba_m.pmbadocno   #(ver:78)
                                               
      FOREACH b_fill_cs USING g_enterprise,g_pmba_m.pmbadocno INTO g_pmbg_d[l_ac].pmbg001,g_pmbg_d[l_ac].pmbg002, 
