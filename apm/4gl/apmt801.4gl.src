PUBLIC FUNCTION apmt801_set_required(p_cmd)
   DEFINE p_cmd     LIKE type_t.chr1
   IF g_pmba_m.pmba000 = 'U' THEN
      CALL cl_set_comp_required("pmba001",TRUE)
   END IF
   IF g_pmba_m.pmba051 = '2' OR g_pmba_m.pmba051 = '3' THEN
      CALL cl_set_comp_required("pmba053,pmba054,pmba055",TRUE)
   END IF
   #當客戶經營類型為'經銷商'時,'採購渠道'必填 20150625 add by beckxie---S
