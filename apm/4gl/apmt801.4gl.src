         AFTER INPUT
            IF INT_FLAG THEN
               EXIT DIALOG
            END IF
 
            #CALL cl_err_collect_show()      #錯誤訊息統整顯示
            #CALL cl_showmsg()
            DISPLAY BY NAME g_pmba_m.pmbadocno
                        
            #add-point:單頭INPUT後 name="input.head.after_input"
            
            #end add-point
                        
            IF p_cmd <> 'u' THEN
    
               CALL s_transaction_begin()
               
               #add-point:單頭新增前 name="input.head.b_insert"
               #geza 20150601 add ---------------------(S)
               IF g_pmba_m.pmba000 = 'I' THEN #add by geza 20160627 #160604-00009#108
                  LET l_str = g_pmba_m.pmba001 #add by geza 20160627 #160604-00009#108
                  #有设置自动编号的话就重新取值
                  LET l_cnt = 0
                  SELECT COUNT(distinct oofg001) INTO l_cnt
                    FROM oofg_t
                   WHERE oofgent = g_enterprise
                     AND oofg002 = '2'
                     AND oofgstus = 'Y'
                  IF l_cnt > 0 THEN
                     IF l_str.getIndexOf('&',1)>0 OR l_str IS NULL THEN
                       CALL s_aooi390_gen('2') RETURNING l_success,l_pmba001,l_oofg_return
                       IF l_success then 
                         LET g_pmba_m.pmba001=l_pmba001
                       END IF 
                     END IF 
                  END IF
                  CALL s_aooi390_get_auto_no('2',g_pmba_m.pmba001) RETURNING l_success,g_pmba_m.pmba001
                  
                  IF NOT l_success THEN
                     CALL s_transaction_end('N','0')
                     NEXT FIELD CURRENT
                  END IF
                  
                  #如果新的編碼與舊的不一樣 應該做更新 
                  IF g_pmba_m_o.pmba001 != g_pmba_m.pmba001 AND NOT cl_null(g_pmba_m_o.pmba001) THEN
                     LET l_count_chk = 0
