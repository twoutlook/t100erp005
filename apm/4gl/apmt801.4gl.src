# Date & Author..: 20150610 By beckxie
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt801_pmbc002_def(l_pmbc002)
   DEFINE l_count_pmbc LIKE type_t.num5
   DEFINE l_pmbc002    LIKE pmbc_t.pmbc002 
   #150519-00031#1 20150609 add by beckxie ---S
   IF g_pmba_m.pmba000 = "I" AND (NOT cl_null(g_pmba_m.pmba001)) THEN
      LET l_count_pmbc = 0
      SELECT COUNT(*) INTO l_count_pmbc
      FROM pmbc_t
      WHERE pmbcent=g_enterprise AND pmbcdocno=g_pmba_m.pmbadocno
      IF l_count_pmbc = 0 THEN
         INSERT INTO pmbc_t (pmbcent,pmbcdocno,pmbc001,pmbc002,pmbc003,pmbc004,pmbcstus)
         VALUES (g_enterprise,g_pmba_m.pmbadocno,g_pmba_m.pmba001,g_pmba_m.pmba001,"1","Y","Y")
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "ins pmbc_t"
            LET g_errparam.popup = TRUE
            CALL cl_err()
            
            CALL s_transaction_end('N','0')
            #CONTINUE DIALOG
         END IF
         INSERT INTO pmbc_t (pmbcent,pmbcdocno,pmbc001,pmbc002,pmbc003,pmbc004,pmbcstus)
         VALUES (g_enterprise,g_pmba_m.pmbadocno,g_pmba_m.pmba001,g_pmba_m.pmba001,"2","Y","Y")
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "ins pmbc_t"
            LET g_errparam.popup = TRUE
            CALL cl_err()
            
            CALL s_transaction_end('N','0')
            #CONTINUE DIALOG
         END IF
         INSERT INTO pmbc_t (pmbcent,pmbcdocno,pmbc001,pmbc002,pmbc003,pmbc004,pmbcstus)
         VALUES (g_enterprise,g_pmba_m.pmbadocno,g_pmba_m.pmba001,g_pmba_m.pmba001,"3","Y","Y")
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "ins pmbc_t"
            LET g_errparam.popup = TRUE
            CALL cl_err()
            
            CALL s_transaction_end('N','0')
            #CONTINUE DIALOG
         END IF
      ELSE
         UPDATE pmbc_t
            SET pmbc002=g_pmba_m.pmba001
          WHERE pmbcent=g_enterprise AND pmbcdocno=g_pmba_m.pmbadocno AND pmbc002=l_pmbc002
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "upd pmbc_t"
            LET g_errparam.popup = TRUE
            CALL cl_err()
            
            CALL s_transaction_end('N','0')
            #CONTINUE DIALOG
         END IF
      END IF
   END IF
   #150519-00031#1 20150609 add by beckxie ---E 
END FUNCTION

