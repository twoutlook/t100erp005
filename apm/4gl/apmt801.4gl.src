PUBLIC FUNCTION apmt801_chk_pmba001()
   DEFINE l_cnt       LIKE type_t.num5
   DEFINE l_pmaa002   LIKE pmaa_t.pmaa002   #160810-00008#1 160829 by lori add
   DEFINE l_pmaa092   LIKE pmaa_t.pmaa092   #160810-00008#1 160829 by lori add
   
   LET g_errno = ''
   CASE g_pmba_m.pmba000
      WHEN 'I'
         LET l_cnt = 0
         SELECT COUNT(*) INTO l_cnt FROM pmaa_t
          WHERE pmaaent = g_enterprise
            AND pmaa001 = g_pmba_m.pmba001
         IF l_cnt > 0 THEN
            #輸入的交易對象編號已存在 交易對象主檔 中
            LET g_errno = 'apm-00240'
         END IF

         LET l_cnt = 0
         SELECT COUNT(*) INTO l_cnt FROM pmba_t
          WHERE pmbaent = g_enterprise
            AND pmba001 = g_pmba_m.pmba001
            AND pmbastus != 'X'
         IF l_cnt > 0 THEN
            #輸入的交易對象編號已存在 交易對象准入檔 中
            LET g_errno = 'apm-00241'
         END IF

      WHEN 'U'
         LET l_cnt = 0
         SELECT COUNT(*) INTO l_cnt FROM pmaa_t
          WHERE pmaaent = g_enterprise
            AND pmaa001 = g_pmba_m.pmba001
         IF l_cnt = 0 THEN
            #輸入的交易對象編號不存在 交易對象主檔 中
            LET g_errno = 'apm-00242'
         END IF

         #160810-00008#1 160829 by lori mark---(S)
         #LET l_cnt = 0
         #SELECT COUNT(*) INTO l_cnt FROM pmba_t
         # WHERE pmbaent = g_enterprise
         #   AND pmbadocno != g_pmba_m.pmbadocno
         #   AND pmba001 = g_pmba_m.pmba001
         #   AND pmbastus = 'N'
         #IF l_cnt > 0 THEN
         #   #已存在一張相同交易對象編號,但未拋轉的交易對象准入單!
         #   LET g_errno = 'apm-00243'
         #END IF
         #160810-00008#1 160829 by lori mark---(E)
         
         #160810-00008#1 160829 by lori add---(S)
         LET l_pmaa002 = ''
         LET l_pmaa092 = ''
         
         SELECT pmaa002,pmaa092 INTO l_pmaa002,l_pmaa092 
           FROM pmaa_t
          WHERE pmaaent = g_enterprise
            AND pmaa001 = g_pmba_m.pmba001         
         
         CALL apmt801_carry_pmaa()
         
         CASE
            WHEN g_pmba_m.pmba002 = '1'                
               IF l_pmaa002 = '2' THEN               
                  IF cl_ask_confirm('apm-00400') THEN 
                  #原2.客戶,欲改成1.供應商-->交易對象改為3.兩者皆是                                 
                     LET g_pmba_m.pmba002 = '3'                  
                     #客戶資訊預設值
                     LET g_pmba_m.pmba230 = "1"
                     LET g_pmba_m.pmba092 = "2"
                     LET g_pmba_m.pmba073 = "01"
                     LET g_pmba_m.pmbb080 = "A"
                     LET g_pmba_m.pmbb082 = g_dlang
                     LET g_pmba_m.pmbb085 = "10"
                     LET g_pmba_m.pmbb107 = "1"
                     LET g_pmba_m.pmbb108 = "1"
                     LET g_pmba_m.pmbb099 = "Y"
                     LET g_pmba_m.pmbb101 = "Y"
                  
                     DISPLAY BY NAME g_pmba_m.pmba002,g_pmba_m.pmba230,g_pmba_m.pmba092,
                                     g_pmba_m.pmba073,g_pmba_m.pmbb080,g_pmba_m.pmbb082,
                                     g_pmba_m.pmbb085,g_pmba_m.pmbb107,g_pmba_m.pmbb108,
                                     g_pmba_m.pmbb099,g_pmba_m.pmbb101
                  ELSE
                     LET g_pmba_m.pmba001 = g_pmba_m_o.pmba001
                  END IF   
               END IF                  
               
            WHEN g_pmba_m.pmba002 = '2' AND l_pmaa092 = '2'  
               IF l_pmaa002 = '1' THEN
               #原1.供應商,欲改成2.客戶(經銷商)-->交易對象改為3.兩者皆是
                  IF cl_ask_confirm('apm-00401') THEN
                     #此經銷商代號的交易對象類別,不為[2.客戶]是否將交易對象類別修改為[3.交易對象],並帶出相關資料？
                     LET g_pmba_m.pmba002 = '3'
                     LET g_pmba_m.pmba092 = '2'
                     LET g_pmba_m.pmba230 = '1'
                     DISPLAY BY NAME g_pmba_m.pmba002,g_pmba_m.pmba092,g_pmba_m.pmba230
                  ELSE
                     LET g_pmba_m.pmba001 = g_pmba_m_o.pmba001
                  END IF
              END IF  
              
            WHEN  g_pmba_m.pmba002 = '2' AND l_pmaa092 = '1'
               IF l_pmaa002 = '1' THEN
               #原1.供應商,欲改成2.客戶(一般)-->交易對象改為3.兩者皆是
                  IF cl_ask_confirm('apm-00494') THEN
                     LET g_pmba_m.pmba002 = '3'
                     LET g_pmba_m.pmba092 = '1'
                     LET g_pmba_m.pmba230 = '1'
                     DISPLAY BY NAME g_pmba_m.pmba002,g_pmba_m.pmba092,g_pmba_m.pmba230
                  ELSE
                     LET g_pmba_m.pmba001 = g_pmba_m_o.pmba001
                  END IF
               END IF                  
         END CASE
         #160810-00008#1 160829 by lori add---(E)
      OTHERWISE
         LET g_errno = 'art-00013'
   END CASE
   
   IF cl_null(g_errno) AND g_pmba_m.pmba004 = '1' THEN
      LET g_pmba_m.pmba005 = g_pmba_m.pmba001
      CALL apmt801_pmba005_ref()
   END IF
END FUNCTION

PUBLIC FUNCTION apmt801_pmba270_visible()
   CASE g_pmba_m.pmba270
      WHEN '1'
         CALL cl_set_comp_visible('group42',TRUE)
         CALL cl_set_comp_visible('group43',FALSE)
         CALL apmt801_pmba271_show()
      WHEN '2'
         CALL cl_set_comp_visible('group42',FALSE)
         CALL cl_set_comp_visible('group43',TRUE)
         CALL apmt801_pmba272_show()
      OTHERWISE
         CALL cl_set_comp_visible('group42',TRUE)
         CALL cl_set_comp_visible('group43',FALSE)
