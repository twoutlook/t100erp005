         LET g_sql = "SELECT  DISTINCT pmbg001,pmbg002,pmbg003,pmbg004,pmbg007,pmbg008,pmbgacti ,t1.oocql004 FROM pmbg_t", 
                
                     " INNER JOIN pmba_t ON pmbaent = " ||g_enterprise|| " AND pmbadocno = pmbgdocno ",
 
