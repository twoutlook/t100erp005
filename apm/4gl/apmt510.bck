DEFINE l_oodb011   LIKE oodb_t.oodb011    #取得稅別類型1:正常稅率2:依料件設定  
DEFINE l_ooba002   LIKE ooba_t.ooba002    #單據別編號  20150827-dorislai-add
   LET l_n = 0
