{<section id="apmt510.set_no_entry" >}
#+ 單頭欄位關閉設定
PRIVATE FUNCTION apmt510_set_no_entry(p_cmd)
   #add-point:set_no_entry段define(客製用) name="set_no_entry.define_customerization"
   
   #end add-point     
   DEFINE p_cmd   LIKE type_t.chr1   
   #add-point:set_no_entry段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="set_no_entry.define"
   DEFINE l_pmam003   LIKE pmam_t.pmam003
   DEFINE l_cnt       LIKE type_t.num5
   #end add-point     
   
   #add-point:Function前置處理  name="set_no_entry.pre_function"
   
   #end add-point
   
   IF p_cmd = 'u' AND g_chkey = 'N' THEN
      CALL cl_set_comp_entry("pmeedocno,pmee900",FALSE)
      #根據azzi850使用者身分開關特定欄位
      IF NOT cl_null(g_no_entry) THEN
         CALL cl_set_comp_entry(g_no_entry,FALSE)
      END IF
      #add-point:set_no_entry段欄位控制 name="set_no_entry.field_control"
      #CALL cl_set_comp_entry("pmeedocdt",FALSE)
      #end add-point 
   END IF 
   
   IF p_cmd = 'u' THEN  #docno,ld欄位確認是絕對關閉
      CALL cl_set_comp_entry("pmeedocno",FALSE)
   END IF 
 
#  IF p_cmd = 'u' THEN  #docdt欄位依照設定關閉(FALSE則為設定不同意修正) #(ver:78)
      IF NOT cl_chk_update_docdt() THEN
         CALL cl_set_comp_entry("pmeedocdt",FALSE)
      END IF
#  END IF 
   
   #add-point:set_no_entry段欄位控制後 name="set_no_entry.after_control"
   IF g_argv[1] = '1' THEN   #委外採購單
      CALL cl_set_comp_entry("pmee005,pmee006,pmee007",FALSE)
   END IF
   
   #單頭"多角性質"=3.代採購指定供應商時，最終供應商栏位才開放輸入，否則由系統清為空白
   IF g_pmee_m.pmee006 != '3' THEN
      CALL cl_set_comp_entry("pmee052",FALSE)
      IF g_pmee_m.pmee007 <> '9' THEN          #160627-00012#2 add
         LET g_pmee_m.pmee052 = ''
      END IF                                   #160627-00012#2 add
   END IF
   
   #單頭"多角性質"=7.點對點時，兩角目的據點欄位才可維護
   IF g_pmee_m.pmee006 != '7' THEN
      CALL cl_set_comp_entry("pmee053",FALSE)
      LET g_pmee_m.pmee053 = ''
   END IF
   
   #單頭"多角性質"=2.代採購或3.代採購指定供應商時，多角流程代碼欄位才允許輸入
   IF g_pmee_m.pmee006 NOT MATCHES '[23]' THEN
      CALL cl_set_comp_entry("pmee051",FALSE)
      IF g_pmee_m.pmee007 <> '9' THEN          #160627-00012#2 add
         LET g_pmee_m.pmee051 = ''
      END IF                                   #160627-00012#2 add   
   END IF
   
   #只有維護多期帳款時，才能輸入預付款發票開立方式 
   CALL cl_set_comp_entry("pmee046",FALSE)

   #資料來源類型=8.預先訂單
   IF g_pmee_m.pmee007 = '8' AND (NOT cl_null(g_pmee_m.pmee008)) THEN
      CALL cl_set_comp_entry("pmee004,pmee005,pmee006,pmee009,pmee010",FALSE)
      CALL cl_set_comp_entry("pmee011,pmee015",FALSE)
      CALL cl_set_comp_entry("pmee017,pmee018,pmee019,pmee020",FALSE)
      CALL cl_set_comp_entry("pmee021,pmee022,pmee023,pmee024,pmee025",FALSE)
      CALL cl_set_comp_entry("pmee026,pmee027,pmee028,pmee029",FALSE)
      CALL cl_set_comp_entry("pmee032,pmee033",FALSE)
      CALL cl_set_comp_entry("pmee043,pmee044,pmee046",FALSE)
      CALL cl_set_comp_entry("pmee051,pmee052,pmee053,pmee054,pmee055",FALSE)
      
   END IF
   
   #留置原因
   CALL cl_set_comp_entry("pmee043",FALSE)
   
   #多角已拋轉單據不可修改"多角性質"、"多角流程代碼"
   IF NOT cl_null(g_pmee_m.pmee031) THEN
      CALL cl_set_comp_entry("pmee006,pmee051",FALSE)
      CALL cl_set_comp_entry("pmee005,pmee007,pmee008",FALSE)  #160627-00012#2 add
   END IF
   
#2015/06/16 by stellar mark ----- (S)
#畫面設定不能輸入
#   #2014/10/27 by stellar add ---------------- (S)
#   #已收貨的採購單，其供應商不可維護
#   LET l_cnt = 0
#   SELECT COUNT(*) INTO l_cnt FROM pmds_t,pmdt_t
#    WHERE pmdsent = g_enterprise
#      AND pmdsent = pmdtent
#      AND pmdsdocno = pmdtdocno
#      AND pmdsstus <> 'X'
#      AND pmdt001 = g_pmee_m.pmeedocno
#   IF NOT cl_null(l_cnt) AND l_cnt > 0 THEN
#      CALL cl_set_comp_entry("pmee004",FALSE)
#   END IF
#   #2014/10/27 by stellar add ---------------- (E)
#2015/06/16 by stellar mark ----- (E)
   #end add-point 
 
