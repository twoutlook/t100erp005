   #則[C:子件特性]的值預設'2:CKD'或是'3:SKD'
   LET g_pmeg_d[l_ac].pmeg008 = ''
   LET g_pmeg_d[l_ac].pmeg008_desc = ''
   LET g_pmeg3_d[l_ac].pmeg033 = ''
   LET g_pmeg_d[l_ac].pmeg019 = '1'
   LET l_imaf158 = ''
   SELECT imaf158,imaf165,imaf015 INTO l_imaf158,g_pmeg3_d[l_ac].pmeg033,g_pmeg_d[l_ac].pmeg008 
     FROM imaf_t
    WHERE imafent = g_enterprise AND imafsite = g_site AND imaf001 = g_pmeg_d[l_ac].pmeg001
   IF l_imaf158 = '1' THEN
      LET g_pmeg_d[l_ac].pmeg019 = '2'
   END IF
   IF l_imaf158 = '2' THEN
      LET g_pmeg_d[l_ac].pmeg019 = '3'
   END IF

   CALL apmt510_pmeg008_ref()

   #整體參數使用採購計價單位時:
   #[C:計價單位]=[T:料件據點進銷存檔].[C:採購計價單位] 
   IF cl_get_para(g_enterprise,g_site,'S-BAS-0019') = "Y" THEN
      SELECT imaf144 INTO g_pmeg_d[l_ac].pmeg010 FROM imaf_t
       WHERE imafent = g_enterprise AND imafsite = g_site AND imaf001 = g_pmeg_d[l_ac].pmeg001
   END IF
   CALL apmt510_pmeg010_ref()

   #若採購料件有設置'交易對象對應料號'(apmi070)有供應商料號時，需將對應的料號抓出顯示在
   #[C:供應商料號]欄位上，若有設置多筆對應料號時以有勾選主要對應料那一筆為預設值
   LET g_pmeg3_d[l_ac].pmeg027 = ''
   SELECT pmao004 INTO g_pmeg3_d[l_ac].pmeg027 
     FROM pmao_t
    WHERE pmaoent = g_enterprise AND pmao001 = g_pmee_m.pmee004 AND pmao002 = g_pmeg_d[l_ac].pmeg001
      AND pmao003 = g_pmeg_d[l_ac].pmeg002 AND pmao007 = 'Y'
      AND pmao000 = '1'    #161221-00064#12 add 
   IF cl_null(g_pmeg3_d[l_ac].pmeg027) THEN
      SELECT pmao004 INTO g_pmeg3_d[l_ac].pmeg027 FROM pmao_t
       WHERE pmaoent = g_enterprise AND pmao001 = g_pmee_m.pmee004 AND pmao002 = g_pmeg_d[l_ac].pmeg001
         AND pmao000 = '1'    #161221-00064#12 add 
         AND pmao003 = g_pmeg_d[l_ac].pmeg002 AND rownum = 1
   END IF      
   SELECT pmao009,pmao010
     INTO g_pmeg3_d[l_ac].pmeg027_desc, g_pmeg3_d[l_ac].pmeg027_desc_desc
     FROM pmao_t
    WHERE pmao001 = g_pmee_m.pmee004
      AND pmao002 = g_pmeg_d[l_ac].pmeg001
      AND pmao003 = g_pmeg_d[l_ac].pmeg002
      AND pmao004 = g_pmeg3_d[l_ac].pmeg027    
      AND pmao000 = '1'    #161221-00064#12 add 
      AND pmaoent = g_enterprise                      
   DISPLAY BY NAME g_pmeg3_d[l_ac].pmeg027_desc, g_pmeg3_d[l_ac].pmeg027_desc_desc   
   DISPLAY BY NAME g_pmeg_d[l_ac].pmeg006,g_pmeg_d[l_ac].pmeg006_desc,
                   g_pmeg_d[l_ac].pmeg008,g_pmeg_d[l_ac].pmeg008_desc,
                   g_pmeg_d[l_ac].pmeg010,g_pmeg_d[l_ac].pmeg010_desc,
                   g_pmeg_d[l_ac].pmegunit,g_pmeg_d[l_ac].pmegunit_desc,
