PRIVATE FUNCTION apmt510_pmeg025_chk()
DEFINE l_ooef012   LIKE ooef_t.ooef012
DEFINE l_n         LIKE type_t.num5
DEFINE r_success   LIKE type_t.num5

       LET r_success = TRUE

       IF NOT cl_null(g_pmeg3_d[l_ac].pmeg025) THEN
          #先判斷是否存在與對應庫位的聯絡地址檔中
          LET l_n = 0
          LET l_ooef012 = ''
          IF (NOT cl_null(g_pmeg_d[l_ac].pmegunit)) AND (NOT cl_null(g_pmeg3_d[l_ac].pmeg028)) THEN
             SELECT inaa004 INTO l_ooef012 FROM inaa_t 
              WHERE inaaent = g_enterprise AND inaasite = g_pmeg_d[l_ac].pmegunit AND inaa001 = g_pmeg3_d[l_ac].pmeg028

             SELECT COUNT(*) INTO l_n FROM oofb_t 
              WHERE oofbent = g_enterprise AND oofb002 = l_ooef012 
                AND oofb019 = g_pmeg3_d[l_ac].pmeg025 AND oofbstus = 'Y' AND oofb008 = '3'
          END IF
          IF l_n = 0 THEN
             #若不存在與對應庫位的聯絡地址檔中，在判斷是否存在與對應營運據點聯絡地址檔中，若都不存在，則報錯
             LET l_ooef012 = ''
             IF NOT cl_null(g_pmeg_d[l_ac].pmegunit) THEN
                SELECT ooef012 INTO l_ooef012 FROM ooef_t 
                 WHERE ooefent = g_enterprise AND ooef001 = g_pmeg_d[l_ac].pmegunit
                SELECT COUNT(*) INTO l_n FROM oofb_t 
                 WHERE oofbent = g_enterprise AND oofb002 = l_ooef012 
                   AND oofb019 = g_pmeg3_d[l_ac].pmeg025 AND oofbstus = 'Y' AND oofb008 = '3'
                IF l_n = 0 THEN
                   INITIALIZE g_errparam TO NULL
                   LET g_errparam.code = 'apm-00216'
                   LET g_errparam.extend = g_pmeg3_d[l_ac].pmeg025
                   LET g_errparam.popup = TRUE
                   CALL cl_err()

                   LET r_success = FALSE
                   RETURN r_success
                END IF
             END IF
          END IF
       END IF
       
       RETURN r_success
END FUNCTION

