PRIVATE FUNCTION apmt510_action_chk()
   #add-point:action_chk段define(客製用) name="action_chk.define_customerization"
   
   #end add-point
   #add-point:action_chk段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="action_chk.define"
   
   #end add-point
   
   #add-point:action_chk段action_chk name="action_chk.action_chk"
   #160818-00017#27--s
   SELECT pmeestus INTO g_pmee_m.pmeestus FROM pmee_t WHERE pmeeent = g_enterprise AND pmeedocno = g_pmee_m.pmeedocno AND pmee900 = g_pmee_m.pmee900
   IF (g_action_choice="modify" OR g_action_choice="delete" OR g_action_choice="modify_detail")  THEN
      LET g_errno = NULL
      CASE g_pmee_m.pmeestus  
        WHEN "Y"
           LET g_errno = 'sub-00178'
        WHEN "W"
           LET g_errno = 'sub-01347'
		WHEN "C"
           LET g_errno = 'ain-00197'   
        WHEN "UH"
           LET g_errno = 'sub-01358'
        WHEN "H"
           LET g_errno = 'sub-01348'
        WHEN "X"
           LET g_errno = 'sub-00229'
      
      END CASE
      
      IF NOT cl_null(g_errno) THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = g_errno
         LET g_errparam.extend = g_pmee_m.pmeestus
         LET g_errparam.popup = TRUE
         CALL cl_err()
         LET g_errno = NULL
         CALL apmt510_set_act_visible()
         CALL apmt510_set_act_no_visible()
         CALL apmt510_show()
         RETURN FALSE
      END IF
   END IF
   #160818-00017#27---e
   #end add-point
      
   RETURN TRUE
   
END FUNCTION
 
