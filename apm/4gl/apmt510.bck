   LET g_pmeg_d[l_ac].pmeg006_desc = ''
   LET g_pmeg_d[l_ac].pmeg010 = ''
   LET g_pmeg_d[l_ac].pmeg010_desc = ''
      
   LET g_pmeg3_d[l_ac].pmeg028 = ''
   LET g_pmeg3_d[l_ac].pmeg028_desc = ''
   LET g_pmeg3_d[l_ac].pmeg029 = ''
   LET g_pmeg3_d[l_ac].pmeg029_desc = ''
   LET g_pmeg3_d[l_ac].pmeg003 = ''
   LET g_pmeg3_d[l_ac].pmeg003_desc = ''
   LET g_pmeg3_d[l_ac].pmeg033 = ''
     
   #先判斷這個供應商是否有設多個當前採購控制組範圍內的供應商預設條件，則開窗，讓user 選擇帶哪一個控制組的資料
   LET l_controlno = ''
   CALL s_control_get_group('4',g_user,g_dept) RETURNING l_success,l_controlno

   IF NOT cl_null(l_controlno) THEN
      SELECT COUNT(*) INTO l_n FROM pmap_t
       WHERE pmapent = g_enterprise AND pmapsite = g_site AND pmap001 = g_pmee_m.pmee004
         AND pmap002 = l_controlno AND pmap003 = g_pmeg_d[l_ac].pmeg001 AND pmap004 = g_pmeg_d[l_ac].pmeg002
      #若採購料件有設置'供應商控制組料件預設條件'(apmi121)時，則需將設置的預設條件值預設到採購單對應欄位
      IF l_n > 0 THEN
         SELECT pmap006,pmap008,pmap009,pmap010,pmap011,pmap012,pmap014,pmap005
           INTO g_pmeg_d[l_ac].pmeg006,g_pmeg_d[l_ac].pmeg010,g_pmeg_d[l_ac].pmegunit,g_pmeg3_d[l_ac].pmeg028,g_pmeg3_d[l_ac].pmeg029,
                g_pmeg3_d[l_ac].pmeg025,g_pmeg3_d[l_ac].pmeg031,g_pmeg3_d[l_ac].pmeg003
           FROM pmdp_t
          WHERE pmdpent = g_enterprise AND pmapsite = g_site AND pmap001 = g_pmee_m.pmee004
            AND pmap002 = l_controlno AND pmap003 = g_pmeg_d[l_ac].pmeg001 AND pmap004 = g_pmeg_d[l_ac].pmeg002
      END IF
   END IF

   IF cl_null(l_controlno) OR l_n = 0 THEN
      #沒有設置'供應商控制組料件預設條件'(apmi121)才改抓料件進銷存檔預設的條件
      SELECT imaf143,imaf144,imaf091,imaf092,imaf157
        INTO g_pmeg_d[l_ac].pmeg006,g_pmeg_d[l_ac].pmeg010,g_pmeg3_d[l_ac].pmeg028,g_pmeg3_d[l_ac].pmeg029,g_pmeg3_d[l_ac].pmeg003
        FROM imaf_t
       WHERE imafent = g_enterprise AND imafsite = g_site AND imaf001 = g_pmeg_d[l_ac].pmeg001
   END IF
   
   #dorislai-20150827-add----(S)
   #預帶預設庫位的值，aooi200抓值優先順序：預設欄位>應用參數
   IF cl_null(g_pmeg3_d[l_ac].pmeg028) THEN
      #預設欄位
      CALL s_aooi200_get_slip(g_pmee_m.pmeedocno) RETURNING l_success,l_ooba002
      IF l_success THEN
