            LET g_ooff_d4_color[p_i].ooff013 = 'red'
          WHEN 'ooff014'                       
            LET g_ooff_d4_color[p_i].ooff014 = 'red'
      END CASE
   END FOREACH   
   
END FUNCTION

#单头备注单身写入变更历程档
#161031-00025#36 add
PRIVATE FUNCTION apmt510_ooff_ins_pmek()
DEFINE l_prog       LIKE  type_t.chr10
DEFINE l_pmdl005    LIKE pmdl_t.pmdl005
DEFINE l_ooff012    LIKE ooff_t.ooff012
DEFINE l_ooff013    LIKE ooff_t.ooff013
DEFINE l_ooff013_1  LIKE ooff_t.ooff013
DEFINE l_n          LIKE type_t.num5
DEFINE r_success    LIKE type_t.num5
DEFINE l_ooff014    LIKE ooff_t.ooff014
DEFINE l_ooff014_1  LIKE ooff_t.ooff014

   LET r_success = TRUE
   
   SELECT pmdl005 INTO l_pmdl005 FROM pmdl_t WHERE pmdlent = g_enterprise AND pmdldocno = g_pmee_m.pmeedocno
   CASE l_pmdl005
      WHEN '2'  #委外採購單
         LET l_prog = 'apmt501'
      WHEN '5'  #重覆性生產委外採購
        LET l_prog = 'apmt503'
      WHEN '6'  #借貨採購
        LET l_prog = 'apmt504'
      OTHERWISE
        LET l_prog = 'apmt500' 
   END CASE   

   DELETE FROM pmek_t WHERE pmekent = g_enterprise 
      AND pmekdocno = g_pmee_m.pmeedocno AND pmekseq = 0 AND pmek001 = g_pmee_m.pmee900
      AND pmek002 LIKE 'ooff%'
   
   DECLARE ooff_ins_pmek_cs CURSOR FOR
       SELECT ooff012,ooff013,ooff014 FROM ooff_t 
           WHERE ooffent = g_enterprise AND ooff001 = '6' AND ooff002 = g_prog 
             AND ooff003 = g_pmee_m.pmeedocno AND ooff004 = g_pmee_m.pmee900 AND ooff005 = 0
   FOREACH ooff_ins_pmek_cs INTO l_ooff012,l_ooff013,l_ooff014
      #判断当前的控制类型+备注是否存在采购单中
      LET l_n = 0
      SELECT COUNT(1) INTO l_n FROM ooff_t 
           WHERE ooffent = g_enterprise AND ooff001 = '6' AND ooff002 = l_prog 
             AND ooff003 = g_pmee_m.pmeedocno AND ooff004 = 0 
             AND ooff012 = l_ooff012 AND ooff013 = l_ooff013 AND ooff014 = l_ooff014
      IF cl_null(l_n) OR l_n = 0 THEN
         #再判断当前的控制类型是否存在，若控制类型存在，比对备注是否一致，若不一致，则说明是修改备注，否则则是新增了备注资料
         SELECT ooff013,ooff014 INTO l_ooff013_1,l_ooff014_1 FROM ooff_t 
              WHERE ooffent = g_enterprise AND ooff001 = '6' AND ooff002 = l_prog 
                AND ooff003 = g_pmee_m.pmeedocno AND ooff004 = 0 
                AND ooff012 = l_ooff012
         
         IF NOT cl_null(l_ooff013_1) THEN
            IF l_ooff013_1 <> l_ooff013 THEN
               #其說明的SQL
               LET g_pmek007 = ''
               
               IF NOT s_apmt510_ins_pmek(0,l_ooff012,0,"ooff013",'2',l_ooff013_1,l_ooff013,g_pmee_m.pmeedocno,g_pmee_m.pmee900,g_pmek007) THEN      
                  LET r_success = FALSE
                  RETURN r_success
               END IF
            END IF
            IF (l_ooff014 <> l_ooff014_1) OR
               (l_ooff014 IS NULL AND l_ooff014_1 IS NOT NULL) OR
               (l_ooff014 IS NOT NULL AND l_ooff014_1 IS NULL) THEN
               #其說明的SQL
               LET g_pmek007 = ''
               
               IF NOT s_apmt510_ins_pmek(0,l_ooff012,0,"ooff014",'2',l_ooff014_1,l_ooff014,g_pmee_m.pmeedocno,g_pmee_m.pmee900,g_pmek007) THEN      
                  LET r_success = FALSE
                  RETURN r_success
               END IF
            END IF
         ELSE
            #其說明的SQL
            LET g_pmek007 = ''
            
            IF NOT s_apmt510_ins_pmek(0,l_ooff012,0,"ooff012",'3','',l_ooff012,g_pmee_m.pmeedocno,g_pmee_m.pmee900,g_pmek007) THEN      
               LET r_success = FALSE
               RETURN r_success
            END IF
            
            #新增資料到變更歷程檔
            IF NOT s_apmt510_ins_pmek(0,l_ooff012,0,"ooff013",'3','',l_ooff013,g_pmee_m.pmeedocno,g_pmee_m.pmee900,g_pmek007) THEN      
               LET r_success = FALSE
               RETURN r_success
            END IF
            IF NOT cl_null(l_ooff014) THEN
               IF NOT s_apmt510_ins_pmek(0,l_ooff012,0,"ooff014",'3','',l_ooff014,g_pmee_m.pmeedocno,g_pmee_m.pmee900,g_pmek007) THEN      
                  LET r_success = FALSE
                  RETURN r_success
               END IF
            END IF
         END IF
      ELSE
         #資料一樣，表示無變更，將變更歷程檔刪除
         IF NOT s_apmt510_del_pmek(0,l_ooff012,0,"ooff012",g_pmee_m.pmeedocno,g_pmee_m.pmee900) THEN
            LET r_success = FALSE
            RETURN r_success
         END IF   
         IF NOT s_apmt510_del_pmek(0,l_ooff012,0,"ooff013",g_pmee_m.pmeedocno,g_pmee_m.pmee900) THEN
            LET r_success = FALSE
            RETURN r_success
         END IF 
         IF NOT s_apmt510_del_pmek(0,l_ooff012,0,"ooff014",g_pmee_m.pmeedocno,g_pmee_m.pmee900) THEN
            LET r_success = FALSE
            RETURN r_success
         END IF          
      END IF
   END FOREACH
   
   RETURN r_success

END FUNCTION

 
