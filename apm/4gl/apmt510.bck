    SELECT pmek002 FROM pmek_t
     WHERE pmekent   = g_enterprise
       AND pmekdocno = g_pmee_m.pmeedocno
       AND pmek001   = g_pmee_m.pmee900
       AND pmekseq   = 0
       AND pmekseq1  = g_ooff_d4[p_i].ooff012
       AND pmekseq2  = 0
   FOREACH sel_pmek_cs7 INTO l_pmek002
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
