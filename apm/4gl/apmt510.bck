{<section id="apmt510.set_no_entry_b" >}
#+ 單身欄位關閉設定
PRIVATE FUNCTION apmt510_set_no_entry_b(p_cmd)
   #add-point:set_no_entry_b段define(客製用) name="set_no_entry_b.define_customerization"
   
   #end add-point    
   DEFINE p_cmd   LIKE type_t.chr1   
   #add-point:set_no_entry_b段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="set_no_entry_b.define"
   DEFINE l_imaa005   LIKE imaa_t.imaa005
   DEFINE l_inaa007   LIKE inaa_t.inaa007
   DEFINE l_imaf015   LIKE imaf_t.imaf015
   DEFINE l_imaf144   LIKE imaf_t.imaf144
   DEFINE l_imaf061   LIKE imaf_t.imaf061
   DEFINE l_imaf055   LIKE imaf_t.imaf055
   DEFINE l_n         LIKE type_t.num5
   #add--2015/08/24 By shiun--(S)
   DEFINE l_success   LIKE type_t.num5
   DEFINE l_slip      LIKE pmee_t.pmeedocno
   #add--2015/08/24 By shiun--(E)
   #end add-point    
   
   #add-point:Function前置處理  name="set_no_entry_b.pre_function"
   
   #end add-point
   
   IF p_cmd = 'u' AND g_chkey = 'N' THEN
      CALL cl_set_comp_entry("",FALSE)
      #add-point:set_no_entry_b段欄位控制 name="set_no_entry_b.field_control"
      
      #end add-point 
   END IF 
   
   #add-point:set_no_entry_b段 name="set_no_entry_b.set_no_entry_b"
   IF (NOT cl_null(g_pmeg6_d_t.pmef007)) AND (g_pmeg6_d_t.pmef008> 0 OR g_pmeg6_d_t.pmef009 > 0) THEN
      CALL cl_set_comp_entry("pmef001,pmef002,pmef003,pmef004,pmef005,pmef006",FALSE)
   END IF
   
   #委外、重覆性生產，若有來源單據，則料號等欄位不可修改
   IF g_argv[1] MATCHES '[12]' THEN
      LET l_n = 0
      SELECT COUNT(*) INTO l_n FROM pmei_t
       WHERE pmeient = g_enterprise
         AND pmeidocno = g_pmee_m.pmeedocno
         AND pmei900 = g_pmee_m.pmee900
         AND pmeiseq = g_pmeg_d[l_ac].pmegseq
      IF (NOT cl_null(g_pmee_m.pmee008)) OR l_n > 0 THEN
         CALL cl_set_comp_entry("pmeg001,pmeg002,pmeg004,pmeg005",FALSE)
      END IF
   END IF

   #料件不使用產品特徵時，產品特徵欄位不可錄入
   LET l_imaa005 = ''
   SELECT imaa005 INTO l_imaa005 FROM imaa_t 
    WHERE imaaent = g_enterprise AND imaa001 = g_pmeg_d[l_ac].pmeg001
   IF cl_null(l_imaa005) THEN
      CALL cl_set_comp_entry("pmeg002",FALSE)
      LET g_pmeg_d[l_ac].pmeg002 = ' '
   ELSE
      IF cl_null(g_pmeg_d[l_ac].pmeg002) THEN
         LET g_pmeg_d[l_ac].pmeg002 = ''
      END IF
   END IF

   #當料件主檔設置要做參考單位管理時，則參考單位，參考數量運行輸入，不允許空白
   LET l_imaf015 = ''
   SELECT imaf015,imaf144,imaf061,imaf055 INTO l_imaf015,l_imaf144,l_imaf061,l_imaf055 FROM imaf_t 
    WHERE imafent = g_enterprise AND imaf001 = g_pmeg_d[l_ac].pmeg001 AND imafsite = g_site
   IF cl_null(l_imaf015) THEN
      CALL cl_set_comp_entry("pmeg008,pmeg009",FALSE)
      LET g_pmeg_d[l_ac].pmeg008= ''
      LET g_pmeg_d[l_ac].pmeg008_desc = ''
      LET g_pmeg_d[l_ac].pmeg009 = ''
   END IF
   
   #[T:料件據點進銷存檔].[C:庫存批號控管]=2時,[C:不可有批號]欄位不可輸入
   IF l_imaf061 = '2' THEN
      CALL cl_set_comp_entry("pmeg030",FALSE)
      LET g_pmeg3_d[l_ac].pmeg030 = ' '
   END IF

   #若設定imaf055(庫存管理特徵)等於'2.不可'時，則[C:庫存管理特徵]欄位不可輸入
   IF l_imaf055 = '2' THEN
      CALL cl_set_comp_entry("pmee053",FALSE)
      LET g_pmeg3_d[l_ac].pmeg053 = ''
   END IF

   IF g_pmeg_d[l_ac].pmeg024 = 'Y' THEN
      CALL cl_set_comp_entry("pmeg012,pmeg013,pmeg014",FALSE)
   END IF
   
   #料件不使用計價單位時，計價單位、計價數量不可維護
   IF cl_null(l_imaf144) THEN
      CALL cl_set_comp_entry("pmeg010,pmeg011",FALSE)
      LET g_pmeg_d[l_ac].pmeg010 = g_pmeg_d[l_ac].pmeg006
      LET g_pmeg_d[l_ac].pmeg010_desc = g_pmeg_d[l_ac].pmeg006_desc
      LET g_pmeg_d[l_ac].pmeg011 = g_pmeg_d[l_ac].pmeg007
   END IF
   
   #若1.正常稅率，則單身稅別不可輸入
   IF g_tax_app = '1' THEN
      CALL cl_set_comp_entry("pmeg016",FALSE)
   END IF   

   #單頭取價方式的基本資料設置不允許修改單價 , 未取到價格不允許人工輸入
   IF g_pmeg_d[l_ac].pmeg901 = '3' THEN  #161202-00006#1
      IF (g_pmeg3_d[l_ac].pmeg043 > 0 OR g_pmam002 <> 'Y') AND g_pmam003 <> 'Y' THEN
         CALL cl_set_comp_entry("pmeg015",FALSE)
      END IF
   END IF  #161202-00006#1
   
   #庫位不使用儲位管理時，儲位不可維護
   SELECT inaa007 INTO l_inaa007 FROM inaa_t WHERE inaaent = g_enterprise AND inaasite = g_site AND inaa001 = g_pmeg3_d[l_ac].pmeg028
   IF l_inaa007 = '5' THEN
      CALL cl_set_comp_entry("pmeg029",FALSE)
      LET g_pmeg3_d[l_ac].pmeg029 = ' '
      LET g_pmeg3_d[l_ac].pmeg029_desc = ''
   END IF

   #單價含稅時，可錄入含稅金額，
   IF g_pmee_m.pmee013 = 'Y' THEN
      CALL cl_set_comp_entry("pmef005",FALSE)
   ELSE
      CALL cl_set_comp_entry("pmef006",FALSE)
   END IF
   
   #當是VMI採購單時庫存管理特徵固定預設供應商編號，且不可以修改
   IF g_pmee_m.pmee005 = '3' THEN
      LET g_pmeg3_d[l_ac].pmeg053 = g_pmee_m.pmee004
      CALL cl_set_comp_entry("pmeg053",FALSE)
   END IF
   
   #當單頭多角性質為"5:統購自付"時,收貨據點才可維護
   IF g_pmee_m.pmee006 <> '5' THEN
      CALL cl_set_comp_entry("pmegunit,pmegorga",FALSE)
      LET g_pmeg_d[l_ac].pmegunit = g_site
      LET g_pmeg_d[l_ac].pmegorga = g_site
   END IF
   
   #子特性是樣品時，單價为0
   IF g_pmeg_d[l_ac].pmeg019 = '9' THEN
      CALL cl_set_comp_entry("pmeg015",FALSE)
   END IF
   
   IF g_pmee_m.pmee007 = '8' AND (NOT cl_null(g_pmee_m.pmee008)) THEN
      CALL cl_set_comp_entry("pmegseq,pmegunit,pmegorga",FALSE)
      CALL cl_set_comp_entry("pmeg001,pmeg002,pmeg003,pmeg004,pmeg005",FALSE)
      CALL cl_set_comp_entry("pmeg006,pmeg008,pmeg010",FALSE)
      CALL cl_set_comp_entry("pmeg012,pmeg013,pmeg014,pmeg015",FALSE)
      CALL cl_set_comp_entry("pmeg016,pmeg017,pmeg019,pmeg020",FALSE)
      CALL cl_set_comp_entry("pmeg021,pmeg022,pmeg023,pmeg024,pmeg025",FALSE)
      CALL cl_set_comp_entry("pmeg026,pmeg027,pmeg028,pmeg029,pmeg030",FALSE)
      CALL cl_set_comp_entry("pmeg031,pmeg032,pmeg033,pmeg035",FALSE)
      CALL cl_set_comp_entry("pmeg036,pmeg037,pmeg038,pmeg039,pmeg040",FALSE)
      CALL cl_set_comp_entry("pmeg041,pmeg042,pmeg043,pmeg044,pmeg045",FALSE)
      CALL cl_set_comp_entry("pmeg046,pmeg047,pmeg048,pmeg049,pmeg050",FALSE)
      CALL cl_set_comp_entry("pmeg052,pmeg053",FALSE)
   
   END IF
   #add--2015/08/24 By shiun--(S)
   CALL s_aooi200_get_slip(g_pmee_m.pmeedocno)
        RETURNING l_success,l_slip
   IF cl_get_doc_para(g_enterprise,g_site,l_slip,'D-FIN-5002') = 'N' THEN
      CALL cl_set_comp_entry("pmeg058",FALSE)
   ELSE
      #有來源單號時，表示預算科目是從前面帶過來的，應鎖起來不給修改
      LET l_n = 0
      SELECT COUNT(*) INTO l_n FROM pmei_t
       WHERE pmeient = g_enterprise
         AND pmeidocno = g_pmee_m.pmeedocno
         AND pmei900 = g_pmee_m.pmee900
         AND pmeiseq = g_pmeg_d[l_ac].pmegseq
         AND pmei003 IS NOT NULL
      IF l_n > 0 THEN
         CALL cl_set_comp_entry("pmeg058",FALSE)
      END IF
   END IF
   #add--2015/08/24 By shiun--(E)
   
   
   #151229-00014#1---add--begin---
   #已有收货单，应该不允许修改单位
   LET l_n = 0
   SELECT COUNT(*) INTO l_n FROM pmdt_t,pmds_t
     WHERE pmdtent = pmdsent AND pmdtdocno = pmdsdocno
       AND pmdtent = g_enterprise AND pmdt001 = g_pmee_m.pmeedocno AND pmdsstus != 'X'
       AND pmds000 IN ('1','8','9','3','23','24','20')  #收貨單
   IF l_n > 0 THEN
      CALL cl_set_comp_entry("pmeg006",FALSE)
   END IF
   #151229-00014#1---add--end---
   
   #161202-00006#1--mark---s
   #有帳款還是可以改單價的,要不要回寫收貨單，依參數決定
   ##160930-00030#1----s
   ##該筆收貨單是否已轉帳款
   #LET l_n = 0
   #SELECT COUNT(*) INTO l_n
   #  FROM apca_t,apcb_t,pmdt_t,pmds_t
   # WHERE apcaent = apcbent
   #   AND apcaent = g_enterprise
   #   AND apcald = apcbld
   #   AND apcadocno = apcbdocno
   #   AND apcastus <> 'X'
   #   AND pmdtent = pmdsent AND pmdtdocno = pmdsdocno AND pmdsent = apcaent
   #   AND apcb002 = pmdtdocno AND apcb003 = pmdtseq AND pmdt001 = g_pmee_m.pmeedocno AND pmdt002 = g_pmeg_d[l_ac].pmegseq
   #   AND pmds000 IN ('1','3','6')   #收貨單/收貨入庫單/入庫單
   #IF l_n > 0 THEN
   #   CALL cl_set_comp_entry("pmeg015",FALSE)
   #END IF
   #160930-00030#1----e
   #161202-00006#1---mark---e
   #end add-point     
END FUNCTION
 
