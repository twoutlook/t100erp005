         CALL s_transaction_end('N','0')
      ELSE
         #新增請購變更歷程檔並修改單頭變更否欄位
         IF NOT apmt510_upd_pmee901() THEN 
            
         END IF
         CALL s_transaction_end('Y','0')
      END IF
