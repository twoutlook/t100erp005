                  ELSE
                     LET l_pmeg024 = 'N'
                  END IF
                  UPDATE pmeg_t SET pmeg012 = g_pmeg_d[g_detail_idx].pmeg012,
                                    pmeg013 = g_pmeg_d[g_detail_idx].pmeg013,
                                    pmeg014 = g_pmeg_d[g_detail_idx].pmeg014,
                                    pmeg024 = l_pmeg024
                   WHERE pmegent = g_enterprise
                     AND pmegdocno = g_pmee_m.pmeedocno
