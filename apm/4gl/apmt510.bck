      CALL s_desc_get_item_desc(g_pmeg5_d[l_ac].pmei001) RETURNING g_pmeg5_d[l_ac].pmei001_desc,g_pmeg5_d[l_ac].imaal0043
      DISPLAY BY NAME g_pmeg5_d[l_ac].pmei001_desc,g_pmeg5_d[l_ac].imaal0043
      
      CALL apmt510_pmei022_ref()

      INSERT INTO pmei_t
                        (pmeient, pmeidocno,pmeisite,pmei900,
                         pmeiseq, pmeiseq1, pmei001, pmei002, pmei003,
                         pmei004, pmei005,  pmei006, pmei007, pmei008,
                         pmei009, pmei010,  pmei021,  pmei022, pmei023, pmei024,
                         pmei025, pmei026,  pmei901)
                  VALUES(g_enterprise, g_pmee_m.pmeedocno, g_pmeg5_d[l_ac].pmeisite, g_pmee_m.pmee900,
                         g_pmeg5_d[l_ac].pmeiseq,g_pmeg5_d[l_ac].pmeiseq1,g_pmeg5_d[l_ac].pmei001,g_pmeg5_d[l_ac].pmei002,g_pmeg5_d[l_ac].pmei003,
                         g_pmeg5_d[l_ac].pmei004,g_pmeg5_d[l_ac].pmei005, g_pmeg5_d[l_ac].pmei006,g_pmeg5_d[l_ac].pmei007,g_pmeg5_d[l_ac].pmei008,
                         g_pmeg5_d[l_ac].pmei009,g_pmeg5_d[l_ac].pmei010, g_pmeg5_d[l_ac].pmei021, g_pmeg5_d[l_ac].pmei022,g_pmeg5_d[l_ac].pmei023,
                         g_pmeg5_d[l_ac].pmei024,g_pmeg5_d[l_ac].pmei025, g_pmeg5_d[l_ac].pmei026, g_pmeg5_d[l_ac].pmei901)
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "pmei_t"
         LET g_errparam.popup = TRUE
         CALL cl_err()

         LET r_success = FALSE
         RETURN r_success
      END IF   
      
      LET l_ac = l_ac+1
      IF l_ac > g_max_rec THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 9035
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()

         EXIT FOREACH
      END IF      
   END FOREACH   

   CALL g_pmeg5_d.deleteElement(g_pmeg5_d.getLength())

   LET g_rec_b = l_ac - 1
