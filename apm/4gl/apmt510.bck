#mod--161109-00085#15 By 08993--(e)


    #取得價格是否允許修改,修改容差率,價格超過容差率的處理方式,允許單價為0
    LET l_pmam002 = ''
    LET l_pmam003 = ''
    SELECT pmam002,pmam003
      INTO l_pmam002,l_pmam003
      FROM pmam_t
     WHERE pmament = g_enterprise
       AND pmam001 = g_pmee_m.pmee017
 
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
   INPUT ARRAY g_pmeg_d FROM s_detail2.*
      ATTRIBUTE(COUNT = g_rec_b,WITHOUT DEFAULTS, #MAXCOUNT = g_max_rec,
                INSERT ROW = FALSE, 
                DELETE ROW = FALSE,
                APPEND ROW = FALSE)


      BEFORE INPUT
          CALL apmt510_b_fill()
          LET g_rec_b = g_pmeg_d.getLength()
          
      BEFORE ROW 
          LET l_ac = ARR_CURR()
          LET g_detail_idx = l_ac
          LET g_detail_idx_list[1] = l_ac
          LET g_current_page = 1
            
          LET l_lock_sw = 'N'            #DEFAULT
          DISPLAY l_ac TO FORMONLY.idx
         
          IF s_transaction_chk("N",0) THEN
             CALL s_transaction_begin()
          END IF
          
          OPEN apmt510_cl USING g_enterprise,g_pmee_m.pmeedocno,g_pmee_m.pmee900
          IF STATUS THEN
             INITIALIZE g_errparam TO NULL 
             LET g_errparam.extend = "OPEN apmt510_cl:" 
             LET g_errparam.code   = STATUS 
             LET g_errparam.popup  = TRUE 
             CALL cl_err()
             CLOSE apmt510_cl
             CALL s_transaction_end('N','0')
             RETURN
          END IF
            
          LET g_rec_b = g_pmeg_d.getLength()
           
          LET g_pmeg_d_t.* = g_pmeg_d[l_ac].*  #BACKUP
          LET g_pmeg_d_o.* = g_pmeg_d[l_ac].*  #BACKUP                    
               
          CALL cl_set_comp_entry("pmegseq,pmegunit,pmegorga",FALSE)
          CALL cl_set_comp_entry("pmeg001,pmeg002,pmeg003,pmeg004,pmeg005",FALSE)
          CALL cl_set_comp_entry("pmeg006,pmeg007,pmeg008,pmeg010",FALSE)
          CALL cl_set_comp_entry("pmeg011,pmeg012,pmeg013,pmeg014,pmeg015",FALSE)
          CALL cl_set_comp_entry("pmeg016,pmeg017,pmeg019,pmeg020",FALSE)
          CALL cl_set_comp_entry("pmeg021,pmeg022,pmeg023,pmeg024,pmeg025",FALSE)
          CALL cl_set_comp_entry("pmeg026,pmeg027,pmeg028,pmeg029,pmeg030",FALSE)
          CALL cl_set_comp_entry("pmeg031,pmeg032,pmeg033,pmeg035",FALSE)
          CALL cl_set_comp_entry("pmeg036,pmeg037,pmeg038,pmeg039,pmeg040",FALSE)
          CALL cl_set_comp_entry("pmeg041,pmeg042,pmeg043,pmeg044,pmeg045",FALSE)
          CALL cl_set_comp_entry("pmeg046,pmeg047,pmeg048,pmeg049,pmeg050",FALSE)
          CALL cl_set_comp_entry("pmeg052,pmeg053",FALSE)         
           
          #只開放單價欄位可以輸入
          #單頭取價方式的基本資料設置不允許修改單價 , 未取到價格不允許人工輸入
          IF (g_pmeg3_d[l_ac].pmeg040 = 'X' AND  l_pmam002 = 'Y') OR l_pmam003 = 'Y' THEN
             CALL cl_set_comp_entry("pmeg015",TRUE)
          ELSE
             INITIALIZE g_errparam TO NULL 
             LET g_errparam.extend = ''
             LET g_errparam.code   = 'aic-00228'        #此單據為最終供應商採購單，取價方式%，不可手動調整單價，故不可做變更！
             LET g_errparam.popup  = TRUE 
             LET g_errparam.replace[1] = g_pmee_m.pmee017
             CALL cl_err()
             LET l_cnt = 0
             LET g_detail_idx = ARR_CURR()
             CASE
               WHEN g_detail_idx < g_rec_b
                 FOR l_i = g_detail_idx + 1 TO g_rec_b
                     IF (g_pmeg3_d[g_detail_idx].pmeg043 = 0 AND l_pmam002 = 'Y') OR l_pmam003 = 'Y' THEN
                         LET l_cnt = l_i
                         EXIT FOR
                     END IF
                 END FOR
               WHEN g_detail_idx >= g_rec_b
                 FOR l_i = g_rec_b - 1 TO 1 STEP -1
                     IF (g_pmeg3_d[g_detail_idx].pmeg043 = 0 AND l_pmam002 = 'Y') OR l_pmam003 = 'Y' THEN
                        LET l_cnt = l_i
                        EXIT FOR
                     END IF
                 END FOR
             END CASE
             CALL FGL_SET_ARR_CURR(l_cnt) 
           END IF         
               
          IF NOT apmt510_lock_b("pmeg_t","'1'") THEN
             LET l_lock_sw='Y'
          ELSE
             FETCH apmt510_bcl INTO g_pmeg_d[l_ac].pmegseq,g_pmeg_d[l_ac].pmeg001,g_pmeg_d[l_ac].pmeg002, 
                   g_pmeg_d[l_ac].pmeg004,g_pmeg_d[l_ac].pmeg005,g_pmeg_d[l_ac].pmeg006,g_pmeg_d[l_ac].pmeg007, 
                   g_pmeg_d[l_ac].pmeg008,g_pmeg_d[l_ac].pmeg009,g_pmeg_d[l_ac].pmeg024,g_pmeg_d[l_ac].pmeg012, 
                   g_pmeg_d[l_ac].pmeg013,g_pmeg_d[l_ac].pmeg014,g_pmeg_d[l_ac].pmeg045,g_pmeg_d[l_ac].pmeg010, 
                   g_pmeg_d[l_ac].pmeg011,g_pmeg_d[l_ac].pmeg015,g_pmeg_d[l_ac].pmeg016,g_pmeg_d[l_ac].pmeg017, 
                   g_pmeg_d[l_ac].pmeg046,g_pmeg_d[l_ac].pmeg047,g_pmeg_d[l_ac].pmeg048,g_pmeg_d[l_ac].pmeg023, 
                   g_pmeg_d[l_ac].pmeg019,g_pmeg_d[l_ac].pmeg020,g_pmeg_d[l_ac].pmeg052,g_pmeg_d[l_ac].pmeg021, 
                   g_pmeg_d[l_ac].pmeg022,g_pmeg_d[l_ac].pmegunit,g_pmeg_d[l_ac].pmegorga,g_pmeg_d[l_ac].pmeg049, 
                   g_pmeg_d[l_ac].pmeg051,g_pmeg_d[l_ac].pmeg050,g_pmeg_d[l_ac].pmeg901,g_pmeg_d[l_ac].pmeg902, 
                   g_pmeg_d[l_ac].pmeg903,g_pmeg_d[l_ac].pmegsite,g_pmeg3_d[l_ac].pmegseq,g_pmeg3_d[l_ac].pmeg027, 
                   g_pmeg3_d[l_ac].pmeg028,g_pmeg3_d[l_ac].pmeg029,g_pmeg3_d[l_ac].pmeg030,g_pmeg3_d[l_ac].pmeg053, 
                   g_pmeg3_d[l_ac].pmeg025,g_pmeg3_d[l_ac].pmeg026,g_pmeg3_d[l_ac].pmeg031,g_pmeg3_d[l_ac].pmeg032, 
                   g_pmeg3_d[l_ac].pmeg033,g_pmeg3_d[l_ac].pmeg003,g_pmeg3_d[l_ac].pmeg036,g_pmeg3_d[l_ac].pmeg037, 
                   g_pmeg3_d[l_ac].pmeg038,g_pmeg3_d[l_ac].pmeg058,g_pmeg3_d[l_ac].pmeg039,g_pmeg3_d[l_ac].pmeg035, 
                   g_pmeg3_d[l_ac].pmeg040,g_pmeg3_d[l_ac].pmeg041,g_pmeg3_d[l_ac].pmeg042,g_pmeg3_d[l_ac].pmeg043, 
                   g_pmeg3_d[l_ac].pmeg044
             IF SQLCA.sqlcode THEN
                INITIALIZE g_errparam TO NULL 
                LET g_errparam.extend = g_pmeg_d_t.pmegseq,":",SQLERRMESSAGE 
                LET g_errparam.code   = SQLCA.sqlcode 
                LET g_errparam.popup  = TRUE 
                CALL cl_err()
                LET l_lock_sw = "Y"
             END IF
                  
             #遮罩相關處理
             LET g_pmeg_d_mask_o[l_ac].* =  g_pmeg_d[l_ac].*
             CALL apmt510_pmeg_t_mask()
             LET g_pmeg_d_mask_n[l_ac].* =  g_pmeg_d[l_ac].*
                  
             LET g_bfill = "N"
             CALL apmt510_show()
             LET g_bfill = "Y"
                  
             CALL cl_show_fld_cont()
          END IF

      AFTER FIELD pmeg015
          IF NOT cl_ap_chk_range(g_pmeg_d[l_ac].pmeg015,"0.000","1","","","azz-00079",1) THEN
             NEXT FIELD pmeg015
          END IF  
 
          IF NOT cl_null(g_pmeg_d[l_ac].pmeg015) THEN
             IF g_pmeg_d[l_ac].pmeg015 != g_pmeg_d_o.pmeg015 OR cl_null(g_pmeg_d_o.pmeg015) THEN
                #依據取價方式設置的單價修改控管方式檢核修改後單價的合理性
                LET l_errno = ''
                IF NOT (cl_null(g_pmee_m.pmee017) OR cl_null(g_pmeg_d[l_ac].pmeg015) OR cl_null(g_pmeg3_d[l_ac].pmeg043) OR
                        cl_null(g_pmeg3_d[l_ac].pmeg040) OR cl_null(g_pmee_m.pmee015) OR cl_null(g_pmeg_d[l_ac].pmeg001) OR
                        cl_null(g_pmeg_d[l_ac].pmeg010)) THEN
                   IF g_pmeg_d[l_ac].pmeg901 = '3' THEN  #161202-00006#1
                      CALL s_purchase_price_check(g_pmee_m.pmee017,g_pmeg_d[l_ac].pmeg015,g_pmeg3_d[l_ac].pmeg043,g_pmeg3_d[l_ac].pmeg040,g_pmee_m.pmee015,g_pmeg_d[l_ac].pmeg001,g_pmeg_d[l_ac].pmeg002,g_pmeg_d[l_ac].pmeg010)
                        RETURNING l_flag,l_errno
                   #161202-00006#1---s
                   ELSE
                      CALL apmt510_price_check(g_pmee_m.pmee017,g_pmeg_d[l_ac].pmeg015,g_pmeg3_d[l_ac].pmeg043,g_pmeg3_d[l_ac].pmeg040,g_pmee_m.pmee015,g_pmeg_d[l_ac].pmeg001,g_pmeg_d[l_ac].pmeg002,g_pmeg_d[l_ac].pmeg010)
                        RETURNING l_flag,l_errno
                   END IF
                   #161202-00006#1---e
                   IF NOT l_flag THEN
                      INITIALIZE g_errparam TO NULL
                      LET g_errparam.code = l_errno
                      LET g_errparam.extend = ''
                      LET g_errparam.popup = TRUE
                      CALL cl_err() 
                      NEXT FIELD pmeg015
                   END IF  
                END IF                          
                
                #呼叫幣別取位應用元件對單價作取位(依單頭幣別做取位基準)
                CALL s_curr_round(g_site,g_pmee_m.pmee015,g_pmeg_d[l_ac].pmeg015,'1') 
                  RETURNING g_pmeg_d[l_ac].pmeg015
                   
                #重新計算[C:未稅金額]、[C:含稅金額]、[稅額]
                IF cl_null(g_pmeg_d[l_ac].pmeg010) OR cl_null(g_pmeg_d[l_ac].pmeg011) THEN
                   LET g_pmeg_d[l_ac].pmeg010 = g_pmeg_d[l_ac].pmeg006
                   LET g_pmeg_d[l_ac].pmeg011 = g_pmeg_d[l_ac].pmeg007
                END IF
                
                CALL s_apmt510_get_amount(g_pmee_m.pmeedocno,g_pmee_m.pmee900,g_pmeg_d[l_ac].pmegseq,g_pmeg_d[l_ac].pmeg011,g_pmeg_d[l_ac].pmeg015,g_pmeg_d[l_ac].pmeg016,g_pmee_m.pmee015,g_pmee_m.pmee016)
                  RETURNING g_pmeg_d[l_ac].pmeg046,g_pmeg_d[l_ac].pmeg048,g_pmeg_d[l_ac].pmeg047
                     
                #計算價差比
                IF g_pmeg3_d[l_ac].pmeg043<> 0 AND g_pmeg3_d[l_ac].pmeg043 <> g_pmeg_d[l_ac].pmeg015 THEN
                   #百分比形式
                   LET g_pmeg3_d[l_ac].pmeg044 = ((g_pmeg_d[l_ac].pmeg015 - g_pmeg3_d[l_ac].pmeg043) / g_pmeg3_d[l_ac].pmeg043) * 100
                END IF                  
                #科目預算處理
                CALL apmt510_detail_abg('4')
                  RETURNING l_success,l_errno_abg,l_bgaf016,l_pmeg058,l_pmeg058_desc,l_pmeg058_desc_sql
                IF NOT l_success THEN
                   CASE l_errno_abg
                      WHEN 'abg-00103'
                           CASE l_bgaf016
                              WHEN '1'
                              WHEN '2'
                                   INITIALIZE g_errparam TO NULL
                                   LET g_errparam.code = l_errno_abg
                                   LET g_errparam.extend = ''
                                   LET g_errparam.popup = TRUE
                                   CALL cl_err()
                              WHEN '3'
                                   INITIALIZE g_errparam TO NULL
                                   LET g_errparam.code = l_errno_abg
                                   LET g_errparam.extend = ''
                                   LET g_errparam.popup = TRUE
                                   CALL cl_err()
                                   NEXT FIELD CURRENT
                           END CASE
                      WHEN 'abg-00104'
                           INITIALIZE g_errparam TO NULL
                           LET g_errparam.code = l_errno_abg
                           LET g_errparam.extend = ''
                           LET g_errparam.popup = TRUE
                           CALL cl_err()
                      OTHERWISE
                           INITIALIZE g_errparam TO NULL
                           LET g_errparam.code = l_errno_abg
                           LET g_errparam.extend = ''
                           LET g_errparam.popup = TRUE
                           CALL cl_err()
                           NEXT FIELD CURRENT
                   END CASE
                END IF                
             END IF       
          END IF           
          LET g_pmeg_d_o.pmeg015 = g_pmeg_d[l_ac].pmeg015

      ON ACTION controlp INFIELD pmeg902
          INITIALIZE g_qryparam.* TO NULL
          LET g_qryparam.state = 'i'
	       LET g_qryparam.reqry = FALSE
          LET g_qryparam.default1 = g_pmeg_d[l_ac].pmeg902             
          LET g_qryparam.arg1 = '267'
          CALL q_oocq002()                                #呼叫開窗
          LET g_pmeg_d[l_ac].pmeg902 = g_qryparam.return1              #將開窗取得的值回傳到變數
          DISPLAY g_pmeg_d[l_ac].pmeg902 TO pmeg902              #顯示到畫面上
          NEXT FIELD pmeg902                          #返回原欄位
       


      AFTER FIELD pmeg902
          CALL apmt510_pmeg902_ref() 
          IF NOT cl_null(g_pmeg_d[l_ac].pmeg902) THEN
             IF (g_pmeg_d[l_ac].pmeg902 <> g_pmeg_d_t.pmeg902 OR g_pmeg_d_t.pmeg902 IS NULL) THEN               
                IF NOT s_azzi650_chk_exist('267',g_pmeg_d[l_ac].pmeg902) THEN
                   LET g_pmeg_d[l_ac].pmeg902 = g_pmeg_d_t.pmeg902
                   DISPLAY BY NAME g_pmeg_d[l_ac].pmeg902
                   CALL apmt510_pmeg902_ref()
                   NEXT FIELD CURRENT
                END IF   
             END IF                 
          END IF
          LET g_pmeg_d_t.pmeg902 = g_pmeg_d[l_ac].pmeg902

         ON ROW CHANGE
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               LET g_pmeg_d[l_ac].* = g_pmeg_d_t.*
               CLOSE apmt510_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = g_pmeg_d[l_ac].pmegseq 
               LET g_errparam.code   = -263 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               LET g_pmeg_d[l_ac].* = g_pmeg_d_t.*
               EXIT DIALOG 
            ELSE            
               IF g_pmeg_d[l_ac].pmeg901 = '1' THEN
                  LET g_pmeg_d[l_ac].pmeg901 = '2'
               END IF               
      
               #將遮罩欄位還原
               CALL apmt510_pmeg_t_mask_restore('restore_mask_o')
      
               UPDATE pmeg_t SET pmeg015 = g_pmeg_d[l_ac].pmeg015,
                                 pmeg046 = g_pmeg_d[l_ac].pmeg046,
                                 pmeg047 = g_pmeg_d[l_ac].pmeg047,
