#已經自動編號過的單據編號，抓取對應的單據別說明
PRIVATE FUNCTION apmt510_pmeedocno_ref(p_pmeedocno)
DEFINE p_pmeedocno       LIKE pmee_t.pmeedocno
DEFINE l_oobal002        LIKE oobal_t.oobal002
DEFINE l_ooef004         LIKE ooef_t.ooef004
DEFINE r_pmeedocno_desc  LIKE oobal_t.oobal004
DEFINE l_flag            LIKE type_t.num5          #标识符，TRUE/FALSE
DEFINE l_flag1           LIKE type_t.num5          #标识符，TRUE/FALSE
DEFINE l_site            LIKE type_t.chr20

      LET l_flag = TRUE
      LET l_flag1 = TRUE
      LET l_ooef004 = NULL
      LET l_oobal002 = NULL
      IF NOT cl_null(p_pmeedocno) THEN
         CALL s_aooi200_get_site(p_pmeedocno) RETURNING l_flag,l_site
         IF l_flag THEN
            SELECT ooef004 INTO l_ooef004 FROM ooef_t
             WHERE ooef005 = l_site
               AND ooefent = g_enterprise
         END IF
         CALL s_aooi200_get_slip(p_pmeedocno) RETURNING l_flag1,l_oobal002
         IF l_flag1 THEN
            IF NOT cl_null(l_oobal002) AND NOT cl_null(l_ooef004) THEN
               SELECT oobal004 INTO r_pmeedocno_desc FROM oobal_t
                WHERE oobal001 = l_ooef004
                  AND oobal002 = l_oobal002
                  AND oobal003 = g_dlang
                  AND oobalent = g_enterprise
            ELSE
               LET r_pmeedocno_desc = ""
            END IF
         END IF
      ELSE
         LET r_pmeedocno_desc = ""
      END IF

      RETURN r_pmeedocno_desc
END FUNCTION
#採購變更單檢查
PRIVATE FUNCTION apmt510_pmeedocno_chk()
DEFINE l_n               LIKE type_t.num5
DEFINE r_success         LIKE type_t.num5
DEFINE l_flag            LIKE type_t.num5
DEFINE l_success         LIKE type_t.num5
DEFINE l_pmdl047         LIKE pmdl_t.pmdl047
#160627-00012#2-s-add
DEFINE l_pmdl006         LIKE pmdl_t.pmdl006   
DEFINE l_pmdl017         LIKE pmdl_t.pmdl017   
DEFINE l_pmam002         LIKE pmam_t.pmam002
DEFINE l_pmam003         LIKE pmam_t.pmam003
DEFINE l_cnt             LIKE type_t.num5
#160627-00012#2-e-add
#161028-00006#1---s
DEFINE l_pmdl005         LIKE pmdl_t.pmdl005
DEFINE l_pmdl007         LIKE pmdl_t.pmdl007
#161028-00006#1---e
   LET r_success = TRUE

   IF cl_null(g_pmee_m.pmeedocno) THEN
      RETURN r_success
   END IF

   #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
   INITIALIZE g_chkparam.* TO NULL
   #160318-00025#15 by 07900 --add-str 
   LET g_errshow = TRUE #是否開窗
   #160318-00025#15 by 07900 --add-end
   #設定g_chkparam.*的參數
   LET g_chkparam.arg1 = g_pmee_m.pmeedocno

  #160318-00025#15 by 07900 --add-str 
  #LET g_chkparam.err_str[1] ="apm-00337:sub-01302|apmt500|",cl_get_progname("apmt500",g_lang,"2"),"|:EXEPROGapmt500"  #161207-00048#1
  #160318-00025#15 by 07900 --add-end
   #呼叫檢查存在並帶值的library
   #150904 earl mod s
   #IF NOT cl_chk_exist("v_pmdldocno") THEN
   IF NOT cl_chk_exist("v_pmdldocno_7") THEN
   #150904 earl mod e
      #檢查失敗時後續處理
      LET r_success = FALSE
      RETURN r_success
   END IF

   #同一張採購單號不能同時存在兩張以上未確認的變更單內
   LET l_n = 0
   SELECT COUNT(pmeedocno) INTO l_n
     FROM pmee_t
    WHERE pmeeent = g_enterprise
      AND pmeedocno = g_pmee_m.pmeedocno
      AND pmeestus = 'N'
      
   IF NOT cl_null(l_n) AND l_n > 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'apm-00396'
      LET g_errparam.extend = g_pmee_m.pmeedocno
      LET g_errparam.popup = TRUE
      CALL cl_err()

      LET r_success = FALSE
      RETURN r_success
   END IF
   
   #檢核輸入的單據別是否可以被key人員對應的控制組使用,'4' 為採購控制組類型
   CALL s_control_chk_doc('1',g_pmee_m.pmeedocno,'4',g_user,g_dept,'','') 
        RETURNING l_success,l_flag
   IF NOT l_success THEN      #处理状态
      LET r_success = FALSE
      RETURN r_success
   ELSE
      IF NOT l_flag THEN      #是否存在
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'apm-00266'
         LET g_errparam.extend = g_pmee_m.pmeedocno
         LET g_errparam.popup = TRUE
         CALL cl_err()

         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF    
   
   #物流結案="Y"的採購單不可以進行變更
   LET l_pmdl047 = ''
   LET l_pmdl006 = ''                                                  #160627-00012#2 add
   LET l_pmdl017 = ''                                                  #160627-00012#2 add
   SELECT pmdl047,pmdl006,pmdl017,pmdl005,pmdl007   #161028-00006#1  pmdl005,pmdl007
       INTO l_pmdl047,l_pmdl006,l_pmdl017,l_pmdl005,l_pmdl007   #160627-00012#2 add pmdl006,pmdl017
     FROM pmdl_t
    WHERE pmdlent = g_enterprise
      AND pmdldocno = g_pmee_m.pmeedocno
   IF l_pmdl047 = 'Y' THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'apm-00639'
      LET g_errparam.extend = g_pmee_m.pmeedocno
      LET g_errparam.popup = TRUE
      CALL cl_err()
      LET r_success = FALSE
      RETURN r_success
   END IF
  #160627-00012#2-s-add
   IF l_pmdl006 = '5' THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'aic-00227'           #此多角採購單不為起始站單據，不可做變更！
      LET g_errparam.extend = g_pmee_m.pmeedocno
      LET g_errparam.popup = TRUE
      CALL cl_err()
      LET r_success = FALSE
      RETURN r_success
   END IF  
   #檢核如為最終供應商採購單，只可修改單價
   IF l_pmdl005 = '1' AND l_pmdl006 = '1' AND l_pmdl007 = '9' THEN    #161028-00006#1
     #檢核取價方式是否可以修改單價
      LET l_pmam002 = ''
      LET l_pmam003 = ''
      SELECT pmam002,pmam003
        INTO l_pmam002,l_pmam003
        FROM pmam_t
       WHERE pmament = g_enterprise
         AND pmam001 = l_pmdl017
    
      LET l_cnt = 0
      SELECT COUNT(*) INTO l_cnt
        FROM pmdn_t
       WHERE pmdnent = g_enterprise
         AND pmdndocno = g_pmee_m.pmeedocno
         AND pmdn040 = 'X'           #取價來源為'X'表示取不到單價
          
     #IF l_pmam002 <> 'Y' AND l_pmam003 <> 'Y'  THEN
      IF (l_cnt > 0 AND l_pmam002 = 'Y') OR l_pmam003 = 'Y' THEN
         #可修改單價，可做變更
      ELSE
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = ''
         LET g_errparam.code   = 'aic-00228'  #此單據為最終供應商採購單，取價方式%，不可手動調整單價，故不可做變更！
         LET g_errparam.popup  = TRUE 
         LET g_errparam.replace[1] = l_pmdl017
         CALL cl_err()
         LET r_success = FALSE
         RETURN r_success
      END IF      
     #160627-00012#2-e-add
   END IF   #161028-00006#1

   RETURN r_success

END FUNCTION
