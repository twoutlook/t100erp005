PRIVATE FUNCTION apmt510_get_qcap006()
DEFINE l_sql   STRING

   LET l_sql = " SELECT qcap006 FROM qcap_t ",
               "  WHERE qcapent = '",g_enterprise,"' ",
               "    AND qcapsite = '",g_site,"' ",
               "    AND qcap001 = '",g_pmeg_d[l_ac].pmeg001,"' ",
               "    AND qcap002 = '",g_pmee_m.pmee004,"' "
   IF g_pmeg_d[l_ac].pmeg002 IS NOT NULL THEN
      LET l_sql = l_sql," AND qcap005 = '",g_pmeg_d[l_ac].pmeg002,"' "
   END IF
   IF (NOT cl_null(g_pmeg_d[l_ac].pmeg004)) AND (NOT cl_null(g_pmeg_d[l_ac].pmeg005)) THEN
      LET l_sql = l_sql," AND qcap003 = '",g_pmeg_d[l_ac].pmeg004,"' AND qcap004 = '",g_pmeg_d[l_ac].pmeg005,"' "
   END IF
   
   PREPARE apmt510_get_qcap006 FROM l_sql
   EXECUTE apmt510_get_qcap006 INTO g_pmeg_d[l_ac].pmeg052
   FREE apmt510_get_qcap006
   
   IF cl_null(g_pmeg_d[l_ac].pmeg052) THEN
      #若沒有維護aqci050,再從aqci040中帶值
      SELECT imae114 INTO g_pmeg_d[l_ac].pmeg052 
        FROM imae_t
       WHERE imaeent = g_enterprise
         AND imaesite = g_site
         AND imae001 = g_pmeg_d[l_ac].pmeg001
    END IF
      
    IF cl_null(g_pmeg_d[l_ac].pmeg052) THEN
       LET g_pmeg_d[l_ac].pmeg052 = 'N'
    END IF
    
END FUNCTION

