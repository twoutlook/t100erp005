                  CALL s_transaction_begin()
                  #如果當前筆資料有多筆交期，則更新採購變更單單身的多交期欄位為'Y'
                  LET l_n = 0
                  SELECT COUNT(*) INTO l_n FROM pmej_t
                   WHERE pmejent = g_enterprise
                     AND pmejdocno = g_pmee_m.pmeedocno
                     AND pmej900 = g_pmee_m.pmee900
                     AND pmejseq = g_pmeg_d[g_detail_idx].pmegseq
                  IF l_n > 0 THEN
