#料件檢查
PRIVATE FUNCTION apmt510_pmeg001_chk()
DEFINE r_success     LIKE type_t.num5
DEFINE l_success     LIKE type_t.num5
DEFINE l_flag        LIKE type_t.num5
DEFINE l_ooba002     LIKE ooba_t.ooba002
DEFINE l_pmei007     LIKE pmei_t.pmei007
DEFINE l_pmei008     LIKE pmei_t.pmei008
DEFINE l_n           LIKE type_t.num5
DEFINE l_imaa005     LIKE imaa_t.imaa005

   LET r_success = TRUE

   IF cl_null(g_pmeg_d[l_ac].pmeg001) THEN
      RETURN r_success
   END IF
   
   #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。   
   INITIALIZE g_chkparam.* TO NULL
   
   #設定g_chkparam.*的參數
   LET g_chkparam.arg1 = g_pmeg_d[l_ac].pmeg001
      
   #呼叫檢查存在並帶值的library
   IF cl_chk_exist("v_imaa001") THEN
      IF NOT cl_chk_exist("v_imaf001_1") THEN   
         #檢查失敗時後續處理
         LET r_success = FALSE
         RETURN r_success
      END IF
   ELSE
      LET r_success = FALSE
      RETURN r_success   
   END IF   
   
   #修改料號時需判斷該採購單項次是否已有收貨單，若有則不允許修改料號
   LET l_n = 0
   SELECT COUNT(*) INTO l_n FROM pmdt_t
      WHERE pmdtent = g_enterprise AND pmdt001 = g_pmee_m.pmeedocno AND pmdt002 = g_pmeg_d[l_ac].pmegseq
   IF l_n>0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'apm-00424'
      LET g_errparam.extend = g_pmeg_d[l_ac].pmeg001
      LET g_errparam.popup = TRUE
      CALL cl_err()

      LET r_success = FALSE
      RETURN r_success   
   END IF
                     
   #判斷輸入的料件編號是否在控制組限制的產品範圍內
   CALL s_control_chk_group('3','4',g_user,g_dept,g_pmeg_d[l_ac].pmeg001,'','','','')
        RETURNING l_success,l_flag
   IF NOT l_success THEN
      LET r_success = FALSE
      RETURN r_success
   ELSE
      IF NOT l_flag THEN      #是否存在
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'apm-00265'
         LET g_errparam.extend = g_pmeg_d[l_ac].pmeg001
         LET g_errparam.popup = TRUE
         CALL cl_err()

         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF   
   
   #檢核輸入的料件的生命週期是否在單據別限制範圍內
   CALL s_control_chk_doc('4',g_pmee_m.pmeedocno,g_pmeg_d[l_ac].pmeg001,'','','','')
        RETURNING l_success,l_flag
   IF NOT l_success THEN      #处理状态
      LET r_success = FALSE
      RETURN r_success
   ELSE
      IF NOT l_flag THEN      #是否存在
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'ain-00015'
         LET g_errparam.extend = g_pmeg_d[l_ac].pmeg001
         LET g_errparam.popup = TRUE
         CALL cl_err()

         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF

   #檢核輸入的料件的產品分類是否在單據別限制範圍內
   CALL s_control_chk_doc('5',g_pmee_m.pmeedocno,g_pmeg_d[l_ac].pmeg001,'','','','')
        RETURNING l_success,l_flag
   IF NOT l_success THEN      #处理状态
      LET r_success = FALSE
      RETURN r_success
   ELSE
      IF NOT l_flag THEN      #是否存在
         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF
   
   #判斷是否有使用產品特徵
   CALL apmt510_get_imaa005(g_enterprise,g_pmeg_d[l_ac].pmeg001) RETURNING l_imaa005
   IF cl_null(l_imaa005) OR g_pmeg_d[l_ac].pmeg002 IS NOT NULL THEN
      #若單據別設置是要作請採勾稽時，則修改料號時必須檢核是否與"關聯單據明細"中的來源料號有替代關係，若沒有則不允許修改
      CALL s_aooi200_get_slip(g_pmee_m.pmeedocno) RETURNING l_success,l_ooba002
      IF cl_get_doc_para(g_enterprise,g_site,l_ooba002,'D-BAS-0061') = "Y" THEN
         DECLARE pmei007_cur CURSOR FOR
            SELECT DISTINCT pmei007,pmei008 FROM pmei_t
              WHERE pmeient = g_enterprise AND pmeidocno = g_pmee_m.pmeedocno
                AND pmeiseq = g_pmeg_d[l_ac].pmegseq AND pmei900 = g_pmee_m.pmee900
         FOREACH pmei007_cur INTO l_pmei007,l_pmei008
            IF l_pmei007 <> g_pmeg_d[l_ac].pmeg001 THEN
               IF NOT s_pmaq_chk_replacement(g_pmee_m.pmee004,l_pmei007,g_pmeg_d[l_ac].pmeg001,'2',l_pmei008,g_pmeg_d[l_ac].pmeg002) THEN
                  LET r_success = FALSE
                  RETURN r_success
               END IF
            END IF
         END FOREACH
      END IF
   END IF

   RETURN r_success   
   
END FUNCTION
#根據料件編號、供應商、採購控制組帶出欄位預設值
PRIVATE FUNCTION apmt510_pmeg001_desc()
DEFINE l_n         LIKE type_t.num5
DEFINE l_success   LIKE type_t.num5
DEFINE l_controlno LIKE ooha_t.ooha001
DEFINE l_imaf158   LIKE imaf_t.imaf158
DEFINE l_oodbl004  LIKE oodbl_t.oodbl004  #稅別名稱
DEFINE l_oodb005   LIKE oodb_t.oodb005    #含稅否
