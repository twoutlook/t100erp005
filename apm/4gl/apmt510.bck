      IF NOT s_apmt510_del_pmek(p_pmeg5_d.pmeiseq,p_pmeg5_d.pmeiseq1,0,"pmdp025",g_pmee_m.pmeedocno,g_pmee_m.pmee900) THEN
         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF
   
   #已入庫量
   IF p_pmeg5_d.pmei901 = '3' OR (p_pmeg5_d.pmei901 = '2' AND
      ((p_pmeg5_d.pmei026 <> l_pmdp.pmdp026) OR
      (p_pmeg5_d.pmei026 IS NULL AND l_pmdp.pmdp026 IS NOT NULL) OR
      (p_pmeg5_d.pmei026 IS NOT NULL AND l_pmdp.pmdp026 IS NULL))) THEN
      #其說明的SQL
      LET g_pmek007 = ''
      #新增資料到變更歷程檔
      IF NOT s_apmt510_ins_pmek(p_pmeg5_d.pmeiseq,p_pmeg5_d.pmeiseq1,0,"pmdp026",p_pmeg5_d.pmei901,l_pmdp.pmdp026,p_pmeg5_d.pmei026,g_pmee_m.pmeedocno,g_pmee_m.pmee900,g_pmek007) THEN
         LET r_success = FALSE
         RETURN r_success
      END IF
      LET l_flag = TRUE
   ELSE
      #資料一樣，表示無變更，將變更歷程檔刪除
      IF NOT s_apmt510_del_pmek(p_pmeg5_d.pmeiseq,p_pmeg5_d.pmeiseq1,0,"pmdp026",g_pmee_m.pmeedocno,g_pmee_m.pmee900) THEN
         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF   
   
   

   #表示此筆單身與採購合約單該筆單身一致
   IF l_flag = FALSE THEN
      UPDATE pmei_t SET pmei901 = '1'
       WHERE pmeient = g_enterprise
         AND pmeidocno = g_pmee_m.pmeedocno
         AND pmei900   = g_pmee_m.pmee900
         AND pmeiseq   = p_pmeg5_d.pmeiseq
         AND pmeiseq1  = p_pmeg5_d.pmeiseq1        
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = 'pmei_t'
         LET g_errparam.popup = TRUE
         CALL cl_err()

         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF

   RETURN r_success
   
END FUNCTION

