                        LET g_errparam.extend = "pmeh_t"
                        LET g_errparam.popup = TRUE
                        CALL cl_err()
                        CALL s_transaction_end('N','0')
                     ELSE
                        CALL s_transaction_end('Y','0')
                     END IF
                  END IF
                  CALL apmt510_show()
               END IF  
