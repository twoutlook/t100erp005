   #add-point: 提交前的ADP name="send.before_cli"
   IF NOT s_apmt510_conf_chk(g_pmee_m.pmeedocno,g_pmee_m.pmee900) THEN
      CLOSE apmt510_cl
      CALL s_transaction_end('N','0')
