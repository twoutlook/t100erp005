   LET l_imaf144 = ''
   SELECT imaf015,imaf144 INTO l_imaf015,l_imaf144 FROM imaf_t WHERE imafent = g_enterprise AND imaf001 = g_pmdd_d[l_ac].pmdd004 AND imafsite = g_site
   
