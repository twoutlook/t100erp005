PRIVATE FUNCTION apmt410_money_amount(p_pmdd019,p_pmdd010,p_pmdc012,p_pmdd018,p_pmdc005)
#151217-00006#1--add---begin----
DEFINE p_pmdd019     LIKE pmdd_t.pmdd019
