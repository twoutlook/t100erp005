   #保存單頭舊值
   LET g_pmdc_m_t.* = g_pmdc_m.*
   LET g_pmdc_m_o.* = g_pmdc_m.*
   
   IF g_pmdc_m.pmdcdocno IS NULL
   OR g_pmdc_m.pmdc900 IS NULL
 
