   SELECT inaa007 INTO l_inaa007 FROM inaa_t WHERE inaaent = g_enterprise AND inaasite = g_pmdd2_d[l_ac].pmdd037 AND inaa001 = g_pmdd2_d[l_ac].pmdd038
