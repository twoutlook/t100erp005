{<section id="apmp841.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION apmp841_b_fill()
 
   DEFINE ls_wc           STRING
   #add-point:b_fill段define
   DEFINE l_where         STRING       
   #end add-point
   #add-point:b_fill段define
   
   #end add-point
 
   #add-point:b_fill段sql_before
   CALL s_aooi500_sql_where(g_prog,'pmdasite') RETURNING l_where
   
   IF cl_null(g_def_wc) THEN 
      LET g_def_wc = " 1=1"
   END IF
   
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   
   LET g_sql = "SELECT 'N',pmdasite,t1.ooefl003,pmdadocno,pmdadocdt, ",       
               "       pmda207,pmda203,t2.ooefl003,pmda002,ooag011, ", 
               "       pmda003,t3.ooefl003,pmda021,pmda201,pmda204, ",         
               "       pmda205 ",
               "  FROM pmda_t ",
               "       LEFT OUTER JOIN ooefl_t t1 ON t1.ooeflent = pmdaent AND t1.ooefl001 = pmdasite AND t1.ooefl002 = '",g_dlang,"' ",               
               "       LEFT OUTER JOIN ooefl_t t2 ON t2.ooeflent = pmdaent AND t2.ooefl001 = pmda203  AND t2.ooefl002 = '",g_dlang,"' ",
               "       LEFT OUTER JOIN ooag_t ON ooagent = pmdaent AND ooag001 = pmda002 ",
               "       LEFT OUTER JOIN ooefl_t t3 ON t3.ooeflent = pmdaent AND t3.ooefl001 = pmda003  AND t3.ooefl002 = '",g_dlang,"' ",
               " WHERE pmdaent = ? ",
               "   AND pmdastus = 'Y' ",
               g_def_wc,
               "   AND ",g_wc,
               "   AND ",l_where,
               "   AND EXISTS(SELECT 1 FROM pmdb_t ",
               "               WHERE pmdbent = pmdaent AND pmdbdocno = pmdadocno ",
               "                 AND pmdb032 ='1'      AND pmdb207 = '0' ",
               #150519-00009#1 150520 by lori add---(S)
               "                 AND pmdb006 - (SELECT COALESCE (SUM (pmdp024), 0) FROM pmdp_t ",
               "                                 WHERE pmdpent = pmdbent AND pmdp003 = pmdbdocno AND pmdp004 = pmdbseq ",
               "                                   AND EXISTS (SELECT 1 FROM pmdl_t ",
               "                                                WHERE pmdlent = pmdpent ",
               "                                                  AND pmdldocno = pmdpdocno ",
               "                                                  AND pmdlstus != 'X')) > 0 ",               
               #150519-00009#1 150520 by lori add---(E)
               "             )"
               
   #DISPLAY "Source Slip: ",g_sql            
   LET g_sql = cl_sql_add_mask(g_sql)
   #end add-point
 
   PREPARE apmp841_sel FROM g_sql
   DECLARE b_fill_curs CURSOR FOR apmp841_sel
   
   CALL g_detail_d.clear()
   #add-point:b_fill段其他頁簽清空
   
   #end add-point
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs USING g_enterprise INTO 
   #add-point:b_fill段foreach_into
           g_detail_d[l_ac].sel,         g_detail_d[l_ac].pmdasite,    g_detail_d[l_ac].pmdasite_desc,  
           g_detail_d[l_ac].pmdadocno,   g_detail_d[l_ac].pmdadocdt,   g_detail_d[l_ac].pmda207,        
           g_detail_d[l_ac].pmda203,     g_detail_d[l_ac].pmda203_desc,g_detail_d[l_ac].pmda002,        
           g_detail_d[l_ac].pmda002_desc,g_detail_d[l_ac].pmda003,     g_detail_d[l_ac].pmda003_desc,   
           g_detail_d[l_ac].pmda021,     g_detail_d[l_ac].pmda201,     g_detail_d[l_ac].pmda204,        
           g_detail_d[l_ac].pmda205        
   #end add-point
   
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #add-point:b_fill段資料填充
      
      #end add-point
      
      CALL apmp841_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
         END IF
         EXIT FOREACH
      END IF
      
   END FOREACH
   LET g_error_show = 0
   
   #add-point:b_fill段資料填充(其他單身)
   CALL g_detail_d.deleteElement(g_detail_d.getLength())  
   LET g_master_idx = 1     
   #end add-point
    
   LET g_detail_cnt = l_ac - 1 
   DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE apmp841_sel
   
   LET l_ac = 1
   CALL apmp841_fetch()
   #add-point:b_fill段資料填充(其他單身)
 
   #end add-point
 
END FUNCTION
 
