#+ 畫面資料初始化
PRIVATE FUNCTION apmp841_init()
   #add-point:init段define
   DEFINE l_success   LIKE type_t.num5
   DEFINE l_sql       STRING
   #end add-point   
   #add-point:init段define
   
   #end add-point   
   
   LET g_error_show  = 1
   LET g_wc_filter   = " 1=1"
   LET g_wc_filter_t = " 1=1"
 
   #add-point:畫面資料初始化
   CALL cl_set_combo_scc("b_pmdb033",'2036')   #150324-00006#16 2015/04/29 By pomelo add
   
   CALL s_aooi500_create_temp() RETURNING l_success   #150308-00001#4 150309 by lori add
   
   LET g_errshow = '1'
  
   #拆單方式
   CALL cl_set_combo_scc('l_type','6743')
   CALL s_apmp841_get_slip_para() RETURNING l_type
   #150610-00030#3 150612 By sakura add(S)   
   #拆單方式參數設定
   CALL s_apmp841_get_slip_para() RETURNING l_type
   DISPLAY l_type TO FORMONLY.l_type
   #150610-00030#3 150612 By sakura add(E)   
   
   CALL s_apmp841_create_tmp() RETURNING l_success  

   LET l_sql = "SELECT COUNT(*) FROM apmp841_tmp01 WHERE pmdbdocno = ? "         #160727-00019#28 Mod   s_apmp841_detail -->apmp841_tmp01     
   LET l_sql = cl_sql_add_mask(l_sql)
   PREPARE apmp841_cnt_pr_detaial_tmp FROM l_sql   
   
   LET l_sql = "DELETE FROM apmp841_tmp01 WHERE pmdadocno = ? "                  #160727-00019#28 Mod   s_apmp841_detail -->apmp841_tmp01    
   LET l_sql = cl_sql_add_mask(l_sql)
   PREPARE apmp841_del_pr_detaial_tmp FROM l_sql

   #150519-00009#1 150520 by lori add---(S) 
   #檢查是否有可轉採購量
   #LET l_sql = "SELECT COUNT(*) FROM s_apmp841_detail WHERE pmdbdocno = ? AND pmdn007 > 0 "   
   LET l_sql = "SELECT COUNT(*) FROM pmdb_t ",
               " WHERE pmdbent = ", g_enterprise,
               "   AND pmdbdocno = ? ",
               "   AND pmdb006 - (SELECT COALESCE(SUM(pmdp024), 0) ",
               "                    FROM pmdp_t ",
               "                   WHERE pmdpent = pmdbent AND pmdp003 = pmdbdocno AND pmdp004 = pmdbseq ",   
               "                     AND EXISTS (SELECT 1 FROM pmdl_t WHERE pmdlent = pmdpent ",
               "                                    AND pmdldocno = pmdpdocno AND pmdlstus != 'X')) > 0 "                                                      
   #150519-00009#1 150520 by lori add---(E) 
   LET l_sql = cl_sql_add_mask(l_sql)
   PREPARE apmp841_cnt_pr_detaial_cnt FROM l_sql 
   #end add-point
   
