################################################################################
# Descriptions...: 將選取的單號的要貨採購相關資訊寫入暫存檔
# Memo...........:
# Usage..........: CALL apmp841_ins_detail(p_pmdadocno)
#                  RETURNING r_success
# Input Parameter: p_pmdadocno   要貨單號
# Return Code....: r_success             
# Date & Author..: 2014/12/25 By Lori
# Modify.........: 2016/03/04 By Lori    #160120-00001#6 調整函式參數
################################################################################
PRIVATE FUNCTION apmp841_ins_detail(p_pmdadocno)
   DEFINE p_pmdadocno   LIKE pmda_t.pmdadocno
   DEFINE r_success     LIKE type_t.num5
   DEFINE l_cnt         LIKE type_t.num5
   DEFINE l_pmdl011     LIKE pmdl_t.pmdl011  #Add By Ken 151006
   DEFINE l_sql         STRING               #Add By Ken 151006 
   DEFINE l_success     LIKE type_t.num5     #160120-00001#6 160302 by lori add
   
   LET r_success = TRUE
   
   #Add By Ken 151006(S)
   LET l_pmdl011 = ''
   IF NOT cl_null(g_argv[1]) THEN
      LET l_sql = " SELECT pmdl011 ",
                  "   FROM pmdl_t ",
                  "  WHERE pmdlent = ? ",
                  "    AND pmdldocno = '",g_argv[1],"' "
      LET l_sql = cl_sql_add_mask(l_sql)
      PREPARE apmp841_pmdl011 FROM l_sql
      EXECUTE apmp841_pmdl011 USING g_enterprise INTO l_pmdl011
   END IF
   #Add By Ken 151006(E)

   #確認暫存檔中有無資料,若無資料先新增
   LET l_cnt = 0 
   EXECUTE apmp841_cnt_pr_detaial_tmp USING p_pmdadocno
                                      INTO l_cnt
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend = 'apmp841_cnt_pr_detaial_tmp'
      LET g_errparam.popup = TRUE
      CALL cl_err()
      
      DISPLAY "ERROR: apmp841_cnt_pr_detaial_tmp"
   END IF                                       
   #DISPLAY "Slip: ", g_detail_d[l_ac].pmdadocno," Record Count: ",l_cnt
   
   IF l_cnt = 0 THEN
     #IF NOT s_apmp841_ins_tmp_pr_detail(p_pmdadocno,p_flag) THEN              #Mark By Ken 151006
     #160120-00001#6 160302 by lori mark and add---(S)
     #IF NOT s_apmp841_ins_tmp_pr_detail(p_pmdadocno,p_flag,l_pmdl011) THEN    #Add By Ken 151006  
     #   RETURN r_success                   
     #END IF       
      
      LET l_success = ''
      CALL s_apmp841_ins_tmp_pr_detail(p_pmdadocno,'N',l_pmdl011) RETURNING l_success        
     #160120-00001#6 160302 by lori mark and add---(E)      
   END IF                                    #150519-00009#1 150520 by lori add
  #ELSE                                      #150519-00009#1 150520 by lori mark
      LET l_cnt = 0 
      EXECUTE apmp841_cnt_pr_detaial_cnt USING p_pmdadocno
                                         INTO l_cnt
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = 'apmp841_cnt_pr_detaial_cnt'
         LET g_errparam.popup = TRUE
         CALL cl_err()
         
         DISPLAY "ERROR: apmp841_cnt_pr_detaial_cnt"
      END IF                      
      
      IF l_cnt = 0 THEN
         #要貨單沒有可轉採購單的數量！
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'apm-00842'
         LET g_errparam.extend = p_pmdadocno
         LET g_errparam.popup = TRUE
         CALL cl_err()           
         
         LET r_success = FALSE
      END IF      
   #END IF                                   #150519-00009#1 150520 by lori mark
   
   RETURN r_success
END FUNCTION

################################################################################
# Descriptions...: 數量轉換
# Memo...........:
# Usage..........: CALL apmp841_convert_qty()
#                  RETURNING r_success
# Input parameter: 無
# Return code....: r_success   處理結果
# Date & Author..: 2016/03/04 By Lori   #160120-00001#6 彙整程式段,數量轉換統一由此函式處理
# Modify.........:
################################################################################
PRIVATE FUNCTION apmp841_convert_qty()
   DEFINE r_success    LIKE type_t.num5
   DEFINE l_success    LIKE type_t.num5
   
   LET r_success = TRUE

   #採購包裝數量異動時
   IF g_detail2_d[l_ac2].l_pmdn202 <> g_detail2_d_t.l_pmdn202 THEN
      #160120-00001#6 160304 by lori add---(S)
      #要貨單未找到採購協議時,採購單位會為空
      IF cl_null(g_detail2_d[l_ac2].l_pmdn006) THEN
         #採購單位為空時,採購數量依要貨單位顯示
         #從採購包裝單位轉換成要貨單位
         IF NOT cl_null(g_detail2_d[l_ac2].l_pmdn201) THEN
            LET l_success = ''
            CALL s_aooi250_convert_qty(g_detail2_d[l_ac2].pmdb004,
                                       g_detail2_d[l_ac2].l_pmdn201,g_detail2_d[l_ac2].pmdb007,
                                       g_detail2_d[l_ac2].l_pmdn007)
               RETURNING l_success,g_detail2_d[l_ac2].l_pmdn007
         END IF               
      ELSE 
      #160120-00001#6 160304 by lori add---(E)     
         #轉換成採購數量
         IF NOT cl_null(g_detail2_d[l_ac2].l_pmdn006) THEN
            LET l_success = ''
            CALL s_aooi250_convert_qty(g_detail2_d[l_ac2].pmdb004,
                                       g_detail2_d[l_ac2].l_pmdn201,g_detail2_d[l_ac2].l_pmdn006,
                                       g_detail2_d[l_ac2].l_pmdn202)
               RETURNING l_success,g_detail2_d[l_ac2].l_pmdn007
            #150903-00007#2 150908 by sakura add(S)
            IF NOT l_success THEN
               #商品%1，採購單位%2與採購包裝單位%3轉換數量失敗！
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 'apm-01000'   
               LET g_errparam.extend = ''
               LET g_errparam.popup = TRUE
               LET g_errparam.replace[1] = g_detail2_d[l_ac2].pmdb004
               LET g_errparam.replace[2] = g_detail2_d[l_ac2].l_pmdn006
               LET g_errparam.replace[3] = g_detail2_d[l_ac2].l_pmdn201
               CALL cl_err()
               
               LET r_success = FALSE
               RETURN r_success            
            END IF
            #150903-00007#2 150908 by sakura add(E)
         ELSE
            LET g_detail2_d[l_ac2].l_pmdn007 = g_detail2_d[l_ac2].l_pmdn202   #160120-00001#6 160304 by lori add
         END IF  
      END IF
      
      #轉換成計價數量
      IF NOT cl_null(g_detail2_d[l_ac2].l_pmdn010) THEN
         LET l_success = ''
         CALL s_aooi250_convert_qty(g_detail2_d[l_ac2].pmdb004,
                                    g_detail2_d[l_ac2].l_pmdn201,g_detail2_d[l_ac2].l_pmdn010,
                                    g_detail2_d[l_ac2].l_pmdn202)
            RETURNING l_success,g_detail2_d[l_ac2].l_pmdn011
         #150903-00007#2 150908 by sakura add(S)
         IF NOT l_success THEN
            #商品%1，採購單位%2與計價單位%3轉換數量失敗！   
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = 'apm-00998'   
            LET g_errparam.extend = ''
            LET g_errparam.popup = TRUE
            LET g_errparam.replace[1] = g_detail2_d[l_ac2].pmdb004
            LET g_errparam.replace[2] = g_detail2_d[l_ac2].l_pmdn006
            LET g_errparam.replace[3] = g_detail2_d[l_ac2].l_pmdn010
            CALL cl_err()

            LET r_success = FALSE
            RETURN r_success 
         END IF
         #150903-00007#2 150908 by sakura add(E)
      END IF
   END IF   #160120-00001#6 160304 by lori add
 
   #採購數量異動時
   IF g_detail2_d[l_ac2].l_pmdn007 <> g_detail2_d_t.l_pmdn007 THEN
      #160120-00001#6 160304 by lori add---(S)
