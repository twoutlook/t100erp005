#+ 資料處理   (r類使用g_master為主處理/p類使用l_param為主)
PRIVATE FUNCTION apmp849_process(ls_js)
 
   #add-point:process段define (客製用)
   
   #end add-point
   DEFINE ls_js         STRING
   DEFINE lc_param      type_parameter
   DEFINE li_stus       LIKE type_t.num5
   DEFINE li_count      LIKE type_t.num10  #progressbar計量
   DEFINE ls_sql        STRING             #主SQL
   DEFINE li_p01_status LIKE type_t.num5
   #add-point:process段define 
   DEFINE i           LIKE type_t.num5
   DEFINE l_sql       STRING
   DEFINE l_cnt		 LIKE type_t.num5       
   DEFINE l_no_cnt	 LIKE type_t.num5    
   DEFINE l_no_cnt2   LIKE type_t.num5      
   DEFINE l_msg       STRING
   DEFINE l_pmdnsite  LIKE pmdn_t.pmdnsite
   DEFINE l_pmdndocno LIKE pmdn_t.pmdndocno
   DEFINE l_pmdnseq   LIKE pmdn_t.pmdnseq   
   DEFINE l_pmdlsite  LIKE pmdl_t.pmdlsite
   DEFINE l_where     STRING
   DEFINE l_pmdl004   LIKE pmdl_t.pmdl004   #150827-00001#1 150914 by sakura add   
   DEFINE l_pmdn001   LIKE pmdn_t.pmdn001   #150827-00001#1 150914 by sakura add   
   #end add-point
 
  #INITIALIZE lc_param TO NULL           #p類不可以清空
   CALL util.JSON.parse(ls_js,lc_param)  #r類作業被t類呼叫時使用, p類主要解開參數處
   LET li_p01_status = 1
 
  #IF lc_param.wc IS NOT NULL THEN
  #   LET g_bgjob = "T"       #特殊情況,此為t類作業鬆耦合串入報表主程式使用
  #END IF
 
   #add-point:process段前處理
   CALL s_aooi500_sql_where(g_prog,'pmdlsite') RETURNING l_where
   LET l_cnt = 1
   LET ls_sql = " SELECT DISTINCT pmdldocno ",
                "   FROM pmdn_t,pmdl_t ",
                "  WHERE pmdndocno = pmdldocno ",
                "    AND pmdlent = ",g_enterprise,
                "    AND pmdlstus = 'Y' ",
                #"    AND (pmdl205 > '",g_today,"'",   #150916-00016#1 150917 by sakura mark
                "    AND (pmdl205 >= '",g_today,"'",   #150916-00016#1 150917 by sakura add
                "     OR (pmdl205 IS NULL AND pmdl206 = 'Y'))", #長效期
                "    AND pmdlstus <> 'C'",
                "    AND COALESCE (pmdl047,'N') = 'N' ",   #150916-00016#1 150917 by sakura add
                "    AND COALESCE (pmdn045,'1') = '1' ",   #150916-00016#1 150917 by sakura add                
                "    AND ",lc_param.wc,
                "    AND ",l_where                
   PREPARE apmp849_sel_pr FROM ls_sql
   DECLARE apmp849_sel_cs CURSOR FOR apmp849_sel_pr  
   FOREACH apmp849_sel_cs INTO g_pmdl[l_cnt].*
     IF SQLCA.sqlcode THEN       
        EXIT FOREACH
     END IF
     LET l_cnt = l_cnt + 1
   END FOREACH   
   CALL g_pmdl.deleteelement(l_cnt)
   LET l_cnt = l_cnt - 1      

   #end add-point
 
   #預先計算progressbar迴圈次數
   IF g_bgjob <> "Y" THEN
      #add-point:process段count_progress
      LET li_count = l_cnt * 2  
      CALL cl_progress_bar_no_window(li_count)
      #end add-point
   END IF
 
   #主SQL及相關FOREACH前置處理
#  DECLARE apmp849_process_cs CURSOR FROM ls_sql
#  FOREACH apmp849_process_cs INTO
   #add-point:process段process

   LET g_success = TRUE 
   CALL cl_err_collect_init()
   LET l_no_cnt = 0 
   IF l_cnt > 0 THEN                    
      FOR i = 1 TO l_cnt  
          
          LET l_no_cnt2 = 0      
          #抓取單身資料                                                         
          #LET l_sql = "SELECT DISTINCT pmdnsite,pmdndocno,pmdnseq ",                  #150827-00001#1 150914 by sakura mark
          LET l_sql = "SELECT DISTINCT pmdl004,pmdnsite,pmdndocno,pmdnseq,pmdn001 ",   #150827-00001#1 150914 by sakura add
                      "  FROM pmdn_t,pmdl_t ",
                      " WHERE pmdndocno = pmdldocno ",
                      "   AND pmdlent = ",g_enterprise,
                      "   AND pmdndocno = '",g_pmdl[i].pmdldocno,"'",                      
                      "   AND pmdlstus = 'Y' ",
                     #"   AND (pmdl205 > '",g_today,"'",   #150916-00016#1 150917 by sakura mark
                      "   AND (pmdl205 >= '",g_today,"'",  #150916-00016#1 150917 by sakura add   
                      "    OR (pmdl205 IS NULL AND pmdl206 = 'Y'))", #長效期
                      "   AND pmdlstus <> 'C'",
                      "   AND COALESCE (pmdl047,'N') = 'N' ",   #150916-00016#1 150917 by sakura add
                      "   AND COALESCE (pmdn045,'1') = '1' ",   #150916-00016#1 150917 by sakura add                      
                      "   AND ",lc_param.wc,          
                      "   AND ",l_where
          PREPARE apmp849_sel_pr2 FROM l_sql
          DECLARE apmp849_sel_cs2 CURSOR FOR apmp849_sel_pr2
          
          #FOREACH apmp849_sel_cs2 INTO l_pmdnsite,l_pmdndocno,l_pmdnseq            #150827-00001#1 150914 by sakura mark   
          FOREACH apmp849_sel_cs2 INTO l_pmdl004,l_pmdnsite,l_pmdndocno,l_pmdnseq,l_pmdn001   #150827-00001#1 150914 by sakura add
            IF SQLCA.SQLcode THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = SQLCA.sqlcode
               LET g_errparam.extend = "FOREACH:"
               LET g_errparam.popup = TRUE
               CALL cl_err()
            
               EXIT FOREACH
            END IF
            #單身狀態結案
            #IF NOT s_apmp849_pmdn045_closed('1',l_pmdndocno,l_pmdnseq,l_pmdnsite) THEN   #150827-00001#1 150914 by sakura mark
            IF NOT s_apmp849_pmdn045_closed('1',l_pmdl004,l_pmdndocno,l_pmdnseq,l_pmdn001,l_pmdnsite) THEN   #150827-00001#1 150914 by sakura add
               LET l_no_cnt2 = l_no_cnt2 + 1
            END IF
          END FOREACH
          
          #取單頭採購組織
          SELECT pmdlsite INTO l_pmdlsite 
            FROM pmdl_t
           WHERE pmdlent = g_enterprise
             AND pmdldocno = g_pmdl[i].pmdldocno          
          #物流結案
          IF g_bgjob <> "Y" THEN
             LET l_msg = cl_getmsg_parm("apm-00402", g_lang,g_pmdl[i].pmdldocno )   #處理(%1)物流結案   
             CALL cl_progress_no_window_ing(l_msg)
          END IF          
          IF NOT s_apmp849_pmdl047_closed(g_pmdl[i].pmdldocno,l_pmdlsite) THEN                
             LET l_no_cnt2 = l_no_cnt2 + 1
          END IF
          IF l_no_cnt2 = 0 THEN                                                                  
             CALL s_transaction_end("Y","0")                                                  
          ELSE                                                                                
             CALL s_transaction_end("N","0")
             LET l_no_cnt = l_no_cnt + 1
          END IF     
          #狀態結案
          IF g_bgjob <> "Y" THEN
             LET l_msg = cl_getmsg_parm("apm-00405", g_lang,g_pmdl[i].pmdldocno)   #處理(%1)狀態結案
             CALL cl_progress_no_window_ing(l_msg)
          END IF  
          CALL s_transaction_begin()
             
          IF NOT s_apmp849_pmdlstus_closed(g_pmdl[i].pmdldocno,l_pmdlsite) THEN     
             CALL s_transaction_end('N','0')
             LET l_no_cnt = l_no_cnt + 1
          ELSE
             CALL s_transaction_end('Y','0')
          END IF        
      END FOR
   END IF
   IF l_no_cnt > 0 THEN    
      CALL cl_err_collect_show()
   END IF 
   #end add-point
#  END FOREACH
 
   IF g_bgjob = "N" THEN
      #前景作業完成處理
      #add-point:process段foreground完成處理
      IF l_cnt = 0 THEN
         CALL cl_ask_confirm3("afa-00067","")
         RETURN
      END IF
      #end add-point
      CALL cl_ask_confirm3("std-00012","")
   ELSE
      #背景作業完成處理
      #add-point:process段background完成處理
      IF cl_chk_process_exists(g_parentsession,g_account) THEN
         CALL cl_ask_confirm("std-00012") RETURNING li_stus
      END IF     
      DISPLAY "!!!!程序已完成!!!"
      #end add-point
      CALL cl_schedule_exec_call(li_p01_status)
   END IF
 
   #呼叫訊息中心傳遞本關完成訊息
   CALL apmp849_msgcentre_notify()
 
END FUNCTION
 
