################################################################################
# Descriptions...: 新增時，依據產品特徵開窗後的值新增到資料庫
# Memo...........:
# Usage..........: CALL apmt850_feature ()
#                  RETURNING 新增時，依據產品特徵開窗後的值新增到資料庫
# Input parameter: 
#                : 
# Return code....: r_success      TRUE/FALSE
#                : 
# Date & Author..: 2014/04/15 By Elise
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt850_feature()
   DEFINE  l_pmegseq    LIKE pmeg_t.pmegseq
   DEFINE  l_pmeg_d     type_g_pmeg_d
   DEFINE  r_success    LIKE type_t.num5
   DEFINE  l_success    LIKE type_t.num5
   DEFINE  l_i          LIKE type_t.num5
   DEFINE  l_pmao004    LIKE pmao_t.pmao004
   
   LET r_success = TRUE   

   LET l_pmegseq = ''   
   SELECT MAX(pmegseq) INTO l_pmegseq
     FROM pmeg_t
    WHERE pmegent = g_enterprise
      AND pmegdocno = g_pmee_m.pmeedocno
      AND pmeg900 = g_pmee_m.pmee900
   IF cl_null(l_pmegseq) THEN
      LET l_pmegseq = 0
   END IF   

   #因為第一筆資料已呈現在畫面並寫入DB, 從第二筆開始處理
   FOR l_i = 2 TO g_inam.getLength()
 
      LET l_pmeg_d.* = g_pmeg_d[l_ac].*
      
      LET l_pmeg_d.pmegseq = l_pmegseq + 1   
      LET l_pmeg_d.pmeg002 = g_inam[l_i].inam002
      LET l_pmeg_d.pmeg011 = g_inam[l_i].inam004
      LET l_pmeg_d.pmeg024 = 'N'            #多交期
      LET l_pmeg_d.pmeg901 = '3' 
      
      #160130-00006#2 160315 by lori add---(S)
      IF NOT apmt850_unique_chk(l_pmeg_d.pmegseq, l_pmeg_d.pmegsite, l_pmeg_d.pmegunit, 
                                l_pmeg_d.pmeg001, l_pmeg_d.pmeg002) THEN
         LET l_pmegseq = l_pmegseq - 1
         CONTINUE FOR
      END IF
      #160130-00006#2 160315 by lori add---(E)
      
      #150413-00018#1 20150414 pomelo add(S)
      #當銷售數量有值，由銷售數量轉換成包裝數量
      CALL s_aooi250_convert_qty(l_pmeg_d.pmeg001,l_pmeg_d.pmeg006,l_pmeg_d.pmeg201,l_pmeg_d.pmeg007)
         RETURNING l_success,l_pmeg_d.pmeg202

      #計算參考數量
      IF NOT cl_null(l_pmeg_d.pmeg008) THEN
         CALL s_aooi250_convert_qty(l_pmeg_d.pmeg001,l_pmeg_d.pmeg006,l_pmeg_d.pmeg008,l_pmeg_d.pmeg007)
            RETURNING l_success,l_pmeg_d.pmeg009
      END IF
      
      #計算計價數量
      IF NOT cl_null(l_pmeg_d.pmeg010) THEN
         CALL s_aooi250_convert_qty(l_pmeg_d.pmeg001,l_pmeg_d.pmeg006,l_pmeg_d.pmeg010,l_pmeg_d.pmeg007)
            RETURNING l_success,l_pmeg_d.pmeg011
      END IF
      
      #可用庫存量 跟 採購在途量
      CALL apmt850_get_sum(l_pmeg_d.pmegunit,l_pmeg_d.pmeg001,l_pmeg_d.pmeg002)
         RETURNING l_pmeg_d.pmeg206,l_pmeg_d.pmeg207
      
      #計算單身日結/月結 銷量欄位
      CALL apmt850_get_sale(l_pmeg_d.pmeg205,l_pmeg_d.pmeg001,l_pmeg_d.pmeg002)
       RETURNING l_pmeg_d.pmeg208,l_pmeg_d.pmeg209,l_pmeg_d.pmeg210,
       　　　　　 l_pmeg_d.pmeg211,l_pmeg_d.pmeg212,l_pmeg_d.pmeg213
      #150413-00018#1 20150414 pomelo add(E)
      #未稅金額、含稅金額、稅額
      #150504-00019#1 150506 by lori522612 mod---(S) 
      CALL s_apmt840_get_amount(g_pmee_m.pmeedocno,l_pmeg_d.pmegseq,g_inam[l_i].inam004,l_pmeg_d.pmeg015, 
                                l_pmeg_d.pmeg016,g_pmee_m.pmee015,g_pmee_m.pmee016)
         RETURNING l_pmeg_d.pmeg046,l_pmeg_d.pmeg048,l_pmeg_d.pmeg047
      #150504-00019#1 150506 by lori522612 mod---(E) 
      INSERT INTO pmeg_t(pmegent,   pmegsite, pmegunit, pmegorga, pmegseq,
                         pmegdocno, pmeg900,  pmeg901,  pmeg902,  pmeg903,                    
                         pmeg001,   pmeg002,  
                         pmeg006,   pmeg007,  pmeg008,  pmeg009,  pmeg010, 
                         pmeg011,   pmeg012,  pmeg013,  pmeg014,  pmeg015, 
                         pmeg016,   pmeg017,  pmeg019,  pmeg020,  pmeg021, 
                         pmeg022,   pmeg023,  pmeg024,  pmeg025,  pmeg026,
                         pmeg028,   pmeg029,  pmeg030,  
                         pmeg043,   pmeg045,  pmeg046,
                         pmeg047,   pmeg048,  pmeg049,  pmeg050,  pmeg051,
                         pmeg052,   pmeg053,  pmeg200,  pmeg201,  pmeg202,
                         pmeg203,   pmeg205,  pmeg206,  pmeg207,  pmeg208,
                         pmeg209,   pmeg210,  pmeg211,  pmeg212,  pmeg213,
                         pmeg214,   pmeg215,  pmeg216,  pmeg217,  pmeg218,
                         pmeg219,   pmeg220,  pmeg221,  pmeg222, pmeg223,
