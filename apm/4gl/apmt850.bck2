   DEFINE l_ooef016   LIKE ooef_t.ooef016

   LET g_pmee_m.pmee009      = ''
   LET g_pmee_m.pmee009_desc = ''
   LET g_pmee_m.pmee010      = ''
   LET g_pmee_m.pmee010_desc = ''
   LET g_pmee_m.pmee011      = ''
   LET g_pmee_m.pmee011_desc = ''
   LET g_pmee_m.pmee012      = ''
   LET g_pmee_m.pmee013      = ''
   LET g_pmee_m.pmee015      = ''
   LET g_pmee_m.pmee015_desc = ''
   LET g_pmee_m.pmee016      = ''
   LET g_pmee_m.pmee017      = ''
   LET g_pmee_m.pmee017_desc = ''
