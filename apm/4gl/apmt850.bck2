         LET g_pmee025_org = g_pmee_m.pmeeunit
      END IF
      
   END IF 
   
   IF p_type = 2 THEN
      LET g_pmeg025_org = ''
      LET g_pmeg025_connid = ''
      
      IF g_pmee_m.pmee203 = '3' THEN
         CALL s_apmt840_get_ooef012(g_pmeg_d[l_ac].pmeg223) RETURNING g_pmeg025_connid
         LET g_pmeg025_org = g_pmeg_d[l_ac].pmeg223
      ELSE
