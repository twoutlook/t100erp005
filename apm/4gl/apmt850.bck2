    CALL apmt850_get_pmeg228(g_pmeg_d[l_ac].pmeg001) RETURNING g_pmeg_d[l_ac].pmeg228,g_pmeg_d[l_ac].pmeg228_desc
    #160118-00013#4 160119 by lori add---(E)
    
    #160130-00006#2 160315 by lori---(S)
    #採購渠道
    IF NOT cl_null(g_pmee_m.pmee023) THEN
