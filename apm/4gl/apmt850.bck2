#維護多帳期預付款
PRIVATE FUNCTION apmt850_open_pmef()
DEFINE  l_cmd           LIKE type_t.chr1
DEFINE  l_ac_t          LIKE type_t.num5                #未取消的ARRAY CNT 
DEFINE  l_n             LIKE type_t.num5                #檢查重複用  
DEFINE  l_cnt           LIKE type_t.num5                #檢查重複用  
DEFINE  l_lock_sw       LIKE type_t.chr1                #單身鎖住否  
DEFINE  l_allow_insert  LIKE type_t.num5                #可新增否 
DEFINE  l_allow_delete  LIKE type_t.num5                #可刪除否  
DEFINE  l_count         LIKE type_t.num5
DEFINE  l_i             LIKE type_t.num5
DEFINE  l_insert        BOOLEAN
DEFINE  ls_return       STRING
DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
DEFINE  l_vars          DYNAMIC ARRAY OF STRING
DEFINE  l_fields        DYNAMIC ARRAY OF STRING
DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
DEFINE  l_pmef005       LIKE pmef_t.pmef005
DEFINE  l_pmef006       LIKE pmef_t.pmef006
DEFINE  l_forupd_sql    STRING
DEFINE  l_pmee040       LIKE pmee_t.pmee040
DEFINE  l_pmee041       LIKE pmee_t.pmee041
DEFINE  l_success       LIKE type_t.num5
DEFINE  l_oodbl004      LIKE oodbl_t.oodbl004
DEFINE  l_oodb005       LIKE oodb_t.oodb005
DEFINE  l_oodb006       LIKE oodb_t.oodb006
DEFINE  l_oodb011       LIKE oodb_t.oodb011

   IF g_pmee_m.pmeedocno IS NULL THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = "std-00003"
      LET g_errparam.extend = ""
      LET g_errparam.popup = TRUE
      CALL cl_err()

      RETURN
   END IF
   
   SELECT pmee040,pmee041 INTO l_pmee040,l_pmee041 FROM pmee_t
    WHERE pmeeent = g_enterprise
      AND pmeedocno = g_pmee_m.pmeedocno
      AND pmee900 = g_pmee_m.pmee900
   IF cl_null(l_pmee040) THEN
      LET l_pmee040 = 0
   END IF
   IF cl_null(l_pmee041) THEN
      LET l_pmee041 = 0
   END IF
 
   ERROR ""
  
   CALL s_transaction_begin()
   

   LET l_forupd_sql = "SELECT pmeesite,pmee901,pmeedocno,pmee900,'',pmeedocdt,pmee902,pmee001,pmee002, ",
                      "       '',pmee003,'',pmeestus,pmee004,'',pmeeacti,pmee005,pmee006,pmee007,pmee008,pmee027,'',pmee009, ",
                      "       '',pmee010,'',pmee011,'',pmee012,pmee013,pmee033,'',pmee015,'',pmee016,pmee017,'',pmee018,'', ",
                      "       pmee019, pmee043,'',pmee044,pmee903,'',pmee904,pmee020,'',pmee025,'','', ",
                      "       pmee029,'',pmee021,'',pmee022,'',pmee032,'',pmee023,'',pmee024,'',pmee030,pmee031,pmee028,pmee040,pmee041, ",
                      "       pmee042,pmee046,pmeeownid,'',pmeeowndp,'',pmeecrtid,'',pmeecrtdp,'',pmeecrtdt,pmeemodid,'',pmeemoddt, ", 
                      "       pmeecnfid,'',pmeecnfdt ",
                      "  FROM pmee_t ",
                      " WHERE pmeeent= ? AND pmeedocno=? ",                        
                      "   AND pmee900=? FOR UPDATE " 

   LET l_forupd_sql = cl_sql_forupd(l_forupd_sql)                #轉換不同資料庫語法
   DECLARE apmt850_cl22 CURSOR FROM l_forupd_sql    
   OPEN apmt850_cl22 USING g_enterprise,g_pmee_m.pmeedocno,g_pmee_m.pmee900

   IF STATUS THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = STATUS
      LET g_errparam.extend = "OPEN apmt850_cl22:"
      LET g_errparam.popup = TRUE
      CALL cl_err()

      CLOSE apmt850_cl22
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料     
    FETCH apmt850_cl22 INTO g_pmee_m.pmeesite,#g_pmee_m.pmee901,
                            g_pmee_m.pmeedocno,g_pmee_m.pmee900,
                            g_pmee_m.pmeedocdt,g_pmee_m.pmee902,
                            g_pmee_m.pmee001,g_pmee_m.pmee002,g_pmee_m.pmee002_desc,g_pmee_m.pmee003,g_pmee_m.pmee003_desc,
                            g_pmee_m.pmeestus,g_pmee_m.pmee004,g_pmee_m.pmee004_desc,g_pmee_m.pmeeacti,g_pmee_m.pmee005,g_pmee_m.pmee006,
                            g_pmee_m.pmee007,g_pmee_m.pmee008,g_pmee_m.pmee027,g_pmee_m.pmee027_desc,g_pmee_m.pmee009,g_pmee_m.pmee009_desc,
