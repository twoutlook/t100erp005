# Memo...........: 依 g_pmeg_d 定義順序
# Usage..........: CALL apmt850_detail_color()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/11/21 by Lori 
# Modify.........: 2016/01/19 by Lori #160118-00013#4 更新單身ARRAY 內容
################################################################################
PRIVATE FUNCTION apmt850_detail_color()
DEFINE l_pmek002         LIKE pmek_t.pmek002

   CALL apmt850_detail_init_color()

   DECLARE sel_pmek_cs CURSOR FOR
    SELECT pmek002 FROM pmek_t
     WHERE pmekent   = g_enterprise
       AND pmekdocno = g_pmee_m.pmeedocno
       AND pmekseq   = g_pmeg_d[l_ac].pmegseq
       AND pmekseq1  = 0                                       
       AND pmekseq2  = 0         
       AND pmek001   = g_pmee_m.pmee900
       AND pmek002 LIKE 'pmdn%'   #160118-00013#4 160121 by lori add               
   FOREACH sel_pmek_cs INTO l_pmek002
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "FOREACH:"
         LET g_errparam.popup = TRUE
         CALL cl_err()

         EXIT FOREACH
      END IF
      
      LET l_pmek002 = cl_replace_str(l_pmek002,'pmdn','pmeg')

      CASE l_pmek002
