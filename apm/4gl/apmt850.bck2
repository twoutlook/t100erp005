################################################################################
# Descriptions...: 計算有無收貨單
# Memo...........:
# Usage..........: CALL apmt850_count_pmds(p_chk_type,p_docno,p_seq)
#                  RETURNING r_cnt
# Input parameter: p_chk_type 檢查方式:1.採購單 2.採購項次
#                  p_docno    採購單號
#                  p_seq      採購項次
# Return code....: r_cnt      計算結果
# Date & Author..: 2014/12/05 By Lori
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt850_count_pmds(p_chk_type,p_docno,p_seq)
   DEFINE p_chk_type   LIKE type_t.num5
   DEFINE p_docno      LIKE pmee_t.pmeedocno
   DEFINE p_seq        LIKE pmeg_t.pmegseq
   DEFINE r_cnt        LIKE type_t.num5
   DEFINE l_sql        STRING
   
   LET r_cnt = 0
   
   LET l_sql = "SELECT COUNT(*) ",
               "  FROM pmdt_t,pmds_t ",
               " WHERE pmdtent = pmdsent ",
               "   AND pmdtdocno = pmdsdocno ",
               "   AND pmdtent = ",g_enterprise ,
               "   AND pmdt001 = '",p_docno,"' ",
               "   AND pmdsstus != 'X' ",
               "   AND (pmds000 = '1'  ", #收貨單
               "    OR  pmds000 = '3') "  #收貨入庫單
   
   IF p_chk_type = 2 THEN
      LET l_sql = l_sql," AND pmdtseq = ",p_seq
   END IF
   PREPARE apmt850_cnt_pmds FROM l_sql
   EXECUTE apmt850_cnt_pmds INTO r_cnt   
   
   RETURN r_cnt   
END FUNCTION
################################################################################
# Descriptions...: 採購單號檢查
# Memo...........: 檢查是否可填變更單
# Usage..........: CALL apmt850_pmeedocno_chk()
# Date & Author..: 2014/12/08 By Lori
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt850_pmeedocno_chk()
   DEFINE r_success         LIKE type_t.num5
   DEFINE l_cnt             LIKE type_t.num5
   DEFINE l_flag            LIKE type_t.num5
   DEFINE l_pmdl047         LIKE pmdl_t.pmdl047
   DEFINE l_pmdlsite        LIKE pmdl_t.pmdlsite

   LET r_success = TRUE

   IF cl_null(g_pmee_m.pmeedocno) THEN
      RETURN r_success
   END IF

   INITIALIZE g_chkparam.* TO NULL
	#160818-00001#1 160818 add by ken(S)       
   IF NOT cl_null(g_slip_wc) THEN                
      LET g_chkparam.where = g_slip_wc           
   END IF                                        
   #160818-00001#1 160818 add by ken(E)    
   LET g_chkparam.arg1 = g_pmee_m.pmeedocno
   LET g_chkparam.err_str[1] = "apm-00336:apm-01077"
  #LET g_chkparam.err_str[2] = "apm-00337:apm-01078"  #160318-00005#38 mark
   LET g_chkparam.err_str[2] = "apm-00337:sub-01302|apmt840|",cl_get_progname("apmt840",g_lang,"2"),"|:EXEPROGapmt840"  #"apm-00337:apm-01078" #160318-00005#38  by 07900--mod
   IF NOT cl_chk_exist("v_pmdldocno") THEN
      LET r_success = FALSE
      RETURN r_success
   END IF

   #採購組織不同不可修改
   IF NOT cl_null(g_pmee_m.pmeesite) THEN
      LET l_pmdlsite = ''
      SELECT pmdlsite INTO l_pmdlsite
        FROM pmdl_t
       WHERE pmdlent = g_enterprise
         AND pmdldocno = g_pmee_m.pmeedocno       
      IF l_pmdlsite <> g_pmee_m.pmeesite THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'apm-00679'
         LET g_errparam.extend = ''
         LET g_errparam.replace[1] = g_pmee_m.pmeedocno
         LET g_errparam.replace[2] = l_pmdlsite
         LET g_errparam.replace[3] = g_pmee_m.pmeesite
         LET g_errparam.popup = TRUE
         CALL cl_err()
         
         LET r_success = FALSE         
         RETURN r_success      
      END IF      
   END IF

   #同一張採購單號不能同時存在兩張以上未確認的變更單內
   LET l_cnt = 0
   SELECT COUNT(pmeedocno) INTO l_cnt
     FROM pmee_t
    WHERE pmeeent = g_enterprise
      AND pmeedocno = g_pmee_m.pmeedocno
      AND pmeestus = 'N'
      
   IF l_cnt > 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'apm-00396'
      LET g_errparam.extend = g_pmee_m.pmeedocno
      LET g_errparam.popup = TRUE
      CALL cl_err()

      LET r_success = FALSE
      RETURN r_success
   END IF
      
   
   #物流結案="Y"的採購單不可以進行變更
   LET l_pmdl047 = ''
   SELECT pmdl047 INTO l_pmdl047 FROM pmdl_t
    WHERE pmdlent = g_enterprise
      AND pmdldocno = g_pmee_m.pmeedocno
   IF l_pmdl047 = 'Y' THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'apm-01073'
      LET g_errparam.extend = g_pmee_m.pmeedocno
      LET g_errparam.popup = TRUE
      CALL cl_err()
   
      LET r_success = FALSE
      RETURN r_success
   END IF

   RETURN r_success

END FUNCTION

