################################################################################
# Descriptions...: 更新單頭採購失效日
# Memo...........:
# Usage..........: CALL apmt850_pmee205_upd(p_type)
#                     RETURNING r_success,r_confirm
# Input parameter: p_type         修改動作 Y：確認 N:放棄
# Return code....: r_success      True/False
#                : r_confirm      是否有修改
# Date & Author..: 2015/06/08 By Ken
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt850_pmee205_upd(p_type)
DEFINE p_type            LIKE type_t.chr1
DEFINE r_success         LIKE type_t.num5
DEFINE r_confirm         LIKE type_t.num5
DEFINE l_pmeh013         LIKE pmeh_t.pmeh013
DEFINE l_cnt             LIKE type_t.num5
DEFINE l_replace         STRING
DEFINE l_msg             STRING

   LET r_success = TRUE
   LET r_confirm = TRUE
   
   #當長效期訂單 = 'Y' 且 單頭採購失效日為空
   # 不需要處理採購失效日
   IF g_pmee_m.pmee206 = 'Y' AND cl_null(g_pmee_m.pmee205) THEN
      RETURN r_success,r_confirm
   END IF
   
   #單身沒有資料
   LET l_cnt = 0
   SELECT COUNT(pmegseq) INTO l_cnt
     FROM pmeg_t
    WHERE pmegent = g_enterprise
      AND pmegdocno = g_pmee_m.pmeedocno
      AND pmeg900 = g_pmee_m.pmee900
   IF l_cnt = 0 THEN
      RETURN r_success,r_confirm
   END IF
   
   #取採購交期明細檔中最大的到庫日期
   LET l_pmeh013 = ''
   SELECT MAX(pmeh013) INTO l_pmeh013
     FROM pmeh_t
    WHERE pmehent   = g_enterprise
      AND pmehdocno = g_pmee_m.pmeedocno  
      AND pmeh900 = g_pmee_m.pmee900
      
    
   LET l_replace = g_pmee_m.pmee205 ,"|",l_pmeh013
   #單頭採購失效日 < 取採購多交期最大到庫日期
   IF g_pmee_m.pmee205 < l_pmeh013 AND NOT cl_null(g_pmee_m.pmee205) THEN
      LET l_msg = cl_getmsg_parm('apm-00884',g_dlang,l_replace)
      IF NOT cl_ask_confirm_parm('std-00008',l_msg) THEN
         LET r_confirm = FALSE
         RETURN r_success,r_confirm
      END IF
   END IF
   
   #單頭採購失效日 > 取採購多交期最大到庫日期
   IF g_pmee_m.pmee205 > l_pmeh013 AND NOT cl_null(g_pmee_m.pmee205) THEN
      LET l_msg = cl_getmsg_parm('apm-00932',g_dlang,l_replace)
      IF NOT cl_ask_confirm_parm('std-00008',l_msg) THEN
         RETURN r_success,r_confirm
      END IF
   END IF
   
   #修改動作為放棄修改
   #先rollback，在開啟trancation
   IF p_type = 'N' THEN
      CALL s_transaction_end('N','0')
   END IF
   IF s_transaction_chk("N",0) THEN
      CALL s_transaction_begin()
   END IF
   
   #更新單頭及單身採購失效日
   UPDATE pmee_t 
      SET pmee205 = l_pmeh013
    WHERE pmeeent = g_enterprise
      AND pmeedocno = g_pmee_m.pmeedocno
      AND pmee900 = g_pmee_m.pmee900
   IF SQLCA.sqlcode THEN
      LET r_success = FALSE
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'sub-00034'
      LET g_errparam.extend = "Upd pmee_t"
      LET g_errparam.popup = TRUE
      CALL s_transaction_end('N','0')
      LET r_success = FALSE
      RETURN r_success,r_confirm      
      CALL cl_err()
   END IF      
      
   UPDATE pmeg_t 
      SET pmeg224 = l_pmeh013
    WHERE pmegent = g_enterprise
      AND pmegdocno = g_pmee_m.pmeedocno  
      AND pmeg900 = g_pmee_m.pmee900
   IF SQLCA.sqlcode THEN
      LET r_success = FALSE
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'sub-00034'
      LET g_errparam.extend = "Upd pmeg_t"
      LET g_errparam.popup = TRUE
      CALL cl_err()
      LET r_success = FALSE
      CALL s_transaction_end('N','0')
      RETURN r_success,r_confirm      
   END IF
     
   LET g_pmee_m.pmee205 = l_pmeh013
   DISPLAY BY NAME g_pmee_m.pmee205
   
   CALL s_transaction_end('Y','0')
   RETURN r_success,r_confirm
END FUNCTION

################################################################################
# Descriptions...: 檢查採購失效日
# Memo...........:
# Usage..........: CALL apmt850_chk_pmee205()
#                  RETURNING r_success
# Input parameter: 無
# Return code....: r_success      True/False
# Date & Author..: 2015/06/08 By Ken #15050604-00026#2
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt850_chk_pmee205()
DEFINE r_success         LIKE type_t.num5
DEFINE l_pmeh013         LIKE pmeh_t.pmeh013
DEFINE l_cnt             LIKE type_t.num5

   LET r_success = TRUE
   
   LET l_cnt = 0
   SELECT COUNT(pmegseq) INTO l_cnt
     FROM pmeg_t
    WHERE pmegent = g_enterprise
      AND pmegdocno = g_pmee_m.pmeedocno
      AND pmeg900 = g_pmee_m.pmee900
      
   IF l_cnt >= 1 THEN
      LET l_pmeh013 = ''
      SELECT MAX(pmeh013) INTO l_pmeh013
        FROM pmeh_t
       WHERE pmehent = g_enterprise
         AND pmehdocno = g_pmee_m.pmeedocno
         AND pmeh900 = g_pmee_m.pmee900         
      
      #單頭採購失效日期 < 單身最大到庫日期
      IF g_pmee_m.pmee205 < l_pmeh013 THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'apm-00933'
         LET g_errparam.extend = ""
         LET g_errparam.popup = TRUE
         LET g_errparam.replace[1] = g_pmee_m.pmee205
         LET g_errparam.replace[2] = l_pmeh013
         CALL cl_err()
         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF
   
   RETURN r_success
END FUNCTION

