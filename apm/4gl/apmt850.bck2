   DEFINE p_pmeedocno         LIKE pmee_t.pmeedocno
   DEFINE p_pmegseq           LIKE pmeg_t.pmegseq
   DEFINE l_sql               STRING
   DEFINE l_seq               LIKE type_t.num5
   TYPE   type_l_pmdp         RECORD
          pmdpdocno           LIKE pmdp_t.pmdpdocno,   #採購單號
          pmdpseq             LIKE pmdp_t.pmdpseq,     #採購項次
          pmdpseq1            LIKE pmdp_t.pmdpseq1,    #項序
          pmdp001             LIKE pmdp_t.pmdp001,     #商品編號
          pmdp001_desc        LIKE imaal_t.imaal003,   #品名
          pmdp001_desc_desc   LIKE imaal_t.imaal004,   #規格
          pmdp002             LIKE pmdp_t.pmdp002,     #產品特徵
          pmdp003             LIKE pmdp_t.pmdp003,     #來源單號
          pmdp004             LIKE pmdp_t.pmdp004,     #來源項次
          pmdp005             LIKE pmdp_t.pmdp005,     #來源項序
          pmdp006             LIKE pmdp_t.pmdp006,     #來源分批序
          pmdp007             LIKE pmdp_t.pmdp007,     #來源商品
          pmdp008             LIKE pmdp_t.pmdp008,     #來源產品特徵
          pmdp009             LIKE pmdp_t.pmdp009,     #來源作業編號
          pmdp010             LIKE pmdp_t.pmdp010,     #製程序
          pmdp011             LIKE pmdp_t.pmdp011,     #來源BOM特性
          pmdp012             LIKE pmdp_t.pmdp012,     #來源生產控制組
          pmdp021             LIKE pmdp_t.pmdp021,     #沖銷順序
          pmdp022             LIKE pmdp_t.pmdp022,     #需求單位
          pmdp022_desc        LIKE oocal_t.oocal003,   #需求單位名稱
          pmdp023             LIKE pmdp_t.pmdp023,     #需求數量
          pmdp024             LIKE pmdp_t.pmdp024,     #折合採購量
          pmdp025             LIKE pmdp_t.pmdp025,     #已收貨量
          pmdp026             LIKE pmdp_t.pmdp026,     #已入庫量
          pmdpsite            LIKE pmdp_t.pmdpsite     #營運據點
                              END RECORD
   DEFINE l_pmdp              DYNAMIC ARRAY OF type_l_pmdp
   DEFINE l_rec_b             LIKE type_t.num5

   OPEN WINDOW w_apmt840_s01 WITH FORM cl_ap_formpath("apm","apmt840_s01")

   CALL l_pmdp.clear()
   LET l_sql = "SELECT pmejdocno,pmejseq, pmejseq1,pmej001,",
               "       imaal003, imaal004,pmej002, pmej003,",
               "       pmej004,  pmej005, pmej006, pmej007,",
               "       pmej008,  pmej009, pmej010, pmej011,",
               "       pmej012,  pmej021, pmej022, oocal003,",
               "       pmej023,  pmej024, pmej025, pmej026,",
               "       pmejsite",
               "  FROM pmej_t",
               "       LEFT OUTER JOIN imaal_t ON imaalent = pmejent AND imaal001 = pmej001 AND imaal002 = '",g_dlang,"'",
               "       LEFT OUTER JOIN oocal_t ON oocalent = pmejent AND oocal001 = pmej022 AND oocal002 = '",g_dlang,"'",
               " WHERE pmejent = ",g_enterprise,
               "   AND pmejdocno = '",p_pmeedocno,"'"
   IF NOT cl_null(p_pmegseq) THEN
      LET l_sql = l_sql,"   AND pmejseq = '",p_pmegseq,"'"
   END IF

   PREPARE apmt850_s01_pre FROM l_sql
   DECLARE apmt850_s01_cs CURSOR FOR apmt850_s01_pre


   LET l_seq = 1
   FOREACH apmt850_s01_cs INTO l_pmdp[l_seq].pmdpdocno,   l_pmdp[l_seq].pmdpseq,          l_pmdp[l_seq].pmdpseq1,l_pmdp[l_seq].pmdp001,
                               l_pmdp[l_seq].pmdp001_desc,l_pmdp[l_seq].pmdp001_desc_desc,l_pmdp[l_seq].pmdp002, l_pmdp[l_seq].pmdp003,
                               l_pmdp[l_seq].pmdp004,     l_pmdp[l_seq].pmdp005,          l_pmdp[l_seq].pmdp006, l_pmdp[l_seq].pmdp007,
                               l_pmdp[l_seq].pmdp008,     l_pmdp[l_seq].pmdp009,          l_pmdp[l_seq].pmdp010, l_pmdp[l_seq].pmdp011,
                               l_pmdp[l_seq].pmdp012,     l_pmdp[l_seq].pmdp021,          l_pmdp[l_seq].pmdp022, l_pmdp[l_seq].pmdp022_desc,
                               l_pmdp[l_seq].pmdp023,     l_pmdp[l_seq].pmdp024,          l_pmdp[l_seq].pmdp025, l_pmdp[l_seq].pmdp026,
                               l_pmdp[l_seq].pmdpsite

       IF l_seq > g_max_rec THEN
          IF g_error_show = 1 THEN
             INITIALIZE g_errparam TO NULL
             LET g_errparam.extend = l_seq
             LET g_errparam.code   = 9035
             LET g_errparam.popup  = TRUE
             CALL cl_err()
          END IF
          EXIT FOREACH
       END IF
       LET l_seq = l_seq + 1
   END FOREACH

   CALL l_pmdp.deleteElement(l_pmdp.getLength())

   DISPLAY ARRAY l_pmdp TO s_detail1.* ATTRIBUTES(COUNT=l_rec_b)

   END DISPLAY
   CLOSE WINDOW w_apmt840_s01
END FUNCTION

################################################################################
# Descriptions...: 取訂單有效期(先取門店再取店群)
# Memo...........: 1.先取門店 = 收貨組織
#                : 2.取不到再取 門店 = 'ALL'
#                : 3.取不到再取 店群
# Usage..........: CALL apmt850_get_rtka010(p_pmeeunit,p_pmee004)
#                  RETURNING r_rtka010
# Input parameter: p_pmeeunit     收貨組織
#                : p_pmee004      供應商
# Return code....: r_rtka010      訂單有效期
# Date & Author..: 2015/05/29 By sakura
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt850_get_rtka010(p_pmeeunit,p_pmee004)
DEFINE p_pmeeunit         LIKE pmee_t.pmeeunit
DEFINE p_pmee004          LIKE pmee_t.pmee004
DEFINE r_rtka010          LIKE rtka_t.rtka010
DEFINE r_rtka012          LIKE rtka_t.rtka012
DEFINE l_rtaa001          LIKE rtaa_t.rtaa001
DEFINE l_sql              STRING

   WHENEVER ERROR CONTINUE
   LET r_rtka010 = NULL
   LET r_rtka012 = NULL
   
   IF cl_null(p_pmeeunit) OR cl_null(p_pmee004) THEN
      RETURN r_rtka010
   END IF
   
   LET l_sql = "SELECT rtka010 FROM rtka_t",
               " WHERE rtkaent = ",g_enterprise,
               "   AND rtka001 = '",p_pmee004,"'",
               "   AND rtka002 = ?",
               "   AND rtka003 = ?",
               "   AND rtkastus = 'Y'"
   PREPARE apmt850_get_rtka010 FROM l_sql
   
   #取門店
   EXECUTE apmt850_get_rtka010 USING '2',p_pmeeunit INTO r_rtka010
   
   IF STATUS = 100 THEN
      #取門店 = 'ALL'
      EXECUTE apmt850_get_rtka010 USING '2','ALL' INTO r_rtka010
      
      IF STATUS = 100 THEN
         #huangrh add rtak_t -----20150603
         LET l_rtaa001 = ''
         SELECT rtaa001 INTO l_rtaa001
           FROM rtak_t,rtaa_t
           LEFT OUTER JOIN rtab_t ON rtaaent = rtabent AND rtaa001 = rtab001
          WHERE rtaaent = g_enterprise
            AND rtakent=rtaaent
            AND rtak001=rtaa001
            AND rtak002='2'
            AND rtak003='Y'
            AND rtaastus = 'Y'
            AND rtab002 = p_pmeeunit
         #取店群
         EXECUTE apmt850_get_rtka010 USING '1',l_rtaa001 INTO r_rtka010
      END IF
   END IF

   RETURN r_rtka010
END FUNCTION

################################################################################
# Descriptions...: 檢查單身到庫日是否有大於單頭採購失效日
# Memo...........:
# Usage..........: CALL apmt850_pmee205_chk(p_pmeedocno,p_pmdnseq,p_pmee205)
#                  RETURNING r_success
# Input parameter: p_pmeedocno    採購單號
#                : p_pmdnseq      採購項次
#                : p_pmee205      採購單頭失效日
# Return code....: r_success      TRUE(FALSE)
# Date & Author..: 2015/05/29 By sakura #150512-00029#3
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt850_pmee205_chk(p_pmeedocno,p_pmdnseq,p_pmee205)
DEFINE p_pmeedocno         LIKE pmee_t.pmeedocno
DEFINE p_pmdnseq           LIKE pmdn_t.pmdnseq
DEFINE p_pmee205           LIKE pmee_t.pmee205
DEFINE l_pmeh013           LIKE pmeh_t.pmeh013
DEFINE r_pmee205           LIKE pmee_t.pmee205
DEFINE r_success           LIKE type_t.num5

  LET r_success = TRUE
  LET r_pmee205 = NULL
  LET l_pmeh013 = ''
  
  IF p_pmdnseq != 0 THEN
     SELECT pmeh013 INTO l_pmeh013 
       FROM pmeh_t
      WHERE pmehent   = g_enterprise
        AND pmehdocno = p_pmeedocno
        AND pmehseq   = p_pmdnseq
     IF l_pmeh013 > p_pmee205 THEN
        LET r_pmee205 = l_pmeh013
        LET r_success = FALSE
     ELSE
        LET r_pmee205 = p_pmee205
     END IF
  ELSE
     SELECT MAX(pmeh013) INTO l_pmeh013
       FROM pmeh_t
      WHERE pmehent   = g_enterprise
        AND pmehdocno = p_pmeedocno
     IF l_pmeh013 > p_pmee205 THEN
        LET r_pmee205 = l_pmeh013
        LET r_success = FALSE
     ELSE
        LET r_pmee205 = p_pmee205
     END IF                
  END IF
  
  RETURN r_success,r_pmee205
END FUNCTION

