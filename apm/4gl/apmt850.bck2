    SELECT pmab037,pmab053,pmab034,
           pmab033,pmab040,
           pmab038,pmab039,pmab056,
           pmab057
      INTO g_pmee_m.pmee009,g_pmee_m.pmee010,g_pmee_m.pmee011,
