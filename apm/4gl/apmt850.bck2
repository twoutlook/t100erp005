PRIVATE FUNCTION apmt850_get_pmeg228(p_pmeg001)
   DEFINE p_pmeg001        LIKE pmeg_t.pmeg001
   DEFINE r_pmeg228        LIKE pmeg_t.pmeg228
   DEFINE r_pmeg228_desc   LIKE rtaxl_t.rtaxl003
   
   LET r_pmeg228 = ''
   LET r_pmeg228_desc = ''
   
   SELECT imaa009,rtaxl003 
     INTO r_pmeg228,r_pmeg228_desc
     FROM imaa_t
          LEFT OUTER JOIN rtaxl_t ON rtaxlent = g_enterprise AND rtaxl001 = imaa009 AND rtaxl002 = g_dlang
