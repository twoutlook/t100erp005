# Usage..........: CALL apmt850_pmeedocno_default()
#                  RETUNRING r_success
# Return code....: r_success
# Date & Author..: 2014/12/05 By Lori
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt850_pmeedocno_default()
   DEFINE r_success   LIKE type_t.num5
   DEFINE l_success   LIKE type_t.num5
   
   LET r_success = TRUE
   
   #依據輸入的單號，帶出單頭欄位的值 
   #採購變更單頭檔
   LET l_success = ''
   CALL apmt850_pmeedocno_pmee() RETURNING l_success
   #依據輸入的合約單號，帶出單身欄位的值
   #採購變更多帳期預付款檔
   IF l_success THEN
      LET l_success = ''
      CALL apmt850_pmeedocno_pmef() RETURNING l_success
   ELSE
      LET r_success = FALSE
      RETURN r_success    
   END IF
   #採購變更單身明細檔
   IF l_success THEN
      LET l_success = ''
      CALL apmt850_pmeedocno_pmeg() RETURNING l_success
   END IF
   #採購變更交期明細檔
   IF l_success THEN
      LET l_success = ''
      CALL apmt850_pmeedocno_pmeh() RETURNING l_success
   END IF
   #採購變更多交期匯總檔
   IF l_success THEN
      LET l_success = ''
      CALL apmt850_pmeedocno_pmei() RETURNING l_success
   END IF
   #採購變更多交期匯總檔
   IF l_success THEN
      LET l_success = ''
      CALL apmt850_pmeedocno_pmej() RETURNING l_success
   END IF
   
   #160130-00006#2 160330 by lori add---(S)
   #交易稅明細檔
   IF l_success THEN
      LET l_success = ''
      CALL apmt850_pmeedocno_xrcd() RETURNING l_success
   END IF
   #160130-00006#2 160330 by lori add---(E)
   
   IF l_success THEN
      CALL apmt850_pmee_ref()
      CALL apmt850_b_fill() 
   ELSE
      LET g_del_question = FALSE
      CALL apmt850_pmee_ref()
      LET g_pmee_m.* = g_pmee_m_o.*
      CALL apmt850_delete()
   END IF 

   LET r_success = l_success
   
   RETURN r_success
END FUNCTION

