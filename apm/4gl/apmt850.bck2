# Modify.........:
################################################################################
PRIVATE FUNCTION apmt850_get_addr_org(p_type)
   DEFINE p_type   LIKE type_t.num5
   
   IF p_type = 1 THEN
      LET g_pmee025_org = ''
      LET g_pmee025_connid = ''
      
      IF g_pmee_m.pmee203 = '3' THEN     
         CALL s_apmt840_get_ooef012(g_pmee_m.pmee204) RETURNING g_pmee025_connid
         LET g_pmee025_org = g_pmee_m.pmee204
      ELSE
