      LET g_chkparam.err_str[1] = "aim-00066:art-00530"
      LET g_chkparam.err_str[2] = "aim-00101:art-00126"
      IF cl_chk_exist("v_imaf001_15") THEN
         INITIALIZE g_chkparam.* TO NULL
         LET g_chkparam.arg1 = p_pmeg001
         LET g_chkparam.arg2 = g_pmeg_d[l_ac].pmegsite
         IF NOT cl_chk_exist("v_rtdx001") THEN
            LET r_success = FALSE
            RETURN r_success
         END IF
         
         #160130-00006#1 160131 by lori add---(S)
         IF NOT cl_null(g_pmee_m.pmee207) THEN
            #商品編號存在檢查與管理品類檢查
            INITIALIZE g_chkparam.* TO NULL
            LET g_chkparam.arg1 = p_pmeg001
            LET g_chkparam.arg2 = cl_get_para(g_enterprise,'',g_para_cir0001) 
            LET g_chkparam.arg3 = g_pmee_m.pmee207
            IF NOT cl_chk_exist("v_imaa001_10") THEN   
               LET r_success = FALSE
               RETURN r_success
            END IF   
         END IF
         #160130-00006#1 160131 by lori add---(E)
         
         LET l_success = ''
         LET l_errno   = ''
         CALL s_apmt840_chk_pact_curr(g_pmeg_d[l_ac].pmeg222,g_pmee_m.pmee004,g_pmee_m.pmeedocdt,g_pmeg_d[l_ac].pmeg001,g_pmee_m.pmee015,g_pmeg_d[l_ac].pmegsite)
            RETURNING l_success,l_errno
         IF l_success = FALSE THEN
            LET r_success = FALSE
            INITIALIZE g_errparam TO NULL
            LET g_errparam.extend = ""
            LET g_errparam.code   = l_errno
            LET g_errparam.popup  = TRUE
            CALL cl_err()
            
            LET r_success = FALSE
            RETURN r_success            
         END IF
         
         #160130-00006#1 160131 by lori add---(S)
         #商品生命周期的判断
         IF NOT s_life_cycle_chk('apmt840',g_pmee_m.pmeesite,'1',g_pmeg_d[l_ac].pmeg001,g_pmee_m.pmee004,g_pmee_m.pmeedocdt) THEN  
            LET r_success = FALSE
            RETURN r_success
         END IF
         #160130-00006#1 160131 by lori add---(E)
      ELSE
         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF
   
   RETURN r_success   
   
END FUNCTION

