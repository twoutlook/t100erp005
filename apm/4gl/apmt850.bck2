PRIVATE FUNCTION apmt850_pmei004_chk(p_pmei003,p_pmei004)
DEFINE p_pmei003    LIKE pmei_t.pmei003
DEFINE p_pmei004    LIKE pmei_t.pmei004
DEFINE r_success    LIKE type_t.num5
DEFINE l_pmdb049    LIKE pmdb_t.pmdb049
DEFINE l_pmdb006    LIKE pmdb_t.pmdb006
DEFINE l_pmdb014    LIKE pmdb_t.pmdb014
DEFINE l_pmdb015    LIKE pmdb_t.pmdb015

       LET r_success = TRUE
       LET l_pmdb049 = ''
       LET l_pmdb006 = ''
       LET l_pmdb014 = ''
       LET l_pmdb015 = ''

       CASE g_pmee_m.pmee007
          WHEN '2'  #請購單
             SELECT COALESCE(pmdb049,0),COALESCE(pmdb006,0),pmdb014,pmdb015
                INTO l_pmdb049,l_pmdb006,l_pmdb014,l_pmdb015
               FROM pmdb_t
               WHERE pmdbent = g_enterprise 
                 AND pmdbdocno = p_pmei003 
                 AND pmdbseq = p_pmei004
             CASE WHEN SQLCA.sqlcode = 100
                       INITIALIZE g_errparam TO NULL
                       LET g_errparam.code = 'apm-01074'
                       LET g_errparam.extend = ''
                       LET g_errparam.replace[1] = p_pmei004
                       LET g_errparam.replace[2] = p_pmei003
                       LET g_errparam.popup = TRUE
                       CALL cl_err()

                       LET r_success = FALSE
                       RETURN r_success
                  WHEN l_pmdb049 < l_pmdb006
                       INITIALIZE g_errparam TO NULL
                       LET g_errparam.code = 'apm-01075'
                       LET g_errparam.extend = ''
                       LET g_errparam.replace[1] = p_pmei003
                       LET g_errparam.replace[2] = p_pmei004
                       LET g_errparam.popup = TRUE
                       CALL cl_err()

                       LET r_success = FALSE
                       RETURN r_success
             END CASE
             IF l_pmdb014 = '3' AND l_pmdb015 <> g_pmee_m.pmee004 THEN
                INITIALIZE g_errparam TO NULL
