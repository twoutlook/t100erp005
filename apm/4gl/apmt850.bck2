      #其說明的SQL
      LET g_pmek007 = "SELECT ooibl004 FROM ooibl_t WHERE ooiblent='"||g_enterprise||"' AND ooibl002='"||l_pmdm.pmdm002||"' AND ooibl003=?"
      #新增資料到變更歷程檔
      IF NOT s_apmt850_ins_pmek(p_pmeg6_d.pmef001,0,0,"pmdm002",p_pmeg6_d.pmef901,l_pmdm.pmdm002,p_pmeg6_d.pmef002,g_pmee_m.pmeedocno,g_pmee_m.pmee900,g_pmek007) THEN      
