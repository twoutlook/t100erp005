################################################################################
# Descriptions...: 若採購數量=0且為多交期，則自動將多交期匯總檔的分批數量變更為0
# Memo...........:
# Usage..........: CALL apmt850_upd_pmej_amount()
#                  RETURNING r_success
# Input parameter: 
# Return code....: r_success      TRUE/FALSE
# Date & Author..: 2014/10/27 By stellar
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt850_upd_pmej_amount()
DEFINE r_success         LIKE type_t.num5
DEFINE l_pmej001         LIKE pmej_t.pmej001
DEFINE l_pmej901         LIKE pmej_t.pmej901
DEFINE l_pmdq002         LIKE pmdq_t.pmdq002

   LET r_success = TRUE
   
   IF g_pmeg_d[l_ac].pmeg007 <> 0 THEN
      RETURN r_success
   END IF
   
   IF g_pmeg_d[l_ac].pmeg024 = 'N' THEN
      RETURN r_success
   END IF
   
   DECLARE apmt850_upd_pmej_amount_cs CURSOR FOR
    SELECT pmej001,pmej901 FROM pmej_t
     WHERE pmejent = g_enterprise
       AND pmejdocno = g_pmee_m.pmeedocno
       AND pmej900 = g_pmee_m.pmee900
       AND pmejseq = g_pmeg_d[l_ac].pmegseq
     ORDER BY pmej001
   FOREACH apmt850_upd_pmej_amount_cs INTO l_pmej001,l_pmej901
      
      #單身非新增，則抓取採購匯總檔的資料，判斷數量是否有變更
      IF l_pmej901 = '1' OR l_pmej901 = '2' THEN
         SELECT pmdq002 INTO l_pmdq002
           FROM pmdq_t
          WHERE pmdqent = g_enterprise
            AND pmdqdocno = g_pmee_m.pmeedocno
            AND pmdqseq = g_pmeg_d[l_ac].pmegseq
            AND pmdqseq2= l_pmej001
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = 'sel pmdq_t'
            LET g_errparam.popup = TRUE
            CALL cl_err()
  
            LET r_success = FALSE
            EXIT FOREACH
         END IF
         
         IF l_pmdq002 <> 0 THEN
            LET l_pmej901 = '2'
         END IF
      END IF
      
      #更新數量及變更類型
      UPDATE pmej_t SET pmej002 = 0,
                        pmej901 = l_pmej901
       WHERE pmejent = g_enterprise
         AND pmejdocno = g_pmee_m.pmeedocno
         AND pmej900 = g_pmee_m.pmee900
         AND pmejseq = g_pmeg_d[l_ac].pmegseq
         AND pmej001 = l_pmej001
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = 'upd pmej_t'
         LET g_errparam.popup = TRUE
         CALL cl_err()
   
         LET r_success = FALSE
         EXIT FOREACH
      END IF
   
      #更新歷程檔
      IF l_pmej901 = '3' OR (l_pmej901 = '2' AND 
         ((l_pmdq002 <> 0) OR (l_pmdq002 IS NULL))) THEN
         #其說明的SQL
         LET g_pmek007 = NULL
         #新增資料到變更歷程檔
         IF NOT s_apmt850_ins_pmek(g_pmeg_d[l_ac].pmegseq,0,l_pmej001,"pmdq002",l_pmej901,l_pmdq002,0,g_pmee_m.pmeedocno,g_pmee_m.pmee900,g_pmek007) THEN
            LET r_success = FALSE
            EXIT FOREACH
         END IF
      ELSE
         #資料一樣，表示無變更，將變更歷程檔刪除
         IF NOT s_apmt850_del_pmek(g_pmeg_d[l_ac].pmegseq,0,l_pmej001,"pmdq002",g_pmee_m.pmeedocno,g_pmee_m.pmee900) THEN
            LET r_success = FALSE
            EXIT FOREACH
         END IF
      END IF
         
   END FOREACH
   
   RETURN r_success
END FUNCTION

