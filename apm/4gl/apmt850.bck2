################################################################################
PRIVATE FUNCTION apmt850_pmef_ins_pmek(p_pmeg6_d)
DEFINE p_pmeg6_d        type_g_pmeg6_d
DEFINE r_success        LIKE type_t.num5
DEFINE l_pmdm           RECORD LIKE pmdm_t.*
DEFINE l_flag           LIKE type_t.num5   #此筆單身是否有修改

   LET r_success = 'Y'

   IF p_pmeg6_d.pmef901 = '1' THEN
      RETURN r_success
   END IF   
   
   INITIALIZE l_pmdm.* TO NULL
   LET l_flag = FALSE   

　 #pmef901 變更類型：1.單頭修改 2.單身修改 3.單身新增 4.單身刪除
   IF p_pmeg6_d.pmef901 = '2' THEN
      SELECT pmdm001,pmdm002,pmdm003,pmdm004,pmdm005,
             pmdm006,pmdm007,pmdm008,pmdm009,pmdm014
        INTO l_pmdm.pmdm001,l_pmdm.pmdm002,l_pmdm.pmdm003,l_pmdm.pmdm004,l_pmdm.pmdm005,
             l_pmdm.pmdm006,l_pmdm.pmdm007,l_pmdm.pmdm008,l_pmdm.pmdm009,l_pmdm.pmdm014
        FROM pmdm_t
       WHERE pmdment = g_enterprise
         AND pmdmdocno = g_pmee_m.pmeedocno
         AND pmdm001 = p_pmeg6_d.pmef001
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = 'sel pmdm_t'
         LET g_errparam.popup = TRUE
         CALL cl_err()

         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF
   
   #期別
   IF p_pmeg6_d.pmef901 = '3' OR (p_pmeg6_d.pmef901 = '2' AND
      ((p_pmeg6_d.pmef001 <> l_pmdm.pmdm001) OR
      (p_pmeg6_d.pmef001 IS NULL AND l_pmdm.pmdm001 IS NOT NULL) OR
      (p_pmeg6_d.pmef001 IS NOT NULL AND l_pmdm.pmdm001 IS NULL))) THEN
      #其說明的SQL
      LET g_pmek007 = ''
      #新增資料到變更歷程檔
      IF NOT s_apmt850_ins_pmek(p_pmeg6_d.pmef001,0,0,"pmdm001",p_pmeg6_d.pmef901,l_pmdm.pmdm001,p_pmeg6_d.pmef001,g_pmee_m.pmeedocno,g_pmee_m.pmee900,g_pmek007) THEN      
         LET r_success = FALSE
         RETURN r_success
      END IF
      LET l_flag = TRUE
   ELSE
      #資料一樣，表示無變更，將變更歷程檔刪除
      IF NOT s_apmt850_del_pmek(p_pmeg6_d.pmef001,0,0,"pmdm001",g_pmee_m.pmeedocno,g_pmee_m.pmee900) THEN
         LET r_success = FALSE
