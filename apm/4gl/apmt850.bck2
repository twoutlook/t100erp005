DEFINE p_pmeg001         LIKE pmeg_t.pmeg001    #商品編號
DEFINE p_pmeg002         LIKE pmeg_t.pmeg002    #產品特徵
DEFINE r_pmeg206         LIKE pmeg_t.pmeg206    #可用庫存量
DEFINE r_pmeg207         LIKE pmeg_t.pmeg207    #採購在途量

   LET r_pmeg206 = 0
   LET r_pmeg207 = 0
   
   #可用庫存量
   CALL s_apmt840_sum_inag008(p_pmegunit,p_pmeg001,p_pmeg002) #150401-00005#3 sakura add pmeg002
      RETURNING r_pmeg206

