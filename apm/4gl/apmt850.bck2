################################################################################
PRIVATE FUNCTION apmt850_pmeh015_chk(p_pmeedocno,p_pmee900,p_pmegseq)
DEFINE p_pmeedocno      LIKE pmee_t.pmeedocno
DEFINE p_pmee900        LIKE pmee_t.pmee900
DEFINE p_pmegseq        LIKE pmeg_t.pmegseq
DEFINE l_cnt            LIKE type_t.num5
DEFINE r_success        LIKE type_t.num5

   LET r_success = TRUE
   LET l_cnt = 0
   
   SELECT COUNT(*) INTO l_cnt
     FROM pmeh_t
    WHERE pmehent = g_enterprise
      AND pmehdocno = p_pmeedocno
      AND pmeh900 = p_pmee900
      AND pmehseq = p_pmegseq
      AND pmeh015 > 0
      
   IF l_cnt > 0 THEN
      LET r_success = FALSE
   END IF
   
   RETURN r_success
END FUNCTION

 
