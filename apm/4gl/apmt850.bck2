#單頭稅別修改時，單身稅別一併修改
PRIVATE FUNCTION apmt850_change_pmeg016()
DEFINE p_cmd             LIKE type_t.chr1
DEFINE l_i               LIKE type_t.num10
DEFINE l_success         LIKE type_t.num5
DEFINE l_pmegseq         LIKE pmeg_t.pmegseq
DEFINE l_pmeg001         LIKE pmeg_t.pmeg001
DEFINE l_pmeg011         LIKE pmeg_t.pmeg011
DEFINE l_pmeg015         LIKE pmeg_t.pmeg015
DEFINE l_pmeg016         LIKE pmeg_t.pmeg016
DEFINE l_pmeg017         LIKE pmeg_t.pmeg017
DEFINE l_pmeg046         LIKE pmeg_t.pmeg046
DEFINE l_pmeg047         LIKE pmeg_t.pmeg047
DEFINE l_pmeg048         LIKE pmeg_t.pmeg048
DEFINE l_pmeg901         LIKE pmeg_t.pmeg901
DEFINE l_pmeg007         LIKE pmeg_t.pmeg007
DEFINE l_pmeg_d          type_g_pmeg_d

      DECLARE sel_pmeg_cs CURSOR FOR
       SELECT pmegseq,pmeg001,pmeg011,pmeg015,pmeg016,pmeg901,pmeg007,pmeg901
         FROM pmeg_t
        WHERE pmegent = g_enterprise
          AND pmegdocno = g_pmee_m.pmeedocno
          AND pmeg900 = g_pmee_m.pmee900

      FOREACH sel_pmeg_cs INTO l_pmegseq,l_pmeg001,l_pmeg011,l_pmeg015,l_pmeg016,l_pmeg901,l_pmeg007,l_pmeg901
         IF g_tax_app = '2' THEN
            #供應商慣用採購分類       
            IF cl_null(g_pmab039) THEN
               SELECT pmab039 INTO g_pmab039 FROM pmab_t
                WHERE pmabent = g_enterprise
                  AND pmabsite= g_pmee_m.pmeesite
                  AND pmab001 = g_pmee_m.pmee004            
            END IF
            #依料件設定
            CALL s_tax_chktype(g_pmee_m.pmeesite,g_pmee_m.pmee004,l_pmeg001,'2',g_pmab039)
                 RETURNING l_success,l_pmeg016,l_pmeg017
                 
            IF apmt850_pmee011_chk('2',l_pmeg016) THEN   #稅別檢查

            ELSE
               #稅別檢查失敗，將稅別、稅率清空
               LET l_pmeg016 = ''
               LET l_pmeg017 = ''
            END IF
         ELSE
            #依正常稅率
            LET l_pmeg016 = g_pmee_m.pmee011
            LET l_pmeg017 = g_pmee_m.pmee012
         END IF

         #重計未稅金額、含稅金額、稅額
         IF l_pmeg015 = 0 OR cl_null(l_pmeg016) THEN
            LET l_pmeg046 = 0
            LET l_pmeg047 = 0
            LET l_pmeg048 = 0
         ELSE
            #150504-00019#1 150506 by lori522612 mod---(S) 
            CALL s_apmt840_get_amount(g_pmee_m.pmeedocno,l_pmegseq,l_pmeg011,l_pmeg015,
                                      l_pmeg016,g_pmee_m.pmee015,g_pmee_m.pmee016)
               RETURNING l_pmeg046,l_pmeg048,l_pmeg047
            #150504-00019#1 150506 by lori522612 mod---(E)    
         END IF
         
         IF l_pmeg901 = '1' THEN
            LET l_pmeg901 = '2'
         END IF
         
         UPDATE pmeg_t SET pmeg016 = l_pmeg016,
                           pmeg017 = l_pmeg017,
                           pmeg046 = l_pmeg046,
                           pmeg047 = l_pmeg047,
                           pmeg048 = l_pmeg048,
                           pmeg901 = l_pmeg901
          WHERE pmegent = g_enterprise
            AND pmegdocno = g_pmee_m.pmeedocno
            AND pmegseq = l_pmegseq 
            AND pmeg900 = g_pmee_m.pmee900
            
         #增加歷程檔
         SELECT pmegseq,pmeg001,pmeg002,pmeg006,pmeg007,pmeg008,pmeg009,pmeg024,
                pmeg012,pmeg013,pmeg014,pmeg045,pmeg010,pmeg011,pmeg015,pmeg016,pmeg017,pmeg046,
                pmeg047,pmeg048,pmeg023,pmeg019,pmeg020,pmeg052,pmeg021,pmeg022,pmegunit,pmegorga,
                pmeg049,pmeg051,pmeg050,pmeg901,pmeg902,pmeg903,pmegsite,
                pmeg028,pmeg029,pmeg030,pmeg053,pmeg025,pmeg026,pmeg043
           INTO l_pmeg_d.pmegseq,l_pmeg_d.pmeg001,l_pmeg_d.pmeg002,
                l_pmeg_d.pmeg006,l_pmeg_d.pmeg007,l_pmeg_d.pmeg008,l_pmeg_d.pmeg009,l_pmeg_d.pmeg024,
                l_pmeg_d.pmeg012,l_pmeg_d.pmeg013,l_pmeg_d.pmeg014,l_pmeg_d.pmeg045,l_pmeg_d.pmeg010,
                l_pmeg_d.pmeg011,l_pmeg_d.pmeg015,l_pmeg_d.pmeg016,l_pmeg_d.pmeg017,l_pmeg_d.pmeg046,
                l_pmeg_d.pmeg047,l_pmeg_d.pmeg048,l_pmeg_d.pmeg023,l_pmeg_d.pmeg019,l_pmeg_d.pmeg020,
                l_pmeg_d.pmeg052,l_pmeg_d.pmeg021,l_pmeg_d.pmeg022,l_pmeg_d.pmegunit,l_pmeg_d.pmegorga,
                l_pmeg_d.pmeg049,l_pmeg_d.pmeg051,l_pmeg_d.pmeg050,l_pmeg_d.pmeg901,l_pmeg_d.pmeg902,
                l_pmeg_d.pmeg903,l_pmeg_d.pmegsite,
                l_pmeg_d.pmeg028,l_pmeg_d.pmeg029,l_pmeg_d.pmeg030,l_pmeg_d.pmeg053,
                l_pmeg_d.pmeg025,l_pmeg_d.pmeg026,l_pmeg_d.pmeg043
           FROM pmeg_t
          WHERE pmegent = g_enterprise
            AND pmegdocno = g_pmee_m.pmeedocno
            AND pmeg900 = g_pmee_m.pmee900
            AND pmegseq = l_pmegseq
            
         LET l_pmeg_d.pmeg016 = l_pmeg016
         LET l_pmeg_d.pmeg017 = l_pmeg017
         LET l_pmeg_d.pmeg046 = l_pmeg046
         LET l_pmeg_d.pmeg047 = l_pmeg047
         LET l_pmeg_d.pmeg048 = l_pmeg048
         LET l_pmeg_d.pmeg901 = l_pmeg901
         
         CALL apmt850_pmeg_ins_pmek(l_pmeg_d.*) RETURNING l_success
            
         #需呼叫"交期明細資料"的Function重新產生交期明細資料
         CALL s_apmt850_gen_pmeh(g_pmee_m.pmeedocno,l_pmegseq,g_pmee_m.pmee900,l_pmeg901)
              RETURNING l_success
              
      END FOREACH
      CALL apmt850_b_fill()
          
END FUNCTION

