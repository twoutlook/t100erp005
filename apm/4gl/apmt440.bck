      RETURN
   END IF
   CALL s_transaction_begin()
   OPEN apmt440_cl USING g_enterprise,g_pmdi_m.pmdidocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = g_pmdi_m.pmdidocno
