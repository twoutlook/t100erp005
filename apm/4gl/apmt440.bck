       g_pmdi_m.pmdicnfdt,g_pmdi_m.pmdi002_desc,g_pmdi_m.pmdi004_desc,g_pmdi_m.pmdi003_desc,g_pmdi_m.pmdi005_desc, 
       g_pmdi_m.pmdi009_desc,g_pmdi_m.pmdi011_desc,g_pmdi_m.pmdi019_desc,g_pmdi_m.pmdiownid_desc,g_pmdi_m.pmdiowndp_desc, 
       g_pmdi_m.pmdicrtid_desc,g_pmdi_m.pmdicrtdp_desc,g_pmdi_m.pmdimodid_desc,g_pmdi_m.pmdicnfid_desc 
 
   
   #遮罩相關處理
