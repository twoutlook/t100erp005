################################################################################
# Descriptions...: 系列或產品分類的單位校驗
# Memo...........:
# Usage..........: CALL axmt420_pmdj031_pmdj032_chk_unit()
#                  RETURNING TRUE/FALSE
# Input parameter: 回傳 TRUE/FALSE
# Return code....: 
# Date & Author..: 2015-06-16 By dorislai
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt440_pmdj031_pmdj032_chk_unit()
   DEFINE  l_sql                 STRING                               #抓取sql語法
   DEFINE  l_chk                 LIKE  type_t.num5                    #判斷是否有抓到符合的值
   DEFINE  l_imaa001             LIKE  type_t.chr100                  #接收抓取到的料件編號值
   
   #先依欄位抓取sql語法
      #---用系列去抓料件編號
   IF NOT cl_null(g_pmdj_d[l_ac].pmdj031) THEN
      LET l_sql = "SELECT imaa001 FROM imaa_t ",
                  "WHERE imaaent = '"||g_enterprise||"' AND imaa127 = '"||g_pmdj_d[l_ac].pmdj031||"'",
                  "ORDER BY imaa001"
   END IF
      #----用產品分類去抓料件編號
   IF NOT cl_null(g_pmdj_d[l_ac].pmdj032) THEN
      LET l_sql = "SELECT imaa001 FROM imaa_t ",
                  "WHERE imaaent = '"||g_enterprise||"' AND imaa009 = '"||g_pmdj_d[l_ac].pmdj032||"'",
                  "ORDER BY imaa001"
   END IF
                  
   PREPARE apmt440_get_imaa001_chk_pre FROM l_sql
   DECLARE apmt440_get_imaa001_chk_curs CURSOR FOR apmt440_get_imaa001_chk_pre
   LET l_imaa001 =''
   LET l_chk = FALSE   #判斷是否有抓到符合的資料
   #一筆一筆比對是否有符合任一 料件編號中的可使用單位
   FOREACH apmt440_get_imaa001_chk_curs INTO l_imaa001 
      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_pmdj_d[l_ac].pmdj008
      #以COUNT來計算，並判斷是否有比對到符合的資料
      CALL ap_ref_array2(g_ref_fields,"SELECT COUNT(*) FROM imao_t WHERE imaoent='"||g_enterprise||"' AND imao002 = ? AND imao001 = '"||l_imaa001||"'","") RETURNING g_rtn_fields
      IF g_rtn_fields[1] > 0 THEN
         LET l_chk = TRUE 
         EXIT FOREACH
      END IF 
   END FOREACH
   FREE apmt440_get_imaa001_chk_pre  #已使用完畢，釋放
   #若料件編號中無相對應的可使用單位，跳出錯誤訊息提示
   IF NOT l_chk THEN
      RETURN FALSE
   END IF    
   RETURN TRUE
END FUNCTION

################################################################################
# Descriptions...: 開窗選擇是否自動產生單身
# Memo...........:
# Usage..........: CALL apmt440_gen_b(p_type)
#                  RETURNING r_success
# Input parameter: p_type       1.選擇是否自動產生單身 2.由詢價單帶出資料
# Return code....: r_success    TRUE/FALSE
# Date & Author..: 20151023 By stellar
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt440_gen_b(p_type)
DEFINE p_type            LIKE type_t.chr1
DEFINE r_success         LIKE type_t.num5
DEFINE l_choice          LIKE type_t.chr1
DEFINE l_wc              STRING
DEFINE l_success         LIKE type_t.num5

   LET r_success = TRUE
   
   IF cl_null(g_pmdi_m.pmdidocno) THEN
      LET r_success = FALSE
      RETURN r_success
   END IF
   
   #檢查是否在事務中
   IF NOT s_transaction_chk('Y',1) THEN
      LET r_success = FALSE
      RETURN r_success
   END IF

   #開窗輸入
   OPEN WINDOW w_apmt440_s01 WITH FORM cl_ap_formpath("apm",'apmt440_s01')
   
   #瀏覽頁簽資料初始化
   CALL cl_ui_init()
   
   CALL cl_set_comp_visible("GridT_quantity",FALSE)
   
   CASE p_type
      WHEN '1'
           LET l_choice = '1'
           CALL cl_set_comp_visible("group_01",FALSE)
      WHEN '2'
           LET l_choice = '2'
           CALL cl_set_comp_visible("grid_1",FALSE)
   END CASE
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
      INPUT l_choice FROM FORMONLY.l_choice
         ATTRIBUTE(WITHOUT DEFAULTS)
       
         BEFORE INPUT
         
         ON CHANGE l_choice
            IF l_choice = '1' THEN
               CALL cl_set_comp_visible("group_01",FALSE)
            ELSE
               CALL cl_set_comp_visible("group_01",TRUE)
            END IF
      
      END INPUT
            
      CONSTRUCT l_wc ON pmdfdocno,pmdfdocdt
           FROM FORMONLY.l_pmdfdocno,FORMONLY.l_pmdfdocdt
           
         ON ACTION controlp INFIELD l_pmdfdocno  
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c'
            LET g_qryparam.reqry = FALSE
            LET g_qryparam.where = " pmdf001 = '",g_pmdi_m.pmdi001,"' ",   #委外否
                               " AND pmdf005 = '",g_pmdi_m.pmdi005,"' ",   #幣別
                               " AND pmdf006 = '",g_pmdi_m.pmdi006,"' ",   #稅別
                               " AND (pmdf009 = '",g_pmdi_m.pmdi009,"' OR pmdf009 IS NULL)",   #付款條件
                               " AND (pmdf010 = '",g_pmdi_m.pmdi011,"' OR pmdf010 IS NULL)",   #交易條件
                               #供應商
                               " AND EXISTS(SELECT pmdgdocno FROM pmdg_t ",
                               "             WHERE pmdgent = pmdfent ",
                               "               AND pmdgdocno = pmdfdocno ",
                               "               AND pmdg002 = '",g_pmdi_m.pmdi004,"') ",
                               " AND pmdfstus = 'Y' "   #確認
            CALL q_pmdfdocno()                      
            DISPLAY g_qryparam.return1 TO FORMONLY.l_pmdfdocno  
            NEXT FIELD l_pmdfdocno  
      END CONSTRUCT
      
      BEFORE DIALOG
         IF p_type = '2' THEN
            NEXT FIELD l_pmdfdocno
         END IF
       
      ON ACTION accept
         ACCEPT DIALOG
         
      ON ACTION cancel
         LET INT_FLAG = TRUE
         EXIT DIALOG
         
      ON ACTION exit
         LET INT_FLAG = TRUE
         EXIT DIALOG
         
      ON ACTION close
         LET INT_FLAG = TRUE
         EXIT DIALOG
   END DIALOG
   
   IF INT_FLAG THEN
      LET INT_FLAG = FALSE
      CLOSE WINDOW w_apmt440_s01
      LET r_success = FALSE
      RETURN r_success
   END IF
   
   #1.自行輸入
   IF l_choice = '1' THEN
      CLOSE WINDOW w_apmt440_s01
      LET r_success = FALSE
      RETURN r_success
   END IF
   
   IF l_choice = '2' THEN
      CALL apmt440_from_apmt420(l_wc)
           RETURNING l_success
      IF NOT l_success THEN
         CLOSE WINDOW w_apmt440_s01
         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF
   
   CLOSE WINDOW w_apmt440_s01
   RETURN r_success
   
END FUNCTION

