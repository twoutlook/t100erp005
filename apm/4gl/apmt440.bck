      LET g_errparam.extend = ""
      LET g_errparam.popup = FALSE
      CALL cl_err()
      RETURN
   END IF
   
   LET l_sql = " SELECT pmdi001,pmdisite,pmdidocno,pmdidocdt,pmdi002,pmdi004,pmdi003, 
                        pmdistus,pmdi005,pmdi006,pmdi007,pmdi008,pmdi009,pmdi011,pmdi019,pmdi030,pmdi013, 
                        pmdi014,pmdi010,pmdi012,pmdi031,pmdi032,pmdi020,pmdi015,pmdi016,pmdi017,pmdi018,pmdi033,pmdi034, 
                        pmdiownid,pmdiowndp,pmdicrtid,pmdicrtdp,pmdicrtdt,pmdimodid,pmdimoddt,pmdicnfid,pmdicnfdt", 
               " FROM pmdi_t",
               " WHERE pmdient= ? AND pmdidocno=? FOR UPDATE"
   LET l_sql = cl_sql_forupd(l_sql)                #轉換不同資料庫語法
   LET l_sql = cl_sql_add_mask(l_sql)              #遮蔽特定資料
   DECLARE apmt440_ooff_cl CURSOR FROM l_sql
   
   CALL s_transaction_begin()
   
   OPEN apmt440_ooff_cl USING g_enterprise,g_pmdi_m.pmdidocno

   IF STATUS THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code =  STATUS
      LET g_errparam.extend = "OPEN apmt440_ooff_cl:"
      LET g_errparam.popup = TRUE
      CALL cl_err()
      CLOSE apmt440_ooff_cl
      CALL s_transaction_end('N','0')
      RETURN
