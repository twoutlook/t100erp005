                              g_pmdi_m.pmdicrtid,g_pmdi_m.pmdicrtdp,g_pmdi_m.pmdicrtdt, 
                              g_pmdi_m.pmdimodid,g_pmdi_m.pmdimoddt,g_pmdi_m.pmdicnfid,g_pmdi_m.pmdicnfdt
                              
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = g_pmdi_m.pmdidocno 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      CLOSE apmt440_ooff_cl
      CALL s_transaction_end('N','0')
      RETURN 
   END IF
 
   #檢查是否允許此動作
   IF NOT apmt440_action_chk() THEN
      CLOSE apmt440_ooff_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
