# Descriptions...: 修改失效日期
# Memo...........:
# Usage..........: CALL apmt440_change_pmdi016()
#                  RETURNING r_success
# Return code....: r_success:TRUE/FALSE
# Date & Author..: 2016/04/27 By ming
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt440_change_pmdi016()
   DEFINE r_success     LIKE type_t.num5

   LET r_success = TRUE

   OPEN apmt440_cl USING g_enterprise,g_pmdi_m.pmdidocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.extend = 'OPEN apmt440_cl:'
      LET g_errparam.code   = STATUS
      LET g_errparam.popup  = TRUE
      CALL cl_err()

      CLOSE apmt440_cl
      LET r_success = FALSE
      RETURN r_success
   END IF

   CALL apmt440_show()
   LET g_pmdidocno_t = g_pmdi_m.pmdidocno

   INPUT BY NAME g_pmdi_m.pmdi016 ATTRIBUTE(WITHOUT DEFAULTS)
      BEFORE INPUT
         LET g_pmdi_m_t.* = g_pmdi_m.*
         LET g_pmdi_m_o.* = g_pmdi_m.*

      AFTER FIELD pmdi016
         IF NOT cl_null(g_pmdi_m.pmdi016) AND (g_pmdi_m_o.pmdi016 IS NULL OR
                                               g_pmdi_m_o.pmdi016 != g_pmdi_m.pmdi016) THEN
            IF g_pmdi_m.pmdi015 > g_pmdi_m.pmdi016 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = g_pmdi_m.pmdi016
               LET g_errparam.code   = 'ais-00048'
               LET g_errparam.popup  = TRUE
               CALL cl_err()

               LET g_pmdi_m.pmdi016 = g_pmdi_m_o.pmdi016
               NEXT FIELD CURRENT
            END IF
         END IF

         LET g_pmdi_m_o.pmdi016 = g_pmdi_m.pmdi016

      AFTER INPUT
         IF INT_FLAG THEN
            EXIT INPUT
         END IF

      ON ACTION accept
         ACCEPT INPUT

      ON ACTION cancel
         LET INT_FLAG = TRUE
         EXIT INPUT

      ON ACTION exit
         LET INT_FLAG = TRUE
         EXIT INPUT

      ON ACTION close
         LET INT_FLAG = TRUE 
         EXIT INPUT

   END INPUT

   IF INT_FLAG THEN
      LET INT_FLAG = FALSE
      LET g_pmdi_m.* = g_pmdi_m_t.*
      CALL apmt440_show()

      INITIALIZE g_errparam TO NULL
      LET g_errparam.extend = ''
      LET g_errparam.code   = 9001
      LET g_errparam.popup  = TRUE
      CALL cl_err()

      CLOSE apmt440_cl
      LET r_success = FALSE
      RETURN r_success
   END IF

   UPDATE pmdi_t SET pmdi016 = g_pmdi_m.pmdi016
    WHERE pmdient   = g_enterprise
      AND pmdidocno = g_pmdidocno_t
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.extend = 'upd pmdi_t'
      LET g_errparam.code   = SQLCA.sqlcode
      LET g_errparam.popup  = TRUE 
      CALL cl_err()

      CLOSE apmt440_cl
      LET r_success = FALSE
      RETURN r_success
   END IF

   CLOSE apmt440_cl

   RETURN r_success 
   
END FUNCTION

