PRIVATE FUNCTION apmt440_pmdk_fill(p_pmdjseq)
DEFINE li_ac           LIKE type_t.num5
DEFINE p_pmdjseq       LIKE pmdj_t.pmdjseq
       CALL g_pmdj2_d.clear()

       LET g_sql = "SELECT  UNIQUE pmdkseq,pmdk001,pmdk002,pmdk003,pmdk004 FROM pmdk_t",    
                   "",
                   #" WHERE pmdkent=? AND pmdkdocno=? AND pmdkseq = ? "   #160711-00025#1 mark
