   #鎖住將被更改的資料
   FETCH apmt440_ooff_cl INTO g_pmdi_m.pmdi001,g_pmdi_m.pmdisite,g_pmdi_m.pmdidocno,g_pmdi_m.pmdidocdt,g_pmdi_m.pmdi002, 
