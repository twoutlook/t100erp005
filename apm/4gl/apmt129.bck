################################################################################
PRIVATE FUNCTION apmt129_before_pmbw()
  DEFINE   l_cnt     LIKE type_t.num5
  DEFINE   l_sql     STRING
  IF cl_null(g_pmbw_d[l_ac].pmbw011) AND cl_null(g_pmbw_d[l_ac].pmbw031) 
      AND cl_null(g_pmbw_d[l_ac].pmbw032) OR cl_null(g_pmbw_d[l_ac].pmbw013) THEN
      RETURN TRUE
   END IF
  IF NOT cl_null(g_pmbw_d[l_ac].pmbw013) THEN
      LET l_cnt = 0
      #dorislai-20150628-modify----(S)
#      SELECT COUNT(*) INTO l_cnt FROM pmbw_t,pmbv_t
#       WHERE pmbwent = pmbvent
#         AND pmbwdocno = pmbvdocno
#         AND pmbwent = g_enterprise
#         AND pmbv001 = g_pmbv_m.pmbv001
#         AND pmbv002 = g_pmbv_m.pmbv002
#         AND pmbw011 = g_pmbw_d[l_ac].pmbw011
#         AND pmbw012 = g_pmbw_d[l_ac].pmbw012
#         AND pmbw013 = g_pmbw_d[l_ac].pmbw013
#         AND pmbvstus = 'N'
#         AND pmbvdocno != g_pmbv_m.pmbvdocno
      LET l_sql = " SELECT COUNT(*) FROM pmbw_t,pmbv_t",
                  " WHERE pmbwent = pmbvent",
                  "   AND pmbwdocno = pmbvdocno",
                  "   AND pmbwent = '",g_enterprise,"'",
                  "   AND pmbv001 = '",g_pmbv_m.pmbv001,"'",
                  "   AND pmbv002 = '",g_pmbv_m.pmbv002,"'",
                  "   AND pmbw013 = '",g_pmbw_d[l_ac].pmbw013,"'",
                  "   AND pmbvstus = 'N'",
                  "   AND pmbvdocno != '",g_pmbv_m.pmbvdocno,"'"
      IF NOT cl_null(g_pmbw_d[l_ac].pmbw011) THEN
         LET l_sql = l_sql clipped,"   AND pmbw011 = '",g_pmbw_d[l_ac].pmbw011,"'"
         IF NOT cl_null(g_pmbw_d[l_ac].pmbw012) THEN
            LET l_sql = l_sql clipped,"   AND pmbw012 = '",g_pmbw_d[l_ac].pmbw012,"'"   
         END IF
      END IF
      IF NOT cl_null(g_pmbw_d[l_ac].pmbw031) THEN
         LET l_sql = l_sql clipped,"   AND pmbw031 = '",g_pmbw_d[l_ac].pmbw031,"'"
      END IF
      IF NOT cl_null(g_pmbw_d[l_ac].pmbw032) THEN
         LET l_sql = l_sql clipped,"   AND pmbw032 = '",g_pmbw_d[l_ac].pmbw032,"'"
      END IF
      PREPARE apmt129_before_pmbw FROM l_sql
      EXECUTE apmt129_before_pmbw INTO l_cnt 
      FREE    apmt129_before_pmbw  #已使用完畢，釋放
      #dorislai-20150628-modify----(E)
      IF l_cnt > 0 THEN 
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'axm-00188'
         LET g_errparam.extend = g_pmbw_d[l_ac].pmbw011
         LET g_errparam.popup = TRUE
         CALL cl_err()

         RETURN FALSE
      END IF         
   END IF
   RETURN TRUE
END FUNCTION

