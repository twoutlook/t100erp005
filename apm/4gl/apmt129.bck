# Descriptions...: 檢查料號之間的參考關係是否形成無窮迴圈
# Memo...........:
# Usage..........: CALL apmt129_pmbw015_upd_chk(p_pmbw015,p_pmbw016)
# Input parameter: p_pmbw015   參考料號
#                : p_pmbw016   參考料號產品特徵 
# Date & Author..: 2014/03/13  By chenjing
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt129_pmbw015_upd_chk(p_pmbw015,p_pmbw016)
   DEFINE   p_pmbw015      LIKE pmbw_t.pmbw015
   DEFINE   p_pmbw016      LIKE pmbw_t.pmbw016
   DEFINE   l_pmbw013      LIKE  pmbw_t.pmbw013
   DEFINE   l_pmbw015      LIKE  pmbw_t.pmbw015
   DEFINE   l_pmbw016      LIKE  pmbw_t.pmbw016
   DEFINE   l_pmbw033      LIKE  pmbw_t.pmbw033
   DEFINE   l_pmbw034      LIKE  pmbw_t.pmbw034
   DEFINE   l_chk          LIKE  type_t.num5
 
   IF p_pmbw015 IS NULL OR p_pmbw016 IS NULL THEN
      RETURN
   END IF  
   #dorislai-20150628-modify----(S)
#   SELECT pmbw015,pmbw016 INTO l_pmbw015,l_pmbw016 FROM pmbw_t 
#    WHERE pmbwent = g_enterprise
#      AND pmbwdocno = g_pmbv_m.pmbvdocno
#      AND pmbw011 = p_pmbw015
#      AND pmbw012 = p_pmbw016
#      AND pmbw013 = g_pmbw_d[l_ac].pmbw013
#      AND pmbw014 = 'Y'   
   LET l_chk = FALSE
   IF g_pmbw_d[l_ac].pmbw014 = 'Y' THEN
      #判斷是否被其他資料參考
      IF NOT cl_null(g_pmbw_d[l_ac].pmbw011) THEN   #若料號有值
         SELECT pmbw013,pmbw015,pmaw016 INTO l_pmbw013,l_pmbw015,l_pmbw016 FROM pmbw_t 
          WHERE pmbwdocno = g_pmbv_m.pmbvdocno
            AND pmbw011 = p_pmbw015
            AND pmbw012 = p_pmbw016
            AND pmbw013 = g_pmbw_d[l_ac].pmbw013
            AND pmbw014 = 'Y'
            AND pmbwent = g_enterprise   #160905-00007#11 Add
         IF l_pmbw013 = g_pmbw_d[l_ac].pmbw013 AND l_pmbw015 = g_pmbw_d[l_ac].pmbw011 AND l_pmbw016 = g_pmbw_d[l_ac].pmbw012 THEN
            LET l_chk = TRUE
         END IF
      END IF
      IF NOT cl_null(g_pmbw_d[l_ac].pmbw031) THEN   #若系列有值
         SELECT pmbw013,pmbw033 INTO l_pmbw013,l_pmbw033 FROM pmbw_t 
          WHERE pmbwdocno = g_pmbv_m.pmbvdocno
            AND pmbw011 = p_pmbw015
            AND pmbw012 = p_pmbw016
            AND pmbw013 = g_pmbw_d[l_ac].pmbw013
            AND pmbw014 = 'Y'
            AND pmbwent = g_enterprise   #160905-00007#11 Add
         IF l_pmbw013 = g_pmbw_d[l_ac].pmbw013 AND l_pmbw033 = g_pmbw_d[l_ac].pmbw031 THEN
            LET l_chk = TRUE
         END IF
      END IF
      IF NOT cl_null(g_pmbw_d[l_ac].pmbw032) THEN   #若產品分類有值
         SELECT pmbw013,pmbw034 INTO l_pmbw013,l_pmbw034 FROM pmbw_t 
          WHERE pmbwdocno = g_pmbv_m.pmbvdocno
            AND pmbw011 = p_pmbw015
            AND pmbw012 = p_pmbw016
            AND pmbw013 = g_pmbw_d[l_ac].pmbw013
            AND pmbw014 = 'Y'
            AND pmbwent = g_enterprise   #160905-00007#11 Add
         IF l_pmbw013 = g_pmbw_d[l_ac].pmbw013 AND l_pmbw034 = g_pmbw_d[l_ac].pmbw032 THEN
            LET l_chk = TRUE
         END IF
      END IF
   END IF
#   IF l_pmbw015 = g_pmbw_d[l_ac].pmbw011 AND l_pmbw016 = g_pmbw_d[l_ac].pmbw012 THEN
   IF l_chk THEN
   #dorislai-20150628-modify----(E)
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'axm-00160'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()

      LET g_success = 'N'
      RETURN  
#   ELSE
#      CALL apmt129_pmbw015_upd_chk(l_pmbw015,l_pmbw016)
   END IF

END FUNCTION

