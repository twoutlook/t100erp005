            RETURN FALSE
         END IF            
         IF cl_null(g_pmbw_d[l_ac].pmbw019) AND cl_null(g_pmbw_d[l_ac].pmbw020) THEN
            SELECT pmaw014,pmaw015,pmaw016,pmaw033,pmaw034,pmaw017,pmaw018,pmaw019,pmaw020,pmaw021,pmaw022 INTO
                   g_pmbw_d[l_ac].pmbw014,g_pmbw_d[l_ac].pmbw015,g_pmbw_d[l_ac].pmbw016,g_pmbw_d[l_ac].pmbw033,
