################################################################################
# Descriptions...: 採購價格表整批產生
# Date & Author..: 2014/02/22 By chenjing
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt129_auto_gen_pmbw(p_wc)
DEFINE    p_wc        STRING  
DEFINE    l_sql       STRING
DEFINE    l_imaa001   LIKE imaa_t.imaa001
#161124-00048#9 mod-S
#DEFINE    l_pmbw      RECORD LIKE pmbw_t.*
DEFINE l_pmbw RECORD  #採購價格表申請單身檔
       pmbwent LIKE pmbw_t.pmbwent, #企业编号
       pmbwdocno LIKE pmbw_t.pmbwdocno, #申请单号
       pmbw010 LIKE pmbw_t.pmbw010, #变更方式
       pmbw011 LIKE pmbw_t.pmbw011, #料件编号
       pmbw012 LIKE pmbw_t.pmbw012, #产品特征
       pmbw013 LIKE pmbw_t.pmbw013, #计价单位
       pmbw014 LIKE pmbw_t.pmbw014, #参考数据否
       pmbw015 LIKE pmbw_t.pmbw015, #参考料号
       pmbw016 LIKE pmbw_t.pmbw016, #参考料号产品特征
       pmbw017 LIKE pmbw_t.pmbw017, #加金额
       pmbw018 LIKE pmbw_t.pmbw018, #加百分比
       pmbw019 LIKE pmbw_t.pmbw019, #标准定价
       pmbw020 LIKE pmbw_t.pmbw020, #一般采购价
       pmbw021 LIKE pmbw_t.pmbw021, #警告容差率
       pmbw022 LIKE pmbw_t.pmbw022, #拒绝容差率
       pmbw031 LIKE pmbw_t.pmbw031, #系列
       pmbw032 LIKE pmbw_t.pmbw032, #产品分类
       pmbw033 LIKE pmbw_t.pmbw033, #参考系列
       pmbw034 LIKE pmbw_t.pmbw034  #参考产品分类
END RECORD
#161124-00048#9 mod-E
DEFINE    l_pmbw011   LIKE pmbw_t.pmbw011
DEFINE    l_pmbw012   LIKE pmbw_t.pmbw012
DEFINE    l_pmbw013   LIKE pmbw_t.pmbw013
DEFINE    l_flag      LIKE type_t.chr1

DEFINE    l_cnt       LIKE type_t.num5
DEFINE    r_success   LIKE type_t.chr1
DEFINE    l_price022  LIKE pmbw_t.pmbw022


   IF cl_null(p_wc) THEN LET p_wc = "1=1" END IF
   LET l_sql = " SELECT imaa001 FROM imaa_t,imaf_t",
               "  WHERE imaaent = imafent ",
               "    AND imaa001 = imaf001" , 
               "    AND imaaent = '",g_enterprise,"'",               
               "    AND imafsite = 'ALL'",
               "    AND imaa001 = imaf001",
               "    AND imaastus = 'Y'" ,
               "    AND ", p_wc
 
   IF cl_null(g_s_imaa001) AND NOT cl_null(g_e_imaa001) THEN 
      LET l_sql = l_sql, " AND imaa001 = '",g_e_imaa001,"'"
   END IF 
   IF cl_null(g_e_imaa001) AND NOT cl_null(g_s_imaa001) THEN 
      LET l_sql = l_sql, " AND imaa001 = '",g_s_imaa001,"'"
   END IF
   IF NOT cl_null(g_s_imaa001) AND NOT cl_null(g_e_imaa001) THEN 
      LET l_sql = l_sql, "    AND imaa001 BETWEEN '",g_s_imaa001,"'", " AND '",g_e_imaa001,"'"
               
   END IF               
   PREPARE apmt129_s01_pre FROM l_sql
   DECLARE apmt129_s01_cur CURSOR FOR apmt129_s01_pre
   LET r_success = 'Y'
   CALL s_transaction_begin()
   FOREACH apmt129_s01_cur INTO l_imaa001

 
      IF g_pmbw_s.pmbw019_1 = '1' OR g_pmbw_s.pmbw019_1 = '2' THEN
         SELECT imai021 INTO g_pmbw_s.pmbw019 FROM imai_t
          WHERE imaient = g_enterprise
            AND imaisite = g_site
            AND imai001 = l_imaa001 
            AND imai035 IN (SELECT MAX(imai035) FROM imai_t WHERE imaient = g_enterprise AND imai001 = l_imaa001) 
         IF SQLCA.sqlcode = 100 OR cl_null(g_pmbw_s.pmbw019) THEN
            IF g_pmbw_s.chk1 = 'Y' THEN
               CONTINUE FOREACH
            ELSE
               LET l_flag = 'Y'
               LET g_pmbw_s.pmbw019 = 0       
            END IF 
         ELSE 
            LET l_flag = 'Y'            
         END IF
      END IF 
      IF g_pmbw_s.pmbw019_1 = '3' THEN
         SELECT imai021 INTO g_pmbw_s.pmbw019 FROM imai_t
          WHERE imaient = g_enterprise
            AND imai001 = l_imaa001 
            AND imai035 IN (SELECT MAX(imai035) FROM imai_t WHERE imaient = g_enterprise AND imai001 = l_imaa001) 
         IF SQLCA.sqlcode = 100 OR cl_null(g_pmbw_s.pmbw019) THEN
            IF g_pmbw_s.chk1 = 'Y' THEN
               CONTINUE FOREACH
            ELSE
               LET l_flag = 'Y'
               LET g_pmbw_s.pmbw019 = 0       
            END IF 
         ELSE 
            LET l_flag = 'Y'  
         END IF
      END IF 
      CALL apmt129_get_price(g_pmbw_s.pmbw019,g_pmbw_s.pmbw019_w,g_pmbw_s.pmbw019_s,g_pmbw_s.pmbw019_r)
           RETURNING g_pmbw_s.pmbw019      
      IF NOT cl_null(g_pmbw_s.pmbw020_mon) THEN 
         LET g_pmbw_s.pmbw020 = g_pmbw_s.pmbw019 + g_pmbw_s.pmbw020_mon    
      END IF 
      IF NOT cl_null(g_pmbw_s.pmbw020_rate) THEN 
         LET g_pmbw_s.pmbw020 = g_pmbw_s.pmbw019*(1+g_pmbw_s.pmbw020_rate/100)
      END IF
      IF cl_null(g_pmbw_s.pmbw020) THEN 
         LET g_pmbw_s.pmbw020 = 0 
      END IF         

     CALL apmt129_get_price(g_pmbw_s.pmbw020,g_pmbw_s.pmbw020_w,g_pmbw_s.pmbw020_s,g_pmbw_s.pmbw020_r)
           RETURNING g_pmbw_s.pmbw020       
     
         LET l_pmbw.pmbwdocno = g_pmbv_m.pmbvdocno
         LET l_pmbw.pmbw010 = 'I'
         LET l_pmbw.pmbw011 = l_imaa001
         LET l_pmbw.pmbw012 = ' '
         LET l_pmbw.pmbw014 = 'N'
         LET l_pmbw.pmbw015 = l_imaa001
         LET l_pmbw.pmbw016 = ' '
         LET l_pmbw.pmbw017 = ''
         LET l_pmbw.pmbw018 = ''
         LET l_pmbw.pmbw019 = g_pmbw_s.pmbw019  
         LET l_pmbw.pmbw020 = g_pmbw_s.pmbw020
         LET l_pmbw.pmbw021 = g_pmbw_s.pmbw021
         LET l_pmbw.pmbw022 = g_pmbw_s.pmbw022

       
         SELECT imaf113 INTO l_pmbw.pmbw013 FROM imaf_t
          WHERE imafent = g_enterprise
            AND imafsite = 'ALL'
            AND imaf001 = l_imaa001
         IF l_pmbw.pmbw013 IS NULL THEN
            LET l_pmbw.pmbw013 = ' '
         END IF         
         
         INSERT INTO pmbw_t
                           (pmbwent,
                            pmbwdocno,pmbw010,
                            pmbw011,pmbw012,pmbw013,
                            pmbw014,pmbw015,pmbw016,pmbw017,pmbw018,pmbw019,pmbw020,pmbw021,pmbw022) 
                     VALUES(g_enterprise,
                            l_pmbw.pmbwdocno,l_pmbw.pmbw010,
                            l_pmbw.pmbw011,l_pmbw.pmbw012,l_pmbw.pmbw013,
                            l_pmbw.pmbw014,l_pmbw.pmbw015,l_pmbw.pmbw016, 
                                l_pmbw.pmbw017,l_pmbw.pmbw018,l_pmbw.pmbw019, 
                                l_pmbw.pmbw020,l_pmbw.pmbw021,l_pmbw.pmbw022)
                                
         IF SQLCA.sqlcode THEN
            LET r_success = 'N'
            EXIT FOREACH
         END IF                                                               

   END FOREACH   
   IF r_success = 'Y' THEN
      CALL s_transaction_end('Y','0')
     #提示
      IF cl_null(l_flag) THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'agl-00167'
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()

      ELSE
      
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'axm-00088'
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()

      END IF 
   ELSE
      CALL s_transaction_end('N','0')
     #提示 
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'axm-00089'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()

   END IF            
END FUNCTION
