   IF g_pmbv_m.pmbvdocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code = "std-00003" 
      LET g_errparam.popup = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_pmbvdocno_t = g_pmbv_m.pmbvdocno
 
   CALL s_transaction_begin()
   
   OPEN apmt129_cl USING g_enterprise,g_pmbv_m.pmbvdocno
   IF SQLCA.SQLCODE THEN   #(ver:78)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN apmt129_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE   #(ver:78)
      LET g_errparam.popup = TRUE 
      CLOSE apmt129_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE apmt129_master_referesh USING g_pmbv_m.pmbvdocno INTO g_pmbv_m.pmbvdocno,g_pmbv_m.pmbvdocdt, 
       g_pmbv_m.pmbv900,g_pmbv_m.pmbv901,g_pmbv_m.pmbvstus,g_pmbv_m.pmbv001,g_pmbv_m.pmbv002,g_pmbv_m.pmbv003, 
       g_pmbv_m.pmbv004,g_pmbv_m.pmbvownid,g_pmbv_m.pmbvowndp,g_pmbv_m.pmbvcrtid,g_pmbv_m.pmbvcrtdp, 
       g_pmbv_m.pmbvcrtdt,g_pmbv_m.pmbvmodid,g_pmbv_m.pmbvmoddt,g_pmbv_m.pmbvcnfid,g_pmbv_m.pmbvcnfdt, 
       g_pmbv_m.pmbv900_desc,g_pmbv_m.pmbv901_desc,g_pmbv_m.pmbv001_desc,g_pmbv_m.pmbv002_desc,g_pmbv_m.pmbv003_desc, 
       g_pmbv_m.pmbvownid_desc,g_pmbv_m.pmbvowndp_desc,g_pmbv_m.pmbvcrtid_desc,g_pmbv_m.pmbvcrtdp_desc, 
       g_pmbv_m.pmbvmodid_desc,g_pmbv_m.pmbvcnfid_desc
   
   #檢查是否允許此動作
   IF NOT apmt129_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_pmbv_m_mask_o.* =  g_pmbv_m.*
   CALL apmt129_pmbv_t_mask()
   LET g_pmbv_m_mask_n.* =  g_pmbv_m.*
   
   
   
   #add-point:modify段show之前 name="modify.before_show"
   
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
 
   
   CALL apmt129_show()
   #add-point:modify段show之後 name="modify.after_show"
   
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
 
 
    
   WHILE TRUE
      LET g_pmbvdocno_t = g_pmbv_m.pmbvdocno
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_pmbv_m.pmbvmodid = g_user 
LET g_pmbv_m.pmbvmoddt = cl_get_current()
LET g_pmbv_m.pmbvmodid_desc = cl_get_username(g_pmbv_m.pmbvmodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      #「D抽單 / R已拒絕」狀況碼更改資料後，需還原為「N未確認」
      IF g_pmbv_m.pmbvstus MATCHES "[DR]" THEN 
         LET g_pmbv_m.pmbvstus = "N"
      END IF
                                          
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      LET g_master_commit = "N"
      CALL apmt129_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後 name="modify.after_input"
                                          
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE pmbv_t SET (pmbvmodid,pmbvmoddt) = (g_pmbv_m.pmbvmodid,g_pmbv_m.pmbvmoddt)
          WHERE pmbvent = g_enterprise AND pmbvdocno = g_pmbvdocno_t
 
      END IF
    
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         #若單頭無commit則還原
         IF g_master_commit = "N" THEN
            LET g_pmbv_m.* = g_pmbv_m_t.*
            CALL apmt129_show()
         END IF
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code = 9001 
         LET g_errparam.popup = FALSE 
         CALL cl_err()
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_pmbv_m.pmbvdocno != g_pmbv_m_t.pmbvdocno
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前 name="modify.body.b_fk_update"
                                                               
         #end add-point
         
         #更新單身key值
         UPDATE pmbw_t SET pmbwdocno = g_pmbv_m.pmbvdocno
 
          WHERE pmbwent = g_enterprise AND pmbwdocno = g_pmbv_m_t.pmbvdocno
 
            
         #add-point:單身fk修改中 name="modify.body.m_fk_update"
                                                               
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            #   INITIALIZE g_errparam TO NULL 
            #   LET g_errparam.extend = "pmbw_t" 
            #   LET g_errparam.code = "std-00009" 
            #   LET g_errparam.popup = TRUE 
            #   CALL cl_err()
            #   CALL s_transaction_end('N','0')
            #   CONTINUE WHILE
            WHEN SQLCA.SQLCODE #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "pmbw_t:",SQLERRMESSAGE 
               LET g_errparam.code = SQLCA.SQLCODE 
               LET g_errparam.popup = TRUE 
               CALL s_transaction_end('N','0')
               CALL cl_err()
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後 name="modify.body.a_fk_update"
                                                               
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL apmt129_set_act_visible()   
   CALL apmt129_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " pmbvent = " ||g_enterprise|| " AND",
                      " pmbvdocno = '", g_pmbv_m.pmbvdocno, "' "
 
   #填到對應位置
   CALL apmt129_browser_fill("")
 
   CLOSE apmt129_cl
   
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL apmt129_msgcentre_notify('modify')
 
END FUNCTION 
 
