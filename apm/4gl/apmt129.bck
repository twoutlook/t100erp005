  #         AND pmaw002 = g_pmbv_m.pmbv002
  #         AND pmaw011 = g_pmbw_d[l_ac].pmbw015
  #         AND pmaw012 = g_pmbw_d[l_ac].pmbw016
  #         AND pmaw013 = g_pmbw_d[l_ac].pmbw013
  #         AND (pmaw011 <> g_pmbw_d[l_ac].pmbw011 OR pmaw012 <> g_pmbw_d[l_ac].pmbw012)
   IF g_pmbw_d[l_ac].pmbw014 = 'Y' AND NOT cl_null(g_pmbw_d[l_ac].pmbw013) AND NOT cl_null(g_pmbw_d[l_ac].pmbw015) THEN
     LET l_sql = " SELECT COUNT(*) FROM pmbw_t ",             
               "  WHERE pmbwent = '",g_enterprise,"'" ,
               "    AND pmbwdocno = '",g_pmbv_m.pmbvdocno,"'",
               "    AND (pmbw011 <>'", g_pmbw_d[l_ac].pmbw011 ,"' OR pmbw012 <> '", g_pmbw_d[l_ac].pmbw012,"')"
      IF NOT cl_null(g_pmbw_d[l_ac].pmbw015) THEN
         LET l_sql = l_sql," AND pmbw011 = '",g_pmbw_d[l_ac].pmbw015,"'" 
      END IF   
      IF g_pmbw_d[l_ac].pmbw016 IS NOT NULL THEN 
         LET l_sql = l_sql," AND pmbw012 = '",g_pmbw_d[l_ac].pmbw016,"'"
      END IF  
      IF NOT cl_null(g_pmbw_d[l_ac].pmbw013) THEN 
         LET l_sql = l_sql," AND pmbw013 = '",g_pmbw_d[l_ac].pmbw013,"'"
      END IF 
      IF NOT cl_null(g_pmbw_d[l_ac].pmbw011) THEN
         LET l_sql = l_sql," AND pmbw011 != '",g_pmbw_d[l_ac].pmbw011,"'"
         IF NOT cl_null(g_pmbw_d[l_ac].pmbw012) THEN
            LET l_sql = l_sql," AND pmbw012 != '",g_pmbw_d[l_ac].pmbw012,"'"
         END IF
      END IF
      PREPARE apmt129_pmbw012 FROM l_sql
      EXECUTE apmt129_pmbw012 INTO l_cnt

      IF g_pmbv_m.pmbv004 = '2' AND l_cnt = 0  THEN
         LET l_sql1 = " SELECT pmaw015,pmawstus FROM pmaw_t ",
                      "  WHERE pmawent = '",g_enterprise,"'" ,
                      "    AND pmaw001 = '",g_pmbv_m.pmbv001,"'",
                      "    AND pmaw002 = '",g_pmbv_m.pmbv002,"'",
                      "    AND (pmaw011 <> '",g_pmbw_d[l_ac].pmbw011 ,"' OR pmaw012 <> '",g_pmbw_d[l_ac].pmbw012,"')"
