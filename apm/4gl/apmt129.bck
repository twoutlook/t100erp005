      END IF
   END IF
   RETURN TRUE
END FUNCTION

################################################################################
# Descriptions...: 系列或產品分類的單位校驗
# Memo...........:
# Usage..........: CALL apmt129_pmbw031_pmbw032_chk_unit()
#                  RETURNING TRUE/FALSE
# Input parameter: 回傳 TRUE/FALSE
# Return code....: 
# Date & Author..: 2015-06-19 By dorislai
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt129_pmbw031_pmbw032_chk_unit()
   DEFINE  l_sql                 STRING                               #抓取sql語法
   DEFINE  l_chk                 LIKE  type_t.num5                    #判斷是否有抓到符合的值
   DEFINE  l_imaa001             LIKE  type_t.chr100                  #接收抓取到的料件編號值
   #先依欄位抓取sql語法
      #---用系列去抓料件編號
   IF NOT cl_null(g_pmbw_d[l_ac].pmbw031) THEN
      LET l_sql = "SELECT imaa001 FROM imaa_t ",
                  "WHERE imaaent = '"||g_enterprise||"' AND imaa127 = '"||g_pmbw_d[l_ac].pmbw031||"'",
                  "ORDER BY imaa001"
   END IF
      #----用產品分類去抓料件編號
   IF NOT cl_null(g_pmbw_d[l_ac].pmbw032) THEN
      LET l_sql = "SELECT imaa001 FROM imaa_t ",
                  "WHERE imaaent = '"||g_enterprise||"' AND imaa009 = '"||g_pmbw_d[l_ac].pmbw032||"'",
                  "ORDER BY imaa001"
   END IF
               
   
   PREPARE apmt129_get_imaa001_chk_pre FROM l_sql
   DECLARE apmt129_get_imaa001_chk_curs CURSOR FOR apmt129_get_imaa001_chk_pre
   LET l_imaa001 =''
   LET l_chk = FALSE  #判斷是否有抓到符合的值
   #一筆一筆比對是否有符合任一 料件編號中的可使用單位
   FOREACH apmt129_get_imaa001_chk_curs INTO l_imaa001 
      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_pmbw_d[l_ac].pmbw013
      #以COUNT來計算，並判斷是否有比對到符合的資料
      CALL ap_ref_array2(g_ref_fields,"SELECT COUNT(*) FROM imao_t WHERE imaoent='"||g_enterprise||"' AND imao002 = ? AND imao001 = '"||l_imaa001||"'","") RETURNING g_rtn_fields
      IF g_rtn_fields[1] > 0 THEN
         LET l_chk = TRUE 
         EXIT FOREACH
      END IF 
   END FOREACH
   FREE  apmt129_get_imaa001_chk_pre  #已使用完畢，釋放
   #若料件編號中無相對應的可使用單位，跳出錯誤訊息提示
   IF NOT l_chk THEN
      RETURN FALSE
   END IF              
   RETURN TRUE 
END FUNCTION

################################################################################
# Descriptions...: 料號、系列、產品分類欄位重複性檢查
# Memo...........:
# Usage..........: CALL apmt129_pmbw_rep_chk(l_field_num)
#                  RETURNING TRUE/FALSE
# Input parameter: 
# Return code....: 回傳 TRUE/FALSE
# Date & Author..: 2015-06-20 By dorislai
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt129_pmbw_rep_chk(p_field_num)
   DEFINE  l_cnt  LIKE  type_t.num5
   DEFINE  l_sql  STRING
   DEFINE  p_field_num STRING
   IF cl_null(g_pmbw_d[l_ac].pmbw011) AND cl_null(g_pmbw_d[l_ac].pmbw031) 
      AND cl_null(g_pmbw_d[l_ac].pmbw032) OR cl_null(g_pmbw_d[l_ac].pmbw013) THEN
      RETURN TRUE
   END IF
   LET l_cnt = 0
   LET l_sql = " SELECT COUNT(*) FROM pmbw_t",
               " WHERE pmbwent = '",g_enterprise,"'",
               "   AND pmbwdocno = '",g_pmbv_m.pmbvdocno,"'",
               "   AND pmbw013 = '",g_pmbw_d[l_ac].pmbw013,"'"
   IF NOT cl_null(g_pmbw_d[l_ac].pmbw011) AND p_field_num = 1 THEN    
         LET l_sql = l_sql clipped," AND pmbw011 = '",g_pmbw_d[l_ac].pmbw011,"'",
                                   " AND pmbw012 = '",g_pmbw_d[l_ac].pmbw012,"'"

   END IF
   IF NOT cl_null(g_pmbw_d[l_ac].pmbw031) AND p_field_num = 2 THEN
      LET l_sql = l_sql clipped," AND pmbw031 = '",g_pmbw_d[l_ac].pmbw031,"'"
   END IF
   IF NOT cl_null(g_pmbw_d[l_ac].pmbw032) AND p_field_num = 3 THEN
      LET l_sql = l_sql clipped," AND pmbw032 = '",g_pmbw_d[l_ac].pmbw032,"'"
   END IF
   PREPARE apmt129_pmbw_rep_chk FROM l_sql
   EXECUTE apmt129_pmbw_rep_chk INTO l_cnt 
   FREE    apmt129_pmbw_rep_chk  #已使用完畢，釋放
   IF l_cnt > 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'std-00004'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()
      RETURN FALSE
   END IF
   RETURN TRUE
END FUNCTION

################################################################################
# Descriptions...: 檢查同表號+幣別是否有符合的資料可參考
# Memo...........:
# Usage..........: CALL apmt129_pmbw014_chk()
#                  RETURNING TRUE/FALSE
# Input parameter: 
# Return code....: 回傳 TRUE/FALSE
# Date & Author..: 20150629 By dorislai
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt129_pmbw014_chk()
   DEFINE l_cnt  LIKE  type_t.num5
   IF g_pmbw_d[l_ac].pmbw014 = 'Y'  THEN
   
      LET l_cnt = 0 
      SELECT COUNT(*) INTO l_cnt 
        FROM xmbw_t 
       WHERE pmbwent = g_enterprise
         AND pmbwdocno = g_pmbv_m.pmbvdocno
         AND pmbw013 = COALESCE(g_pmbw_d[l_ac].pmbw013,' ') 
         AND pmbw014 = 'N'
         AND NOT (     pmbw011 = COALESCE(g_pmbw_d[l_ac].pmbw011,' ')  
                   AND pmbw012 = COALESCE(g_pmbw_d[l_ac].pmbw012,' ')  
                   AND pmbw031 = COALESCE(g_pmbw_d[l_ac].pmbw031,' ')  
                   AND pmbw032 = COALESCE(g_pmbw_d[l_ac].pmbw032,' ')) 
      IF cl_null(l_cnt) OR l_cnt = 0 THEN  
         #用單頭的參照表號+幣別欄位，去找apmi129是否有存在資料 
         #若無，不可以勾選參考，因為無參考資料可選
         LET l_cnt = 0 
         SELECT COUNT(*) INTO l_cnt 
           FROM pmaw_t 
          WHERE pmawent = g_enterprise 
            AND pmaw001 = g_pmbv_m.pmbv001  
            AND pmaw002 = g_pmbv_m.pmbv002 
            AND pmawstus = 'Y'
            AND pmaw013 = COALESCE(g_pmbw_d[l_ac].pmbw013,' ')
            AND pmaw014 = 'N'
            AND NOT (     pmaw011 = COALESCE(g_pmbw_d[l_ac].pmbw011,' ')  
                      AND pmaw012 = COALESCE(g_pmbw_d[l_ac].pmbw012,' ')  
                      AND pmaw031 = COALESCE(g_pmbw_d[l_ac].pmbw031,' ')  
                      AND pmaw032 = COALESCE(g_pmbw_d[l_ac].pmbw032,' ')) 
         IF cl_null(l_cnt) OR l_cnt = 0 THEN 
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = 'apm-00964'
            LET g_errparam.extend = ""
            LET g_errparam.replace[1] = g_pmbv_m.pmbv001
            LET g_errparam.replace[2] = g_pmbv_m.pmbv002 
            LET g_errparam.popup = TRUE
            CALL cl_err()
            RETURN FALSE
         END IF
      END IF
      RETURN TRUE
   END IF
END FUNCTION

 
