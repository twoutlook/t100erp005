                         g_pmbw_d[l_ac].pmbw016,g_pmbw_d[l_ac].pmbw017,g_pmbw_d[l_ac].pmbw018,g_pmbw_d[l_ac].pmbw019,
                         g_pmbw_d[l_ac].pmbw020,g_pmbw_d[l_ac].pmbw021,g_pmbw_d[l_ac].pmbw022
                      

         #LET g_pmbw_d_t.* = g_pmbw_d[l_ac].*  
      ELSE          
         LET g_pmbw_d[l_ac].pmbw010 = 'I'          
      END IF  
      DISPLAY g_pmbw_d[l_ac].pmbw010 TO pmbw010             
   END IF 
   RETURN TRUE   
END FUNCTION
################################################################################
# Descriptions...: 單身資料整批產生
# Date & Author..: 2014/02/26 By chenjing
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt129_auto_gen()
   DEFINE   l_wc              STRING
   DEFINE   l_wc_1            STRING  
   DEFINE   l_s_imaa001_t     LIKE imaa_t.imaa001
   DEFINE   l_e_imaa001_t     LIKE imaa_t.imaa001   

   #畫面開啟 (identifier)
   OPEN WINDOW w_apmt129_s01 WITH FORM cl_ap_formpath("apm","apmt129_s01")
   INITIALIZE g_pmbw_s.* TO NULL
   LET g_errshow = 1   
   LET l_s_imaa001_t = NULL
   LET l_e_imaa001_t = NULL   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
   
      #螢幕上取條件
      CONSTRUCT BY NAME l_wc ON s_imaa001,e_imaa001

         AFTER FIELD s_imaa001
            LET g_s_imaa001 = GET_FLDBUF(s_imaa001)   
            

         ON ACTION controlp INFIELD s_imaa001

	        INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'i'
	        LET g_qryparam.reqry = FALSE
	        LET l_s_imaa001_t = g_s_imaa001
            LET g_qryparam.arg1 = "ALL"
            IF g_pmbv_m.pmbv003 = 'ALL' THEN            
               CALL q_imaf001_8()                #呼叫開窗
            ELSE
               LET g_qryparam.arg2 = g_pmbv_m.pmbv003
               CALL q_imaf001_11()    
            END IF                                   #呼叫開窗
            LET g_s_imaa001 = g_qryparam.return1
            IF cl_null(g_s_imaa001) THEN 
               LET g_s_imaa001 = l_s_imaa001_t
            END IF
            DISPLAY g_s_imaa001 TO s_imaa001  #顯示到畫面上

            NEXT FIELD s_imaa001
            
