            AND pmaw002 = g_pmbv_m.pmbv002
            AND pmaw011 = g_pmbw_d[l_ac].pmbw011
            AND pmaw012 = g_pmbw_d[l_ac].pmbw012
            AND pmaw013 = g_pmbw_d[l_ac].pmbw013
      END IF
      IF NOT cl_null(g_pmbw_d[l_ac].pmbw031) THEN
         SELECT COUNT(*) INTO l_cnt FROM pmaw_t
          WHERE pmawent = g_enterprise
            AND pmaw001 = g_pmbv_m.pmbv001
            AND pmaw002 = g_pmbv_m.pmbv002
            AND pmaw031 = g_pmbw_d[l_ac].pmbw031
            AND pmaw013 = g_pmbw_d[l_ac].pmbw013
      END IF
      IF NOT cl_null(g_pmbw_d[l_ac].pmbw032) THEN
         SELECT COUNT(*) INTO l_cnt FROM pmaw_t
          WHERE pmawent = g_enterprise
            AND pmaw001 = g_pmbv_m.pmbv001
            AND pmaw002 = g_pmbv_m.pmbv002
            AND pmaw032 = g_pmbw_d[l_ac].pmbw032
            AND pmaw013 = g_pmbw_d[l_ac].pmbw013
      END IF
   #dorislai-20150628-modify----(E)         
      IF l_cnt > 0 THEN 
         LET g_pmbw_d[l_ac].pmbw010 = 'U'
