               AND pmawstus = 'Y'
            DISPLAY g_pmbw_d[l_ac].pmbw019,g_pmbw_d[l_ac].pmbw020
         END IF         
      ELSE
         IF l_cnt = 0 THEN
            LET l_errno = 'apm-00319'
         END IF 
