################################################################################
# Descriptions...: 參考系列校驗
# Memo...........:
# Usage..........: CALL apmt129_pmbw033_chk() 
#                  RETURNING TURE/FALSE
# Input parameter: 
# Return code....: 回傳 TRUE/FALSE
# Date & Author..: 2015/6/15 By dorislai 
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt129_pmbw033_chk()
   DEFINE   l_pmaw001       LIKE pmaw_t.pmaw001
   DEFINE   l_pmawstus      LIKE pmaw_t.pmawstus
   DEFINE   l_cnt           LIKE type_t.num5
   DEFINE   l_sql           STRING
   DEFINE   l_msg           STRING

   IF g_pmbw_d[l_ac].pmbw014 = 'Y' AND NOT cl_null(g_pmbw_d[l_ac].pmbw013) AND NOT cl_null(g_pmbw_d[l_ac].pmbw033) THEN 
      LET l_cnt = 0
      #比對是否存在apmt129的資料中
      LET l_sql = " SELECT COUNT(*) FROM pmbw_t,pmbv_t",
                  "  WHERE pmbvdocno = pmbwdocno",
                  "    AND pmbvent = pmbwent",
                  "    AND pmbwent = '",g_enterprise,"'",
                  "    AND pmbwdocno = '",g_pmbv_m.pmbvdocno,"'",
                  "    AND pmbw031 = '",g_pmbw_d[l_ac].pmbw033,"'",
                  "    AND pmbw013 = '",g_pmbw_d[l_ac].pmbw013,"'",
                  "    AND pmbvstus = 'N'"
      IF NOT cl_null(g_pmbw_d[l_ac].pmbw031) THEN
         LET l_sql = l_sql clipped,"  AND pmbw031 != '",g_pmbw_d[l_ac].pmbw031,"'"
      END IF
      PREPARE apmt129_pmbw033 FROM l_sql
      EXECUTE apmt129_pmbw033 INTO l_cnt
      FREE    apmt129_pmbw033  #已使用完畢，釋放 

      #申請外的資料為保留，則去比對apmi129相對應表號、幣別中的資料
      IF cl_null(l_cnt) THEN LET l_cnt = 0 END IF         
      IF g_pmbv_m.pmbv004 = '2' AND l_cnt = 0  THEN
         LET l_sql =" SELECT pmaw001,pmawstus FROM pmaw_t",
                    "  WHERE pmawent = '",g_enterprise,"'",
                    "    AND pmaw001 = '",g_pmbv_m.pmbv001,"'",
                    "    AND pmaw002 = '", g_pmbv_m.pmbv002,"'",
                    "    AND pmaw031 = '", g_pmbw_d[l_ac].pmbw033,"'",
                    "    AND pmaw013 = '",g_pmbw_d[l_ac].pmbw013,"'"
         IF NOT cl_null(g_pmbw_d[l_ac].pmbw031) THEN   #修改方式U時，避免apmt129系列在apmi129有找到，視為可用
            LET l_sql = l_sql clipped,"  AND pmaw031 != '",g_pmbw_d[l_ac].pmbw031,"'"
         END IF
         PREPARE apmt129_pmbw033_2 FROM l_sql
         EXECUTE apmt129_pmbw033_2 INTO l_pmaw001,l_pmawstus
         FREE    apmt129_pmbw033_2  #已使用完畢，釋放 
         #在apmi129中沒抓到對應的資料
         IF cl_null(l_pmaw001) THEN 
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = 'apm-00949'
            LET g_errparam.extend = g_pmbw_d[l_ac].pmbw033
            LET g_errparam.popup = TRUE
            CALL cl_err()

            RETURN FALSE
         END IF
         #---apmi129抓到的資料是 無效的
         IF l_pmawstus = 'N' THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = 'apm-00951'
            LET g_errparam.extend = g_pmbw_d[l_ac].pmbw033
            LET g_errparam.popup = TRUE
            CALL cl_err()

            RETURN FALSE
         #---apmi129抓到的資料是 有效的，則抓取要參考資料的價格    
         ELSE
            IF cl_null(g_pmbw_d[l_ac].pmbw019) AND cl_null(g_pmbw_d[l_ac].pmbw020) THEN  
               SELECT pmaw019,pmaw020 
                 INTO g_pmbw_d[l_ac].pmbw019,g_pmbw_d[l_ac].pmbw020
                 FROM pmaw_t
                WHERE pmawent = g_enterprise
                  AND pmaw001 = g_pmbv_m.pmbv001
                  AND pmaw002 = g_pmbv_m.pmbv002
                  AND pmaw011 = g_pmbw_d[l_ac].pmbw033
                  AND pmaw013 = g_pmbw_d[l_ac].pmbw013
                  AND pmawstus = 'Y'
               DISPLAY BY NAME g_pmbw_d[l_ac].pmbw019,g_pmbw_d[l_ac].pmbw020
                    
            END IF        
            RETURN TRUE
         END IF 
      #申請外的資料為失效   
      ELSE
         IF l_cnt < 1 THEN
            CALL cl_getmsg('apm-00940',g_lang) RETURNING l_msg   #%1替換成  系列
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = 'apm-00960'
            LET g_errparam.extend = g_pmbw_d[l_ac].pmbw033
            LET g_errparam.popup = TRUE
            LET g_errparam.replace[1] = l_msg
            CALL cl_err()

            RETURN FALSE
         ELSE
            RETURN TRUE
         END IF   
      END IF 
   END IF   
   RETURN TRUE
END FUNCTION

