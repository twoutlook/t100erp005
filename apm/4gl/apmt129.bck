PRIVATE FUNCTION apmt129_pmbw011_chk()
DEFINE   l_cnt       LIKE type_t.num5

   LET l_cnt = 0
   IF NOT cl_null(g_pmbv_m.pmbvdocno) AND NOT cl_null(g_pmbw_d[l_ac].pmbw011) 
      AND g_pmbw_d[l_ac].pmbw012 IS NOT NULL AND NOT cl_null(g_pmbw_d[l_ac].pmbw013) THEN
      SELECT COUNT(*) INTO l_cnt FROM pmbw_t
       WHERE pmbwent = g_enterprise
         AND pmbwdocno = g_pmbv_m.pmbvdocno
         AND pmbw013 = g_pmbw_d_t.pmbw013
         AND pmbw015 = g_pmbw_d_t.pmbw011
         AND pmbw016 = g_pmbw_d_t.pmbw012
         AND (pmbw011 != g_pmbw_d_t.pmbw011 OR pmbw012 != g_pmbw_d_t.pmbw012)                

         
      IF l_cnt > 0 THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'axm-00082'
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()

         LET g_pmbw_d[l_ac].pmbw011 = g_pmbw_d_t.pmbw011
         LET g_pmbw_d[l_ac].pmbw012 = g_pmbw_d_t.pmbw012
         LET g_pmbw_d[l_ac].pmbw013 = g_pmbw_d_t.pmbw013
         LET g_pmbw_d[l_ac].pmbw015 = g_pmbw_d_t.pmbw015
         LET g_pmbw_d[l_ac].pmbw016 = g_pmbw_d_t.pmbw016
         CALL apmt129_pmbw011_ref()
         CALL apmt129_pmbw013_ref()
         CALL apmt129_pmbw015_ref()
         DISPLAY BY NAME g_pmbw_d[l_ac].pmbw011,g_pmbw_d[l_ac].pmbw012,g_pmbw_d[l_ac].pmbw013,g_pmbw_d[l_ac].pmbw015
         RETURN FALSE         
      END IF  
   END IF 
   RETURN TRUE   
END FUNCTION
################################################################################
# Descriptions...: 抓取一般採購單價和標準採購單價
# Date & Author..: 2014/02/26 By chenjing
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt129_pmbw_price()
DEFINE    l_pmbw019     LIKE pmbw_t.pmbw019
DEFINE    l_pmbw020     LIKE pmbw_t.pmbw020

   IF g_pmbw_d[l_ac].pmbw014 = 'Y' AND NOT cl_null(g_pmbw_d[l_ac].pmbw013) THEN
      IF cl_null(g_pmbw_d[l_ac].pmbw015) AND cl_null(g_pmbw_d[l_ac].pmbw033) AND cl_null(g_pmbw_d[l_ac].pmbw034) THEN
         RETURN
      END IF
      
      IF NOT cl_null(g_pmbw_d[l_ac].pmbw015) AND g_pmbw_d[l_ac].pmbw016 IS NOT NULL THEN   
         SELECT pmbw019,pmbw020 INTO l_pmbw019,l_pmbw020
           FROM pmbw_t       
          WHERE pmbwent = g_enterprise
            AND pmbwdocno = g_pmbv_m.pmbvdocno
            AND pmbw011 = g_pmbw_d[l_ac].pmbw015
            AND pmbw012 = g_pmbw_d[l_ac].pmbw016
            AND pmbw013 = g_pmbw_d[l_ac].pmbw013
      END IF      
      #2015-06-15---dorislai---add
      IF NOT cl_null(g_pmbw_d[l_ac].pmbw033) THEN
         SELECT pmbw019,pmbw020 INTO 
                l_pmbw019,l_pmbw020
           FROM pmbw_t       
          WHERE pmbwent = g_enterprise
            AND pmbwdocno = g_pmbv_m.pmbvdocno
            AND pmbw031 = g_pmbw_d[l_ac].pmbw033
            AND pmbw013 = g_pmbw_d[l_ac].pmbw013
      END IF

      IF NOT cl_null(g_pmbw_d[l_ac].pmbw034) THEN
         SELECT pmbw019,pmbw020 INTO 
                l_pmbw019,l_pmbw020
           FROM pmbw_t       
          WHERE pmbwent = g_enterprise
            AND pmbwdocno = g_pmbv_m.pmbvdocno
            AND pmbw032 = g_pmbw_d[l_ac].pmbw034
            AND pmbw013 = g_pmbw_d[l_ac].pmbw013
      END IF
      #2015-06-15---dorislai---end      
       
      IF SQLCA.sqlcode = 100 THEN
         IF NOT cl_null(g_pmbw_d[l_ac].pmbw015) AND g_pmbw_d[l_ac].pmbw016 IS NOT NULL THEN 
            SELECT pmaw019,pmaw020 INTO l_pmbw019,l_pmbw020
              FROM pmaw_t
             WHERE pmawent = g_enterprise
               AND pmaw001 = g_pmbv_m.pmbv001
               AND pmaw002 = g_pmbv_m.pmbv002
               AND pmaw011 = g_pmbw_d[l_ac].pmbw015
               AND pmaw012 = g_pmbw_d[l_ac].pmbw016
               AND pmaw013 = g_pmbw_d[l_ac].pmbw013
         END IF
         #2015-06-15---dorislai---add
         IF NOT cl_null(g_pmbw_d[l_ac].pmbw033) THEN
            SELECT pmaw019,pmaw020 INTO 
                   l_pmbw019,l_pmbw020
              FROM pmaw_t       
             WHERE pmawent = g_enterprise
               AND pmaw001 = g_pmbv_m.pmbv001
               AND pmaw002 = g_pmbv_m.pmbv002
               AND pmaw031 = g_pmbw_d[l_ac].pmbw033
               AND pmaw013 = g_pmbw_d[l_ac].pmbw013 
         END IF 
         IF NOT cl_null(g_pmbw_d[l_ac].pmbw034) THEN
            SELECT pmaw019,pmaw020 INTO 
                   l_pmbw019,l_pmbw020
              FROM pmaw_t       
             WHERE pmawent = g_enterprise
               AND pmaw001 = g_pmbv_m.pmbv001
               AND pmaw002 = g_pmbv_m.pmbv002
               AND pmaw032 = g_pmbw_d[l_ac].pmbw034
               AND pmaw013 = g_pmbw_d[l_ac].pmbw013 
         END IF  
         #2015-06-15---dorislai---end  
      END IF   

      IF NOT cl_null(g_pmbw_d[l_ac].pmbw017) THEN
         LET g_pmbw_d[l_ac].pmbw019 = l_pmbw019 + g_pmbw_d[l_ac].pmbw017 
         LET g_pmbw_d[l_ac].pmbw020 = l_pmbw020 + g_pmbw_d[l_ac].pmbw017 
      END IF
      IF NOT cl_null(g_pmbw_d[l_ac].pmbw018) THEN  
         LET g_pmbw_d[l_ac].pmbw019 = l_pmbw019 * (1+g_pmbw_d[l_ac].pmbw018/100)
         LET g_pmbw_d[l_ac].pmbw020 = l_pmbw020 * (1+g_pmbw_d[l_ac].pmbw018/100)        
      END IF   
      IF cl_null(g_pmbw_d[l_ac].pmbw017) AND cl_null(g_pmbw_d[l_ac].pmbw018) THEN
         LET g_pmbw_d[l_ac].pmbw019 = l_pmbw019  
         LET g_pmbw_d[l_ac].pmbw020 = l_pmbw020   
      END IF
      DISPLAY BY NAME g_pmbw_d[l_ac].pmbw019,g_pmbw_d[l_ac].pmbw020      
   END IF  
   
END FUNCTION
