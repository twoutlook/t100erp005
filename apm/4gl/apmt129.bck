################################################################################
PRIVATE FUNCTION apmt129_pmbw031_chk()
   DEFINE   l_cnt       LIKE type_t.num5
   DEFINE   l_msg       STRING

   LET l_cnt = 0
   IF NOT cl_null(g_pmbv_m.pmbvdocno) AND NOT cl_null(g_pmbw_d[l_ac].pmbw031) 
      AND NOT cl_null(g_pmbw_d[l_ac].pmbw013) THEN
      SELECT COUNT(*) INTO l_cnt FROM pmbw_t
       WHERE pmbwent = g_enterprise
         AND pmbwdocno = g_pmbv_m.pmbvdocno
         AND pmbw013 = g_pmbw_d_t.pmbw013
         AND pmbw033 = g_pmbw_d_t.pmbw031
         AND pmbw031 != g_pmbw_d_t.pmbw031

         
      IF l_cnt > 0 THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'apm-00399'
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()

         LET g_pmbw_d[l_ac].pmbw031 = g_pmbw_d_t.pmbw031
         LET g_pmbw_d[l_ac].pmbw013 = g_pmbw_d_t.pmbw013
         LET g_pmbw_d[l_ac].pmbw033 = g_pmbw_d_t.pmbw033
         CALL apmt129_pmbw031_ref()
         CALL apmt129_pmbw013_ref()
         CALL apmt129_pmbw033_ref()
         DISPLAY BY NAME g_pmbw_d[l_ac].pmbw031,g_pmbw_d[l_ac].pmbw031_desc      
         DISPLAY BY NAME g_pmbw_d[l_ac].pmbw013,g_pmbw_d[l_ac].pmbw013_desc
         DISPLAY BY NAME g_pmbw_d[l_ac].pmbw033,g_pmbw_d[l_ac].pmbw033_desc
         RETURN FALSE
      END IF  
   END IF 
   RETURN TRUE
END FUNCTION

