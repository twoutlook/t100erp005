################################################################################
PRIVATE FUNCTION apmt129_pmbw_upd(p_pmbw011,p_pmbw012,p_pmbw031,p_pmbw032,p_pmbw013,p_pmbw019,p_pmbw020)
#dorislai-20150628-modify----(S)
#PRIVATE FUNCTION apmt129_pmbw_upd(p_pmbw011,p_pmbw012,p_pmbw013,p_pmbw019,p_pmbw020)
#dorislai-20150628-modify----(E)
DEFINE  p_pmbw011             LIKE pmbw_t.pmbw011
DEFINE  p_pmbw012             LIKE pmbw_t.pmbw012
#dorislai-20150628-add---(S)
DEFINE  p_pmbw031             LIKE pmbw_t.pmbw031
DEFINE  p_pmbw032             LIKE pmbw_t.pmbw032
#dorislai-20150628-add---(E)
DEFINE  p_pmbw013             LIKE pmbw_t.pmbw013
DEFINE  p_pmbw019             LIKE pmbw_t.pmbw019
DEFINE  p_pmbw020             LIKE pmbw_t.pmbw020
DEFINE  l_sql                 STRING
DEFINE  sr                    DYNAMIC ARRAY OF RECORD
        pmbw011               LIKE pmbw_t.pmbw011,
        pmbw012               LIKE pmbw_t.pmbw012,
        #dorislai-20150628-add---(S)
        pmbw031               LIKE pmbw_t.pmbw031,
        pmbw032               LIKE pmbw_t.pmbw032,
        #dorislai-20150629-add---(E)
        pmbw013               LIKE pmbw_t.pmbw013,
        pmbw015               LIKE pmbw_t.pmbw015,
        pmbw016               LIKE pmbw_t.pmbw016,
        pmbw017               LIKE pmbw_t.pmbw017,
        pmbw018               LIKE pmbw_t.pmbw018
                              END RECORD
DEFINE l_ac                   LIKE type_t.num5
DEFINE l_i                    LIKE type_t.num5
DEFINE l_pmbw019              LIKE pmbw_t.pmbw019
DEFINE l_pmbw020              LIKE pmbw_t.pmbw020
   #dorislai-20150628-modify---(S)
#   LET l_sql =" SELECT pmbw011,pmbw012,pmbw013,pmbw015,pmbw016,pmbw017,pmbw018 FROM pmbw_t ",
#              "  WHERE pmbwent='",g_enterprise,"' AND pmbwdocno='",g_pmbv_m.pmbvdocno,"'",
#              "    AND pmbw013='",p_pmbw013,"' AND pmbw015='",p_pmbw011,"' AND pmbw016='",p_pmbw012,"'",
#              "    AND pmbw014 = 'Y'"
   LET l_sql =" SELECT pmbw011,pmbw012,pmbw031,pmbw032,pmbw013,pmbw015,pmbw016,pmbw017,pmbw018 FROM pmbw_t ",
              "  WHERE pmbwent='",g_enterprise,"' AND pmbwdocno='",g_pmbv_m.pmbvdocno,"'",
              "    AND pmbw013='",p_pmbw013,"'","    AND pmbw014 = 'Y'"
              
   IF NOT cl_null(p_pmbw011) THEN
      LET l_sql = l_sql clipped,"  AND pmbw015='",p_pmbw011,"'"
      IF NOT cl_null(p_pmbw012) THEN
         LET l_sql = l_sql clipped,"  AND pmbw016='",p_pmbw012,"'"
      END IF
   END IF
   IF NOT cl_null(p_pmbw031) THEN
      LET l_sql = l_sql clipped," AND pmbw033='",p_pmbw031,"'"
   END IF
   IF NOT cl_null(p_pmbw032) THEN
      LET l_sql = l_sql clipped," AND pmbw034='",p_pmbw032,"'"
   END IF
   #dorislai-20150628-modify---(E)
   PREPARE apmt129_pmbw_upd_pre FROM l_sql
   DECLARE apmt129_pmbw_upd_cs CURSOR FOR apmt129_pmbw_upd_pre
   LET l_ac = 1
   FOREACH apmt129_pmbw_upd_cs INTO sr[l_ac].*
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "FOREACH:"
         LET g_errparam.popup = TRUE
         CALL cl_err()

         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
   END FOREACH
   IF l_ac = 1 THEN
      RETURN
   END IF 
   FOR l_i = 1 TO l_ac -1 
      IF NOT cl_null(sr[l_i].pmbw017) THEN
         LET l_pmbw019 = p_pmbw019 + sr[l_i].pmbw017
         LET l_pmbw020 = p_pmbw020 + sr[l_i].pmbw017
      END IF 
      IF NOT cl_null(sr[l_i].pmbw018) THEN
         LET l_pmbw019 = p_pmbw019 * (1 + sr[l_i].pmbw018/100)
         LET l_pmbw020 = p_pmbw020 * (1 + sr[l_i].pmbw018/100)
      END IF
      UPDATE pmbw_t SET pmbw019 = l_pmbw019,pmbw020 = l_pmbw020 
       WHERE pmbwent = g_enterprise AND pmbwdocno=g_pmbv_m.pmbvdocno 
         AND pmbw011=sr[l_i].pmbw011 AND pmbw012=sr[l_i].pmbw012 
         AND pmbw031=sr[l_i].pmbw031 AND pmbw032=sr[l_i].pmbw032
         AND pmbw013=sr[l_i].pmbw013
      
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "upd pmbw_t"
         LET g_errparam.popup = TRUE
         CALL cl_err()

         RETURN
      END IF
      #dorislai-20150628-modify----(S)
#      CALL apmt129_pmbw_upd(sr[l_i].pmbw011,sr[l_i].pmbw012,sr[l_i].pmbw013,l_pmbw019,l_pmbw020)
      CALL apmt129_pmbw_upd(sr[l_i].pmbw011,sr[l_i].pmbw012,sr[l_i].pmbw031,sr[l_i].pmbw032,sr[l_i].pmbw013,l_pmbw019,l_pmbw020)
      #dorislai-20150628-modify----(E)
   END FOR
END FUNCTION
