            LET l_sql1 = l_sql1," AND pmaw013 = '",g_pmbw_d[l_ac].pmbw013,"'"
         END IF 
         PREPARE apmt129_pmbw015 FROM l_sql1
         EXECUTE apmt129_pmbw015 INTO l_pmaw015,l_pmawstus       
         LET l_errno = '' 
