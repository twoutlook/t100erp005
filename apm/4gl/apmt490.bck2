         LET g_detail_idx2 = g_pmeb3_d.getLength()
      END IF
      IF g_detail_idx2 = 0 AND g_pmeb3_d.getLength() <> 0 THEN
         LET g_detail_idx2 = 1
      END IF
      DISPLAY g_detail_idx2 TO FORMONLY.idx
