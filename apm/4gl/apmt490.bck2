# Return code....: r_success      TRUE/FALSE
#                : 
# Date & Author..: 2014/02/20 By stellar0130
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt490_ins_pmeb()
DEFINE l_i               LIKE type_t.num5
DEFINE r_success         LIKE type_t.num5

   LET r_success = TRUE
   
   FOR l_i = 1 TO g_pmeb_d.getLength()
      CALL s_apmt490_get_amount(g_pmea_m.pmeadocno,g_pmea_m.pmea900,
                                g_pmeb_d[l_i].pmebseq,g_pmeb_d[l_i].pmeb009,
                                g_pmeb_d[l_i].pmeb010,g_pmeb_d[l_i].pmeb011,
                                g_pmea_m.pmea005)
           RETURNING g_pmeb_d[l_i].pmeb017,g_pmeb_d[l_i].pmeb018,
                     g_pmeb_d[l_i].pmeb019
      INSERT INTO pmeb_t(pmebent,pmebsite,pmebdocno,pmeb900,pmebseq,pmeb001,pmeb002,pmeb003,
                         pmeb004,pmeb005,pmeb006,pmeb007,pmeb008,pmeb009,pmeb010,pmeb011,
                         pmeb012,pmeb013,pmeb014,pmeb017,pmeb018,pmeb019,pmeb020,pmeb021,
                         pmeb022,pmeb023,pmeb024,pmeb030,pmeb901)
         VALUES (g_enterprise,g_pmea_m.pmeasite,g_pmea_m.pmeadocno,g_pmea_m.pmea900,
                 g_pmeb_d[l_i].pmebseq,g_pmeb_d[l_i].pmeb001,g_pmeb_d[l_i].pmeb002,
                 g_pmeb_d[l_i].pmeb003,g_pmeb_d[l_i].pmeb004,g_pmeb_d[l_i].pmeb005,
                 g_pmeb_d[l_i].pmeb006,g_pmeb_d[l_i].pmeb007,g_pmeb_d[l_i].pmeb008,
                 g_pmeb_d[l_i].pmeb009,g_pmeb_d[l_i].pmeb010,g_pmeb_d[l_i].pmeb011,
                 g_pmeb_d[l_i].pmeb012,g_pmeb_d[l_i].pmeb013,g_pmeb_d[l_i].pmeb014,
                 g_pmeb_d[l_i].pmeb017,g_pmeb_d[l_i].pmeb018,g_pmeb_d[l_i].pmeb019,
                 g_pmeb2_d[l_i].pmeb020,g_pmeb2_d[l_i].pmeb021,g_pmeb2_d[l_i].pmeb022,
                 g_pmeb2_d[l_i].pmeb023,g_pmeb_d[l_i].pmeb024,g_pmeb_d[l_i].pmeb030,
                 g_pmeb_d[l_i].pmeb901)
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = 'ins pmeb_t'
         LET g_errparam.popup = TRUE
         CALL cl_err()

         LET r_success = FALSE
         EXIT FOR
      END IF
                    
   END FOR
   
   RETURN r_success
END FUNCTION
