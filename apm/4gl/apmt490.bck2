{<section id="apmt490.set_no_entry_b" >}
#+ 單身欄位關閉設定
PRIVATE FUNCTION apmt490_set_no_entry_b(p_cmd)
   #add-point:set_no_entry_b段define(客製用) name="set_no_entry_b.define_customerization"
   
   #end add-point    
   DEFINE p_cmd   LIKE type_t.chr1   
   #add-point:set_no_entry_b段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="set_no_entry_b.define"
   DEFINE l_n     LIKE type_t.num10
   #end add-point    
   
   #add-point:Function前置處理  name="set_no_entry_b.pre_function"
   
   #end add-point
   
   IF p_cmd = 'u' AND g_chkey = 'N' THEN
      CALL cl_set_comp_entry("",FALSE)
      #add-point:set_no_entry_b段欄位控制 name="set_no_entry_b.field_control"
      
      #end add-point 
   END IF 
   
   #add-point:set_no_entry_b段 name="set_no_entry_b.set_no_entry_b"
   #項次不可修改，避免歷程檔記錄問題
   IF p_cmd = 'u' THEN
      CALL cl_set_comp_entry("pmebseq",FALSE)
   END IF
   
   #若1.正常稅率，則單身稅別不可輸入
   IF g_tax_app = '1' THEN
      CALL cl_set_comp_entry("pmeb011",FALSE)
   END IF
   
   #若此採購合約已有轉採購單，則不可修改製程序、單位
   IF p_cmd = 'u' THEN
      LET l_n = 0
      SELECT COUNT(pmdn041) INTO l_n
        FROM pmdl_t,pmdn_t
       WHERE pmdlent = pmdnent
         AND pmdldocno = pmdndocno
         AND pmdlstus <> 'X'
         AND pmdn041 = g_pmea_m.pmeadocno
         AND pmdn042 = g_pmeb_d[l_ac].pmebseq
      IF NOT cl_null(l_n) AND l_n > 0 THEN
         CALL cl_set_comp_entry("pmeb002,pmeb005,pmeb006,pmeb007,pmeb008,pmeb011",FALSE)
      END IF
   END IF
   
   #當料件有使用產品特徵功能時，產品特徵才可輸入
   LET g_imaa005 = ''
   SELECT imaa005 INTO g_imaa005
     FROM imaa_t
    WHERE imaaent = g_enterprise
      AND imaa001 = g_pmeb_d[l_ac].pmeb002
   IF cl_null(g_imaa005) THEN
      LET g_pmeb_d[l_ac].pmeb003 = ''
      CALL cl_set_comp_entry("pmeb003",FALSE)
   END IF
   
   #當單頭的管制方式為'1:數量管制'時，未稅金額、含稅金額不可維護
   IF g_pmea_m.pmea018 = '1' THEN
      CALL cl_set_comp_entry("pmeb017,pmeb018",FALSE)
   END IF
   #end add-point     
END FUNCTION
 
