      #其說明的SQL
      LET g_pmed007 = "SELECT oofa011 FROM oofa_t WHERE oofaent ='"||g_enterprise||"' AND oofa002 = '2' AND oofa003 ='"||l_pmdx_m.pmdx002||"' "
      #新增資料到變更歷程檔
