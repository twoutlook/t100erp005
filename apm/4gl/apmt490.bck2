# Modify.........:
################################################################################
PRIVATE FUNCTION apmt490_ins_pmec()
DEFINE l_pmec_d          RECORD LIKE pmec_t.*
DEFINE r_success         LIKE type_t.num5

   LET r_success = TRUE
   
   DECLARE sel_pmdz_cur CURSOR FOR
    SELECT pmdzseq,pmdzseq1,pmdz001,pmdz002,pmdz003,pmdz004,pmdz005
      FROM pmdz_t
     WHERE pmdzent = g_enterprise
       AND pmdzdocno = g_pmea_m.pmeadocno
   
   FOREACH sel_pmdz_cur INTO l_pmec_d.pmecseq,l_pmec_d.pmecseq1,l_pmec_d.pmec001,
                             l_pmec_d.pmec002,l_pmec_d.pmec003,l_pmec_d.pmec004,
                             l_pmec_d.pmec005
                             
      LET l_pmec_d.pmec901 = 1  #未變更
      INSERT INTO pmec_t (pmecent,pmecsite,pmecdocno,pmec900,pmecseq,pmecseq1,
                          pmec001,pmec002,pmec003,pmec004,pmec005,pmec901)
         VALUES (g_enterprise,g_pmea_m.pmeasite,g_pmea_m.pmeadocno,g_pmea_m.pmea900,
                 l_pmec_d.pmecseq,l_pmec_d.pmecseq1,l_pmec_d.pmec001,l_pmec_d.pmec002,
                 l_pmec_d.pmec003,l_pmec_d.pmec004,l_pmec_d.pmec005,l_pmec_d.pmec901)
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = 'ins pmec_t'
         LET g_errparam.popup = TRUE
         CALL cl_err()

         LET r_success = FALSE
         EXIT FOREACH
      END IF
   END FOREACH
   
   RETURN r_success
END FUNCTION
