# Return code....: r_success   TRUE/FALSE
#                : 
# Date & Author..: 2014/02/18 By stellar0130
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt490_upd_pmea901()
DEFINE l_pmdx_m     RECORD LIKE pmdx_t.*
DEFINE l_flag       LIKE type_t.num5   #記錄是否有欄位變更
DEFINE r_success    LIKE type_t.num5
DEFINE l_ooef019    LIKE ooef_t.ooef019

   LET r_success = TRUE
   LET l_flag = FALSE
   
   #抓取採購合約單頭的值
   INITIALIZE l_pmdx_m.* TO NULL
   
   SELECT pmdx002,pmdx003,pmdx004,pmdx005,pmdx006,pmdx007,pmdx008,
          pmdx009,pmdx010,pmdx011,pmdx012,pmdx014,pmdx015,pmdx016,
          pmdx017,pmdx018,pmdx019,pmdx020,pmdx030
     INTO l_pmdx_m.pmdx002,l_pmdx_m.pmdx003,l_pmdx_m.pmdx004,
          l_pmdx_m.pmdx005,l_pmdx_m.pmdx006,l_pmdx_m.pmdx007,
