           pmeb008,pmeb009,pmeb010,pmeb024,pmeb011,pmeb012,pmeb017,pmeb018,pmeb019,pmeb013,pmeb004,pmeb030, 
           pmeb901,pmeb902,pmeb903 
      FROM pmeb_t 
     WHERE pmebent = g_enterprise
       AND pmebdocno = g_pmea_m.pmeadocno
       AND pmeb900 = g_pmea_m.pmea900
     ORDER BY pmebseq
   FOREACH apmt490_detail_chk_cs INTO l_pmeb_d.pmeb001,l_pmeb_d.pmebsite,l_pmeb_d.pmebseq, 
                                      l_pmeb_d.pmeb002,l_pmeb_d.pmeb003,l_pmeb_d.pmeb005,l_pmeb_d.pmeb006, 
                                      l_pmeb_d.pmeb007,l_pmeb_d.pmeb014,l_pmeb_d.pmeb008,l_pmeb_d.pmeb009, 
                                      l_pmeb_d.pmeb010,l_pmeb_d.pmeb024,l_pmeb_d.pmeb011,l_pmeb_d.pmeb012, 
                                      l_pmeb_d.pmeb017,l_pmeb_d.pmeb018,l_pmeb_d.pmeb019,l_pmeb_d.pmeb013, 
                                      l_pmeb_d.pmeb004,l_pmeb_d.pmeb030,l_pmeb_d.pmeb901,l_pmeb_d.pmeb902, 
                                      l_pmeb_d.pmeb903
      LET g_pmeb_d_t.* = l_pmeb_d.*
      CALL apmt490_pmeb_ins_pmed(l_pmeb_d.*)
           RETURNING r_success
      IF NOT r_success THEN
         EXIT FOREACH
      END IF
   END FOREACH
   
   RETURN r_success
END FUNCTION

 
