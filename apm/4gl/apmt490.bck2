PRIVATE FUNCTION apmt490_detail3_color()
DEFINE l_pmed002         LIKE pmed_t.pmed002

   CALL apmt490_detail3_init_color()
   
   DECLARE sel_pmed_cs3 CURSOR FOR
    SELECT pmed002 FROM pmed_t
     WHERE pmedent = g_enterprise
       AND pmeddocno = g_pmea_m.pmeadocno
       AND pmed001 = g_pmea_m.pmea900
       AND pmedseq = g_pmeb_d[g_detail_idx].pmebseq
       AND pmedseq1= g_pmeb3_d[l_ac].pmecseq1
   FOREACH sel_pmed_cs3 INTO l_pmed002
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "FOREACH:"
         LET g_errparam.popup = TRUE
         CALL cl_err()

         EXIT FOREACH
      END IF

      LET l_pmed002 = cl_replace_str(l_pmed002,'pmdz','pmec')
      
      CASE l_pmed002
         WHEN 'pmecseq1'
              LET g_pmeb3_d_color[l_ac].pmecseq1 = 'red'
         WHEN 'pmec001'
              LET g_pmeb3_d_color[l_ac].pmec001 = 'red'
         WHEN 'pmec002'
              LET g_pmeb3_d_color[l_ac].pmec002 = 'red'
         WHEN 'pmec003'
              LET g_pmeb3_d_color[l_ac].pmec003 = 'red'
         WHEN 'pmec004'
              LET g_pmeb3_d_color[l_ac].pmec004 = 'red'
         WHEN 'pmec005'
              LET g_pmeb3_d_color[l_ac].pmec005 = 'red'
      END CASE
