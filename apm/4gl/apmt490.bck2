{<section id="apmt490.set_no_entry" >}
#+ 單頭欄位關閉設定
PRIVATE FUNCTION apmt490_set_no_entry(p_cmd)
   #add-point:set_no_entry段define(客製用) name="set_no_entry.define_customerization"
   
   #end add-point     
   DEFINE p_cmd   LIKE type_t.chr1   
   #add-point:set_no_entry段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="set_no_entry.define"
                  
   #end add-point     
   
   #add-point:Function前置處理  name="set_no_entry.pre_function"
   
   #end add-point
   
   IF p_cmd = 'u' AND g_chkey = 'N' THEN
      CALL cl_set_comp_entry("pmeadocno,pmea900",FALSE)
      #根據azzi850使用者身分開關特定欄位
      IF NOT cl_null(g_no_entry) THEN
         CALL cl_set_comp_entry(g_no_entry,FALSE)
      END IF
      #add-point:set_no_entry段欄位控制 name="set_no_entry.field_control"
                                    
      #end add-point 
   END IF 
   
   IF p_cmd = 'u' THEN  #docno,ld欄位確認是絕對關閉
      CALL cl_set_comp_entry("pmeadocno",FALSE)
   END IF 
 
   IF p_cmd = 'u' THEN  #docdt欄位依照設定關閉(FALSE則為設定不同意修正)
      IF NOT cl_chk_update_docdt() THEN
         CALL cl_set_comp_entry("pmeadocdt",FALSE)
      END IF
   END IF 
   
   #add-point:set_no_entry段欄位控制後 name="set_no_entry.after_control"
                     #新增或修改，變更序都不可以輸入
   CALL cl_set_comp_entry("pmea900",FALSE)
   
   #此採購合約已有轉採購單
   IF g_carry = 'Y' THEN
      #供應商、合約對象管制、合約使用管制不可修改
      CALL cl_set_comp_entry("pmea004,pmea016,pmea017",FALSE)
      #若限定幣別='Y'，幣別不可修改
      IF g_pmea_m.pmea019 = 'Y' THEN
         CALL cl_set_comp_entry("pmea005",FALSE)
      END IF
      #若限定稅別='Y'，稅別不可修改
      IF g_pmea_m.pmea020 = 'Y' THEN
         CALL cl_set_comp_entry("pmea006",FALSE)
      END IF
      #若限定付款條件='Y'，付款條件不可修改
      IF g_pmea_m.pmea010 = 'Y' THEN
         CALL cl_set_comp_entry("pmea009",FALSE)
      END IF
      #若限定交易條件='Y'，交易條件不可修改
      IF g_pmea_m.pmea012 = 'Y' THEN
         CALL cl_set_comp_entry("pmea011",FALSE)
      END IF
   END IF
   
   IF cl_null(g_pmea_m.pmea009) THEN
      CALL cl_set_comp_entry("pmea010",FALSE)
      LET g_pmea_m.pmea010 = 'N'
      DISPLAY BY NAME g_pmea_m.pmea010
   END IF
   
   IF cl_null(g_pmea_m.pmea011) THEN
      CALL cl_set_comp_entry("pmea012",FALSE)
      LET g_pmea_m.pmea012 = 'N'
      DISPLAY BY NAME g_pmea_m.pmea012
   END IF
   #end add-point 
 
END FUNCTION
 
