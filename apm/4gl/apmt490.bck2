             pmec903  FROM pmec_t",    
                     "",
                     
                     " WHERE pmecent=? AND pmecdocno=? AND pmec900=? AND pmecseq=?"
         
         IF NOT cl_null(g_wc2_table2) THEN
            LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
         END IF
         
         LET g_sql = g_sql, " ORDER BY  pmec_t.pmecseq1" 
                            
         #add-point:單身填充前 name="b_fill2.before_fill2"
         IF cl_null(g_detail_idx) OR g_detail_idx = 0 THEN
            RETURN
         END IF            
         #end add-point
         
         #先清空資料
               CALL g_pmeb3_d.clear()
 
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE apmt490_pb2 FROM g_sql
         DECLARE b_fill_curs2 CURSOR FOR apmt490_pb2
         
         OPEN b_fill_curs2 USING g_enterprise,g_pmea_m.pmeadocno,g_pmea_m.pmea900,g_pmeb_d[g_detail_idx].pmebseq 
 
         LET l_ac = 1
         FOREACH b_fill_curs2 INTO g_pmeb3_d[l_ac].pmecsite,g_pmeb3_d[l_ac].pmecseq1,g_pmeb3_d[l_ac].pmec001, 
             g_pmeb3_d[l_ac].pmec002,g_pmeb3_d[l_ac].pmec003,g_pmeb3_d[l_ac].pmec004,g_pmeb3_d[l_ac].pmec005, 
             g_pmeb3_d[l_ac].pmec902,g_pmeb3_d[l_ac].pmec903 
            IF SQLCA.sqlcode THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               EXIT FOREACH
            END IF
            
            #add-point:b_fill段資料填充 name="b_fill2.fill2"
            CALL apmt490_acc_ref(g_acc2,g_pmeb3_d[l_ac].pmec902)
                 RETURNING g_pmeb3_d[l_ac].pmec902_desc
            CALL apmt490_detail3_color()                                  
            #end add-point
           
            IF l_ac > g_max_rec THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               EXIT FOREACH
            END IF
            
            LET l_ac = l_ac + 1
            
         END FOREACH
               CALL g_pmeb3_d.deleteElement(g_pmeb3_d.getLength())
 
      END IF
   END IF
 
 
      
   LET g_pmeb3_d_mask_o.* =  g_pmeb3_d.*
   FOR l_ac = 1 TO g_pmeb3_d.getLength()
      LET g_pmeb3_d_mask_o[l_ac].* =  g_pmeb3_d[l_ac].*
      CALL apmt490_pmec_t_mask()
      LET g_pmeb3_d_mask_n[l_ac].* =  g_pmeb3_d[l_ac].*
   END FOR
 
      
   #add-point:單身填充後 name="b_fill2.after_fill"
            
   #end add-point
    
   LET l_ac = li_ac
   
   CALL apmt490_detail_show()
   
   LET g_detail_idx = li_detail_idx_tmp
   
END FUNCTION
 
