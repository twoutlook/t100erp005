   #累計稅額
   IF p_pmeb_d.pmeb901 = '3' THEN
      #其說明的SQL
      LET g_pmed007 = ''
      #新增資料到變更歷程檔
      IF NOT apmt490_ins_pmed(l_pmedseq,0,"pmdy023",p_pmeb_d.pmeb901,l_pmdy.pmdy023,0) THEN
         LET r_success = FALSE
         RETURN r_success
      END IF
      LET l_flag = TRUE
   ELSE
      #資料一樣，表示無變更，將變更歷程檔刪除
      IF NOT apmt490_del_pmed(l_pmedseq,0,"pmdy023") THEN
         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF
   
   #累計量定價
   IF p_pmeb_d.pmeb901 = '3' OR (p_pmeb_d.pmeb901 = '2' AND 
      ((p_pmeb_d.pmeb024 <> l_pmdy.pmdy024) OR
      (p_pmeb_d.pmeb024 IS NULL AND l_pmdy.pmdy024 IS NOT NULL) OR
      (p_pmeb_d.pmeb024 IS NOT NULL AND l_pmdy.pmdy024 IS NULL))) THEN
      #其說明的SQL
      LET g_pmed007 = ''
      #新增資料到變更歷程檔
      IF NOT apmt490_ins_pmed(l_pmedseq,0,"pmdy024",p_pmeb_d.pmeb901,l_pmdy.pmdy024,p_pmeb_d.pmeb024) THEN
         LET r_success = FALSE
         RETURN r_success
      END IF
      LET l_flag = TRUE
   ELSE
      #資料一樣，表示無變更，將變更歷程檔刪除
      IF NOT apmt490_del_pmed(l_pmedseq,0,"pmdy024") THEN
         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF
   
   #表示此筆單身與採購合約單該筆單身一致
   IF l_flag = FALSE THEN
      UPDATE pmeb_t SET pmeb901 = '1' 
       WHERE pmebent = g_enterprise
         AND pmebdocno = g_pmea_m.pmeadocno
         AND pmeb900 = g_pmea_m.pmea900
         AND pmebseq = p_pmeb_d.pmebseq
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = 'pmeb_t'
         LET g_errparam.popup = TRUE
         CALL cl_err()

         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF
   
   RETURN r_success
      
END FUNCTION
