################################################################################
# Descriptions...: 依據合約單號+第一筆項次，帶出累計量計價單身
# Memo...........:
# Usage..........: CALL apmt490_pmeadocno_pmec()
#                  
# Input parameter: 
#                : 
# Return code....: 
#                : 
# Date & Author..: 2014/03/11 By stellar0130
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt490_pmeadocno_pmec()
DEFINE l_sql             STRING

   IF cl_null(g_pmea_m.pmeadocno) THEN
      RETURN 
   END IF
   
   IF cl_null(g_pmeb_d[1].pmebseq) THEN
      CALL g_pmeb_d.deleteElement(1)
      RETURN
   END IF
   
   CALL g_pmeb3_d.clear()
   IF g_pmeb_d[1].pmeb024 = 'N' THEN
      RETURN
   END IF
   
   LET l_sql = "SELECT pmdzseq1,pmdz001,pmdz002,pmdz003,pmdz004,pmdz005 ",
               "  FROM pmdz_t ",
               " WHERE pmdzent = '",g_enterprise,"'",
               "   AND pmdzdocno = '",g_pmea_m.pmeadocno,"'",
               "   AND pmdzseq = ",g_pmeb_d[1].pmebseq,
               " ORDER BY pmdzseq1 "
   PREPARE sel_pmdz_pre1 FROM l_sql
   DECLARE sel_pmdz_cur1 CURSOR FOR sel_pmdz_pre1
   
   LET l_ac = 1
   FOREACH sel_pmdz_cur1 INTO g_pmeb3_d[l_ac].pmecseq1,g_pmeb3_d[l_ac].pmec001,
                              g_pmeb3_d[l_ac].pmec002,g_pmeb3_d[l_ac].pmec003,
                              g_pmeb3_d[l_ac].pmec004,g_pmeb3_d[l_ac].pmec005
           
      
      LET l_ac = l_ac+1
      IF l_ac > g_max_rec THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 9035
         LET g_errparam.extend = ''
         LET g_errparam.popup = FALSE
         CALL cl_err()

         EXIT FOREACH
      END IF
   END FOREACH
   
   CALL g_pmeb3_d.deleteElement(g_pmeb_d.getLength())
   FREE sel_pmdz_pre1
   
   DISPLAY ARRAY g_pmeb3_d TO s_detail3.* ATTRIBUTES(COUNT=l_ac)
      BEFORE DISPLAY 
         EXIT DISPLAY
   END DISPLAY
   
   LET l_ac = 0
   
   RETURN 
END FUNCTION

