{<section id="apmt490.ui_headershow" >}
#+ 單頭資料重新顯示
PRIVATE FUNCTION apmt490_ui_headershow()
   #add-point:ui_headershow段define(客製用) name="ui_headershow.define_customerization"
   
   #end add-point  
   #add-point:ui_headershow段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="ui_headershow.define"
                  
   #end add-point      
   
   #add-point:Function前置處理  name="ui_headershow.pre_function"
   
   #end add-point
   
   LET g_pmea_m.pmeadocno = g_browser[g_current_idx].b_pmeadocno   
   LET g_pmea_m.pmea900 = g_browser[g_current_idx].b_pmea900   
 
   EXECUTE apmt490_master_referesh USING g_pmea_m.pmeadocno,g_pmea_m.pmea900 INTO g_pmea_m.pmea001,g_pmea_m.pmea901, 
