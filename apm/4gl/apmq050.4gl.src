{<section id="apmq050.b_fill2" >}
#+ 單身陣列填充2
PRIVATE FUNCTION apmq050_b_fill2()
   #add-point:b_fill2段define-客製 name="b_fill2.define_customerization"
   
   #end add-point
   DEFINE li_ac           LIKE type_t.num10
   #add-point:b_fill2段define-標準  (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill2.define"
   DEFINE l_sql           STRING
   DEFINE l_i             LIKE type_t.num10
   DEFINE l_success       LIKE type_t.num5  
   DEFINE l_pmdl004       LIKE pmdl_t.pmdl004
   #end add-point
 
   #add-point:FUNCTION前置處理 name="b_fill2.before_function"
   
   #end add-point
 
   LET li_ac = l_ac
 
   #單身組成
   #應用 qs07 樣板自動產生(Version:7)
   #+ b_fill2段table資料取得(包含sql組成及資料填充)
 
   #add-point:陣列清空 name="b_fill2.array_clear"
   CALL g_pmdn2_d.clear()

   LET l_sql = "SELECT DISTINCT pmdl004 ",
               "  FROM apmq050_tmp02 ",  #160727-00019#12   16/08/03 By 08734 临时表长度超过15码的减少到15码以下 apmq050_apmt500_t ——> apmq050_tmp02
               " WHERE pmdn001 = '",g_pmdl_d[l_ac].pmdn001,"' ",
               "   AND pmdl015 = '",g_pmdl_d[l_ac].pmdl015,"' "
   PREPARE apmq050_b_fill2_pmdl004_prep FROM l_sql
   DECLARE apmq050_b_fill2_pmdl004_curs CURSOR FOR apmq050_b_fill2_pmdl004_prep

   LET l_i = 1
   FOREACH apmq050_b_fill2_pmdl004_curs INTO l_pmdl004
      #最低單價 
      IF g_master.radio = '1' THEN
         LET l_sql = "SELECT pmdldocno,pmdldocdt,pmdl004,'',pmdl002,'',pmdl003,'', ",
                     "       pmdnseq,pmdn019,pmdn002,'',pmdn006,'',pmdn007,pmdn0152,pmdn015 ",
                     "  FROM apmq050_tmp02 ",  #160727-00019#12   16/08/03 By 08734 临时表长度超过15码的减少到15码以下 apmq050_apmt500_t ——> apmq050_tmp02
                     " WHERE pmdn001 = '",g_pmdl_d[l_ac].pmdn001,"' ",
                     "   AND pmdl015 = '",g_pmdl_d[l_ac].pmdl015,"' ",
                     "   AND pmdl004 = '",l_pmdl004,"' ",
                     "   AND pmdn015 = (SELECT MIN(pmdn015) ",
                     "                    FROM apmq050_tmp02 ",  #160727-00019#12   16/08/03 By 08734 临时表长度超过15码的减少到15码以下 apmq050_apmt500_t ——> apmq050_tmp02
                     "                   WHERE pmdn001 = '",g_pmdl_d[l_ac].pmdn001,"' ",
                     "                     AND pmdl015 = '",g_pmdl_d[l_ac].pmdl015,"' ",
                     "                     AND pmdl004 = '",l_pmdl004,"' ) "
      END IF

      #平均單價 
      IF g_master.radio = '2' THEN
         LET l_sql = "SELECT pmdldocno,pmdldocdt,pmdl004,'',pmdl002,'',pmdl003,'', ",
                     "       pmdnseq,pmdn019,pmdn002,'',pmdn006,'',pmdn007,pmdn0152,pmdn015 ",
                     "  FROM apmq050_tmp02 ",  #160727-00019#12   16/08/03 By 08734 临时表长度超过15码的减少到15码以下 apmq050_apmt500_t ——> apmq050_tmp02
                     " WHERE pmdn001 = '",g_pmdl_d[l_ac].pmdn001,"' ", 
                     "   AND pmdl015 = '",g_pmdl_d[l_ac].pmdl015,"' ",
                     "   AND pmdl004 = '",l_pmdl004,"' "
      END IF

      #最高單價 
      IF g_master.radio = '3' THEN
         LET l_sql = "SELECT pmdldocno,pmdldocdt,pmdl004,'',pmdl002,'',pmdl003,'', ",
                     "       pmdnseq,pmdn019,pmdn002,'',pmdn006,'',pmdn007,pmdn0152,pmdn015 ",
                     "  FROM apmq050_tmp02 ",  #160727-00019#12   16/08/03 By 08734 临时表长度超过15码的减少到15码以下 apmq050_apmt500_t ——> apmq050_tmp02
                     " WHERE pmdn001 = '",g_pmdl_d[l_ac].pmdn001,"' ",
                     "   AND pmdl015 = '",g_pmdl_d[l_ac].pmdl015,"' ",
                     "   AND pmdl004 = '",l_pmdl004,"' ",
                     "   AND pmdn015 = (SELECT MAX(pmdn015) ",
                     "                    FROM apmq050_tmp02 ",  #160727-00019#12   16/08/03 By 08734 临时表长度超过15码的减少到15码以下 apmq050_apmt500_t ——> apmq050_tmp02
                     "                   WHERE pmdn001 = '",g_pmdl_d[l_ac].pmdn001,"' ",
                     "                     AND pmdl015 = '",g_pmdl_d[l_ac].pmdl015,"' ",
                     "                     AND pmdl004 = '",l_pmdl004,"' ) "
      END IF

      #最近單價 
      IF g_master.radio = '4' THEN
         LET l_sql = "SELECT pmdldocno,pmdldocdt,pmdl004,'',pmdl002,'',pmdl003,'', ",
                     "       pmdnseq,pmdn019,pmdn002,'',pmdn006,'',pmdn007,pmdn0152,pmdn015 ",
                     "  FROM apmq050_tmp02 ",  #160727-00019#12   16/08/03 By 08734 临时表长度超过15码的减少到15码以下 apmq050_apmt500_t ——> apmq050_tmp02
                     " WHERE pmdn001 = '",g_pmdl_d[l_ac].pmdn001,"' ",
                     "   AND pmdl015 = '",g_pmdl_d[l_ac].pmdl015,"' ",
                     "   AND pmdl004 = '",l_pmdl004,"' ",
                     "   AND pmdldocdt = (SELECT MAX(pmdldocdt) ",
                     "                    FROM apmq050_tmp02 ",  #160727-00019#12   16/08/03 By 08734 临时表长度超过15码的减少到15码以下 apmq050_apmt500_t ——> apmq050_tmp02
                     "                   WHERE pmdn001 = '",g_pmdl_d[l_ac].pmdn001,"' ", 
                     "                     AND pmdl015 = '",g_pmdl_d[l_ac].pmdl015,"' ",
                     "                     AND pmdl004 = '",l_pmdl004,"' ",
                     "                     AND pmdldocdt <= '",g_today,"') "
      END IF

      PREPARE apmq050_b_fill2_prep FROM l_sql
      DECLARE apmq050_b_fill2_curs CURSOR FOR apmq050_b_fill2_prep

      FOREACH apmq050_b_fill2_curs INTO g_pmdn2_d[l_i].pmdldocno,g_pmdn2_d[l_i].pmdldocdt,
                                        g_pmdn2_d[l_i].pmdl004,g_pmdn2_d[l_i].pmdl004_desc,
                                        g_pmdn2_d[l_i].pmdl002,g_pmdn2_d[l_i].pmdl002_desc,
                                        g_pmdn2_d[l_i].pmdl003,g_pmdn2_d[l_i].pmdl003_desc,
                                        g_pmdn2_d[l_i].pmdnseq,g_pmdn2_d[l_i].pmdn019,
                                        g_pmdn2_d[l_i].pmdn002,g_pmdn2_d[l_i].pmdn002_desc,
                                        g_pmdn2_d[l_i].pmdn006,g_pmdn2_d[l_i].pmdn006_desc,
                                        g_pmdn2_d[l_i].pmdn007,g_pmdn2_d[l_i].pmdn015, 
                                        g_pmdn2_d[l_i].pmdn015_1

         CALL s_desc_get_trading_partner_abbr_desc(g_pmdn2_d[l_i].pmdl004)
              RETURNING g_pmdn2_d[l_i].pmdl004_desc

         CALL s_desc_get_person_desc(g_pmdn2_d[l_i].pmdl002)
              RETURNING g_pmdn2_d[l_i].pmdl002_desc

         CALL s_desc_get_department_desc(g_pmdn2_d[l_i].pmdl003)
              RETURNING g_pmdn2_d[l_i].pmdl003_desc

