#      RETURN
#   END IF
#   
#   #對話框多顯示交易對象.說明
#   SELECT pmba001 INTO l_pmba001
#     FROM pmba_t
#    WHERE pmbaent = g_enterprise
#      AND pmbadocno = l_pmbadocno
#      
#   SELECT pmbal003 INTO l_pmba001_desc
#     FROM pmbal_t
#    WHERE pmbalent = g_enterprise
#      AND pmbaldocno = l_pmbadocno
#      AND pmbal001 = g_dlang

   IF g_pmba_m.pmba000 = 'I' THEN
      DECLARE apmt100_pmba003_ref_cs CURSOR FOR
       SELECT pmaa001 
         FROM pmaa_t
        WHERE pmaaent = g_enterprise
          AND pmaa003 = g_pmba_m.pmba003
        ORDER BY pmaa001
      FOREACH apmt100_pmba003_ref_cs INTO l_pmba001
         EXIT FOREACH
      END FOREACH
   ELSE
      DECLARE apmt100_pmba003_ref_cs1 CURSOR FOR
       SELECT pmaa001 
         FROM pmaa_t
        WHERE pmaaent = g_enterprise
          AND (pmaa001 <> g_pmba_m.pmba001 OR g_pmba_m.pmba001 IS NULL)
          AND pmaa003 = g_pmba_m.pmba003
        ORDER BY pmaa001
      FOREACH apmt100_pmba003_ref_cs1 INTO l_pmba001
         EXIT FOREACH
      END FOREACH
   END IF
   
   IF cl_null(l_pmba001) OR (l_pmba001 = g_pmba_m.pmba001) THEN
      RETURN
   END IF
   
   SELECT pmaal004 INTO l_pmba001_desc
     FROM pmaal_t
    WHERE pmaalent = g_enterprise
      AND pmaal001 = l_pmba001
      AND pmaal002 = g_dlang
   #2015/10/14 by stellar modify ----- (E)
    
   LET l_pmba001_desc = l_pmba001 CLIPPED,".",l_pmba001_desc CLIPPED
   
   CALL cl_getmsg_parm('apm-01001',g_dlang,l_pmba001_desc)
        RETURNING g_pmba_m.pmba003_desc
        
   DISPLAY BY NAME g_pmba_m.pmba003_desc
END FUNCTION
