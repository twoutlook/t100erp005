PRIVATE FUNCTION apmt100_pmbadocno_chk(p_pmbadocno)
DEFINE p_pmbadocno  LIKE pmba_t.pmbadocno
DEFINE l_ooef004    LIKE ooef_t.ooef004
DEFINE l_stus       LIKE pmba_t.pmbastus
DEFINE r_success    LIKE type_t.num5
      
      LET l_ooef004 = ''
      LET l_stus = ''
      LET r_success = TRUE
      
      LET g_errno = ''

      SELECT ooef004 INTO l_ooef004 FROM ooef_t
       WHERE ooef001 = g_site
         AND ooefent = g_enterprise
      
      SELECT oobastus INTO l_stus 
        FROM ooba_t
       WHERE oobaent = g_enterprise
         AND ooba001 = l_ooef004
         AND ooba002 = p_pmbadocno
      
      CASE
         WHEN SQLCA.sqlcode = 100  LET g_errno = 'aim-00056'
         WHEN l_stus <> 'Y'        LET g_errno = 'aim-00057'
         OTHERWISE                 LET g_errno = SQLCA.sqlcode USING '------'
      END CASE

      IF NOT cl_null(g_errno) THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = g_errno
         LET g_errparam.extend = p_pmbadocno
         LET g_errparam.popup = TRUE
         CALL cl_err()

         LET r_success = FALSE
         RETURN r_success
      END IF
      RETURN r_success
      
