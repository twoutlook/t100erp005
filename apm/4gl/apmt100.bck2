   #160818-00017#26--s
   SELECT pmbastus INTO g_pmba_m.pmbastus FROM pmba_t WHERE pmbaent = g_enterprise AND pmbadocno = g_pmba_m.pmbadocno
   IF (g_action_choice="modify" OR g_action_choice="delete" OR g_action_choice="modify_detail")  THEN
