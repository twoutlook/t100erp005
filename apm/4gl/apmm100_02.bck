#+
PUBLIC FUNCTION apmm100_02_b_fill(p_pmaa001)
   DEFINE p_pmaa001   LIKE pmaa_t.pmaa001
   DEFINE l_sql       STRING

   DEFINE l_ac1       LIKE type_t.num5
   
   LET l_sql = "SELECT pmafstus,pmaf001,pmaf002,'',pmaf003,pmaf004,pmaf008,pmaf009,pmaf005,pmaf007,pmaf006,pmaf010 FROM pmaf_t WHERE pmafent = '",g_enterprise,"' AND pmaf001 = '",p_pmaa001,"' "
   IF NOT cl_null(g_wc2_m10002) THEN
      LET l_sql = l_sql CLIPPED, " AND ",g_wc2_m10002 CLIPPED
   END IF
   PREPARE apmm100_02_pb FROM l_sql
   
   DECLARE b_fill_curs CURSOR FOR apmm100_02_pb

   CALL g_pmaf_d.clear()
   LET l_ac1 = 1
   FOREACH b_fill_curs INTO g_pmaf_d[l_ac1].*
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "FOREACH:"
         LET g_errparam.popup = TRUE
         CALL cl_err()

         EXIT FOREACH
      END IF
      
      CALL apmm100_02_pmaf002_ref(g_pmaf_d[l_ac1].pmaf002) RETURNING g_pmaf_d[l_ac1].pmaf002_desc
          
      LET l_ac1 = l_ac1 + 1
      IF l_ac1 > g_max_rec THEN
         #CALL cl_err( "", 9035, 0 )
         EXIT FOREACH
      END IF

   END FOREACH
   CALL g_pmaf_d.deleteElement(g_pmaf_d.getLength())
   LET g_rec_b = l_ac1 - 1
   DISPLAY g_rec_b TO FORMONLY.cnt
   LET g_pmaa2_d.* = g_pmaf_d.*
   #將總筆數, 目前筆數指定到共用變數中
   IF g_rec_b > 0 THEN
      LET g_d_idx_m10002 = 1
   ELSE
      LET g_d_idx_m10002 = 0
   END IF
   LET g_d_cnt_m10002 = g_rec_b
   
   CLOSE b_fill_curs
   FREE apmm100_02_pb
   
END FUNCTION
