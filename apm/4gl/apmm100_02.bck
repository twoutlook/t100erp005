DIALOG apmm100_02_display()
   
   DISPLAY ARRAY g_pmaf_d TO s_detail1_apmm100_02.* ATTRIBUTE(COUNT=g_d_cnt_m10002)

      BEFORE DISPLAY
         CALL FGL_SET_ARR_CURR(g_d_idx_m10002)
