         AFTER FIELD pmaf009
            IF NOT cl_null(g_pmaf_d[l_ac].pmaf001) AND NOT cl_null(g_pmaf_d[l_ac].pmaf009) AND NOT cl_null(g_pmaf_d[l_ac].pmaf003) THEN 
