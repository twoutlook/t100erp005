################################################################################
# Descriptions...: 被主程式嵌入的單身輸入模式
# Memo...........: 
# Usage..........: CALL apmm100_02_input()
# Input parameter: None
# Return code....: None
# Date & Author..: 2014/09/24 By lixiang
# Modify.........:
################################################################################
DIALOG apmm100_02_input()
   DEFINE l_ac_t          LIKE type_t.num5        #未取消的ARRAY CNT   
   DEFINE l_count         LIKE type_t.num5
   DEFINE l_insert        LIKE type_t.num5
   DEFINE l_cmd           LIKE type_t.chr5
   DEFINE l_lock_sw       LIKE type_t.chr1
   DEFINE l_forupd_sql    STRING
   DEFINE l_n             LIKE type_t.num5                #檢查重複用  
   DEFINE l_cnt           LIKE type_t.num5
   
   
   
   INPUT ARRAY g_pmaf_d FROM s_detail1_apmm100_02.*
          ATTRIBUTE(COUNT = g_d_cnt_m10002,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = FALSE,
                  DELETE ROW = g_detail_delete,
                  APPEND ROW = g_detail_insert)

         BEFORE INPUT
            LET l_forupd_sql = "SELECT pmafstus,pmaf001,pmaf002,'',pmaf003,pmaf004,pmaf008,pmaf009,pmaf005,pmaf007,pmaf006,pmaf010 FROM pmaf_t WHERE pmafent = ? AND pmaf001 = ? AND pmaf002 = ? AND pmaf003 = ? FOR UPDATE"
            LET l_forupd_sql = cl_sql_forupd(l_forupd_sql)
            PREPARE apmm100_02_dialog_b FROM l_forupd_sql
            DECLARE apmm100_02_dialog_bcl CURSOR FOR apmm100_02_dialog_b
            IF g_appoint_idx > 0 THEN
               CALL DIALOG.setCurrentRow("s_detail1_apmm100_02",g_appoint_idx)
            END IF
            
         BEFORE ROW 
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET g_detail_idx = l_ac
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
         
            CALL s_transaction_begin()
   
            LET g_rec_b = g_pmaf_d.getLength()
            
            IF g_rec_b >= l_ac 
               AND NOT cl_null(g_pmaf_d[l_ac].pmaf001)
               AND NOT cl_null(g_pmaf_d[l_ac].pmaf002) 
               AND NOT cl_null(g_pmaf_d[l_ac].pmaf003) 
 
 
            THEN
               LET l_cmd='u'
			      LET g_pmaf_d_t.* = g_pmaf_d[l_ac].*  #BACKUP
			   
			      OPEN apmm100_02_dialog_bcl USING g_enterprise,g_pmaa001_d,g_pmaf_d[l_ac].pmaf002,g_pmaf_d[l_ac].pmaf003
               IF SQLCA.sqlcode THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = SQLCA.sqlcode
                  LET g_errparam.extend = "apmm100_02_bcl"
                  LET g_errparam.popup = TRUE
                  CALL cl_err()

                  LET l_lock_sw='Y'
               ELSE
                  FETCH apmm100_02_dialog_bcl INTO g_pmaf_d[l_ac].pmafstus,g_pmaf_d[l_ac].pmaf001,g_pmaf_d[l_ac].pmaf002,g_pmaf_d[l_ac].pmaf002_desc,g_pmaf_d[l_ac].pmaf003,g_pmaf_d[l_ac].pmaf004,g_pmaf_d[l_ac].pmaf008,g_pmaf_d[l_ac].pmaf009,g_pmaf_d[l_ac].pmaf005,g_pmaf_d[l_ac].pmaf007,g_pmaf_d[l_ac].pmaf006
                  
                  IF SQLCA.sqlcode THEN
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code = SQLCA.sqlcode
                     LET g_errparam.extend = g_pmaa001_d
                     LET g_errparam.popup = TRUE
                     CALL cl_err()

                     LET l_lock_sw = "Y"
                  END IF
				      CALL apmm100_02_pmaf002_ref(g_pmaf_d[l_ac].pmaf002) RETURNING g_pmaf_d[l_ac].pmaf002_desc
                  
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
        
         BEFORE INSERT
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_pmaf_d[l_ac].* TO NULL 
            
            LET g_pmaf_d[l_ac].pmaf001 = g_pmaa001_d
            LET g_pmaf_d[l_ac].pmafstus = 'Y'
            LET g_pmaf_d[l_ac].pmaf008 = 'N'
            LET g_pmaf_d[l_ac].pmaf009 = 'N'
            
            LET g_pmaf_d_t.* = g_pmaf_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            
            
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 9001
               LET g_errparam.extend = ''
               LET g_errparam.popup = FALSE
               CALL cl_err()

               LET INT_FLAG = 0
               #CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM pmaf_t 
             WHERE pmafent = g_enterprise AND pmaf001 = g_pmaf_d[l_ac].pmaf001
               AND pmaf002 = g_pmaf_d[l_ac].pmaf002
               AND pmaf003 = g_pmaf_d[l_ac].pmaf003
      
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               INSERT INTO pmaf_t (pmafent,pmaf001,pmaf002,pmaf003,pmaf004,pmaf005,pmaf006,pmaf007,pmaf008,pmaf009,pmafstus,pmaf010)
                  VALUES (g_enterprise,g_pmaf_d[l_ac].pmaf001,g_pmaf_d[l_ac].pmaf002,g_pmaf_d[l_ac].pmaf003,g_pmaf_d[l_ac].pmaf004,g_pmaf_d[l_ac].pmaf005,g_pmaf_d[l_ac].pmaf006,g_pmaf_d[l_ac].pmaf007,g_pmaf_d[l_ac].pmaf008,g_pmaf_d[l_ac].pmaf009,g_pmaf_d[l_ac].pmafstus,g_pmaf_d[l_ac].pmaf010)
            ELSE    
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = "std-00006"
               LET g_errparam.extend = 'INSERT'
               LET g_errparam.popup = TRUE
               CALL cl_err()

               INITIALIZE g_pmaf_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               #CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = SQLCA.sqlcode
               LET g_errparam.extend = "pmaf_t"
               LET g_errparam.popup = TRUE
               CALL cl_err()
  
               CALL s_transaction_end('N','0')                    
               #CANCEL INSERT
            ELSE
               CALL s_transaction_end('Y','0')
               ERROR 'INSERT O.K'
               LET g_rec_b = g_rec_b + 1
            END IF
              
         BEFORE DELETE                            #是否取消單身
            IF NOT cl_null(g_pmaf_d[l_ac].pmaf001)
               AND NOT cl_null(g_pmaf_d[l_ac].pmaf002) 
               AND NOT cl_null(g_pmaf_d[l_ac].pmaf003) THEN 
               
               IF cl_ask_del_detail() THEN
                  IF l_lock_sw = "Y" THEN
                     INITIALIZE g_errparam TO NULL
