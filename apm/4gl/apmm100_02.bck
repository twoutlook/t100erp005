DEFINE r_success      LIKE type_t.num5

       LET l_n = 0
       LET r_success = TRUE       
       SELECT COUNT(*) INTO l_n FROM pmaf_t WHERE pmafent = g_enterprise AND pmaf001 = g_pmaf_d[l_ac].pmaf001 AND pmaf008 = 'Y'
       #同一個銀和帳號已勾選了主要收款帳戶，不可再次勾選
       IF l_n > 0 THEN
          INITIALIZE g_errparam TO NULL
