               LET g_errparam.code = -263
               LET g_errparam.extend = g_pmaf_d[l_ac].pmaf002
               LET g_errparam.popup = TRUE
               CALL cl_err()

               LET g_pmaf_d[l_ac].* = g_pmaf_d_t.*
