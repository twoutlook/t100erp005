                  CALL s_transaction_end('N','0')
               ELSE
                  CALL s_transaction_end('Y','0')
               END IF
            END IF
            
         AFTER ROW
            CLOSE apmm100_02_dialog_bcl
            CALL s_transaction_end('Y','0')

         #此段落由子樣板a02產生
         AFTER FIELD pmaf002
            #確認資料無重複
            CALL apmm100_02_pmaf002_ref(g_pmaf_d[l_ac].pmaf002) RETURNING g_pmaf_d[l_ac].pmaf002_desc
