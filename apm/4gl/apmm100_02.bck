               UPDATE pmaf_t SET (pmaf002,pmaf003,pmaf004,pmaf005,pmaf006,pmaf007,pmaf008,pmaf009,pmafstus,pmaf010) = (g_pmaf_d[l_ac].pmaf002,g_pmaf_d[l_ac].pmaf003,g_pmaf_d[l_ac].pmaf004,g_pmaf_d[l_ac].pmaf005,g_pmaf_d[l_ac].pmaf006,g_pmaf_d[l_ac].pmaf007,g_pmaf_d[l_ac].pmaf008,g_pmaf_d[l_ac].pmaf009,g_pmaf_d[l_ac].pmafstus,g_pmaf_d[l_ac].pmaf010)
                WHERE pmafent = g_enterprise 
                  AND pmaf001 = g_pmaf_d_t.pmaf001
