         END IF
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code = 9001 
         LET g_errparam.popup = FALSE 
         CALL cl_err()
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_pmdj_m.pmdjdocno != g_pmdj_m_t.pmdjdocno
      OR g_pmdj_m.pmdjseq != g_pmdj_m_t.pmdjseq
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前 name="modify.body.b_fk_update"
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE pmdk_t SET pmdkdocno = g_pmdj_m.pmdjdocno
                                       ,pmdkseq = g_pmdj_m.pmdjseq
 
          WHERE pmdkent = g_enterprise AND pmdkdocno = g_pmdj_m_t.pmdjdocno
            AND pmdkseq = g_pmdj_m_t.pmdjseq
 
            
         #add-point:單身fk修改中 name="modify.body.m_fk_update"
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            #   INITIALIZE g_errparam TO NULL 
            #   LET g_errparam.extend = "pmdk_t" 
            #   LET g_errparam.code = "std-00009" 
            #   LET g_errparam.popup = TRUE 
            #   CALL cl_err()
            #   CONTINUE WHILE
            WHEN SQLCA.SQLCODE #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "pmdk_t:",SQLERRMESSAGE 
               LET g_errparam.code = SQLCA.SQLCODE 
               LET g_errparam.popup = TRUE 
               CALL cl_err()
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後 name="modify.body.a_fk_update"
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL apmt440_01_set_act_visible()   
   CALL apmt440_01_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " pmdjent = " ||g_enterprise|| " AND",
                      " pmdjdocno = '", g_pmdj_m.pmdjdocno, "' "
                      ," AND pmdjseq = '", g_pmdj_m.pmdjseq, "' "
 
   #填到對應位置
   CALL apmt440_01_browser_fill("")
 
   CLOSE apmt440_01_cl
   
 
   #功能已完成,通報訊息中心
   CALL apmt440_01_msgcentre_notify('modify')
 
