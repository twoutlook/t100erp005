PUBLIC FUNCTION apmt440_01(--)
   #add-point:main段變數傳入 name="main.get_var"
   {<point name="main.get_var"/>}
   #end add-point
   )
   #add-point:main段define(客製用) name="main.define_customerization"
   {<point name="main.define_customerization" edit="c" mark="Y"/>}
   #end add-point   
   #add-point:main段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="main.define"
   {<point name="main.define"/>}
   #end add-point   
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
 
   #add-point:作業初始化 name="main.init"
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define name="main.define_sql"
   {<point name="main.define_sql" mark="Y"/>}
   #end add-point
   LET g_forupd_sql = "SELECT pmdjdocno,pmdjseq,'',pmdj002,'','',pmdj031,pmdj032,pmdj003,pmdj004,pmdj006, 
       pmdj007 FROM pmdj_t WHERE pmdjent= ? AND pmdjdocno=? AND pmdjseq=? FOR UPDATE"
   #add-point:SQL_define name="main.after_define_sql"
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)   #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE apmt440_01_cl CURSOR FROM g_forupd_sql     # LOCK CURSOR
 
   LET g_sql = " SELECT t0.pmdjdocno,t0.pmdjseq,t0.pmdj002,t0.pmdj031,t0.pmdj032,t0.pmdj003,t0.pmdj004, 
       t0.pmdj006,t0.pmdj007",
               " FROM pmdj_t t0",
               " WHERE pmdjent = " ||g_enterprise|| " AND t0.pmdjdocno = ? AND t0.pmdjseq = ?"
   #add-point:SQL_define name="main.after_refresh_sql"
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE apmt440_01_master_referesh FROM g_sql
   
 
 
   
   #畫面開啟 (identifier)
   OPEN WINDOW w_apmt440_01 WITH FORM cl_ap_formpath("apm","apmt440_01")
   
   #瀏覽頁簽資料初始化
   CALL cl_ui_init()
   
   #程式初始化
   CALL apmt440_01_init()   
 
   #進入選單 Menu (="N")
