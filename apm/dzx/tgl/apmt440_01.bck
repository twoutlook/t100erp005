   LET g_pmdj_m_t.* = g_pmdj_m.*
   LET g_pmdj_m_o.* = g_pmdj_m.*
   
   IF g_pmdj_m.pmdjdocno IS NULL
   OR g_pmdj_m.pmdjseq IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code = "std-00003" 
      LET g_errparam.popup = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_pmdjdocno_t = g_pmdj_m.pmdjdocno
   LET g_pmdjseq_t = g_pmdj_m.pmdjseq
 
   CALL s_transaction_begin()
   
   OPEN apmt440_01_cl USING g_enterprise,g_pmdj_m.pmdjdocno,g_pmdj_m.pmdjseq
   IF SQLCA.SQLCODE THEN   #(ver:78)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN apmt440_01_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE   #(ver:78)
      LET g_errparam.popup = TRUE 
      CLOSE apmt440_01_cl
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE apmt440_01_master_referesh USING g_pmdj_m.pmdjdocno,g_pmdj_m.pmdjseq INTO g_pmdj_m.pmdjdocno, 
       g_pmdj_m.pmdjseq,g_pmdj_m.pmdj002,g_pmdj_m.pmdj031,g_pmdj_m.pmdj032,g_pmdj_m.pmdj003,g_pmdj_m.pmdj004, 
       g_pmdj_m.pmdj006,g_pmdj_m.pmdj007
   
   #檢查是否允許此動作
   IF NOT apmt440_01_action_chk() THEN
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_pmdj_m_mask_o.* =  g_pmdj_m.*
