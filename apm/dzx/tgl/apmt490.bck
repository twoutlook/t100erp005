   CALL cl_bpm_set_detail_data("s_detail2", util.JSONArray.fromFGL(g_pmeb2_d))
   CALL cl_bpm_set_detail_data("s_detail3", util.JSONArray.fromFGL(g_pmeb3_d))
 
 
