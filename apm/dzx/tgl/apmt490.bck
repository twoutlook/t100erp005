{<section id="apmt490.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION apmt490_b_fill()
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   {<point name="b_fill.define_customerization" edit="c" mark="Y"/>}
   #end add-point     
   DEFINE p_wc2      STRING
   DEFINE li_idx     LIKE type_t.num10
   #add-point:b_fill段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   {<point name="b_fill.define"/>} 
   #end add-point     
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   {<point name="b_fill.pre_function"/>}
   #end add-point
   
   #清空第一階單身
   CALL g_pmeb_d.clear()
   CALL g_pmeb2_d.clear()
 
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF apmt490_fill_chk(1) THEN
      #切換上下筆時不重組SQL
      IF (g_action_choice = "query" OR cl_null(g_action_choice))
      #add-point:b_fill段long_sql_if name="b_fill.long_sql_if"
      {<point name="b_fill.long_sql_if"/>}
      #end add-point
      THEN
         LET g_sql = "SELECT  DISTINCT pmeb001,pmebsite,pmebseq,pmeb002,pmeb003,pmeb005,pmeb006,pmeb007, 
             pmeb014,pmeb008,pmeb009,pmeb010,pmeb024,pmeb011,pmeb012,pmeb017,pmeb018,pmeb019,pmeb013, 
             pmeb004,pmeb030,pmeb901,pmeb902,pmeb903,pmebseq,pmeb020,pmeb021,pmeb022,pmeb023 ,t1.imaal003 , 
             t2.imaal004 ,t5.oocql004 ,t6.oocal003 ,t7.oocql004 ,t8.imaal003 ,t9.imaal003 ,t10.imaal004 , 
             t11.oocql004 ,t12.oocal003 FROM pmeb_t",   
                     " INNER JOIN pmea_t ON pmeaent = " ||g_enterprise|| " AND pmeadocno = pmebdocno ",
                     " AND pmea900 = pmeb900 ",
 
                     #"",
                     " LEFT JOIN pmec_t ON pmebent = pmecent AND pmebdocno = pmecdocno AND pmeb900 = pmec900 AND pmebseq = pmecseq ",
                     "",
                     #下層單身所需的join條件
                     " ",
 
 
                                    " LEFT JOIN imaal_t t1 ON t1.imaalent="||g_enterprise||" AND t1.imaal001=pmeb002 AND t1.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t2 ON t2.imaalent="||g_enterprise||" AND t2.imaal001=pmeb002 AND t2.imaal002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t5 ON t5.oocqlent="||g_enterprise||" AND t5.oocql001='221' AND t5.oocql002=pmeb006 AND t5.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t6 ON t6.oocalent="||g_enterprise||" AND t6.oocal001=pmeb008 AND t6.oocal002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t7 ON t7.oocqlent="||g_enterprise||" AND t7.oocql001='263' AND t7.oocql002=pmeb013 AND t7.oocql003='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t8 ON t8.imaalent="||g_enterprise||" AND t8.imaal001=pmeb004 AND t8.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t9 ON t9.imaalent="||g_enterprise||" AND t9.imaal001=pmeb002 AND t9.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t10 ON t10.imaalent="||g_enterprise||" AND t10.imaal001=pmeb002 AND t10.imaal002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t11 ON t11.oocqlent="||g_enterprise||" AND t11.oocql001='221' AND t11.oocql002=pmeb006 AND t11.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t12 ON t12.oocalent="||g_enterprise||" AND t12.oocal001=pmeb008 AND t12.oocal002='"||g_dlang||"' ",
 
                     " WHERE pmebent=? AND pmebdocno=? AND pmeb900=?"
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before name="b_fill.body.fill_sql"
         {<point name="b_fill.body.fill_sql"/>}
         #end add-point
         IF NOT cl_null(g_wc2_table1) THEN
            LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
         END IF
         
         #子單身的WC
            IF NOT cl_null(g_wc2_table2) THEN 
      LET g_sql = g_sql CLIPPED," AND ", g_wc2_table2 CLIPPED
   END IF 
 
         
         LET g_sql = g_sql, " ORDER BY pmeb_t.pmebseq"
         
         #add-point:單身填充控制 name="b_fill.sql"
         {<point name="b_fill.sql"/>}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE apmt490_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR apmt490_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
   #  OPEN b_fill_cs USING g_enterprise,g_pmea_m.pmeadocno,g_pmea_m.pmea900   #(ver:78)
                                               
      FOREACH b_fill_cs USING g_enterprise,g_pmea_m.pmeadocno,g_pmea_m.pmea900 INTO g_pmeb_d[l_ac].pmeb001, 
          g_pmeb_d[l_ac].pmebsite,g_pmeb_d[l_ac].pmebseq,g_pmeb_d[l_ac].pmeb002,g_pmeb_d[l_ac].pmeb003, 
          g_pmeb_d[l_ac].pmeb005,g_pmeb_d[l_ac].pmeb006,g_pmeb_d[l_ac].pmeb007,g_pmeb_d[l_ac].pmeb014, 
          g_pmeb_d[l_ac].pmeb008,g_pmeb_d[l_ac].pmeb009,g_pmeb_d[l_ac].pmeb010,g_pmeb_d[l_ac].pmeb024, 
          g_pmeb_d[l_ac].pmeb011,g_pmeb_d[l_ac].pmeb012,g_pmeb_d[l_ac].pmeb017,g_pmeb_d[l_ac].pmeb018, 
          g_pmeb_d[l_ac].pmeb019,g_pmeb_d[l_ac].pmeb013,g_pmeb_d[l_ac].pmeb004,g_pmeb_d[l_ac].pmeb030, 
          g_pmeb_d[l_ac].pmeb901,g_pmeb_d[l_ac].pmeb902,g_pmeb_d[l_ac].pmeb903,g_pmeb2_d[l_ac].pmebseq, 
          g_pmeb2_d[l_ac].pmeb020,g_pmeb2_d[l_ac].pmeb021,g_pmeb2_d[l_ac].pmeb022,g_pmeb2_d[l_ac].pmeb023, 
          g_pmeb_d[l_ac].pmeb002_desc,g_pmeb_d[l_ac].pmeb002_desc_desc,g_pmeb_d[l_ac].pmeb006_desc,g_pmeb_d[l_ac].pmeb008_desc, 
          g_pmeb_d[l_ac].pmeb013_desc,g_pmeb_d[l_ac].pmeb004_desc,g_pmeb2_d[l_ac].pmeb0022_desc,g_pmeb2_d[l_ac].pmeb0022_desc_desc, 
          g_pmeb2_d[l_ac].pmeb0062_desc,g_pmeb2_d[l_ac].pmeb0082_desc   #(ver:78)
         IF SQLCA.SQLCODE THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code = SQLCA.SQLCODE 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充 name="b_fill.fill"
         {<point name="b_fill.fill"/>}
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code = 9035 
               LET g_errparam.popup = TRUE 
               CALL cl_err()
            END IF
            EXIT FOREACH
         END IF
         
         LET l_ac = l_ac + 1
      END FOREACH
      LET g_error_show = 0
   
   END IF
    
 
   
   #add-point:browser_fill段其他table處理 name="browser_fill.other_fill"
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_pmeb_d.deleteElement(g_pmeb_d.getLength())
   CALL g_pmeb2_d.deleteElement(g_pmeb2_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE apmt490_pb
 
   
   LET li_idx = l_ac
   
   #遮罩相關處理
   FOR l_ac = 1 TO g_pmeb_d.getLength()
      LET g_pmeb_d_mask_o[l_ac].* =  g_pmeb_d[l_ac].*
      CALL apmt490_pmeb_t_mask()
      LET g_pmeb_d_mask_n[l_ac].* =  g_pmeb_d[l_ac].*
   END FOR
   
   LET g_pmeb2_d_mask_o.* =  g_pmeb2_d.*
   FOR l_ac = 1 TO g_pmeb2_d.getLength()
      LET g_pmeb2_d_mask_o[l_ac].* =  g_pmeb2_d[l_ac].*
      CALL apmt490_pmeb_t_mask()
      LET g_pmeb2_d_mask_n[l_ac].* =  g_pmeb2_d[l_ac].*
   END FOR
   LET g_pmeb3_d_mask_o.* =  g_pmeb3_d.*
   FOR l_ac = 1 TO g_pmeb3_d.getLength()
      LET g_pmeb3_d_mask_o[l_ac].* =  g_pmeb3_d[l_ac].*
      CALL apmt490_pmec_t_mask()
      LET g_pmeb3_d_mask_n[l_ac].* =  g_pmeb3_d[l_ac].*
   END FOR
 
   
   LET l_ac = li_idx
   
   CALL cl_ap_performance_next_end()
 
END FUNCTION

