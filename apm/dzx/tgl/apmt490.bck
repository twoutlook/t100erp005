   CALL cl_ap_init("apm","")
 
   #add-point:作業初始化 name="main.init"
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define name="main.define_sql"
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT pmea001,pmea901,pmeadocno,pmea900,'',pmeasite,pmea902,pmea000,pmeadocdt, 
       pmea002,'',pmea003,'',pmeastus,pmea004,'',pmeaacti,pmea005,'',pmea006,'',pmea007,pmea008,pmea009, 
       '',pmea011,'',pmea030,pmea016,pmea017,pmea018,pmea019,pmea020,pmea010,pmea012,pmea014,pmea015, 
       pmea903,'',pmea904,pmeaownid,'',pmeaowndp,'',pmeacrtid,'',pmeacrtdp,'',pmeacrtdt,pmeamodid,'', 
       pmeamoddt,pmeacnfid,'',pmeacnfdt", 
                      " FROM pmea_t",
                      " WHERE pmeaent= ? AND pmeadocno=? AND pmea900=? FOR UPDATE"
   #add-point:SQL_define name="main.after_define_sql"
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE apmt490_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.pmea001,t0.pmea901,t0.pmeadocno,t0.pmea900,t0.pmeasite,t0.pmea902, 
       t0.pmea000,t0.pmeadocdt,t0.pmea002,t0.pmea003,t0.pmeastus,t0.pmea004,t0.pmeaacti,t0.pmea005,t0.pmea006, 
       t0.pmea007,t0.pmea008,t0.pmea009,t0.pmea011,t0.pmea030,t0.pmea016,t0.pmea017,t0.pmea018,t0.pmea019, 
       t0.pmea020,t0.pmea010,t0.pmea012,t0.pmea014,t0.pmea015,t0.pmea903,t0.pmea904,t0.pmeaownid,t0.pmeaowndp, 
       t0.pmeacrtid,t0.pmeacrtdp,t0.pmeacrtdt,t0.pmeamodid,t0.pmeamoddt,t0.pmeacnfid,t0.pmeacnfdt,t1.ooag011 , 
       t2.ooefl003 ,t3.pmaal004 ,t4.ooail003 ,t5.ooibl004 ,t6.oocql004 ,t7.ooag011 ,t8.ooefl003 ,t9.ooag011 , 
       t10.ooefl003 ,t11.ooag011 ,t12.ooag011",
               " FROM pmea_t t0",
                              " LEFT JOIN ooag_t t1 ON t1.ooagent="||g_enterprise||" AND t1.ooag001=t0.pmea002  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent="||g_enterprise||" AND t2.ooefl001=t0.pmea003 AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN pmaal_t t3 ON t3.pmaalent="||g_enterprise||" AND t3.pmaal001=t0.pmea004 AND t3.pmaal002='"||g_dlang||"' ",
               " LEFT JOIN ooail_t t4 ON t4.ooailent="||g_enterprise||" AND t4.ooail001=t0.pmea005 AND t4.ooail002='"||g_dlang||"' ",
               " LEFT JOIN ooibl_t t5 ON t5.ooiblent="||g_enterprise||" AND t5.ooibl002=t0.pmea009 AND t5.ooibl003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t6 ON t6.oocqlent="||g_enterprise||" AND t6.oocql001='238' AND t6.oocql002=t0.pmea011 AND t6.oocql003='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent="||g_enterprise||" AND t7.ooag001=t0.pmeaownid  ",
               " LEFT JOIN ooefl_t t8 ON t8.ooeflent="||g_enterprise||" AND t8.ooefl001=t0.pmeaowndp AND t8.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t9 ON t9.ooagent="||g_enterprise||" AND t9.ooag001=t0.pmeacrtid  ",
               " LEFT JOIN ooefl_t t10 ON t10.ooeflent="||g_enterprise||" AND t10.ooefl001=t0.pmeacrtdp AND t10.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t11 ON t11.ooagent="||g_enterprise||" AND t11.ooag001=t0.pmeamodid  ",
               " LEFT JOIN ooag_t t12 ON t12.ooagent="||g_enterprise||" AND t12.ooag001=t0.pmeacnfid  ",
 
               " WHERE t0.pmeaent = " ||g_enterprise|| " AND t0.pmeadocno = ? AND t0.pmea900 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define name="main.after_refresh_sql"
