                  {<point name="browser_fill.join.pmbz_t1"/>}
                  #end add-point
 
                  "  LEFT JOIN pmbc_t ON pmbcent = pmbaent AND pmbadocno = pmbcdocno", "  ", 
                  #add-point:browser_fill段sql(pmbc_t1) name="browser_fill.join.pmbc_t1"
                  {<point name="browser_fill.join.pmbc_t1"/>}
                  #end add-point
 
                  "  LEFT JOIN pmbf_t ON pmbfent = pmbaent AND pmbadocno = pmbfdocno", "  ", 
                  #add-point:browser_fill段sql(pmbf_t1) name="browser_fill.join.pmbf_t1"
                  {<point name="browser_fill.join.pmbf_t1"/>}
                  #end add-point
 
                  "  LEFT JOIN pmbd_t ON pmbdent = pmbaent AND pmbadocno = pmbddocno", "  ", 
                  #add-point:browser_fill段sql(pmbd_t1) name="browser_fill.join.pmbd_t1"
                  {<point name="browser_fill.join.pmbd_t1"/>}
                  #end add-point
 
 
 
                  " ", 
                  " ",                      
 
                  " ",                      
 
                  " ",                      
 
                  " ",                      
 
                  " ",                      
 
                  " ",                      
 
 
 
                                 " LEFT JOIN pmbal_t t1 ON t1.pmbalent="||g_enterprise||" AND t1.pmbaldocno=t0.pmbadocno AND t1.pmbal001='"||g_dlang||"' ",
 
                  " WHERE t0.pmbaent = " ||g_enterprise|| " AND ",l_wc," AND ",l_wc2, cl_sql_add_filter("pmba_t")
   ELSE
