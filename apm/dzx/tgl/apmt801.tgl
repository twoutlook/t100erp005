                                 " LEFT JOIN pmbal_t t1 ON t1.pmbalent="||g_enterprise||" AND t1.pmbaldocno=t0.pmbadocno AND t1.pmbal001='"||g_dlang||"' ",
 
                  " WHERE t0.pmbaent = " ||g_enterprise|| " AND ",l_wc, cl_sql_add_filter("pmba_t")
   END IF
   #add-point:browser_fill,sql wc name="browser_fill.fill_sqlwc"
   {<point name="browser_fill.fill_sqlwc"/>}
