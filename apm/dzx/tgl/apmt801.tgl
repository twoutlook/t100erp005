      CALL g_pmbg2_d.clear() 
      CALL g_pmbg3_d.clear() 
      CALL g_pmbg4_d.clear() 
      CALL g_pmbg5_d.clear() 
      CALL g_pmbg6_d.clear() 
      CALL g_pmbg7_d.clear() 
 
