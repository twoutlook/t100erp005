         SET (pmdkdocno,
              pmdkseq,pmdk001,pmdk002
              ,pmdk003,pmdk004) 
              = 
             (ps_keys[1],ps_keys[2],ps_keys[3],ps_keys[4]
              ,g_pmdj2_d[g_detail_idx].pmdk003,g_pmdj2_d[g_detail_idx].pmdk004) 
         WHERE pmdkent = g_enterprise AND pmdkdocno = ps_keys_bak[1] AND pmdkseq = ps_keys_bak[2] AND pmdk001 = ps_keys_bak[3] AND pmdk002 = ps_keys_bak[4]
      #add-point:update_b段修改中 name="update_b.m_update2"
