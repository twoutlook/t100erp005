   #add-point:完成複製段落後 name="reproduce.after_reproduce"
   {<point name="reproduce.after_reproduce" />}
   #end add-point
   
   CALL apmt440_idx_chk()
   
   LET g_data_owner = g_pmdi_m.pmdiownid      
   LET g_data_dept  = g_pmdi_m.pmdiowndp
   
   #功能已完成,通報訊息中心
   CALL apmt440_msgcentre_notify('reproduce')
 
