                     LET g_errparam.code = "std-00009" 
                     LET g_errparam.popup = TRUE 
                     CALL s_transaction_end('N','0')
                     CALL cl_err()
                     
                  WHEN SQLCA.SQLCODE #其他錯誤
                     LET g_pmdj_d[l_ac].* = g_pmdj_d_t.*  
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "pmdj_t:",SQLERRMESSAGE 
                     LET g_errparam.code = SQLCA.SQLCODE 
                     LET g_errparam.popup = TRUE 
                     CALL s_transaction_end('N','0')
                     CALL cl_err()                   
                     
                  OTHERWISE
                     #資料多語言用-增/改
                     
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_pmdi_m.pmdidocno
               LET gs_keys_bak[1] = g_pmdidocno_t
               LET gs_keys[2] = g_pmdj_d[g_detail_idx].pmdjseq
               LET gs_keys_bak[2] = g_pmdj_d_t.pmdjseq
               CALL apmt440_update_b('pmdj_t',gs_keys,gs_keys_bak,"'1'")
               END CASE
 
               #將遮罩欄位進行遮蔽
               CALL apmt440_pmdj_t_mask_restore('restore_mask_n')
               
               #判斷key是否有改變
               INITIALIZE gs_keys TO NULL
               IF NOT(g_pmdj_d[g_detail_idx].pmdjseq = g_pmdj_d_t.pmdjseq 
 
                  ) THEN
                  LET gs_keys[01] = g_pmdi_m.pmdidocno
 
                  LET gs_keys[gs_keys.getLength()+1] = g_pmdj_d_t.pmdjseq
 
                  CALL apmt440_key_update_b(gs_keys,'pmdj_t')
               END IF
               
               #修改歷程記錄(單身修改)
               LET g_log1 = util.JSON.stringify(g_pmdi_m),util.JSON.stringify(g_pmdj_d_t)
               LET g_log2 = util.JSON.stringify(g_pmdi_m),util.JSON.stringify(g_pmdj_d[l_ac])
               IF NOT cl_log_modified_record_d(g_log1,g_log2) THEN 
                  CALL s_transaction_end('N','0')
               END IF
               
               #add-point:單身修改後 name="input.body.a_update"
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
