               #add-point:單身修改前 name="input.body.b_update"
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               #寫入修改者/修改日期資訊(單身)
               
      
               #將遮罩欄位還原
               CALL apmt440_pmdj_t_mask_restore('restore_mask_o')
      
               UPDATE pmdj_t SET (pmdjdocno,pmdj001,pmdjsite,pmdjseq,pmdj002,pmdj003,pmdj031,pmdj032, 
                   pmdj004,pmdj005,pmdj006,pmdj007,pmdj015,pmdj008,pmdj009,pmdj010,pmdj011,pmdj016,pmdj012, 
                   pmdj013,pmdj014,pmdj030) = (g_pmdi_m.pmdidocno,g_pmdj_d[l_ac].pmdj001,g_pmdj_d[l_ac].pmdjsite, 
                   g_pmdj_d[l_ac].pmdjseq,g_pmdj_d[l_ac].pmdj002,g_pmdj_d[l_ac].pmdj003,g_pmdj_d[l_ac].pmdj031, 
                   g_pmdj_d[l_ac].pmdj032,g_pmdj_d[l_ac].pmdj004,g_pmdj_d[l_ac].pmdj005,g_pmdj_d[l_ac].pmdj006, 
                   g_pmdj_d[l_ac].pmdj007,g_pmdj_d[l_ac].pmdj015,g_pmdj_d[l_ac].pmdj008,g_pmdj_d[l_ac].pmdj009, 
                   g_pmdj_d[l_ac].pmdj010,g_pmdj_d[l_ac].pmdj011,g_pmdj_d[l_ac].pmdj016,g_pmdj_d[l_ac].pmdj012, 
                   g_pmdj_d[l_ac].pmdj013,g_pmdj_d[l_ac].pmdj014,g_pmdj_d[l_ac].pmdj030)
                WHERE pmdjent = g_enterprise AND pmdjdocno = g_pmdi_m.pmdidocno 
 
                  AND pmdjseq = g_pmdj_d_t.pmdjseq #項次   
 
                  
               #add-point:單身修改中 name="input.body.m_update"
               {<point name="input.body.m_update"/>}
