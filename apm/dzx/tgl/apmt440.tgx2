#+ 資料刪除
PRIVATE FUNCTION apmt440_delete()
   #add-point:delete段define(客製用) name="delete.define_customerization"
   {<point name="delete.define_customerization" edit="c" mark="Y"/>}
   #end add-point     
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="delete.define"
   {<point name="delete.define"/>} 
   #end add-point     
   
   #add-point:Function前置處理  name="delete.pre_function"
   {<point name="delete.pre_function"/>}
   #end add-point
   
   IF g_pmdi_m.pmdidocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code = "std-00003" 
      LET g_errparam.popup = FALSE 
      CALL cl_err()
      RETURN
   END IF
   
   
   
   CALL s_transaction_begin()
 
   OPEN apmt440_cl USING g_enterprise,g_pmdi_m.pmdidocno
   IF SQLCA.SQLCODE THEN   #(ver:78)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN apmt440_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE   #(ver:78)
      LET g_errparam.popup = TRUE 
      CLOSE apmt440_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE apmt440_master_referesh USING g_pmdi_m.pmdidocno INTO g_pmdi_m.pmdi001,g_pmdi_m.pmdisite, 
       g_pmdi_m.pmdidocno,g_pmdi_m.pmdidocdt,g_pmdi_m.pmdi002,g_pmdi_m.pmdi004,g_pmdi_m.pmdi003,g_pmdi_m.pmdistus, 
       g_pmdi_m.pmdi005,g_pmdi_m.pmdi006,g_pmdi_m.pmdi007,g_pmdi_m.pmdi008,g_pmdi_m.pmdi009,g_pmdi_m.pmdi011, 
       g_pmdi_m.pmdi019,g_pmdi_m.pmdi030,g_pmdi_m.pmdi013,g_pmdi_m.pmdi014,g_pmdi_m.pmdi010,g_pmdi_m.pmdi012, 
       g_pmdi_m.pmdi031,g_pmdi_m.pmdi032,g_pmdi_m.pmdi020,g_pmdi_m.pmdi015,g_pmdi_m.pmdi016,g_pmdi_m.pmdi017, 
       g_pmdi_m.pmdi018,g_pmdi_m.pmdi033,g_pmdi_m.pmdi034,g_pmdi_m.pmdiownid,g_pmdi_m.pmdiowndp,g_pmdi_m.pmdicrtid, 
       g_pmdi_m.pmdicrtdp,g_pmdi_m.pmdicrtdt,g_pmdi_m.pmdimodid,g_pmdi_m.pmdimoddt,g_pmdi_m.pmdicnfid, 
       g_pmdi_m.pmdicnfdt,g_pmdi_m.pmdi002_desc,g_pmdi_m.pmdi004_desc,g_pmdi_m.pmdi003_desc,g_pmdi_m.pmdi005_desc, 
       g_pmdi_m.pmdi009_desc,g_pmdi_m.pmdi011_desc,g_pmdi_m.pmdi019_desc,g_pmdi_m.pmdiownid_desc,g_pmdi_m.pmdiowndp_desc, 
       g_pmdi_m.pmdicrtid_desc,g_pmdi_m.pmdicrtdp_desc,g_pmdi_m.pmdimodid_desc,g_pmdi_m.pmdicnfid_desc 

   
   
   #檢查是否允許此動作
   IF NOT apmt440_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_pmdi_m_mask_o.* =  g_pmdi_m.*
   CALL apmt440_pmdi_t_mask()
   LET g_pmdi_m_mask_n.* =  g_pmdi_m.*
   
   CALL apmt440_show()
   
   #add-point:delete段before ask name="delete.before_ask"
   {<point name="delete.before_ask"/>}
   #end add-point 
 
   IF cl_ask_del_master() THEN              #確認一下
   
      #add-point:單頭刪除前 name="delete.head.b_delete"
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point   
      
      #應用 a47 樣板自動產生(Version:4)
      #刪除相關文件
      CALL apmt440_set_pk_array()
      #add-point:相關文件刪除前 name="delete.befroe.related_document_remove"
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
  
  
      #資料備份
      LET g_pmdidocno_t = g_pmdi_m.pmdidocno
 
 
      DELETE FROM pmdi_t
       WHERE pmdient = g_enterprise AND pmdidocno = g_pmdi_m.pmdidocno
 
       
      #add-point:單頭刪除中 name="delete.head.m_delete"
      {<point name="delete.head.m_delete"/>}
      #end add-point
       
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = g_pmdi_m.pmdidocno,":",SQLERRMESSAGE  
         LET g_errparam.code = SQLCA.SQLCODE 
         LET g_errparam.popup = FALSE 
         CALL s_transaction_end('N','0')
         CALL cl_err()
         RETURN
      END IF
      
      #add-point:單頭刪除後 name="delete.head.a_delete"
      {<point name="delete.head.a_delete"/>}
      #end add-point
  
      #add-point:單身刪除前 name="delete.body.b_delete"
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM pmdj_t
       WHERE pmdjent = g_enterprise AND pmdjdocno = g_pmdi_m.pmdidocno
 
 
      #add-point:單身刪除中 name="delete.body.m_delete"
      {<point name="delete.body.m_delete"/>}
      #end add-point
         
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "pmdj_t:",SQLERRMESSAGE 
         LET g_errparam.code = SQLCA.SQLCODE 
         LET g_errparam.popup = FALSE 
         CALL s_transaction_end('N','0')
         CALL cl_err()
         RETURN
      END IF    
 
      #add-point:單身刪除後 name="delete.body.a_delete"
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
            
                                                               
      #add-point:單身刪除前 name="delete.body.b_delete2"
      {<point name="delete.body.b_delete2" mark="Y"/>}
      #end add-point
      DELETE FROM pmdk_t
       WHERE pmdkent = g_enterprise AND
             pmdkdocno = g_pmdi_m.pmdidocno
      #add-point:單身刪除中 name="delete.body.m_delete2"
      {<point name="delete.body.m_delete2"/>}
      #end add-point
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "pmdk_t:",SQLERRMESSAGE 
         LET g_errparam.code = SQLCA.SQLCODE 
         LET g_errparam.popup = FALSE 
         CALL s_transaction_end('N','0')
         CALL cl_err()
         RETURN
      END IF      
 
      #add-point:單身刪除後 name="delete.body.a_delete2"
      {<point name="delete.body.a_delete2"/>}
      #end add-point
 
 
 
 
      
      #修改歷程記錄(刪除)
      LET g_log1 = util.JSON.stringify(g_pmdi_m)   #(ver:78)
      IF NOT cl_log_modified_record(g_log1,'') THEN    #(ver:78)
         CLOSE apmt440_cl
         CALL s_transaction_end('N','0')
         RETURN
      END IF
             
      CLEAR FORM
      CALL g_pmdj_d.clear() 
      CALL g_pmdj2_d.clear()       
 
     
      CALL apmt440_ui_browser_refresh()  
      #CALL apmt440_ui_headershow()  
      #CALL apmt440_ui_detailshow()
 
      #add-point:多語言刪除 name="delete.lang.before_delete"
      {<point name="delete.lang.before_delete"/>}
      #end add-point
      
      #單頭多語言刪除
      
      
      #單身多語言刪除
      
      
 
   
      #add-point:多語言刪除 name="delete.lang.delete"
      {<point name="delete.lang.delete"/>}
      #end add-point
      
      IF g_browser_cnt > 0 THEN 
         #CALL apmt440_browser_fill("")
         CALL apmt440_fetch('P')
         DISPLAY g_browser_cnt TO FORMONLY.h_count   #總筆數的顯示
         DISPLAY g_browser_cnt TO FORMONLY.b_count   #總筆數的顯示
      ELSE
         CLEAR FORM
      END IF
      
      CALL s_transaction_end('Y','0')
   ELSE
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE apmt440_cl
 
   #功能已完成,通報訊息中心
   CALL apmt440_msgcentre_notify('delete')
    
END FUNCTION
