{<section id="apmt480.ui_headershow" >}
#+ 單頭資料重新顯示
PRIVATE FUNCTION apmt480_ui_headershow()
   #add-point:ui_headershow段define(客製用) name="ui_headershow.define_customerization"
   {<point name="ui_headershow.define_customerization" edit="c" mark="Y"/>}
   #end add-point  
   #add-point:ui_headershow段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="ui_headershow.define"
   {<point name="ui_headershow.define"/>} 
   #end add-point      
   
   #add-point:Function前置處理  name="ui_headershow.pre_function"
   {<point name="ui_headershow.pre_function"/>}
   #end add-point
   
   LET g_pmdx_m.pmdxdocno = g_browser[g_current_idx].b_pmdxdocno   
 
   EXECUTE apmt480_master_referesh USING g_pmdx_m.pmdxdocno INTO g_pmdx_m.pmdxdocno,g_pmdx_m.pmdx000, 
       g_pmdx_m.pmdxdocdt,g_pmdx_m.pmdx002,g_pmdx_m.pmdx004,g_pmdx_m.pmdx003,g_pmdx_m.pmdxstus,g_pmdx_m.pmdx005, 
       g_pmdx_m.pmdx006,g_pmdx_m.pmdx007,g_pmdx_m.pmdx008,g_pmdx_m.pmdx009,g_pmdx_m.pmdx011,g_pmdx_m.pmdx030, 
       g_pmdx_m.pmdx016,g_pmdx_m.pmdx017,g_pmdx_m.pmdx018,g_pmdx_m.pmdx019,g_pmdx_m.pmdx020,g_pmdx_m.pmdx010, 
       g_pmdx_m.pmdx012,g_pmdx_m.pmdx001,g_pmdx_m.pmdx014,g_pmdx_m.pmdx015,g_pmdx_m.pmdxsite,g_pmdx_m.pmdxownid, 
       g_pmdx_m.pmdxowndp,g_pmdx_m.pmdxcrtid,g_pmdx_m.pmdxcrtdp,g_pmdx_m.pmdxcrtdt,g_pmdx_m.pmdxmodid, 
       g_pmdx_m.pmdxmoddt,g_pmdx_m.pmdxcnfid,g_pmdx_m.pmdxcnfdt,g_pmdx_m.pmdx002_desc,g_pmdx_m.pmdx004_desc, 
       g_pmdx_m.pmdx003_desc,g_pmdx_m.pmdx005_desc,g_pmdx_m.pmdx009_desc,g_pmdx_m.pmdx011_desc,g_pmdx_m.pmdxownid_desc, 
       g_pmdx_m.pmdxowndp_desc,g_pmdx_m.pmdxcrtid_desc,g_pmdx_m.pmdxcrtdp_desc,g_pmdx_m.pmdxmodid_desc, 
