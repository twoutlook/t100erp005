   {<point name="main.define"/>}
   #end add-point   
   
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("apm","")
 
   #add-point:作業初始化 name="main.init"
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define name="main.define_sql"
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT pmdxdocno,pmdx000,pmdxdocdt,pmdx002,'','',pmdx004,'',pmdx003,'',pmdxstus, 
       pmdx005,'',pmdx006,'',pmdx007,pmdx008,pmdx009,'',pmdx011,'',pmdx030,pmdx016,pmdx017,pmdx018,pmdx019, 
       pmdx020,pmdx010,pmdx012,pmdx001,pmdx014,pmdx015,pmdxsite,pmdxownid,'',pmdxowndp,'',pmdxcrtid, 
       '',pmdxcrtdp,'',pmdxcrtdt,pmdxmodid,'',pmdxmoddt,pmdxcnfid,'',pmdxcnfdt", 
                      " FROM pmdx_t",
                      " WHERE pmdxent= ? AND pmdxdocno=? FOR UPDATE"
   #add-point:SQL_define name="main.after_define_sql"
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE apmt480_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.pmdxdocno,t0.pmdx000,t0.pmdxdocdt,t0.pmdx002,t0.pmdx004,t0.pmdx003, 
       t0.pmdxstus,t0.pmdx005,t0.pmdx006,t0.pmdx007,t0.pmdx008,t0.pmdx009,t0.pmdx011,t0.pmdx030,t0.pmdx016, 
       t0.pmdx017,t0.pmdx018,t0.pmdx019,t0.pmdx020,t0.pmdx010,t0.pmdx012,t0.pmdx001,t0.pmdx014,t0.pmdx015, 
       t0.pmdxsite,t0.pmdxownid,t0.pmdxowndp,t0.pmdxcrtid,t0.pmdxcrtdp,t0.pmdxcrtdt,t0.pmdxmodid,t0.pmdxmoddt, 
       t0.pmdxcnfid,t0.pmdxcnfdt,t1.ooag011 ,t2.pmaal004 ,t3.ooefl003 ,t4.ooail003 ,t5.ooibl004 ,t6.oocql004 , 
       t7.ooag011 ,t8.ooefl003 ,t9.ooag011 ,t10.ooefl003 ,t11.ooag011 ,t12.ooag011",
               " FROM pmdx_t t0",
                              " LEFT JOIN ooag_t t1 ON t1.ooagent="||g_enterprise||" AND t1.ooag001=t0.pmdx002  ",
               " LEFT JOIN pmaal_t t2 ON t2.pmaalent="||g_enterprise||" AND t2.pmaal001=t0.pmdx004 AND t2.pmaal002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent="||g_enterprise||" AND t3.ooefl001=t0.pmdx003 AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooail_t t4 ON t4.ooailent="||g_enterprise||" AND t4.ooail001=t0.pmdx005 AND t4.ooail002='"||g_dlang||"' ",
               " LEFT JOIN ooibl_t t5 ON t5.ooiblent="||g_enterprise||" AND t5.ooibl002=t0.pmdx009 AND t5.ooibl003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t6 ON t6.oocqlent="||g_enterprise||" AND t6.oocql001='238' AND t6.oocql002=t0.pmdx011 AND t6.oocql003='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent="||g_enterprise||" AND t7.ooag001=t0.pmdxownid  ",
               " LEFT JOIN ooefl_t t8 ON t8.ooeflent="||g_enterprise||" AND t8.ooefl001=t0.pmdxowndp AND t8.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t9 ON t9.ooagent="||g_enterprise||" AND t9.ooag001=t0.pmdxcrtid  ",
               " LEFT JOIN ooefl_t t10 ON t10.ooeflent="||g_enterprise||" AND t10.ooefl001=t0.pmdxcrtdp AND t10.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t11 ON t11.ooagent="||g_enterprise||" AND t11.ooag001=t0.pmdxmodid  ",
               " LEFT JOIN ooag_t t12 ON t12.ooagent="||g_enterprise||" AND t12.ooag001=t0.pmdxcnfid  ",
 
               " WHERE t0.pmdxent = " ||g_enterprise|| " AND t0.pmdxdocno = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define name="main.after_refresh_sql"
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE apmt480_master_referesh FROM g_sql
 
    
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call name="main.servicecall"
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_apmt480 WITH FORM cl_ap_formpath("apm",g_code)
   
