         FOREACH b_fill_curs3 USING g_enterprise,g_pmdx_m.pmdxdocno,g_pmdy_d[g_detail_idx].pmdyseq INTO g_pmdy4_d[l_ac].pmepsite, 
             g_pmdy4_d[l_ac].pmep002,g_pmdy4_d[l_ac].pmep003,g_pmdy4_d[l_ac].pmep004,g_pmdy4_d[l_ac].pmep005, 
             g_pmdy4_d[l_ac].pmep006,g_pmdy4_d[l_ac].pmep007,g_pmdy4_d[l_ac].pmep008,g_pmdy4_d[l_ac].pmep009, 
             g_pmdy4_d[l_ac].pmep010,g_pmdy4_d[l_ac].pmep011,g_pmdy4_d[l_ac].pmep012,g_pmdy4_d[l_ac].pmep013, 
             g_pmdy4_d[l_ac].pmep012_desc,g_pmdy4_d[l_ac].pmep013_desc   #(ver:78)
            IF SQLCA.SQLCODE THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
               LET g_errparam.code = SQLCA.SQLCODE 
               LET g_errparam.popup = TRUE 
               CALL cl_err()
               EXIT FOREACH
            END IF
            
            #add-point:b_fill段資料填充 name="b_fill2.fill3"
            {<point name="b_fill2.fill3"/>}
            #end add-point
           
            IF l_ac > g_max_rec THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code = 9035 
               LET g_errparam.popup = TRUE 
               CALL cl_err()
               EXIT FOREACH
            END IF
            
            LET l_ac = l_ac + 1
            
         END FOREACH
               CALL g_pmdy4_d.deleteElement(g_pmdy4_d.getLength())
 
      END IF
   END IF
 
 
      
   LET g_pmdy3_d_mask_o.* =  g_pmdy3_d.*
   FOR l_ac = 1 TO g_pmdy3_d.getLength()
      LET g_pmdy3_d_mask_o[l_ac].* =  g_pmdy3_d[l_ac].*
      CALL apmt480_pmdz_t_mask()
      LET g_pmdy3_d_mask_n[l_ac].* =  g_pmdy3_d[l_ac].*
   END FOR
   LET g_pmdy4_d_mask_o.* =  g_pmdy4_d.*
   FOR l_ac = 1 TO g_pmdy4_d.getLength()
      LET g_pmdy4_d_mask_o[l_ac].* =  g_pmdy4_d[l_ac].*
      CALL apmt480_pmep_t_mask()
      LET g_pmdy4_d_mask_n[l_ac].* =  g_pmdy4_d[l_ac].*
   END FOR
 
      
   #add-point:單身填充後 name="b_fill2.after_fill"
   {<point name="b_fill2.after_fill" />}
   #end add-point
    
   LET l_ac = li_ac
   
   CALL apmt480_detail_show()
   
   LET g_detail_idx = li_detail_idx_tmp
   
END FUNCTION

{</section>}

