   DISPLAY g_browser_idx TO FORMONLY.h_index   #當下筆數
   
   CALL cl_navigator_setting( g_current_idx, g_browser_cnt )
 
   #代表沒有資料
   IF g_current_idx = 0 OR g_browser.getLength() = 0 THEN
      RETURN
   END IF
   
   #避免超出browser資料筆數上限
   IF g_current_idx > g_browser.getLength() THEN
      LET g_browser_idx = g_browser.getLength()
      LET g_current_idx = g_browser.getLength()
   END IF
   
   LET g_pmel_m.pmeldocno = g_browser[g_current_idx].b_pmeldocno
 
   
   #重讀DB,因TEMP有不被更新特性
   EXECUTE apmt859_master_referesh USING g_pmel_m.pmeldocno INTO g_pmel_m.pmel000,g_pmel_m.pmelsite, 
       g_pmel_m.pmeldocdt,g_pmel_m.pmeldocno,g_pmel_m.pmel001,g_pmel_m.pmel006,g_pmel_m.pmel007,g_pmel_m.pmel002, 
       g_pmel_m.pmel003,g_pmel_m.pmel010,g_pmel_m.pmel009,g_pmel_m.pmel011,g_pmel_m.pmel012,g_pmel_m.pmel004, 
       g_pmel_m.pmel005,g_pmel_m.pmelunit,g_pmel_m.pmel013,g_pmel_m.pmelstus,g_pmel_m.pmelownid,g_pmel_m.pmelowndp, 
       g_pmel_m.pmelcrtid,g_pmel_m.pmelcrtdp,g_pmel_m.pmelcrtdt,g_pmel_m.pmelmodid,g_pmel_m.pmelmoddt, 
       g_pmel_m.pmelcnfid,g_pmel_m.pmelcnfdt,g_pmel_m.pmelsite_desc,g_pmel_m.pmel006_desc,g_pmel_m.pmel007_desc, 
       g_pmel_m.pmel003_desc,g_pmel_m.pmel009_desc,g_pmel_m.pmel004_desc,g_pmel_m.pmel005_desc,g_pmel_m.pmelownid_desc, 
       g_pmel_m.pmelowndp_desc,g_pmel_m.pmelcrtid_desc,g_pmel_m.pmelcrtdp_desc,g_pmel_m.pmelmodid_desc, 
       g_pmel_m.pmelcnfid_desc
   
   #遮罩相關處理
   LET g_pmel_m_mask_o.* =  g_pmel_m.*
   CALL apmt859_pmel_t_mask()
   LET g_pmel_m_mask_n.* =  g_pmel_m.*
   
   #根據資料狀態切換action狀態
