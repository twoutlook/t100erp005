         ON ROW CHANGE
            IF INT_FLAG THEN
               LET INT_FLAG = 0
               LET g_pmem_d[l_ac].* = g_pmem_d_t.*
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code = 9001 
               LET g_errparam.popup = FALSE 
               CLOSE apmt859_bcl
               CALL s_transaction_end('N','0')
               CALL cl_err()
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = g_pmem_d[l_ac].pmemseq 
               LET g_errparam.code = -263 
               LET g_errparam.popup = TRUE 
               CALL cl_err()
               LET g_pmem_d[l_ac].* = g_pmem_d_t.*
            ELSE
            
               #add-point:單身修改前 name="input.body.b_update"
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               #寫入修改者/修改日期資訊(單身)
               
      
               #將遮罩欄位還原
               CALL apmt859_pmem_t_mask_restore('restore_mask_o')
      
               UPDATE pmem_t SET (pmemdocno,pmemseq,pmem000,pmem001,pmem002,pmem003,pmem004,pmem005, 
                   pmem006,pmem007,pmem008,pmem009,pmem010,pmem011,pmem012,pmem013,pmem014,pmem015,pmem016, 
                   pmem022,pmem020,pmem017,pmem018,pmem019,pmemsite,pmemunit) = (g_pmel_m.pmeldocno, 
                   g_pmem_d[l_ac].pmemseq,g_pmem_d[l_ac].pmem000,g_pmem_d[l_ac].pmem001,g_pmem_d[l_ac].pmem002, 
                   g_pmem_d[l_ac].pmem003,g_pmem_d[l_ac].pmem004,g_pmem_d[l_ac].pmem005,g_pmem_d[l_ac].pmem006, 
                   g_pmem_d[l_ac].pmem007,g_pmem_d[l_ac].pmem008,g_pmem_d[l_ac].pmem009,g_pmem_d[l_ac].pmem010, 
                   g_pmem_d[l_ac].pmem011,g_pmem_d[l_ac].pmem012,g_pmem_d[l_ac].pmem013,g_pmem_d[l_ac].pmem014, 
                   g_pmem_d[l_ac].pmem015,g_pmem_d[l_ac].pmem016,g_pmem_d[l_ac].pmem022,g_pmem_d[l_ac].pmem020, 
                   g_pmem_d[l_ac].pmem017,g_pmem_d[l_ac].pmem018,g_pmem_d[l_ac].pmem019,g_pmem_d[l_ac].pmemsite, 
                   g_pmem_d[l_ac].pmemunit)
                WHERE pmement = g_enterprise AND pmemdocno = g_pmel_m.pmeldocno 
 
                  AND pmemseq = g_pmem_d_t.pmemseq #項次   
 
                  
               #add-point:單身修改中 name="input.body.m_update"
               {<point name="input.body.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     LET g_pmem_d[l_ac].* = g_pmem_d_t.*
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "pmem_t" 
                     LET g_errparam.code = "std-00009" 
                     LET g_errparam.popup = TRUE 
                     CALL s_transaction_end('N','0')
                     CALL cl_err()
                     
                  WHEN SQLCA.SQLCODE #其他錯誤
                     LET g_pmem_d[l_ac].* = g_pmem_d_t.*  
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "pmem_t:",SQLERRMESSAGE 
                     LET g_errparam.code = SQLCA.SQLCODE 
                     LET g_errparam.popup = TRUE 
                     CALL s_transaction_end('N','0')
                     CALL cl_err()                   
                     
                  OTHERWISE
                     #資料多語言用-增/改
                     
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_pmel_m.pmeldocno
               LET gs_keys_bak[1] = g_pmeldocno_t
               LET gs_keys[2] = g_pmem_d[g_detail_idx].pmemseq
               LET gs_keys_bak[2] = g_pmem_d_t.pmemseq
               CALL apmt859_update_b('pmem_t',gs_keys,gs_keys_bak,"'1'")
               END CASE
 
               #將遮罩欄位進行遮蔽
               CALL apmt859_pmem_t_mask_restore('restore_mask_n')
               
