   DECLARE apmt430_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.pmevdocno,t0.pmevdocdt,t0.pmev003,t0.pmev001,t0.pmev002,t0.pmevstus, 
       t0.pmev004,t0.pmev005,t0.pmev006,t0.pmev007,t0.pmev008,t0.pmev009,t0.pmev010,t0.pmev013,t0.pmev014, 
