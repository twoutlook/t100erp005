   LET g_forupd_sql = " SELECT pmevdocno,'',pmevdocdt,pmev003,'',pmev001,'',pmev002,'',pmevstus,pmev004, 
       '',pmev005,'',pmev006,pmev007,pmev008,'',pmev009,'',pmev010,'',pmev013,pmev014,pmev015,pmev016, 
