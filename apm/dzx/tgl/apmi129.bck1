{<section id="apmi129.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION apmi129_b_fill(p_wc2)
   #add-point:b_fill段define name="b_fill.define_customerization"
   {<point name="b_fill.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE p_wc2      STRING
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   {<point name="b_fill.define"/>}
   #end add-point     
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   {<point name="b_fill.pre_function"/>}
   #end add-point
   
   #先清空單身變數內容
   CALL g_pmaw_d.clear()
   CALL g_pmaw2_d.clear()
 
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  DISTINCT pmawstus,pmaw011,pmaw012,pmaw031,pmaw032,pmaw013,pmaw014,pmaw015,pmaw016, 
       pmaw033,pmaw034,pmaw017,pmaw018,pmaw019,pmaw020,pmaw021,pmaw022,pmaw100,pmaw011,pmaw012,pmaw013, 
       pmawownid,pmawowndp,pmawcrtid,pmawcrtdp,pmawcrtdt,pmawmodid,pmawmoddt,t1.imaal003 ,t2.imaal004 , 
       t3.oocql004 ,t5.oocal003 ,t6.imaal003 ,t7.imaal004 ,t8.oocql004 ,t9.rtaxl003 ,t10.ooag011 ,t11.ooefl003 , 
       t12.ooag011 ,t13.ooefl003 ,t14.ooag011 FROM pmaw_t",   
               "",
               
                              " LEFT JOIN imaal_t t1 ON t1.imaalent="||g_enterprise||" AND t1.imaal001=pmaw011 AND t1.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t2 ON t2.imaalent="||g_enterprise||" AND t2.imaal001=pmaw011 AND t2.imaal002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t3 ON t3.oocqlent="||g_enterprise||" AND t3.oocql001='2003' AND t3.oocql002=pmaw031 AND t3.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t5 ON t5.oocalent="||g_enterprise||" AND t5.oocal001=pmaw013 AND t5.oocal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t6 ON t6.imaalent="||g_enterprise||" AND t6.imaal001=pmaw015 AND t6.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t7 ON t7.imaalent="||g_enterprise||" AND t7.imaal001=pmaw015 AND t7.imaal002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t8 ON t8.oocqlent="||g_enterprise||" AND t8.oocql001='2003' AND t8.oocql002=pmaw033 AND t8.oocql003='"||g_dlang||"' ",
               " LEFT JOIN rtaxl_t t9 ON t9.rtaxlent="||g_enterprise||" AND t9.rtaxl001=pmaw034 AND t9.rtaxl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t10 ON t10.ooagent="||g_enterprise||" AND t10.ooag001=pmawownid  ",
               " LEFT JOIN ooefl_t t11 ON t11.ooeflent="||g_enterprise||" AND t11.ooefl001=pmawowndp AND t11.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t12 ON t12.ooagent="||g_enterprise||" AND t12.ooag001=pmawcrtid  ",
               " LEFT JOIN ooefl_t t13 ON t13.ooeflent="||g_enterprise||" AND t13.ooefl001=pmawcrtdp AND t13.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t14 ON t14.ooagent="||g_enterprise||" AND t14.ooag001=pmawmodid  ",
 
               " WHERE pmawent= ? AND pmaw001=? AND pmaw002=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("pmaw_t")
   END IF
   
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF apmi129_fill_chk(1) THEN
      IF (g_action_choice = "query" OR cl_null(g_action_choice))
         #add-point:b_fill段long_sql_if name="b_fill.long_sql_if"
         {<point name="b_fill.long_sql_if"/>}
         #end add-point
      THEN
         LET g_sql = g_sql, " ORDER BY pmaw_t.pmaw011,pmaw_t.pmaw012,pmaw_t.pmaw013,pmaw_t.pmaw031,pmaw_t.pmaw032"
         #add-point:b_fill段fill_before name="b_fill.fill_before"
         {<point name="b_fill.fill_before"/>}
         #end add-point
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE apmi129_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR apmi129_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_pmaw_m.pmaw001,g_pmaw_m.pmaw002
                                               
      FOREACH b_fill_cs INTO g_pmaw_d[l_ac].pmawstus,g_pmaw_d[l_ac].pmaw011,g_pmaw_d[l_ac].pmaw012,g_pmaw_d[l_ac].pmaw031, 
          g_pmaw_d[l_ac].pmaw032,g_pmaw_d[l_ac].pmaw013,g_pmaw_d[l_ac].pmaw014,g_pmaw_d[l_ac].pmaw015, 
          g_pmaw_d[l_ac].pmaw016,g_pmaw_d[l_ac].pmaw033,g_pmaw_d[l_ac].pmaw034,g_pmaw_d[l_ac].pmaw017, 
          g_pmaw_d[l_ac].pmaw018,g_pmaw_d[l_ac].pmaw019,g_pmaw_d[l_ac].pmaw020,g_pmaw_d[l_ac].pmaw021, 
          g_pmaw_d[l_ac].pmaw022,g_pmaw_d[l_ac].pmaw100,g_pmaw2_d[l_ac].pmaw011,g_pmaw2_d[l_ac].pmaw012, 
          g_pmaw2_d[l_ac].pmaw013,g_pmaw2_d[l_ac].pmawownid,g_pmaw2_d[l_ac].pmawowndp,g_pmaw2_d[l_ac].pmawcrtid, 
          g_pmaw2_d[l_ac].pmawcrtdp,g_pmaw2_d[l_ac].pmawcrtdt,g_pmaw2_d[l_ac].pmawmodid,g_pmaw2_d[l_ac].pmawmoddt, 
          g_pmaw_d[l_ac].pmaw011_desc,g_pmaw_d[l_ac].pmaw011_desc_desc,g_pmaw_d[l_ac].pmaw031_desc,g_pmaw_d[l_ac].pmaw013_desc, 
          g_pmaw_d[l_ac].pmaw015_desc,g_pmaw_d[l_ac].pmaw015_desc_desc,g_pmaw_d[l_ac].pmaw033_desc,g_pmaw_d[l_ac].pmaw034_desc, 
          g_pmaw2_d[l_ac].pmawownid_desc,g_pmaw2_d[l_ac].pmawowndp_desc,g_pmaw2_d[l_ac].pmawcrtid_desc, 
          g_pmaw2_d[l_ac].pmawcrtdp_desc,g_pmaw2_d[l_ac].pmawmodid_desc
                             
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充 name="b_fill.fill"
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #應用 a12 樣板自動產生(Version:4)
 
 
 
 
        
         #add-point:單身資料抓取 name="bfill.foreach"
         {<point name="bfill.foreach"/>}
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF 
            EXIT FOREACH
         END IF
      
         LET l_ac = l_ac + 1
         
      END FOREACH
 
            CALL g_pmaw_d.deleteElement(g_pmaw_d.getLength())
      CALL g_pmaw2_d.deleteElement(g_pmaw2_d.getLength())
 
      
   END IF
   
   #add-point:b_fill段more name="b_fill.more"
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_pmaw_d.getLength()
      LET g_pmaw_d_mask_o[l_ac].* =  g_pmaw_d[l_ac].*
      CALL apmi129_pmaw_t_mask()
      LET g_pmaw_d_mask_n[l_ac].* =  g_pmaw_d[l_ac].*
   END FOR
   
   LET g_pmaw2_d_mask_o.* =  g_pmaw2_d.*
   FOR l_ac = 1 TO g_pmaw2_d.getLength()
      LET g_pmaw2_d_mask_o[l_ac].* =  g_pmaw2_d[l_ac].*
      CALL apmi129_pmaw_t_mask()
      LET g_pmaw2_d_mask_n[l_ac].* =  g_pmaw2_d[l_ac].*
   END FOR
 
 
   FREE apmi129_pb   
   
END FUNCTION

