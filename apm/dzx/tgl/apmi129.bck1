   LET l_insert = FALSE
   LET g_action_choice = ""
 
   #add-point:input段define_sql name="input.define_sql"
   {<point name="input.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT pmawstus,pmaw011,pmaw012,pmaw031,pmaw032,pmaw013,pmaw014,pmaw015,pmaw016, 
       pmaw033,pmaw034,pmaw017,pmaw018,pmaw019,pmaw020,pmaw021,pmaw022,pmaw100,pmaw011,pmaw012,pmaw013, 
       pmawownid,pmawowndp,pmawcrtid,pmawcrtdp,pmawcrtdt,pmawmodid,pmawmoddt FROM pmaw_t WHERE pmawent=?  
       AND pmaw001=? AND pmaw002=? AND pmaw011=? AND pmaw012=? AND pmaw013=? AND pmaw031=? AND pmaw032=?  
       FOR UPDATE"
   #add-point:input段define_sql name="input.after_define_sql"
   {<point name="input.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE apmi129_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR
 
 
   
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   LET g_qryparam.state = 'i'
   
   #控制key欄位可否輸入
   CALL apmi129_set_entry(p_cmd)
   #add-point:set_entry後 name="input.after_set_entry"
   {<point name="input.after_set_entry"/>}
   #end add-point
   CALL apmi129_set_no_entry(p_cmd)
   #add-point:set_no_entry後 name="input.after_set_no_entry"
   {<point name="input.after_set_no_entry"/>}
   #end add-point
 
   DISPLAY BY NAME g_pmaw_m.pmaw001,g_pmaw_m.pmaw002
   
   LET lb_reproduce = FALSE
   
   #關閉被遮罩相關欄位輸入, 無法確定USER是否會需要輸入此欄位
   #因此先行關閉, 若有需要可於下方add-point中自行開啟
   CALL cl_mask_set_no_entry()
   
   #add-point:進入修改段前 name="input.before_input"
   {<point name="input.before_input"/>}
   #end add-point
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)

{</section>}

{<section id="apmi129.input.head" >}
   
      #單頭段
      INPUT BY NAME g_pmaw_m.pmaw001,g_pmaw_m.pmaw002 
         ATTRIBUTE(WITHOUT DEFAULTS)
         
         #自訂單頭ACTION
         
         
         BEFORE INPUT
            IF s_transaction_chk("N",0) THEN
               CALL s_transaction_begin()
            END IF
            
            IF l_cmd_t = 'r' THEN
               
            END IF
            #add-point:單頭input前 name="input.head.b_input"
            {<point name="input.head.b_input"/>}
            #end add-point
          
                  #應用 a02 樣板自動產生(Version:2)
         AFTER FIELD pmaw001
            
            #add-point:AFTER FIELD pmaw001 name="input.a.pmaw001"
            {<point name="input.a.pmaw001" />}
            #END add-point
            
 
 
         #應用 a01 樣板自動產生(Version:2)
         BEFORE FIELD pmaw001
            #add-point:BEFORE FIELD pmaw001 name="input.b.pmaw001"
            {<point name="input.b.pmaw001" />}
            #END add-point
 
 
         #應用 a04 樣板自動產生(Version:3)
         ON CHANGE pmaw001
            #add-point:ON CHANGE pmaw001 name="input.g.pmaw001"
            {<point name="input.g.pmaw001" />}
            #END add-point 
 
 
         #應用 a02 樣板自動產生(Version:2)
