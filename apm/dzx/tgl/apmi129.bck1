               LET gs_keys_bak[3] = g_pmaw_d_t.pmaw011
               LET gs_keys[4] = g_pmaw_d[g_detail_idx].pmaw012
               LET gs_keys_bak[4] = g_pmaw_d_t.pmaw012
               LET gs_keys[5] = g_pmaw_d[g_detail_idx].pmaw013
               LET gs_keys_bak[5] = g_pmaw_d_t.pmaw013
               LET gs_keys[6] = g_pmaw_d[g_detail_idx].pmaw031
               LET gs_keys_bak[6] = g_pmaw_d_t.pmaw031
               LET gs_keys[7] = g_pmaw_d[g_detail_idx].pmaw032
               LET gs_keys_bak[7] = g_pmaw_d_t.pmaw032
               CALL apmi129_update_b('pmaw_t',gs_keys,gs_keys_bak,"'1'")
                     
 
                     #add-point:單頭修改後 name="input.head.a_update"
                     {<point name="input.head.a_update"/>}
                     #end add-point
    
                     #頭先不紀錄
                     #LET g_log1 = util.JSON.stringify(g_pmaw_m_t)
                     #LET g_log2 = util.JSON.stringify(g_pmaw_m)
                     #IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                     #   CALL s_transaction_end('N','0')
                     #ELSE
                        CALL s_transaction_end('Y','0')
                     #END IF
               END CASE
            
               #將遮罩欄位進行遮蔽
               CALL apmi129_pmaw_t_mask_restore('restore_mask_n')
            
            ELSE    
               #add-point:單頭新增 name="input.head.a_insert"
               {<point name="input.head.a_insert"/>}
               #end add-point
               
               #多語言處理
               
                         
               IF l_cmd_t = 'r' AND p_cmd = 'a' THEN
                  CALL apmi129_detail_reproduce()
               END IF
               
               LET p_cmd = 'u'
            END IF
 
           LET g_pmaw001_t = g_pmaw_m.pmaw001
           LET g_pmaw002_t = g_pmaw_m.pmaw002
 
           
           IF g_pmaw_d.getLength() = 0 THEN
