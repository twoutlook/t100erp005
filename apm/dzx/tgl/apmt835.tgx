               LET g_log1 = util.JSON.stringify(g_pmdc_m),util.JSON.stringify(g_pmdd_d_t)
               LET g_log2 = util.JSON.stringify(g_pmdc_m),util.JSON.stringify(g_pmdd_d[l_ac])
