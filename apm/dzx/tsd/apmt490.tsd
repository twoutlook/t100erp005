  <field cite_std="N" name="pmea_t.pmeaacti" ver="1" src="s" column="pmeaacti" table="pmea_t" attribute="C001" type="varchar2(1)" req="Y" i_zoom="" c_zoom="" chk_ref="" items="" default="N" max="" min="" can_edit="Y" can_query="Y" widget="CheckBox" status="">
