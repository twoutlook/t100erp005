  <field cite_std="N" name="pmaa_t.pmaa069" ver="1" src="s" column="pmaa069" table="pmaa_t" attribute="" type="" req="Y" i_zoom="" c_zoom="" chk_ref="" items="" default="Y" max_compare="" max="" min_compare="" min="" can_edit="Y" can_query="Y" widget="CheckBox" status="">
