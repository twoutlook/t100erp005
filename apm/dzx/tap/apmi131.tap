  <section id="apmi131.b_fill" ver="1" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION apmi131_b_fill(p_wc2)
   {<Local define>}
   DEFINE p_wc2      STRING
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_pmaq_d.clear()
   CALL g_pmaq2_d.clear()
 
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
      LET g_sql = "SELECT  UNIQUE pmaq003,'','',pmaqstus,'',pmaqownid,'',pmaqowndp,'',pmaqcrtid,'',pmaqcrtdp, 
          '',pmaqcrtdt,pmaqmodid,'',pmaqmoddt FROM pmaq_t",   
                  "",
                  
                  " WHERE pmaqent= ? AND pmaqsite= ? AND pmaq001=? AND pmaq002=? AND pmaq004=?"  
                  
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF apmi131_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY pmaq_t.pmaq003"
      
      PREPARE apmi131_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR apmi131_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise, g_site,g_pmaq_m.pmaq001
                                               ,g_pmaq_m.pmaq002
 
                                               ,g_pmaq_m.pmaq004
 
 
                                               
      FOREACH b_fill_cs INTO g_pmaq_d[l_ac].pmaq003,g_pmaq_d[l_ac].pmaq003_desc,g_pmaq_d[l_ac].imaal004, 
          g_pmaq_d[l_ac].pmaqstus,g_pmaq2_d[l_ac].pmaq003,g_pmaq2_d[l_ac].pmaqownid,g_pmaq2_d[l_ac].pmaqownid_desc, 
          g_pmaq2_d[l_ac].pmaqowndp,g_pmaq2_d[l_ac].pmaqowndp_desc,g_pmaq2_d[l_ac].pmaqcrtid,g_pmaq2_d[l_ac].pmaqcrtid_desc, 
          g_pmaq2_d[l_ac].pmaqcrtdp,g_pmaq2_d[l_ac].pmaqcrtdp_desc,g_pmaq2_d[l_ac].pmaqcrtdt,g_pmaq2_d[l_ac].pmaqmodid, 
          g_pmaq2_d[l_ac].pmaqmodid_desc,g_pmaq2_d[l_ac].pmaqmoddt
                             ,g_pmaq_d[l_ac].pmaq003,g_pmaq_d[l_ac].pmaq003_desc,g_pmaq_d[l_ac].imaal004, 
                                 g_pmaq_d[l_ac].pmaqstus,g_pmaq2_d[l_ac].pmaq003,g_pmaq2_d[l_ac].pmaqownid, 
                                 g_pmaq2_d[l_ac].pmaqownid_desc,g_pmaq2_d[l_ac].pmaqowndp,g_pmaq2_d[l_ac].pmaqowndp_desc, 
                                 g_pmaq2_d[l_ac].pmaqcrtid,g_pmaq2_d[l_ac].pmaqcrtid_desc,g_pmaq2_d[l_ac].pmaqcrtdp, 
                                 g_pmaq2_d[l_ac].pmaqcrtdp_desc,g_pmaq2_d[l_ac].pmaqcrtdt,g_pmaq2_d[l_ac].pmaqmodid, 
                                 g_pmaq2_d[l_ac].pmaqmodid_desc,g_pmaq2_d[l_ac].pmaqmoddt
 
 
                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_pmaq2_d[l_ac].pmaqownid_desc = cl_get_username(g_pmaq2_d[l_ac].pmaqownid)
      #LET g_pmaq2_d[l_ac].pmaqowndp_desc = cl_get_deptname(g_pmaq2_d[l_ac].pmaqowndp)
      #LET g_pmaq2_d[l_ac].pmaqcrtid_desc = cl_get_username(g_pmaq2_d[l_ac].pmaqcrtid)
      #LET g_pmaq2_d[l_ac].pmaqcrtdp_desc = cl_get_deptname(g_pmaq2_d[l_ac].pmaqcrtdp)
      #LET g_pmaq2_d[l_ac].pmaqmodid_desc = cl_get_username(g_pmaq2_d[l_ac].pmaqmodid)
      ##LET g_pmaq_d[l_ac].pmaqcnfid_desc = cl_get_deptname(g_pmaq_d[l_ac].pmaqcnfid)
      ##LET g_pmaq_d[l_ac].pmaqpstid_desc = cl_get_deptname(g_pmaq_d[l_ac].pmaqpstid)
      
 
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_pmaq_d.deleteElement(g_pmaq_d.getLength())
      CALL g_pmaq2_d.deleteElement(g_pmaq2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE apmi131_pb   
   
END FUNCTION
]]>
