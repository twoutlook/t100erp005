  <point name="function.apmi131_pmaq003_chk" order="5" ver="1" cite_std="N" cite_ver="" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 可替代料號檢查,檢查是否有輸入*號的料件，若有，只能輸入ABC*格式
# Memo...........:
# Usage..........: CALL apmi131_pmaq003_chk(p_pmaq003)
#                  RETURNING r_success
# Input parameter: p_pmaq003   料號
# Return code....: r_success   TRUE/FALSE
# Date & Author..: 2013/11/11 by lixiang
# Modify.........:
################################################################################
PRIVATE FUNCTION apmi131_pmaq003_chk(p_pmaq003)
DEFINE p_pmaq003    STRING
DEFINE l_index      LIKE type_t.num5
DEFINE l_len        LIKE type_t.num5
DEFINE r_success    LIKE type_t.num5

       LET l_index = 0
       LET r_success = TRUE
       
       IF NOT cl_null(p_pmaq003) THEN
          LET l_index = p_pmaq003.getIndexOf('*',1)
          LET l_len = p_pmaq003.getLength()
          IF l_index > 0 THEN  #存在*號
             #*號在第一位，或者只輸入的*號的情況，不符合條件
             #*號不在最後一位，不符合條件，只能輸入ABC*格式
             IF l_index = 1 OR l_index < l_len THEN     
                CALL cl_err(p_pmaq003,'apm-00211',1)
                LET r_success = FALSE
                RETURN r_success              
             END IF
          END IF
       END IF
       RETURN r_success
       
END FUNCTION]]>
