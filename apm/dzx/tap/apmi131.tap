   LET g_forupd_sql = "SELECT pmaq004,pmaq002,'','',pmaq001,'' FROM pmaq_t WHERE pmaqent= ? AND pmaqsite=  
       ? AND pmaq001=? AND pmaq002=? AND pmaq004=? FOR UPDATE"
   #add-point:SQL_define
