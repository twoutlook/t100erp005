  <section id="apmi131.modify" ver="1" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION apmi131_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_pmaq_m.pmaq001 IS NULL
   OR g_pmaq_m.pmaq002 IS NULL
 
   OR g_pmaq_m.pmaq004 IS NULL
 
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
    SELECT UNIQUE pmaq004,pmaq002,pmaq001
 INTO g_pmaq_m.pmaq004,g_pmaq_m.pmaq002,g_pmaq_m.pmaq001
 FROM pmaq_t
 WHERE pmaqent = g_enterprise AND pmaqsite = g_site AND pmaq001 = g_pmaq_m.pmaq001 AND pmaq002 = g_pmaq_m.pmaq002 AND pmaq004 = g_pmaq_m.pmaq004
 
   ERROR ""
  
   LET g_pmaq001_t = g_pmaq_m.pmaq001
   LET g_pmaq002_t = g_pmaq_m.pmaq002
 
   LET g_pmaq004_t = g_pmaq_m.pmaq004
 
 
   CALL s_transaction_begin()
   
   OPEN apmi131_cl USING g_enterprise, g_site,g_pmaq_m.pmaq001
                                                       ,g_pmaq_m.pmaq002
 
                                                       ,g_pmaq_m.pmaq004
 
 
   IF STATUS THEN
      CALL cl_err("OPEN apmi131_cl:", STATUS, 1)
      CLOSE apmi131_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH apmi131_cl INTO g_pmaq_m.pmaq004,g_pmaq_m.pmaq002,g_pmaq_m.imaal003,g_pmaq_m.imaal004,g_pmaq_m.pmaq001, 
       g_pmaq_m.pmaal004
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_pmaq_m.pmaq001,SQLCA.sqlcode,0)
      CLOSE apmi131_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL apmi131_show()
   WHILE TRUE
      LET g_pmaq001_t = g_pmaq_m.pmaq001
      LET g_pmaq002_t = g_pmaq_m.pmaq002
 
      LET g_pmaq004_t = g_pmaq_m.pmaq004
 
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL apmi131_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_pmaq_m.* = g_pmaq_m_t.*
         CALL apmi131_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_pmaq_m.pmaq001 != g_pmaq001_t 
      OR g_pmaq_m.pmaq002 != g_pmaq002_t 
 
      OR g_pmaq_m.pmaq004 != g_pmaq004_t 
 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE pmaq_t SET pmaq001 = g_pmaq_m.pmaq001
                                       , pmaq002 = g_pmaq_m.pmaq002
 
                                       , pmaq004 = g_pmaq_m.pmaq004
 
 
          WHERE pmaqent = g_enterprise AND pmaqsite = g_site AND pmaq001 = g_pmaq001_t
            AND pmaq002 = g_pmaq002_t
 
            AND pmaq004 = g_pmaq004_t
 
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("pmaq_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("pmaq_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_pmaq_m.pmaq001,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE apmi131_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_pmaq_m.pmaq001,'U')
 
   CALL apmi131_b_fill("1=1")
   
END FUNCTION   
]]>
  </section>
