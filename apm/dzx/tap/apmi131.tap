   DELETE FROM pmaq_t
    WHERE pmaqent = g_enterprise AND pmaqsite = g_site AND pmaq001 = g_pmaq_m.pmaq001 AND
                              pmaq002 = g_pmaq_m.pmaq002 AND
 
