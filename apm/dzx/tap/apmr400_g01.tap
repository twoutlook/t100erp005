  <section id="apmr400_g01.sel_prep" ver="13" status="" src="s">
    <![CDATA[#+ 選單功能實際執行處
PRIVATE FUNCTION apmr400_g01_sel_prep()
   #add-point:sel_prep段define
   {<point name="sel_prep.define"/>}
   #end add-point
 
   #add-point:sel_prep before
   {<point name="sel_prep.before" mark="Y"/>}
   #end add-point
   
   #add-point:sel_prep g_select
   {<point name="sel_prep.g_select" mark="Y"/>}
   #end add-point
   LET g_select = " SELECT pmda001,pmda002,pmda003,pmda004,pmda005,pmda006,pmda007,pmda008,pmda009,pmda010, 
       pmda011,pmda012,pmda020,pmda021,pmda022,pmdadocdt,pmdadocno,pmdaent,pmdasite,pmdastus,pmdb001, 
       pmdb002,pmdb003,pmdb004,pmdb005,pmdb006,pmdb007,pmdb008,pmdb009,pmdb010,pmdb011,pmdb012,pmdb014, 
       pmdb015,pmdb016,pmdb017,pmdb018,pmdb019,pmdb020,pmdb021,pmdb030,pmdb031,pmdb032,pmdb033,pmdb034, 
       pmdb035,pmdb036,pmdb037,pmdb038,pmdb039,pmdb040,pmdb041,pmdb042,pmdb043,pmdb044,pmdb045,pmdb046, 
       pmdb048,pmdb049,pmdb050,pmdb051,pmdbseq,pmdbsite,oofa_t.oofa011,t1.ooefl003,x.t4_ooefl003,x.t5_ooefl003, 
       ooefl_t.ooefl003,ooail_t.ooail003,oocql_t.oocql004,x.t2_oocql004,x.t3_oocql004,x.t6_oocql004, 
       x.t7_oocql004,x.imaal_t_imaal003,x.t10_imaal003,x.oocal_t_oocal003,x.t8_oocal003,x.t9_oocal003, 
       x.pmaal_t_pmaal004,x.ooibl_t_ooibl004,x.inaa_t_inaa002,x.inab_t_inab003,trim(pmdb014)||'.'||trim(pmdb015), 
       x.t10_imaal004,trim(pmda002)||'.'||trim(oofa_t.oofa011),'','',trim(pmda003)||'.'||trim(t1.ooefl003), 
       trim(pmda007)||'.'||trim(ooefl_t.ooefl003),'',trim(pmdb015)||'.'||trim(x.pmaal_t_pmaal004)"
 
   #add-point:sel_prep g_from
   {<point name="sel_prep.g_from" mark="Y"/>}
   #end add-point
    LET g_from = " FROM pmda_t LEFT OUTER JOIN ooefl_t ON ooefl_t.ooefl001 = pmda_t.pmda007 AND ooefl_t.ooeflent = pmda_t.pmdaent AND ooefl_t.ooefl002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN ooail_t ON ooail_t.ooail001 = pmda_t.pmda005 AND ooail_t.ooailent = pmda_t.pmdaent AND ooail_t.ooail002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN ooefl_t t1 ON t1.ooefl001 = pmda_t.pmda003 AND t1.ooeflent = pmda_t.pmdaent AND t1.ooefl002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oofa_t ON oofa_t.oofa002 = '2' AND oofa_t.oofa003 = pmda_t.pmda002 AND oofa_t.oofaent = pmda_t.pmdaent             LEFT OUTER JOIN oocql_t ON oocql_t.oocql001 = '263' AND oocql_t.oocql002 = pmda_t.pmda021 AND oocql_t.oocqlent = pmda_t.pmdaent AND oocql_t.oocql003 = '" , 
        g_dlang,"'" ," LEFT OUTER JOIN ( SELECT pmdb_t.*,t4.ooefl003 t4_ooefl003,t5.ooefl003 t5_ooefl003, 
        t2.oocql004 t2_oocql004,t3.oocql004 t3_oocql004,t6.oocql004 t6_oocql004,t7.oocql004 t7_oocql004, 
        imaal_t.imaal003 imaal_t_imaal003,t10.imaal003 t10_imaal003,oocal_t.oocal003 oocal_t_oocal003, 
        t8.oocal003 t8_oocal003,t9.oocal003 t9_oocal003,pmaal_t.pmaal004 pmaal_t_pmaal004,ooibl_t.ooibl004 ooibl_t_ooibl004, 
        inaa_t.inaa002 inaa_t_inaa002,inab_t.inab003 inab_t_inab003,t10.imaal004 t10_imaal004 FROM pmdb_t LEFT OUTER JOIN oocql_t t2 ON t2.oocql001 = '238' AND t2.oocql002 = pmdb_t.pmdb017 AND t2.oocqlent = pmdb_t.pmdbent AND t2.oocql003 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oocql_t t3 ON t3.oocql001 = '265' AND t3.oocql002 = pmdb_t.pmdb031 AND t3.oocqlent = pmdb_t.pmdbent AND t3.oocql003 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN ooefl_t t4 ON t4.ooefl001 = pmdb_t.pmdb037 AND t4.ooeflent = pmdb_t.pmdbent AND t4.ooefl002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN inaa_t ON inaa_t.inaa001 = pmdb_t.pmdb038 AND inaa_t.inaaent = pmdb_t.pmdbent AND inaa_t.inaasite = pmdb_t.pmdbsite             LEFT OUTER JOIN inab_t ON inab_t.inab001 = pmdb_t.pmdb038 AND inab_t.inab002 = pmdb_t.pmdb039 AND inab_t.inabent = pmdb_t.pmdbent AND inab_t.inabsite = pmdb_t.pmdbsite             LEFT OUTER JOIN ooefl_t t5 ON t5.ooefl001 = pmdb_t.pmdb046 AND t5.ooeflent = pmdb_t.pmdbent AND t5.ooefl002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oocql_t t6 ON t6.oocql001 = '274' AND t6.oocql002 = pmdb_t.pmdb048 AND t6.oocqlent = pmdb_t.pmdbent AND t6.oocql003 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oocql_t t7 ON t7.oocql001 = '265' AND t7.oocql002 = pmdb_t.pmdb051 AND t7.oocqlent = pmdb_t.pmdbent AND t7.oocql003 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN ooibl_t ON ooibl_t.ooibl002 = pmdb_t.pmdb016 AND ooibl_t.ooiblent = pmdb_t.pmdbent AND ooibl_t.ooibl003 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN imaal_t ON imaal_t.imaal001 = pmdb_t.pmdb012 AND imaal_t.imaalent = pmdb_t.pmdbent AND imaal_t.imaal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oocal_t ON oocal_t.oocal001 = pmdb_t.pmdb011 AND oocal_t.oocalent = pmdb_t.pmdbent AND oocal_t.oocal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oocal_t t8 ON t8.oocal001 = pmdb_t.pmdb009 AND t8.oocalent = pmdb_t.pmdbent AND t8.oocal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oocal_t t9 ON t9.oocal001 = pmdb_t.pmdb007 AND t9.oocalent = pmdb_t.pmdbent AND t9.oocal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN imaal_t t10 ON t10.imaal001 = pmdb_t.pmdb004 AND t10.imaalent = pmdb_t.pmdbent AND t10.imaal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN pmaal_t ON pmaal_t.pmaal001 = pmdb_t.pmdb015 AND pmaal_t.pmaalent = pmdb_t.pmdbent AND pmaal_t.pmaal002 = '" , 
        g_dlang,"'" ," ) x  ON pmda_t.pmdaent = x.pmdbent AND pmda_t.pmdadocno = x.pmdbdocno AND pmda_t.pmdasite  
        = x.pmdbsite"
 
   #add-point:sel_prep g_where
   {<point name="sel_prep.g_where" mark="Y"/>}
   #end add-point
    LET g_where = " WHERE " ,tm.wc CLIPPED
 
   #add-point:sel_prep g_order
   {<point name="sel_prep.g_order" mark="Y"/>}
   #end add-point
    LET g_order = " ORDER BY pmdadocno,pmdbseq"
 
   #add-point:sel_prep.sql.before
   {<point name="sel_prep.sql.before mark="Y""/>}
   #end add-point:sel_prep.sql.before
 
