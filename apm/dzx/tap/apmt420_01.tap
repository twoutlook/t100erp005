  <point name="function.apmt420_01_pmdg_show" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION apmt420_01_pmdg_show()
DEFINE l_pmdfstus   LIKE pmdf_t.pmdfstus

    #SELECT pmdgdocno,pmdgseq,pmdg003,pmdg004,pmdg005,pmdg014,pmdg015,pmdg007 
    #  INTO g_pmdg_m.pmdgdocno,g_pmdg_m.pmdgseq,g_pmdg_m.pmdg003,g_pmdg_m.pmdg004,g_pmdg_m.pmdg005,
    #       g_pmdg_m.pmdg014,g_pmdg_m.pmdg015,g_pmdg_m.pmdg007
    #  FROM pmdg_t
    #  WHERE pmdgent = g_enterprise AND pmdgdocno = g_pmdgdocno AND pmdgseq = g_pmdgseq
      
    LET g_pmdg_m.pmdgdocno = g_pmdgdocno
    LET g_pmdg_m.pmdgseq = g_pmdgseq
    LET g_pmdg_m.pmdg003 = g_pmdg003
    LET g_pmdg_m.pmdg004 = g_pmdg004
    LET g_pmdg_m.pmdg005 = g_pmdg005
    LET g_pmdg_m.pmdg014 = g_pmdg014
    LET g_pmdg_m.pmdg015 = g_pmdg015
    LET g_pmdg_m.pmdg007 = g_pmdg007
   
    INITIALIZE g_ref_fields TO NULL
    LET g_ref_fields[1] = g_pmdg_m.pmdg003
    CALL ap_ref_array2(g_ref_fields,"SELECT imaal003,imaal004 FROM imaal_t WHERE imaalent='"||g_enterprise||"' AND imaal001=? AND imaal002='"||g_dlang||"'","") RETURNING g_rtn_fields
    LET g_pmdg_m.imaal003 = '', g_rtn_fields[1] , ''
    LET g_pmdg_m.imaal004 = '', g_rtn_fields[2] , ''  
    
    DISPLAY BY NAME g_pmdg_m.pmdgdocno,g_pmdg_m.pmdgseq,g_pmdg_m.pmdg003,g_pmdg_m.imaal003,g_pmdg_m.imaal004,
                    g_pmdg_m.pmdg004,g_pmdg_m.pmdg005,g_pmdg_m.pmdg014,g_pmdg_m.pmdg015,g_pmdg_m.pmdg007
                    
    LET l_pmdfstus = ''
    SELECT pmdfstus INTO l_pmdfstus FROM pmdf_t WHERE pmdfent = g_enterprise AND pmdfdocno = g_pmdgdocno
    CALL cl_set_act_visible("modify",TRUE)
    IF l_pmdfstus != 'N' THEN
       CALL cl_set_act_visible("modify",FALSE)
    END IF
                    
END FUNCTION]]>
  </point>
