       LET r_success = TRUE
       LET l_pmdh001 = 0
       LET l_pmdh002 = 0
       #IF cl_null(p_pmdh001) THEN
       #   LET p_pmdh001 = 0
       #END IF

       LET l_sql = " SELECT pmdh001,pmdh002 FROM pmdh_t ",
                   " WHERE pmdhent = '",g_enterprise,"' AND pmdhdocno = '",g_pmdgdocno,"' AND pmdhseq = '",g_pmdgseq,"' "
       
       IF NOT cl_null(g_pmdh_d_t.pmdh001) AND NOT cl_null(g_pmdh_d_t.pmdh002) THEN  #修改的時候，排除當前筆的舊值
          LET l_sql = l_sql," AND pmdh001 != '",g_pmdh_d_t.pmdh001,"' AND pmdh002 != '",g_pmdh_d_t.pmdh002,"' "
       END IF
       
       DECLARE pmdh_cs CURSOR FROM l_sql
       FOREACH pmdh_cs INTO l_pmdh001,l_pmdh002
          #輸入的數量不可在當前起始和截止數量之間，即p_pmdh001不可 l_pmdh001<= p_pmdh001<=l_pmdh00
          IF NOT cl_null(p_pmdh001) THEN
             IF p_pmdh001 >= l_pmdh001 AND p_pmdh001 <= l_pmdh002 THEN  #輸入的數量不可在當前起始和截止數量之間，即p_pmdh001不可 l_pmdh001<= p_pmdh001<=l_pmdh002
                INITIALIZE g_errparam TO NULL
                LET g_errparam.code = 'apm-00227'
                LET g_errparam.extend = p_pmdh001
                LET g_errparam.popup = TRUE
                CALL cl_err()

                LET r_success = FALSE
                RETURN r_success
             END IF
          END IF
          IF NOT cl_null(p_pmdh002) THEN
             IF p_pmdh002 >= l_pmdh001 AND p_pmdh002 <= l_pmdh002 THEN  #輸入的數量不可在當前起始和截止數量之間，即p_pmdh001不可 l_pmdh001<= p_pmdh001<=l_pmdh002
                INITIALIZE g_errparam TO NULL
                LET g_errparam.code = 'apm-00227'
                LET g_errparam.extend = p_pmdh002
                LET g_errparam.popup = TRUE
                CALL cl_err()

                LET r_success = FALSE
                RETURN r_success
             END IF
          END IF
          #輸入的數量，起始數量，截止數量的範圍大於當前存在的
          IF NOT cl_null(p_pmdh001) AND NOT cl_null(p_pmdh002) THEN
             IF p_pmdh001 < l_pmdh001 AND p_pmdh002 > l_pmdh002 THEN
                INITIALIZE g_errparam TO NULL
                LET g_errparam.code = 'apm-00227'
                LET g_errparam.extend = ''
                LET g_errparam.popup = TRUE
                CALL cl_err()

                LET r_success = FALSE
                RETURN r_success
             END IF
          END IF
       END FOREACH
       RETURN r_success
       
END FUNCTION]]>
