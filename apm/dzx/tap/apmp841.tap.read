  <point name="function.apmp841_ins_detail" order="4" ver="3" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 將選取的單號的要貨採購相關資訊寫入暫存檔
# Memo...........:
# Usage..........: CALL apmp841_ins_detail(p_pmdadocno,p_flag)
#                  RETURNING r_success
# Input Parameter: p_pmdadocno   要貨單號
#                  p_flag        Y-新增暫存檔資料;N-僅檢查資料 
# Return Code....: r_success             
# Date & Author..: 2014/12/25 By Lori
# Modify.........:
################################################################################
PRIVATE FUNCTION apmp841_ins_detail(p_pmdadocno,p_flag)
   DEFINE p_pmdadocno   LIKE pmda_t.pmdadocno
   DEFINE p_flag        LIKE type_t.chr1
   DEFINE r_success     LIKE type_t.num5
   DEFINE l_cnt         LIKE type_t.num5
   
   LET r_success = TRUE

   #確認暫存檔中有無資料,若無資料先新增
   LET l_cnt = 0 
   EXECUTE apmp841_cnt_pr_detaial_tmp USING p_pmdadocno
                                      INTO l_cnt
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend = 'apmp841_cnt_pr_detaial_tmp'
      LET g_errparam.popup = TRUE
      CALL cl_err()
      
      DISPLAY "ERROR: apmp841_cnt_pr_detaial_tmp"
   END IF                                       
   #DISPLAY "Slip: ", g_detail_d[l_ac].pmdadocno," Record Count: ",l_cnt
   
   IF l_cnt = 0 THEN
      IF NOT s_apmp841_ins_tmp_pr_detail(p_pmdadocno,p_flag) THEN
         RETURN r_success                    #150519-00009#1 150520 by lori522612 add
      END IF
   END IF                                    #150519-00009#1 150520 by lori522612 add
  #ELSE                                      #150519-00009#1 150520 by lori522612 mark
      LET l_cnt = 0 
      EXECUTE apmp841_cnt_pr_detaial_cnt USING p_pmdadocno
                                         INTO l_cnt
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = 'apmp841_cnt_pr_detaial_cnt'
         LET g_errparam.popup = TRUE
         CALL cl_err()
         
         DISPLAY "ERROR: apmp841_cnt_pr_detaial_cnt"
      END IF                      
      
      IF l_cnt = 0 THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'apm-00842'
         LET g_errparam.extend = p_pmdadocno
         LET g_errparam.popup = TRUE
         CALL cl_err()           
         
         LET r_success = FALSE
      END IF      
   #END IF                                   #150519-00009#1 150520 by lori522612 mark
   
   RETURN r_success
END FUNCTION]]>
  </point>
