  <point name="ui_dialog.more_action" order="" ver="4" cite_std="N" new="N" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[         ON ACTION batch_execute
            #150404-00001#5 Add By Ken 150409 如沒有選取資料或未查詢出資料 則提示錯誤訊息
            LET l_flag = 'N'
            FOR li_idx = 1 TO g_detail_d.getLength()
               IF g_detail_d[li_idx].sel = "Y" THEN
                  LET l_flag = 'Y' 
                  #150519-00009#1 150520 by huangpomelo add---(S) 
                  IF g_detail_idx = 0 THEN
                     LET g_detail_idx = li_idx
                  END IF
                  #150519-00009#1 150520 by huangpomelo add---(S) 
               END IF 
            END FOR

            IF l_flag = 'N' THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 'adb-00078'
               LET g_errparam.extend = ''
               LET g_errparam.popup = TRUE
               CALL cl_err()

               CONTINUE DIALOG
            ELSE            
            #150404-00001#5 
               LET l_pmdn007 = '' 
               LET l_pmdn202 = ''
               
               SELECT pmdn007,pmdn202
                 INTO l_pmdn007,l_pmdn202            
                 FROM s_apmp841_pr_detail              
                WHERE pmdbdocno = g_detail_d[g_detail_idx].pmdadocno
                  AND pmdbseq = g_detail2_d[l_ac2].pmdbseq
                     
               IF l_pmdn007 <> g_detail2_d[l_ac2].l_pmdn007 THEN
                  #轉換成採購包裝數量
                  IF NOT cl_null(g_detail2_d[l_ac2].l_pmdn202) THEN
                     CALL s_aooi250_convert_qty(g_detail2_d[l_ac2].pmdb004,
                                                g_detail2_d[l_ac2].l_pmdn006,g_detail2_d[l_ac2].l_pmdn201,
                                                g_detail2_d[l_ac2].l_pmdn007)
                        RETURNING l_success,g_detail2_d[l_ac2].l_pmdn202
                  END IF
               
                  #轉換成計價數量
                  IF NOT cl_null(g_detail2_d[l_ac2].l_pmdn010) THEN
                     CALL s_aooi250_convert_qty(g_detail2_d[l_ac2].pmdb004,
                                                g_detail2_d[l_ac2].l_pmdn006,g_detail2_d[l_ac2].l_pmdn010,
                                                g_detail2_d[l_ac2].l_pmdn007)
                        RETURNING l_success,g_detail2_d[l_ac2].l_pmdn011
                  END IF                     
               ELSE
                  IF l_pmdn202 <> g_detail2_d[l_ac2].l_pmdn202 THEN
                     #轉換成採購數量
                     IF NOT cl_null(g_detail2_d[l_ac2].l_pmdn007) THEN
                        CALL s_aooi250_convert_qty(g_detail2_d[l_ac2].pmdb004,
                                                   g_detail2_d[l_ac2].l_pmdn201,g_detail2_d[l_ac2].l_pmdn006,
                                                   g_detail2_d[l_ac2].l_pmdn202)
                           RETURNING l_success,g_detail2_d[l_ac2].l_pmdn007
                     END IF
               
                     #轉換成計價數量
                     IF NOT cl_null(g_detail2_d[l_ac2].l_pmdn010) THEN
                        CALL s_aooi250_convert_qty(g_detail2_d[l_ac2].pmdb004,
                                                   g_detail2_d[l_ac2].l_pmdn006,g_detail2_d[l_ac2].l_pmdn010,
                                                   g_detail2_d[l_ac2].l_pmdn007)
                           RETURNING l_success,g_detail2_d[l_ac2].l_pmdn011
                     END IF                
                  END IF
               END IF
               
               CALL s_transaction_begin()
               UPDATE s_apmp841_pr_detail
                  SET pmdn007 = g_detail2_d[l_ac2].l_pmdn007,
                      pmdn202 = g_detail2_d[l_ac2].l_pmdn202,
                      pmdn011 = g_detail2_d[l_ac2].l_pmdn011
                WHERE pmdbdocno = g_detail_d[g_detail_idx].pmdadocno
                  AND pmdbseq = g_detail2_d[l_ac2].pmdbseq
               IF SQLCA.sqlcode THEN
                  CALL s_transaction_end('N',0)
                  
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = SQLCA.sqlcode
                  LET g_errparam.extend = 'Update s_apmp841_pr_detail'
                  LET g_errparam.popup = TRUE
                  CALL cl_err()                
               END IF  
               
               CALL s_transaction_end('Y',0)              
               
               CALL s_transaction_begin()
               CALL cl_err_collect_init()
               
               FOR li_idx = 1 TO g_detail_d.getLength()
                  IF g_detail_d[li_idx].sel = "Y"  THEN                                  
                     LET l_cnt = 0 
                     EXECUTE apmp841_cnt_pr_tmp USING g_detail_d[li_idx].pmdadocno
                                                INTO l_cnt
                     IF SQLCA.sqlcode THEN
                        INITIALIZE g_errparam TO NULL
                        LET g_errparam.code = SQLCA.sqlcode
                        LET g_errparam.extend = 'apmp841_cnt_pr_tmp'
                        LET g_errparam.popup = TRUE
                        CALL cl_err()
                        
                        DISPLAY "ERROR: apmp841_cnt_pr_tmp"
                        CONTINUE FOR     
                     END IF    
                     
                     IF l_cnt = 0 THEN
                        CALL apmp841_ins_pr(li_idx)
                     END IF
                  END IF                
               END FOR
               
               #CALL s_apmp841_1(g_argv[1]) RETURNING l_success,l_wc    #150610-00030#3 150625 By mark 
               #150610-00030#3 150625 By sakura add(S)
               #依拆單方式彙總拋轉資料(預防背景執行時,抓不到l_type)
               CALL s_apmp841_get_slip_para() RETURNING g_type               
               IF g_type = '1' OR g_type = '2' OR g_type = '3' THEN
                  CALL s_apmp841_1(g_argv[1]) RETURNING l_success,l_wc
               ELSE
                  CALL s_apmp841_3(g_argv[1]) RETURNING l_success,l_wc
               END IF
               #150610-00030#3 150625 By sakura add(E)
               IF l_success THEN
                  CALL cl_err_collect_show()
                  CALL s_transaction_end('Y',0)
                  IF cl_null(g_argv[1]) THEN
                     IF NOT cl_null(l_wc) THEN
                     #如有採購單號,則無需開啟採購單維護作業
                        DISPLAY "PR. DocNo.: ",l_wc
                        LET la_param.prog = 'apmt840'
                        LET la_param.param[1] = ''
                        LET la_param.param[2] = l_wc
