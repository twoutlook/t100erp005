     pmdasite        LIKE pmda_t.pmdasite       ,   
     pmdasite_desc   LIKE ooefl_t.ooefl003      ,   
     pmdadocno       LIKE pmda_t.pmdadocno      ,   
     pmdadocdt       LIKE pmda_t.pmdadocdt      ,   
     pmda207         LIKE pmda_t.pmda207        ,    #到貨日期
     pmda203         LIKE pmda_t.pmda203        ,    #需求組織
     pmda203_desc    LIKE ooefl_t.ooefl003      ,   
     pmda002         LIKE pmda_t.pmda002        ,    #要貨人員
     pmda002_desc    LIKE ooag_t.ooag011        ,   
     pmda003         LIKE pmda_t.pmda003        ,    #要貨部門
     pmda003_desc    LIKE ooefl_t.ooefl003      ,   
