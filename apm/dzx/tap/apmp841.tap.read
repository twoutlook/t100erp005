  <point name="b_fill.sql_before" order="" ver="3" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   CALL s_aooi500_sql_where(g_prog,'pmdasite') RETURNING l_where
   
   IF cl_null(g_def_wc) THEN 
      LET g_def_wc = " 1=1"
   END IF
   
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   
   LET g_sql = "SELECT 'N',pmdasite,t1.ooefl003,pmdadocno,pmdadocdt, ",       
               "       pmda207,pmda203,t2.ooefl003,pmda002,ooag011, ", 
               "       pmda003,t3.ooefl003,pmda021,pmda201,pmda204, ",         
               "       pmda205 ",
               "  FROM pmda_t ",
               "       LEFT OUTER JOIN ooefl_t t1 ON t1.ooeflent = pmdaent AND t1.ooefl001 = pmdasite AND t1.ooefl002 = '",g_dlang,"' ",               
               "       LEFT OUTER JOIN ooefl_t t2 ON t2.ooeflent = pmdaent AND t2.ooefl001 = pmda203  AND t2.ooefl002 = '",g_dlang,"' ",
               "       LEFT OUTER JOIN ooag_t ON ooagent = pmdaent AND ooag001 = pmda002 ",
               "       LEFT OUTER JOIN ooefl_t t3 ON t3.ooeflent = pmdaent AND t3.ooefl001 = pmda003  AND t3.ooefl002 = '",g_dlang,"' ",
               " WHERE pmdaent = ? ",
               "   AND pmdastus = 'Y' ",
               g_def_wc,
               "   AND ",g_wc,
               "   AND ",l_where,
               "   AND EXISTS(SELECT 1 FROM pmdb_t ",
               "               WHERE pmdbent = pmdaent AND pmdbdocno = pmdadocno ",
               "                 AND pmdb032 ='1'      AND pmdb207 = '0' ",
               #150519-00009#1 150520 by lori522612 add---(S)
               "                 AND pmdb006 - (SELECT COALESCE (SUM (pmdp024), 0) FROM pmdp_t ",
               "                                 WHERE pmdpent = pmdbent AND pmdp003 = pmdbdocno AND pmdp004 = pmdbseq ",
               "                                   AND EXISTS (SELECT 1 FROM pmdl_t ",
               "                                                WHERE pmdlent = pmdpent ",
               "                                                  AND pmdldocno = pmdpdocno ",
               "                                                  AND pmdlstus != 'X')) > 0 ",               
               #150519-00009#1 150520 by lori522612 add---(E)
               "             )"
               
   DISPLAY "Source Slip: ",g_sql            ]]>
  </point>
  <point name="fetch.after_fill" order="" ver="4" cite_std="N" new="N" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[   CALL g_detail2_d.clear()
   
   IF NOT cl_null(g_detail_d[l_ac].pmdadocno) THEN
      IF g_q_fetch THEN
         LET l_flag = 'Y'
      ELSE
         LET l_flag = 'N'
      END IF         
      
      CALL cl_err_collect_init()
      CALL apmp841_ins_detail(g_detail_d[l_ac].pmdadocno,l_flag) RETURNING l_success
      #150519-00009#1 150520 by lori522612 add---(S)
      #查詢後的第一筆資料填充不需要顯示錯誤訊息,待勾選時才需要
      IF l_flag = 'N' THEN
         CALL g_errcollect.clear()
      END IF
      #150519-00009#1 150520 by lori522612 add---(E)
      CALL cl_err_collect_show()
      
      LET l_ac2 = 1
      LET g_sql = "SELECT pmdbdocno,     pmdbseq,       pmdb200,     pmdb004,    t1.imaal003, ",     
                  "       t1.imaal004,   pmdb005,       ",
                  "       pmda202,       '',            imaa009,",   #150610-00030#3 150625 by sakura add
                  "       pmdb227,       pmdb201,       t2.oocal003,pmdb212,",   #150710-00016#9 150713 by sakura add pmdb227
                  "       pmdb007,       t3.oocal003,   pmdb006,     pmdb015,    t4.pmaal003, ",     
                  "       unpurchase_qty,pmdb030,       pmdn014,     pmdn020,    pmdn206,", #150324-00006#16 2015/04/29 By pomelo add pmdn020
                  "       pmdb253,       pmdb254,       pmdb255,     pmdb256,    pmdb257,",
                  "       pmdb206,       t5.ooag011,    pmdb037,     t6.ooefl003,pmdb260,",
                  "       t10.ooefl003,  purchase_date, pmdn227,     pmdn201,    t11.oocal003,pmdn202, ",   #150710-00016#9 150713 by sakura add pmdn227
                  "       pmdn006,       t7.oocal003,   pmdn007,     pmdn010,    t8.oocal003, ",     
                  "       pmdn011,       pmdl015,       t9.ooail003, pmdb016,    pmdb017, ",
                  "       pmdb034,       pmdb035,       pmdb036,     pmdb038,    pmdb039,",    
                  "       pmdb049,       pmdb203,       pmdb205,     pmdb207,    pmdb208,",     
                  "       pmdb209,       pmdbsite ",  
                  "  FROM s_apmp841_pr_detail ",
                  "       LEFT OUTER JOIN imaal_t t1 ON t1.imaalent = ",g_enterprise," AND t1.imaal001 = pmdb004 AND t1.imaal002 = '",g_dlang,"' ",
                  "       LEFT OUTER JOIN oocal_t t2 ON t2.oocalent = ",g_enterprise," AND t2.oocal001 = pmdb201 AND t2.oocal002 = '",g_dlang,"' ",
                  "       LEFT OUTER JOIN oocal_t t3 ON t3.oocalent = ",g_enterprise," AND t3.oocal001 = pmdb007 AND t3.oocal002 = '",g_dlang,"' ",
                  "       LEFT OUTER JOIN pmaal_t t4 ON t4.pmaalent = ",g_enterprise," AND t4.pmaal001 = pmdb015 AND t4.pmaal002 = '",g_dlang,"' ",
                  "       LEFT OUTER JOIN ooag_t  t5 ON t5.ooagent =  ",g_enterprise," AND t5.ooag001 = pmdb206 ",
                  "       LEFT OUTER JOIN ooefl_t t6 ON t6.ooeflent = ",g_enterprise," AND t6.ooefl001 = pmdb037 AND t6.ooefl002 = '",g_dlang,"' ",
                  "       LEFT OUTER JOIN oocal_t t7 ON t7.oocalent = ",g_enterprise," AND t7.oocal001 = pmdn006 AND t7.oocal002 = '",g_dlang,"' ",
                  "       LEFT OUTER JOIN oocal_t t8 ON t8.oocalent = ",g_enterprise," AND t8.oocal001 = pmdn010 AND t8.oocal002 = '",g_dlang,"' ",
                  "       LEFT OUTER JOIN ooail_t t9 ON t9.ooailent = ",g_enterprise," AND t9.ooail001 = pmdl015 AND t9.ooail002 = '",g_dlang,"' ",
                  "       LEFT OUTER JOIN ooefl_t t10 ON t10.ooeflent = ",g_enterprise," AND t10.ooefl001 = pmdb260 AND t10.ooefl002 = '",g_dlang,"' ",
                  "       LEFT OUTER JOIN oocal_t t11 ON t11.oocalent = ",g_enterprise," AND t11.oocal001 = pmdn201 AND t11.oocal002 = '",g_dlang,"' ",
                  " WHERE pmdbdocno = ? ",
                  "   AND pmdn007 > 0 ",      #預設可拋轉的採購數量需大於0
                  " ORDER BY pmdbseq "
      PREPARE apmp841_sel_detail_pre FROM g_sql
      DECLARE apmp841_sel_detail_cur CURSOR FOR apmp841_sel_detail_pre
      FOREACH apmp841_sel_detail_cur 
                 USING g_detail_d[l_ac].pmdadocno
                  INTO g_detail2_d[l_ac2].pmdbdocno,       g_detail2_d[l_ac2].pmdbseq,       g_detail2_d[l_ac2].pmdb200,       
                       g_detail2_d[l_ac2].pmdb004,         g_detail2_d[l_ac2].pmdb004_desc,  g_detail2_d[l_ac2].pmdb004_desc_1,  
                       g_detail2_d[l_ac2].pmdb005,
                       g_detail2_d[l_ac2].pmda202,         g_detail2_d[l_ac2].pmda202_desc,  g_detail2_d[l_ac2].l_imaa009,   #150610-00030#3 150625 By sakura add
                       g_detail2_d[l_ac2].pmdb227,   #150710-00016#9 150713 by sakura add
                       g_detail2_d[l_ac2].pmdb201,         g_detail2_d[l_ac2].pmdb201_desc,
                       g_detail2_d[l_ac2].pmdb212,         g_detail2_d[l_ac2].pmdb007,       g_detail2_d[l_ac2].pmdb007_desc,
                       g_detail2_d[l_ac2].pmdb006,         g_detail2_d[l_ac2].pmdb015,       g_detail2_d[l_ac2].pmdb015_desc,  
                       g_detail2_d[l_ac2].l_unpurchase_qty,g_detail2_d[l_ac2].pmdb030,       g_detail2_d[l_ac2].l_pmdn014,
                       g_detail2_d[l_ac2].pmdb033,         g_detail2_d[l_ac2].l_pmdn206,     g_detail2_d[l_ac2].pmdb253,       #150324-00006#16 2015/04/29 By pomelo pmdb033
                       g_detail2_d[l_ac2].pmdb254,         g_detail2_d[l_ac2].pmdb255,       g_detail2_d[l_ac2].pmdb256,
                       g_detail2_d[l_ac2].pmdb257,         g_detail2_d[l_ac2].pmdb206,       g_detail2_d[l_ac2].pmdb206_desc,
                       g_detail2_d[l_ac2].pmdb037,         g_detail2_d[l_ac2].pmdb037_desc,  g_detail2_d[l_ac2].pmdb260,
                       g_detail2_d[l_ac2].pmdb260_desc,    g_detail2_d[l_ac2].l_purchase_date,
                       g_detail2_d[l_ac2].l_pmdn227,   #150710-00016#9 150713 by sakura add
                       g_detail2_d[l_ac2].l_pmdn201,       g_detail2_d[l_ac2].l_pmdn201_desc,g_detail2_d[l_ac2].l_pmdn202,   
                       g_detail2_d[l_ac2].l_pmdn006,       g_detail2_d[l_ac2].l_pmdn006_desc,g_detail2_d[l_ac2].l_pmdn007,   
                       g_detail2_d[l_ac2].l_pmdn010,       g_detail2_d[l_ac2].l_pmdn010_desc,g_detail2_d[l_ac2].l_pmdn011,   
                       g_detail2_d[l_ac2].l_pmdl015,       g_detail2_d[l_ac2].l_pmdl015_desc,g_detail2_d[l_ac2].pmdb016,
                       g_detail2_d[l_ac2].pmdb017,         g_detail2_d[l_ac2].pmdb034,       g_detail2_d[l_ac2].pmdb035,
                       g_detail2_d[l_ac2].pmdb036,         g_detail2_d[l_ac2].pmdb038,       g_detail2_d[l_ac2].pmdb039,    
                       g_detail2_d[l_ac2].pmdb049,         g_detail2_d[l_ac2].pmdb203,       g_detail2_d[l_ac2].pmdb205,     
                       g_detail2_d[l_ac2].pmdb207,         g_detail2_d[l_ac2].pmdb208,       g_detail2_d[l_ac2].pmdb209, 
                       g_detail2_d[l_ac2].pmdbsite  
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:apmp841_sel_detail_cur" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
      
            EXIT FOREACH
         END IF
         
         #產品特徵說明
         CALL s_feature_description(g_detail2_d[l_ac2].pmdb004,g_detail2_d[l_ac2].pmdb005) 
            RETURNING l_success,g_detail2_d[l_ac2].pmdb005_desc
         
         #150610-00030#3 150625 By sakura add(S)
         #所屬管理品類說明
         CALL s_desc_get_rtaxl003_desc(g_detail2_d[l_ac2].pmda202) 
           RETURNING g_detail2_d[l_ac2].pmda202_desc         
         #150610-00030#3 150625 By sakura add(E)         
            
         LET l_ac2 = l_ac2 + 1
         IF l_ac2 > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code =  9035
               LET g_errparam.extend =  ""
               LET g_errparam.popup = TRUE
               CALL cl_err()
         
            END IF
            EXIT FOREACH
         END IF              
      END FOREACH  
      
      CALL g_detail2_d.deleteElement(g_detail2_d.getLength())
      LET g_detail2_cnt = l_ac2 - 1 
      DISPLAY g_detail2_cnt TO FORMONLY.cnt
      
      LET l_ac2 = 1
      LET g_detail2_idx = l_ac2
      DISPLAY g_detail2_idx TO FORMONLY.idx         
   END IF ]]>
  </point>
