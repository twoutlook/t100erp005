   LET l_pr.pmda203   = g_detail_d[p_ac].pmda203  
   LET l_pr.pmda002   = g_detail_d[p_ac].pmda002  
   LET l_pr.pmda003   = g_detail_d[p_ac].pmda003  
   LET l_pr.pmda021   = g_detail_d[p_ac].pmda021  
   LET l_pr.pmda201   = g_detail_d[p_ac].pmda201  
   LET l_pr.pmda204   = g_detail_d[p_ac].pmda204  
   LET l_pr.pmda205   = g_detail_d[p_ac].pmda205  
   
   CALL s_apmp841_ins_tmp_pr(l_pr.*)             
END FUNCTION]]>
