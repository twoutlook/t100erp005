                              "             WHERE pmdbent = pmdaent AND pmdbdocno = pmdadocno ",
                              "               AND pmdb015 = '",g_argv[3],"') "   
   END IF
   
   IF NOT cl_null(g_argv[4]) THEN #採購員
      LET g_def_wc = g_def_wc," AND EXISTS(SELECT pmdbdocno FROM pmdb_t ",
                              "             WHERE pmdbent = pmdaent AND pmdbdocno = pmdadocno ",
                              "               AND pmdb206 = '",g_argv[4],"') "     
   END IF
   
   IF NOT cl_null(g_argv[5]) THEN #採購方式
      LET g_def_wc = g_def_wc," AND EXISTS(SELECT pmdbdocno FROM pmdb_t ",
                              "             WHERE pmdbent = pmdaent AND pmdbdocno = pmdadocno ",
                              "               AND pmdb207 = '",g_argv[5],"') "     
   END IF
   
   IF NOT cl_null(g_argv[6]) THEN #收貨組織
      LET g_def_wc = g_def_wc," AND EXISTS(SELECT pmdbdocno FROM pmdb_t ",
                              "             WHERE pmdbent = pmdaent AND pmdbdocno = pmdadocno ",
                              "               AND pmdb037 = '",g_argv[6],"') "      
    END IF
   
   IF NOT cl_null(g_argv[7]) THEN #收貨部門
      LET g_def_wc = g_def_wc," AND EXISTS(SELECT pmdbdocno FROM pmdb_t ",
                              "             WHERE pmdbent = pmdaent AND pmdbdocno = pmdadocno ",
                              "               AND pmdb260 = '",g_argv[7],"') "       
   END IF
   
   IF NOT cl_null(g_argv[8]) THEN #配送中心
      LET g_def_wc = g_def_wc," AND (pmda205 = '",g_argv[8],"' ",
                              "      OR EXISTS(SELECT pmdbdocno FROM pmdb_t ",
                              "                 WHERE pmdbent = pmdaent AND pmdbdocno = pmdadocno ",
                              "                   AND pmdb203 = '",g_argv[8],"') "    
   END IF   ]]>
