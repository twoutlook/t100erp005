  <point name="statechange.b_update" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[   CASE lc_state
      WHEN 'X'
         IF g_pmah_m.pmahstus = 'Y' THEN
            RETURN
         END IF
         CALL s_transaction_begin()
         UPDATE pmah_t SET pmahcnfid = null,
                           pmahcnfdt = null         
          WHERE pmahent = g_enterprise AND pmah001 = g_pmah_m.pmah001
            AND pmah002 = g_pmah_m.pmah002
         IF SQLCA.sqlcode THEN
            CALL cl_err("",SQLCA.sqlcode,0)
            CALL s_transaction_end('N','0')
            RETURN
         ELSE
            CALL s_transaction_end('Y','1')
         END IF         
      WHEN 'Y'
         LET g_pmah_m.pmahcnfdt = cl_get_current()
         IF g_pmah_m.pmahstus = 'X' THEN
            RETURN
         END IF
         IF g_pmah_m.pmahstus = 'Y' THEN
            RETURN
         END IF
         CALL s_transaction_begin()
         UPDATE pmah_t SET pmahcnfid = g_user,
                           pmahcnfdt = g_pmah_m.pmahcnfdt        
          WHERE pmahent = g_enterprise AND pmah001 = g_pmah_m.pmah001
            AND pmah002 = g_pmah_m.pmah002
         IF SQLCA.sqlcode THEN
            CALL cl_err("",SQLCA.sqlcode,0)
            CALL s_transaction_end('N','0')
            RETURN
         ELSE
            CALL s_transaction_end('Y','1')
         END IF 
      WHEN 'N'
         
         CALL s_transaction_begin()
         UPDATE pmah_t SET pmahcnfid = NULL,
                           pmahcnfdt = NULL         
          WHERE pmahent = g_enterprise AND pmah001 = g_pmah_m.pmah001
            AND pmah002 = g_pmah_m.pmah002
         IF SQLCA.sqlcode THEN
            CALL cl_err("",SQLCA.sqlcode,0)
            CALL s_transaction_end('N','0')
            RETURN
         ELSE
            CALL s_transaction_end('Y','1')
         END IF         
   END CASE         
         ]]>
