  <section id="apmm825.browser_expand" ver="2" status="" src="s">
    <![CDATA[#+ Tree子節點展開
PRIVATE FUNCTION apmm825_browser_expand(p_id)
   DEFINE p_id          LIKE type_t.num10
   DEFINE l_id          LIKE type_t.num10
   DEFINE l_cnt         LIKE type_t.num10
   DEFINE l_keyvalue    LIKE type_t.chr50
   DEFINE l_typevalue   LIKE type_t.chr50
   DEFINE l_type        LIKE type_t.chr50
   DEFINE l_sql         LIKE type_t.chr500
   DEFINE ls_source     LIKE type_t.chr500
   DEFINE ls_exp_code   LIKE type_t.chr500
   DEFINE l_return      LIKE type_t.num5
   #add-point:browser_expand段define
   {<point name="browser_expand.define"/>}
   #end add-point
 
   
   #若已經展開
   IF g_browser[p_id].b_isExp = 1 THEN
      RETURN
   END IF
   
   LET l_return = FALSE
 
   LET l_keyvalue = g_browser[p_id].b_pmah004
   #此程式無root欄位:LET l_typevalue = g_browser[p_id].b_
   
   CASE g_browser[p_id].b_expcode
      WHEN -1
         CALL g_browser.deleteElement(p_id)
      WHEN 0
         RETURN
      WHEN 1
         LET ls_source = "apmm825_tmp"
         LET ls_exp_code = "exp_code"
      WHEN 2
         LET ls_source = "pmah_t"
         LET ls_exp_code = "'2'"
   END CASE
   
   LET l_sql = " SELECT UNIQUE '','','','FALSE','','',",ls_exp_code,",pmah001,'',pmah002,pmah004,pmah003, 
       pmah005,'',pmah006,pmahstus",
                 " FROM ",ls_source,
                " WHERE pmah001 = '", l_keyvalue,
                 "' AND pmah004 <> pmah001",
               #此程式無root欄位:" AND  = '", l_typevalue,"'", 
                " ORDER BY pmah004"
   
   #add-point:browser_expand段before_pre
   {<point name="browser_expand.before_pre"/>}
   #end add-point
   
   PREPARE tree_expand FROM l_sql
   DECLARE tree_ex_cur CURSOR FOR tree_expand
  
   LET l_id = p_id + 1
   CALL g_browser.insertElement(l_id)
   LET l_cnt = 1
   FOREACH tree_ex_cur INTO g_browser[l_id].*
      #pid=父節點id
      LET g_browser[l_id].b_pid = g_browser[p_id].b_id
      #id=本身節點id(採流水號遞增)
      LET g_browser[l_id].b_id = g_browser[p_id].b_id||"."||l_cnt
      #hasC=確認該節點是否有子孫
      #LET g_browser[l_id].b_pmah004 = g_browser[l_id].b_pmah004 CLIPPED
      CALL apmm825_desc_show(l_id)
      LET g_browser[l_id].b_hasC = apmm825_chk_hasC(l_id)
      LET l_id = l_id + 1
      CALL g_browser.insertElement(l_id)
      LET l_cnt = l_cnt + 1
      
      LET l_return = TRUE
   END FOREACH
   
   #刪除空資料
   CALL g_browser.deleteElement(l_id)
 
END FUNCTION
]]>
  </section>
  <section id="apmm825.browser_fill" ver="3" status="" src="s">
    <![CDATA[#+ 瀏覽頁簽where條件組成
PRIVATE FUNCTION apmm825_browser_fill(p_wc,p_type)
   DEFINE p_wc       STRING 
   DEFINE p_type     LIKE type_t.chr10
   DEFINE l_cnt      LIKE type_t.num10
   DEFINE l_cnt2     LIKE type_t.num10   
   #add-point:browser_fill段define
   {<point name="browser_fill.define"/>}
   #end add-point
   
   #add-point:browser_fill段fill資料前
   {<point name="browser_fill.before_fill"/>}
   #end add-point
 
   CALL g_browser.clear()
   CLEAR FORM
   LET l_cnt = 0
   LET l_cnt2 = 0
   
   DROP TABLE apmm825_tmp
   
   #Create temp table
   CREATE TEMP TABLE apmm825_tmp
   (
         pmah001 VARCHAR(500),
   pmah001_desc VARCHAR(500),
   pmah002 VARCHAR(500),
   pmah004 VARCHAR(500),
   pmah003 VARCHAR(500),
   pmah005 VARCHAR(500),
   pmah005_desc VARCHAR(500),
   pmah006 VARCHAR(500),
   pmahstus VARCHAR(500),
      #僅含browser的欄位
      exp_code  VARCHAR(5)
   );
 
   #先確定搜尋範圍(若無條件搜尋則只找root出來)
   SELECT COUNT(*) INTO l_cnt FROM pmah_t 
   
   #取得符合p_wc的所有資料
   LET g_sql = "SELECT COUNT(*)",
               " FROM pmah_t  ",
               " WHERE pmahent = '" ||g_enterprise|| "' AND ",p_wc #,cl_get_extra_cond('zzuser', 'zzgrup')
              
   PREPARE master_cnt FROM g_sql
   DECLARE master_cntcur CURSOR FOR master_cnt
   OPEN master_cntcur
   FETCH master_cntcur INTO l_cnt2
   LET g_root_search = FALSE
   
   IF l_cnt2 = 0 THEN
      CALL cl_err("","-100",1)
      RETURN
   END IF
   
   IF l_cnt = l_cnt2 THEN
      #未輸入條件時則只查找root節點
      LET p_wc = " pmah004 = pmah001 "
      LET g_root_search = TRUE
   END IF
 
   #取得符合p_wc的所有資料
   LET g_sql = "SELECT pmah001,'',pmah002,pmah004,pmah003,pmah005,'',pmah006,pmahstus ",
               " FROM pmah_t  ",
               " WHERE pmahent = '" ||g_enterprise|| "' AND ",p_wc #,cl_get_extra_cond('zzuser', 'zzgrup')
              
   PREPARE master_ext FROM g_sql
   DECLARE master_extcur CURSOR FOR master_ext
 
   #搜尋建構樹所需的節點
   CASE p_type
      WHEN "1" #上推
         CALL apmm825_match_node(p_wc,p_type) 
      WHEN "2" #下展
         #CALL apmm825_find_speed_tbl(p_wc,p_type) 
         CALL apmm825_match_node(p_wc,p_type) 
      WHEN "3" #全部
         CALL apmm825_match_node(p_wc,p_type) 
   END CASE
 
   CALL apmm825_browser_create(p_type)
     
END FUNCTION
]]>
  </section>
  <section id="apmm825.check" ver="2" status="" src="s">
    <![CDATA[#+ 避免資料錯誤
PRIVATE FUNCTION apmm825_check(ps_id,ps_pid)
   DEFINE ps_id       STRING
   DEFINE ps_pid      STRING
   DEFINE ps_type     STRING
   DEFINE ls_pid      LIKE type_t.chr100
   DEFINE ls_id       LIKE type_t.chr100
   DEFINE ls_type     LIKE type_t.chr100
   DEFINE ls_return   LIKE type_t.num5
   #add-point:check段define
   {<point name="check.define"/>}
   #end add-point
   
   LET ls_return = FALSE
   LET ls_pid = ps_pid
   LET ls_type = ps_type
   
   #比對所有祖先節點
   WHILE TRUE
      
      SELECT pmah001,pmah004 INTO ls_pid,ls_id FROM pmah_t
         WHERE pmah004 = ls_pid
        #此程式無root欄位: AND  = ls_type
 
      IF SQLCA.sqlcode OR ls_pid = ls_id THEN
         EXIT WHILE
      END IF
      
      IF ls_pid = ps_id THEN
         LET ls_return = TRUE
         EXIT WHILE
      END IF
      
   END WHILE
   
   RETURN ls_return
   
END FUNCTION
