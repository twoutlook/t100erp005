  <section id="apmm825.match_node" ver="3" status="" src="s">
    <![CDATA[PRIVATE FUNCTION apmm825_match_node(p_wc,p_type)
   DEFINE p_wc         LIKE type_t.chr200
   DEFINE p_type       LIKE type_t.chr10
   DEFINE ls_code      LIKE type_t.chr50
   DEFINE ls_code2     LIKE type_t.chr50
   DEFINE l_bstmp      RECORD    #body欄位
             pmah001 VARCHAR(500),
   pmah001_desc VARCHAR(500),
   pmah002 VARCHAR(500),
   pmah004 VARCHAR(500),
   pmah003 VARCHAR(500),
   pmah005 VARCHAR(500),
   pmah005_desc VARCHAR(500),
   pmah006 VARCHAR(500),
   pmahstus VARCHAR(500)
          #僅含單身table的欄位
   END RECORD 
   DEFINE l_child_list DYNAMIC ARRAY OF RECORD    #body欄位
             pmah001 VARCHAR(500),
   pmah001_desc VARCHAR(500),
   pmah002 VARCHAR(500),
   pmah004 VARCHAR(500),
   pmah003 VARCHAR(500),
   pmah005 VARCHAR(500),
   pmah005_desc VARCHAR(500),
   pmah006 VARCHAR(500),
   pmahstus VARCHAR(500)
          #僅含單身table的欄位
   END RECORD    
   #add-point:match_node段define
   {<point name="match_node.define"/>}
   #end add-point
 
   #先找出符合條件的節點並給予展開值
   CASE p_type
      WHEN 1
         LET ls_code = "0"
      WHEN 2
         LET ls_code = "2"
      WHEN 3
         LET ls_code = "2"
   END CASE
   
   IF cl_null("pmah001") THEN
      LET ls_code = "0"
   END IF 
   
   CALL s_transaction_begin()
 
   LET g_sql = " INSERT INTO apmm825_tmp (pmah001,pmah001_desc,pmah002,pmah004,pmah003,pmah005,pmah005_desc, 
       pmah006,pmahstus,exp_code) VALUES (?,?,?,?,?,?,?,?,?,?)"
   PREPARE master_tmp FROM g_sql
   
   IF g_root_search THEN
      #DECLARE master_tmp_cur CURSOR FOR master_tmp
      #OPEN master_tmp_cur
      FOREACH master_extcur INTO l_bstmp.*
         EXECUTE master_tmp USING l_bstmp.*,ls_code
         #PUT master_tmp_cur FROM l_bstmp.*,ls_code
      END FOREACH
      #FLUSH master_tmp_cur
      CALL s_transaction_end("Y","0")
      RETURN
   END IF
 
   FOREACH master_extcur INTO l_bstmp.*
      EXECUTE master_tmp USING l_bstmp.*,ls_code
      LET l_child_list[l_child_list.getLength()+1].* = l_bstmp.*
       
      #找出符合條件的節點的所有祖先並給予展開值
      CASE p_type
         WHEN 1
            LET ls_code2 = "1"
         WHEN 2
            LET ls_code2 = "-1"
         WHEN 3
            LET ls_code2 = "1"
      END CASE
      
      #若pid欄位存在才進行後續處理
      #擷取該節點的父節點到temp table中
      LET g_sql = " SELECT pmah001,'',pmah002,pmah004,pmah003,pmah005,'',pmah006,pmahstus ",
                  " FROM pmah_t  ",
                  " WHERE pmahent = '" ||g_enterprise|| "' AND pmah004 = ? "
                  #此程式無root欄位:," AND  = ? " #,cl_get_extra_cond('zzuser', 'zzgrup')
      PREPARE master_getparent_up FROM g_sql
      
      #擷取該節點的所有父節點
      WHILE TRUE
         IF cl_null(l_child_list[1].pmah004) THEN
            IF l_child_list.getLength() = 1 THEN
               EXIT WHILE
            ELSE
               CALL l_child_list.deleteElement(1)
               CONTINUE WHILE
            END IF
         END IF
      
         EXECUTE master_getparent_up USING l_child_list[1].pmah001
                                           #此程式無root欄位:,l_child_list[1].
                                           INTO l_bstmp.*
         IF SQLCA.sqlcode THEN
            FREE master_getparent_up
            EXIT WHILE
         END IF
         FREE master_getparent_up
      #確定該節點是否存在於temp table中
         
         IF STATUS = 0 AND apmm825_tmp_tbl_chk(l_bstmp.pmah004,ls_code2
                     #此程式無root欄位:,l_bstmp.
                     ) THEN
            EXECUTE master_tmp USING l_bstmp.*,ls_code2
            LET l_child_list[l_child_list.getLength()+1].* = l_bstmp.*
         END IF
         CALL l_child_list.deleteElement(1)
      
      END WHILE
   
   END FOREACH
   
   CLOSE master_tmp
   
   CALL s_transaction_end("Y","0")
 
END FUNCTION
]]>
  </section>
  <section id="apmm825.modify" ver="3" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION apmm825_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point
   
   IF g_pmah_m.pmah001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF 
   
   EXECUTE apmm825_master_referesh USING g_pmah_m.pmah001,g_pmah_m.pmah002 INTO g_pmah_m.pmah001,g_pmah_m.pmah002, 
       g_pmah_m.pmah003,g_pmah_m.pmah004,g_pmah_m.pmah005,g_pmah_m.pmah008,g_pmah_m.pmahstus,g_pmah_m.pmah006, 
       g_pmah_m.pmah007,g_pmah_m.pmahownid,g_pmah_m.pmahowndp,g_pmah_m.pmahcrtid,g_pmah_m.pmahcrtdp, 
       g_pmah_m.pmahcrtdt,g_pmah_m.pmahmodid,g_pmah_m.pmahmoddt,g_pmah_m.pmahcnfid,g_pmah_m.pmahcnfdt 

 
   ERROR ""
  
   LET g_pmah001_t = g_pmah_m.pmah001
   LET g_pmah002_t = g_pmah_m.pmah002
 
   
   CALL s_transaction_begin()
   
   OPEN apmm825_cl USING g_enterprise,g_pmah_m.pmah001,g_pmah_m.pmah002
   IF STATUS THEN
      CALL cl_err("OPEN apmm825_cl:", STATUS, 1)
      CLOSE apmm825_cl
      CALL s_transaction_end("N","0")
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH apmm825_cl INTO g_pmah_m.pmah001,g_pmah_m.pmah001_desc,g_pmah_m.pmaastus,g_pmah_m.pmaa083,g_pmah_m.pmah002, 
       g_pmah_m.pmah003,g_pmah_m.pmah004,g_pmah_m.pmah005,g_pmah_m.pmah005_desc,g_pmah_m.pmah008,g_pmah_m.pmah008_desc, 
       g_pmah_m.pmahstus,g_pmah_m.pmah006,g_pmah_m.pmah007,g_pmah_m.pmahownid,g_pmah_m.pmahownid_desc, 
       g_pmah_m.pmahowndp,g_pmah_m.pmahowndp_desc,g_pmah_m.pmahcrtid,g_pmah_m.pmahcrtid_desc,g_pmah_m.pmahcrtdp, 
       g_pmah_m.pmahcrtdp_desc,g_pmah_m.pmahcrtdt,g_pmah_m.pmahmodid,g_pmah_m.pmahmodid_desc,g_pmah_m.pmahmoddt, 
       g_pmah_m.pmahcnfid,g_pmah_m.pmahcnfid_desc,g_pmah_m.pmahcnfdt
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_pmah_m.pmah004,SQLCA.sqlcode,0)
      CLOSE apmm825_cl
      CALL s_transaction_end("N","0")
      RETURN
   END IF
 
   CALL apmm825_show()
   WHILE TRUE
      LET g_pmah_m.pmah001 = g_pmah001_t
      LET g_pmah_m.pmah002 = g_pmah002_t
 
      
      #寫入修改者/修改日期資訊
      LET g_pmah_m.pmahmodid = g_user 
LET g_pmah_m.pmahmoddt = cl_get_current()
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL apmm825_input("u")     #欄位更改
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
          LET INT_FLAG = 0
          LET g_pmah_m.* = g_pmah_m_t.*
          CALL apmm825_show()
          CALL cl_err("",9001,0)
          EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE pmah_t SET (pmahmodid,pmahmoddt) = (g_pmah_m.pmahmodid,g_pmah_m.pmahmoddt)
       WHERE pmahent = g_enterprise AND pmah001 = g_pmah001_t
         AND pmah002 = g_pmah002_t
 
 
      EXIT WHILE
  
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_pmah_m.pmah004,g_site) THEN 
      CALL s_transaction_end("N","0")
   END IF
 
   CLOSE apmm825_cl
   CALL s_transaction_end("Y","0")
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_pmah_m.pmah004,"U")
   
   
END FUNCTION
]]>
