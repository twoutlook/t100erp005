  <point name="function.apmm825_pmah002" order="2" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#+create  pmah002
PRIVATE FUNCTION apmm825_pmah002(p_pmah001)
   DEFINE p_pmah001 LIKE pmah_t.pmah001
   IF cl_null(g_pmah_m.pmah002) OR g_pmah_m.pmah002 = 0 THEN
      SELECT max(pmah002)+1 INTO g_pmah_m.pmah002 FROM pmah_t
       WHERE pmah001 = p_pmah001 AND pmahent=g_enterprise
   END IF
   IF cl_null(g_pmah_m.pmah002) OR g_pmah_m.pmah002 = 0 THEN
      LET g_pmah_m.pmah002 = 1
   END IF
END FUNCTION]]>
