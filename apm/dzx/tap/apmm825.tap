  <point name="function.apmm825_create_tree" order="8" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#+ 手动调树
PRIVATE FUNCTION apmm825_create_tree()
   DEFINE l_pid    LIKE type_t.chr50   #用於樹的第一層
   DEFINE l_pid2    LIKE type_t.chr50  #用於樹的第二層
   #先找出所有的收支項目版本資料

   IF cl_null(g_wc_def) THEN
      LET g_wc_def = " 1=1"
   END IF
   LET g_sql = " SELECT UNIQUE pmah001 FROM pmah_t ",
               "  WHERE  ",g_wc_def,
               "   ORDER BY pmah001"
  
   PREPARE master_type_0 FROM g_sql
   DECLARE master_typecur_0 CURSOR FOR master_type_0
   #再找出某个供应商的谈判日期資料
   LET g_sql = " SELECT UNIQUE pmah004 FROM pmah_t WHERE pmah001 = ?  AND ",g_wc_def CLIPPED,
               "   ORDER BY pmah004 desc"
   PREPARE master_type_1 FROM g_sql
   DECLARE master_typecur_1 CURSOR FOR master_type_1


   INITIALIZE g_browser_root TO NULL
   #初始化l_ac
   LET l_ac = 1
   FOREACH master_typecur_0 INTO g_browser[l_ac].b_pmah001
      
      #確定第一层root節點所在
      LET g_browser_root[g_browser_root.getLength()+1] = l_ac
      #此處(LV-1)
      LET g_browser[l_ac].b_pmah001 = g_browser[l_ac].b_pmah001
      LET g_browser[l_ac].b_pid = '0' CLIPPED
      LET g_browser[l_ac].b_id = l_ac USING "<<<"
      LET g_browser[l_ac].b_exp = TRUE
      LET g_browser[l_ac].b_hasC = TRUE
      #第一層節點編號
      LET l_pid = g_browser[l_ac].b_id CLIPPED
      LET l_ac = l_ac + 1
      LET g_cnt = l_ac
         

            LET g_sql = " SELECT UNIQUE pmah001,pmah002,pmah004,pmah003,pmah005,'',pmah006,pmahstus FROM pmah_t ",
                  " WHERE  ",g_wc_def,
#                  "   AND pmah001 = '",g_browser[g_cnt-1].b_pmah001,"' and pmah004 = '",g_browser[g_cnt-1].b_pmah004,"'  ",
                  "   AND pmah001 = '",g_browser[g_cnt-1].b_pmah001,"' ",
            "  ORDER BY pmah001,pmah004 desc,pmah002"
            PREPARE master_getLV2_1 FROM g_sql
            DECLARE master_getLV2cur_1 CURSOR FOR master_getLV2_1

            #以下為一般資料root(LV-4)
            OPEN master_getLV2cur_1

            FOREACH master_getLV2cur_1 INTO g_browser[g_cnt].b_pmah001,g_browser[g_cnt].b_pmah002,g_browser[g_cnt].b_pmah004,g_browser[g_cnt].b_pmah003,
               g_browser[g_cnt].b_pmah005,g_browser[g_cnt].b_pmah005_desc,g_browser[g_cnt].b_pmah006,g_browser[g_cnt].b_pmahstus
               
               INITIALIZE g_ref_fields TO NULL
               LET g_ref_fields[1] = g_browser[g_cnt].b_pmah005
               CALL ap_ref_array2(g_ref_fields,"SELECT oocql004 FROM oocql_t WHERE oocqlent='"||g_enterprise||"' AND oocql001='2047' AND oocql002=? AND oocql003='"||g_dlang||"'","") RETURNING g_rtn_fields
               LET g_browser[g_cnt].b_pmah005_desc = '', g_rtn_fields[1] , ''
               DISPLAY BY NAME g_browser[g_cnt].b_pmah005_desc
               
               LET g_browser[g_cnt].b_pid = l_pid
               LET g_browser[g_cnt].b_id = l_pid,".",g_cnt USING "<<<"
               LET g_browser[g_cnt].b_exp = FALSE
               LET g_browser[g_cnt].b_expcode = 2
               LET g_browser[g_cnt].b_hasC = FALSE
               LET g_cnt = g_cnt + 1
            END FOREACH

#      END FOREACH
      LET l_ac = g_browser.getLength()
   END FOREACH
   FOR l_ac = 1 TO g_browser.getLength()
      IF cl_null(g_browser[l_ac].b_pmah001) THEN
         CALL g_browser.deleteElement(l_ac)
         LET l_ac = l_ac - 1
      ELSE
         CALL apmm825_desc_show(l_ac)
      END IF
   END FOR
   CALL g_browser.deleteElement(l_ac)
   LET g_browser_cnt = g_browser.getLength() - g_browser_root.getLength()
   DISPLAY g_browser_cnt TO FORMONLY.b_count
   FREE tree_expand
   FREE master_getLV2_1
END FUNCTION]]>
  </point>
