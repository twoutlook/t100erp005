  <section id="apmm825.browser_create" ver="2" status="" src="s">
    <![CDATA[PRIVATE FUNCTION apmm825_browser_create(p_type)
   DEFINE p_type   LIKE type_t.chr50
   DEFINE l_pid    LIKE type_t.chr50
   #add-point:browser_create
   {<point name="browser_create.define"/>}
   #end add-point
   
   #先找出所有的帳別資料
   #此程式無root欄位:LET g_sql = " SELECT UNIQUE  FROM apmm825_tmp ORDER BY "
   #此程式無root欄位:PREPARE master_type FROM g_sql
   #此程式無root欄位:DECLARE master_typecur CURSOR FOR master_type
   
   #此程式無root欄位:INITIALIZE g_browser_root TO NULL
   
   LET l_ac = 1
   #此程式無root欄位:FOREACH master_typecur INTO g_browser[l_ac].b_
      #確定root節點所在
      #此程式無root欄位:LET g_browser_root[g_browser_root.getLength()+1] = l_ac
      #此處為帳別部分(LV-1)
      #此程式無root欄位:LET g_browser[l_ac].b_pmah004  = g_browser[l_ac].b_
      #此程式無root欄位:LET g_browser[l_ac].b_ = g_browser[l_ac].b_
      #此程式無root欄位:LET g_browser[l_ac].b_pid = "0" CLIPPED
      #此程式無root欄位:LET g_browser[l_ac].b_id = l_ac USING "<<<"
      #此程式無root欄位:LET g_browser[l_ac].b_exp = TRUE
      #此程式無root欄位:LET g_browser[l_ac].b_hasC = TRUE
      #此程式無root欄位:LET l_pid = g_browser[l_ac].b_id CLIPPED
      #此程式無root欄位:LET l_ac = l_ac + 1
      
      #抓出LV2的所有資料
      #LET g_sql = " SELECT UNIQUE pmah001,'',pmah002,pmah004,pmah003,pmah005,'',pmah006,pmahstus,exp_code FROM apmm825_tmp a ", 

      LET g_sql = " SELECT UNIQUE pmah001,'',pmah002,pmah004,pmah003,pmah005,'',pmah006,pmahstus FROM apmm825_tmp a ", 

                  " WHERE ",
                  #此程式無root欄位:"a. = ? ",
                  #此程式無root欄位:" AND ",
                  " (( SELECT COUNT(*) FROM apmm825_tmp b WHERE a.pmah001 = b.pmah004 ", 
                  #此程式無root欄位:" AND a. = b.",
                  ") = 0 OR ", 
                  " a.pmah004 = a.pmah001 )", 
                  " ORDER BY a.pmah004"
      #add-point:browser_create.before_pre
      {<point name="browser_create.before_pre"/>}
      #end add-point
      PREPARE master_getLV2 FROM g_sql
      DECLARE master_getLV2cur CURSOR FOR master_getLV2
      
      #以下為一般資料root(LV-2)
      #此程式無root欄位:OPEN master_getLV2cur USING g_browser[l_ac-1].b_
      
      LET g_cnt = l_ac
      
      FOREACH master_getLV2cur INTO g_browser[g_cnt].b_pmah001,g_browser[g_cnt].b_pmah001_desc,g_browser[g_cnt].b_pmah002, 
          g_browser[g_cnt].b_pmah004,g_browser[g_cnt].b_pmah003,g_browser[g_cnt].b_pmah005,g_browser[g_cnt].b_pmah005_desc, 
          g_browser[g_cnt].b_pmah006,g_browser[g_cnt].b_pmahstus
         #去除多餘空白
         #LET g_browser[g_cnt].b_pmah004 = g_browser[g_cnt].b_pmah004 CLIPPED
         LET g_browser[g_cnt].b_pid = l_pid
         LET g_browser[g_cnt].b_id = l_pid,".",g_cnt USING "<<<"
         LET g_browser[g_cnt].b_exp = FALSE
         LET g_browser[g_cnt].b_expcode = 2
         #CASE g_browser[g_cnt].b_expcode
         #   WHEN 2
