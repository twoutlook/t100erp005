  <point name="function.apmm825_pmah001" order="3" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#+chk pmah001
PRIVATE FUNCTION apmm825_pmah001(p_pmah001)
   DEFINE l_cnt   LIKE type_t.num5
   DEFINE l_cnt1  LIKE type_t.num5
   DEFINE l_success LIKE type_t.num5
   DEFINE p_pmah001 LIKE pmah_t.pmah001
   LET l_success = TRUE
   LET l_cnt =0 
   LET l_cnt1 = 0
   LET g_errno = NULL
   SELECT count(*) INTO l_cnt FROM pmaa_t WHERE pmaaent = g_enterprise AND pmaa001 = p_pmah001
      AND pmaa002 IN ('1','3')
   IF l_cnt<=0 THEN
      LET g_errno="apm-00082"
      LET l_success = FALSE
      RETURN l_success
   END IF 
   SELECT count(*) INTO l_cnt1 FROM pmaa_t WHERE pmaaent = g_enterprise AND pmaa001 = p_pmah001
      AND pmaa002 IN ('1','3') AND pmaastus='Y'
   IF l_cnt1<=0 THEN
      LET g_errno = "apm-00025"
      LET l_success = FALSE 
      RETURN l_success
   END IF
   RETURN l_success  
END FUNCTION]]>
  </point>
