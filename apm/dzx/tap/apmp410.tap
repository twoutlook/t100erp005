   ELSE
      LET g_wc2 = " pmdastus = 'C' AND pmdb032 <> '1'"                
   END IF  
   LET l_sql = "SELECT DISTINCT '','',pmdbdocno,pmdbseq,pmdb006,pmdb049 ",
