  <point name="function.apmp410_upd_tmp" order="2" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#畫面單身勾選/取消勾選，需更新tmptable資料
PRIVATE FUNCTION apmp410_upd_tmp(p_type,p_flag,p_pmdbdocno,p_pmdbseq,p_closed)
DEFINE p_type        LIKE type_t.chr1
DEFINE p_flag        LIKE type_t.chr1
DEFINE p_pmdbdocno   LIKE pmdb_t.pmdbdocno
DEFINE p_pmdbseq     LIKE pmdb_t.pmdbseq
DEFINE p_closed      LIKE   pmdb_t.pmdb051
DEFINE i             LIKE type_t.num5

   CASE p_type
      
     WHEN '2'           #單頭全選
       UPDATE apmp410_tmp
          SET sel2 = p_flag,
              closed2 = p_closed    
        WHERE pmdbdocno = p_pmdbdocno
     
     WHEN '3'           #單身一筆勾選
       UPDATE apmp410_tmp
          SET sel2 = p_flag,
              closed2 = p_closed 
        WHERE pmdbdocno = p_pmdbdocno
          AND pmdbseq = p_pmdbseq
    END CASE 
    

END FUNCTION]]>
  </point>
