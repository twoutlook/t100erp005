  <point name="ui_dialog.more_action" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[         ON ACTION batch_execute        #執行結案
            LET l_cnt = 0
            SELECT COUNT(*) INTO l_cnt
              FROM apmp410_tmp
             WHERE sel2 = 'Y'
            IF l_cnt = 0 THEN
               CALL cl_err('','apm-00481',1)     #未勾選任何資料
               LET g_error_show = 1
               CALL apmp410_b_fill()
               CALL apmp410_fetch()               
            ELSE
               IF g_type = '1' THEN
                  CALL s_apmp410_closed_upd()
               ELSE
                  CALL s_apmp410_unclosed_upd()
               END IF
               IF cl_ask_confirm('asf-00182') THEN
                 CLEAR FORM
                 CALL g_detail_d.clear()                 
                 CALL g_pmdb_d.clear()
                 EXIT DIALOG
               ELSE 
                 LET g_action_choice = 'exit'
                 EXIT DIALOG
               END IF
            END IF]]>
