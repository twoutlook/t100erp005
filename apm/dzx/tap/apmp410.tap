  <point name="ui_dialog.more_input" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[         INPUT ARRAY g_detail_d FROM s_detail1.*
            ATTRIBUTE(COUNT = g_detail_cnt,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS,
                      INSERT ROW = FALSE,
                      DELETE ROW = FALSE,
                      APPEND ROW = FALSE)
                      
            BEFORE ROW
               LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
               LET l_ac = g_detail_idx
               DISPLAY g_detail_idx TO FORMONLY.h_index
               DISPLAY g_detail_d.getLength() TO FORMONLY.h_count
               LET g_master_idx = l_ac
               CALL apmp410_fetch() 
               IF g_type = '1' THEN
                  NEXT FIELD closed
               ELSE
                  NEXT FIELD sel
               END IF             

            ON CHANGE sel
               IF g_detail_d[l_ac].sel = 'Y' THEN
                  IF cl_null(g_detail_d[l_ac].closed) THEN
                     CALL cl_err('','axm-00266','1')             #結案理由碼不可為空
                     LET g_detail_d[l_ac].sel = 'N'
                     DISPLAY BY NAME g_detail_d[l_ac].sel
                     NEXT FIELD closed
                  END IF
                  CALL apmp410_upd_tmp('2','Y',g_detail_d[l_ac].pmdadocno,'',g_detail_d[l_ac].closed)
               ELSE 
                  LET g_detail_d[l_ac].closed = ''
                  LET g_detail_d[l_ac].closed_desc = ''
