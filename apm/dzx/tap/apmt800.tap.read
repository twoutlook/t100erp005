  <section id="apmt800.input.body" ver="27" status="" src="s" readonly="">
    <![CDATA[   
      #Page1 預設值產生於此處
      INPUT ARRAY g_pmbg_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,WITHOUT DEFAULTS, #MAXCOUNT = g_max_rec,
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            #add-point:資料輸入前
            {<point name="input.body.before_input2"/>}
            #end add-point
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_pmbg_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL apmt800_b_fill()
            #如果一直都在單身1則控制筆數位置
            IF g_loc = 'm' AND g_rec_b != 0 THEN
               CALL FGL_SET_ARR_CURR(g_detail_idx)
            END IF
            LET g_loc = 'm'
            LET g_rec_b = g_pmbg_d.getLength()
            #add-point:資料輸入前
            {<point name="input.d.before_input"/>}
            #end add-point
         
         BEFORE ROW
            #add-point:modify段before row2
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET l_insert = FALSE
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET g_detail_idx = l_ac
            LET g_current_page = 1
            
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
         
            CALL s_transaction_begin()
            OPEN apmt800_cl USING g_enterprise,g_pmba_m.pmbadocno
            IF STATUS THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "OPEN apmt800_cl:" 
               LET g_errparam.code   = STATUS 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CLOSE apmt800_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            
            LET g_rec_b = g_pmbg_d.getLength()
            
            IF g_rec_b >= l_ac 
               AND g_pmbg_d[l_ac].pmbg002 IS NOT NULL
               AND g_pmbg_d[l_ac].pmbg003 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_pmbg_d_t.* = g_pmbg_d[l_ac].*  #BACKUP
               LET g_pmbg_d_o.* = g_pmbg_d[l_ac].*  #BACKUP
               CALL apmt800_set_entry_b(l_cmd)
               #add-point:modify段after_set_entry_b
               {<point name="input.body.after_set_entry_b"/>}
               #end add-point  
               CALL apmt800_set_no_entry_b(l_cmd)
               IF NOT apmt800_lock_b("pmbg_t","'1'") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH apmt800_bcl INTO g_pmbg_d[l_ac].pmbg001,g_pmbg_d[l_ac].pmbg002,g_pmbg_d[l_ac].pmbg003, 
                      g_pmbg_d[l_ac].pmbg004,g_pmbg_d[l_ac].pmbg007,g_pmbg_d[l_ac].pmbg008,g_pmbg_d[l_ac].pmbgacti 

                  IF SQLCA.sqlcode THEN
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = g_pmbg_d_t.pmbg002 
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     LET l_lock_sw = "Y"
                  END IF
                  
                  #遮罩相關處理
                  LET g_pmbg_d_mask_o[l_ac].* =  g_pmbg_d[l_ac].*
                  CALL apmt800_pmbg_t_mask()
                  LET g_pmbg_d_mask_n[l_ac].* =  g_pmbg_d[l_ac].*
                  
                  LET g_bfill = "N"
                  CALL apmt800_show()
                  LET g_bfill = "Y"
                  
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
        
         BEFORE INSERT
            
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_pmbg_d[l_ac].* TO NULL 
            INITIALIZE g_pmbg_d_t.* TO NULL 
            INITIALIZE g_pmbg_d_o.* TO NULL 
            #公用欄位給值(單身)
            
            #自定義預設值
                  LET g_pmbg_d[l_ac].pmbgacti = "Y"
 
            #add-point:modify段before備份
            {<point name="input.body.insert.before_bak"/>}
            #end add-point
            LET g_pmbg_d_t.* = g_pmbg_d[l_ac].*     #新輸入資料
            LET g_pmbg_d_o.* = g_pmbg_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL apmt800_set_entry_b(l_cmd)
            #add-point:modify段after_set_entry_b
            {<point name="input.body.insert.after_set_entry_b"/>}
            #end add-point
            CALL apmt800_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_pmbg_d[li_reproduce_target].* = g_pmbg_d[li_reproduce].*
 
               LET g_pmbg_d[li_reproduce_target].pmbg002 = NULL
               LET g_pmbg_d[li_reproduce_target].pmbg003 = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            #add-point:單身新增
            {<point name="input.body.b_a_insert"/>}
            #end add-point
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM pmbg_t 
             WHERE pmbgent = g_enterprise AND pmbgdocno = g_pmba_m.pmbadocno
 
               AND pmbg002 = g_pmbg_d[l_ac].pmbg002
               AND pmbg003 = g_pmbg_d[l_ac].pmbg003
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前
               {<point name="input.body.b_insert"/>}
               #end add-point
            
               #同步新增到同層的table
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_pmba_m.pmbadocno
               LET gs_keys[2] = g_pmbg_d[g_detail_idx].pmbg002
               LET gs_keys[3] = g_pmbg_d[g_detail_idx].pmbg003
               CALL apmt800_insert_b('pmbg_t',gs_keys,"'1'")
                           
               #add-point:單身新增後
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE    
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = 'INSERT' 
               LET g_errparam.code   = "std-00006" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               INITIALIZE g_pmbg_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "pmbg_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL apmt800_b_fill()
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert2"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               #ERROR 'INSERT O.K'
               LET g_rec_b = g_rec_b + 1
            END IF
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' THEN
               LET l_cmd='d'
               #add-point:單身刪除後(=d)
               {<point name="input.body.after_delete_d"/>}
               #end add-point
            ELSE
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "" 
                  LET g_errparam.code   = -263 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  CANCEL DELETE
               END IF
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete" mark="Y"/>}
               #end add-point 
               
               #取得該筆資料key值
               INITIALIZE gs_keys TO NULL
               LET gs_keys[01] = g_pmba_m.pmbadocno
 
               LET gs_keys[gs_keys.getLength()+1] = g_pmbg_d_t.pmbg002
               LET gs_keys[gs_keys.getLength()+1] = g_pmbg_d_t.pmbg003
 
            
               #刪除同層單身
               IF NOT apmt800_delete_b('pmbg_t',gs_keys,"'1'") THEN
                  CALL s_transaction_end('N','0')
                  CLOSE apmt800_bcl
                  CANCEL DELETE
               END IF
    
               #刪除下層單身
               IF NOT apmt800_key_delete_b(gs_keys,'pmbg_t') THEN
                  CALL s_transaction_end('N','0')
                  CLOSE apmt800_bcl
                  CANCEL DELETE
               END IF
               
               #刪除多語言
               
               
               #add-point:單身刪除中
               {<point name="input.body.m_delete"/>}
               #end add-point 
               
               CALL s_transaction_end('Y','0')
               CLOSE apmt800_bcl
            
               LET g_rec_b = g_rec_b-1
               #add-point:單身刪除後
               {<point name="input.body.a_delete"/>}
               #end add-point
               LET l_count = g_pmbg_d.getLength()
               
               #add-point:單身刪除後(<>d)
               {<point name="input.body.after_delete"/>}
               #end add-point
            END IF
 
         AFTER DELETE
            #如果是最後一筆
            IF l_ac = (g_pmbg_d.getLength() + 1) THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
            END IF
 
                  #應用 a01 樣板自動產生(Version:1)
         BEFORE FIELD pmbg001
            #add-point:BEFORE FIELD pmbg001
            {<point name="input.b.page1.pmbg001" />}
            #END add-point
 
         #應用 a02 樣板自動產生(Version:1)
         AFTER FIELD pmbg001
            
            #add-point:AFTER FIELD pmbg001
            {<point name="input.a.page1.pmbg001" />}
            #END add-point
            
 
         #應用 a04 樣板自動產生(Version:2)
         ON CHANGE pmbg001
            #add-point:ON CHANGE pmbg001
            {<point name="input.g.page1.pmbg001" />}
            #END add-point 
 
         #應用 a02 樣板自動產生(Version:1)
         AFTER FIELD pmbg002
            
            #add-point:AFTER FIELD pmbg002
            {<point name="input.a.page1.pmbg002" />}
            #END add-point
            
 
         #應用 a01 樣板自動產生(Version:1)
         BEFORE FIELD pmbg002
            #add-point:BEFORE FIELD pmbg002
            {<point name="input.b.page1.pmbg002" />}
            #END add-point
 
         #應用 a04 樣板自動產生(Version:2)
         ON CHANGE pmbg002
            #add-point:ON CHANGE pmbg002
            {<point name="input.g.page1.pmbg002" />}
            #END add-point 
 
