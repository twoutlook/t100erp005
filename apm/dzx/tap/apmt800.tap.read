         BEFORE FIELD pmbg003
            #add-point:BEFORE FIELD pmbg003
            {<point name="input.b.page1.pmbg003" />}
            #END add-point
 
         #應用 a02 樣板自動產生(Version:1)
         AFTER FIELD pmbg003
            
            #add-point:AFTER FIELD pmbg003
            {<point name="input.a.page1.pmbg003" />}
            #END add-point
            
 
         #應用 a04 樣板自動產生(Version:2)
         ON CHANGE pmbg003
            #add-point:ON CHANGE pmbg003
            {<point name="input.g.page1.pmbg003" />}
            #END add-point 
 
         #應用 a01 樣板自動產生(Version:1)
         BEFORE FIELD pmbg004
            #add-point:BEFORE FIELD pmbg004
            {<point name="input.b.page1.pmbg004" />}
            #END add-point
 
         #應用 a02 樣板自動產生(Version:1)
         AFTER FIELD pmbg004
            
            #add-point:AFTER FIELD pmbg004
            {<point name="input.a.page1.pmbg004" />}
            #END add-point
            
 
         #應用 a04 樣板自動產生(Version:2)
         ON CHANGE pmbg004
            #add-point:ON CHANGE pmbg004
            {<point name="input.g.page1.pmbg004" />}
            #END add-point 
 
         #應用 a01 樣板自動產生(Version:1)
         BEFORE FIELD pmbg007
            #add-point:BEFORE FIELD pmbg007
            {<point name="input.b.page1.pmbg007" />}
            #END add-point
 
         #應用 a02 樣板自動產生(Version:1)
         AFTER FIELD pmbg007
            
            #add-point:AFTER FIELD pmbg007
            {<point name="input.a.page1.pmbg007" />}
            #END add-point
            
 
         #應用 a04 樣板自動產生(Version:2)
         ON CHANGE pmbg007
            #add-point:ON CHANGE pmbg007
            {<point name="input.g.page1.pmbg007" />}
            #END add-point 
 
         #應用 a01 樣板自動產生(Version:1)
         BEFORE FIELD pmbg008
            #add-point:BEFORE FIELD pmbg008
            {<point name="input.b.page1.pmbg008" />}
            #END add-point
 
         #應用 a02 樣板自動產生(Version:1)
         AFTER FIELD pmbg008
            
            #add-point:AFTER FIELD pmbg008
            {<point name="input.a.page1.pmbg008" />}
            #END add-point
            
 
         #應用 a04 樣板自動產生(Version:2)
         ON CHANGE pmbg008
            #add-point:ON CHANGE pmbg008
            {<point name="input.g.page1.pmbg008" />}
            #END add-point 
 
         #應用 a01 樣板自動產生(Version:1)
         BEFORE FIELD pmbgacti
            #add-point:BEFORE FIELD pmbgacti
            {<point name="input.b.page1.pmbgacti" />}
            #END add-point
 
         #應用 a02 樣板自動產生(Version:1)
         AFTER FIELD pmbgacti
            
            #add-point:AFTER FIELD pmbgacti
            {<point name="input.a.page1.pmbgacti" />}
            #END add-point
            
 
         #應用 a04 樣板自動產生(Version:2)
         ON CHANGE pmbgacti
            #add-point:ON CHANGE pmbgacti
            {<point name="input.g.page1.pmbgacti" />}
            #END add-point 
 
 
                  #Ctrlp:input.c.page1.pmbg001
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD pmbg001
            #add-point:ON ACTION controlp INFIELD pmbg001
            {<point name="input.c.page1.pmbg001" />}
            #END add-point
 
         #Ctrlp:input.c.page1.pmbg002
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD pmbg002
            #add-point:ON ACTION controlp INFIELD pmbg002
            {<point name="input.c.page1.pmbg002" />}
            #END add-point
 
         #Ctrlp:input.c.page1.pmbg003
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD pmbg003
            #add-point:ON ACTION controlp INFIELD pmbg003
            {<point name="input.c.page1.pmbg003" />}
            #END add-point
 
         #Ctrlp:input.c.page1.pmbg004
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD pmbg004
            #add-point:ON ACTION controlp INFIELD pmbg004
            {<point name="input.c.page1.pmbg004" />}
            #END add-point
 
         #Ctrlp:input.c.page1.pmbg007
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD pmbg007
            #add-point:ON ACTION controlp INFIELD pmbg007
            {<point name="input.c.page1.pmbg007" />}
            #END add-point
 
         #Ctrlp:input.c.page1.pmbg008
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD pmbg008
            #add-point:ON ACTION controlp INFIELD pmbg008
            {<point name="input.c.page1.pmbg008" />}
            #END add-point
 
         #Ctrlp:input.c.page1.pmbgacti
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD pmbgacti
            #add-point:ON ACTION controlp INFIELD pmbgacti
            {<point name="input.c.page1.pmbgacti" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               LET g_pmbg_d[l_ac].* = g_pmbg_d_t.*
               CLOSE apmt800_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = g_pmbg_d[l_ac].pmbg002 
               LET g_errparam.code   = -263 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               LET g_pmbg_d[l_ac].* = g_pmbg_d_t.*
            ELSE
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               #寫入修改者/修改日期資訊(單身)
               
      
               #將遮罩欄位還原
               CALL apmt800_pmbg_t_mask_restore('restore_mask_o')
      
               UPDATE pmbg_t SET (pmbgdocno,pmbg001,pmbg002,pmbg003,pmbg004,pmbg007,pmbg008,pmbgacti) = (g_pmba_m.pmbadocno, 
                   g_pmbg_d[l_ac].pmbg001,g_pmbg_d[l_ac].pmbg002,g_pmbg_d[l_ac].pmbg003,g_pmbg_d[l_ac].pmbg004, 
                   g_pmbg_d[l_ac].pmbg007,g_pmbg_d[l_ac].pmbg008,g_pmbg_d[l_ac].pmbgacti)
                WHERE pmbgent = g_enterprise AND pmbgdocno = g_pmba_m.pmbadocno 
 
                  AND pmbg002 = g_pmbg_d_t.pmbg002 #項次   
                  AND pmbg003 = g_pmbg_d_t.pmbg003  
 
                  
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "pmbg_t" 
                     LET g_errparam.code   = "std-00009" 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     CALL s_transaction_end('N','0')
                     LET g_pmbg_d[l_ac].* = g_pmbg_d_t.*
                  WHEN SQLCA.sqlcode #其他錯誤
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "pmbg_t" 
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()                   
                     CALL s_transaction_end('N','0')
                     LET g_pmbg_d[l_ac].* = g_pmbg_d_t.*  
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_pmba_m.pmbadocno
               LET gs_keys_bak[1] = g_pmbadocno_t
               LET gs_keys[2] = g_pmbg_d[g_detail_idx].pmbg002
               LET gs_keys_bak[2] = g_pmbg_d_t.pmbg002
               LET gs_keys[3] = g_pmbg_d[g_detail_idx].pmbg003
               LET gs_keys_bak[3] = g_pmbg_d_t.pmbg003
               CALL apmt800_update_b('pmbg_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
               END CASE
 
               #將遮罩欄位進行遮蔽
               CALL apmt800_pmbg_t_mask_restore('restore_mask_n')
               
               #判斷key是否有改變
               INITIALIZE gs_keys TO NULL
               IF NOT(g_pmbg_d[g_detail_idx].pmbg002 = g_pmbg_d_t.pmbg002 
                  AND g_pmbg_d[g_detail_idx].pmbg003 = g_pmbg_d_t.pmbg003 
 
                  ) THEN
                  LET gs_keys[01] = g_pmba_m.pmbadocno
 
                  LET gs_keys[gs_keys.getLength()+1] = g_pmbg_d_t.pmbg002
                  LET gs_keys[gs_keys.getLength()+1] = g_pmbg_d_t.pmbg003
 
                  CALL apmt800_key_update_b(gs_keys,'pmbg_t')
               END IF
               
               #修改歷程記錄(單身修改)
               LET g_log1 = util.JSON.stringify(g_pmba_m),util.JSON.stringify(g_pmbg_d_t)
               LET g_log2 = util.JSON.stringify(g_pmba_m),util.JSON.stringify(g_pmbg_d[l_ac])
               IF NOT cl_log_modified_record_d(g_log1,g_log2) THEN 
                  CALL s_transaction_end('N','0')
               END IF
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            #add-point:單身after_row
            {<point name="input.body.after_row"/>}
            #end add-point
            CALL apmt800_unlock_b("pmbg_t","'1'")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock
            #add-point:單身after_row2
            {<point name="input.body.after_row2"/>}
            #end add-point
              
         AFTER INPUT
            #add-point:input段after input 
            {<point name="input.body.after_input"/>}
            #end add-point 
 
         ON ACTION controlo    
            CALL FGL_SET_ARR_CURR(g_pmbg_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_pmbg_d.getLength()+1
            
         #ON ACTION cancel
         #   LET INT_FLAG = 1
         #   LET g_detail_idx = 1
         #   EXIT DIALOG 
 
      END INPUT
      
      INPUT ARRAY g_pmbg2_d FROM s_detail2.*
         ATTRIBUTE(COUNT = g_rec_b,WITHOUT DEFAULTS, #MAXCOUNT = g_max_rec,
