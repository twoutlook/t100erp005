            LET g_pmbg8_d[l_ac].pmbd002_1_desc = ''
            IF  g_pmba_m.pmbadocno IS NOT NULL AND g_pmbg8_d[l_ac].pmbd002_1 IS NOT NULL AND g_pmbg8_d[l_ac].pmbd003_1 IS NOT NULL THEN 
               IF l_cmd = 'a' OR ( l_cmd = 'u' AND (g_pmba_m.pmbadocno != g_pmbadocno_t OR g_pmbg8_d[l_ac].pmbd002_1 != g_pmbg8_d_t.pmbd002_1 OR g_pmbg8_d[l_ac].pmbd003_1 != g_pmbg8_d_t.pmbd003_1)) THEN 
                  IF NOT ap_chk_notDup("","SELECT COUNT(*) FROM pmbd_t WHERE "||"pmbdent = '" ||g_enterprise|| "' AND "||"pmbddocno = '"||g_pmba_m.pmbadocno ||"' AND "|| "pmbd002 = '"||g_pmbg8_d[l_ac].pmbd002_1 ||"' AND "|| "pmbd003 = '"||g_pmbg8_d[l_ac].pmbd003_1 ||"'",'std-00004',0) THEN 
                     LET g_pmbg8_d[l_ac].pmbd002_1 = g_pmbg8_d_t.pmbd002_1
                     NEXT FIELD CURRENT
                  END IF
               END IF
            END IF
            
            IF NOT cl_null(g_pmbg8_d[l_ac].pmbd002_1) THEN
               IF p_cmd = 'a' OR (p_cmd = 'u' AND (g_pmbg8_d[l_ac].pmbd002_1 != g_pmbg8_d_t.pmbd002_1 OR g_pmbg8_d_t.pmbd002_1 IS NULL )) THEN
                  INITIALIZE g_chkparam.* TO NULL
                  LET g_chkparam.arg1 = g_pmbg8_d[l_ac].pmbd002_1
                  IF NOT cl_chk_exist("v_ooib002_1") THEN
                     LET g_pmbg8_d[l_ac].pmbd002_1 = g_pmbg8_d_t.pmbd002_1
                     CALL apmt800_pmbd002_1_ref()
                     NEXT FIELD CURRENT                  
                  END IF
               END IF
            END IF
            CALL apmt800_pmbd002_1_ref()  
            
         ON CHANGE pmbd004_1
            IF g_pmbg8_d[l_ac].pmbd004_1 = 'Y' THEN
               LET l_cnt = 0 
               IF cl_null(g_pmbg8_d_t.pmbd002_1) THEN
                  SELECT COUNT(*) INTO l_cnt FROM pmbd_t
                   WHERE pmbddocno = g_pmba_m.pmbadocno
                     AND pmbdent = g_enterprise
                     AND pmbd004 = 'Y'
                     AND pmbd003 = '2'
               ELSE
                  SELECT COUNT(*) INTO l_cnt FROM pmbd_t
                   WHERE pmbddocno = g_pmba_m.pmbadocno
                     AND pmbdent = g_enterprise
                     AND pmbd004 = 'Y'
                     AND pmbd003 = '2'
                     AND pmbd002 != g_pmbg8_d_t.pmbd002_1
                     
               END IF
               IF l_cnt > 0 THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = 'apm-00310'
                  LET g_errparam.extend = ''
                  LET g_errparam.popup = TRUE
                  CALL cl_err()

                  LET g_pmbg8_d[l_ac].pmbd004_1 = g_pmbg8_d_t.pmbd004_1
                  NEXT FIELD CURRENT
               END IF
               IF g_pmbg8_d[l_ac].pmbd004_1 = 'Y' AND g_pmbg8_d[l_ac].pmbd002_1 <> g_pmba_m.pmbb087 THEN
                  LET g_pmba_m.pmbb087 = g_pmbg8_d[l_ac].pmbd002_1
                  DISPLAY BY NAME g_pmba_m.pmbb087
               END IF
            END IF

         ON ACTION controlp INFIELD pmbd002_1
            #此段落由子樣板a07產生            
            #開窗i段
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'i'
            LET g_qryparam.reqry = FALSE
            LET g_qryparam.default1 = g_pmbg8_d[l_ac].pmbd002_1  #給予default值
            CALL q_ooib002_2()                                   #呼叫開窗
            LET g_pmbg8_d[l_ac].pmbd002_1 = g_qryparam.return1   #將開窗取得的值回傳到變數
            DISPLAY g_pmbg8_d[l_ac].pmbd002_1 TO pmbd002_1       #顯示到畫面上
            CALL apmt800_pmbd002_1_ref()
            NEXT FIELD pmbd002_1                                 #返回原欄位       
 
         AFTER ROW
            LET l_ac = ARR_CURR()
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 9001
               LET g_errparam.extend = ''
               LET g_errparam.popup = FALSE
               CALL cl_err()

               LET INT_FLAG = 0
               IF l_cmd = 'u' THEN
                  LET g_pmbg8_d[l_ac].* = g_pmbg8_d_t.*
               END IF
               CLOSE apmt800_bcl8
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
            
            #其他table進行unlock
            CALL apmt800_unlock_b("pmbd_t","'8'")
            CALL s_transaction_end('Y','0')
 
         AFTER INPUT
            #單身主要的收款條件 update 單頭資料
            LET l_pmbd002 = ''
            SELECT pmbd002 INTO l_pmbd002 FROM pmbd_t
             WHERE pmbdent = g_enterprise
               AND pmbddocno = g_pmba_m.pmbadocno
               AND pmbd003 = '2'
               AND pmbd004 = 'Y'
            IF NOT cl_null(l_pmbd002) THEN
               IF l_pmbd002 != g_pmba_m.pmbb087 THEN
                  UPDATE pmbb_t SET pmbb087 = l_pmbd002
                   WHERE pmbbent = g_enterprise
                     AND pmbbdocno = g_pmba_m.pmbadocno
                     AND pmbbsite = 'ALL'
                  LET g_pmba_m.pmbb087 = l_pmbd002
                  DISPLAY BY NAME g_pmba_m.pmbb087
                  CALL apmt800_pmbb087_ref()
               END IF
            END IF
            
         ON ACTION controlo
            CALL FGL_SET_ARR_CURR(g_pmbg8_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_pmbg8_d.getLength()+1
 
      END INPUT
 
      SUBDIALOG aoo_aooi350_01.aooi350_01_input
      SUBDIALOG aoo_aooi350_02.aooi350_02_input]]>
  </point>
