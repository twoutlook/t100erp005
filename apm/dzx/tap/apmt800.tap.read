  <point name="statechange.b_update" order="" ver="3" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[               LET l_success = TRUE
   CASE lc_state
      WHEN 'Y'
         CALL s_apmt800_conf_chk(g_argv[1],g_pmba_m.pmbadocno,g_pmba_m.pmbastus) RETURNING l_success,g_errno
         IF l_success THEN
            IF cl_ask_confirm('apm-00348') THEN
               CALL s_transaction_begin()
               CALL s_apmt800_carry_upd(g_argv[1],g_pmba_m.pmbadocno) RETURNING l_success,g_errno
               IF NOT l_success THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = g_errno
                  LET g_errparam.extend = g_pmba_m.pmbadocno
                  LET g_errparam.popup = TRUE
                  CALL cl_err()

                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_apmt800_conf_upd(g_pmba_m.pmbadocno) RETURNING l_success,g_errno
                  IF NOT l_success THEN
                     INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = g_errno
                  LET g_errparam.extend = g_pmba_m.pmbadocno
                  LET g_errparam.popup = TRUE
                  CALL cl_err()

                     CALL s_transaction_end('N','0')
                     RETURN
                  ELSE
                     CALL s_transaction_end('Y','1')
                  END IF
               END IF
            ELSE
               RETURN
            END IF
         ELSE
            IF NOT cl_null(g_errno) THEN
               INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = g_errno
                  LET g_errparam.extend = g_pmba_m.pmbadocno
                  LET g_errparam.popup = TRUE
                  CALL cl_err()

            END IF
            RETURN
         END IF
      WHEN 'X'
         CALL s_apmt800_invalid_chk(g_pmba_m.pmbadocno,g_pmba_m.pmbastus) RETURNING l_success,g_errno
         IF l_success THEN
            IF cl_ask_confirm('lib-016') THEN
               CALL s_transaction_begin()
               CALL s_apmt800_invalid_upd(g_pmba_m.pmbadocno) RETURNING l_success,g_errno
               IF NOT l_success THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = g_errno
                  LET g_errparam.extend = g_pmba_m.pmbadocno
                  LET g_errparam.popup = TRUE
                  CALL cl_err()

                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','1')
               END IF
            ELSE
               RETURN
            END IF
         ELSE
            INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = g_errno
                  LET g_errparam.extend = g_pmba_m.pmbadocno
                  LET g_errparam.popup = TRUE
                  CALL cl_err()

            RETURN
         END IF
   END CASE  ]]>
