    <![CDATA[
PUBLIC FUNCTION apmt800_chk_uni_business_no()
   DEFINE l_cnt            LIKE type_t.num5
   DEFINE l_pmaa002          LIKE pmaa_t.pmaa002

   LET g_errno = ''
   LET g_pmaa002_flag = ''
   LET l_cnt = 0
   IF NOT cl_null(g_pmba_m.pmbadocno) THEN
      SELECT COUNT(*) INTO l_cnt FROM pmba_t
       WHERE pmba003 = g_pmba_m.pmba003
         AND pmbaent = g_enterprise
         AND pmbadocno != g_pmba_m.pmbadocno
         AND pmbastus = 'N'
   ELSE
      SELECT COUNT(*) INTO l_cnt FROM pmba_t
       WHERE pmba003 = g_pmba_m.pmba003
         AND pmbaent = g_enterprise
         AND pmbastus = 'N'
   END IF

   IF l_cnt > 0 THEN
      LET g_errno = 'art-00267'
      RETURN
   END IF 
   
   SELECT COUNT(*) INTO l_cnt FROM pmaa_t
    WHERE pmaa003 = g_pmba_m.pmba003
      AND pmaaent = g_enterprise
   IF l_cnt = 1 THEN
      SELECT pmaa002 INTO l_pmaa002 FROM pmaa_t
       WHERE pmaa003 = g_pmba_m.pmba003
