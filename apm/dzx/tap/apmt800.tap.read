          WHERE oobaent = ooblent   AND ooba001 = oobl001
            AND ooba002 = oobl002   AND oobl003 = 'apmt800'
            AND oobl001 = g_ooef004 AND oobl002 = g_pmba_m.pmbadocno
            AND ooblent = g_enterprise
      WHEN '2'
         LET g_errno = ''
         SELECT oobastus INTO l_stus  FROM ooba_t,oobl_t
          WHERE oobaent = ooblent   AND ooba001 = oobl001
            AND ooba002 = oobl002   AND oobl003 = 'adbt200'
            AND oobl001 = g_ooef004 AND oobl002 = g_pmba_m.pmbadocno
            AND ooblent = g_enterprise
      WHEN '3'
         LET g_errno = ''
         SELECT oobastus INTO l_stus  FROM ooba_t,oobl_t
          WHERE oobaent = ooblent   AND ooba001 = oobl001
            AND ooba002 = oobl002   AND oobl003 = 'axmt800'
            AND oobl001 = g_ooef004 AND oobl002 = g_pmba_m.pmbadocno
            AND ooblent = g_enterprise    
   END CASE
   CASE WHEN SQLCA.SQLCODE = 100   LET g_errno = 'aim-00056'
        WHEN l_stus='N'            LET g_errno = 'aoo-00102'
   END CASE
END FUNCTION]]>
