  <point name="browser_fill.other_fill" order="" ver="16" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[                     
   #判斷是否填充
   IF (apmt800_fill_chk(8) AND (cl_null(g_wc2_table8) OR g_wc2_table8.trim() = '1=1'))
   OR((NOT cl_null(g_wc2_table8) AND g_wc2_table8.trim() <> '1=1')) THEN
      #150519-00032#1 Modify By Ken 150520
      #LET g_sql = "SELECT  UNIQUE pmbd001,pmbd003,pmbdstus,pmbd002,'',pmbd004 FROM pmbd_t",   
      #            " INNER JOIN pmba_t ON pmbadocno = pmbddocno ",
      #            " WHERE pmbdent=? AND pmbddocno=?"  
      LET g_sql = "SELECT  UNIQUE pmbd001,pmbd003,pmbdstus,pmbd002,t8.ooibl004,pmbd004 FROM pmbd_t",   
                  " INNER JOIN pmba_t ON pmbadocno = pmbddocno ",
                  " LEFT JOIN ooibl_t t8 ON t8.ooiblent='"||g_enterprise||"' AND t8.ooibl002=pmbd002 AND t8.ooibl003='"||g_dlang||"' ",  
                  " WHERE pmbdent=? AND pmbddocno=?" 
      #150519-00032#1 Modify By Ken 150520

      LET g_sql = g_sql CLIPPED," AND pmbd003 = '2'"        

      IF NOT cl_null(g_wc2_table8) THEN
         LET g_sql = g_sql CLIPPED," AND ",g_wc2_table8 CLIPPED
      END IF
      
      LET g_sql = g_sql, " ORDER BY pmbd_t.pmbd002,pmbd_t.pmbd003"
      
      PREPARE apmt800_pb8 FROM g_sql
      DECLARE b_fill_cs8 CURSOR FOR apmt800_pb8
      
      LET l_ac = 1
      
      OPEN b_fill_cs8 USING g_enterprise,g_pmba_m.pmbadocno
 
                                               
      FOREACH b_fill_cs8 INTO g_pmbg8_d[l_ac].pmbd001_1,g_pmbg8_d[l_ac].pmbd003_1,g_pmbg8_d[l_ac].pmbdstus_1, 
          g_pmbg8_d[l_ac].pmbd002_1,g_pmbg8_d[l_ac].pmbd002_1_desc,g_pmbg8_d[l_ac].pmbd004_1
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "FOREACH:"
            LET g_errparam.popup = TRUE
            CALL cl_err()

            EXIT FOREACH
         END IF
         CALL apmt800_pmbd002_1_ref()
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code =  9035
            LET g_errparam.extend =  ''
            LET g_errparam.popup = FALSE
            CALL cl_err()

            EXIT FOREACH
         END IF
         
      END FOREACH
   END IF
   
   CALL g_pmbg8_d.deleteElement(g_pmbg8_d.getLength())
   
   FREE apmt800_pb8
   
   IF NOT cl_null(g_pmba_m.pmba027) THEN
      LET g_pmaa027_d = g_pmba_m.pmba027
      CALL aooi350_01_b_fill(g_pmaa027_d)
      CALL aooi350_02_b_fill(g_pmaa027_d)
   END IF  ]]>
  </point>
