   #將資料輸出到畫面上
   DISPLAY BY NAME g_pmca_m.pmcadocno,g_pmca_m.pmcadocdt,g_pmca_m.pmca001,g_pmca_m.pmca002,g_pmca_m.pmca002_desc, 
