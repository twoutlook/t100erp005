  <section id="apmr410_g01.sel_prep" ver="1" status="" src="s">
<![CDATA[#+ 選單功能實際執行處
PRIVATE FUNCTION apmr410_g01_sel_prep()
   #add-point:sel_prep段define
   {<point name="sel_prep.define"/>}
   #end add-point
 
   #add-point:sel_prep before
   {<point name="sel_prep.before" mark="Y"/>}
   #end add-point
   
   #add-point:sel_prep g_select
   {<point name="sel_prep.g_select" mark="Y"/>}
   #end add-point
   LET g_select = " SELECT pmdc001,pmdc002,pmdc003,pmdc004,pmdc005,pmdc006,pmdc007,pmdc010,pmdc011,pmdc012, 
       pmdc020,pmdc021,pmdc022,pmdc900,pmdc901,pmdc902,pmdc905,pmdc906,pmdcacti,pmdcdocdt,pmdcdocno, 
       pmdcent,pmdcsite,pmdcstus,pmdd001,pmdd002,pmdd003,pmdd004,pmdd005,pmdd006,pmdd007,pmdd008,pmdd009, 
       pmdd010,pmdd011,pmdd012,pmdd014,pmdd015,pmdd016,pmdd017,pmdd018,pmdd019,pmdd020,pmdd021,pmdd030, 
       pmdd031,pmdd032,pmdd033,pmdd034,pmdd035,pmdd036,pmdd037,pmdd038,pmdd039,pmdd040,pmdd041,pmdd042, 
       pmdd043,pmdd044,pmdd045,pmdd046,pmdd048,pmdd049,pmdd050,pmdd901,pmdd902,pmdd903,pmddseq,pmddsite, 
       oofa_t.oofa011,ooefl_t.ooefl003,t2.ooefl003,x.t5_ooefl003,x.t6_ooefl003,ooail_t.ooail003,oodbl_t.oodbl004, 
       oocql_t.oocql004,t1.oocql004,x.t3_oocql004,x.t4_oocql004,x.t7_oocql004,x.t8_oocql004,x.imaal_t_imaal003, 
       x.t10_imaal003,x.oocal_t_oocal003,x.t9_oocal003,x.t11_oocal003,x.pmaal_t_pmaal004,x.ooibl_t_ooibl004, 
       x.inaa_t_inaa002,x.inab_t_inab003,trim(pmdc002)||'.'||trim(oofa_t.oofa011),trim(pmdc003)||'.'||trim(t2.ooefl003), 
       trim(pmdc007)||'.'||trim(ooefl_t.ooefl003),trim(pmdd015)||'.'||trim(x.pmaal_t_pmaal004),trim(pmdd046)||'.'||trim(x.t6_ooefl003), 
       trim(pmdd039)||'.'||trim(x.inab_t_inab003),trim(pmdd037)||'.'||trim(x.t5_ooefl003),trim(pmdd038)||'.'||trim(x.inaa_t_inaa002), 
       NULL,x.t10_imaal004"
 
   #add-point:sel_prep g_from
   {<point name="sel_prep.g_from" mark="Y"/>}
   #end add-point
    LET g_from = " FROM pmdc_t LEFT OUTER JOIN oocql_t ON oocql_t.oocql001 = '267' AND oocql_t.oocql002 = pmdc_t.pmdc905 AND oocql_t.oocqlent = pmdc_t.pmdcent AND oocql_t.oocql003 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oocql_t t1 ON t1.oocql001 = '263' AND t1.oocql002 = pmdc_t.pmdc021 AND t1.oocqlent = pmdc_t.pmdcent AND t1.oocql003 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oodbl_t ON oodbl_t.oodbl002 = pmdc_t.pmdc010 AND oodbl_t.oodblent = pmdc_t.pmdcent AND oodbl_t.oodbl003 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN ooefl_t ON ooefl_t.ooefl001 = pmdc_t.pmdc007 AND ooefl_t.ooeflent = pmdc_t.pmdcent AND ooefl_t.ooefl002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN ooail_t ON ooail_t.ooail001 = pmdc_t.pmdc005 AND ooail_t.ooailent = pmdc_t.pmdcent AND ooail_t.ooail002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN ooefl_t t2 ON t2.ooefl001 = pmdc_t.pmdc003 AND t2.ooeflent = pmdc_t.pmdcent AND t2.ooefl002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oofa_t ON oofa_t.oofa002 = '2' AND oofa_t.oofa003 = pmdc_t.pmdc002 AND oofa_t.oofaent = pmdc_t.pmdcent LEFT OUTER JOIN ( SELECT pmdd_t.*, 
        t5.ooefl003 t5_ooefl003,t6.ooefl003 t6_ooefl003,t3.oocql004 t3_oocql004,t4.oocql004 t4_oocql004, 
        t7.oocql004 t7_oocql004,t8.oocql004 t8_oocql004,imaal_t.imaal003 imaal_t_imaal003,t10.imaal003 t10_imaal003, 
        oocal_t.oocal003 oocal_t_oocal003,t9.oocal003 t9_oocal003,t11.oocal003 t11_oocal003,pmaal_t.pmaal004 pmaal_t_pmaal004, 
        ooibl_t.ooibl004 ooibl_t_ooibl004,inaa_t.inaa002 inaa_t_inaa002,inab_t.inab003 inab_t_inab003, 
        t10.imaal004 t10_imaal004 FROM pmdd_t LEFT OUTER JOIN ooibl_t ON ooibl_t.ooibl002 = pmdd_t.pmdd016 AND ooibl_t.ooiblent = pmdd_t.pmddent AND ooibl_t.ooibl003 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oocql_t t3 ON t3.oocql001 = '238' AND t3.oocql002 = pmdd_t.pmdd017 AND t3.oocqlent = pmdd_t.pmddent AND t3.oocql003 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oocql_t t4 ON t4.oocql001 = '265' AND t4.oocql002 = pmdd_t.pmdd031 AND t4.oocqlent = pmdd_t.pmddent AND t4.oocql003 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN ooefl_t t5 ON t5.ooefl001 = pmdd_t.pmdd037 AND t5.ooeflent = pmdd_t.pmddent AND t5.ooefl002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN inaa_t ON inaa_t.inaasite = pmdd_t.pmddsite AND inaa_t.inaa001 = pmdd_t.pmdd038 AND inaa_t.inaaent = pmdd_t.pmddent             LEFT OUTER JOIN inab_t ON inab_t.inabsite = pmdd_t.pmddsite AND inab_t.inab001 = pmdd_t.pmdd038 AND inab_t.inab002 = pmdd_t.pmdd039 AND inab_t.inabent = pmdd_t.pmddent             LEFT OUTER JOIN ooefl_t t6 ON t6.ooefl001 = pmdd_t.pmdd046 AND t6.ooeflent = pmdd_t.pmddent AND t6.ooefl002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oocql_t t7 ON t7.oocql001 = '274' AND t7.oocql002 = pmdd_t.pmdd048 AND t7.oocqlent = pmdd_t.pmddent AND t7.oocql003 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN pmaal_t ON pmaal_t.pmaal001 = pmdd_t.pmdd015 AND pmaal_t.pmaalent = pmdd_t.pmddent AND pmaal_t.pmaal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN imaal_t ON imaal_t.imaal001 = pmdd_t.pmdd012 AND imaal_t.imaalent = pmdd_t.pmddent AND imaal_t.imaal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oocql_t t8 ON t8.oocql001 = '267' AND t8.oocql002 = pmdd_t.pmdd902 AND t8.oocqlent = pmdd_t.pmddent AND t8.oocql003 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oocal_t ON oocal_t.oocal001 = pmdd_t.pmdd009 AND oocal_t.oocalent = pmdd_t.pmddent AND oocal_t.oocal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oocal_t t9 ON t9.oocal001 = pmdd_t.pmdd007 AND t9.oocalent = pmdd_t.pmddent AND t9.oocal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN imaal_t t10 ON t10.imaal001 = pmdd_t.pmdd004 AND t10.imaalent = pmdd_t.pmddent AND t10.imaal002 = '" , 
        g_dlang,"'" ,"             LEFT OUTER JOIN oocal_t t11 ON t11.oocal001 = pmdd_t.pmdd011 AND t11.oocalent = pmdd_t.pmddent AND t11.oocal002 = '" , 
        g_dlang,"'" ," ) x  ON pmdc_t.pmdcent = x.pmddent AND pmdc_t.pmdcdocno = x.pmdddocno AND pmdc_t.pmdc900  
        = x.pmdd900"
 
   #add-point:sel_prep g_where
   {<point name="sel_prep.g_where" mark="Y"/>}
   #end add-point
    LET g_where = " WHERE " ,tm.wc CLIPPED
 
   #add-point:sel_prep g_order
   {<point name="sel_prep.g_order" mark="Y"/>}
   #end add-point
    LET g_order = " ORDER BY pmdcdocno,pmddseq"
 
   #add-point:sel_prep.sql.before
   {<point name="sel_prep.sql.before mark="Y""/>}
   #end add-point:sel_prep.sql.before
 
   LET g_sql = g_select CLIPPED ," ",g_from CLIPPED ," ",g_where CLIPPED ," ",g_order CLIPPED 
 
   #add-point:sel_prep.sql.after
   {<point name="sel_prep.sql.after mark="Y""/>}
   #end add-point
   PREPARE apmr410_g01_prepare FROM g_sql
   IF STATUS THEN
      CALL cl_err('prepare:',STATUS,1)
      CALL cl_ap_exitprogram("0")
   END IF
   DECLARE apmr410_g01_curs CURSOR FOR apmr410_g01_prepare
 
END FUNCTION
]]>
