  <point name="function.apmt480_pmdy_upd" order="24" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION apmt480_pmdy_upd()
DEFINE l_pmdyseq   LIKE pmdy_t.pmdyseq
DEFINE l_pmdy009   LIKE pmdy_t.pmdy009
DEFINE l_pmdy010   LIKE pmdy_t.pmdy010
DEFINE l_pmdy011   LIKE pmdy_t.pmdy011
DEFINE l_pmdy017   LIKE pmdy_t.pmdy017
DEFINE l_pmdy018   LIKE pmdy_t.pmdy018
DEFINE l_pmdy019   LIKE pmdy_t.pmdy019
DEFINE l_sql       STRING

   LET l_sql = " SELECT pmdyseq,pmdy009,pmdy010,pmdy011 FROM pmdy_t ",
               "  WHERE pmdyent = '",g_enterprise,"'"  ,
               "    AND pmdydocno = '",g_pmdx_m.pmdxdocno,"'"
   PREPARE t440_pmdy_pre FROM l_sql
   DECLARE t440_pmdy_cs CURSOR FOR t440_pmdy_pre
   FOREACH t440_pmdy_cs INTO l_pmdyseq,l_pmdy009,l_pmdy010,l_pmdy011
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF  
      CALL s_curr_round(g_site,g_pmdx_m.pmdx005,l_pmdy010,'1') RETURNING l_pmdy010
      CALL apmt480_get_amount(l_pmdyseq,l_pmdy009,l_pmdy010,l_pmdy011)  
         RETURNING l_pmdy017,l_pmdy018,l_pmdy019
      UPDATE pmdy_t 
         SET pmdy010 = l_pmdy010,
             pmdy017 = l_pmdy017,
             pmdy018 = l_pmdy018,
             pmdy019 = l_pmdy019
       WHERE pmdyent = g_enterprise
         AND pmdydocno = g_pmdx_m.pmdxdocno
         AND pmdyseq = l_pmdyseq
      IF SQLCA.sqlcode THEN
         CALL cl_err("pmdy_t:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
   END FOREACH 
   CALL apmt480_b_fill()   
END FUNCTION]]>
