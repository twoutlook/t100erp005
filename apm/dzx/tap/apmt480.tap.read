               CALL apmt480_filter()
               EXIT DIALOG
 
 
 
         
         ON ACTION first
            CALL apmt480_fetch('F')
            LET g_current_row = g_current_idx
            LET g_curr_diag = ui.DIALOG.getCurrent()
            CALL apmt480_idx_chk()
            
         ON ACTION previous
            CALL apmt480_fetch('P')
            LET g_current_row = g_current_idx
            LET g_curr_diag = ui.DIALOG.getCurrent()
            CALL apmt480_idx_chk()
            
         ON ACTION jump
            CALL apmt480_fetch('/')
            LET g_current_row = g_current_idx
            LET g_curr_diag = ui.DIALOG.getCurrent()
            CALL apmt480_idx_chk()
            
