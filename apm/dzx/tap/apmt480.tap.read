  <section id="apmt480.b_fill" ver="8" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION apmt480_b_fill()
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_pmdy_d.clear()    #g_pmdy_d 單頭及單身 
   CALL g_pmdy2_d.clear()
   CALL g_pmdy3_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF apmt480_fill_chk(1) THEN
   
      LET g_sql = "SELECT  UNIQUE pmdysite,pmdyseq,pmdy002,pmdy003,pmdy005,pmdy006,pmdy007,pmdy014,pmdy008, 
          pmdy009,pmdy010,pmdy024,pmdy011,pmdy012,pmdy017,pmdy018,pmdy019,pmdy013,pmdy004,pmdy030,pmdy001, 
          pmdy020,pmdy021,pmdy022,pmdy023 ,t1.imaal003 ,t2.imaal004 ,t3.oocql004 ,t4.oocql004 ,t5.oocql004 FROM pmdy_t", 
             
                  " INNER JOIN pmdx_t ON pmdxdocno = pmdydocno ",
 
                  #"",
                  " LEFT JOIN pmdz_t ON pmdyent = pmdzent AND pmdydocno = pmdzdocno AND pmdyseq = pmdzseq ",
                  "",
                                 " LEFT JOIN imaal_t t1 ON t1.imaalent='"||g_enterprise||"' AND t1.imaal001=pmdy002 AND t1.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t2 ON t2.imaalent='"||g_enterprise||"' AND t2.imaal001=pmdy002 AND t2.imaal002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t3 ON t3.oocqlent='"||g_enterprise||"' AND t3.oocql001='221' AND t3.oocql002=pmdy006 AND t3.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t4 ON t4.oocqlent='"||g_enterprise||"' AND t4.oocql001='277' AND t4.oocql002=pmdy014 AND t4.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t5 ON t5.oocqlent='"||g_enterprise||"' AND t5.oocql001='263' AND t5.oocql002=pmdy013 AND t5.oocql003='"||g_dlang||"' ",
 
                  " WHERE pmdyent=? AND pmdydocno=?"
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      #add-point:b_fill段sql_before
      {<point name="b_fill.body.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table1) THEN
         LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
      END IF
      
      #子單身的WC
         IF NOT cl_null(g_wc2_table2) THEN 
      LET g_sql = g_sql CLIPPED," AND ", g_wc2_table2 CLIPPED
   END IF 
 
      
      LET g_sql = g_sql, " ORDER BY pmdy_t.pmdyseq"
      
      #add-point:單身填充控制
      {<point name="b_fill.sql"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE apmt480_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR apmt480_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_pmdx_m.pmdxdocno
                                               
      FOREACH b_fill_cs INTO g_pmdy_d[l_ac].pmdysite,g_pmdy_d[l_ac].pmdyseq,g_pmdy_d[l_ac].pmdy002,g_pmdy_d[l_ac].pmdy003, 
          g_pmdy_d[l_ac].pmdy005,g_pmdy_d[l_ac].pmdy006,g_pmdy_d[l_ac].pmdy007,g_pmdy_d[l_ac].pmdy014, 
          g_pmdy_d[l_ac].pmdy008,g_pmdy_d[l_ac].pmdy009,g_pmdy_d[l_ac].pmdy010,g_pmdy_d[l_ac].pmdy024, 
          g_pmdy_d[l_ac].pmdy011,g_pmdy_d[l_ac].pmdy012,g_pmdy_d[l_ac].pmdy017,g_pmdy_d[l_ac].pmdy018, 
          g_pmdy_d[l_ac].pmdy019,g_pmdy_d[l_ac].pmdy013,g_pmdy_d[l_ac].pmdy004,g_pmdy_d[l_ac].pmdy030, 
          g_pmdy_d[l_ac].pmdy001,g_pmdy2_d[l_ac].pmdy020,g_pmdy2_d[l_ac].pmdy021,g_pmdy2_d[l_ac].pmdy022, 
          g_pmdy2_d[l_ac].pmdy023,g_pmdy_d[l_ac].pmdy002_desc,g_pmdy_d[l_ac].pmdy002_desc_desc,g_pmdy_d[l_ac].pmdy006_desc, 
          g_pmdy_d[l_ac].pmdy014_desc,g_pmdy_d[l_ac].pmdy013_desc
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               CALL cl_err( '', 9035, 1)
            END IF
            EXIT FOREACH
         END IF
         
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
 
   
   #add-point:browser_fill段其他table處理
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_pmdy_d.deleteElement(g_pmdy_d.getLength())
   CALL g_pmdy2_d.deleteElement(g_pmdy2_d.getLength())
   CALL g_pmdy3_d.deleteElement(g_pmdy3_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE apmt480_pb
 
   
END FUNCTION
]]>
  </section>
  <section id="apmt480.b_fill2" ver="3" status="" src="s">
    <![CDATA[#+ 單身陣列填充2
PRIVATE FUNCTION apmt480_b_fill2(pi_idx)
   DEFINE pi_idx          LIKE type_t.num5
   DEFINE li_ac           LIKE type_t.num5
   DEFINE ls_chk          LIKE type_t.chr1
   #add-point:b_fill2段define
   {<point name="b_fill2.define"/>}
   #end add-point
   
   LET li_ac = l_ac 
   
   IF g_detail_idx <= 0 THEN
      RETURN
   END IF
   
   IF apmt480_fill_chk(2) THEN
      IF (pi_idx = 2 OR pi_idx = 0 ) AND g_pmdy_d.getLength() > 0 THEN
               CALL g_pmdy3_d.clear()
 
         LET g_sql = "SELECT  UNIQUE pmdzseq,pmdzseq1,pmdz001,pmdz002,pmdz003,pmdz004,pmdz005  FROM pmdz_t", 
                 
                     "",
                     
                     " WHERE pmdzent=? AND pmdzdocno=? AND pmdzseq=?"
         
         IF NOT cl_null(g_wc2_table2) THEN
            LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
         END IF
         
         LET g_sql = g_sql, " ORDER BY  pmdz_t.pmdzseq1" 
                            
         #add-point:單身填充前
         {<point name="b_fill2.before_fill2" />}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE apmt480_pb2 FROM g_sql
         DECLARE b_fill_curs2 CURSOR FOR apmt480_pb2
         
         OPEN b_fill_curs2 USING g_enterprise,
