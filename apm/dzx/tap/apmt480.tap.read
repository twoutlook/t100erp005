  <section id="apmt480.input.body" ver="7" status="" src="s">
    <![CDATA[   
      #Page1 預設值產生於此處
      INPUT ARRAY g_pmdy_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_pmdy_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL apmt480_b_fill()
            LET g_rec_b = g_pmdy_d.getLength()
            #add-point:資料輸入前
            {<point name="input.d.before_input"/>}
            #end add-point
         
         BEFORE ROW
            #add-point:modify段before row2
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET l_insert = FALSE
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET g_detail_idx = l_ac
                           CALL apmt480_b_fill2('2')
 
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
         
            CALL s_transaction_begin()
            OPEN apmt480_cl USING g_enterprise,g_pmdx_m.pmdxdocno
            IF STATUS THEN
               CALL cl_err("OPEN apmt480_cl:", STATUS, 1)
               CLOSE apmt480_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            
            LET g_rec_b = g_pmdy_d.getLength()
            
            IF g_rec_b >= l_ac 
               AND g_pmdy_d[l_ac].pmdyseq IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_pmdy_d_t.* = g_pmdy_d[l_ac].*  #BACKUP
               LET g_pmdy_d_o.* = g_pmdy_d[l_ac].*  #BACKUP
               CALL apmt480_set_entry_b(l_cmd)
               #add-point:modify段after_set_entry_b
               {<point name="input.body.after_set_entry_b"/>}
               #end add-point  
               CALL apmt480_set_no_entry_b(l_cmd)
               IF NOT apmt480_lock_b("pmdy_t","'1'") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH apmt480_bcl INTO g_pmdy_d[l_ac].pmdysite,g_pmdy_d[l_ac].pmdyseq,g_pmdy_d[l_ac].pmdy002, 
                      g_pmdy_d[l_ac].pmdy003,g_pmdy_d[l_ac].pmdy005,g_pmdy_d[l_ac].pmdy006,g_pmdy_d[l_ac].pmdy007, 
                      g_pmdy_d[l_ac].pmdy014,g_pmdy_d[l_ac].pmdy008,g_pmdy_d[l_ac].pmdy009,g_pmdy_d[l_ac].pmdy010, 
                      g_pmdy_d[l_ac].pmdy024,g_pmdy_d[l_ac].pmdy011,g_pmdy_d[l_ac].pmdy012,g_pmdy_d[l_ac].pmdy017, 
                      g_pmdy_d[l_ac].pmdy018,g_pmdy_d[l_ac].pmdy019,g_pmdy_d[l_ac].pmdy013,g_pmdy_d[l_ac].pmdy004, 
                      g_pmdy_d[l_ac].pmdy030,g_pmdy_d[l_ac].pmdy001,g_pmdy2_d[l_ac].pmdy020,g_pmdy2_d[l_ac].pmdy021, 
                      g_pmdy2_d[l_ac].pmdy022,g_pmdy2_d[l_ac].pmdy023
                  IF SQLCA.sqlcode THEN
                     CALL cl_err(g_pmdy_d_t.pmdyseq,SQLCA.sqlcode,1)
                     LET l_lock_sw = "Y"
                  END IF
                  
                  LET g_bfill = "N"
                  CALL apmt480_show()
                  LET g_bfill = "Y"
                  
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
        
         BEFORE INSERT
            
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_pmdy_d[l_ac].* TO NULL 
            INITIALIZE g_pmdy_d_t.* TO NULL 
            INITIALIZE g_pmdy_d_o.* TO NULL 
                  LET g_pmdy_d[l_ac].pmdy024 = "N"
      LET g_pmdy_d[l_ac].pmdy012 = "0"
 
            LET g_pmdy_d_t.* = g_pmdy_d[l_ac].*     #新輸入資料
            LET g_pmdy_d_o.* = g_pmdy_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL apmt480_set_entry_b(l_cmd)
            #add-point:modify段after_set_entry_b
            {<point name="input.body.insert.after_set_entry_b"/>}
            #end add-point
            CALL apmt480_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_pmdy_d[li_reproduce_target].* = g_pmdy_d[li_reproduce].*
               LET g_pmdy2_d[li_reproduce_target].* = g_pmdy2_d[li_reproduce].*
 
               LET g_pmdy_d[li_reproduce_target].pmdyseq = NULL
 
            END IF
            #公用欄位給值(單身)
            
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            #add-point:單身新增
            {<point name="input.body.b_a_insert"/>}
            #end add-point
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM pmdy_t 
             WHERE pmdyent = g_enterprise AND pmdydocno = g_pmdx_m.pmdxdocno
 
               AND pmdyseq = g_pmdy_d[l_ac].pmdyseq
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前
               {<point name="input.body.b_insert"/>}
               #end add-point
            
               #同步新增到同層的table
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_pmdx_m.pmdxdocno
               LET gs_keys[2] = g_pmdy_d[g_detail_idx].pmdyseq
               CALL apmt480_insert_b('pmdy_t',gs_keys,"'1'")
                           
               #add-point:單身新增後
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_pmdy_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("pmdy_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL apmt480_b_fill()
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert2"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR 'INSERT O.K'
               LET g_rec_b = g_rec_b + 1
            END IF
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_pmdy_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_pmdy_d.deleteElement(l_ac)
               NEXT FIELD pmdyseq
            END IF
         
            IF g_pmdy_d[l_ac].pmdyseq IS NOT NULL
 
               THEN 
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete" mark="Y"/>}
               #end add-point 
               
               DELETE FROM pmdy_t
                WHERE pmdyent = g_enterprise AND pmdydocno = g_pmdx_m.pmdxdocno AND
 
                      pmdyseq = g_pmdy_d_t.pmdyseq
 
                  
               #add-point:單身刪除中
               {<point name="input.body.m_delete"/>}
               #end add-point 
               
               IF SQLCA.sqlcode THEN
                  CALL cl_err("pmdy_t",SQLCA.sqlcode,1)
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               ELSE
                  LET g_rec_b = g_rec_b-1
                  
                  #add-point:單身刪除後
                  {<point name="input.body.a_delete"/>}
                  #end add-point
                  CALL s_transaction_end('Y','0')
               END IF 
               CLOSE apmt480_bcl
               LET l_count = g_pmdy_d.getLength()
            END IF 
            
                           INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_pmdx_m.pmdxdocno
               LET gs_keys[2] = g_pmdy_d[g_detail_idx].pmdyseq
 
              
         AFTER DELETE 
            #add-point:單身刪除後2
            {<point name="input.body.after_delete"/>}
            #end add-point
                           CALL apmt480_delete_b('pmdy_t',gs_keys,"'1'")
 
                  #此段落由子樣板a01產生
         BEFORE FIELD pmdysite
            #add-point:BEFORE FIELD pmdysite
            {<point name="input.b.page1.pmdysite" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD pmdysite
            
