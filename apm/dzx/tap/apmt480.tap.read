  <section id="apmt480.filter" ver="5" status="" src="s">
    <![CDATA[#+ 此段落由子樣板a50產生
#+ filter過濾功能
PRIVATE FUNCTION apmt480_filter()
   #add-point:filter段define
   {<point name="filter.define"/>}
   #end add-point   
 
   #切換畫面
   IF NOT g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",1)
      CALL gfrm_curr.setElementHidden("worksheet",0)
      LET g_main_hidden = 1
   END IF   
 
   LET INT_FLAG = 0
 
   LET g_qryparam.state = 'c'
 
   LET g_wc_filter_t = g_wc_filter.trim()
   LET g_wc_t = g_wc
 
   LET g_wc = cl_replace_str(g_wc, g_wc_filter_t, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON pmdxdocno,pmdxdocdt,pmdx002,pmdx003,pmdx004,pmdx005,pmdx014,pmdx015
                          FROM s_browse[1].b_pmdxdocno,s_browse[1].b_pmdxdocdt,s_browse[1].b_pmdx002, 
                              s_browse[1].b_pmdx003,s_browse[1].b_pmdx004,s_browse[1].b_pmdx005,s_browse[1].b_pmdx014, 
                              s_browse[1].b_pmdx015
 
         BEFORE CONSTRUCT
               DISPLAY apmt480_filter_parser('pmdxdocno') TO s_browse[1].b_pmdxdocno
            DISPLAY apmt480_filter_parser('pmdxdocdt') TO s_browse[1].b_pmdxdocdt
            DISPLAY apmt480_filter_parser('pmdx002') TO s_browse[1].b_pmdx002
            DISPLAY apmt480_filter_parser('pmdx003') TO s_browse[1].b_pmdx003
            DISPLAY apmt480_filter_parser('pmdx004') TO s_browse[1].b_pmdx004
            DISPLAY apmt480_filter_parser('pmdx005') TO s_browse[1].b_pmdx005
            DISPLAY apmt480_filter_parser('pmdx014') TO s_browse[1].b_pmdx014
            DISPLAY apmt480_filter_parser('pmdx015') TO s_browse[1].b_pmdx015
      
         #add-point:filter段cs_ctrl
         {<point name="filter.cs_ctrl"/>}
         #end add-point
      
      END CONSTRUCT
 
      #add-point:filter段add_cs
      {<point name="filter.add_cs"/>}
      #end add-point
 
      BEFORE DIALOG
         #add-point:filter段b_dialog
         {<point name="filter.b_dialog"/>}
         #end add-point  
      
      ON ACTION accept
         ACCEPT DIALOG
 
      ON ACTION cancel
         LET INT_FLAG = 1
         EXIT DIALOG 
 
      #交談指令共用ACTION
      &include "common_action.4gl" 
         CONTINUE DIALOG
   
   END DIALOG
 
   IF NOT INT_FLAG THEN
      LET g_wc_filter = "   AND   ", g_wc_filter, "   "
      LET g_wc = g_wc , g_wc_filter
   ELSE
      LET g_wc_filter = g_wc_filter_t
      LET g_wc = g_wc_t
   END IF
 
      CALL apmt480_filter_show('pmdxdocno')
   CALL apmt480_filter_show('pmdxdocdt')
   CALL apmt480_filter_show('pmdx002')
   CALL apmt480_filter_show('pmdx003')
   CALL apmt480_filter_show('pmdx004')
   CALL apmt480_filter_show('pmdx005')
   CALL apmt480_filter_show('pmdx014')
   CALL apmt480_filter_show('pmdx015')
 
END FUNCTION
