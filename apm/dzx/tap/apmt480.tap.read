  <section id="apmt480.modify" ver="11" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION apmt480_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_pmdx_m.pmdxdocno IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   ERROR ""
  
   LET g_pmdxdocno_t = g_pmdx_m.pmdxdocno
 
   CALL s_transaction_begin()
   
   OPEN apmt480_cl USING g_enterprise,g_pmdx_m.pmdxdocno
   IF STATUS THEN
      CALL cl_err("OPEN apmt480_cl:", STATUS, 1)
      CLOSE apmt480_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE apmt480_master_referesh USING g_pmdx_m.pmdxdocno INTO g_pmdx_m.pmdxdocno,g_pmdx_m.pmdx000, 
