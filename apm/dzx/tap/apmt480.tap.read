  <section id="apmt480.delete" ver="11" status="" src="s">
    <![CDATA[#+ 資料刪除
PRIVATE FUNCTION apmt480_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_pmdx_m.pmdxdocno IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   
 
   CALL apmt480_show()
   
   CALL s_transaction_begin()
 
   OPEN apmt480_cl USING g_enterprise,g_pmdx_m.pmdxdocno
   IF STATUS THEN
      CALL cl_err("OPEN apmt480_cl:", STATUS, 1)
      CLOSE apmt480_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE apmt480_master_referesh USING g_pmdx_m.pmdxdocno INTO g_pmdx_m.pmdxdocno,g_pmdx_m.pmdx000, 
       g_pmdx_m.pmdxdocdt,g_pmdx_m.pmdx002,g_pmdx_m.pmdx004,g_pmdx_m.pmdx003,g_pmdx_m.pmdxstus,g_pmdx_m.pmdx005, 
       g_pmdx_m.pmdx006,g_pmdx_m.pmdx007,g_pmdx_m.pmdx008,g_pmdx_m.pmdx009,g_pmdx_m.pmdx011,g_pmdx_m.pmdx030, 
       g_pmdx_m.pmdx016,g_pmdx_m.pmdx017,g_pmdx_m.pmdx018,g_pmdx_m.pmdx019,g_pmdx_m.pmdx020,g_pmdx_m.pmdx010, 
       g_pmdx_m.pmdx012,g_pmdx_m.pmdx001,g_pmdx_m.pmdx014,g_pmdx_m.pmdx015,g_pmdx_m.pmdxsite,g_pmdx_m.pmdxownid, 
       g_pmdx_m.pmdxowndp,g_pmdx_m.pmdxcrtid,g_pmdx_m.pmdxcrtdp,g_pmdx_m.pmdxcrtdt,g_pmdx_m.pmdxmodid, 
       g_pmdx_m.pmdxmoddt,g_pmdx_m.pmdxcnfid,g_pmdx_m.pmdxcnfdt,g_pmdx_m.pmdx002_desc,g_pmdx_m.pmdx004_desc, 
       g_pmdx_m.pmdx003_desc,g_pmdx_m.pmdx005_desc,g_pmdx_m.pmdx009_desc,g_pmdx_m.pmdx011_desc,g_pmdx_m.pmdxownid_desc, 
       g_pmdx_m.pmdxowndp_desc,g_pmdx_m.pmdxcrtid_desc,g_pmdx_m.pmdxcrtdp_desc,g_pmdx_m.pmdxmodid_desc, 
       g_pmdx_m.pmdxcnfid_desc
 
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_pmdx_m.pmdxdocno,SQLCA.sqlcode,0)          #資料被他人LOCK
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:delete段before ask
   {<point name="delete.before_ask"/>}
   #end add-point 
 
   IF cl_ask_del_master() THEN              #確認一下
   
      #add-point:單頭刪除前
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point   
      
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL apmt480_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
  
      #資料備份
      LET g_pmdxdocno_t = g_pmdx_m.pmdxdocno
 
 
      DELETE FROM pmdx_t
       WHERE pmdxent = g_enterprise AND pmdxdocno = g_pmdx_m.pmdxdocno
 
       
      #add-point:單頭刪除中
      {<point name="delete.head.m_delete"/>}
      #end add-point
       
      IF SQLCA.sqlcode THEN
         CALL cl_err(g_pmdx_m.pmdxdocno,SQLCA.sqlcode,0) 
         CALL s_transaction_end('N','0')
         RETURN
      END IF
      
      #add-point:單頭刪除後
      {<point name="delete.head.a_delete"/>}
      #end add-point
  
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM pmdy_t
       WHERE pmdyent = g_enterprise AND pmdydocno = g_pmdx_m.pmdxdocno
 
 
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
         
      IF SQLCA.sqlcode THEN
         CALL cl_err("pmdy_t",SQLCA.sqlcode,0) 
         CALL s_transaction_end('N','0')
         RETURN
      END IF    
 
      #add-point:單身刪除後
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
            
                                                               
 
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete2" mark="Y"/>}
      #end add-point
      DELETE FROM pmdz_t
       WHERE pmdzent = g_enterprise AND
             pmdzdocno = g_pmdx_m.pmdxdocno
      #add-point:單身刪除中
      {<point name="delete.body.m_delete2"/>}
      #end add-point
      IF SQLCA.sqlcode THEN
         CALL cl_err("pmdz_t",SQLCA.sqlcode,0) 
         CALL s_transaction_end('N','0')
         RETURN
      END IF      
 
      #add-point:單身刪除後
      {<point name="delete.body.a_delete2"/>}
      #end add-point
 
 
                                                               
      CLEAR FORM
      CALL g_pmdy_d.clear() 
      CALL g_pmdy2_d.clear()       
      CALL g_pmdy3_d.clear()       
 
     
      #CALL apmt480_ui_browser_refresh()  
      CALL apmt480_ui_headershow()  
      CALL apmt480_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL apmt480_browser_fill("")
         CALL apmt480_fetch('P')
         DISPLAY g_browser_cnt TO FORMONLY.h_count   #總筆數的顯示
      ELSE
         LET g_wc = " 1=1"
         CALL apmt480_browser_fill("F")
      END IF
      
      #單頭多語言刪除
      
      
      #單身多語言刪除
      
 
      
 
 
   
      #add-point:多語言刪除
      {<point name="delete.lang.delete"/>}
      #end add-point
   
   END IF
 
   CALL s_transaction_end('Y','0')
   
   CLOSE apmt480_cl
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_pmdx_m.pmdxdocno,'D')
    
END FUNCTION
