  <point name="function.apmt480_change_pmdx011" order="20" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION apmt480_change_pmdx011(p_cmd)
DEFINE p_cmd             LIKE type_t.chr1
DEFINE l_i               LIKE type_t.num10
DEFINE l_success         LIKE type_t.num5
DEFINE l_pmdyseq         LIKE pmdy_t.pmdyseq
DEFINE l_pmdy002         LIKE pmdy_t.pmdy002
DEFINE l_pmdy009         LIKE pmdy_t.pmdy009
DEFINE l_pmdy010         LIKE pmdy_t.pmdy010
DEFINE l_pmdy011         LIKE pmdy_t.pmdy011
DEFINE l_pmdy012         LIKE pmdy_t.pmdy012
DEFINE l_pmdy017         LIKE pmdy_t.pmdy017
DEFINE l_pmdy018         LIKE pmdy_t.pmdy018
DEFINE l_pmdy019         LIKE pmdy_t.pmdy019

   #單頭新增時，因單身資料還沒insert到資料庫，所以只改變畫面上的顯示
   IF p_cmd = 'a' THEN
      FOR l_i = 1 TO g_pmdy_d.getLength()
         IF g_tax_app = '2' THEN
            #依料件設定
            CALL s_tax_chktype(g_pmdx_m.pmdxsite,g_pmdx_m.pmdx004,g_pmdy_d[l_i].pmdy002,
                               '1',g_pmab039)
                 RETURNING l_success,g_pmdy_d[l_i].pmdy011,g_pmdy_d[l_i].pmdy012
         ELSE
            #依正常稅率
            LET g_pmdy_d[l_i].pmdy011 = g_pmdx_m.pmdx006
            LET g_pmdy_d[l_i].pmdy012 = g_pmdx_m.pmdx007
         END IF

         #重計未稅金額、含稅金額、稅額
         CALL apmt480_get_amount(g_pmdy_d[l_i].pmdyseq,g_pmdy_d[l_i].pmdy009,
                                 g_pmdy_d[l_i].pmdy010,g_pmdy_d[l_i].pmdy011)
              RETURNING g_pmdy_d[l_i].pmdy017,g_pmdy_d[l_i].pmdy018,g_pmdy_d[l_i].pmdy019
      END FOR
   ELSE
      DECLARE sel_pmdy_cs CURSOR FOR
       SELECT pmdyseq,pmdy002,pmdy009,pmdy010
         FROM pmdy_t
        WHERE pmdyent = g_enterprise
          AND pmdydocno = g_pmdx_m.pmdxdocno
         
      FOREACH sel_pmdy_cs INTO l_pmdyseq,l_pmdy002,l_pmdy009,l_pmdy010
         IF g_tax_app = '2' THEN
            #依料件設定
            CALL s_tax_chktype(g_pmdx_m.pmdxsite,g_pmdx_m.pmdx004,l_pmdy002,
                               '1',g_pmab039)
                 RETURNING l_success,l_pmdy011,l_pmdy012
         ELSE
            #依正常稅率
            LET l_pmdy011 = g_pmdx_m.pmdx006
            LET l_pmdy012 = g_pmdx_m.pmdx007
         END IF

         #重計未稅金額、含稅金額、稅額
         CALL apmt480_get_amount(l_pmdyseq,l_pmdy009,
                                 l_pmdy010,l_pmdy011)
              RETURNING l_pmdy017,l_pmdy018,l_pmdy019

         UPDATE pmdy_t SET pmdy011 = l_pmdy011,
                           pmdy012 = l_pmdy012,
                           pmdy017 = l_pmdy017,
                           pmdy018 = l_pmdy018,
                           pmdy019 = l_pmdy019
          WHERE pmdyent = g_enterprise
            AND pmdydocno = g_pmdx_m.pmdxdocno
            AND pmdyseq = l_pmdyseq
         IF SQLCA.sqlcode THEN
            CALL cl_err("pmdy_t:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF            
      END FOREACH
      CALL apmt480_b_fill()
   END IF
END FUNCTION]]>
  </point>
  <point name="function.apmt480_get_amount" order="21" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION apmt480_get_amount(p_pmdyseq,p_pmdy009,p_pmdy010,p_pmdy011)
DEFINE p_pmdyseq         LIKE pmdy_t.pmdyseq
DEFINE p_pmdy009         LIKE pmdy_t.pmdy009
DEFINE p_pmdy010         LIKE pmdy_t.pmdy010
DEFINE p_pmdy011         LIKE pmdy_t.pmdy011
DEFINE r_pmdy017         LIKE pmdy_t.pmdy017
DEFINE r_pmdy018         LIKE pmdy_t.pmdy018
DEFINE r_pmdy019         LIKE pmdy_t.pmdy019
DEFINE l_money           LIKE pmdy_t.pmdy017
DEFINE l_xrcd113         LIKE xrcd_t.xrcd113
DEFINE l_xrcd114         LIKE xrcd_t.xrcd114
DEFINE l_xrcd115         LIKE xrcd_t.xrcd115
DEFINE l_xrcd123         LIKE xrcd_t.xrcd113
DEFINE l_xrcd124         LIKE xrcd_t.xrcd114
DEFINE l_xrcd125         LIKE xrcd_t.xrcd115
DEFINE l_xrcd133         LIKE xrcd_t.xrcd113
DEFINE l_xrcd134         LIKE xrcd_t.xrcd114
DEFINE l_xrcd135         LIKE xrcd_t.xrcd115

   LET r_pmdy017 = 0
   LET r_pmdy018 = 0
   LET r_pmdy019 = 0

   IF cl_null(p_pmdyseq) OR cl_null(p_pmdy009) OR
      cl_null(p_pmdy010) OR cl_null(p_pmdy011) THEN
      RETURN r_pmdy017,r_pmdy018,r_pmdy019
   END IF

   LET l_money = p_pmdy009 * p_pmdy010
