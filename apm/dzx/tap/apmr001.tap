  <point name="ui_dialog.more_construct" cite_std="N" status="u" ver="" src="s" new="Y">
<![CDATA[         CONSTRUCT BY NAME lc_param.wc ON pmdadocno,pmdadocdt,pmdb030,pmda002,pmda003,pmdb004,imaa009,imaf142
            
            #請購單號
            ON ACTION controlp INFIELD pmdadocno
               INITIALIZE g_qryparam.* TO NULL
               LET g_qryparam.state = 'c'
               LET g_qryparam.reqry = FALSE
               CALL q_pmdadocno()
               DISPLAY g_qryparam.return1 TO pmdadocno
               NEXT FIELD pmdadocno
            
            #請購人員
            ON ACTION controlp INFIELD pmda002
               INITIALIZE g_qryparam.* TO NULL
               LET g_qryparam.state = 'c'
               LET g_qryparam.reqry = FALSE
               CALL q_ooag001()    
               DISPLAY g_qryparam.return1 TO pmda002
               NEXT FIELD pmda002
             
            #請購部門 
            ON ACTION controlp INFIELD pmda003
               INITIALIZE g_qryparam.* TO NULL
               LET g_qryparam.state = 'c'
               LET g_qryparam.reqry = FALSE
               CALL q_ooeg001()
               DISPLAY g_qryparam.return1 TO pmda003
               NEXT FIELD pmda003
            
            #料件編號   
            ON ACTION controlp INFIELD pmdb004
               INITIALIZE g_qryparam.* TO NULL
               LET g_qryparam.state = 'c'
               LET g_qryparam.reqry = FALSE
               CALL q_imaa001() 
               DISPLAY g_qryparam.return1 TO pmdb004
               NEXT FIELD pmdb004       
            
            #產品分類   
            ON ACTION controlp INFIELD imaa009
               INITIALIZE g_qryparam.* TO NULL
               LET g_qryparam.state = 'c'
               LET g_qryparam.reqry = FALSE
               CALL q_rtax001() 
               DISPLAY g_qryparam.return1 TO imaa009
               NEXT FIELD imaa009  

            #採購人員
            ON ACTION controlp INFIELD imaf142
               INITIALIZE g_qryparam.* TO NULL
               LET g_qryparam.state = 'c'
               LET g_qryparam.reqry = FALSE
               CALL q_ooag001_2() 
               DISPLAY g_qryparam.return1 TO imaf142
               NEXT FIELD imaf142  
               
         END CONSTRUCT]]>
