  <point name="function.apmt490_pmeb002_other" cite_std="N" status="" ver="1" src="s" new="Y" order="32" mark_hard="N">
<![CDATA[################################################################################
# Descriptions...: 料件其他處理
# Memo...........:
# Usage..........: CALL apmt490_pmeb002_other(p_pmeb002)
#                  RETURNING r_pmeb004,r_pmeb008,r_pmeb011,r_pmeb012
# Input parameter: p_pmeb002      料件編號
#                : 
# Return code....: r_pmeb004      包裝容器
#                : r_pmeb008      單位
#                : r_pmeb011      稅別
#                : r_pmeb012      稅率
# Date & Author..: 2014/02/25 By stellar0130
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt490_pmeb002_other(p_pmeb002)
DEFINE p_pmeb002         LIKE pmeb_t.pmeb002
DEFINE r_pmeb004         LIKE pmeb_t.pmeb004
DEFINE r_pmeb008         LIKE pmeb_t.pmeb008
DEFINE r_pmeb011         LIKE pmeb_t.pmeb011
DEFINE r_pmeb012         LIKE pmeb_t.pmeb012
DEFINE l_imaf144         LIKE imaf_t.imaf144
DEFINE l_success         LIKE type_t.num5

   LET r_pmeb004 = ''
   LET r_pmeb008 = ''
   LET r_pmeb011 = ''
   LET r_pmeb012 = ''
   
   IF cl_null(p_pmeb002) THEN
      RETURN r_pmeb004,r_pmeb008,r_pmeb011,r_pmeb012
   END IF
   
   #帶出採購單位、慣用包裝容器
   SELECT imaf143,imaf157,imaf144 
     INTO r_pmeb008,r_pmeb004,l_imaf144
     FROM imaf_t
    WHERE imafent = g_enterprise
      AND imafsite= g_pmea_m.pmeasite
      AND imaf001 = p_pmeb002
      
   #若採購計價單位不為NULL，則採購合約單位=採購計價單位
   IF NOT cl_null(l_imaf144) THEN
      LET r_pmeb008 = l_imaf144
   END IF
   
   #若稅別應用為2.依料件設定，呼叫應用元件帶出稅別、稅率
   IF g_tax_app = '2' THEN
      CALL s_tax_chktype(g_pmea_m.pmeasite,g_pmea_m.pmea004,p_pmeb002,
                         '2',g_pmab039)
           RETURNING l_success,r_pmeb011,r_pmeb012
   ELSE
      LET r_pmeb011 = g_pmea_m.pmea006
      LET r_pmeb012 = g_pmea_m.pmea007
   END IF
   
   RETURN r_pmeb004,r_pmeb008,r_pmeb011,r_pmeb012
END FUNCTION]]>
