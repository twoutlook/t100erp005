  <section id="apmt490.modify" ver="12" status="" src="s">
<![CDATA[#+ 資料修改
PRIVATE FUNCTION apmt490_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_pmea_m.pmeadocno IS NULL
   OR g_pmea_m.pmea900 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE apmt490_master_referesh USING g_pmea_m.pmeadocno,g_pmea_m.pmea900 INTO g_pmea_m.pmea001,g_pmea_m.pmea901, 
       g_pmea_m.pmeadocno,g_pmea_m.pmea900,g_pmea_m.pmeasite,g_pmea_m.pmea902,g_pmea_m.pmea000,g_pmea_m.pmeadocdt, 
       g_pmea_m.pmea002,g_pmea_m.pmea003,g_pmea_m.pmeastus,g_pmea_m.pmea004,g_pmea_m.pmeaacti,g_pmea_m.pmea005, 
       g_pmea_m.pmea006,g_pmea_m.pmea007,g_pmea_m.pmea008,g_pmea_m.pmea009,g_pmea_m.pmea011,g_pmea_m.pmea030, 
       g_pmea_m.pmea016,g_pmea_m.pmea017,g_pmea_m.pmea018,g_pmea_m.pmea019,g_pmea_m.pmea020,g_pmea_m.pmea010, 
       g_pmea_m.pmea012,g_pmea_m.pmea014,g_pmea_m.pmea015,g_pmea_m.pmea903,g_pmea_m.pmea904,g_pmea_m.pmeaownid, 
       g_pmea_m.pmeaowndp,g_pmea_m.pmeacrtid,g_pmea_m.pmeacrtdp,g_pmea_m.pmeacrtdt,g_pmea_m.pmeamodid, 
       g_pmea_m.pmeamoddt,g_pmea_m.pmeacnfid,g_pmea_m.pmeacnfdt,g_pmea_m.pmea002_desc,g_pmea_m.pmea003_desc, 
       g_pmea_m.pmea004_desc,g_pmea_m.pmea005_desc,g_pmea_m.pmea009_desc,g_pmea_m.pmea011_desc,g_pmea_m.pmeaownid_desc, 
       g_pmea_m.pmeaowndp_desc,g_pmea_m.pmeacrtid_desc,g_pmea_m.pmeacrtdp_desc,g_pmea_m.pmeamodid_desc, 
       g_pmea_m.pmeacnfid_desc
 
   ERROR ""
  
   LET g_pmeadocno_t = g_pmea_m.pmeadocno
   LET g_pmea900_t = g_pmea_m.pmea900
 
   CALL s_transaction_begin()
   
   OPEN apmt490_cl USING g_enterprise,g_pmea_m.pmeadocno,g_pmea_m.pmea900
   IF STATUS THEN
      CALL cl_err("OPEN apmt490_cl:", STATUS, 1)
      CLOSE apmt490_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE apmt490_master_referesh USING g_pmea_m.pmeadocno,g_pmea_m.pmea900 INTO g_pmea_m.pmea001,g_pmea_m.pmea901, 
       g_pmea_m.pmeadocno,g_pmea_m.pmea900,g_pmea_m.pmeasite,g_pmea_m.pmea902,g_pmea_m.pmea000,g_pmea_m.pmeadocdt, 
       g_pmea_m.pmea002,g_pmea_m.pmea003,g_pmea_m.pmeastus,g_pmea_m.pmea004,g_pmea_m.pmeaacti,g_pmea_m.pmea005, 
       g_pmea_m.pmea006,g_pmea_m.pmea007,g_pmea_m.pmea008,g_pmea_m.pmea009,g_pmea_m.pmea011,g_pmea_m.pmea030, 
       g_pmea_m.pmea016,g_pmea_m.pmea017,g_pmea_m.pmea018,g_pmea_m.pmea019,g_pmea_m.pmea020,g_pmea_m.pmea010, 
       g_pmea_m.pmea012,g_pmea_m.pmea014,g_pmea_m.pmea015,g_pmea_m.pmea903,g_pmea_m.pmea904,g_pmea_m.pmeaownid, 
       g_pmea_m.pmeaowndp,g_pmea_m.pmeacrtid,g_pmea_m.pmeacrtdp,g_pmea_m.pmeacrtdt,g_pmea_m.pmeamodid, 
       g_pmea_m.pmeamoddt,g_pmea_m.pmeacnfid,g_pmea_m.pmeacnfdt,g_pmea_m.pmea002_desc,g_pmea_m.pmea003_desc, 
       g_pmea_m.pmea004_desc,g_pmea_m.pmea005_desc,g_pmea_m.pmea009_desc,g_pmea_m.pmea011_desc,g_pmea_m.pmeaownid_desc, 
       g_pmea_m.pmeaowndp_desc,g_pmea_m.pmeacrtid_desc,g_pmea_m.pmeacrtdp_desc,g_pmea_m.pmeamodid_desc, 
       g_pmea_m.pmeacnfid_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_pmea_m.pmeadocno,SQLCA.sqlcode,0)
      CLOSE apmt490_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   CALL apmt490_show()
   WHILE TRUE
      LET g_pmeadocno_t = g_pmea_m.pmeadocno
      LET g_pmea900_t = g_pmea_m.pmea900
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_pmea_m.pmeamodid = g_user 
LET g_pmea_m.pmeamoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      CALL apmt490_input("u")
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_pmea_m.* = g_pmea_m_t.*
         CALL apmt490_show()
         CALL cl_err('',9001,0)
         CALL s_transaction_end('N','0')
         EXIT WHILE
      END IF 
      
      #若有modid跟moddt則進行update
      UPDATE pmea_t SET (pmeamodid,pmeamoddt) = (g_pmea_m.pmeamodid,g_pmea_m.pmeamoddt)
       WHERE pmeaent = g_enterprise AND pmeadocno = g_pmeadocno_t
         AND pmea900 = g_pmea900_t
 
                  
      #若單頭key欄位有變更
      IF g_pmea_m.pmeadocno != g_pmeadocno_t 
      OR g_pmea_m.pmea900 != g_pmea900_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE pmeb_t SET pmebdocno = g_pmea_m.pmeadocno
                                       ,pmeb900 = g_pmea_m.pmea900
 
          WHERE pmebent = g_enterprise AND pmebdocno = g_pmeadocno_t
            AND pmeb900 = g_pmea900_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("pmeb_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("pmeb_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
         #更新單身key值
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update2" mark="Y"/>}
         #end add-point
         UPDATE pmec_t
            SET pmecdocno = g_pmea_m.pmeadocno
               ,pmec900 = g_pmea_m.pmea900
 
          WHERE pmecent = g_enterprise AND
