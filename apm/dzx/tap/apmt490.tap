      cl_null(p_pmeb010) OR cl_null(p_pmeb011) THEN
      RETURN r_pmeb017,r_pmeb018,r_pmeb019
   END IF
   
   LET l_money = p_pmeb009 * p_pmeb010
   CALL s_tax_ins(g_pmea_m.pmeadocno,p_pmebseq,0,g_pmea_m.pmeasite,l_money,p_pmeb011,
                  p_pmeb009,g_pmea_m.pmea005,1,'','','')
        RETURNING r_pmeb017,r_pmeb019,r_pmeb018,l_xrcd113,l_xrcd114,l_xrcd115,
