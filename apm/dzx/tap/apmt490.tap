  <section id="apmt490.ui_dialog" ver="73" status="" src="s">
<![CDATA[#+ 功能選單
PRIVATE FUNCTION apmt490_ui_dialog()
   DEFINE li_idx    LIKE type_t.num5
   DEFINE ls_wc     STRING
