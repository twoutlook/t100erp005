  <point name="function.apmt490_detail_color" cite_std="N" status="" ver="1" src="s" new="Y" order="39" mark_hard="N">
<![CDATA[################################################################################
# Descriptions...: 單身欄位的顯示顏色
# Memo...........:
# Usage..........: CALL apmt490_detail_color()
#                  
# Input parameter: 
#                : 
# Return code....: 
#                : 
# Date & Author..: 2014/03/03 By stellar0130
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt490_detail_color()
DEFINE l_pmed002         LIKE pmed_t.pmed002

   CALL apmt490_detail_init_color()
   
   DECLARE sel_pmed_cs CURSOR FOR
    SELECT pmed002 FROM pmed_t
     WHERE pmedent = g_enterprise
       AND pmeddocno = g_pmea_m.pmeadocno
       AND pmed001 = g_pmea_m.pmea900
       AND pmedseq = g_pmeb_d[l_ac].pmebseq
       AND pmedseq1= 0
   FOREACH sel_pmed_cs INTO l_pmed002
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
      
      LET l_pmed002 = cl_replace_str(l_pmed002,'pmdy','pmeb')
      
      CASE l_pmed002
         WHEN 'pmebseq'
              LET g_pmeb_d_color[l_ac].pmebseq = 'red'
