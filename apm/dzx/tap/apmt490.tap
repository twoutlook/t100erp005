  <point name="statechange.b_update" cite_std="N" status="" ver="1" src="s" new="N" mark_hard="N">
<![CDATA[   CALL s_transaction_begin()
   IF lc_state = 'Y' THEN
      CALL s_apmt490_conf_chk(g_pmea_m.pmeadocno,g_pmea_m.pmea900) 
           RETURNING l_success
      IF NOT l_success THEN
         RETURN
      ELSE
         IF NOT cl_ask_confirm('aim-00108') THEN
            RETURN
         ELSE
            CALL s_apmt490_conf_upd(g_pmea_m.pmeadocno,g_pmea_m.pmea900) 
                 RETURNING l_success
            IF NOT l_success THEN
               CALL s_transaction_end('N','0')
               RETURN
            ELSE
               CALL s_transaction_end('Y','0')
            END IF
         END IF
      END IF
   END IF

   IF lc_state = 'X' THEN
      CALL s_apmt490_invalid_chk(g_pmea_m.pmeadocno,g_pmea_m.pmea900)
           RETURNING l_success
      IF NOT l_success THEN
         RETURN
      ELSE
         IF NOT cl_ask_confirm('aim-00109') THEN
            RETURN
         ELSE
            CALL s_apmt490_invalid_upd(g_pmea_m.pmeadocno,g_pmea_m.pmea900) 
                 RETURNING l_success 
            IF NOT l_success THEN
               CALL s_transaction_end('N','0')
               RETURN
            ELSE
               CALL s_transaction_end('Y','0')
            END IF
         END IF
      END IF
   END IF]]>
</point>
