  <section id="apmt490.update_b" ver="5" status="" src="s">
<![CDATA[#+ 修改單身後其他table連動
PRIVATE FUNCTION apmt490_update_b(ps_table,ps_keys,ps_keys_bak,ps_page)
   DEFINE ps_table         STRING
   DEFINE ps_page          STRING
   DEFINE ps_keys          DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ps_keys_bak      DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ls_group         STRING
   DEFINE li_idx           LIKE type_t.num5 
   DEFINE lb_chk           BOOLEAN
   DEFINE l_new_key        DYNAMIC ARRAY OF STRING
   DEFINE l_old_key        DYNAMIC ARRAY OF STRING
   DEFINE l_field_key      DYNAMIC ARRAY OF STRING
   #add-point:update_b段define
   {<point name="update_b.define"/>}
   #end add-point     
   
   #判斷key是否有改變
   LET lb_chk = TRUE
   FOR li_idx = 1 TO ps_keys.getLength()
      IF ps_keys[li_idx] <> ps_keys_bak[li_idx] THEN
         LET lb_chk = FALSE
         EXIT FOR
      END IF
   END FOR
   
   #不需要做處理
   IF lb_chk THEN
      RETURN
   END IF
   
   #判斷是否是同一群組的table
   LET ls_group = "'1','2',"
   IF ls_group.getIndexOf(ps_page,1) > 0 AND ps_table <> "pmeb_t" THEN
      #add-point:update_b段修改前
      {<point name="update_b.before_update" mark="Y"/>}
      #end add-point     
      UPDATE pmeb_t 
         SET (pmebdocno,pmeb900,
              pmebseq
              ,pmeb001,pmebsite,pmeb002,pmeb003,pmeb005,pmeb006,pmeb007,pmeb014,pmeb008,pmeb009,pmeb010,pmeb024,pmeb011,pmeb012,pmeb017,pmeb018,pmeb019,pmeb013,pmeb004,pmeb030,pmeb901,pmeb902,pmeb903,pmeb020,pmeb021,pmeb022,pmeb023) 
              = 
             (ps_keys[1],ps_keys[2],ps_keys[3]
              ,g_pmeb_d[g_detail_idx].pmeb001,g_pmeb_d[g_detail_idx].pmebsite,g_pmeb_d[g_detail_idx].pmeb002, 
                  g_pmeb_d[g_detail_idx].pmeb003,g_pmeb_d[g_detail_idx].pmeb005,g_pmeb_d[g_detail_idx].pmeb006, 
                  g_pmeb_d[g_detail_idx].pmeb007,g_pmeb_d[g_detail_idx].pmeb014,g_pmeb_d[g_detail_idx].pmeb008, 
                  g_pmeb_d[g_detail_idx].pmeb009,g_pmeb_d[g_detail_idx].pmeb010,g_pmeb_d[g_detail_idx].pmeb024, 
                  g_pmeb_d[g_detail_idx].pmeb011,g_pmeb_d[g_detail_idx].pmeb012,g_pmeb_d[g_detail_idx].pmeb017, 
                  g_pmeb_d[g_detail_idx].pmeb018,g_pmeb_d[g_detail_idx].pmeb019,g_pmeb_d[g_detail_idx].pmeb013, 
                  g_pmeb_d[g_detail_idx].pmeb004,g_pmeb_d[g_detail_idx].pmeb030,g_pmeb_d[g_detail_idx].pmeb901, 
                  g_pmeb_d[g_detail_idx].pmeb902,g_pmeb_d[g_detail_idx].pmeb903,g_pmeb2_d[g_detail_idx].pmeb020, 
                  g_pmeb2_d[g_detail_idx].pmeb021,g_pmeb2_d[g_detail_idx].pmeb022,g_pmeb2_d[g_detail_idx].pmeb023)  

         WHERE pmebent = g_enterprise AND pmebdocno = ps_keys_bak[1] AND pmeb900 = ps_keys_bak[2] AND pmebseq = ps_keys_bak[3]
      #add-point:update_b段修改中
      {<point name="update_b.m_update"/>}
      #end add-point   
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            CALL cl_err("pmeb_t","std-00009",1)
            CALL s_transaction_end('N','0')
         WHEN SQLCA.sqlcode #其他錯誤
            CALL cl_err("pmeb_t",SQLCA.sqlcode,1)  
            CALL s_transaction_end('N','0')
         OTHERWISE
            
      END CASE
      #add-point:update_b段修改後
      {<point name="update_b.after_update"/>}
      #end add-point  
   END IF
   
 
   
   LET ls_group = "'3',"
   #判斷是否是同一群組的table
   IF ls_group.getIndexOf(ps_page,1) > 0 AND ps_table <> "pmec_t" THEN
      #add-point:update_b段修改前
      {<point name="update_b.before_update2" mark="Y"/>}
      #end add-point     
      UPDATE pmec_t 
         SET (pmecdocno,pmec900,pmecseq,
              pmecseq1
              ,pmec001,pmec002,pmec003,pmec004,pmec005,pmec902,pmec903) 
              = 
             (ps_keys[1],ps_keys[2],ps_keys[3],ps_keys[4]
              ,g_pmeb3_d[g_detail_idx2].pmec001,g_pmeb3_d[g_detail_idx2].pmec002,g_pmeb3_d[g_detail_idx2].pmec003, 
                  g_pmeb3_d[g_detail_idx2].pmec004,g_pmeb3_d[g_detail_idx2].pmec005,g_pmeb3_d[g_detail_idx2].pmec902, 
                  g_pmeb3_d[g_detail_idx2].pmec903) 
         WHERE pmecent = g_enterprise AND pmecdocno = ps_keys_bak[1] AND pmec900 = ps_keys_bak[2] AND pmecseq = ps_keys_bak[3] AND pmecseq1 = ps_keys_bak[4]
      #add-point:update_b段修改中
      {<point name="update_b.m_update2"/>}
      #end add-point  
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            CALL cl_err("pmec_t","std-00009",1)
            CALL s_transaction_end('N','0')
         WHEN SQLCA.sqlcode #其他錯誤
            CALL cl_err("pmec_t",SQLCA.sqlcode,1)  
            CALL s_transaction_end('N','0')
         OTHERWISE
            
      END CASE
      #add-point:update_b段修改後
      {<point name="update_b.after_update2"/>}
      #end add-point  
   END IF
 
 
   
   LET ls_group = "'1','2',"
   #判斷是否是同一群組的table
   IF ls_group.getIndexOf(ps_page,1) > 0 AND ps_table <> "pmec_t" THEN
      #add-point:update_b段修改前
      {<point name="update_b.key.before_update2" mark="Y"/>}
      #end add-point     
      UPDATE pmec_t 
         SET (pmecdocno,pmec900,pmecseq) 
              = 
             (ps_keys[1],ps_keys[2],ps_keys[3]) 
         WHERE pmecent = g_enterprise AND pmecdocno = ps_keys_bak[1] AND pmec900 = ps_keys_bak[2] AND pmecseq = ps_keys_bak[3]
      #add-point:update_b段修改中
      {<point name="update_b.key.m_update2"/>}
      #end add-point
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            CALL cl_err("pmec_t","std-00009",1)
            CALL s_transaction_end('N','0')
         WHEN SQLCA.sqlcode #其他錯誤
            CALL cl_err("pmec_t",SQLCA.sqlcode,1)  
            CALL s_transaction_end('N','0')
         OTHERWISE
            
      END CASE
      #add-point:update_b段修改後
      {<point name="update_b.key.after_update2"/>}
      #end add-point  
   END IF
 
 
   
   #add-point:update_b段other
   {<point name="update_b.other"/>}
   #end add-point  
   
END FUNCTION
]]>
