  <section id="apmt490.b_fill" ver="16" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION apmt490_b_fill()
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_pmeb_d.clear()    #g_pmeb_d 單頭及單身 
   CALL g_pmeb2_d.clear()
   CALL g_pmeb3_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF apmt490_fill_chk(1) THEN
   
      LET g_sql = "SELECT  UNIQUE pmeb001,pmebsite,pmebseq,pmeb002,pmeb003,pmeb005,pmeb006,pmeb007,pmeb014, 
          pmeb008,pmeb009,pmeb010,pmeb024,pmeb011,pmeb012,pmeb017,pmeb018,pmeb019,pmeb013,pmeb004,pmeb030, 
          pmeb901,pmeb902,pmeb903,pmebseq,pmeb020,pmeb021,pmeb022,pmeb023 ,t1.imaal003 ,t2.imaal004 , 
          t3.oocql004 ,t4.oocal003 ,t5.oocql004 ,t6.imaal003 ,t7.imaal003 ,t8.imaal004 ,t9.oocql004 , 
          t10.oocal003 FROM pmeb_t",   
                  " INNER JOIN pmea_t ON pmeadocno = pmebdocno ",
                  " AND pmea900 = pmeb900 ",
 
                  #"",
                  " LEFT JOIN pmec_t ON pmebent = pmecent AND pmebdocno = pmecdocno AND pmeb900 = pmec900 AND pmebseq = pmecseq ",
                  "",
                                 " LEFT JOIN imaal_t t1 ON t1.imaalent='"||g_enterprise||"' AND t1.imaal001=pmeb002 AND t1.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t2 ON t2.imaalent='"||g_enterprise||"' AND t2.imaal001=pmeb002 AND t2.imaal002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t3 ON t3.oocqlent='"||g_enterprise||"' AND t3.oocql001='221' AND t3.oocql002=pmeb006 AND t3.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t4 ON t4.oocalent='"||g_enterprise||"' AND t4.oocal001=pmeb008 AND t4.oocal002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t5 ON t5.oocqlent='"||g_enterprise||"' AND t5.oocql001='263' AND t5.oocql002=pmeb013 AND t5.oocql003='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t6 ON t6.imaalent='"||g_enterprise||"' AND t6.imaal001=pmeb004 AND t6.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t7 ON t7.imaalent='"||g_enterprise||"' AND t7.imaal001=pmeb002 AND t7.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t8 ON t8.imaalent='"||g_enterprise||"' AND t8.imaal001=pmeb002 AND t8.imaal002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t9 ON t9.oocqlent='"||g_enterprise||"' AND t9.oocql001='221' AND t9.oocql002=pmeb006 AND t9.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t10 ON t10.oocalent='"||g_enterprise||"' AND t10.oocal001=pmeb008 AND t10.oocal002='"||g_dlang||"' ",
 
                  " WHERE pmebent=? AND pmebdocno=? AND pmeb900=?"
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      #add-point:b_fill段sql_before
      {<point name="b_fill.body.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table1) THEN
         LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
      END IF
      
      #子單身的WC
         IF NOT cl_null(g_wc2_table2) THEN 
      LET g_sql = g_sql CLIPPED," AND ", g_wc2_table2 CLIPPED
   END IF 
 
      
      LET g_sql = g_sql, " ORDER BY pmeb_t.pmebseq"
      
      #add-point:單身填充控制
      {<point name="b_fill.sql"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE apmt490_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR apmt490_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_pmea_m.pmeadocno,g_pmea_m.pmea900
                                               
      FOREACH b_fill_cs INTO g_pmeb_d[l_ac].pmeb001,g_pmeb_d[l_ac].pmebsite,g_pmeb_d[l_ac].pmebseq,g_pmeb_d[l_ac].pmeb002, 
          g_pmeb_d[l_ac].pmeb003,g_pmeb_d[l_ac].pmeb005,g_pmeb_d[l_ac].pmeb006,g_pmeb_d[l_ac].pmeb007, 
          g_pmeb_d[l_ac].pmeb014,g_pmeb_d[l_ac].pmeb008,g_pmeb_d[l_ac].pmeb009,g_pmeb_d[l_ac].pmeb010, 
          g_pmeb_d[l_ac].pmeb024,g_pmeb_d[l_ac].pmeb011,g_pmeb_d[l_ac].pmeb012,g_pmeb_d[l_ac].pmeb017, 
          g_pmeb_d[l_ac].pmeb018,g_pmeb_d[l_ac].pmeb019,g_pmeb_d[l_ac].pmeb013,g_pmeb_d[l_ac].pmeb004, 
          g_pmeb_d[l_ac].pmeb030,g_pmeb_d[l_ac].pmeb901,g_pmeb_d[l_ac].pmeb902,g_pmeb_d[l_ac].pmeb903, 
          g_pmeb2_d[l_ac].pmebseq,g_pmeb2_d[l_ac].pmeb020,g_pmeb2_d[l_ac].pmeb021,g_pmeb2_d[l_ac].pmeb022, 
          g_pmeb2_d[l_ac].pmeb023,g_pmeb_d[l_ac].pmeb002_desc,g_pmeb_d[l_ac].pmeb002_desc_desc,g_pmeb_d[l_ac].pmeb006_desc, 
          g_pmeb_d[l_ac].pmeb008_desc,g_pmeb_d[l_ac].pmeb013_desc,g_pmeb_d[l_ac].pmeb004_desc,g_pmeb2_d[l_ac].pmeb0022_desc, 
          g_pmeb2_d[l_ac].pmeb0022_desc_desc,g_pmeb2_d[l_ac].pmeb0062_desc,g_pmeb2_d[l_ac].pmeb0082_desc 

         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               CALL cl_err( '', 9035, 1)
            END IF
            EXIT FOREACH
         END IF
         
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
 
   
   #add-point:browser_fill段其他table處理
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_pmeb_d.deleteElement(g_pmeb_d.getLength())
   CALL g_pmeb2_d.deleteElement(g_pmeb2_d.getLength())
   CALL g_pmeb3_d.deleteElement(g_pmeb3_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE apmt490_pb
 
   
END FUNCTION
]]>
</section>
  <section id="apmt490.b_fill2" ver="5" status="" src="s">
<![CDATA[#+ 單身陣列填充2
PRIVATE FUNCTION apmt490_b_fill2(pi_idx)
   DEFINE pi_idx          LIKE type_t.num5
   DEFINE li_ac           LIKE type_t.num5
   DEFINE ls_chk          LIKE type_t.chr1
   #add-point:b_fill2段define
   {<point name="b_fill2.define"/>}
   #end add-point
   
   LET li_ac = l_ac 
   
   IF g_detail_idx <= 0 THEN
      RETURN
   END IF
   
   IF apmt490_fill_chk(2) THEN
      IF (pi_idx = 2 OR pi_idx = 0 ) AND g_pmeb_d.getLength() > 0 THEN
               CALL g_pmeb3_d.clear()
 
         LET g_sql = "SELECT  UNIQUE pmecseq1,pmec001,pmec002,pmec003,pmec004,pmec005,pmec902,pmec903  FROM pmec_t", 
                 
                     "",
                     
                     " WHERE pmecent=? AND pmecdocno=? AND pmec900=? AND pmecseq=?"
         
         IF NOT cl_null(g_wc2_table2) THEN
            LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
         END IF
         
         LET g_sql = g_sql, " ORDER BY  pmec_t.pmecseq1" 
                            
         #add-point:單身填充前
         {<point name="b_fill2.before_fill2" />}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE apmt490_pb2 FROM g_sql
         DECLARE b_fill_curs2 CURSOR FOR apmt490_pb2
         
         OPEN b_fill_curs2 USING g_enterprise,
                                        g_pmea_m.pmeadocno,g_pmea_m.pmea900,g_pmeb_d[g_detail_idx].pmebseq 

         LET l_ac = 1
         FOREACH b_fill_curs2 INTO g_pmeb3_d[l_ac].pmecseq1,g_pmeb3_d[l_ac].pmec001,g_pmeb3_d[l_ac].pmec002, 
             g_pmeb3_d[l_ac].pmec003,g_pmeb3_d[l_ac].pmec004,g_pmeb3_d[l_ac].pmec005,g_pmeb3_d[l_ac].pmec902, 
             g_pmeb3_d[l_ac].pmec903 
            IF SQLCA.sqlcode THEN
               CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
               EXIT FOREACH
            END IF
            
            #add-point:b_fill段資料填充
            {<point name="b_fill2.fill2"/>}
            #end add-point
           
            LET l_ac = l_ac + 1
            IF l_ac > g_max_rec THEN
               CALL cl_err( '', 9035, 0 )
               EXIT FOREACH
            END IF
            
         END FOREACH
               CALL g_pmeb3_d.deleteElement(g_pmeb3_d.getLength())
 
      END IF
   END IF
 
 
      
   #add-point:單身填充後
   {<point name="b_fill2.after_fill" />}
   #end add-point
    
   LET l_ac = li_ac
   
   CALL apmt490_detail_show()
   
END FUNCTION
]]>
</section>
  <section id="apmt490.browser_fill" ver="12" status="" src="s">
<![CDATA[#+ 瀏覽頁簽資料填充
PRIVATE FUNCTION apmt490_browser_fill(ps_page_action)
   DEFINE ps_page_action    STRING
   DEFINE l_wc              STRING
   DEFINE l_wc2             STRING
   DEFINE l_sql             STRING
   DEFINE l_sub_sql         STRING
   DEFINE l_sql_rank        STRING
   DEFINE l_searchcol       STRING
   #add-point:browser_fill段define
   {<point name="browser_fill.define"/>}
   #end add-point    
 
   #清除畫面
   CLEAR FORM                
   INITIALIZE g_pmea_m.* TO NULL
   CALL g_pmeb_d.clear()        
   CALL g_pmeb2_d.clear() 
   CALL g_pmeb3_d.clear() 
 
   CALL g_browser.clear()
   
   #搜尋用
   IF cl_null(g_searchcol) OR g_searchcol = '0' THEN
      LET l_searchcol = "pmeadocno,pmea900"
   ELSE
      LET l_searchcol = g_searchcol
   END IF
   
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   LET l_wc  = g_wc.trim() 
   LET l_wc2 = g_wc2.trim()
 
   #add-point:browser_fill,foreach前
   {<point name="browser_fill.before_foreach"/>}
   #end add-point
    
   IF g_wc2 <> " 1=1" THEN
      #單身有輸入搜尋條件                      
      LET l_sub_sql = " SELECT UNIQUE pmeadocno,pmea900 ",
                      " FROM pmea_t ",
                      " ",
                      " LEFT JOIN pmeb_t ON pmebent = pmeaent AND pmeadocno = pmebdocno AND pmea900 = pmeb900 ",
 
                      " LEFT JOIN pmec_t ON pmecent = pmeaent AND pmebdocno = pmecdocno AND pmeb900 = pmec900 AND pmebseq = pmecseq", 
 
 
                      " ", 
                      " ", 
                      " WHERE pmeaent = '" ||g_enterprise|| "' AND pmebent = '" ||g_enterprise|| "' AND ",l_wc, " AND ", l_wc2, cl_sql_add_filter("pmea_t")
   ELSE
      #單身未輸入搜尋條件
      LET l_sub_sql = " SELECT UNIQUE pmeadocno,pmea900 ",
                      " FROM pmea_t ", 
                      "  ",
                      "  ",
                      " WHERE pmeaent = '" ||g_enterprise|| "' AND ",l_wc CLIPPED, cl_sql_add_filter("pmea_t")
   END IF
   
   LET g_sql = " SELECT COUNT(*) FROM (",l_sub_sql,")"
   
   #add-point:browser_fill,count前
   {<point name="browser_fill.before_count"/>}
   #end add-point
   
   PREPARE header_cnt_pre FROM g_sql
   EXECUTE header_cnt_pre INTO g_browser_cnt   #總筆數
   FREE header_cnt_pre
 
   DISPLAY g_browser_cnt TO FORMONLY.b_count   #總筆數的顯示
   DISPLAY g_browser_cnt TO FORMONLY.h_count   #總筆數的顯示
 
   #依照pmeadocno,pmea900,pmea902,pmea000,pmea004,'',pmea002,'',pmea003,'' Browser欄位定義(取代原本bs_sql功能)
   LET g_sql = " SELECT DISTINCT pmeastus,pmeadocno,pmea900,pmea902,pmea000,pmea004,'',pmea002,'',pmea003, 
       '',t1.pmaal004 ,t2.oofa011 ,t3.ooefl003 ",
               " FROM pmea_t ",
               "  ",
               "  LEFT JOIN pmeb_t ON pmebent = pmeaent AND pmeadocno = pmebdocno AND pmea900 = pmeb900 ",
 
               "  LEFT JOIN pmec_t ON pmecent = pmeaent AND pmebdocno = pmecdocno AND pmeb900 = pmec900 AND pmebseq = pmecseq",
 
 
               "  ",
               "  ",
                              " LEFT JOIN pmaal_t t1 ON t1.pmaalent='"||g_enterprise||"' AND t1.pmaal001=pmea004 AND t1.pmaal002='"||g_lang||"' ",
               " LEFT JOIN oofa_t t2 ON t2.oofaent='"||g_enterprise||"' AND t2.oofa002='2' AND t2.oofa003=pmea002  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent='"||g_enterprise||"' AND t3.ooefl001=pmea003 AND t3.ooefl002='"||g_lang||"' ",
 
               " WHERE pmeaent = '" ||g_enterprise|| "' AND ",l_wc," AND ",l_wc2, cl_sql_add_filter("pmea_t"),
               " ORDER BY ",l_searchcol," ",g_order
 
   #add-point:browser_fill,before_prepare
   {<point name="browser_fill.before_prepare"/>}
   #end add-point
        
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE browse_pre FROM g_sql
   DECLARE browse_cur CURSOR FOR browse_pre
   
   #add-point:browser_fill,open
   {<point name="browser_fill.open"/>}
   #end add-point
 
   CALL g_browser.clear()
   LET g_cnt = 1
   FOREACH browse_cur INTO g_browser[g_cnt].b_statepic,g_browser[g_cnt].b_pmeadocno,g_browser[g_cnt].b_pmea900, 
       g_browser[g_cnt].b_pmea902,g_browser[g_cnt].b_pmea000,g_browser[g_cnt].b_pmea004,g_browser[g_cnt].b_pmea004_desc, 
       g_browser[g_cnt].b_pmea002,g_browser[g_cnt].b_pmea002_desc,g_browser[g_cnt].b_pmea003,g_browser[g_cnt].b_pmea003_desc, 
       g_browser[g_cnt].b_pmea004_desc,g_browser[g_cnt].b_pmea002_desc,g_browser[g_cnt].b_pmea003_desc 

      IF SQLCA.sqlcode THEN
         CALL cl_err('foreach:',SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:browser_fill段reference
      {<point name="browser_fill.reference"/>}
      #end add-point
  
            #此段落由子樣板a24產生
      CASE g_browser[g_cnt].b_statepic
         WHEN "N"
            LET g_browser[g_cnt].b_statepic = "stus/16/unconfirmed.png"
         WHEN "X"
            LET g_browser[g_cnt].b_statepic = "stus/16/invalid.png"
         WHEN "Y"
            LET g_browser[g_cnt].b_statepic = "stus/16/confirmed.png"
         WHEN "A"
            LET g_browser[g_cnt].b_statepic = "stus/16/approved.png"
         WHEN "D"
            LET g_browser[g_cnt].b_statepic = "stus/16/withdraw.png"
         WHEN "R"
            LET g_browser[g_cnt].b_statepic = "stus/16/rejection.png"
         WHEN "W"
            LET g_browser[g_cnt].b_statepic = "stus/16/signing.png"
         
      END CASE
 
 
      LET g_cnt = g_cnt + 1
      IF g_cnt > g_max_rec THEN
         CALL cl_err('',9035,0)
         EXIT FOREACH
      END IF
      
   END FOREACH
 
   CALL g_browser.deleteElement(g_cnt)
   LET g_header_cnt = g_browser.getLength()
 
   LET g_rec_b = g_cnt - 1
   LET g_detail_cnt = g_rec_b
   LET g_cnt = 0
   
   FREE browse_pre
   
   #add-point:browser_fill段結束前
   {<point name="browser_fill.after"/>}
   #end add-point
   
   #若無資料則關閉相關功能
   IF g_browser_cnt = 0 THEN
      CALL cl_set_act_visible("statechange,modify,delete,reproduce", FALSE)
   ELSE
      CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   END IF
   
END FUNCTION
]]>
</section>
  <section id="apmt490.browser_search" ver="3" status="" src="s">
<![CDATA[#+ 瀏覽頁簽資料搜尋
PRIVATE FUNCTION apmt490_browser_search(p_type)
   DEFINE p_type LIKE type_t.chr10
   #add-point:browser_search段define
   {<point name="browser_search.define"/>}
   #end add-point    
   
   #若無輸入關鍵字則查找出所有資料
   IF NOT cl_null(g_searchstr) AND g_searchcol='0' THEN
      CALL cl_err("searchcol","std-00005",0)
      RETURN FALSE 
   END IF 
   
   IF NOT cl_null(g_searchstr) THEN
      LET g_wc = " lower(", g_searchcol, ") LIKE '%", g_searchstr, "%'"
      LET g_wc = g_wc.toLowerCase()
   ELSE
      LET g_wc = " 1=1 "
   END IF         
   
   #若為排序搜尋則添加以下條件
   IF cl_null(g_searchcol) OR g_searchcol = "0" THEN
      LET g_wc = g_wc, " ORDER BY pmeadocno"
   ELSE
      LET g_wc = g_wc, " ORDER BY ", g_searchcol
   END IF 
 
   CALL apmt490_browser_fill("F")
   RETURN TRUE
 
END FUNCTION
]]>
</section>
