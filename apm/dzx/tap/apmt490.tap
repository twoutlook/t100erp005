      RETURN FALSE
   END IF
 
   #依據單據個數，需要指定所有單身條件為" 1=1"  (單身有幾個就要設幾個)
   LET g_wc2_table1 = " 1=1"
 
 
   CALL apmt490_show()
   CALL apmt490_set_pk_array()
   
   #add-point: 提交前的ADP
   {<point name="send.before_send" />}
   #end add-point
   
   #公用變數初始化
   CALL cl_bpm_data_init()
                  
   #依照主檔/單身個數產生 CALL cl_bpm_set_master_data() / cl_bpm_set_detail_data() 
   #單頭固定為 CALL cl_bpm_set_master_data(util.JSONObject.fromFGL(xxxx)) 傳入參數: (1)單頭陣列  ; 回傳值: 無
   CALL cl_bpm_set_master_data(util.JSONObject.fromFGL(g_pmea_m))
                              
   #單身固定為 CALL cl_bpm_set_detail_data(s_detailX, util.JSONArray.fromFGL(xxxx)) 傳入參數: (1)單身SR名稱  (2)單身陣列  ; 回傳值: 無
   CALL cl_bpm_set_detail_data("s_detail1", util.JSONArray.fromFGL(g_pmeb_d))
   CALL cl_bpm_set_detail_data("s_detail2", util.JSONArray.fromFGL(g_pmeb2_d))   CALL cl_bpm_set_detail_data("s_detail3", util.JSONArray.fromFGL(g_pmeb3_d))
 
   # cl_bpm_cli() 裡有包含以前的aws_condition()=>送簽資料檢核和更新單據狀況碼為'W'
   # cl_bpm_cli() 傳入參數:無  ;  回傳值: 0 開單失敗; 1 開單成功
 
   #開單失敗
   IF NOT cl_bpm_cli() THEN 
      CLOSE apmt490_cl
      CALL s_transaction_end('N','0')
      RETURN FALSE
   END IF
 
   #add-point: 提交後的ADP
   {<point name="send.after_send" />}
   #end add-point
   
   #完成狀態更新
   CLOSE apmt490_cl
   CALL s_transaction_end('Y','0')
             
   #此段落不需要刪除資料,但是否需要refresh圖片樣式???
   #CALL apmt490_ui_browser_refresh()
 
   #重新指定此筆單據資料狀態圖片=>送簽中
   LET g_browser[g_current_row].b_statepic = "stus/16/signing.png"
 
   #重新取得單頭/單身資料,DISPLAY在畫面上
   CALL apmt490_ui_headershow()
   CALL apmt490_ui_detailshow()
 
   RETURN TRUE
   
END FUNCTION
 
 
#此段落由子樣板a40產生
#+ BPM抽單
PRIVATE FUNCTION apmt490_draw_out()
 
   #add-point:draw段define
   {<point name="draw.define"/>}
   #end add-point
 
   #檢查資料是否存在
   IF g_pmea_m.pmeadocno IS NULL
   OR g_pmea_m.pmea900 IS NULL
   THEN
      CALL cl_err("","std-00003",0)
      RETURN FALSE
   END IF
 
   #LOCK主檔資料
   CALL s_transaction_begin()
 
   #進行BPM抽單功能
   OPEN apmt490_cl USING g_enterprise,g_pmea_m.pmeadocno,g_pmea_m.pmea900
   IF STATUS THEN
      CALL cl_err("OPEN apmt490_cl:", STATUS, 1)
      CLOSE apmt490_cl
      CALL s_transaction_end('N','0')
      RETURN FALSE
   END IF
 
   #鎖住將被更改的資料
   FETCH apmt490_cl INTO g_pmea_m.pmea001,g_pmea_m.pmea901,g_pmea_m.pmeadocno,g_pmea_m.pmea900,g_pmea_m.pmeadocno_desc, 
       g_pmea_m.pmeasite,g_pmea_m.pmea902,g_pmea_m.pmea000,g_pmea_m.pmeadocdt,g_pmea_m.pmea002,g_pmea_m.pmea002_desc, 
       g_pmea_m.pmea003,g_pmea_m.pmea003_desc,g_pmea_m.pmeastus,g_pmea_m.pmea004,g_pmea_m.pmea004_desc, 
       g_pmea_m.pmeaacti,g_pmea_m.pmea005,g_pmea_m.pmea005_desc,g_pmea_m.pmea006,g_pmea_m.pmea006_desc, 
       g_pmea_m.pmea007,g_pmea_m.pmea008,g_pmea_m.pmea009,g_pmea_m.pmea009_desc,g_pmea_m.pmea011,g_pmea_m.pmea011_desc, 
       g_pmea_m.pmea030,g_pmea_m.pmea016,g_pmea_m.pmea017,g_pmea_m.pmea018,g_pmea_m.pmea019,g_pmea_m.pmea020, 
       g_pmea_m.pmea010,g_pmea_m.pmea012,g_pmea_m.pmea014,g_pmea_m.pmea015,g_pmea_m.pmea903,g_pmea_m.pmea903_desc, 
       g_pmea_m.pmea904,g_pmea_m.pmeaownid,g_pmea_m.pmeaownid_desc,g_pmea_m.pmeaowndp,g_pmea_m.pmeaowndp_desc, 
       g_pmea_m.pmeacrtid,g_pmea_m.pmeacrtid_desc,g_pmea_m.pmeacrtdp,g_pmea_m.pmeacrtdp_desc,g_pmea_m.pmeacrtdt, 
       g_pmea_m.pmeamodid,g_pmea_m.pmeamodid_desc,g_pmea_m.pmeamoddt,g_pmea_m.pmeacnfid,g_pmea_m.pmeacnfid_desc, 
       g_pmea_m.pmeacnfdt
   
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_pmea_m.pmeadocno,SQLCA.sqlcode,0)
      CLOSE apmt490_cl
      CALL s_transaction_end('N','0')
      RETURN FALSE
   END IF
 
   #抽單失敗
   IF NOT cl_bpm_draw_out() THEN 
      CLOSE apmt490_cl
      CALL s_transaction_end('N','0')
      RETURN FALSE
   END IF    
 
   #完成狀態更新
   CLOSE apmt490_cl
   CALL s_transaction_end('Y','0')
          
   #重新指定此筆單據資料狀態圖片=>抽單
   LET g_browser[g_current_row].b_statepic = "stus/16/draw_out.png"
 
   #重新取得單頭/單身資料,DISPLAY在畫面上
   CALL apmt490_ui_headershow()  
   CALL apmt490_ui_detailshow()
 
   RETURN TRUE
   
END FUNCTION
 
 
 
]]>
