          g_pmea_m.pmea018,g_pmea_m.pmea019,g_pmea_m.pmea020,g_pmea_m.pmea030,
          g_pmea_m.pmeadocdt
     FROM pmdx_t
    WHERE pmdxent = g_enterprise
