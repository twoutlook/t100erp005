  <point name="function.apmt490_feature" cite_std="N" status="" ver="1" src="s" new="Y" order="43" mark_hard="N">
<![CDATA[################################################################################
# Descriptions...: 新增時，依據產品特徵開窗後的值新增到資料庫
# Memo...........:
# Usage..........: CALL apmt490_feature()
#                  RETURNING r_success
# Input parameter: 
#                : 
# Return code....: r_success      TRUE/FALSE
#                : 
# Date & Author..: 2014/03/06 By stellar0130
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt490_feature()
DEFINE l_pmeb_d          type_g_pmeb_d
DEFINE l_i               LIKE type_t.num5
DEFINE r_success         LIKE type_t.num5
DEFINE l_pmebseq         LIKE pmeb_t.pmebseq

   LET r_success = TRUE
   
   LET l_pmebseq = ''
   SELECT MAX(pmebseq) INTO l_pmebseq 
     FROM pmeb_t
    WHERE pmebent = g_enterprise
      AND pmebdocno = g_pmea_m.pmeadocno
      AND pmeb900 = g_pmea_m.pmea900
   IF cl_null(l_pmebseq) THEN
      LET l_pmebseq = 0
   END IF
   
   FOR l_i = 2 TO g_inam.getLength()
   
      LET l_pmebseq = l_pmebseq + 1
      
      LET l_pmeb_d.* = g_pmeb_d[l_ac].*
      LET l_pmeb_d.pmebseq = l_pmebseq
      
      LET l_pmeb_d.pmeb003 = g_inam[l_i].inam002
      LET l_pmeb_d.pmeb009 = g_inam[l_i].inam004
      
      #未稅金額、含稅金額、稅額
      CALL apmt490_get_amount(l_pmeb_d.pmebseq,l_pmeb_d.pmeb009,l_pmeb_d.pmeb010,l_pmeb_d.pmeb011)
           RETURNING l_pmeb_d.pmeb017,l_pmeb_d.pmeb018,l_pmeb_d.pmeb019
      
      #交易對象料號
      CALL apmt490_get_pmeb005(l_pmeb_d.pmeb002,l_pmeb_d.pmeb003)
           RETURNING l_pmeb_d.pmeb005
      
      LET l_pmeb_d.pmeb901 = '3'
      
      INSERT INTO pmeb_t (pmebent,pmebsite,pmebdocno,pmeb900,pmebseq,pmeb001,pmeb002,pmeb003,
                          pmeb004,pmeb005,pmeb006,pmeb007,pmeb008,pmeb009,pmeb010,pmeb011,
                          pmeb012,pmeb013,pmeb014,pmeb017,pmeb018,pmeb019,pmeb020,pmeb021,
                          pmeb022,pmeb023,pmeb024,pmeb901)
             VALUES (g_enterprise,g_pmea_m.pmeasite,g_pmea_m.pmeadocno,g_pmea_m.pmea900,l_pmeb_d.pmebseq,
                     g_pmea001,l_pmeb_d.pmeb002,l_pmeb_d.pmeb003,l_pmeb_d.pmeb004,l_pmeb_d.pmeb005,
                     l_pmeb_d.pmeb006,l_pmeb_d.pmeb007,l_pmeb_d.pmeb008,l_pmeb_d.pmeb009,l_pmeb_d.pmeb010,
                     l_pmeb_d.pmeb011,l_pmeb_d.pmeb012,l_pmeb_d.pmeb013,l_pmeb_d.pmeb014,l_pmeb_d.pmeb017,
                     l_pmeb_d.pmeb018,l_pmeb_d.pmeb019,0,0,0,0,
                     l_pmeb_d.pmeb024,l_pmeb_d.pmeb901)
      IF SQLCA.sqlcode THEN
         CALL cl_err('pmeb_t',SQLCA.sqlcode,1)
         LET r_success = FALSE
         EXIT FOR
      END IF
      
      #新增變更歷程檔
      CALL apmt490_pmeb_ins_pmed(l_pmeb_d.*)
           RETURNING r_success
      IF NOT r_success THEN
         EXIT FOR
      END IF
      
      #若有累計量定價時，一并產生累計量定價檔
      IF l_pmeb_d.pmeb024 = 'Y' THEN
         CALL apmt490_feature_ins_pmec(l_pmeb_d.pmebseq)
              RETURNING r_success
         IF NOT r_success THEN
            EXIT FOR
         END IF
      END IF
      
   END FOR
   
   CALL g_inam.clear()
   
   RETURN r_success
END FUNCTION]]>
</point>
  <point name="function.apmt490_feature_ins_pmec" cite_std="N" status="" ver="1" src="s" new="Y" order="44" mark_hard="N">
<![CDATA[################################################################################
# Descriptions...: 由apmt490_feature呼叫，產生累計量定價檔
# Memo...........:
# Usage..........: CALL apmt490_feature_ins_pmec(p_pmebseq)
#                  RETURNING r_success
# Input parameter: p_pmebseq      欲新增的項次
#                : 
# Return code....: r_success      TRUE/FALSE
#                : 
# Date & Author..: 2014/03/06 By stellar0130
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt490_feature_ins_pmec(p_pmebseq)
DEFINE p_pmebseq         LIKE pmeb_t.pmebseq
DEFINE r_success         LIKE type_t.num5
DEFINE l_pmec_d          RECORD
         pmecdocno       LIKE pmec_t.pmecdocno, 
         pmec900         LIKE pmec_t.pmec900, 
         pmecsite        LIKE pmec_t.pmecsite, 
         pmecseq         LIKE pmec_t.pmecseq, 
         pmecseq1        LIKE pmec_t.pmecseq1, 
         pmec001         LIKE pmec_t.pmec001, 
         pmec002         LIKE pmec_t.pmec002, 
         pmec003         LIKE pmec_t.pmec003, 
         pmec004         LIKE pmec_t.pmec004, 
         pmec005         LIKE pmec_t.pmec005, 
         pmec901         LIKE pmec_t.pmec901,
         pmec902         LIKE pmec_t.pmec902,
         pmec902_desc    LIKE oocql_t.oocql004,
         pmec903         LIKE pmec_t.pmec903
                         END RECORD

   LET r_success = TRUE
   
   DECLARE sel_pmec_cs CURSOR FOR
    SELECT pmecseq1,pmec001,pmec002,pmec003,pmec004,pmec005
      FROM pmec_t
     WHERE pmecent = g_enterprise
       AND pmecdocno = g_pmea_m.pmeadocno
       AND pmec900 = g_pmea_m.pmea900
       AND pmecseq = g_pmeb_d[l_ac].pmebseq
   
   FOREACH sel_pmec_cs INTO l_pmec_d.pmecseq1,l_pmec_d.pmec001,l_pmec_d.pmec002,
                            l_pmec_d.pmec003,l_pmec_d.pmec004,l_pmec_d.pmec005
      LET l_pmec_d.pmecdocno = g_pmea_m.pmeadocno
      LET l_pmec_d.pmec900 = g_pmea_m.pmea900
      LET l_pmec_d.pmecsite= g_pmea_m.pmeasite
      LET l_pmec_d.pmecseq = p_pmebseq
      LET l_pmec_d.pmec901 = '3'
      
      INSERT INTO pmec_t (pmecent,pmecsite,pmecdocno,pmec900,pmecseq,pmecseq1,
                          pmec001,pmec002,pmec003,pmec004,pmec005,pmec901)
             VALUES (g_enterprise,l_pmec_d.pmecsite,l_pmec_d.pmecdocno,
                     l_pmec_d.pmec900,l_pmec_d.pmecseq,l_pmec_d.pmecseq1,
                     l_pmec_d.pmec001,l_pmec_d.pmec002,l_pmec_d.pmec003,
                     l_pmec_d.pmec004,l_pmec_d.pmec005,l_pmec_d.pmec901)
      IF SQLCA.sqlcode THEN
         CALL cl_err('pmec_t',SQLCA.sqlcode,1)
         LET r_success = FALSE
         EXIT FOREACH
      END IF
      
      #新增變更歷程檔
      IF NOT apmt490_01_pmec_ins_pmed(l_pmec_d.*) THEN
         LET r_success = FALSE
         EXIT FOREACH
      END IF
   END FOREACH
   
   RETURN r_success
END FUNCTION]]>
</point>
  <point name="function.apmt490_change_pmeb011" cite_std="N" status="" ver="1" src="s" new="Y" order="45" mark_hard="N">
<![CDATA[################################################################################
# Descriptions...: 單頭稅別修改時，單身稅別一并修改
# Memo...........:
# Usage..........: CALL apmt490_change_pmeb011(p_cmd)
#                  
# Input parameter: p_cmd          新增/修改狀態
#                : 
# Return code....: 
#                : 
# Date & Author..: 2014/03/04 By stellar0130
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt490_change_pmeb011(p_cmd)
DEFINE p_cmd             LIKE type_t.chr1
DEFINE l_i               LIKE type_t.num10
DEFINE l_success         LIKE type_t.num5
DEFINE l_pmebseq         LIKE pmeb_t.pmebseq
DEFINE l_pmeb002         LIKE pmeb_t.pmeb002
DEFINE l_pmeb009         LIKE pmeb_t.pmeb009
DEFINE l_pmeb010         LIKE pmeb_t.pmeb010
DEFINE l_pmeb011         LIKE pmeb_t.pmeb011
DEFINE l_pmeb012         LIKE pmeb_t.pmeb012
DEFINE l_pmeb017         LIKE pmeb_t.pmeb017
DEFINE l_pmeb018         LIKE pmeb_t.pmeb018
DEFINE l_pmeb019         LIKE pmeb_t.pmeb019

   #單頭新增時，因單身資料還沒insert到資料庫，所以只改變畫面上的顯示
   IF p_cmd = 'a' THEN
      FOR l_i = 1 TO g_pmeb_d.getLength()
         IF g_tax_app = '2' THEN
            #依料件設定
            CALL s_tax_chktype(g_pmea_m.pmeasite,g_pmea_m.pmea004,g_pmeb_d[l_i].pmeb002,
                               '2',g_pmab039)
                 RETURNING l_success,g_pmeb_d[l_i].pmeb011,g_pmeb_d[l_i].pmeb012
         ELSE
            #依正常稅率
            LET g_pmeb_d[l_i].pmeb011 = g_pmea_m.pmea006
            LET g_pmeb_d[l_i].pmeb012 = g_pmea_m.pmea007
         END IF
         CALL apmt490_tax_ref(g_pmeb_d[l_i].pmeb011)
              RETURNING g_pmeb_d[l_i].pmeb011_desc
            
         #重計未稅金額、含稅金額、稅額
         CALL apmt490_get_amount(g_pmeb_d[l_i].pmebseq,g_pmeb_d[l_i].pmeb009,
                                 g_pmeb_d[l_i].pmeb010,g_pmeb_d[l_i].pmeb011)
              RETURNING g_pmeb_d[l_i].pmeb017,g_pmeb_d[l_i].pmeb018,g_pmeb_d[l_i].pmeb019
      END FOR
   ELSE
      DECLARE sel_pmeb_cs CURSOR FOR
       SELECT pmebseq,pmeb002,pmeb009,pmeb010 
         FROM pmeb_t
        WHERE pmebent = g_enterprise
          AND pmebdocno = g_pmea_m.pmeadocno
          AND pmeb900 = g_pmea_m.pmea900
      FOREACH sel_pmeb_cs INTO l_pmebseq,l_pmeb002,l_pmeb009,l_pmeb010
         IF g_tax_app = '2' THEN
            #依料件設定
            CALL s_tax_chktype(g_pmea_m.pmeasite,g_pmea_m.pmea004,l_pmeb002,
                               '2',g_pmab039)
                 RETURNING l_success,l_pmeb011,l_pmeb012
         ELSE
            #依正常稅率
            LET l_pmeb011 = g_pmea_m.pmea006
            LET l_pmeb012 = g_pmea_m.pmea007
         END IF
            
         #重計未稅金額、含稅金額、稅額
         CALL apmt490_get_amount(l_pmebseq,l_pmeb009,
                                 l_pmeb010,l_pmeb011)
              RETURNING l_pmeb017,l_pmeb018,l_pmeb019
         
         UPDATE pmeb_t SET pmeb011 = l_pmeb011,
                           pmeb012 = l_pmeb012,
                           pmeb017 = l_pmeb017,
                           pmeb018 = l_pmeb018,
                           pmeb019 = l_pmeb019
          WHERE pmebent = g_enterprise
            AND pmebdocno = g_pmea_m.pmeadocno
            AND pmeb900 = g_pmea_m.pmea900
