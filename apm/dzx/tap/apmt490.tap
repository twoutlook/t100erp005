################################################################################
PRIVATE FUNCTION apmt490_del_pmed(p_pmedseq,p_pmedseq1,p_pmed002)
DEFINE p_pmedseq    LIKE pmed_t.pmedseq
DEFINE p_pmedseq1   LIKE pmed_t.pmedseq1
DEFINE p_pmed002    LIKE pmed_t.pmed002
DEFINE r_success    LIKE type_t.num5

   LET r_success = TRUE
   
   DELETE FROM pmed_t
    WHERE pmedent = g_enterprise
      AND pmeddocno = g_pmea_m.pmeadocno
