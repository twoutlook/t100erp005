       pmeaowndp,pmeacrtid,pmeacrtdp,pmeacrtdt,pmeamodid,pmeamoddt,pmeacnfid,pmeacnfdt,t1.oofa011 ,t2.ooefl003 , 
       t3.pmaal004 ,t4.ooail003 ,t5.ooibl004 ,t6.oocql004 ,t7.oofa011 ,t8.ooefl003 ,t9.oofa011 ,t10.ooefl003 , 
       t11.oofa011 ,t12.oofa011",
               " FROM pmea_t",
                              " LEFT JOIN oofa_t t1 ON t1.oofaent='"||g_enterprise||"' AND t1.oofa002='2' AND t1.oofa003=pmea002  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent='"||g_enterprise||"' AND t2.ooefl001=pmea003 AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN pmaal_t t3 ON t3.pmaalent='"||g_enterprise||"' AND t3.pmaal001=pmea004 AND t3.pmaal002='"||g_dlang||"' ",
               " LEFT JOIN ooail_t t4 ON t4.ooailent='"||g_enterprise||"' AND t4.ooail001=pmea005 AND t4.ooail002='"||g_dlang||"' ",
               " LEFT JOIN ooibl_t t5 ON t5.ooiblent='"||g_enterprise||"' AND t5.ooibl002=pmea009 AND t5.ooibl003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t6 ON t6.oocqlent='"||g_enterprise||"' AND t6.oocql001='238' AND t6.oocql002=pmea011 AND t6.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t7 ON t7.oofaent='"||g_enterprise||"' AND t7.oofa002='2' AND t7.oofa003=pmeaownid  ",
               " LEFT JOIN ooefl_t t8 ON t8.ooeflent='"||g_enterprise||"' AND t8.ooefl001=pmeaowndp AND t8.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t9 ON t9.oofaent='"||g_enterprise||"' AND t9.oofa002='2' AND t9.oofa003=pmeacrtid  ",
               " LEFT JOIN ooefl_t t10 ON t10.ooeflent='"||g_enterprise||"' AND t10.ooefl001=pmeacrtdp AND t10.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t11 ON t11.oofaent='"||g_enterprise||"' AND t11.oofa002='2' AND t11.oofa003=pmeamodid  ",
               " LEFT JOIN oofa_t t12 ON t12.oofaent='"||g_enterprise||"' AND t12.oofa002='2' AND t12.oofa003=pmeacnfid  ",
 
               " WHERE pmeaent = '" ||g_enterprise|| "' AND pmeadocno = ? AND pmea900 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
