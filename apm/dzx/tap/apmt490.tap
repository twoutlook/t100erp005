  <point name="function.apmt490_pmea004_other" cite_std="N" status="" ver="1" src="s" new="Y" order="14" mark_hard="N">
<![CDATA[################################################################################
# Descriptions...: 供應商的其他處理
# Memo...........:
# Usage..........: CALL apmt490_pmea004_other(p_cmd)
#                  RETURNING 回传参数
# Input parameter: p_cmd          新增/修改狀態
#                :
# Return code....: 
#                : 
# Date & Author..: 2014/02/24 By stellar0130
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt490_pmea004_other(p_cmd)
DEFINE p_cmd             LIKE type_t.chr1
DEFINE l_success         LIKE type_t.num5
DEFINE l_controlno       LIKE pmal_t.pmal002
   
   IF cl_null(g_pmea_m.pmea004) THEN
      RETURN
   END IF
   
   #預設供應商慣用幣別、稅別、付款條件、交易條件，並抓取採購分類
   IF (g_pmea_m.pmea004 <> g_pmea_m_t.pmea004) OR cl_null(g_pmea_m_t.pmea004) THEN
      #用控制組預設
      CALL s_control_get_group('4',g_pmea_m.pmea002,g_pmea_m.pmea003)
           RETURNING l_success,l_controlno
      IF NOT cl_null(l_controlno) THEN
         SELECT pmal003,pmal004,pmal006,pmal020,pmal009
           INTO g_pmea_m.pmea005,g_pmea_m.pmea006,g_pmea_m.pmea009,g_pmea_m.pmea011,
                g_pmab039
           FROM pmal_t
          WHERE pmalent = g_enterprise
            AND pmal001 = g_pmea_m.pmea004
            AND pmal002 = l_controlno
      END IF
      IF cl_null(l_controlno) OR STATUS = 100 THEN
         SELECT pmab033,pmab034,pmab037,pmab053,pmab039
           INTO g_pmea_m.pmea005,g_pmea_m.pmea006,g_pmea_m.pmea009,g_pmea_m.pmea011,
                g_pmab039
           FROM pmab_t
          WHERE pmabent = g_enterprise
            AND pmabsite= g_pmea_m.pmeasite
            AND pmab001 = g_pmea_m.pmea004
      END IF
      #依據稅別帶出其他資訊
      CALL s_tax_chk(g_pmea_m.pmeasite,g_pmea_m.pmea006) 
           RETURNING l_success,g_pmea_m.pmea006_desc,g_pmea_m.pmea008,
                     g_pmea_m.pmea007,g_tax_app
   ELSE
      LET g_pmea_m.pmea005 = g_pmea_m_t.pmea005
      LET g_pmea_m.pmea006 = g_pmea_m_t.pmea006
      LET g_pmea_m.pmea006_desc = g_pmea_m_t.pmea006_desc
      LET g_pmea_m.pmea007 = g_pmea_m_t.pmea007
      LET g_pmea_m.pmea008 = g_pmea_m_t.pmea008
      LET g_pmea_m.pmea009 = g_pmea_m_t.pmea009
      LET g_pmea_m.pmea011 = g_pmea_m_t.pmea011
   END IF
   DISPLAY BY NAME g_pmea_m.pmea005,g_pmea_m.pmea006,g_pmea_m.pmea007,
                   g_pmea_m.pmea008,g_pmea_m.pmea009,g_pmea_m.pmea011
   CALL apmt490_pmea005_ref()
   DISPLAY BY NAME g_pmea_m.pmea006_desc
   CALL apmt490_pmea009_ref()
   CALL apmt490_acc_ref('238',g_pmea_m.pmea011) RETURNING g_pmea_m.pmea011_desc
   DISPLAY BY NAME g_pmea_m.pmea011_desc
   
   #判斷是否跳出詢問視窗修改單身稅別
   IF NOT cl_null(g_pmea_m.pmea006) THEN
      IF g_pmea_m.pmea006 <> g_pmea_m_o.pmea006 OR cl_null(g_pmea_m_o.pmea006) THEN
         IF cl_ask_confirm('apm-00351') THEN
            CALL apmt490_change_pmeb011(p_cmd)
         END IF
      END IF
      LET g_pmea_m_o.pmea006 = g_pmea_m.pmea006
   END IF
END FUNCTION]]>
</point>
