   IF cl_null(r_pmeb017) THEN LET r_pmeb017 = 0 END IF
   IF cl_null(r_pmeb018) THEN LET r_pmeb018 = 0 END IF
   IF cl_null(r_pmeb019) THEN LET r_pmeb019 = 0 END IF
   
