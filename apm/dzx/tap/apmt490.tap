<![CDATA[            LET g_pmeb_d[l_ac].pmeb002_desc = ''
            LET g_pmeb_d[l_ac].pmeb002_desc_desc = ''
            DISPLAY BY NAME g_pmeb_d[l_ac].pmeb002_desc,g_pmeb_d[l_ac].pmeb002_desc_desc
            
            IF NOT cl_null(g_pmeb_d[l_ac].pmeb002) THEN
               IF l_cmd = 'a' OR 
                  (l_cmd = 'u' AND (g_pmeb_d[l_ac].pmeb002 <> g_pmeb_d_t.pmeb002 OR g_pmeb_d_t.pmeb002 IS NULL)) THEN
                  IF NOT apmt490_pmeb002_chk() THEN
                     LET g_pmeb_d[l_ac].pmeb002 = g_pmeb_d_t.pmeb002
                     DISPLAY BY NAME g_pmeb_d[l_ac].pmeb002
                     CALL apmt490_pmeb002_ref()
                     NEXT FIELD CURRENT
                  ELSE
                     CALL apmt490_pmeb002_other(g_pmeb_d[l_ac].pmeb002)
                          RETURNING g_pmeb_d[l_ac].pmeb004,g_pmeb_d[l_ac].pmeb008,
                                    g_pmeb_d[l_ac].pmeb011,g_pmeb_d[l_ac].pmeb012
                     DISPLAY BY NAME g_pmeb_d[l_ac].pmeb004,g_pmeb_d[l_ac].pmeb008,
                                     g_pmeb_d[l_ac].pmeb011,g_pmeb_d[l_ac].pmeb012
                  END IF
               END IF
            END IF
            CALL apmt490_set_entry_b(l_cmd)
            CALL apmt490_set_no_entry_b(l_cmd)
            #若產品特徵不能輸入，那就要先抓取交易對象料號
            IF NOT cl_null(g_pmeb_d[l_ac].pmeb002) THEN
               IF cl_null(g_imaa005) THEN
                  LET g_pmeb_d[l_ac].pmeb003 = ' '
