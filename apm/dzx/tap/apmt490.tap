      IF NOT apmt490_ins_pmed(0,0,"pmdx020",'1',l_pmdx_m.pmdx020,g_pmea_m.pmea020) THEN
         LET r_success = FALSE
         RETURN r_success
      ELSE
         LET l_flag = TRUE     #有欄位變更，則更新為'Y'
      END IF
   ELSE
      #資料一樣，表示無變更，將變更歷程檔刪除
      IF NOT apmt490_del_pmed(0,0,"pmdx020") THEN
         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF
   
   #備註
   IF (NOT cl_null(g_pmea_m.pmea030) AND (g_pmea_m.pmea030 != l_pmdx_m.pmdx030 OR cl_null(l_pmdx_m.pmdx030))) OR
      (cl_null(g_pmea_m.pmea030) AND NOT cl_null(l_pmdx_m.pmdx030)) THEN
      #其說明的SQL
      LET g_pmed007 = NULL
      #新增資料到變更歷程檔
      IF NOT apmt490_ins_pmed(0,0,"pmdx030",'1',l_pmdx_m.pmdx030,g_pmea_m.pmea030) THEN
         LET r_success = FALSE
         RETURN r_success
      ELSE
         LET l_flag = TRUE     #有欄位變更，則更新為'Y'
      END IF
   ELSE
      #資料一樣，表示無變更，將變更歷程檔刪除
      IF NOT apmt490_del_pmed(0,0,"pmdx030") THEN
         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF
   
   IF l_flag THEN
      UPDATE pmea_t SET pmea901 = 'Y'
       WHERE pmeaent = g_enterprise
         AND pmeadocno = g_pmea_m.pmeadocno
         AND pmea900 = g_pmea_m.pmea900
      IF SQLCA.sqlcode THEN
         CALL cl_err("upd pmea901",SQLCA.sqlcode,1)
         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF

   RETURN r_success
END FUNCTION]]>
</point>
