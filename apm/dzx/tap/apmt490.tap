  <point name="function.apmt490_pmeadocno_pmeb" cite_std="N" status="" ver="1" src="s" new="Y" order="7" mark_hard="N">
<![CDATA[################################################################################
# Descriptions...: 依據合約單號，帶出採購合約單單身對應欄位的值
# Memo...........:
# Usage..........: CALL apmt490_pmeadocno_pmeb()
#                  RETURNING r_success
# Input parameter: 
#                : 
# Return code....: r_success      TRUE/FALSE
#                : 
# Date & Author..: 2014/02/19 By stellar0130
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt490_pmeadocno_pmeb()
DEFINE l_sql             STRING
DEFINE r_success         LIKE type_t.num5

   LET r_success = TRUE
   
   IF cl_null(g_pmea_m.pmeadocno) THEN
      RETURN r_success
   END IF
   
   LET l_sql = "SELECT pmdyseq,pmdy002,pmdy003,pmdy004,pmdy005,pmdy006, ",
               "       pmdy007,pmdy008,pmdy009,pmdy010,pmdy011,pmdy012, ",
               "       pmdy013,pmdy014,pmdy017,pmdy018,pmdy019,pmdy020, ",
               "       pmdy021,pmdy022,pmdy023,pmdy024,pmdy030 ",
               "  FROM pmdy_t ",
               " WHERE pmdyent = '",g_enterprise,"'",
               "   AND pmdydocno = '",g_pmea_m.pmeadocno,"'",
               " ORDER BY pmdyseq "
   PREPARE sel_pmdy_pre FROM l_sql
   DECLARE sel_pmdy_cur CURSOR FOR sel_pmdy_pre
   
   LET l_ac = 1
   FOREACH sel_pmdy_cur INTO g_pmeb_d[l_ac].pmebseq,g_pmeb_d[l_ac].pmeb002,g_pmeb_d[l_ac].pmeb003,
                             g_pmeb_d[l_ac].pmeb004,g_pmeb_d[l_ac].pmeb005,g_pmeb_d[l_ac].pmeb006,
                             g_pmeb_d[l_ac].pmeb007,g_pmeb_d[l_ac].pmeb008,g_pmeb_d[l_ac].pmeb009,
                             g_pmeb_d[l_ac].pmeb010,g_pmeb_d[l_ac].pmeb011,g_pmeb_d[l_ac].pmeb012,
                             g_pmeb_d[l_ac].pmeb013,g_pmeb_d[l_ac].pmeb014,g_pmeb_d[l_ac].pmeb017,
                             g_pmeb_d[l_ac].pmeb018,g_pmeb_d[l_ac].pmeb019,g_pmeb2_d[l_ac].pmeb020,
                             g_pmeb2_d[l_ac].pmeb021,g_pmeb2_d[l_ac].pmeb022,g_pmeb2_d[l_ac].pmeb023,
                             g_pmeb_d[l_ac].pmeb024,g_pmeb_d[l_ac].pmeb030
                             
      LET g_pmeb_d[l_ac].pmeb001 = g_pmea_m.pmea001
      LET g_pmeb_d[l_ac].pmebsite = g_pmea_m.pmeasite
      LET g_pmeb_d[l_ac].pmeb901 = '1'   #未變更
      CALL apmt490_pmeb002_ref()
      CALL apmt490_acc_ref('221',g_pmeb_d[l_ac].pmeb006)
           RETURNING g_pmeb_d[l_ac].pmeb006_desc
      CALL apmt490_pmeb008_ref()
      CALL apmt490_tax_ref(g_pmeb_d[l_ac].pmeb011)
           RETURNING g_pmeb_d[l_ac].pmeb011_desc
      CALL apmt490_acc_ref('263',g_pmeb_d[l_ac].pmeb013)
           RETURNING g_pmeb_d[l_ac].pmeb013_desc
           
      #已轉採購狀況的欄位顯示
      CALL apmt490_pmeb2_show(l_ac)
      
      LET l_ac = l_ac+1
      IF l_ac > g_max_rec THEN
         CALL cl_err('',9035,0)
         EXIT FOREACH
      END IF
   END FOREACH
   
   CALL g_pmeb_d.deleteElement(g_pmeb_d.getLength())
   CALL g_pmeb2_d.deleteElement(g_pmeb2_d.getLength())
   
   LET g_rec_b = l_ac - 1
   LET l_ac = 0
   FREE sel_pmdy_pre
   
   DISPLAY ARRAY g_pmeb_d TO s_detail1.* ATTRIBUTES(COUNT=g_rec_b)
      BEFORE DISPLAY 
         EXIT DISPLAY
   END DISPLAY
   
   DISPLAY ARRAY g_pmeb2_d TO s_detail2.* ATTRIBUTES(COUNT=g_rec_b)
      BEFORE DISPLAY
         EXIT DISPLAY
   END DISPLAY
   
   RETURN r_success
END FUNCTION]]>
</point>
