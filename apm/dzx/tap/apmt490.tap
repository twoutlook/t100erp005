                   g_pmea_m.pmea005,g_pmea_m.pmea006,g_pmea_m.pmea007,g_pmea_m.pmea008,
                   g_pmea_m.pmea009,g_pmea_m.pmea010,g_pmea_m.pmea011,g_pmea_m.pmea012,
                   g_pmea_m.pmea014,g_pmea_m.pmea015,g_pmea_m.pmea016,g_pmea_m.pmea017,
                   g_pmea_m.pmea018,g_pmea_m.pmea019,g_pmea_m.pmea020,g_pmea_m.pmea030,
                   g_pmea_m.pmeadocdt
                   
   CALL apmt490_pmea002_ref()
   CALL apmt490_pmea003_ref()
   CALL apmt490_pmea004_ref()
   CALL apmt490_pmea005_ref()
   CALL apmt490_tax_ref(g_pmea_m.pmea006) RETURNING g_pmea_m.pmea006_desc
   DISPLAY BY NAME g_pmea_m.pmea006_desc
