  <point name="ins_data.define" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[DEFINE l_ooef019  LIKE  ooef_t.ooef019
DEFINE l_gzcbl004 LIKE  gzcbl_t.gzcbl004
DEFINE l_acc      LIKE  type_t.chr20
DEFINE l_success  LIKE  type_t.num5]]>
  </point>
  <point name="ins_data.foreach" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[       #稅別說明
       INITIALIZE l_ooef019 TO NULL
       SELECT ooef019 INTO l_ooef019
         FROM ooef_t
        WHERE ooef001=sr_s.pmdssite
          AND ooefent=sr_s.pmdsent
       CALL s_desc_get_tax_desc (l_ooef019,sr_s.pmds033) RETURNING sr_s.l_pmds033_desc
       
       #退廠方式SCC   
       CALL s_desc_gzcbl004_desc('2062',sr_s.pmds100) RETURNING sr_s.l_pmds100_desc
       
       #理由碼說明
       SELECT gzcb004  INTO l_acc FROM gzcb_t WHERE gzcb001 = '24' AND gzcb002 = 'apmt890'
       CALL s_desc_get_acc_desc(l_acc,sr_s.pmdt051) RETURNING sr_s.l_pmdt051_desc
       
       #儲位
       IF NOT cl_null(sr_s.pmdt017) THEN
          CALL s_desc_get_locator_desc(sr_s.pmdssite,sr_s.pmdt016,sr_s.pmdt017) RETURNING sr_s.l_pmdt017_inab003
          LET sr_s.l_pmdt017_inab003 = sr_s.pmdt017 CLIPPED ,'.', sr_s.l_pmdt017_inab003 CLIPPED
       END IF

       #產品特徵
       LET l_success = FALSE
       IF NOT cl_null(sr_s.pmdt007) THEN
          CALL s_feature_description(sr_s.pmdt006,sr_s.pmdt007) RETURNING l_success,sr_s.l_pmdt007_desc
       END IF
       IF NOT l_success THEN
          LET sr_s.l_pmdt007_desc = ''
       END IF
       
       #當前面編號為空時，清空編號.說明的字串(避免編號為空會只顯示一個 .)
       CALL apmr890_g01_initialize(sr_s.pmdt016,sr_s.l_pmdt016_inayl003) RETURNING sr_s.l_pmdt016_inayl003
       CALL apmr890_g01_initialize(sr_s.pmds007,sr_s.l_pmds007_pmaal004) RETURNING sr_s.l_pmds007_pmaal004
       CALL apmr890_g01_initialize(sr_s.pmds008,sr_s.l_pmds008_pmaal004) RETURNING sr_s.l_pmds008_pmaal004
       CALL apmr890_g01_initialize(sr_s.pmds009,sr_s.l_pmds009_pmaal004) RETURNING sr_s.l_pmds009_pmaal004
       CALL apmr890_g01_initialize(sr_s.pmds002,sr_s.l_pmds002_ooag011) RETURNING sr_s.l_pmds002_ooag011
       CALL apmr890_g01_initialize(sr_s.pmds003,sr_s.l_pmds003_ooefl003) RETURNING sr_s.l_pmds003_ooefl003]]>
  </point>
  <point name="rep.a_group.pmdsdocno.after" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[           LET  l_pmdt038_sum = GROUP SUM(sr1.pmdt038)
           LET  l_pmdt039_sum = GROUP SUM(sr1.pmdt039)
           IF cl_null(l_pmdt038_sum) THEN LET  l_pmdt038_sum = 0 END IF
