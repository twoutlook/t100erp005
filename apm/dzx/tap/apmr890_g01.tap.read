  <point name="rep.everyrow.after" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[          START REPORT apmr890_g01_subrep05
          IF tm.a1 ="Y" THEN
             IF NOT cl_null(sr1.pmdtseq) THEN

                LET g_sql = "SELECT pmdudocno,pmduseq,pmduseq1,pmdu005,pmdu006,pmdu007,pmdu008,pmdu010,pmdu011,trim(pmdu006)||'.'||trim(inayl003),trim(pmdu007)||'.'||trim(inab003) ",
                            "  FROM pmdu_t LEFT OUTER JOIN inayl_t ON inaylent = pmduent AND inayl001 = pmdu006 AND inayl002 = '",g_dlang,"'",
                            "              LEFT OUTER JOIN inab_t ON inabent = pmduent AND inabsite = pmdusite AND inab001= pmdu006 AND inab002 = pmdu007",
                            " WHERE pmdudocno = '",sr1.pmdsdocno CLIPPED,"'",
                            "   AND pmduent   = '",sr1.pmdsent   CLIPPED,"'",
                            "   AND pmduseq   = '",sr1.pmdtseq   CLIPPED,"' ",
                            "   ORDER BY pmduseq1 "

                DECLARE apmr890_g01_repcur05 CURSOR FROM g_sql
                FOREACH apmr890_g01_repcur05 INTO sr3.*
                   CALL apmr890_g01_initialize(sr3.pmdu006,sr3.l_pmdu006_inayl003) RETURNING sr3.l_pmdu006_inayl003
                   CALL apmr890_g01_initialize(sr3.pmdu007,sr3.l_pmdu007_inab003) RETURNING sr3.l_pmdu007_inab003
                   OUTPUT TO REPORT apmr890_g01_subrep05(sr3.*)
                END FOREACH
             END IF
          END IF
          FINISH REPORT apmr890_g01_subrep05]]>
  </point>
  <point name="rep.everyrow.afterrow" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
