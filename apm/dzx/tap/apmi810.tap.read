      LET g_wc = g_wc, " ORDER BY pmcc001"
   ELSE
      LET g_wc = g_wc, " ORDER BY ", g_searchcol
   END IF 
 
   CALL apmi810_browser_fill("F")
   RETURN TRUE
 
END FUNCTION
