  <point name="function.apmp840_num_change" order="5" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 單位間的轉換數量
# Memo...........: 
# Usage..........: CALL apmp840_num_change(p_type,p_pmdn001,p_pmdn006,p_pmdn007,p_pmdn010,p_pmdn201,p_pmdn202)
#                :    RETURNING r_pmdn007,r_pmdn011,r_pmdn202
# Input parameter: p_type         1.由採購數量轉換成包裝數量及計價數量
#                :                2.由包裝數量轉換成採購數量及計價數量
#                : p_pmdn001      商品編號
#                : p_pmdn006      採購單位
#                : p_pmdn007      採購數量
#                : p_pmdn010      計價單位
#                : p_pmdn201      包裝單位
#                : p_pmdn202      包裝數量
# Return code....: r_pmdn007      採購數量
#                : r_pmdn011      計價數量
#                : r_pmdn202      包裝數量
# Date & Author..: 2014/12/12 By pomelo
# Modify.........:
################################################################################
PUBLIC FUNCTION apmp840_num_change(p_type,p_pmdn001,p_pmdn006,p_pmdn007,p_pmdn010,p_pmdn201,p_pmdn202)
DEFINE p_type           LIKE type_t.num5       #轉換方式
DEFINE p_pmdn001        LIKE pmdn_t.pmdn001    #商品編號
DEFINE p_pmdn006        LIKE pmdn_t.pmdn006    #採購單位
DEFINE p_pmdn007        LIKE pmdn_t.pmdn007    #採購數量
DEFINE p_pmdn010        LIKE pmdn_t.pmdn010    #計價單位
DEFINE p_pmdn201        LIKE pmdn_t.pmdn201    #包裝單位
DEFINE p_pmdn202        LIKE pmdn_t.pmdn202    #包裝數量
DEFINE r_pmdn007        LIKE pmdn_t.pmdn007    #採購數量
DEFINE r_pmdn011        LIKE pmdn_t.pmdn011    #計價數量
DEFINE r_pmdn202        LIKE pmdn_t.pmdn202    #包裝數量
DEFINE l_pmdn007        LIKE pmdn_t.pmdn007    #採購數量
DEFINE l_pmdn011        LIKE pmdn_t.pmdn011    #計價數量
DEFINE l_pmdn202        LIKE pmdn_t.pmdn202    #包裝數量
DEFINE l_success        LIKE type_t.num5

   LET r_pmdn007 = 0
   LET r_pmdn011 = 0
   LET r_pmdn202 = 0
   
   #當包裝單位或採購單位都為空，表示無法轉換
    IF cl_null(p_pmdn201) OR cl_null(p_pmdn006) THEN
       RETURN r_pmdn007,r_pmdn011,r_pmdn202
    END IF
   
   LET l_pmdn007 = 0
   LET l_pmdn011 = 0
   LET l_pmdn202 = 0
   
   #由採購數量轉換成包裝數量及計價數量
   IF p_type = '1' THEN
      LET l_pmdn007 = p_pmdn007
      #當採購數量有值，由採購數量轉換成包裝數量
      CALL s_aooi250_convert_qty(p_pmdn001,p_pmdn006,p_pmdn201,p_pmdn007)
         RETURNING l_success,l_pmdn202
   ELSE
      LET l_pmdn202 = p_pmdn202
      #當採購數量為空，由包裝數量轉換成採購數量
      CALL s_aooi250_convert_qty(p_pmdn001,p_pmdn201,p_pmdn006,p_pmdn202)
         RETURNING l_success,l_pmdn007
   END IF

    #計算計價數量
    IF NOT cl_null(p_pmdn010) THEN
       CALL s_aooi250_convert_qty(p_pmdn001,p_pmdn006,p_pmdn010,l_pmdn007)
          RETURNING l_success,l_pmdn011
    END IF
    
    LET r_pmdn007 = l_pmdn007
    LET r_pmdn011 = l_pmdn011
    LET r_pmdn202 = l_pmdn202
    
    RETURN r_pmdn007,r_pmdn011,r_pmdn202
END FUNCTION]]>
  </point>
