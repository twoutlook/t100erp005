  <point name="function.apmp840_chk_num" order="8" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 採購數量檢查
# Memo...........:
# Usage..........: CALL apmp840_chk_num(p_type)
#                     RETURNING r_success
# Input parameter: p_type         1.採購數量
#                                 2.包裝數量
# Return code....: r_success      True/False
# Date & Author..: 2015/02/02 By pomelo
# Modify.........:
################################################################################
PRIVATE FUNCTION apmp840_chk_num(p_type)
DEFINE p_type           LIKE type_t.num5       #轉換方式
DEFINE r_success        LIKE type_t.num5

   LET r_success = TRUE
   
   CALL apmp840_num_change(p_type,g_detail2_d[l_ac2].pmdn001,g_detail2_d[l_ac2].pmdn006,
                           g_detail2_d[l_ac2].pmdn007,g_detail2_d[l_ac2].pmdn010,
                           g_detail2_d[l_ac2].pmdn201,g_detail2_d[l_ac2].pmdn202)
      RETURNING g_detail2_d[l_ac2].pmdn007,g_detail2_d[l_ac2].pmdn011,g_detail2_d[l_ac2].pmdn202
   
   #檢查輸入採購數量是否大於未轉採購量
   IF g_detail2_d[l_ac2].pmdn007 > g_detail2_d[l_ac2].l_num1 AND p_type = '1' THEN
      #輸入的採購數量%1 > 未轉採購量%2！
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = "apm-00682"
      LET g_errparam.extend = ""
      LET g_errparam.popup = TRUE
      LET g_errparam.replace[1] = g_detail2_d[l_ac2].pmdn007
      LET g_errparam.replace[2] = g_detail2_d[l_ac2].l_num1
      CALL cl_err()
      LET r_success = FALSE
      RETURN r_success
   END IF   
   
   #檢查輸入包裝數量轉成採購數量是否大於未轉採購量
   IF g_detail2_d[l_ac2].pmdn007 > g_detail2_d[l_ac2].l_num1 AND p_type = '2' THEN
      #輸入的包裝數量轉為採購數量%1 > 未轉採購量%2！
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = "apm-00683"
      LET g_errparam.extend = ""
      LET g_errparam.popup = TRUE
      LET g_errparam.replace[1] = g_detail2_d[l_ac2].pmdn007
      LET g_errparam.replace[2] = g_detail2_d[l_ac2].l_num1
      CALL cl_err()
      LET r_success = FALSE
      RETURN r_success
   END IF
   
   RETURN r_success
END FUNCTION]]>
  </point>
