                           LET g_errparam.code   = l_errno
                           LET g_errparam.popup  = TRUE
                           CALL cl_err()
                           LET g_qbe.pmdb205 = l_pmdb205_o
                           CALL s_desc_get_department_desc(g_qbe.pmdb205)
                               RETURNING g_qbe.pmdb205_desc
                           DISPLAY BY NAME g_qbe.pmdb205_desc
                           NEXT FIELD CURRENT
                        END IF
                     ELSE
                        INITIALIZE g_chkparam.* TO NULL
                        LET g_chkparam.arg1 = g_qbe.pmdb205
                        IF NOT cl_chk_exist("v_ooef001_34") THEN
                           LET g_qbe.pmdb205 = l_pmdb205_o
                           CALL s_desc_get_department_desc(g_qbe.pmdb205)
                               RETURNING g_qbe.pmdb205_desc
                           DISPLAY BY NAME g_qbe.pmdb205_desc
                           NEXT FIELD CURRENT
                        END IF
