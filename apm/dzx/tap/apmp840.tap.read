  <point name="ui_dialog.more_action" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[         #點選
         ON ACTION select_item
            LET l_i = DIALOG.getCurrentRow("s_detail1")
            IF g_detail_d[l_i].sel = "Y" THEN
               LET g_detail_d[l_i].sel = "N"
            ELSE
               LET g_detail_d[l_i].sel = "Y"
            END IF
            
         #產生採購底稿
         ON ACTION gen_papers
            LET l_sel_cnt = 0
            CALL cl_err_collect_init()
            FOR li_idx = 1 TO g_detail_d.getLength()
               IF g_detail_d[li_idx].sel = "Y" THEN
                  LET l_cnt = 0
                  #確認是否已經存在temp table中
                  EXECUTE apmp840_chk
                    USING g_detail_d[li_idx].pmdadocno,g_detail_d[li_idx].pmdbseq
                     INTO l_cnt
                  IF l_cnt = 0 THEN
                     CALL s_transaction_begin()
                     #新增到table table
                     CALL apmp840_ins_tmp(li_idx) RETURNING l_success
                     IF l_success THEN
                        CALL s_transaction_end('Y','0')
                     ELSE
                        CALL s_transaction_end('N','0')
                     END IF
                  END IF
                  LET l_sel_cnt = l_sel_cnt + 1
               END IF
            END FOR
            
            CALL cl_err_collect_show()
            #當沒有勾選任何一筆資料，報錯
            IF l_sel_cnt = 0 THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 'adb-00078'
               LET g_errparam.extend = ''
               LET g_errparam.popup = TRUE
               CALL cl_err()
               CONTINUE DIALOG
            END IF
            CALL apmp840_b_fill2()
            
         #產生採購資料
         ON ACTION batch_execute
            #確保直接按產生採購資料，可是數量並沒有過欄位
            LET l_ac2 = DIALOG.getCurrentRow("s_detail2")
            IF l_ac2 >= 1 THEN
               # 當採購數量為空，表示輸入包裝數量
               IF cl_null(g_detail2_d[l_ac2].pmdn007) THEN
                  LET l_type = '2'
               ELSE
                  LET l_type = '1'
               END IF
               IF NOT apmp840_chk_num(l_type) THEN
                  CONTINUE DIALOG
               END IF
               UPDATE s_apmp840_tmp
                  SET pmdn007 = g_detail2_d[l_ac2].pmdn007,
                      pmdn011 = g_detail2_d[l_ac2].pmdn011,
                      pmdn202 = g_detail2_d[l_ac2].pmdn202
                WHERE pmdp003 = g_detail2_d[l_ac2].pmdp003
                  AND pmdp004 = g_detail2_d[l_ac2].pmdp004
            END IF
               
            LET l_cnt = 0
            SELECT COUNT(pmdp003) INTO l_cnt
              FROM s_apmp840_tmp
            
            IF l_cnt = 0 THEN
               #採購底稿中尚未有任何資料！
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 'apm-00684'
               LET g_errparam.extend = ''
               LET g_errparam.popup = TRUE
               CALL cl_err()
               CONTINUE DIALOG
            ELSE
               CALL s_transaction_begin()
               CALL cl_err_collect_init()
               CALL s_apmp840_gen_po(g_parameter.type,g_parameter.pmdldocno)
                  RETURNING l_success,l_docno_str
               IF l_success THEN
                  LET l_success = ''
                  CALL s_apmp840_del_tmp()
                     RETURNING l_success
                  CALL s_transaction_end('Y','1')
                  IF cl_null(g_parameter.pmdldocno) AND NOT cl_null(l_docno_str) THEN
                     LET la_param.prog = 'apmt840'
                     LET la_param.param[1] = ''
                     LET la_param.param[2] = l_docno_str
                     LET ls_js = util.JSON.stringify(la_param)
                     CALL cl_cmdrun_wait(ls_js)
                  END IF
               ELSE
                  CALL s_transaction_end('N','1')
               END IF
               CALL cl_err_collect_show()
               CALL apmp840_b_fill()
               CALL apmp840_b_fill2()
            END IF]]>
  </point>
  <point name="ui_dialog.more_construct" order="" ver="2" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[         INPUT BY NAME g_qbe.pmdb205,g_qbe.pmdb207
            
            BEFORE INPUT 
               #單號有值表示 由T類程式串入
               IF cl_null(g_parameter.pmdb207) THEN
                  IF cl_null(g_qbe.pmdb207) THEN
                     LET g_qbe.pmdb207 = '1'
                  END IF
               ELSE
                 LET g_qbe.pmdb205 = g_parameter.pmdb205     #採購中心
                 LET g_qbe.pmdb015 = g_parameter.pmdb015     #供應商
                 LET g_qbe.pmdb206 = g_parameter.pmdb206     #採購員
                 LET g_qbe.pmdb207 = g_parameter.pmdb207     #採購方式
                 LET g_qbe.pmdb037 = g_parameter.pmdb037     #收貨組織
                 LET g_qbe.pmdb260 = g_parameter.pmdb260     #收貨部門
                 LET g_qbe.pmdb203 = g_parameter.pmdb203     #配送中心
                 DISPLAY BY NAME g_qbe.pmdb205,g_qbe.pmdb205_desc,
                                 g_qbe.pmdb015,g_qbe.pmdb206,
                                 g_qbe.pmdb207,g_qbe.pmdb037,
                                 g_qbe.pmdb260,g_qbe.pmdb203
               END IF
               CALL apmp840_set_entry()
               CALL apmp840_set_no_entry()
               
            AFTER FIELD pmdb205
               LET g_qbe.pmdb205_desc = ' '
               DISPLAY BY NAME g_qbe.pmdb205_desc
               IF NOT cl_null(g_qbe.pmdb205) THEN
                  IF g_qbe.pmdb205 != l_pmdb205_o OR cl_null(l_pmdb205_o) THEN
                     IF s_aooi500_setpoint(g_prog,'pmdb205') THEN
                        CALL s_aooi500_chk(g_prog,'pmdb205',g_qbe.pmdb205,g_site)
                           RETURNING l_success,l_errno
