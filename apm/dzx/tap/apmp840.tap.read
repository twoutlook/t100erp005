    <![CDATA[   #檢查選取的資料是否已經存在temp table 中
   LET l_sql = "SELECT COUNT(*)",
               "  FROM s_apmp840_tmp",
               " WHERE pmdp003 = ?",
               "   AND pmdp004 = ?"
   PREPARE apmp840_chk FROM l_sql
   
   #取得需求組織的慣用幣別
   LET l_sql = "SELECT ooef016",
               "  FROM ooef_t",
               " WHERE ooefent = ",g_enterprise,
               "   AND ooef001 = ?",
               "   AND ooefstus = 'Y'"
   PREPARE apmp840_ooef016 FROM l_sql

   #算出相同 商品編號 產品特徵 採購單位 計價單位
   #且到庫日期小於等於此筆資料的到庫日期 筆數
   LET l_sql = "SELECT COALESCE(MAX(pmdp021),0)+1",
               "  FROM s_apmp840_tmp",
               " WHERE pmdn001 = ?",
               "   AND pmdn002 = ?",
               "   AND pmdn006 = ?",
               "   AND pmdn010 = ?",
               "   AND pmdn014 <= ?"
   PREPARE apmp840_max_pmdp021 FROM l_sql
   
   #取要貨單上的庫位
   LET l_sql = "SELECT pmdb038",
               "  FROM pmdb_t",
               " WHERE pmdbent = ",g_enterprise,
               "   AND pmdbdocno = ?",
               "   AND pmdbseq = ?"
   PREPARE apmp840_pmdb038 FROM l_sql
   
   #取採購單單據日期
   LET l_sql = "SELECT pmdldocdt",
               "  FROM pmdl_t",
               " WHERE pmdlent = ",g_enterprise,
               "   AND pmdldocno = ?"
   PREPARE apmp840_pmdldocdt FROM l_sql
   
   #拆單方式
   LET g_parameter.type = cl_get_para(g_enterprise,g_site,'E-CIR-0026')
   DISPLAY g_parameter.type TO b_type
   ]]>
