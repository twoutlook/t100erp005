  <point name="function.apmp840_set_entry" order="1" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 欄位開啟
# Memo...........:
# Usage..........: CALL apmp840_set_entry()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/12/08 BY pomelo
# Modify.........:
################################################################################
PUBLIC FUNCTION apmp840_set_entry()
   CALL cl_set_comp_entry("pmdb205",TRUE)   #採購中心
   CALL cl_set_comp_entry("pmdb015",TRUE)   #供應商
   CALL cl_set_comp_entry("pmdb206",TRUE)   #採購員
   CALL cl_set_comp_entry("pmdb207",TRUE)   #採購方式
   CALL cl_set_comp_entry("pmdb037",TRUE)   #收貨組織
   CALL cl_set_comp_entry("pmdb260",TRUE)   #收貨部門
   CALL cl_set_comp_entry("pmdb203",TRUE)   #配送中心
END FUNCTION]]>
  </point>
  <point name="function.apmp840_set_no_entry" order="2" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 欄位關閉
# Memo...........:
# Usage..........: CALL apmp840_set_no_entry()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/12/08 By pomelo
# Modify.........:
################################################################################
PUBLIC FUNCTION apmp840_set_no_entry()
   IF NOT cl_null(g_parameter.pmdldocno) THEN
      CALL cl_set_comp_entry("pmdb205",FALSE)   #採購中心
      CALL cl_set_comp_entry("pmdb015",FALSE)   #供應商
      CALL cl_set_comp_entry("pmdb206",FALSE)   #採購員
      CALL cl_set_comp_entry("pmdb207",FALSE)   #採購方式
      CALL cl_set_comp_entry("pmdb037",FALSE)   #收貨組織
      CALL cl_set_comp_entry("pmdb260",FALSE)   #收貨部門
      CALL cl_set_comp_entry("pmdb203",FALSE)   #配送中心
   END IF
END FUNCTION]]>
  </point>
  <point name="function.apmp840_ins_tmp" order="3" ver="4" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 把選取的資料新增到temp table裡
# Memo...........:
# Usage..........: CALL apmp840_ins_tmp(p_ac)
#                :    RETURNING r_success
# Input parameter: p_ac           陣列位置
# Return code....: r_success      True/False
# Date & Author..: 2014/12/10 By pomelo
# Modify.........:
################################################################################
PUBLIC FUNCTION apmp840_ins_tmp(p_ac)
DEFINE p_ac             LIKE type_t.num5
DEFINE r_success        LIKE type_t.num5
DEFINE l_success        LIKE type_t.num5
DEFINE l_pmdn200        LIKE pmdn_t.pmdn200
DEFINE l_pmdp           RECORD
       pmdb007          LIKE pmdb_t.pmdb007,   #要貨單位
       pmdb006          LIKE pmdb_t.pmdb006,   #要貨量
       num              LIKE type_t.num20_6,   #未轉採購量
          
       #採購單單頭
       pmdldocdt        LIKE pmdl_t.pmdldocdt, #單據日期
       pmdl004          LIKE pmdl_t.pmdl004,   #供應商
       pmdl015          LIKE pmdl_t.pmdl015,   #幣別
       pmdl203          LIKE pmdl_t.pmdl203,   #採購方式
       pmdl207          LIKE pmdl_t.pmdl207,   #所屬品類   #150610-00030#1 150612 by sakura add
          
       #採購單單身
       pmdnunit         LIKE pmdn_t.pmdnunit,  #收貨組織
       pmdnorga         LIKE pmdn_t.pmdnorga,  #帳務組織
       pmdn001          LIKE pmdn_t.pmdn001,   #商品編號
       pmdn002          LIKE pmdn_t.pmdn002,   #產品特徵
       l_imaa0091       LIKE imaa_t.imaa009,   #商品品類   #150610-00030#1 150612 by sakura add    
       pmdn006          LIKE pmdn_t.pmdn006,   #採購單位
       pmdn007          LIKE pmdn_t.pmdn007,   #採購數量
       pmdn010          LIKE pmdn_t.pmdn010,   #計價單位
       pmdn011          LIKE pmdn_t.pmdn011,   #計價數量
       pmdn014          LIKE pmdn_t.pmdn014,   #到庫日期
       pmdn015          LIKE pmdn_t.pmdn015,   #單價
       pmdn016          LIKE pmdn_t.pmdn016,   #稅別
       pmdn017          LIKE pmdn_t.pmdn017,   #稅率
       pmdn020          LIKE pmdn_t.pmdn020,   #緊急度
       pmdn025          LIKE pmdn_t.pmdn025,   #送貨地址碼
       pmdn028          LIKE pmdn_t.pmdn028,   #庫位
       pmdn200          LIKE pmdn_t.pmdn200,   #商品條碼
       pmdn227          LIKE pmdn_t.pmdn227,   #補貨規格說明   #150710-00016#8 150713 by sakura add
       pmdn201          LIKE pmdn_t.pmdn201,   #包裝單位
       pmdn202          LIKE pmdn_t.pmdn202,   #採購包裝數量
       pmdn203          LIKE pmdn_t.pmdn203,   #要貨部門
       pmdn205          LIKE pmdn_t.pmdn205,   #要貨組織
       pmdn216          LIKE pmdn_t.pmdn216,   #經營方式
       pmdn217          LIKE pmdn_t.pmdn217,   #結算方式
       pmdn218          LIKE pmdn_t.pmdn218,   #合約編號
       pmdn219          LIKE pmdn_t.pmdn219,   #協議編號
       pmdn220          LIKE pmdn_t.pmdn220,   #採購人員
       pmdn221          LIKE pmdn_t.pmdn221,   #採購部門
       pmdn222          LIKE pmdn_t.pmdn222,   #採購中心
       pmdn223          LIKE pmdn_t.pmdn223,   #配送中心
       pmdn224          LIKE pmdn_t.pmdn224,   #採購失效日
       
       #採購單關聯單據
       pmdp003          LIKE pmdp_t.pmdp003,   #要貨單號
       pmdp004          LIKE pmdp_t.pmdp004,   #要貨項次
       pmdp021          LIKE pmdp_t.pmdp021    #沖銷順序
                        END RECORD
DEFINE l_cnt            LIKE type_t.num5

   LET r_success = TRUE
   LET l_success = TRUE
   INITIALIZE l_pmdp.* TO NULL
   
   #要貨單
   LET l_pmdp.pmdb007 = g_detail_d[p_ac].pmdb007      #要貨單位
   LET l_pmdp.pmdb006 = g_detail_d[p_ac].pmdb006      #要貨量
   LET l_pmdp.num     = g_detail_d[p_ac].l_num        #未轉採購量
   IF l_pmdp.num <= 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = "apm-00681"
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      LET g_errparam.replace[1] = g_detail_d[p_ac].pmdadocno
      LET g_errparam.replace[2] = g_detail_d[p_ac].pmdbseq
      CALL cl_err()
      LET l_success = FALSE
   END IF
   
   #採購單單頭
   LET l_pmdp.pmdldocdt = g_detail_d[p_ac].l_pdate    #單據日期 = 採購出單日
   LET l_pmdp.pmdl004 = g_detail_d[p_ac].pmdb015      #供應商
   LET l_pmdp.pmdl203 = g_detail_d[p_ac].pmdb207      #採購方式
   LET l_pmdp.pmdl015 = g_detail_d[p_ac].l_pmdl015    #幣別
   LET l_pmdp.pmdl207 = g_detail_d[p_ac].pmda202      #所屬品類   #150610-00030#1 150612 by sakura add
   
   #EXECUTE apmp840_ooef016 USING g_detail_d[p_ac].pmdbsite
   #   INTO l_pmdp.pmdl015                             #幣別
   #IF cl_null(l_pmdp.pmdl015) THEN
   #   INITIALIZE g_errparam TO NULL
   #   LET g_errparam.code = "apm-00675"
   #   LET g_errparam.extend = ''
   #   LET g_errparam.popup = TRUE
   #   LET g_errparam.replace[1] = g_detail_d[p_ac].pmdbsite
   #   CALL cl_err()
   #   LET l_success = FALSE
   #END IF
   
   #採購單單身
   LET l_pmdp.pmdnunit = g_detail_d[p_ac].pmdb037     #收貨組織
   LET l_pmdp.pmdn001 = g_detail_d[p_ac].pmdb004      #商品編號
   LET l_pmdp.pmdn002 = g_detail_d[p_ac].pmdb005      #產品特徵
   #避免SQL錯誤
   IF cl_null(l_pmdp.pmdn002) THEN
      LET l_pmdp.pmdn002 = ' '
   END IF
   LET l_pmdp.pmdn014 = g_detail_d[p_ac].l_ddate      #到庫日期
   LET l_pmdp.pmdn020 = g_detail_d[p_ac].pmdn020      #緊急度
   LET l_pmdp.pmdn200 = g_detail_d[p_ac].pmdb200      #商品條碼
   LET l_pmdp.pmdn203 = g_detail_d[p_ac].pmdb260      #要貨部門
   LET l_pmdp.pmdn205 = g_detail_d[p_ac].pmdbsite     #收貨組織
   LET l_pmdp.pmdn222 = g_detail_d[p_ac].pmdb205      #採購中心
   LET l_pmdp.pmdn223 = g_detail_d[p_ac].pmdb203      #配送中心
   LET l_pmdp.pmdn224 = g_detail_d[p_ac].pmdn224      #採購失效日
   #150710-00016#8 150713 by sakura add(S)
   LET l_pmdp.pmdn227 = g_detail_d[p_ac].pmdb227      #要貨補貨規格說明
   LET l_pmdp.pmdn201 = g_detail_d[p_ac].pmdb201      #要貨包裝單位
   #150710-00016#8 150713 by sakura add(E)   
   #庫位
   EXECUTE apmp840_pmdb038
     USING g_detail_d[p_ac].pmdadocno,g_detail_d[p_ac].pmdbseq
      INTO l_pmdp.pmdn028
   IF cl_null(l_pmdp.pmdn014) THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = "apm-00676"
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()
      LET l_success = FALSE
   END IF
   
   #採購單關聯單據
   LET l_pmdp.pmdp003 = g_detail_d[p_ac].pmdadocno    #要貨單號
   LET l_pmdp.pmdp004 = g_detail_d[p_ac].pmdbseq      #要貨項次
   
   IF cl_null(g_detail_d[p_ac].l_pmdl015) THEN
      #基本資料頁籤中 要貨單號%1+項次%2，幣別為空！
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = "apm-00840"
      LET g_errparam.extend = ""
      LET g_errparam.popup = TRUE
      LET g_errparam.replace[1] = g_detail_d[p_ac].pmdadocno
      LET g_errparam.replace[2] = g_detail_d[p_ac].pmdbseq
      CALL cl_err()
      LET l_success = FALSE
   ELSE
      #取協議資料
      CALL s_apmt840_get_pact(l_pmdp.pmdn222,l_pmdp.pmdl015,
                              l_pmdp.pmdl004,l_pmdp.pmdldocdt,l_pmdp.pmdn001,l_pmdp.pmdn205)
         RETURNING l_pmdp.pmdnorga,   #帳務組織
                   l_pmdp.pmdn006,    #採購單位
                   l_pmdp.pmdn010,    #計價單位
                   l_pmdp.pmdn015,    #單價
                   l_pmdp.pmdn016,    #稅別
                   l_pmdp.pmdn017,    #稅率
                   l_pmdp.pmdn216,    #經營方式
                   l_pmdp.pmdn217,    #結算方式
                   l_pmdp.pmdn218,    #合約編號
                   l_pmdp.pmdn219,    #協議編號
                   l_pmdp.pmdn220,    #採購人員
                   l_pmdp.pmdn221     #採購部門
      #帳務組織
      IF cl_null(l_pmdp.pmdnorga) THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = "apm-00677"
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         LET g_errparam.replace[1] = l_pmdp.pmdn222
         LET g_errparam.replace[2] = l_pmdp.pmdl015
         LET g_errparam.replace[3] = l_pmdp.pmdl004
         LET g_errparam.replace[4] = l_pmdp.pmdldocdt
         LET g_errparam.replace[5] = l_pmdp.pmdn001
         CALL cl_err()
         LET l_success = FALSE
      END IF
      #採購單位
      IF cl_null(l_pmdp.pmdn006) THEN
         #採購中心 %1 + 幣別 %2 + 供應商 %3 + 日期 %4 + 商品編號%5 取到的採購單位為空！
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = "apm-00700"
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         LET g_errparam.replace[1] = l_pmdp.pmdn222
         LET g_errparam.replace[2] = l_pmdp.pmdl015
         LET g_errparam.replace[3] = l_pmdp.pmdl004
         LET g_errparam.replace[4] = l_pmdp.pmdldocdt
         LET g_errparam.replace[5] = l_pmdp.pmdn001
         CALL cl_err()
         LET l_success = FALSE
      END IF
      #計價單位
      IF cl_null(l_pmdp.pmdn010) THEN
         #採購中心 %1 + 幣別 %2 + 供應商 %3 + 日期 %4 + 商品編號%5 取到的計價單位為空！
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = "apm-00701"
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         LET g_errparam.replace[1] = l_pmdp.pmdn222
         LET g_errparam.replace[2] = l_pmdp.pmdl015
         LET g_errparam.replace[3] = l_pmdp.pmdl004
         LET g_errparam.replace[4] = l_pmdp.pmdldocdt
         LET g_errparam.replace[5] = l_pmdp.pmdn001
         CALL cl_err()
         LET l_success = FALSE
      END IF
      #單價
      IF cl_null(l_pmdp.pmdn010) THEN
         #採購中心 %1 + 幣別 %2 + 供應商 %3 + 日期 %4 + 商品編號%5 取到的單價為空！
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = "apm-00702"
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         LET g_errparam.replace[1] = l_pmdp.pmdn222
         LET g_errparam.replace[2] = l_pmdp.pmdl015
         LET g_errparam.replace[3] = l_pmdp.pmdl004
         LET g_errparam.replace[4] = l_pmdp.pmdldocdt
         LET g_errparam.replace[5] = l_pmdp.pmdn001
         CALL cl_err()
         LET l_success = FALSE
      END IF
   END IF
   
   #150710-00016#8 150713 by sakura mark(S)   
   ##取的門店清單裡的商品條碼及包裝單位
   #CALL s_apmt840_get_rtdx(g_site,l_pmdp.pmdn001)
   #   RETURNING l_pmdn200,l_pmdp.pmdn201
   #IF cl_null(l_pmdp.pmdn201) THEN
   #   INITIALIZE g_errparam TO NULL
   #   LET g_errparam.code = "apm-00678"
   #   LET g_errparam.extend = ''
   #   LET g_errparam.popup = TRUE
   #   LET g_errparam.replace[1] = l_pmdp.pmdn001
   #   LET g_errparam.replace[2] = g_site
   #   CALL cl_err()
   #   LET l_success = FALSE
   #END IF
   #150710-00016#8 150713 by sakura mark(E)
   
   #當需要的欄位都有值的時候，才可以新增到temp table裡，維護數量
   IF l_success THEN
      
      #要貨單位轉採購單位
      IF l_pmdp.pmdb007 = l_pmdp.pmdn006 THEN
         LET l_pmdp.pmdn007 = l_pmdp.num
      ELSE
         CALL s_aooi250_convert_qty(l_pmdp.pmdn001,l_pmdp.pmdb007,l_pmdp.pmdn006,l_pmdp.num)
            RETURNING l_success,l_pmdp.pmdn007
      END IF
      
      #單位間的轉換數量
      CALL apmp840_num_change('1',l_pmdp.pmdn001,l_pmdp.pmdn006,l_pmdp.pmdn007,
                              l_pmdp.pmdn010,l_pmdp.pmdn201,l_pmdp.pmdn202)
         RETURNING l_pmdp.pmdn007,l_pmdp.pmdn011,l_pmdp.pmdn202

      #沖銷順序
      #當相同 商品編號 產品特徵 採購單位 計價單位 且到庫日期大於此筆資料的到庫日期
      #沖銷順序往後+1
      UPDATE s_apmp840_tmp SET pmdp021 = pmdp021 + 1
       WHERE pmdn001 = l_pmdp.pmdn001
         AND pmdn002 = l_pmdp.pmdn002
         AND pmdn006 = l_pmdp.pmdn006
         AND pmdn010 = l_pmdp.pmdn010
         AND pmdn014 > l_pmdp.pmdn014
      
      #算出相同 商品編號 產品特徵 採購單位 計價單位
      #且到庫日期小於等於此筆資料的到庫日期 筆數
      EXECUTE apmp840_max_pmdp021
        USING l_pmdp.pmdn001,l_pmdp.pmdn002,l_pmdp.pmdn006,
              l_pmdp.pmdn010,l_pmdp.pmdn014
         INTO l_pmdp.pmdp021

      INSERT INTO s_apmp840_tmp(pmdb007, pmdb006, num,     pmdldocdt,
                                pmdl004, pmdl015, pmdl203, pmdnunit,
                                pmdnorga,pmdn001, pmdn002, pmdn006,
                                pmdn007, pmdn010, pmdn011, pmdn014,
                                pmdn015, pmdn016, pmdn017, pmdn020,
                                pmdn025, pmdn028, pmdn200, pmdn201,
                                pmdn202, pmdn203, pmdn205, pmdn216,
                                pmdn217, pmdn218, pmdn219, pmdn220,
                                pmdn221, pmdn222, pmdn223, pmdn224,
                                pmdp003, pmdp004, pmdp021, pmdl207,   #150610-00030#1 150612 by sakura add pmdl207
                                imaa009, pmdn227)   #150610-00030#1 150612 by sakura add imaa009   #150710-00016#8 150713 by sakura add pmdn227
         VALUES(l_pmdp.pmdb007,    l_pmdp.pmdb006, l_pmdp.num,    l_pmdp.pmdldocdt,
                l_pmdp.pmdl004,    l_pmdp.pmdl015, l_pmdp.pmdl203,l_pmdp.pmdnunit,
                l_pmdp.pmdnorga,   l_pmdp.pmdn001, l_pmdp.pmdn002,l_pmdp.pmdn006,
                l_pmdp.pmdn007,    l_pmdp.pmdn010, l_pmdp.pmdn011,l_pmdp.pmdn014,
                l_pmdp.pmdn015,    l_pmdp.pmdn016, l_pmdp.pmdn017,l_pmdp.pmdn020,
                l_pmdp.pmdn025,    l_pmdp.pmdn028, l_pmdp.pmdn200,l_pmdp.pmdn201,
                l_pmdp.pmdn202,    l_pmdp.pmdn203, l_pmdp.pmdn205,l_pmdp.pmdn216,
                l_pmdp.pmdn217,    l_pmdp.pmdn218, l_pmdp.pmdn219,l_pmdp.pmdn220,
                l_pmdp.pmdn221,    l_pmdp.pmdn222, l_pmdp.pmdn223,l_pmdp.pmdn224,
                l_pmdp.pmdp003,    l_pmdp.pmdp004, l_pmdp.pmdp021,l_pmdp.pmdl207,   #150610-00030#1 150612 by sakura add pmda202
                l_pmdp.l_imaa0091, l_pmdp.pmdn227)   #150610-00030#1 150612 by sakura add imaa009   #150710-00016#8 150713 by sakura add pmdn227
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = 'Insert s_apmp840_tmp'
         LET g_errparam.popup = TRUE
         CALL cl_err()
         LET r_success = FALSE
      END IF
   END IF
   
   RETURN r_success
END FUNCTION]]>
  </point>
  <point name="function.apmp840_b_fill2" order="4" ver="4" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 撈取單身資料
# Memo...........:
# Usage..........: CALL apmp840_b_fill2()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/12/11 By pomelo
# Modify.........:
################################################################################
PUBLIC FUNCTION apmp840_b_fill2()
DEFINE l_ac2           LIKE type_t.num5

   LET g_sql = "SELECT pmdp003,pmdp004,pmdn200,pmdn001,",
               "       '',     '',     pmdn002,",
               "       pmdl207,'',imaa009,",   #150610-00030#1 150612 by sakura add
               "       pmdb007,",
               "       '',     pmdb006,num,    pmdn014,",
               "       pmdp021,pmdn227,pmdn201,'',     pmdn202,",   #150710-00016#8 150713 by sakura add pmdn227
               "       pmdn006,'',     pmdn007,pmdn010,",
               "       '',     pmdn011",
               "  FROM s_apmp840_tmp",
               " ORDER BY pmdp003,pmdp004"
 
   PREPARE apmp840_sel2 FROM g_sql
   DECLARE b_fill2_curs CURSOR FOR apmp840_sel2
   
   CALL g_detail2_d.clear()
   LET l_ac2 = 1
 
   FOREACH b_fill2_curs INTO 
      g_detail2_d[l_ac2].pmdp003,       g_detail2_d[l_ac2].pmdp004,
      g_detail2_d[l_ac2].pmdn200,       g_detail2_d[l_ac2].pmdn001,
      g_detail2_d[l_ac2].pmdn001_desc,  g_detail2_d[l_ac2].pmdn001_desc_desc,
      g_detail2_d[l_ac2].pmdn002,
      #150610-00030#1 150612 By sakura add(S)      
      g_detail2_d[l_ac2].pmdl207,       g_detail2_d[l_ac2].pmdl207_desc,
      g_detail2_d[l_ac2].l_imaa009,
      #150610-00030#1 150612 By sakura add(E)
      g_detail2_d[l_ac2].pmdb007_1,
      g_detail2_d[l_ac2].pmdb007_1_desc,g_detail2_d[l_ac2].pmdb006_1,
      g_detail2_d[l_ac2].l_num1,        g_detail2_d[l_ac2].l_ddate1,
      g_detail2_d[l_ac2].pmdp021,       g_detail2_d[l_ac2].pmdn227,g_detail2_d[l_ac2].pmdn201,   #150710-00016#8 150713 by sakura add pmdn227
      g_detail2_d[l_ac2].pmdn201_desc,  g_detail2_d[l_ac2].pmdn202,
      g_detail2_d[l_ac2].pmdn006,       g_detail2_d[l_ac2].pmdn006_desc,
      g_detail2_d[l_ac2].pmdn007,       g_detail2_d[l_ac2].pmdn010,
      g_detail2_d[l_ac2].pmdn010_desc,  g_detail2_d[l_ac2].pmdn011
   
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "Foreach b_fill2_curs" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
      
      #商品編號
      CALL s_desc_get_item_desc(g_detail2_d[l_ac2].pmdn001)
         RETURNING g_detail2_d[l_ac2].pmdn001_desc,g_detail2_d[l_ac2].pmdn001_desc_desc
      
      #150610-00030#1 150612 By sakura add(S)
      #所屬品類說明
      CALL s_desc_get_rtaxl003_desc(g_detail2_d[l_ac2].pmdl207) 
        RETURNING g_detail2_d[l_ac2].pmdl207_desc
      #150610-00030#1 150612 By sakura add(S)
      
      #要貨單位
      CALL s_desc_get_unit_desc(g_detail2_d[l_ac2].pmdb007_1)
         RETURNING g_detail2_d[l_ac2].pmdb007_1_desc
      
      #採購包裝單位
      CALL s_desc_get_unit_desc(g_detail2_d[l_ac2].pmdn201)
         RETURNING g_detail2_d[l_ac2].pmdn201_desc
      
      #採購單位
      CALL s_desc_get_unit_desc(g_detail2_d[l_ac2].pmdn006)
         RETURNING g_detail2_d[l_ac2].pmdn006_desc
      
      #計價單位
      CALL s_desc_get_unit_desc(g_detail2_d[l_ac2].pmdn010)
         RETURNING g_detail2_d[l_ac2].pmdn010_desc
 
      LET l_ac2 = l_ac2 + 1
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
      
   END FOREACH
   LET g_error_show = 0
   
   CALL g_detail2_d.deleteElement(g_detail2_d.getLength())
    
   LET g_detail_cnt = l_ac2 - 1 
   DISPLAY g_detail_cnt TO FORMONLY.h_count
   
   CLOSE b_fill2_curs
   FREE apmp840_sel2
   
END FUNCTION]]>
