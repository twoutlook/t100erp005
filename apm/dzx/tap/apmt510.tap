  <point name="input.a.page2.pmeg001" order="" ver="3" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[            CALL s_desc_get_item_desc(g_pmeg_d[l_ac].pmeg001) RETURNING g_pmeg_d[l_ac].pmeg001_desc,g_pmeg_d[l_ac].pmeg001_desc_desc 
            DISPLAY BY NAME g_pmeg_d[l_ac].pmeg001_desc,g_pmeg_d[l_ac].pmeg001_desc_desc       
            
            IF NOT cl_null(g_pmeg_d[l_ac].pmeg001) THEN 
               IF l_cmd = 'a' OR ( l_cmd = 'u' AND (g_pmeg_d[l_ac].pmeg001 != g_pmeg_d_o.pmeg001)) THEN            
                  IF NOT apmt510_pmeg001_chk() THEN
                     LET g_pmeg_d[l_ac].pmeg001 = g_pmeg_d_t.pmeg001
                     DISPLAY BY NAME g_pmeg_d[l_ac].pmeg001
                     CALL s_desc_get_item_desc(g_pmeg_d[l_ac].pmeg001) RETURNING g_pmeg_d[l_ac].pmeg001_desc,g_pmeg_d[l_ac].pmeg001_desc_desc
                     DISPLAY BY NAME g_pmeg_d[l_ac].pmeg001_desc,g_pmeg_d[l_ac].pmeg001_desc_desc   
                     NEXT FIELD CURRENT  
                  ELSE

                     LET g_pmeg_d[l_ac].pmeg002 = ' ' #產品特徵                           
                     LET g_pmeg_d[l_ac].pmeg006 = ''  #單位
                     LET g_pmeg_d[l_ac].pmeg008 = ''  #參考單位
                     LET g_pmeg3_d[l_ac].pmeg003 = ''  #包裝容器
                     
                     SELECT imaf143,imaf015,imaf157 
                       INTO g_pmeg_d[l_ac].pmeg006,g_pmeg_d[l_ac].pmeg008,g_pmeg3_d[l_ac].pmeg003
                       FROM imaf_t
                      WHERE imafent = g_enterprise AND imafsite = g_site AND imaf001 = g_pmeg_d[l_ac].pmeg001

                     DISPLAY BY NAME g_pmeg_d[l_ac].pmeg006,g_pmeg_d[l_ac].pmeg008,g_pmeg3_d[l_ac].pmeg003

                     IF cl_get_para(g_enterprise,g_site,'S-BAS-0019') = "Y" THEN  #整體參數使用採購計價單位
                        SELECT imaf144 INTO g_pmeg_d[l_ac].pmeg010 FROM imaf_t
                         WHERE imafent = g_enterprise AND imafsite = g_site AND imaf001 = g_pmeg_d[l_ac].pmeg001
                        DISPLAY BY NAME g_pmeg_d[l_ac].pmeg010
                     END IF                     
                  END IF

                  #客戶料號帶值
                  IF NOT cl_null(g_pmeg_d[l_ac].pmeg002) THEN
                     CALL s_apmi070_get_pmao004(g_pmee_m.pmee004,g_pmeg_d[l_ac].pmeg001,g_pmeg_d[l_ac].pmeg002)
                          RETURNING l_success,g_pmeg3_d[l_ac].pmeg027
                     DISPLAY BY NAME g_pmeg3_d[l_ac].pmeg027
                  END IF
                     
                  CALL apmt510_pmeg001_desc()
                  
                  #修改料號時需要開窗詢問是否重新取價，若選擇要時則需呼叫取價應用元件重新取價，
                  #並重新呼叫'計算金額'應用元件計算[C:含稅金額]、[C:未稅金額]、[C:稅額]                  
               END IF 
               LET g_pmeg_d_o.pmeg001 = g_pmeg_d[l_ac].pmeg001
            END IF 

            CALL apmt510_set_entry_b(l_cmd)
            CALL apmt510_set_no_entry_b(l_cmd)

            #若產品特徵不能輸入，那就要先抓取交易對象料號
            IF NOT cl_null(g_pmeg_d[l_ac].pmeg001) THEN
               LET l_imaa005=''
               CALL apmt510_get_imaa005(g_enterprise,g_pmeg_d[l_ac].pmeg001) RETURNING l_imaa005
               IF cl_null(l_imaa005) THEN
                  LET g_pmeg_d[l_ac].pmeg002 = ' '
               END IF
               IF l_cmd = 'a' OR
                  (l_cmd = 'u' AND (g_pmeg_d[l_ac].pmeg001 <> g_pmeg_d_t.pmeg001 OR g_pmeg_d_t.pmeg001 IS NULL)) THEN
                  IF cl_null(l_imaa005) THEN
                     LET g_pmeg_d[l_ac].pmeg002 = ' '
                  END IF
                  #帶出交易對象料件
                  CALL apmt510_get_pmeg027(g_pmeg_d[l_ac].pmeg001,g_pmeg_d[l_ac].pmeg002)
                       RETURNING g_pmeg3_d[l_ac].pmeg027
                  DISPLAY BY NAME g_pmeg3_d[l_ac].pmeg027
               END IF
            END IF

            LET g_pmeg3_d[l_ac].pmeg0011  = g_pmeg_d[l_ac].pmeg001
            LET g_pmeg3_d[l_ac].imaal0034 = g_pmeg_d[l_ac].pmeg001_desc
            LET g_pmeg3_d[l_ac].imaal0044 = g_pmeg_d[l_ac].pmeg001_desc_desc 

]]>
