  <point name="function.apmt510_pmeedocno_ref" order="1" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#已經自動編號過的單據編號，抓取對應的單據別說明
PRIVATE FUNCTION apmt510_pmeedocno_ref(p_pmeedocno)
DEFINE p_pmeedocno       LIKE pmee_t.pmeedocno
DEFINE l_oobal002        LIKE oobal_t.oobal002
DEFINE l_ooef004         LIKE ooef_t.ooef004
DEFINE r_pmeedocno_desc  LIKE oobal_t.oobal004
DEFINE l_flag            LIKE type_t.num5          #标识符，TRUE/FALSE
DEFINE l_flag1           LIKE type_t.num5          #标识符，TRUE/FALSE
DEFINE l_site            LIKE type_t.chr20

      LET l_flag = TRUE
      LET l_flag1 = TRUE
      LET l_ooef004 = NULL
      LET l_oobal002 = NULL
      IF NOT cl_null(p_pmeedocno) THEN
         CALL s_aooi200_get_site(p_pmeedocno) RETURNING l_flag,l_site
         IF l_flag THEN
            SELECT ooef004 INTO l_ooef004 FROM ooef_t
             WHERE ooef005 = l_site
               AND ooefent = g_enterprise
         END IF
         CALL s_aooi200_get_slip(p_pmeedocno) RETURNING l_flag1,l_oobal002
         IF l_flag1 THEN
            IF NOT cl_null(l_oobal002) AND NOT cl_null(l_ooef004) THEN
               SELECT oobal004 INTO r_pmeedocno_desc FROM oobal_t
                WHERE oobal001 = l_ooef004
                  AND oobal002 = l_oobal002
                  AND oobal003 = g_dlang
                  AND oobalent = g_enterprise
            ELSE
               LET r_pmeedocno_desc = ""
            END IF
         END IF
      ELSE
         LET r_pmeedocno_desc = ""
      END IF

      RETURN r_pmeedocno_desc
END FUNCTION]]>
  </point>
  <point name="function.apmt510_pmeedocno_chk" order="2" ver="3" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#採購變更單檢查
PRIVATE FUNCTION apmt510_pmeedocno_chk()
DEFINE l_n               LIKE type_t.num5
DEFINE r_success         LIKE type_t.num5
DEFINE l_flag            LIKE type_t.num5
DEFINE l_success         LIKE type_t.num5
DEFINE l_pmdl047         LIKE pmdl_t.pmdl047

   LET r_success = TRUE

   IF cl_null(g_pmee_m.pmeedocno) THEN
      RETURN r_success
   END IF

   #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
   INITIALIZE g_chkparam.* TO NULL

   #設定g_chkparam.*的參數
   LET g_chkparam.arg1 = g_pmee_m.pmeedocno


   #呼叫檢查存在並帶值的library
   IF NOT cl_chk_exist("v_pmdldocno") THEN
      #檢查失敗時後續處理
      LET r_success = FALSE
      RETURN r_success
   END IF

   #同一張採購單號不能同時存在兩張以上未確認的變更單內
   LET l_n = 0
   SELECT COUNT(pmeedocno) INTO l_n
     FROM pmee_t
    WHERE pmeeent = g_enterprise
      AND pmeedocno = g_pmee_m.pmeedocno
      AND pmeestus = 'N'
      
   IF NOT cl_null(l_n) AND l_n > 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'apm-00396'
      LET g_errparam.extend = g_pmee_m.pmeedocno
      LET g_errparam.popup = TRUE
      CALL cl_err()

      LET r_success = FALSE
      RETURN r_success
   END IF
   
   #檢核輸入的單據別是否可以被key人員對應的控制組使用,'4' 為採購控制組類型
   CALL s_control_chk_doc('1',g_pmee_m.pmeedocno,'4',g_user,g_dept,'','') 
        RETURNING l_success,l_flag
   IF NOT l_success THEN      #处理状态
      LET r_success = FALSE
      RETURN r_success
   ELSE
      IF NOT l_flag THEN      #是否存在
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'apm-00266'
         LET g_errparam.extend = g_pmee_m.pmeedocno
         LET g_errparam.popup = TRUE
         CALL cl_err()

         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF    
   
   #物流結案="Y"的採購單不可以進行變更
   LET l_pmdl047 = ''
   SELECT pmdl047 INTO l_pmdl047 FROM pmdl_t
    WHERE pmdlent = g_enterprise
      AND pmdldocno = g_pmee_m.pmeedocno
   IF l_pmdl047 = 'Y' THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'apm-00639'
      LET g_errparam.extend = g_pmee_m.pmeedocno
      LET g_errparam.popup = TRUE
      CALL cl_err()

      LET r_success = FALSE
      RETURN r_success
   END IF

   RETURN r_success

END FUNCTION]]>
  </point>
