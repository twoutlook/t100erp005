      LET g_pmee_m.pmee020 = ''
      LET g_pmee_m.pmee020_desc = ''
      LET g_pmee_m.pmee023 = ''
      LET g_pmee_m.pmee023_desc = ''
      LET g_pmee_m.pmee024 = ''
      LET g_pmee_m.pmee024_desc = ''
      LET g_pmee_m.pmee033 = ''
      LET g_pmee_m.pmee033_desc = ''
    
      #先判斷這個供應商是否有設多個當前採購控制組範圍內的供應商預設條件，則開窗，讓user 選擇帶哪一個控制組的資料
      LET l_controlno = ''
      CALL s_control_get_group('4',g_user,g_dept) 
           RETURNING l_success,l_controlno

      IF NOT cl_null(l_controlno) THEN
         #判斷供應商是否有設置採購控制組供應商預設條件(apmi110)，若有則抓取相關的預設條件default到採購單上
         SELECT COUNT(*) INTO l_n FROM pmal_t 
          WHERE pmalent = g_enterprise 
            AND pmal001 = g_pmee_m.pmee004 AND pmal002 = l_controlno AND pmalstus = 'Y'
         IF l_n > 0 THEN
            SELECT pmal006,pmal020,pmal004,pmal003,pmal021,pmal011,pmal008,pmal009,pmal022
                  ,pmal019,pmal025   #2015/03/04 by stellar add
              INTO g_pmee_m.pmee009,g_pmee_m.pmee010,g_pmee_m.pmee011,g_pmee_m.pmee015,g_pmee_m.pmee017,g_pmee_m.pmee020,
                   g_pmee_m.pmee023,g_pmee_m.pmee024,g_pmee_m.pmee033
                  ,g_pmee_m.pmee002,g_pmee_m.pmee003    #2015/03/04 by stellar add
              FROM pmal_t WHERE pmalent = g_enterprise AND pmal001 = g_pmee_m.pmee004 AND pmal002 = l_controlno AND pmalstus = 'Y'
         END IF
      END IF  

      #若沒有設置採購控制組供應商預設條件資料則改抓供應商據點預設條件(apmm202)資料  
      IF cl_null(l_controlno) OR l_n = 0 THEN
         SELECT pmab037,pmab053,pmab034,pmab033,pmab054,pmab040,pmab038,pmab039,pmab056
               ,pmab031,pmab059   #2015/03/04 by stellar add
           INTO g_pmee_m.pmee009,g_pmee_m.pmee010,g_pmee_m.pmee011,g_pmee_m.pmee015,g_pmee_m.pmee017,g_pmee_m.pmee020,
                g_pmee_m.pmee023,g_pmee_m.pmee024,g_pmee_m.pmee033
               ,g_pmee_m.pmee002,g_pmee_m.pmee003   #2015/03/04 by stellar add
           FROM pmab_t WHERE pmabent = g_enterprise AND pmabsite = g_site AND pmab001 = g_pmee_m.pmee004

      END IF
      
      #2015/03/04 by stellar add ----- (S)
      IF cl_null(g_pmee_m.pmee002) OR cl_null(g_pmee_m.pmee003) THEN
         LET l_pmaa086 = ''
         LET l_pmaa087 = ''
         SELECT pmaa086,pmaa087 INTO l_pmaa086,l_pmaa087
           FROM pmaa_t
          WHERE pmaaent = g_enterprise
            AND pmaa001 = g_pmee_m.pmee004
         IF cl_null(g_pmee_m.pmee002) THEN
            LET g_pmee_m.pmee002 = l_pmaa086
         END IF
         
         IF cl_null(g_pmee_m.pmee003) THEN
            LET g_pmee_m.pmee003 = l_pmaa087
         END IF
      END IF
      
      IF NOT cl_null(g_pmee_m.pmee002) AND cl_null(g_pmee_m.pmee003) THEN
         SELECT ooag003 INTO g_pmee_m.pmee003
           FROM ooag_t 
          WHERE ooagent = g_enterprise 
            AND ooag001 = g_pmee_m.pmee002
      END IF
      
      IF cl_null(g_pmee_m.pmee002) THEN
         LET g_pmee_m.pmee002 = l_pmee002_o
      END IF
      
      IF cl_null(g_pmee_m.pmee003) THEN
         LET g_pmee_m.pmee003 = l_pmee003_o
      END IF
      
      CALL s_desc_get_person_desc(g_pmee_m.pmee002) RETURNING g_pmee_m.pmee002_desc
      CALL s_desc_get_department_desc(g_pmee_m.pmee003) RETURNING g_pmee_m.pmee003_desc
      
      DISPLAY BY NAME g_pmee_m.pmee002,g_pmee_m.pmee002_desc,
                      g_pmee_m.pmee003,g_pmee_m.pmee003_desc
      #2015/03/04 by stellar add ----- (E)
      
      #取得銷售分類
      LET g_pmab039 = g_pmee_m.pmee024
      
      #取稅率、單價含稅否
      IF NOT cl_null(g_pmee_m.pmee011) THEN
         SELECT oodb006,oodb005 
           INTO g_pmee_m.pmee012,g_pmee_m.pmee013 FROM oodb_t,ooef_t
           WHERE ooefent = oodbent AND ooef001 = g_site AND ooef019 = oodb001
             AND oodbent = g_enterprise AND oodb002 = g_pmee_m.pmee011
      END IF   
      #取匯率
      IF NOT cl_null(g_pmee_m.pmee015) THEN
         #根據內外購獲取當前營運據點參數設置的汇率类型
         LET l_scc40 = ''
         IF g_pmee_m.pmee054 = '1' THEN   #內購
            LET l_scc40 = cl_get_para(g_enterprise,g_site,'S-BAS-0014')
         END IF
         IF g_pmee_m.pmee054 = '2' THEN   #外購
            LET l_scc40 = cl_get_para(g_enterprise,g_site,'S-BAS-0015')
         END IF
         LET l_ooef016 = ''
         SELECT ooef016 INTO l_ooef016 FROM ooef_t WHERE ooefent = g_enterprise AND ooef001 = g_site
         IF NOT cl_null(l_scc40) THEN
            CALL s_aooi160_get_exrate('1',g_site,g_today,g_pmee_m.pmee015,l_ooef016,0,l_scc40) RETURNING g_pmee_m.pmee016
         END IF
      END IF      

      CALL apmt510_pmee009_ref()
      CALL apmt510_pmee010_ref()
      CALL apmt510_pmee011_ref()
      CALL apmt510_pmee015_ref()

      CALL apmt510_pmee017_ref()
      CALL apmt510_pmee023_ref()
      CALL apmt510_pmee024_ref(g_pmee_m.pmee024) RETURNING g_pmee_m.pmee024_desc
      CALL apmt510_pmee033_ref()
      CALL apmt510_pmee025_ref()
      CALL apmt510_pmee026_ref()
      #組合地址
      CALL apmt510_pmee025_get_address()
      CALL apmt510_pmee026_get_address()                 
      
      DISPLAY BY NAME g_pmee_m.pmee009,     g_pmee_m.pmee009_desc,g_pmee_m.pmee010,     g_pmee_m.pmee010_desc,g_pmee_m.pmee011,g_pmee_m.pmee011_desc,
                      g_pmee_m.pmee012,     g_pmee_m.pmee013,     g_pmee_m.pmee015,     g_pmee_m.pmee015_desc,g_pmee_m.pmee016,g_pmee_m.pmee017,
                      g_pmee_m.pmee017_desc,g_pmee_m.pmee020,     g_pmee_m.pmee020_desc,g_pmee_m.pmee023,     g_pmee_m.pmee023_desc,
                      g_pmee_m.pmee024,     g_pmee_m.pmee024_desc,g_pmee_m.pmee033,     g_pmee_m.pmee033_desc

      #抓取交易對象夥伴關係檔且交易類型為"1:收/付款對象"的交易對象顯示在採購單上的
      #[C:帳款供應商]，若有設置多筆收/付款交易對象時，則取有勾選主要的交易對象那一個，
      #若沒有設交易對象夥伴關係檔且交易類型為"1:收/付款對象"的交易對象時，則[C:帳款供應商]預設採購供應商
      LET g_pmee_m.pmee021 = ''
      LET g_pmee_m.pmee021_desc = ''
      SELECT pmac002 INTO g_pmee_m.pmee021 FROM pmac_t 
       WHERE pmacent = g_enterprise AND pmac001 = g_pmee_m.pmee004 AND pmac003 = '1' AND pmacstus = 'Y' AND pmac004 = 'Y'
      IF cl_null(g_pmee_m.pmee021) THEN
         SELECT pmac002 INTO g_pmee_m.pmee021 FROM pmac_t 
          WHERE pmacent = g_enterprise AND pmac001 = g_pmee_m.pmee004 AND pmac003 = '1' AND pmacstus = 'Y' AND rownum = 1
         IF cl_null(g_pmee_m.pmee021) THEN
            LET g_pmee_m.pmee021 = g_pmee_m.pmee004
         END IF
      END IF

      CALL apmt510_pmee004_ref(g_pmee_m.pmee021) RETURNING g_pmee_m.pmee021_desc
      DISPLAY BY NAME g_pmee_m.pmee021,g_pmee_m.pmee021_desc      
      
      
      #抓取交易對象夥伴關係檔且交易類型為"2:出貨對象"的交易對象顯示在採購單上的[C:送貨供應商]，
      #若有設置多筆出貨交易對象時，則取有勾選主要的交易對象那一個，
      #若沒有設交易對象夥伴關係檔且交易類型為"2:出貨對象"的交易對象時，則[C:送貨供應商]預設採購供應商
      LET g_pmee_m.pmee022 = ''
      LET g_pmee_m.pmee022_desc = ''
      SELECT pmac002 INTO g_pmee_m.pmee022 FROM pmac_t 
       WHERE pmacent = g_enterprise AND pmac001 = g_pmee_m.pmee004 AND pmac003 = '2' AND pmacstus = 'Y' AND pmac004 = 'Y'
      IF cl_null(g_pmee_m.pmee022) THEN
         SELECT pmac002 INTO g_pmee_m.pmee022 FROM pmac_t 
          WHERE pmacent = g_enterprise AND pmac001 = g_pmee_m.pmee004 AND pmac003 = '2' AND pmacstus = 'Y' AND rownum = 1
         IF cl_null(g_pmee_m.pmee022) THEN
            LET g_pmee_m.pmee022 = g_pmee_m.pmee004
         END IF
      END IF

      CALL apmt510_pmee004_ref(g_pmee_m.pmee022) RETURNING g_pmee_m.pmee022_desc
      DISPLAY BY NAME g_pmee_m.pmee022,g_pmee_m.pmee022_desc   

      #抓取交易對象聯絡人明細檔的聯絡對像識別碼顯示在採購單上的[C:供應商連絡人]，
      #若有設置多個聯絡人時，則取有勾選主要聯絡人的那一個
      LET g_pmee_m.pmee027 = ''
      LET g_pmee_m.pmee027_desc = ''
      SELECT pmaj002 INTO g_pmee_m.pmee027 FROM pmaj_t 
       WHERE pmajent = g_enterprise AND pmaj001 = g_pmee_m.pmee004 AND pmajstus = 'Y' AND pmaj004 = 'Y'
      IF cl_null(g_pmee_m.pmee022) THEN
         SELECT pmaj002 INTO g_pmee_m.pmee027 FROM pmaj_t 
          WHERE pmajent = g_enterprise AND pmaj001 = g_pmee_m.pmee004 AND pmajstus = 'Y' AND pmaj004 = 'Y' AND rownum = 1
      END IF
      
      CALL apmt510_pmee027_ref()
      DISPLAY BY NAME g_pmee_m.pmee027,g_pmee_m.pmee027_desc
      
END FUNCTION]]>
  </point>
  <point name="function.apmt510_pmei023_chk" order="76" ver="11" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
#來源單據中需求數量的檢查
PRIVATE FUNCTION apmt510_pmei023_chk(p_pmee900,p_pmei003,p_pmei004,p_pmei023,p_pmei023_t,p_pmeedocno,p_pmeiseq,p_pmeiseq1)
   DEFINE p_pmee900    LIKE pmee_t.pmee900
   DEFINE p_pmei003    LIKE pmei_t.pmei003
   DEFINE p_pmei004    LIKE pmei_t.pmei004
   DEFINE p_pmei023    LIKE pmei_t.pmei023
   DEFINE p_pmei023_t  LIKE pmei_t.pmei023 
   DEFINE p_pmeedocno  LIKE pmee_t.pmeedocno  #採購變更單號 = 採購單號  
   DEFINE p_pmeiseq    LIKE pmei_t.pmeiseq    #採購項次  
   DEFINE p_pmeiseq1   LIKE pmei_t.pmeiseq1   #項序  
   DEFINE r_success    LIKE type_t.num5
   DEFINE l_pmdb049    LIKE pmdb_t.pmdb049
   DEFINE l_pmdb006    LIKE pmdb_t.pmdb006
   DEFINE l_pmei023_1  LIKE pmei_t.pmei023   #來源單據的最大需求數量
   DEFINE l_pmei023_2  LIKE pmei_t.pmei023   #採購單中已錄入來源單據的數量 
   DEFINE l_pmdp023    LIKE pmdp_t.pmdp023 
   DEFINE l_cnt        LIKE type_t.num5 
   DEFINE l_success    LIKE type_t.num5
   DEFINE l_ooba002    LIKE ooba_t.ooba002
   DEFINE l_rate       LIKE type_t.num5
   DEFINE l_type       LIKE type_t.chr1

   LET r_success = TRUE
   LET l_pmdb049 = ''
   LET l_pmdb006 = ''
   LET l_pmei023_1 = 0
   LET l_pmei023_2 = 0
   IF cl_null(p_pmei023_t) THEN
      LET p_pmei023_t = 0
   END IF

   #ming 20150121 modify ------------------------------------------------------------(S) 
   #不應該看單頭的來源，而是看單身的單號，如果能在請購單中被找到，就可直接更新資料 
   #CASE g_pmee_m.pmee007
   #   WHEN '2'  #請購單
   #      SELECT NVL(pmdb049,0),NVL(pmdb006,0)
   #        INTO l_pmdb049,l_pmdb006
   #        FROM pmdb_t 
   #       WHERE pmdbent = g_enterprise AND pmdbdocno = p_pmei003 AND pmdbseq = p_pmei004
   #        
   #      #ming 20141219 modify -----------------------------------------(S) 
   #      #採購變更單，原檢核本次採購量是否大於請購量 - 轉採購量 – 已轉採購位確認量， 
   #      #此部分要調整不可以大於請購量 - 轉採購量就好
   #      #LET l_pmei023_1 = l_pmdb006  - l_pmdb049  #需求數量-已轉採購量
   #      LET l_pmei023_1 = l_pmdb006
   #      #ming 20141219 modify -----------------------------------------(E) 
   #      
   #      SELECT SUM(pmei023) INTO l_pmei023_2 FROM pmei_t,pmee_t 
   #        WHERE pmeient = pmeeent 
   #          AND pmeidocno = pmeedocno 
   #          AND pmeestus = 'N'
   #          AND pmeient = g_enterprise 
   #          AND pmei900 = p_pmee900
   #          AND pmei003 = p_pmei003
   #          AND pmei004 = p_pmei004
   #      IF cl_null(l_pmei023_2) THEN
   #         LET l_pmei023_2 = 0
   #      END IF
   #      IF p_pmei023 > l_pmei023_1 - l_pmei023_2 + p_pmei023_t THEN
   #         INITIALIZE g_errparam TO NULL
   #         LET g_errparam.code = 'apm-00296'
   #         LET g_errparam.extend = p_pmei004
   #         LET g_errparam.popup = TRUE
   #         CALL cl_err()
   #      
   #         LET r_success = FALSE
   #         RETURN r_success
   #      END IF
   #      
   #      #採購變更單修改請購單的數量時，要即時回寫請購單的已轉採購量   
   #
   #      #參考原本確認段的邏輯  
   #      #抓取原本的需求數量 
   #      LET l_pmdp023 = 0
   #      SELECT pmdp023 INTO l_pmdp023
   #        FROM pmdp_t
   #       WHERE pmdpent   = g_enterprise
   #         AND pmdpdocno = p_pmeedocno
   #         AND pmdpseq   = p_pmeiseq
   #         AND pmdpseq1  = p_pmeiseq1
   #      IF cl_null(l_pmdp023) THEN
   #         LET l_pmdp023 = 0
   #      END IF
   #
   #      UPDATE pmdb_t SET pmdb049 = NVL(pmdb049,0) + p_pmei023 - l_pmdp023
   #       WHERE pmdbent   = g_enterprise
   #         AND pmdbdocno = p_pmei003
   #         AND pmdbseq   = p_pmei004
   #      IF SQLCA.sqlcode THEN
   #         INITIALIZE g_errparam TO NULL
   #         LET g_errparam.extend = 'upd pmdb_t'
   #         LET g_errparam.code   = SQLCA.sqlcode
   #         LET g_errparam.popup  = TRUE
   #         CALL cl_err()
   #
   #         LET r_success = FALSE
   #         RETURN r_success
   #      END IF
   #      
   #  #WHEN '3'  #訂單
   #  #WHEN '4'  #工單
   #  #WHEN '5'  #MRP
   #END CASE
   
   LET l_cnt = 0 
   SELECT COUNT(*) INTO l_cnt 
     FROM pmdb_t 
    WHERE pmdbent   = g_enterprise
      AND pmdbdocno = p_pmei003
      AND pmdbseq   = p_pmei004

   IF l_cnt > 0 THEN  
      #ming 20150202 add ------------------------------------------(S) 
      #修改成不論是否有請採勾稽，採購數量都不控卡 
      #只控卡需求數量，如果有勾稽，則需求數量不可超出容差率 
      #                如果不勾稽，則完全不做需求數量的控卡 

      #取得單別 
      CALL s_aooi200_get_slip(p_pmeedocno) RETURNING l_success,l_ooba002
      #檢查此單別是否有要做請採勾稽 
      IF NOT cl_get_doc_para(g_enterprise,g_site,l_ooba002,'D-BAS-0061') THEN
         RETURN r_success
      END IF

      #取得大於容差率時的處理方式 1.拒絕 2.警告  
      CALL cl_get_doc_para(g_enterprise,g_site,l_ooba002,'D-BAS-0084')
           RETURNING l_type

      #取得最大容差率 
      CALL cl_get_doc_para(g_enterprise,g_site,l_ooba002,'D-BAS-0085')
           RETURNING l_rate
      IF cl_null(l_rate) THEN
         LET l_rate = 0
      END IF
      #ming 20150202 add ------------------------------------------(E) 
   
      #找出請購單的請購數量與已轉採購量 
      SELECT NVL(pmdb049,0),NVL(pmdb006,0)
        INTO l_pmdb049,l_pmdb006
        FROM pmdb_t 
       WHERE pmdbent   = g_enterprise 
         AND pmdbdocno = p_pmei003 
         AND pmdbseq   = p_pmei004
           
      ##採購變更單，原檢核本次採購量是否大於請購量 - 轉採購量 – 已轉採購位確認量， 
      ##此部分要調整不可以大於請購量 - 轉採購量就好
      #LET l_pmei023_1 = l_pmdb006
      #其實原本的邏輯才是對的 
      #ming 20150202 modify ------------------------(S) 
      #LET l_pmei023_1 = l_pmdb006  - l_pmdb049  #需求數量-已轉採購量
      IF l_rate > 0 THEN
         LET l_pmei023_1 = ((1 + l_rate/100) * l_pmdb006) - l_pmdb049
      ELSE
         LET l_pmei023_1 = l_pmdb006  - l_pmdb049  #需求數量-已轉採購量
      END IF
      #ming 20150202 modify ------------------------(E) 
         
      #計算此張請購單是否存在其他未確認的採購變更單之中 所以要排除掉自己這筆 
      SELECT SUM(pmei023) INTO l_pmei023_2 FROM pmei_t,pmee_t
       WHERE pmeient = pmeeent
         AND pmeidocno = pmeedocno
         AND pmeestus = 'N'
         AND pmeient = g_enterprise
         AND pmei900 = p_pmee900
         AND pmei003 = p_pmei003
         AND pmei004 = p_pmei004
         AND pmeedocno != p_pmeedocno
         AND pmeiseq   != p_pmeiseq
         AND pmeiseq1  != p_pmeiseq1
      IF cl_null(l_pmei023_2) THEN
         LET l_pmei023_2 = 0
      END IF
      
      #檢查輸入的值是否大於 (請購量-已轉採購量) - 其他單據的請購需求數量 +此欄位的舊值  
      IF p_pmei023 > l_pmei023_1 - l_pmei023_2 + p_pmei023_t THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'apm-00585'     #採購量不可大於總請購量-已轉採購量-已經登打的採購量！ 
         LET g_errparam.extend = p_pmei004
         LET g_errparam.popup = TRUE
         CALL cl_err()
         
         #ming 20150202 modify --------------------(S) 
         #LET r_success = FALSE
         #RETURN r_success
         IF l_type = '2' THEN
            LET r_success = FALSE
            RETURN r_success
         END IF
         #ming 20150202 modify --------------------(E) 
      END IF
         
      #採購變更單修改請購單的數量時，要即時回寫請購單的已轉採購量   
  
      #ming 20150202 mark --------------------(S) 
      #因為現在是修改完後就會直接回寫請購單上的已轉採購量  
      #所以不能再依採購單上的需求數量做處理，而是應該以更動前的數據做計算  
      ##參考原本確認段的邏輯  
      ##抓取原本的需求數量 
      #LET l_pmdp023 = 0
      #SELECT pmdp023 INTO l_pmdp023
      #  FROM pmdp_t
      # WHERE pmdpent   = g_enterprise
