DEFINE  l_success    LIKE type_t.num5
DEFINE  l_i          LIKE type_t.num5
DEFINE  l_pmao004    LIKE pmao_t.pmao004
   
   LET r_success = TRUE   

   LET l_pmegseq = ''   
   SELECT MAX(pmegseq) INTO l_pmegseq
     FROM pmeg_t
    WHERE pmegent = g_enterprise
      AND pmegdocno = g_pmee_m.pmeedocno
      AND pmeg900 = g_pmee_m.pmee900
   IF cl_null(l_pmegseq) THEN
      LET l_pmegseq = 0
   END IF   

   #因為第一筆資料已呈現在畫面並寫入DB, 從第二筆開始處理
   FOR l_i = 2 TO g_inam.getLength()
 
      LET l_pmeg_d.* = g_pmeg_d[l_ac].*
      LET l_pmeg3_d.* = g_pmeg3_d[l_ac].*  
      
      LET l_pmegseq = l_pmegseq + 1             
      LET l_pmeg_d.pmegseq = l_pmegseq

      LET l_pmeg_d.pmeg002 = g_inam[l_i].inam002
      #2015/02/12 by stellar modify ----- (S)
#      LET l_pmeg_d.pmeg011 = g_inam[l_i].inam004
      LET l_pmeg_d.pmeg007 = g_inam[l_i].inam004
      IF NOT cl_null(l_pmeg_d.pmeg008) THEN
         CALL s_aooi250_convert_qty(l_pmeg_d.pmeg001,l_pmeg_d.pmeg006,l_pmeg_d.pmeg008,l_pmeg_d.pmeg007)
              RETURNING l_success,l_pmeg_d.pmeg009
         IF NOT cl_null(l_pmeg_d.pmeg009) THEN
            CALL apmt510_unit_round(l_pmeg_d.pmeg008,l_pmeg_d.pmeg009) 
                 RETURNING l_pmeg_d.pmeg009
         END IF
      END IF
                     
      #若參數有使用計價單位時，則輸入[C:需求數量]時則應自動推算計價數量，
      #[C:計價數量]=[C:需求數量]*[C:單位]與[C:計價單位]換算率
      IF cl_get_para(g_enterprise,g_site,'S-BAS-0019') = "Y" THEN  #整體參數使用採購計價單位
         CALL s_aooi250_convert_qty(l_pmeg_d.pmeg001,l_pmeg_d.pmeg006,l_pmeg_d.pmeg010,l_pmeg_d.pmeg007)
              RETURNING l_success,l_pmeg_d.pmeg011
         IF NOT cl_null(l_pmeg_d.pmeg011) THEN
            CALL apmt510_unit_round(l_pmeg_d.pmeg010,l_pmeg_d.pmeg011) 
                 RETURNING l_pmeg_d.pmeg011
         END IF 
      ELSE 
         LET l_pmeg_d.pmeg011 = l_pmeg_d.pmeg007
      END IF
      #2015/02/12 by stellar modify ----- (E)
      
      LET l_pmeg_d.pmeg024 = 'N'            #多交期
      
      IF cl_null(l_pmeg3_d.pmeg033) THEN LET l_pmeg3_d.pmeg033 = 0 END IF      
      IF cl_null(l_pmeg3_d.pmeg042) THEN LET l_pmeg3_d.pmeg042 = 0 END IF   
      IF cl_null(l_pmeg3_d.pmeg043) THEN LET l_pmeg3_d.pmeg043 = 0 END IF   
      IF cl_null(l_pmeg3_d.pmeg044) THEN LET l_pmeg3_d.pmeg044 = 0 END IF   

    
      #未稅金額、含稅金額、稅額
      CALL s_apmt510_get_amount(g_pmee_m.pmeedocno,g_pmee_m.pmee900,l_pmeg_d.pmegseq,l_pmeg_d.pmeg011,l_pmeg_d.pmeg015,l_pmeg_d.pmeg016,g_pmee_m.pmee015,g_pmee_m.pmee016)
           RETURNING l_pmeg_d.pmeg046,l_pmeg_d.pmeg048,l_pmeg_d.pmeg047
           
      #交易對象料號
      CALL apmt510_get_pmeg027(l_pmeg_d.pmeg001,l_pmeg_d.pmeg002)
           RETURNING l_pmeg3_d.pmeg027
         
      LET l_pmeg_d.pmeg901 = '3'
      
      INSERT INTO pmeg_t
                 (pmegent, pmegdocno,pmeg900, pmegsite, pmeg901,
                  pmegseq, pmeg001,  pmeg002, pmeg004,  pmeg005,
                  pmeg006, pmeg007,  pmeg008, pmeg009,  pmeg024,
                  pmeg012, pmeg013,  pmeg014, pmeg045,  pmeg010,
                  pmeg011, pmeg015,  pmeg016, pmeg017,  pmeg046,
                  pmeg047, pmeg048,  pmeg023, pmeg019,  pmeg020,
                  pmeg052, pmeg021,
                  pmeg022, pmegunit, pmegorga,pmeg049,  
                  pmeg027, pmeg028,  pmeg029, pmeg030,  pmeg025,
                  pmeg026, pmeg031,  pmeg032, pmeg033,  
                  pmeg003, pmeg036,  pmeg037, pmeg038,  pmeg039,
                  pmeg035, pmeg040,  pmeg041, pmeg042,  pmeg043,
                  pmeg044)               
           VALUES(g_enterprise,  g_pmee_m.pmeedocno, g_pmee_m.pmee900, g_pmee_m.pmeesite,l_pmeg_d.pmeg901,
                  l_pmeg_d.pmegseq,l_pmeg_d.pmeg001, l_pmeg_d.pmeg002, l_pmeg_d.pmeg004,l_pmeg_d.pmeg005,
                  l_pmeg_d.pmeg006,l_pmeg_d.pmeg007, l_pmeg_d.pmeg008, l_pmeg_d.pmeg009,l_pmeg_d.pmeg024,
                  l_pmeg_d.pmeg012,l_pmeg_d.pmeg013, l_pmeg_d.pmeg014, l_pmeg_d.pmeg045,l_pmeg_d.pmeg010,
                  l_pmeg_d.pmeg011,l_pmeg_d.pmeg015, l_pmeg_d.pmeg016, l_pmeg_d.pmeg017,l_pmeg_d.pmeg046,
                  l_pmeg_d.pmeg047,l_pmeg_d.pmeg048, l_pmeg_d.pmeg023, l_pmeg_d.pmeg019,l_pmeg_d.pmeg020,
                  l_pmeg_d.pmeg052,l_pmeg_d.pmeg021,
                  l_pmeg_d.pmeg022,l_pmeg_d.pmegunit,l_pmeg_d.pmegorga,l_pmeg_d.pmeg049,
                  l_pmeg3_d.pmeg027,l_pmeg3_d.pmeg028,l_pmeg3_d.pmeg029,l_pmeg3_d.pmeg030,l_pmeg3_d.pmeg025,
                  l_pmeg3_d.pmeg026,l_pmeg3_d.pmeg031,l_pmeg3_d.pmeg032,l_pmeg3_d.pmeg033,
                  l_pmeg3_d.pmeg003,l_pmeg3_d.pmeg036,l_pmeg3_d.pmeg037,l_pmeg3_d.pmeg038,l_pmeg3_d.pmeg039,
                  l_pmeg3_d.pmeg035,l_pmeg3_d.pmeg040,l_pmeg3_d.pmeg041,l_pmeg3_d.pmeg042,l_pmeg3_d.pmeg043,
                  l_pmeg3_d.pmeg044)                           
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "pmeg_t"
         LET g_errparam.popup = TRUE
         CALL cl_err()

         LET r_success = FALSE
         EXIT FOR         
      END IF
      

      CALL s_apmt510_gen_pmeh(g_pmee_m.pmeedocno,l_pmeg_d.pmegseq,g_pmee_m.pmee900,l_pmeg_d.pmeg901)
           RETURNING l_success
      IF NOT l_success THEN
         INITIALIZE g_errparam TO NULL
