            l_pmeg_d.pmeg011,l_pmeg_d.pmeg015,l_pmeg_d.pmeg016,l_pmeg_d.pmeg017,l_pmeg_d.pmeg046,
            l_pmeg_d.pmeg047,l_pmeg_d.pmeg048,l_pmeg_d.pmeg023,l_pmeg_d.pmeg019,l_pmeg_d.pmeg020,
