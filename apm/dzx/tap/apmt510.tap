  <point name="function.apmt510_pmeg001_chk" order="48" ver="3" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#料件檢查
PRIVATE FUNCTION apmt510_pmeg001_chk()
DEFINE r_success     LIKE type_t.num5
DEFINE l_success     LIKE type_t.num5
DEFINE l_flag        LIKE type_t.num5
DEFINE l_ooba002     LIKE ooba_t.ooba002
DEFINE l_pmei007     LIKE pmei_t.pmei007
DEFINE l_pmei008     LIKE pmei_t.pmei008
DEFINE l_n           LIKE type_t.num5
DEFINE l_imaa005     LIKE imaa_t.imaa005

   LET r_success = TRUE

   IF cl_null(g_pmeg_d[l_ac].pmeg001) THEN
      RETURN r_success
   END IF
   
   #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。   
   INITIALIZE g_chkparam.* TO NULL
   
   #設定g_chkparam.*的參數
   LET g_chkparam.arg1 = g_pmeg_d[l_ac].pmeg001
      
   #呼叫檢查存在並帶值的library
   IF cl_chk_exist("v_imaa001") THEN
      IF NOT cl_chk_exist("v_imaf001_1") THEN   
         #檢查失敗時後續處理
         LET r_success = FALSE
         RETURN r_success
      END IF
   ELSE
      LET r_success = FALSE
      RETURN r_success   
   END IF   
   
   #修改料號時需判斷該採購單項次是否已有收貨單，若有則不允許修改料號
   LET l_n = 0
   SELECT COUNT(*) INTO l_n FROM pmdt_t
      WHERE pmdtent = g_enterprise AND pmdt001 = g_pmee_m.pmeedocno AND pmdt002 = g_pmeg_d[l_ac].pmegseq
   IF l_n>0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'apm-00424'
      LET g_errparam.extend = g_pmeg_d[l_ac].pmeg001
      LET g_errparam.popup = TRUE
      CALL cl_err()

      LET r_success = FALSE
      RETURN r_success   
   END IF
                     
   #判斷輸入的料件編號是否在控制組限制的產品範圍內
   CALL s_control_chk_group('3','4',g_user,g_dept,g_pmeg_d[l_ac].pmeg001,'','','','')
        RETURNING l_success,l_flag
   IF NOT l_success THEN
      LET r_success = FALSE
      RETURN r_success
   ELSE
      IF NOT l_flag THEN      #是否存在
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'apm-00265'
         LET g_errparam.extend = g_pmeg_d[l_ac].pmeg001
         LET g_errparam.popup = TRUE
         CALL cl_err()

         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF   
   
   #檢核輸入的料件的生命週期是否在單據別限制範圍內
   CALL s_control_chk_doc('4',g_pmee_m.pmeedocno,g_pmeg_d[l_ac].pmeg001,'','','','')
        RETURNING l_success,l_flag
   IF NOT l_success THEN      #处理状态
      LET r_success = FALSE
      RETURN r_success
   ELSE
      IF NOT l_flag THEN      #是否存在
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'ain-00015'
         LET g_errparam.extend = g_pmeg_d[l_ac].pmeg001
         LET g_errparam.popup = TRUE
         CALL cl_err()

         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF

   #檢核輸入的料件的產品分類是否在單據別限制範圍內
   CALL s_control_chk_doc('5',g_pmee_m.pmeedocno,g_pmeg_d[l_ac].pmeg001,'','','','')
        RETURNING l_success,l_flag
   IF NOT l_success THEN      #处理状态
      LET r_success = FALSE
      RETURN r_success
   ELSE
      IF NOT l_flag THEN      #是否存在
         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF
   
   #判斷是否有使用產品特徵
   CALL apmt510_get_imaa005(g_enterprise,g_pmeg_d[l_ac].pmeg001) RETURNING l_imaa005
   IF cl_null(l_imaa005) OR g_pmeg_d[l_ac].pmeg002 IS NOT NULL THEN
      #若單據別設置是要作請採勾稽時，則修改料號時必須檢核是否與"關聯單據明細"中的來源料號有替代關係，若沒有則不允許修改
      CALL s_aooi200_get_slip(g_pmee_m.pmeedocno) RETURNING l_success,l_ooba002
      IF cl_get_doc_para(g_enterprise,g_site,l_ooba002,'D-BAS-0061') = "Y" THEN
         DECLARE pmei007_cur CURSOR FOR
            SELECT DISTINCT pmei007,pmei008 FROM pmei_t
              WHERE pmeient = g_enterprise AND pmeidocno = g_pmee_m.pmeedocno
                AND pmeiseq = g_pmeg_d[l_ac].pmegseq AND pmei900 = g_pmee_m.pmee900
         FOREACH pmei007_cur INTO l_pmei007,l_pmei008
            IF l_pmei007 <> g_pmeg_d[l_ac].pmeg001 THEN
               IF NOT s_pmaq_chk_replacement(g_pmee_m.pmee004,l_pmei007,g_pmeg_d[l_ac].pmeg001,'2',l_pmei008,g_pmeg_d[l_ac].pmeg002) THEN
                  LET r_success = FALSE
                  RETURN r_success
               END IF
            END IF
         END FOREACH
      END IF
   END IF

   RETURN r_success   
   
END FUNCTION]]>
  </point>
  <point name="function.apmt510_pmeg001_desc" order="49" ver="3" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#根據料件編號、供應商、採購控制組帶出欄位預設值
PRIVATE FUNCTION apmt510_pmeg001_desc()
DEFINE l_n         LIKE type_t.num5
DEFINE l_success   LIKE type_t.num5
DEFINE l_controlno LIKE ooha_t.ooha001
DEFINE l_imaf158   LIKE imaf_t.imaf158
DEFINE l_oodbl004  LIKE oodbl_t.oodbl004  #稅別名稱
DEFINE l_oodb005   LIKE oodb_t.oodb005    #含稅否
DEFINE l_oodb006   LIKE oodb_t.oodb006    #稅率 
DEFINE l_oodb011   LIKE oodb_t.oodb011    #取得稅別類型1:正常稅率2:依料件設定  

   LET l_n = 0
   LET g_pmeg_d[l_ac].pmeg006 = ''
   LET g_pmeg_d[l_ac].pmeg006_desc = ''
   LET g_pmeg_d[l_ac].pmeg010 = ''
   LET g_pmeg_d[l_ac].pmeg010_desc = ''
      
   LET g_pmeg3_d[l_ac].pmeg028 = ''
   LET g_pmeg3_d[l_ac].pmeg028_desc = ''
   LET g_pmeg3_d[l_ac].pmeg029 = ''
   LET g_pmeg3_d[l_ac].pmeg029_desc = ''
   LET g_pmeg3_d[l_ac].pmeg003 = ''
   LET g_pmeg3_d[l_ac].pmeg003_desc = ''
   LET g_pmeg3_d[l_ac].pmeg033 = ''
     
   #先判斷這個供應商是否有設多個當前採購控制組範圍內的供應商預設條件，則開窗，讓user 選擇帶哪一個控制組的資料
   LET l_controlno = ''
   CALL s_control_get_group('4',g_user,g_dept) RETURNING l_success,l_controlno

   IF NOT cl_null(l_controlno) THEN
      SELECT COUNT(*) INTO l_n FROM pmap_t
       WHERE pmapent = g_enterprise AND pmapsite = g_site AND pmap001 = g_pmee_m.pmee004
         AND pmap002 = l_controlno AND pmap003 = g_pmeg_d[l_ac].pmeg001 AND pmap004 = g_pmeg_d[l_ac].pmeg002
      #若採購料件有設置'供應商控制組料件預設條件'(apmi121)時，則需將設置的預設條件值預設到採購單對應欄位
      IF l_n > 0 THEN
         SELECT pmap006,pmap008,pmap009,pmap010,pmap011,pmap012,pmap014,pmap005
           INTO g_pmeg_d[l_ac].pmeg006,g_pmeg_d[l_ac].pmeg010,g_pmeg_d[l_ac].pmegunit,g_pmeg3_d[l_ac].pmeg028,g_pmeg3_d[l_ac].pmeg029,
                g_pmeg3_d[l_ac].pmeg025,g_pmeg3_d[l_ac].pmeg031,g_pmeg3_d[l_ac].pmeg003
           FROM pmdp_t
          WHERE pmdpent = g_enterprise AND pmapsite = g_site AND pmap001 = g_pmee_m.pmee004
            AND pmap002 = l_controlno AND pmap003 = g_pmeg_d[l_ac].pmeg001 AND pmap004 = g_pmeg_d[l_ac].pmeg002
      END IF
   END IF

   IF cl_null(l_controlno) OR l_n = 0 THEN
      #沒有設置'供應商控制組料件預設條件'(apmi121)才改抓料件進銷存檔預設的條件
      SELECT imaf143,imaf144,imaf091,imaf092,imaf157
        INTO g_pmeg_d[l_ac].pmeg006,g_pmeg_d[l_ac].pmeg010,g_pmeg3_d[l_ac].pmeg028,g_pmeg3_d[l_ac].pmeg029,g_pmeg3_d[l_ac].pmeg003
        FROM imaf_t
       WHERE imafent = g_enterprise AND imafsite = g_site AND imaf001 = g_pmeg_d[l_ac].pmeg001
   END IF

   CALL apmt510_pmeg006_ref()
   CALL apmt510_pmeg010_ref()
   CALL apmt510_pmegunit_ref()
      
   #若稅別應用為2.依料件設定，呼叫應用元件帶出稅別、稅率
   IF g_tax_app = '2' THEN
      CALL s_tax_chktype(g_pmee_m.pmeesite,g_pmee_m.pmee004,g_pmeg_d[l_ac].pmeg001,'2',g_pmab039)
           RETURNING l_success,g_pmeg_d[l_ac].pmeg016,g_pmeg_d[l_ac].pmeg017
      IF apmt510_pmee011_chk('2',g_pmeg_d[l_ac].pmeg016) THEN
         #帶出單身稅率
         LET l_oodb006 = ''
         CALL s_tax_chk(g_site,g_pmeg_d[l_ac].pmeg016)
              RETURNING l_success,g_oodbl004,l_oodb005,l_oodb006,l_oodb011
         IF l_success THEN
            LET g_pmeg_d[l_ac].pmeg017 = l_oodb006
         END IF
      ELSE
         #檢查失敗時後續處理
         LET g_pmeg_d[l_ac].pmeg016 = ''
         LET g_pmeg_d[l_ac].pmeg017 = ''
      END IF
   ELSE
      LET g_pmeg_d[l_ac].pmeg016 = g_pmee_m.pmee011
      LET g_pmeg_d[l_ac].pmeg017 = g_pmee_m.pmee012
   END IF
   CALL apmt510_pmee011_ref()
   DISPLAY BY NAME g_pmeg_d[l_ac].pmeg017
      
   #[C:備品率] = [T:料件進銷存檔].[C:採購備品率]  imaf165
   #[C:參考單位] = [T:料件進銷存檔][C:參考單位]   imaf015
   #若[T:料件進銷存檔].[C:接單拆解方式(採購)]的值為'1:自動CKD'或是'2:自動SKD'時，imaf158
   #則[C:子件特性]的值預設'2:CKD'或是'3:SKD'
   LET g_pmeg_d[l_ac].pmeg008 = ''
   LET g_pmeg_d[l_ac].pmeg008_desc = ''
   LET g_pmeg3_d[l_ac].pmeg033 = ''
   LET g_pmeg_d[l_ac].pmeg019 = '1'
   LET l_imaf158 = ''
   SELECT imaf158,imaf165,imaf015 INTO l_imaf158,g_pmeg3_d[l_ac].pmeg033,g_pmeg_d[l_ac].pmeg008 
     FROM imaf_t
    WHERE imafent = g_enterprise AND imafsite = g_site AND imaf001 = g_pmeg_d[l_ac].pmeg001
   IF l_imaf158 = '1' THEN
      LET g_pmeg_d[l_ac].pmeg019 = '2'
   END IF
   IF l_imaf158 = '2' THEN
      LET g_pmeg_d[l_ac].pmeg019 = '3'
   END IF

   CALL apmt510_pmeg008_ref()

   #整體參數使用採購計價單位時:
   #[C:計價單位]=[T:料件據點進銷存檔].[C:採購計價單位] 
   IF cl_get_para(g_enterprise,g_site,'S-BAS-0019') = "Y" THEN
      SELECT imaf144 INTO g_pmeg_d[l_ac].pmeg010 FROM imaf_t
       WHERE imafent = g_enterprise AND imafsite = g_site AND imaf001 = g_pmeg_d[l_ac].pmeg001
   END IF
   CALL apmt510_pmeg010_ref()

   #若採購料件有設置'交易對象對應料號'(apmi070)有供應商料號時，需將對應的料號抓出顯示在
   #[C:供應商料號]欄位上，若有設置多筆對應料號時以有勾選主要對應料那一筆為預設值
   LET g_pmeg3_d[l_ac].pmeg027 = ''
   SELECT pmao004 INTO g_pmeg3_d[l_ac].pmeg027 
     FROM pmao_t
    WHERE pmaoent = g_enterprise AND pmao001 = g_pmee_m.pmee004 AND pmao002 = g_pmeg_d[l_ac].pmeg001
      AND pmao003 = g_pmeg_d[l_ac].pmeg002 AND pmao007 = 'Y'
   IF cl_null(g_pmeg3_d[l_ac].pmeg027) THEN
      SELECT pmao004 INTO g_pmeg3_d[l_ac].pmeg027 FROM pmao_t
       WHERE pmaoent = g_enterprise AND pmao001 = g_pmee_m.pmee004 AND pmao002 = g_pmeg_d[l_ac].pmeg001
         AND pmao003 = g_pmeg_d[l_ac].pmeg002 AND rownum = 1
   END IF      
      
   DISPLAY BY NAME g_pmeg_d[l_ac].pmeg006,g_pmeg_d[l_ac].pmeg006_desc,
                   g_pmeg_d[l_ac].pmeg008,g_pmeg_d[l_ac].pmeg008_desc,
                   g_pmeg_d[l_ac].pmeg010,g_pmeg_d[l_ac].pmeg010_desc,
                   g_pmeg_d[l_ac].pmegunit,g_pmeg_d[l_ac].pmegunit_desc,
                   g_pmeg3_d[l_ac].pmeg028,g_pmeg3_d[l_ac].pmeg028_desc,
                   g_pmeg3_d[l_ac].pmeg029,g_pmeg3_d[l_ac].pmeg029_desc 

   CALL apmt510_get_qcap006()
END FUNCTION]]>
