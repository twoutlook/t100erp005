  FOREACH sel_pmeg015_cur INTO l_pmegseq,l_pmeg001,l_pmeg002,l_pmeg006,l_pmeg007,l_pmeg010,l_pmeg011,l_pmeg016,l_pmeg004,l_pmeg005,l_pmeg901
