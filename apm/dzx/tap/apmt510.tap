  <section id="apmt510.filter" ver="11" status="" src="s" readonly="">
    <![CDATA[#應用 a50 樣板自動產生(Version:5)
#+ filter過濾功能
PRIVATE FUNCTION apmt510_filter()
   #add-point:filter段define
   {<point name="filter.define" edit="s"/>}
   #end add-point   
   #add-point:filter段define
   {<point name="filter.define_customerization" edit="c"/>}
   #end add-point   
   
   #切換畫面
   IF NOT g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",1)
      CALL gfrm_curr.setElementHidden("worksheet",0)
      LET g_main_hidden = 1
   END IF   
 
   LET INT_FLAG = 0
 
   LET g_qryparam.state = 'c'
 
   LET g_wc_filter_t = g_wc_filter.trim()
   LET g_wc_t = g_wc
 
   LET g_wc = cl_replace_str(g_wc, g_wc_filter_t, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON pmeedocno,pmee902,pmee900,pmee001,pmee002,pmee003,pmee004
                          FROM s_browse[1].b_pmeedocno,s_browse[1].b_pmee902,s_browse[1].b_pmee900,s_browse[1].b_pmee001, 
                              s_browse[1].b_pmee002,s_browse[1].b_pmee003,s_browse[1].b_pmee004
 
         BEFORE CONSTRUCT
               DISPLAY apmt510_filter_parser('pmeedocno') TO s_browse[1].b_pmeedocno
            DISPLAY apmt510_filter_parser('pmee902') TO s_browse[1].b_pmee902
            DISPLAY apmt510_filter_parser('pmee900') TO s_browse[1].b_pmee900
            DISPLAY apmt510_filter_parser('pmee001') TO s_browse[1].b_pmee001
            DISPLAY apmt510_filter_parser('pmee002') TO s_browse[1].b_pmee002
            DISPLAY apmt510_filter_parser('pmee003') TO s_browse[1].b_pmee003
            DISPLAY apmt510_filter_parser('pmee004') TO s_browse[1].b_pmee004
      
         #add-point:filter段cs_ctrl
         {<point name="filter.cs_ctrl"/>}
         #end add-point
      
      END CONSTRUCT
 
      #add-point:filter段add_cs
      {<point name="filter.add_cs"/>}
      #end add-point
 
      BEFORE DIALOG
         #add-point:filter段b_dialog
         {<point name="filter.b_dialog"/>}
         #end add-point  
      
      ON ACTION accept
         ACCEPT DIALOG
 
      ON ACTION cancel
         LET INT_FLAG = 1
         EXIT DIALOG 
 
      #交談指令共用ACTION
      &include "common_action.4gl" 
         CONTINUE DIALOG
   
   END DIALOG
 
   IF NOT INT_FLAG THEN
      LET g_wc_filter = "   AND   ", g_wc_filter, "   "
      LET g_wc = g_wc , g_wc_filter
   ELSE
      LET g_wc_filter = g_wc_filter_t
      LET g_wc = g_wc_t
   END IF
 
      CALL apmt510_filter_show('pmeedocno')
   CALL apmt510_filter_show('pmee902')
   CALL apmt510_filter_show('pmee900')
   CALL apmt510_filter_show('pmee001')
   CALL apmt510_filter_show('pmee002')
   CALL apmt510_filter_show('pmee003')
   CALL apmt510_filter_show('pmee004')
 
END FUNCTION
