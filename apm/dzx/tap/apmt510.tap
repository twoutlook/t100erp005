   CLEAR FORM                    
   CALL g_pmeg_d.clear()   
   CALL g_pmeg3_d.clear()  
   CALL g_pmeg4_d.clear()  
   CALL g_pmeg5_d.clear()  
   CALL g_pmeg6_d.clear()  
 
 
   INITIALIZE g_pmee_m.* LIKE pmee_t.*             #DEFAULT 設定
   
   LET g_pmeedocno_t = NULL
   LET g_pmee900_t = NULL
 
   
   LET g_master_insert = FALSE
   
