  <point name="function.apmt510_pmeedocno_pmef" order="22" ver="3" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#依據採購單，帶出採購單對應欄位的值
PRIVATE FUNCTION apmt510_pmeedocno_pmef()
DEFINE l_sql             STRING
DEFINE r_success         LIKE type_t.num5


   LET r_success = TRUE

   IF cl_null(g_pmee_m.pmeedocno) THEN
      RETURN r_success
   END IF  
  
   LET l_sql = " SELECT pmdmsite,pmdm001,pmdm002,'',pmdm003,pmdm004,pmdm005,pmdm006,pmdm007, ",
               "        pmdm008,pmdm009,pmdm014 ",
               "   FROM pmdm_t ",
               "  WHERE pmdment = '",g_enterprise,"' ",
               "    AND pmdmdocno = '",g_pmee_m.pmeedocno,"' ",
               "  ORDER BY pmdm001 "     
   PREPARE sel_pmdm_pre FROM l_sql
   DECLARE sel_pmdm_cur CURSOR FOR sel_pmdm_pre
   
   LET l_ac = 1   
   FOREACH sel_pmdm_cur INTO g_pmeg6_d[l_ac].pmefsite,    g_pmeg6_d[l_ac].pmef001,      g_pmeg6_d[l_ac].pmef002,
                             g_pmeg6_d[l_ac].pmef002_desc,g_pmeg6_d[l_ac].pmef003,      g_pmeg6_d[l_ac].pmef004,      g_pmeg6_d[l_ac].pmef005,
                             g_pmeg6_d[l_ac].pmef006,     g_pmeg6_d[l_ac].pmef007,      g_pmeg6_d[l_ac].pmef008,      g_pmeg6_d[l_ac].pmef009,
                             g_pmeg6_d[l_ac].pmef014
                             
      LET g_pmeg6_d[l_ac].pmefsite = g_pmee_m.pmeesite   
      LET g_pmeg6_d[l_ac].pmef901 = '1'   #未變更
      
      CALL apmt510_pmef002_ref(g_pmeg6_d[l_ac].pmef002) RETURNING g_pmeg6_d[l_ac].pmef002_desc
      DISPLAY BY NAME g_pmeg6_d[l_ac].pmef002_desc

      CALL apmt510_pmef902_ref()

      INSERT INTO pmef_t
                  (pmefent,pmefdocno,pmefsite,pmef001,pmef900,pmef002,pmef003,pmef004,pmef005,
                   pmef006,pmef007  ,pmef008, pmef009,pmef014,pmef901)
            VALUES(g_enterprise,g_pmee_m.pmeedocno,g_pmeg6_d[l_ac].pmefsite,g_pmeg6_d[l_ac].pmef001,g_pmee_m.pmee900,
                   g_pmeg6_d[l_ac].pmef002,g_pmeg6_d[l_ac].pmef003,
                   g_pmeg6_d[l_ac].pmef004,g_pmeg6_d[l_ac].pmef005,g_pmeg6_d[l_ac].pmef006,g_pmeg6_d[l_ac].pmef007,
                   g_pmeg6_d[l_ac].pmef008,g_pmeg6_d[l_ac].pmef009,g_pmeg6_d[l_ac].pmef014,g_pmeg6_d[l_ac].pmef901)
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "pmef_t"
         LET g_errparam.popup = TRUE
         CALL cl_err()

         LET r_success = FALSE
         RETURN r_success
      END IF                   
       
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 9035
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()

         EXIT FOREACH
      END IF      
   END FOREACH         

   CALL g_pmeg6_d.deleteElement(g_pmeg6_d.getLength())
   
   LET g_rec_b = l_ac - 1
   LET l_ac = 0
   FREE sel_pmdm_pre   

   DISPLAY ARRAY g_pmeg6_d TO s_detail6.* ATTRIBUTES(COUNT=g_rec_b)
      BEFORE DISPLAY
         EXIT DISPLAY
   END DISPLAY

   RETURN r_success
               
END FUNCTION]]>
  </point>
