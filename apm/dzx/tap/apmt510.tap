     END IF
          
     CALL s_apmt510_get_price(g_pmee_m.pmee017,g_pmee_m.pmee004,l_pmeg001,l_pmeg002,g_pmee_m.pmee015,
                              l_pmeg016,g_pmee_m.pmee009,g_pmee_m.pmee010,g_pmee_m.pmee023,g_pmee_m.pmeedocno,g_pmee_m.pmee900,
