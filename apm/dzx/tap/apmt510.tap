         LET g_errparam.extend = "pmeh_t"
         LET g_errparam.popup = TRUE
         CALL cl_err()

         LET r_success = FALSE
         EXIT FOR
      END IF
               
      #新增變更歷程檔
      CALL apmt510_pmeg_ins_pmek(l_pmeg_d.*,l_pmeg3_d.*)
           RETURNING r_success
      IF NOT r_success THEN
         EXIT FOR
      END IF
        
   END FOR   
   
   CALL apmt510_b_fill()
   RETURN r_success
   
END FUNCTION]]>
