  <point name="function.apmt820_01_key_chk" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#+
PRIVATE FUNCTION apmt820_01_key_chk()
DEFINE l_n     LIKE type_t.num5

   IF NOT cl_null(g_pmcb_m.pmcb001) AND NOT cl_null(g_pmcb_m.pmcb002) AND NOT cl_null(g_pmcb_m.pmcb003) THEN
      LET l_n = 0 
      SELECT COUNT(*) INTO l_n FROM pmcb_t WHERE pmcbent = g_enterprise
         AND pmcbdocno = p_pmcbdocno
         AND pmcb001 = g_pmcb_m.pmcb001
         AND pmcb002 = g_pmcb_m.pmcb002
         AND pmcb003 = g_pmcb_m.pmcb003
      IF l_n > 0 THEN   
         RETURN FALSE
      END IF
   END IF
   RETURN TRUE
END FUNCTION]]>
