  <point name="function.apmt820_01_get_bottom" order="4" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION apmt820_01_get_bottom()
DEFINE l_rtax001        DYNAMIC ARRAY OF LIKE rtax_t.rtax001
DEFINE l_i    LIKE type_t.num5
DEFINE l_str  STRING
      CALL　s_arti202_search_bottom(g_pmcb_m.pmcb005)　RETURNING l_rtax001
      
      LET l_str = ''
      FOR l_i=1 TO l_rtax001.getLength()
         IF l_i = 1 THEN
            LET l_str = "'",l_rtax001[l_i],"'" 
         ELSE
            LET l_str = l_str,",'",l_rtax001[l_i],"'" 
         END IF         
      END FOR
      RETURN l_str
END FUNCTION]]>
  </point>
  <point name="function.apmt820_01_pmcb005_pmcb006_chk" order="5" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION apmt820_01_pmcb005_pmcb006_chk()
DEFINE r_bottom   STRING
DEFINE l_n    LIKE type_t.num5
DEFINE l_sql  STRING
   IF NOT cl_null(g_pmcb_m.pmcb005) AND NOT cl_null(g_pmcb_m.pmcb006) THEN
      
      CALL apmt820_01_get_bottom() RETURNING r_bottom
      LET l_n = 0 
      LET l_sql = "SELECT COUNT(*) FROM imaa_t  WHERE imaa001 =  '",g_pmcb_m.pmcb006,"'", 
                  "   AND imaa009 IN (",r_bottom,")",
                  "   AND imaaent = ",g_enterprise   
      PREPARE rtax001_pre FROM l_sql  
      EXECUTE rtax001_pre INTO l_n      
      IF l_n = 0 THEN
         RETURN FALSE
      END IF   
   END IF
   RETURN TRUE
END FUNCTION]]>
