  <section id="apmt820_01.modify" ver="5" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION apmt820_01_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point
   
   IF g_pmcb_m.pmcbdocno IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF 
   
   EXECUTE apmt820_01_master_referesh USING g_pmcb_m.pmcbdocno,g_pmcb_m.pmcbseq INTO g_pmcb_m.pmcb009, 
       g_pmcb_m.pmcbdocno,g_pmcb_m.pmcbseq,g_pmcb_m.pmcb001,g_pmcb_m.pmcb002,g_pmcb_m.pmcb003,g_pmcb_m.pmcb004, 
       g_pmcb_m.pmcb005,g_pmcb_m.pmcb006,g_pmcb_m.pmcb007,g_pmcb_m.pmcb008,g_pmcb_m.pmcbacti,g_pmcb_m.pmcb009_desc, 
       g_pmcb_m.pmcb001_desc,g_pmcb_m.pmcb002_desc,g_pmcb_m.pmcb005_desc,g_pmcb_m.pmcb006_desc
 
   ERROR ""
  
   LET g_pmcbdocno_t = g_pmcb_m.pmcbdocno
   LET g_pmcbseq_t = g_pmcb_m.pmcbseq
 
   
   CALL s_transaction_begin()
   
   OPEN apmt820_01_cl USING g_enterprise,g_pmcb_m.pmcbdocno,g_pmcb_m.pmcbseq
   IF STATUS THEN
      CALL cl_err("OPEN apmt820_01_cl:", STATUS, 1)
      CLOSE apmt820_01_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE apmt820_01_master_referesh USING g_pmcb_m.pmcbdocno,g_pmcb_m.pmcbseq INTO g_pmcb_m.pmcb009, 
       g_pmcb_m.pmcbdocno,g_pmcb_m.pmcbseq,g_pmcb_m.pmcb001,g_pmcb_m.pmcb002,g_pmcb_m.pmcb003,g_pmcb_m.pmcb004, 
       g_pmcb_m.pmcb005,g_pmcb_m.pmcb006,g_pmcb_m.pmcb007,g_pmcb_m.pmcb008,g_pmcb_m.pmcbacti,g_pmcb_m.pmcb009_desc, 
       g_pmcb_m.pmcb001_desc,g_pmcb_m.pmcb002_desc,g_pmcb_m.pmcb005_desc,g_pmcb_m.pmcb006_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err("pmcb_t",SQLCA.sqlcode,0)
      CLOSE apmt820_01_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
 
   CALL apmt820_01_show()
   
   WHILE TRUE
      LET g_pmcb_m.pmcbdocno = g_pmcbdocno_t
      LET g_pmcb_m.pmcbseq = g_pmcbseq_t
 
      
      #寫入修改者/修改日期資訊
      
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
 
      CALL apmt820_01_input("u")     #欄位更改
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_pmcb_m.* = g_pmcb_m_t.*
         CALL apmt820_01_show()
         CALL cl_err("",9001,0)
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
 
 
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_pmcb_m.pmcbdocno,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE apmt820_01_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_pmcb_m.pmcbdocno,"U")
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
