       g_pmco2_d[l_ac].pmcp005,g_pmco2_d[l_ac].pmcp005_desc,g_pmco2_d[l_ac].pmcp011,g_pmco2_d[l_ac].pmcp009, 
       g_pmco2_d[l_ac].pmcp009_desc,g_pmco2_d[l_ac].pmcp013,g_pmco2_d[l_ac].pmcp012,g_pmco2_d[l_ac].pmcp012_desc, 
       g_pmco2_d[l_ac].pmcp007,g_pmco2_d[l_ac].pmcp008,g_pmco2_d[l_ac].pmcp008_desc,g_pmco2_d[l_ac].pmcp014 

      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
