  <point name="b_fill.sql_after" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   LET g_sql = "SELECT UNIQUE 'N', ",
               "              pmcosite, t2.ooefl003, pmcodocno,  pmcodocdt, pmco003, ",
               "              pmco004,  pmco001,     t3.ooag011, pmco002,   t4.ooefl003, ",
               "              pmcostus ", 
               "  FROM pmco_t ",
               "       LEFT JOIN pmcp_t  t1 ON t1.pmcpent  = pmcoent AND t1.pmcpdocno = pmcodocno ",
               "       LEFT JOIN ooefl_t t2 ON t2.ooeflent = pmcoent AND t2.ooefl001 = pmcosite AND t2.ooefl002 = '",g_dlang,"' ",
               "       LEFT JOIN ooag_t  t3 ON t3.ooagent  = pmcoent AND t3.ooag001 = pmco001 ",
               "       LEFT JOIN ooefl_t t4 ON t4.ooeflent = pmcoent AND t4.ooefl001 = pmco002 AND t4.ooefl002 = '",g_dlang,"' ",
               " WHERE pmcoent= ? AND 1=1 AND ", ls_wc
 
   LET g_sql = g_sql, cl_sql_add_filter("pmco_t"),
                      " ORDER BY pmco_t.pmcodocno"]]>
  </point>
  <point name="b_fill.sql_before" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   CALL s_aooi500_sql_where(g_prog,'pmcosite') RETURNING l_where]]>
  </point>
  <point name="b_fill2.before_fill2" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[      LET g_sql = "SELECT UNIQUE pmcpseq,     pmcpsite,    pmcp006,     t1.inayl003, t7.ooefl003, ",
                  "              pmcp002,     pmcp001,     t2.imaal003, t2.imaal004, pmcp003, ",
                  "              pmcp004,     pmcp005,     t3.oocal003, pmcp011,     pmcp009, ",
                  "              t4.oocal003, pmcp013,     pmcp012,     t5.pmaal005, pmcp007, ",
                  "              pmcp008,     t6.oocql004, pmcp014 ",
                  "  FROM pmcp_t ",
                  "       LEFT JOIN inayl_t t1 ON t1.inaylent = pmcpent AND t1.inayl001 = pmcp006 AND t1.inayl002 = '",g_dlang,"' ",
                  "       LEFT JOIN imaal_t t2 ON t2.imaalent = pmcpent AND t2.imaal001 = pmcp001 AND t2.imaal002 = '",g_dlang,"' ",
                  "       LEFT JOIN oocal_t t3 ON t3.oocalent = pmcpent AND t3.oocal001 = pmcp005 AND t3.oocal002 = '",g_dlang,"' ",
                  "       LEFT JOIN oocal_t t4 ON t4.oocalent = pmcpent AND t4.oocal002 = pmcp009 AND t4.oocal002 = '",g_dlang,"' ",
                  "       LEFT JOIN pmaal_t t5 ON t5.pmaalent = pmcpent AND t5.pmaal001 = pmcp012 AND t5.pmaal002 = '",g_dlang,"' ",
                  "       LEFT JOIN oocql_t t6 ON t6.oocqlent = pmcpent AND t6.oocql001='274'     AND t6.oocql002 = pmcp008 AND t6.oocql003 = '",g_dlang,"' ",
                  "       LEFT JOIN ooefl_t t7 ON t7.ooeflent = pmcpent AND t7.ooefl002 = pmcpsite AND t7.ooefl002 = '",g_dlang,"' ",
                  " WHERE pmcpent=? AND pmcpdocno=?"
  
      IF NOT cl_null(g_wc2_table2) THEN
         LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
      END IF
      LET g_sql = g_sql, " ORDER BY pmcp_t.pmcpseq"]]>
  </point>
