  <section id="apmq837.b_fill" ver="2" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION apmq837_b_fill()
   DEFINE ls_wc           STRING
   DEFINE l_pid           LIKE type_t.chr50
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準
   {<point name="b_fill.define" edit="s"/>}
   #end add-point
   #add-point:b_fill段define-客製
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
 
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
 
   CALL g_pmco_d.clear()
 
   #add-point:陣列清空
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1
 
   # b_fill段sql組成及FOREACH撰寫
   #應用 qs04 樣板自動產生(Version:4)
   #+ b_fill段資料取得(包含sql組成及FOREACH段撰寫)
   LET ls_sql_rank = "SELECT  UNIQUE '',pmcosite,'',pmcodocno,pmcodocdt,pmco003,pmco004,pmco001,'',pmco002, 
       '',''  ,DENSE_RANK() OVER( ORDER BY pmco_t.pmcodocno) AS RANK FROM pmco_t",
 
#table2
                     " LEFT JOIN pmcp_t ON pmcpent = pmcoent AND pmcodocno = pmcpdocno",
 
                     "",
                     " WHERE pmcoent= ? AND 1=1 AND ", ls_wc
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("pmco_t"),
                     " ORDER BY pmco_t.pmcodocno"
 
   #add-point:b_fill段rank_sql_after
   {<point name="b_fill.rank_sql_after"/>}
   #end add-point
 
   LET g_sql = "SELECT COUNT(*) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql  #總筆數
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT '',pmcosite,'',pmcodocno,pmcodocdt,pmco003,pmco004,pmco001,'',pmco002,'',''",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE apmq837_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR apmq837_pb
 
   OPEN b_fill_curs USING g_enterprise
 
   FOREACH b_fill_curs INTO g_pmco_d[l_ac].sel,g_pmco_d[l_ac].pmcosite,g_pmco_d[l_ac].pmcosite_desc, 
       g_pmco_d[l_ac].pmcodocno,g_pmco_d[l_ac].pmcodocdt,g_pmco_d[l_ac].pmco003,g_pmco_d[l_ac].pmco004, 
       g_pmco_d[l_ac].pmco001,g_pmco_d[l_ac].pmco001_desc,g_pmco_d[l_ac].pmco002,g_pmco_d[l_ac].pmco002_desc, 
       g_pmco_d[l_ac].pmcostus
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
 
      
 
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL apmq837_detail_show("'1'")
 
      CALL apmq837_pmco_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
 
   END FOREACH
 
 
 
 
 
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   CALL g_pmco_d.deleteElement(g_pmco_d.getLength())
 
   #add-point:陣列長度調整
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
 
   LET g_error_show = 0
 
   LET g_detail_cnt = g_pmco_d.getLength()
   LET l_ac = g_cnt
   LET g_cnt = 0
 
   #應用 qs06 樣板自動產生(Version:2)
   #+ b_fill段CURSOR釋放
   CLOSE b_fill_curs
   FREE apmq837_pb
 
 
 
 
 
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL apmq837_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL apmq837_detail_action_trans()
 
   LET l_ac = 1
   CALL apmq837_b_fill2()
 
      CALL apmq837_filter_show('pmcosite','b_pmcosite')
   CALL apmq837_filter_show('pmcodocno','b_pmcodocno')
   CALL apmq837_filter_show('pmcodocdt','b_pmcodocdt')
   CALL apmq837_filter_show('pmco003','b_pmco003')
   CALL apmq837_filter_show('pmco004','b_pmco004')
   CALL apmq837_filter_show('pmco001','b_pmco001')
   CALL apmq837_filter_show('pmco002','b_pmco002')
   CALL apmq837_filter_show('pmcpseq','b_pmcpseq')
   CALL apmq837_filter_show('pmcpsite','b_pmcpsite')
   CALL apmq837_filter_show('pmcp006','b_pmcp006')
   CALL apmq837_filter_show('pmcp002','b_pmcp002')
   CALL apmq837_filter_show('pmcp001','b_pmcp001')
   CALL apmq837_filter_show('pmcp003','b_pmcp003')
   CALL apmq837_filter_show('pmcp004','b_pmcp004')
   CALL apmq837_filter_show('pmcp005','b_pmcp005')
   CALL apmq837_filter_show('pmcp011','b_pmcp011')
   CALL apmq837_filter_show('pmcp009','b_pmcp009')
   CALL apmq837_filter_show('pmcp013','b_pmcp013')
   CALL apmq837_filter_show('pmcp012','b_pmcp012')
   CALL apmq837_filter_show('pmcp007','b_pmcp007')
   CALL apmq837_filter_show('pmcp008','b_pmcp008')
   CALL apmq837_filter_show('pmcp014','b_pmcp014')
 
 
END FUNCTION
]]>
  </section>
