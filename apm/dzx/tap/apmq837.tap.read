                  IF NOT ap_chk_notDup("","SELECT COUNT(*) FROM pmcp_t WHERE "||"pmcpent = '" ||g_enterprise|| "' AND "||"pmcpdocno = '"||g_pmco_d[g_detail_idx].pmcodocno ||"' AND "|| "pmcpseq = '"||g_pmco2_d[g_detail_idx2].pmcpseq ||"'",'std-00004',0) THEN 
