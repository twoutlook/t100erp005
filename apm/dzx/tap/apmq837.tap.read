  <point name="input.a.page1.pmcodocno" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[            #應用 a05 樣板自動產生(Version:2)
            #確認資料無重複
            IF  g_pmco_d[g_detail_idx].pmcodocno IS NOT NULL THEN 
               IF l_cmd = 'a' OR ( l_cmd = 'u' AND (g_pmco_d[g_detail_idx].pmcodocno != g_pmco_d_t.pmcodocno)) THEN 
                  IF NOT ap_chk_notDup("","SELECT COUNT(*) FROM pmco_t WHERE "||"pmcoent = '" ||g_enterprise|| "' AND "||"pmcodocno = '"||g_pmco_d[g_detail_idx].pmcodocno ||"'",'std-00004',0) THEN 
                     NEXT FIELD CURRENT
                  END IF
               END IF
            END IF

]]>
