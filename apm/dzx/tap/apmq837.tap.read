  <point name="menu.output" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[               LET l_cmd = ''
               LET l_where = ''
               FOR li_idx = 1 TO g_pmco_d.getLength()
                  IF g_pmco_d[li_idx].sel = "Y" THEN
                     IF cl_null(l_cmd) THEN
                        LET l_cmd = "'",g_pmco_d[li_idx].pmcodocno,"'"
                     ELSE
                        LET l_cmd = "'",g_pmco_d[li_idx].pmcodocno,"',",l_cmd CLIPPED
                     END IF
                  END IF
