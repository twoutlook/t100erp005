  <section id="apmi900.modify" ver="3" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION apmi900_modify()
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point  
   #add-point:modify段define
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point    
   
   IF g_pmen_m.pmensite IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
   LET g_pmensite_t = g_pmen_m.pmensite
 
   CALL s_transaction_begin()
   
   OPEN apmi900_cl USING g_enterprise,g_pmen_m.pmensite
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN apmi900_cl:" 
      LET g_errparam.code   =  STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE apmi900_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE apmi900_master_referesh USING g_pmen_m.pmensite INTO g_pmen_m.pmensite,g_pmen_m.pmensite_desc 

   
   #遮罩相關處理
   LET g_pmen_m_mask_o.* =  g_pmen_m.*
   CALL apmi900_pmen_t_mask()
   LET g_pmen_m_mask_n.* =  g_pmen_m.*
   
   CALL s_transaction_end('Y','0')
 
   CALL apmi900_show()
   WHILE TRUE
      LET g_pmensite_t = g_pmen_m.pmensite
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL apmi900_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_pmen_m.* = g_pmen_m_t.*
         CALL apmi900_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更(更新單身table的key欄位值)
      IF g_pmen_m.pmensite != g_pmensite_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("modify,modify_detail,delete,reproduce", TRUE)
   CALL apmi900_set_act_visible()
   CALL apmi900_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " pmenent = '" ||g_enterprise|| "' AND",
                      " pmensite = '", g_pmen_m.pmensite, "' "
 
   #填到對應位置
   CALL apmi900_browser_fill("")
 
   CALL apmi900_idx_chk()
 
   CLOSE apmi900_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL apmi900_msgcentre_notify('modify')
   
END FUNCTION   
]]>
  </section>
