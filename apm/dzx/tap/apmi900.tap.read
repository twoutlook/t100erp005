  <point name="function.apmi900_pmenstus" order="4" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 檢查碼頭編號是否存在
# Memo...........:
# Usage..........: CALL apmi900_pmenstus()
# Input parameter: 無
# Return code....: TRUE/FALSE
# Date & Author..: 2015/02/09 By sakura
# Modify.........:
################################################################################
PRIVATE FUNCTION apmi900_pmenstus()
   DEFINE  l_cnt      LIKE type_t.num5
   DEFINE  l_success  LIKE type_t.num5
   DEFINE  l_errno    STRING

   LET l_cnt = 0
   LET l_success = TRUE
   LET l_errno = NULL
   
   IF g_pmen_d[l_ac].pmenstus='N' THEN
      SELECT COUNT(*) INTO l_cnt 
       FROM pmel_t 
        WHERE pmelent = g_enterprise
          AND pmel009 = g_pmen_d[l_ac].pmen001
      IF l_cnt >= 1 THEN
         LET l_errno = "apm-00825"
         LET l_success = FALSE
         RETURN l_success,l_errno         
      END IF
   END IF
   RETURN l_success,l_errno
END FUNCTION]]>
  </point>
