  <section id="apmi900.b_fill" ver="12" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION apmi900_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define" edit="s"/>}
   #end add-point     
   #add-point:b_fill段define
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
   
   #先清空單身變數內容
   CALL g_pmen_d.clear()
   CALL g_pmen2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE pmenstus,pmenunit,pmen001,pmen002,pmen009,pmen003,pmen004,pmen005,pmen006, 
       pmen007,pmen008,pmen001,pmenownid,pmenowndp,pmencrtid,pmencrtdp,pmencrtdt,pmenmodid,pmenmoddt, 
       t1.ooefl003 ,t2.inayl003 ,t3.mhadl006 ,t4.mhacl005 ,t5.mhabl004 ,t6.mhaal003 ,t7.ooag011 ,t8.ooefl003 , 
       t9.ooag011 ,t10.ooefl003 ,t11.ooag011 FROM pmen_t",   
               " LEFT JOIN pmenl_t ON pmenlent = '"||g_enterprise||"' AND pmensite = pmenlsite AND pmen001 = pmenl001 AND pmenl002 = '",g_dlang,"'",
               
                              " LEFT JOIN ooefl_t t1 ON t1.ooeflent='"||g_enterprise||"' AND t1.ooefl001=pmenunit AND t1.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN inayl_t t2 ON t2.inaylent='"||g_enterprise||"' AND t2.inayl001=pmen004 AND t2.inayl002='"||g_dlang||"' ",
               " LEFT JOIN mhadl_t t3 ON t3.mhadlent='"||g_enterprise||"' AND t3.mhadl001=pmen008 AND t3.mhadl002=pmen007 AND t3.mhadl003=pmen006 AND t3.mhadl004=pmen005 AND t3.mhadl005='"||g_dlang||"' ",
               " LEFT JOIN mhacl_t t4 ON t4.mhaclent='"||g_enterprise||"' AND t4.mhacl001=pmen007 AND t4.mhacl002=pmen008 AND t4.mhacl003=pmen006 AND t4.mhacl004='"||g_dlang||"' ",
               " LEFT JOIN mhabl_t t5 ON t5.mhablent='"||g_enterprise||"' AND t5.mhabl001=pmen008 AND t5.mhabl002=pmen007 AND t5.mhabl003='"||g_dlang||"' ",
               " LEFT JOIN mhaal_t t6 ON t6.mhaalent='"||g_enterprise||"' AND t6.mhaal001=pmen008 AND t6.mhaal002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent='"||g_enterprise||"' AND t7.ooag001=pmenownid  ",
               " LEFT JOIN ooefl_t t8 ON t8.ooeflent='"||g_enterprise||"' AND t8.ooefl001=pmenowndp AND t8.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t9 ON t9.ooagent='"||g_enterprise||"' AND t9.ooag001=pmencrtid  ",
               " LEFT JOIN ooefl_t t10 ON t10.ooeflent='"||g_enterprise||"' AND t10.ooefl001=pmencrtdp AND t10.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t11 ON t11.ooagent='"||g_enterprise||"' AND t11.ooag001=pmenmodid  ",
 
               " WHERE pmenent= ? AND pmensite=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("pmen_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF apmi900_fill_chk(1) THEN
      IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
         LET g_sql = g_sql, " ORDER BY pmen_t.pmen001"
         #add-point:b_fill段fill_before
         {<point name="b_fill.fill_before"/>}
         #end add-point
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE apmi900_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR apmi900_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_pmen_m.pmensite
                                               
      FOREACH b_fill_cs INTO g_pmen_d[l_ac].pmenstus,g_pmen_d[l_ac].pmenunit,g_pmen_d[l_ac].pmen001, 
          g_pmen_d[l_ac].pmen002,g_pmen_d[l_ac].pmen009,g_pmen_d[l_ac].pmen003,g_pmen_d[l_ac].pmen004, 
          g_pmen_d[l_ac].pmen005,g_pmen_d[l_ac].pmen006,g_pmen_d[l_ac].pmen007,g_pmen_d[l_ac].pmen008, 
