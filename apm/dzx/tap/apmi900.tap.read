  <section id="apmi900.insert" ver="3" status="" src="s" readonly="">
    <![CDATA[#+ 資料新增
PRIVATE FUNCTION apmi900_insert()
   #add-point:insert段define
   {<point name="insert.define" edit="s"/>}
   #end add-point
   #add-point:insert段define
   {<point name="insert.define_customerization" edit="c"/>}
   #end add-point   
   
   #add-point:insert段before
   {<point name="insert.before"/>}
   #end add-point    
   
   #清除相關資料
   CLEAR FORM                    
   CALL g_pmen_d.clear()
   CALL g_pmen2_d.clear()
 
 
   INITIALIZE g_pmen_m.* LIKE pmen_t.*             #DEFAULT 設定
   LET g_pmensite_t = NULL
 
   LET g_master_insert = FALSE
   CALL s_transaction_begin()
   WHILE TRUE
     
      #單頭預設值
      
     
      #add-point:單頭預設值
      {<point name="insert.default"/>}
      #end add-point 
 
      CALL apmi900_input("a")
      
      #add-point:單頭輸入後
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG AND NOT g_master_insert THEN
         LET INT_FLAG = 0
         LET g_pmen_m.* = g_pmen_m_t.*
         CALL apmi900_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
         DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
         INITIALIZE g_pmen_m.* TO NULL
         INITIALIZE g_pmen_d TO NULL
         INITIALIZE g_pmen2_d TO NULL
 
         CALL apmi900_show()
         RETURN
      END IF
    
      #CALL g_pmen_d.clear()
      #CALL g_pmen2_d.clear()
 
      
      #add-point:單頭輸入後2
      {<point name="insert.after_insert2"/>}
      #end add-point
 
      LET g_rec_b = 0
      EXIT WHILE
      
   END WHILE
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("modify,modify_detail,delete,reproduce", TRUE)
   CALL apmi900_set_act_visible()
   CALL apmi900_set_act_no_visible()
 
   #將新增的資料併入搜尋條件中
   LET g_state = "insert"
   
   LET g_pmensite_t = g_pmen_m.pmensite
 
   
   #組合新增資料的條件
   LET g_add_browse = " pmenent = '" ||g_enterprise|| "' AND",
                      " pmensite = '", g_pmen_m.pmensite, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL apmi900_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   CALL apmi900_idx_chk()
   
   #撈取異動後的資料(主要是帶出reference)
   EXECUTE apmi900_master_referesh USING g_pmen_m.pmensite INTO g_pmen_m.pmensite,g_pmen_m.pmensite_desc 

   
   #遮罩相關處理
   LET g_pmen_m_mask_o.* =  g_pmen_m.*
   CALL apmi900_pmen_t_mask()
   LET g_pmen_m_mask_n.* =  g_pmen_m.*
   
   #將資料顯示到畫面上
   DISPLAY BY NAME g_pmen_m.pmensite,g_pmen_m.pmensite_desc
   
   #功能已完成,通報訊息中心
   CALL apmi900_msgcentre_notify('insert')
   
END FUNCTION
]]>
