  <section id="apmi900.reproduce" ver="4" status="" src="s" readonly="">
    <![CDATA[#+ 資料複製
PRIVATE FUNCTION apmi900_reproduce()
   DEFINE l_newno     LIKE pmen_t.pmensite 
   DEFINE l_oldno     LIKE pmen_t.pmensite 
 
   DEFINE l_master    RECORD LIKE pmen_t.*
   DEFINE l_detail    RECORD LIKE pmen_t.*
 
   DEFINE l_cnt       LIKE type_t.num10
   #add-point:reproduce段define
   {<point name="reproduce.define" edit="s"/>}
   #end add-point
   #add-point:reproduce段define
   {<point name="reproduce.define_customerization" edit="c"/>}
   #end add-point
 
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF     
 
   IF g_pmen_m.pmensite IS NULL
 
      THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
   
   LET g_pmensite_t = g_pmen_m.pmensite
 
   
   LET g_pmen_m.pmensite = ""
 
   LET g_master_insert = FALSE
   CALL apmi900_set_entry('a')
   CALL apmi900_set_no_entry('a')
   
   CALL cl_set_head_visible("","YES")
   CALL s_transaction_begin()
   
   #add-point:複製輸入前
   {<point name="reproduce.head.b_input"/>}
   #end add-point
   
   #清空key欄位的desc
      LET g_pmen_m.pmensite_desc = ''
   DISPLAY BY NAME g_pmen_m.pmensite_desc
 
   
   CALL apmi900_input("r")
    
   IF INT_FLAG AND NOT g_master_insert THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = '' 
      LET g_errparam.code   = 9001 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
      DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
      INITIALIZE g_pmen_m.* TO NULL
      INITIALIZE g_pmen_d TO NULL
      INITIALIZE g_pmen2_d TO NULL
 
      CALL apmi900_show()
      LET INT_FLAG = 0
      RETURN 
   END IF
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("modify,modify_detail,delete,reproduce", TRUE)
   CALL apmi900_set_act_visible()
   CALL apmi900_set_act_no_visible()
 
   #將新增的資料併入搜尋條件中
   LET g_state = "insert"
   
   LET g_pmensite_t = g_pmen_m.pmensite
 
   
   #組合新增資料的條件
   LET g_add_browse = " pmenent = '" ||g_enterprise|| "' AND",
                      " pmensite = '", g_pmen_m.pmensite, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL apmi900_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   LET g_curr_diag = ui.DIALOG.getCurrent()
   CALL apmi900_idx_chk()
   
   #add-point:完成複製段落後
   {<point name="reproduce.after_reproduce" />}
   #end add-point
   
   #功能已完成,通報訊息中心
   CALL apmi900_msgcentre_notify('reproduce')
   
END FUNCTION
]]>
