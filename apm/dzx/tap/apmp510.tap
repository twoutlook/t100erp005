PRIVATE FUNCTION apmp510_closed_ref(p_closed)
DEFINE p_closed  LIKE pmdn_t.pmdn051
DEFINE r_oocql004 LIKE oocql_t.oocql004
        INITIALIZE g_ref_fields TO NULL
        LET g_ref_fields[1] = p_closed
        CALL ap_ref_array2(g_ref_fields,"SELECT oocql004 FROM oocql_t WHERE oocqlent='"||g_enterprise||"' AND oocql001='258' AND oocql002=? AND oocql003='"||g_dlang||"'","") RETURNING g_rtn_fields
        LET r_oocql004 = '', g_rtn_fields[1] , ''
        RETURN r_oocql004
END FUNCTION]]>
