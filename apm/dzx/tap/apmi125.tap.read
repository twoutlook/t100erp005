  <point name="input.a.page1.pmar002" order="0" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[            CALL apmi125_pmar002_ref(g_pmar_d[l_ac].pmar002) RETURNING g_pmar_d[l_ac].pmar002_desc,g_pmar_d[l_ac].imaal004
            DISPLAY BY NAME g_pmar_d[l_ac].pmar002_desc,g_pmar_d[l_ac].imaal004 

            #此段落由子樣板a05產生
            IF NOT cl_null(g_pmar_m.pmar000) AND NOT cl_null(g_pmar_m.pmar001) AND NOT cl_null(g_pmar_d[l_ac].pmar002) AND g_pmar_d[l_ac].pmar003 IS NOT NULL AND g_pmar_d[l_ac].pmar004 IS NOT NULL AND g_pmar_d[l_ac].pmar005 IS NOT NULL AND NOT cl_null(g_pmar_d[l_ac].pmar006) AND NOT cl_null(g_pmar_d[l_ac].pmar007) AND NOT cl_null(g_pmar_d[l_ac].pmar009) THEN 
               IF l_cmd = 'a' OR ( l_cmd = 'u' AND (g_pmar_m.pmar000 != g_pmar000_t OR g_pmar_m.pmar001 != g_pmar001_t OR g_pmar_d[l_ac].pmar002 != g_pmar_d_t.pmar002 OR g_pmar_d[l_ac].pmar003 != g_pmar_d_t.pmar003 OR g_pmar_d[l_ac].pmar004 != g_pmar_d_t.pmar004 OR g_pmar_d[l_ac].pmar005 != g_pmar_d_t.pmar005 OR g_pmar_d[l_ac].pmar006 != g_pmar_d_t.pmar006 OR g_pmar_d[l_ac].pmar007 != g_pmar_d_t.pmar007 OR g_pmar_d[l_ac].pmar009 != g_pmar_d_t.pmar009)) THEN 
                  IF NOT ap_chk_notDup("","SELECT COUNT(*) FROM pmar_t WHERE "||"pmarent = '" ||g_enterprise|| "' AND pmarsite = '" ||g_site|| "' AND "||"pmar000 = '"||g_pmar_m.pmar000 ||"' AND "|| "pmar001 = '"||g_pmar_m.pmar001 ||"' AND "|| "pmar002 = '"||g_pmar_d[l_ac].pmar002 ||"' AND "|| "pmar003 = '"||g_pmar_d[l_ac].pmar003 ||"' AND "|| "pmar004 = '"||g_pmar_d[l_ac].pmar004 ||"' AND "|| "pmar005 = '"||g_pmar_d[l_ac].pmar005 ||"' AND "|| "pmar006 = '"||g_pmar_d[l_ac].pmar006 ||"' AND "|| "pmar007 = '"||g_pmar_d[l_ac].pmar007 ||"' AND "|| "pmar009 = '"||g_pmar_d[l_ac].pmar009 ||"'",'std-00004',0) THEN 
                     LET g_pmar_d[l_ac].pmar002 = g_pmar_d_t.pmar002
                     CALL apmi125_pmar002_ref(g_pmar_d[l_ac].pmar002) RETURNING g_pmar_d[l_ac].pmar002_desc,g_pmar_d[l_ac].imaal004
                     DISPLAY BY NAME g_pmar_d[l_ac].pmar002_desc,g_pmar_d[l_ac].imaal004
                     NEXT FIELD CURRENT
                  END IF
               END IF
            END IF  

            IF NOT cl_null(g_pmar_d[l_ac].pmar002) THEN 
               #此段落由子樣板a19產生
               #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
               INITIALIZE g_chkparam.* TO NULL
               
               #設定g_chkparam.*的參數
               LET g_chkparam.arg1 = g_pmar_d[l_ac].pmar002
                
               #呼叫檢查存在並帶值的library
               IF cl_chk_exist("v_imaa001") THEN
                  IF cl_chk_exist("v_imaf001_1") THEN
                     #檢查成功時後續處理
                     #採購計價單位
                     SELECT imaf144 INTO g_pmar_d[l_ac].pmar006 FROM imaf_t WHERE imafent = g_enterprise AND imafsite = g_site AND imaf001 = g_pmar_d[l_ac].pmar002
                     IF cl_null(g_pmar_d[l_ac].pmar006) THEN
                        #採購單位
                        SELECT imaf143 INTO g_pmar_d[l_ac].pmar006 FROM imaf_t WHERE imafent = g_enterprise AND imafsite = g_site AND imaf001 = g_pmar_d[l_ac].pmar002
                     END IF
                     CALL apmi125_pmar006_ref(g_pmar_d[l_ac].pmar006) RETURNING g_pmar_d[l_ac].pmar006_desc
                     DISPLAY BY NAME g_pmar_d[l_ac].pmar006_desc
                     
                     #料件不使用產品特徵時，產品特徵欄位值給空白' '
                     SELECT imaa005 INTO l_imaa005 FROM imaa_t WHERE imaaent = g_enterprise AND imaa001 = g_pmar_d[l_ac].pmar002
                     IF cl_null(l_imaa005) THEN
                        LET g_pmar_d[l_ac].pmar003 = ' '
                     END IF
                  ELSE
                     LET g_pmar_d[l_ac].pmar002 = g_pmar_d_t.pmar002
                     CALL apmi125_pmar002_ref(g_pmar_d[l_ac].pmar002) RETURNING g_pmar_d[l_ac].pmar002_desc,g_pmar_d[l_ac].imaal004
                     DISPLAY BY NAME g_pmar_d[l_ac].pmar002_desc,g_pmar_d[l_ac].imaal004
                     NEXT FIELD CURRENT
                  END IF
               ELSE
                  #檢查失敗時後續處理
                  LET g_pmar_d[l_ac].pmar002 = g_pmar_d_t.pmar002
                  CALL apmi125_pmar002_ref(g_pmar_d[l_ac].pmar002) RETURNING g_pmar_d[l_ac].pmar002_desc,g_pmar_d[l_ac].imaal004
                  DISPLAY BY NAME g_pmar_d[l_ac].pmar002_desc,g_pmar_d[l_ac].imaal004
                  NEXT FIELD CURRENT
               END IF
               CALL apmi125_set_entry_b(l_cmd)
               CALL apmi125_set_no_entry_b(l_cmd)
            END IF
            ]]>
