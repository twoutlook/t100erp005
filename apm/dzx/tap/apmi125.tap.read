  <section id="apmi125.b_fill" ver="7" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION apmi125_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_pmar_d.clear()
   CALL g_pmar2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE pmarstus,pmar002,pmar003,pmar004,pmar005,pmar006,pmar007,pmar008,pmar009, 
       pmar010,pmar011,pmar012,pmar013,pmar014,pmar015,pmar016,pmar017,pmar018,pmar019,pmar020,pmar002, 
       pmar003,pmar004,pmar005,pmar006,pmar007,pmar009,pmarownid,pmarowndp,pmarcrtid,pmarcrtdp,pmarcrtdt, 
       pmarmodid,pmarmoddt,t1.imaal003 ,t2.oocql004 ,t3.oocal003 ,t4.ooail003 ,t5.oocal003 ,t6.oofa011 , 
       t7.oofa011 ,t8.ooefl003 ,t9.oofa011 ,t10.ooefl003 ,t11.oofa011 FROM pmar_t",   
               "",
               
                              " LEFT JOIN imaal_t t1 ON t1.imaalent='"||g_enterprise||"' AND t1.imaal001=pmar002 AND t1.imaal002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t2 ON t2.oocqlent='"||g_enterprise||"' AND t2.oocql001='221' AND t2.oocql002=pmar004 AND t2.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t3 ON t3.oocalent='"||g_enterprise||"' AND t3.oocal001=pmar006 AND t3.oocal002='"||g_dlang||"' ",
               " LEFT JOIN ooail_t t4 ON t4.ooailent='"||g_enterprise||"' AND t4.ooail001=pmar007 AND t4.ooail002='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t5 ON t5.oocalent='"||g_enterprise||"' AND t5.oocal001=pmar014 AND t5.oocal002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t6 ON t6.oofaent='"||g_enterprise||"' AND t6.oofa002='2' AND t6.oofa003=pmar020  ",
               " LEFT JOIN oofa_t t7 ON t7.oofaent='"||g_enterprise||"' AND t7.oofa002='2' AND t7.oofa003=pmarownid  ",
               " LEFT JOIN ooefl_t t8 ON t8.ooeflent='"||g_enterprise||"' AND t8.ooefl001=pmarowndp AND t8.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t9 ON t9.oofaent='"||g_enterprise||"' AND t9.oofa002='2' AND t9.oofa003=pmarcrtid  ",
               " LEFT JOIN ooefl_t t10 ON t10.ooeflent='"||g_enterprise||"' AND t10.ooefl001=pmarcrtdp AND t10.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t11 ON t11.oofaent='"||g_enterprise||"' AND t11.oofa002='2' AND t11.oofa003=pmarmodid  ",
 
               " WHERE pmarent= ? AND pmarsite= ? AND pmar000=? AND pmar001=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
