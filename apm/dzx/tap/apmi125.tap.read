  <section id="apmi125.delete" ver="5" status="" src="s">
    <![CDATA[#+ 資料刪除
PRIVATE FUNCTION apmi125_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_pmar_m.pmar000 IS NULL
   OR g_pmar_m.pmar001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE apmi125_master_referesh USING g_pmar_m.pmar000,g_pmar_m.pmar001 INTO g_pmar_m.pmar000,g_pmar_m.pmar001 

   
   CALL apmi125_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN apmi125_cl USING g_enterprise, g_site,g_pmar_m.pmar000
                                                       ,g_pmar_m.pmar001
 
   IF STATUS THEN
      CALL cl_err("OPEN apmi125_cl:", STATUS, 1)
      CLOSE apmi125_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH apmi125_cl INTO g_pmar_m.pmar000,g_pmar_m.pmar001,g_pmar_m.pmaal004,g_pmar_m.pmaal003,g_pmar_m.pmaa003 

   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_pmar_m.pmar000,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL apmi125_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
