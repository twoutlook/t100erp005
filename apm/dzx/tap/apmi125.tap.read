  <section id="apmi125.ui_dialog" ver="9" status="" src="s">
    <![CDATA[#+ 功能選單
PRIVATE FUNCTION apmi125_ui_dialog()
   DEFINE la_param  RECORD
             prog   STRING,
             param  DYNAMIC ARRAY OF STRING
                    END RECORD
   DEFINE ls_js     STRING
   DEFINE li_idx    LIKE type_t.num5
   DEFINE ls_wc     STRING
