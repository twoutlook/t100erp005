            #END add-point
            
 
 
         
      END CONSTRUCT
 
      CONSTRUCT g_wc2_table1 ON pmarstus,pmar002,pmar003,pmar004,pmar005,pmar006,pmar007,pmar008,pmar009, 
          pmar010,pmar011,pmar012,pmar013,pmar014,pmar015,pmar016,pmar017,pmar018,pmar019,pmar020,pmarownid, 
          pmarowndp,pmarcrtid,pmarcrtdp,pmarcrtdt,pmarmodid,pmarmoddt
           FROM s_detail1[1].pmarstus,s_detail1[1].pmar002,s_detail1[1].pmar003,s_detail1[1].pmar004, 
               s_detail1[1].pmar005,s_detail1[1].pmar006,s_detail1[1].pmar007,s_detail1[1].pmar008,s_detail1[1].pmar009, 
               s_detail1[1].pmar010,s_detail1[1].pmar011,s_detail1[1].pmar012,s_detail1[1].pmar013,s_detail1[1].pmar014, 
               s_detail1[1].pmar015,s_detail1[1].pmar016,s_detail1[1].pmar017,s_detail1[1].pmar018,s_detail1[1].pmar019, 
               s_detail1[1].pmar020,s_detail2[1].pmarownid,s_detail2[1].pmarowndp,s_detail2[1].pmarcrtid, 
               s_detail2[1].pmarcrtdp,s_detail2[1].pmarcrtdt,s_detail2[1].pmarmodid,s_detail2[1].pmarmoddt 

                      
         BEFORE CONSTRUCT
            #add-point:cs段more_construct
