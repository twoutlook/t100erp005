    <![CDATA[#+ 資料新增
PRIVATE FUNCTION apmi125_insert()
   #add-point:insert段define
   {<point name="insert.define"/>}
   #end add-point    
   
   #add-point:insert段before
   {<point name="insert.before"/>}
   #end add-point    
   
   #清除相關資料
   CLEAR FORM                    
   CALL g_pmar_d.clear()
   CALL g_pmar2_d.clear()
 
 
   INITIALIZE g_pmar_m.* LIKE pmar_t.*             #DEFAULT 設定
   LET g_pmar000_t = NULL
   LET g_pmar001_t = NULL
 
   CALL s_transaction_begin()
   WHILE TRUE
     
      #單頭預設值
      
     
      #add-point:單頭預設值
      {<point name="insert.default"/>}
      #end add-point 
 
      CALL apmi125_input("a")
      
      #add-point:單頭輸入後
