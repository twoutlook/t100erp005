  <section id="apmq125.b_fill" ver="3" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION apmq125_b_fill()
   {<Local define>}
   DEFINE ls_wc           STRING
   DEFINE l_pid           LIKE type_t.chr50
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_detail_idx  = 1
   LET g_detail_idx2 = 1
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
 
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
 
   CALL g_pmar_d.clear()
   CALL g_pmar2_d.clear()
 
 
   #add-point:陣列清空
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   LET l_ac = 1
   ERROR "Searching!"
 
   # b_fill段sql組成及FOREACH撰寫
   #+ 此段落由子樣板qs04產生
   #+ b_fill段資料取得(包含sql組成及FOREACH段撰寫)
   LET g_sql = "SELECT  UNIQUE '',pmar002,'','',pmar003,pmar004,pmar005,pmar001,'',pmar007,pmar009,pmar006, 
       '','','','','','','','','','','','','','','','','','','','' FROM pmar_t",
 
 
               "",
               " WHERE pmarent= ? AND pmarsite= ? AND 1=1 AND ", ls_wc
 
   LET g_sql = g_sql, " ORDER BY pmar_t.pmar000,pmar_t.pmar001,pmar_t.pmar002,pmar_t.pmar003,pmar_t.pmar004,pmar_t.pmar005,pmar_t.pmar006,pmar_t.pmar007,pmar_t.pmar009"
 
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   PREPARE apmq125_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR apmq125_pb
 
   OPEN b_fill_curs USING g_enterprise, g_site
 
   FOREACH b_fill_curs INTO g_pmar_d[l_ac].sel,g_pmar_d[l_ac].pmar002,g_pmar_d[l_ac].pmar002_desc,g_pmar_d[l_ac].pmar002_desc_desc, 
       g_pmar_d[l_ac].pmar003,g_pmar_d[l_ac].pmar004,g_pmar_d[l_ac].pmar005,g_pmar_d[l_ac].pmar001,g_pmar_d[l_ac].pmar001_desc, 
       g_pmar_d[l_ac].pmar007,g_pmar_d[l_ac].pmar009,g_pmar_d[l_ac].pmar006,g_pmar2_d[l_ac].l_pmar002, 
       g_pmar2_d[l_ac].l_pmar002_desc,g_pmar2_d[l_ac].l_pmar002_desc_desc,g_pmar2_d[l_ac].l_pmar003, 
       g_pmar2_d[l_ac].l_pmar004,g_pmar2_d[l_ac].l_pmar005,g_pmar2_d[l_ac].l_pmar001,g_pmar2_d[l_ac].l_pmar001_desc, 
       g_pmar2_d[l_ac].l_pmar007,g_pmar2_d[l_ac].l_pmar009,g_pmar2_d[l_ac].l_pmar006,g_pmar2_d[l_ac].l_pmar012, 
       g_pmar2_d[l_ac].l_pmar014,g_pmar2_d[l_ac].l_pmar015,g_pmar2_d[l_ac].l_pmar016,g_pmar2_d[l_ac].l_pmar017, 
       g_pmar2_d[l_ac].l_pmar018,g_pmar2_d[l_ac].l_pmar019,g_pmar2_d[l_ac].l_pmar020,g_pmar2_d[l_ac].l_pmar020_desc 

      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
 
      
 
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL apmq125_detail_show("'1'")
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            CALL cl_err( "", 9035, 1 )
         END IF
         EXIT FOREACH
      END IF
 
   END FOREACH
 
 
 
 
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   CALL g_pmar_d.deleteElement(g_pmar_d.getLength())
   CALL g_pmar2_d.deleteElement(g_pmar2_d.getLength())
 
 
   #add-point:陣列長度調整
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
 
   LET g_error_show = 0
 
   LET g_detail_cnt = l_ac - 1
   DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
 
   #+ 此段落由子樣板qs06產生
   #+ b_fill段CURSOR釋放
   CLOSE b_fill_curs
   FREE apmq125_pb
 
 
 
 
   LET l_ac = 1
   CALL apmq125_b_fill2()
 
      CALL apmq125_filter_show('pmar002','b_pmar002')
   CALL apmq125_filter_show('pmar003','b_pmar003')
   CALL apmq125_filter_show('pmar004','b_pmar004')
   CALL apmq125_filter_show('pmar005','b_pmar005')
   CALL apmq125_filter_show('pmar001','b_pmar001')
   CALL apmq125_filter_show('pmar007','b_pmar007')
   CALL apmq125_filter_show('pmar009','b_pmar009')
   CALL apmq125_filter_show('pmar006','b_pmar006')
 
 
END FUNCTION
]]>
  </section>
