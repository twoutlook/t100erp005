  <point name="function.apmq125_b_fill" order="3" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION apmq125_b_fill()
   {<Local define>}
   DEFINE ls_wc           STRING
   DEFINE l_pid           LIKE type_t.chr50
   {</Local define>}
   #add-point:b_fill段define
   DEFINE l_sql_order     STRING
   DEFINE l_sql           STRING
   DEFINE l_item          STRING
   #end add-point

   #add-point:b_fill段sql_before

   #end add-point

   LET g_detail_idx  = 1
   LET g_detail_idx2 = 1

   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF

   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter

   CALL g_pmar_d.clear()
   CALL g_pmar2_d.clear()


   #add-point:陣列清空

   #end add-point

   LET g_cnt = l_ac
   LET l_ac = 1
   ERROR "Searching!"

   # b_fill段sql組成及FOREACH撰寫
   LET l_sql = "SELECT UNIQUE "
   LET l_sql_order = " ORDER BY "
   #單投資料內容
   IF g_pmar3_d.pmar002 = 'Y' THEN
      LET l_sql = l_sql,"pmar002,'',''"
      LET l_sql_order = l_sql_order,"pmar002"
   ELSE
      LET l_sql = l_sql,"'','',''"
      LET l_sql_order = l_sql_order,"''"
   END IF

   IF g_pmar3_d.pmar004 = 'Y' THEN
      LET l_sql = l_sql,",pmar004"
      LET l_sql_order = l_sql_order,",pmar004"
   ELSE
      LET l_sql = l_sql,",''"
      LET l_sql_order = l_sql_order,",''"
   END IF

   IF g_pmar3_d.pmar001 = 'Y' THEN
      LET l_sql = l_sql,",pmar001,''"
      LET l_sql_order = l_sql_order,",pmar001"
   ELSE
      LET l_sql = l_sql,",'',''"
      LET l_sql_order = l_sql_order,",''"
   END IF

   IF g_pmar3_d.pmar007 = 'Y' THEN
      LET l_sql = l_sql,",pmar007"
      LET l_sql_order = l_sql_order,",pmar007"
   ELSE
      LET l_sql = l_sql,",''"
      LET l_sql_order = l_sql_order,",''"
   END IF

   IF g_pmar3_d.pmar003 = 'Y' THEN
      LET l_sql = l_sql,",pmar003"
      LET l_sql_order = l_sql_order,",pmar003"
   ELSE
      LET l_sql = l_sql,",''"
      LET l_sql_order = l_sql_order,",''"
   END IF

   IF g_pmar3_d.pmar005 = 'Y' THEN
      LET l_sql = l_sql,",pmar005"
      LET l_sql_order = l_sql_order,",pmar005"
   ELSE
      LET l_sql = l_sql,",''"
      LET l_sql_order = l_sql_order,",''"
   END IF

   IF g_pmar3_d.pmar006 = 'Y' THEN
      LET l_sql = l_sql,",pmar006"
      LET l_sql_order = l_sql_order,",pmar006"
   ELSE
      LET l_sql = l_sql,",''"
      LET l_sql_order = l_sql_order,",''"
   END IF

   IF g_pmar3_d.pmar009 = 'Y' THEN
      LET l_sql = l_sql,",pmar009"
      LET l_sql_order = l_sql_order,",pmar009"
   ELSE
      LET l_sql = l_sql,",''"
      LET l_sql_order = l_sql_order,",''"
   END IF

   #價格類型
   IF NOT cl_null(g_pmar000) THEN
      LET ls_wc = ls_wc," AND pmar000='",g_pmar000,"'"
   END IF

   #CALL ls_wc.getItemName(1,l_item) #RETURNING l_item
#   CASE ls_wc.getItemName(1,l_item)
#      WHEN "imaa"
#
#         LET g_sql = l_sql,"  FROM pmar_t,imaa_t,pmaa_t ",
#                           " WHERE pmarent = imaaent AND pmar002 = imaa001 AND pmarent = pmaarnt AND pmar001 = pmaa001 ",
#                           "   AND pmarent=",g_enterprise," AND pmarsite='",g_site,"' AND ", ls_wc
#      WHEN "pmaa"
#
#         LET g_sql = l_sql,"  FROM pmar_t,pmaa_t ",
#                           " WHERE pmarent = pmaarnt AND pmar001 = pmaa001 ",
#                           "   AND pmarent=",g_enterprise," AND pmarsite='",g_site,"' AND ", ls_wc
#      OTHERWISE
#
#         LET g_sql = l_sql,"  FROM pmar_t ",
#                           " WHERE pmarent=",g_enterprise," AND pmarsite='",g_site,"' AND ", ls_wc
#   END CASE

   LET g_sql = l_sql,"  FROM pmar_t,imaa_t,pmaa_t ",
                     " WHERE pmarent = imaaent AND pmar002 = imaa001 AND pmarent = pmaarnt AND pmar001 = pmaa001 ",
                     "   AND pmarent=",g_enterprise," AND pmarsite='",g_site,"' AND ", ls_wc
   LET g_sql = g_sql,l_sql_order
   #end add-point

   PREPARE apmq125_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR apmq125_pb

   OPEN b_fill_curs USING g_enterprise, g_site

   FOREACH b_fill_curs INTO g_pmar_d[l_ac].sel,g_pmar_d[l_ac].pmar002,g_pmar_d[l_ac].pmar002_desc,g_pmar_d[l_ac].pmar002_desc_desc,
       g_pmar_d[l_ac].pmar003,g_pmar_d[l_ac].pmar004,g_pmar_d[l_ac].pmar005,g_pmar_d[l_ac].pmar001,g_pmar_d[l_ac].pmar001_desc,
       g_pmar_d[l_ac].pmar007,g_pmar_d[l_ac].pmar009,g_pmar_d[l_ac].pmar006

      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF



      #add-point:b_fill段資料填充

      #end add-point

      CALL apmq125_detail_show("'1'")

      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            CALL cl_err( "", 9035, 1 )
         END IF
         EXIT FOREACH
      END IF

   END FOREACH





   #add-point:b_fill段資料填充(其他單身)

   #end add-point

   CALL g_pmar_d.deleteElement(g_pmar_d.getLength())
   CALL g_pmar2_d.deleteElement(g_pmar2_d.getLength())


   #add-point:陣列長度調整

   #end add-point

   LET g_error_show = 0

   LET g_detail_cnt = l_ac - 1
   DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0

   #+ 此段落由子樣板qs06產生
   #+ b_fill段CURSOR釋放
   CLOSE b_fill_curs
   FREE apmq125_pb




   LET l_ac = 1
   CALL apmq125_b_fill2()

      CALL apmq125_filter_show('pmar002','b_pmar002')
   CALL apmq125_filter_show('pmar003','b_pmar003')
   CALL apmq125_filter_show('pmar004','b_pmar004')
   CALL apmq125_filter_show('pmar005','b_pmar005')
   CALL apmq125_filter_show('pmar001','b_pmar001')
   CALL apmq125_filter_show('pmar007','b_pmar007')
   CALL apmq125_filter_show('pmar009','b_pmar009')
   CALL apmq125_filter_show('pmar006','b_pmar006')


END FUNCTION]]>
  </point>
