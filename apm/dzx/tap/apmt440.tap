  <point name="input.a.page1.pmdj002" order="" ver="18" cite_std="N" new="N" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[            CALL apmt440_pmdj002_desc()
            IF NOT cl_null(g_pmdj_d[l_ac].pmdj002) THEN 
               IF l_cmd = 'a' OR ( l_cmd = 'u' AND (g_pmdj_d[l_ac].pmdj002 != g_pmdj_d_o.pmdj002 OR cl_null(g_pmdj_d_o.pmdj002))) THEN
                  IF NOT apmt440_pmdj002_chk(g_pmdj_d[l_ac].pmdj002) THEN
                     LET g_pmdj_d[l_ac].pmdj002 = g_pmdj_d_o.pmdj002
                     CALL apmt440_pmdj002_desc() 
                     NEXT FIELD CURRENT
                  ELSE
                     LET g_pmdj_d[l_ac].pmdj003 = ' '
                     DISPLAY BY NAME g_pmdj_d[l_ac].pmdj003
                     
                     LET g_pmdj_d[l_ac].pmdj004 = ''
                     LET g_pmdj_d[l_ac].pmdj008 = ''
                     SELECT imaf157,imaf143 INTO g_pmdj_d[l_ac].pmdj004,g_pmdj_d[l_ac].pmdj008 FROM imaf_t
                        WHERE imafent = g_enterprise AND imafsite = g_site AND imaf001 = g_pmdj_d[l_ac].pmdj002
                        
                     #如果整體參數使用採購計價單位時，[C:詢價單位]=[T:料件據點進銷存檔].[C:採購計價單位] 
                     IF cl_get_para(g_enterprise,g_site,'S-BAS-0019') THEN
                        SELECT imaf144 INTO g_pmdj_d[l_ac].pmdj008 FROM imaf_t
                        WHERE imafent = g_enterprise AND imafsite = g_site AND imaf001 = g_pmdj_d[l_ac].pmdj002
                     END IF
                     DISPLAY BY NAME g_pmdj_d[l_ac].pmdj004,g_pmdj_d[l_ac].pmdj008
                     CALL apmt440_pmdj004_desc()
                     CALL apmt440_pmdj008_desc()
                     #依據單頭的稅別與單身的料號呼叫取稅率的應用元件，取得稅率值後更新顯示在pmdj012欄位
                     LET l_pmab039 = ''
                     SELECT pmab039 INTO l_pmab039 FROM pmab_t,pmaa_t 
                        WHERE pmaaent = pmabent AND pmaa001 = pmab001 AND pmaastus = 'Y'
                          AND pmabent = g_enterprise AND pmabsite = g_site AND pmab001 = g_pmdi_m.pmdi004
                     IF SQLCA.sqlcode = 100 THEN
                        SELECT pmab039 INTO l_pmab039 FROM pmab_t,pmaa_t 
                           WHERE pmaaent = pmabent AND pmaa001 = pmab001 AND pmaastus = 'Y'
                             AND pmabent = g_enterprise AND pmabsite = 'ALL' AND pmab001 = g_pmdi_m.pmdi004
                     END IF
                     IF g_tax_app = '2' THEN
                     #依料件設定
                        IF (NOT cl_null(g_pmdi_m.pmdi004)) AND (NOT cl_null(l_pmab039)) THEN
                           CALL s_tax_chktype(g_site,g_pmdi_m.pmdi004,g_pmdj_d[l_ac].pmdj002,'2',l_pmab039)
                           RETURNING l_success,g_pmdj_d[l_ac].pmdj016,g_pmdj_d[l_ac].pmdj012
                        END IF
                     ELSE
                     #依正常稅率
                        DISPLAY BY NAME g_pmdj_d[l_ac].pmdj016,g_pmdj_d[l_ac].pmdj012
                        CALL apmt440_pmdi006_desc(g_pmdj_d[l_ac].pmdj016) RETURNING g_pmdj_d[l_ac].pmdj016_desc
                        DISPLAY BY NAME g_pmdj_d[l_ac].pmdj016_desc 
                     END IF                                      
                  END IF
                  IF NOT cl_null(g_pmdj_d[l_ac].pmdj002) AND g_pmdj_d[l_ac].pmdj003 IS NOT NULL THEN 
                     CALL s_apmi070_get_pmao004(g_pmdi_m.pmdi004,g_pmdj_d[l_ac].pmdj002,g_pmdj_d[l_ac].pmdj003) RETURNING l_success,l_pmao004
                     IF l_success THEN
                        LET g_pmdj_d[l_ac].pmdj005 = l_pmao004
                     END IF
                     DISPLAY BY NAME g_pmdj_d[l_ac].pmdj005
                  END IF
                  CALL apmt440_get_last_price(g_pmdi_m.pmdi004,g_pmdi_m.pmdi005,g_pmdi_m.pmdi006,g_pmdi_m.pmdi015,g_pmdj_d[l_ac].pmdj001,g_pmdj_d[l_ac].pmdj002,
                       g_pmdj_d[l_ac].pmdj003,g_pmdj_d[l_ac].pmdj006,g_pmdj_d[l_ac].pmdj007,g_pmdj_d[l_ac].pmdj008)
                 RETURNING l_success,g_pmdj_d[l_ac].pmdj010
               END IF
            END IF 
            

            #2015-06-04----dorislai----add
            #判斷料件編號、系列、產品分類，是否僅有一欄位有值
               #料件編號有值時，系列、產品分類是否也有值
            IF NOT cl_null(g_pmdj_d[l_ac].pmdj002) AND (cl_null(g_pmdj_d[l_ac].pmdj031)=FALSE OR cl_null(g_pmdj_d[l_ac].pmdj032)=FALSE) THEN
               LET l_msg = ''   #清空值
               LET l_msg1 = ''
               LET l_msg2 = ''
               CALL cl_getmsg('apm-00939',g_lang) RETURNING l_msg1  #%1替換成 料件編號
               
               IF NOT cl_null(g_pmdj_d[l_ac].pmdj031) THEN
                  CALL cl_getmsg('apm-00940',g_lang) RETURNING l_msg2  #%2替換成 系列
               ELSE
                  CALL cl_getmsg('apm-00941',g_lang) RETURNING l_msg2  #%2替換成 產品分類             
               END IF
               
               #替換apm-00937的字串(料件編號、系列、產品分類擇一輸入，%1已有值，是否將%2清空？)
               CALL cl_getmsg_parm('apm-00937',g_lang,l_msg1 ||'|'|| l_msg2) RETURNING l_msg
               #跳出提醒視窗，確認：保留當下欄位值、清除另一個欄位值  ; 取消：保留另一個欄位值、清除當下欄位值
               IF cl_ask_confirm2('',l_msg) THEN
                  LET g_pmdj_d[l_ac].pmdj031 = ''        #清空系列
                  LET g_pmdj_d[l_ac].pmdj031_desc=''     #清空系列說明
                  LET g_pmdj_d_o.pmdj031 = ''            #清空系列舊值
                  LET g_pmdj_d[l_ac].pmdj032 = ''        #清空產品分類
                  LET g_pmdj_d[l_ac].pmdj032_desc=''     #清空產品分類說明
                  LET g_pmdj_d_o.pmdj032 = ''            #清空產品分類舊值
                  DISPLAY BY NAME g_pmdj_d[l_ac].pmdj031,g_pmdj_d[l_ac].pmdj031_desc
                  DISPLAY BY NAME g_pmdj_d[l_ac].pmdj032,g_pmdj_d[l_ac].pmdj032_desc
               ELSE 
                  LET g_pmdj_d[l_ac].pmdj002 = ''             #清空料號編號
                  LET g_pmdj_d_o.pmdj002 = ''                 #清空料號舊值
                  LET g_pmdj_d[l_ac].pmdj002_desc = ''        #清空品名
                  LET g_pmdj_d[l_ac].pmdj002_desc_desc= ''    #清空規格
                  LET g_pmdj_d[l_ac].pmdj003 = ''             #清空產品特徵
                  LET g_pmdj_d[l_ac].pmdj003_desc = ''        #清空產品特徵說明
                  CAll apmt440_set_no_entry_b(p_cmd)
                  DISPLAY BY NAME g_pmdj_d[l_ac].pmdj002,g_pmdj_d[l_ac].pmdj002_desc,g_pmdj_d[l_ac].pmdj002_desc_desc
                  DISPLAY BY NAME g_pmdj_d[l_ac].pmdj003,g_pmdj_d[l_ac].pmdj003_desc
               END IF
                  
            END IF
            #2015-06-04----dorislai----end
            LET  g_pmdj_d_o.pmdj002 = g_pmdj_d[l_ac].pmdj002
            CALL apmt440_set_entry_b(l_cmd)
            CALL apmt440_set_no_entry_b(l_cmd)
]]>
  </point>
