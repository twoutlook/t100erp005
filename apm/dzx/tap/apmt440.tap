              WHERE pmabent = pmaaent AND pmab001 = pmaa001 AND pmaastus = 'Y'
                AND pmabent = g_enterprise AND pmabsite = g_site AND pmab001 = g_pmdi_m.pmdi004  
        #據點級沒有設置慣用資料，則抓集團層
        IF SQLCA.sqlcode = 100 THEN
           SELECT pmab_t.* INTO l_pmab.* FROM pmab_t,pmaa_t 
              WHERE pmabent = pmaaent AND pmab001 = pmaa001 AND pmaastus = 'Y'
                AND pmabent = g_enterprise AND pmabsite = 'ALL' AND pmab001 = g_pmdi_m.pmdi004  
        END IF     
        
        #先判斷欄位是否在單據別設定的預設欄位中，如果存在，則不重新帶值，不存在，則根據供應商帶預設值
        IF NOT s_aooi200_chk_doc_fields(g_site,'1',g_pmdi_m.pmdidocno,'pmdi005') THEN
           LET g_pmdi_m.pmdi005 = l_pmab.pmab033
           CALL apmt440_pmdi005_desc()
        END IF
        
        IF NOT s_aooi200_chk_doc_fields(g_site,'1',g_pmdi_m.pmdidocno,'pmdi006') THEN
           LET g_pmdi_m.pmdi006 = l_pmab.pmab034
           CALL apmt440_pmdi006_desc(g_pmdi_m.pmdi006) RETURNING g_pmdi_m.pmdi006_desc
           DISPLAY BY NAME g_pmdi_m.pmdi006_desc 
           
           #根據稅別帶出稅率、含稅否等
           IF NOT cl_null(g_pmdi_m.pmdi006) THEN
              CALL s_tax_chk(g_site,g_pmdi_m.pmdi006)
                  RETURNING l_success,g_pmdi_m.pmdi004_desc,g_pmdi_m.pmdi008,g_pmdi_m.pmdi007,g_tax_app
              DISPLAY BY NAME g_pmdi_m.pmdi008,g_pmdi_m.pmdi007 
           END IF
        END IF
        
        IF NOT s_aooi200_chk_doc_fields(g_site,'1',g_pmdi_m.pmdidocno,'pmdi009') THEN
           LET g_pmdi_m.pmdi009 = l_pmab.pmab037
           CALL apmt440_pmdi009_desc()
        END IF
        
        IF NOT s_aooi200_chk_doc_fields(g_site,'1',g_pmdi_m.pmdidocno,'pmdi011') THEN
           LET g_pmdi_m.pmdi011 = l_pmab.pmab053
           CALL apmt440_pmdi011_desc()
        END IF
        
        IF NOT s_aooi200_chk_doc_fields(g_site,'1',g_pmdi_m.pmdidocno,'pmdi019') THEN
           LET g_pmdi_m.pmdi019 = l_pmab.pmab038
           CALL s_desc_get_acc_desc('275',g_pmdi_m.pmdi019) RETURNING g_pmdi_m.pmdi019_desc
           DISPLAY BY NAME g_pmdi_m.pmdi019_desc
        END IF
        
END FUNCTION]]>
