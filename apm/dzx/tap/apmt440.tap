  <point name="statechange.before_menu" order="" ver="17" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#            CALL cl_set_act_visible("open,invalid,confirmed",TRUE)
#      CASE g_pmdi_m.pmdistus 
#         WHEN "N"
#            CALL cl_set_act_visible("open",FALSE)
#            
