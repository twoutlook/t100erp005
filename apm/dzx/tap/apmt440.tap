  <section id="apmt440.insert" ver="31" status="" src="s" readonly="">
    <![CDATA[#+ 資料新增
PRIVATE FUNCTION apmt440_insert()
   #add-point:insert段define
   {<point name="insert.define" edit="s"/>}
   #end add-point    
   #add-point:insert段define(客製用)
   {<point name="insert.define_customerization" edit="c"/>}
   #end add-point    
   
   #清畫面欄位內容
   CLEAR FORM                    
   CALL g_pmdj_d.clear()   
   CALL g_pmdj2_d.clear()  
 
 
   INITIALIZE g_pmdi_m.* LIKE pmdi_t.*             #DEFAULT 設定
   
   LET g_pmdidocno_t = NULL
 
   
   LET g_master_insert = FALSE
   
   #add-point:insert段before
   {<point name="insert.before"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   WHILE TRUE
      #公用欄位給值(單頭)
      #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_pmdi_m.pmdiownid = g_user
      LET g_pmdi_m.pmdiowndp = g_dept
      LET g_pmdi_m.pmdicrtid = g_user
      LET g_pmdi_m.pmdicrtdp = g_dept 
      LET g_pmdi_m.pmdicrtdt = cl_get_current()
      LET g_pmdi_m.pmdimodid = g_user
      LET g_pmdi_m.pmdimoddt = cl_get_current()
      LET g_pmdi_m.pmdistus = 'N'
 
 
 
      #append欄位給值
      
     
      #一般欄位給值
            LET g_pmdi_m.pmdistus = "N"
      LET g_pmdi_m.pmdi008 = "N"
      LET g_pmdi_m.pmdi013 = "1"
      LET g_pmdi_m.pmdi014 = "1"
      LET g_pmdi_m.pmdi010 = "N"
      LET g_pmdi_m.pmdi012 = "N"
      LET g_pmdi_m.pmdi031 = "N"
      LET g_pmdi_m.pmdi032 = "N"
      LET g_pmdi_m.pmdi020 = "N"
      LET g_pmdi_m.pmdi017 = "1"
      LET g_pmdi_m.pmdi018 = "1"
 
  
      #add-point:單頭預設值
      {<point name="insert.default"/>}
      #end add-point 
      
      #保存單頭舊值(用於資料輸入錯誤還原預設值時使用)
      LET g_pmdi_m_t.* = g_pmdi_m.*
      LET g_pmdi_m_o.* = g_pmdi_m.*
      
      #顯示狀態(stus)圖片
            #應用 a21 樣板自動產生(Version:2)
	  #根據當下狀態碼顯示圖片
      CASE g_pmdi_m.pmdistus 
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "A"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/approved.png")
         WHEN "D"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/withdraw.png")
         WHEN "R"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/rejection.png")
         WHEN "W"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/signing.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
 
 
    
      CALL apmt440_input("a")
      
      #add-point:單頭輸入後
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
      END IF
      
      IF NOT g_master_insert THEN
         DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
         DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
         INITIALIZE g_pmdi_m.* TO NULL
         INITIALIZE g_pmdj_d TO NULL
         INITIALIZE g_pmdj2_d TO NULL
 
         #add-point:取消新增後
         {<point name="insert.cancel"/>}
         #end add-point 
         CALL apmt440_show()
         RETURN
      END IF
      
      LET INT_FLAG = 0
      #CALL g_pmdj_d.clear()
      #CALL g_pmdj2_d.clear()
 
 
      LET g_rec_b = 0
      CALL s_transaction_end('Y','0')
      EXIT WHILE
        
   END WHILE
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL apmt440_set_act_visible()   
   CALL apmt440_set_act_no_visible()
   
   #將新增的資料併入搜尋條件中
   LET g_pmdidocno_t = g_pmdi_m.pmdidocno
 
   
   #組合新增資料的條件
   LET g_add_browse = " pmdient = '" ||g_enterprise|| "' AND",
                      " pmdidocno = '", g_pmdi_m.pmdidocno, "' "
 
                      
   #add-point:組合新增資料的條件後
   {<point name="insert.after.add_browse"/>}
   #end add-point
      
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL apmt440_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   CLOSE apmt440_cl
   
   CALL apmt440_idx_chk()
   
   #撈取異動後的資料(主要是帶出reference)
   EXECUTE apmt440_master_referesh USING g_pmdi_m.pmdidocno INTO g_pmdi_m.pmdi001,g_pmdi_m.pmdisite, 
       g_pmdi_m.pmdidocno,g_pmdi_m.pmdidocdt,g_pmdi_m.pmdi002,g_pmdi_m.pmdi004,g_pmdi_m.pmdi003,g_pmdi_m.pmdistus, 
       g_pmdi_m.pmdi005,g_pmdi_m.pmdi006,g_pmdi_m.pmdi007,g_pmdi_m.pmdi008,g_pmdi_m.pmdi009,g_pmdi_m.pmdi011, 
       g_pmdi_m.pmdi019,g_pmdi_m.pmdi030,g_pmdi_m.pmdi013,g_pmdi_m.pmdi014,g_pmdi_m.pmdi010,g_pmdi_m.pmdi012, 
       g_pmdi_m.pmdi031,g_pmdi_m.pmdi032,g_pmdi_m.pmdi020,g_pmdi_m.pmdi015,g_pmdi_m.pmdi016,g_pmdi_m.pmdi017, 
       g_pmdi_m.pmdi018,g_pmdi_m.pmdiownid,g_pmdi_m.pmdiowndp,g_pmdi_m.pmdicrtid,g_pmdi_m.pmdicrtdp, 
       g_pmdi_m.pmdicrtdt,g_pmdi_m.pmdimodid,g_pmdi_m.pmdimoddt,g_pmdi_m.pmdicnfid,g_pmdi_m.pmdicnfdt, 
       g_pmdi_m.pmdi002_desc,g_pmdi_m.pmdi004_desc,g_pmdi_m.pmdi003_desc,g_pmdi_m.pmdi005_desc,g_pmdi_m.pmdi009_desc, 
       g_pmdi_m.pmdi011_desc,g_pmdi_m.pmdi019_desc,g_pmdi_m.pmdiownid_desc,g_pmdi_m.pmdiowndp_desc,g_pmdi_m.pmdicrtid_desc, 
       g_pmdi_m.pmdicrtdp_desc,g_pmdi_m.pmdimodid_desc,g_pmdi_m.pmdicnfid_desc
   
   #遮罩相關處理
   LET g_pmdi_m_mask_o.* =  g_pmdi_m.*
   CALL apmt440_pmdi_t_mask()
   LET g_pmdi_m_mask_n.* =  g_pmdi_m.*
   
   #將資料顯示到畫面上
   DISPLAY BY NAME g_pmdi_m.pmdi001,g_pmdi_m.pmdisite,g_pmdi_m.pmdidocno,g_pmdi_m.pmdidocdt,g_pmdi_m.pmdi002, 
       g_pmdi_m.pmdi002_desc,g_pmdi_m.oobal004,g_pmdi_m.pmdi004,g_pmdi_m.pmdi004_desc,g_pmdi_m.pmdi003, 
       g_pmdi_m.pmdi003_desc,g_pmdi_m.pmdistus,g_pmdi_m.pmdi005,g_pmdi_m.pmdi005_desc,g_pmdi_m.pmdi006, 
       g_pmdi_m.pmdi006_desc,g_pmdi_m.pmdi007,g_pmdi_m.pmdi008,g_pmdi_m.pmdi009,g_pmdi_m.pmdi009_desc, 
       g_pmdi_m.pmdi011,g_pmdi_m.pmdi011_desc,g_pmdi_m.pmdi019,g_pmdi_m.pmdi019_desc,g_pmdi_m.pmdi030, 
       g_pmdi_m.pmdi013,g_pmdi_m.pmdi014,g_pmdi_m.pmdi010,g_pmdi_m.pmdi012,g_pmdi_m.pmdi031,g_pmdi_m.pmdi032, 
       g_pmdi_m.pmdi020,g_pmdi_m.pmdi015,g_pmdi_m.pmdi016,g_pmdi_m.pmdi017,g_pmdi_m.pmdi018,g_pmdi_m.pmdiownid, 
       g_pmdi_m.pmdiownid_desc,g_pmdi_m.pmdiowndp,g_pmdi_m.pmdiowndp_desc,g_pmdi_m.pmdicrtid,g_pmdi_m.pmdicrtid_desc, 
       g_pmdi_m.pmdicrtdp,g_pmdi_m.pmdicrtdp_desc,g_pmdi_m.pmdicrtdt,g_pmdi_m.pmdimodid,g_pmdi_m.pmdimodid_desc, 
       g_pmdi_m.pmdimoddt,g_pmdi_m.pmdicnfid,g_pmdi_m.pmdicnfid_desc,g_pmdi_m.pmdicnfdt
   
   #add-point:新增結束後
   {<point name="insert.after"/>}
   #end add-point 
   
   #功能已完成,通報訊息中心
   CALL apmt440_msgcentre_notify('')
   
END FUNCTION
]]>
  </section>
