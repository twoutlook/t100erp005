   #end add-point  
   
   IF g_current_page = 1 THEN
      LET g_detail_idx = g_curr_diag.getCurrentRow("s_detail1")
      IF g_detail_idx > g_pmdj_d.getLength() THEN
         LET g_detail_idx = g_pmdj_d.getLength()
