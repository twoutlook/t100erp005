  <point name="function.apmt440_pmdk_fill" order="19" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION apmt440_pmdk_fill(p_pmdjseq)
DEFINE li_ac           LIKE type_t.num5
DEFINE p_pmdjseq       LIKE pmdj_t.pmdjseq
       CALL g_pmdj2_d.clear()

       LET g_sql = "SELECT  UNIQUE pmdkseq,pmdk001,pmdk002,pmdk003,pmdk004 FROM pmdk_t",    
                   "",
                   " WHERE pmdkent=? AND pmdkdocno=? AND pmdkseq = ? "
       
       IF NOT cl_null(g_wc2_table2) THEN
          LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
       END IF
       
       LET g_sql = g_sql, " ORDER BY  pmdk_t.pmdkseq,pmdk_t.pmdk001,pmdk_t.pmdk002" 
       
       PREPARE apmt440_pb3 FROM g_sql
       DECLARE b_fill_curs3 CURSOR FOR apmt440_pb3
       
       OPEN b_fill_curs3 USING g_enterprise,g_pmdi_m.pmdidocno,p_pmdjseq
       LET li_ac = 1
       FOREACH b_fill_curs3 INTO g_pmdj2_d[li_ac].pmdkseq,g_pmdj2_d[li_ac].pmdk001,g_pmdj2_d[li_ac].pmdk002,g_pmdj2_d[li_ac].pmdk003,g_pmdj2_d[li_ac].pmdk004
          IF SQLCA.sqlcode THEN
             INITIALIZE g_errparam TO NULL
             LET g_errparam.code = SQLCA.sqlcode
             LET g_errparam.extend = "FOREACH:"
             LET g_errparam.popup = TRUE
             CALL cl_err()

             EXIT FOREACH
          END IF

          LET li_ac = li_ac + 1
          IF l_ac > g_max_rec THEN
             INITIALIZE g_errparam TO NULL
             LET g_errparam.code =  9035
             LET g_errparam.extend =  ''
             LET g_errparam.popup = FALSE
             CALL cl_err()

             EXIT FOREACH
          END IF
          
       END FOREACH
       CALL g_pmdj2_d.deleteElement(g_pmdj2_d.getLength())

END FUNCTION
]]>
