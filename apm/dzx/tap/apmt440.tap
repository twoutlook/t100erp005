DEFINE l_sql          STRING
DEFINE l_success      LIKE type_t.num5
   LET l_sql = " SELECT pmdjseq,pmdj001,pmdj002,pmdj003,pmdj006,pmdj007,pmdj008 FROM pmdj_t ",
               "  WHERE pmdjent = '",g_enterprise,"'"  ,
               "    AND pmdjdocno = '",g_pmdi_m.pmdidocno,"'"
   PREPARE apmt440_pmdj011_pre FROM l_sql
   DECLARE apmt440_pmdj011_cs CURSOR FOR apmt440_pmdj011_pre
