      IF l_success THEN
         UPDATE pmdj_t SET pmdj010 = l_pmdj011
          WHERE pmdjent = g_enterprise
            AND pmdjdocno = g_pmdi_m.pmdidocno
            AND pmdjseq = l_pmdjseq
