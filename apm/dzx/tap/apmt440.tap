  <section id="apmt440.b_fill" ver="19" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION apmt440_b_fill()
   DEFINE p_wc2      STRING
   DEFINE li_idx     LIKE type_t.num10
   #add-point:b_fill段define
   {<point name="b_fill.define" edit="s"/>}
   #end add-point     
   #add-point:b_fill段define(客製用)
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point     
 
   CALL g_pmdj_d.clear()    #g_pmdj_d 單頭及單身 
   CALL g_pmdj2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF apmt440_fill_chk(1) THEN
      #切換上下筆時不重組SQL
      IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
         LET g_sql = "SELECT  UNIQUE pmdj001,pmdjsite,pmdjseq,pmdj002,pmdj003,pmdj031,pmdj032,pmdj004, 
             pmdj005,pmdj006,pmdj007,pmdj015,pmdj008,pmdj009,pmdj010,pmdj011,pmdj016,pmdj012,pmdj013, 
             pmdj014,pmdj030 ,t1.imaal003 ,t2.imaal004 ,t3.oocql004 ,t4.rtaxl003 ,t5.imaal003 ,t6.oocql004 , 
             t7.oocql004 ,t8.oocal003 ,t9.oocql004 FROM pmdj_t",   
                     " INNER JOIN pmdi_t ON pmdidocno = pmdjdocno ",
 
                     #"",
                     
                     "",
                                    " LEFT JOIN imaal_t t1 ON t1.imaalent='"||g_enterprise||"' AND t1.imaal001=pmdj002 AND t1.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t2 ON t2.imaalent='"||g_enterprise||"' AND t2.imaal001=pmdj002 AND t2.imaal002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t3 ON t3.oocqlent='"||g_enterprise||"' AND t3.oocql001='2003' AND t3.oocql002=pmdj031 AND t3.oocql003='"||g_dlang||"' ",
               " LEFT JOIN rtaxl_t t4 ON t4.rtaxlent='"||g_enterprise||"' AND t4.rtaxl001=pmdj032 AND t4.rtaxl002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t5 ON t5.imaalent='"||g_enterprise||"' AND t5.imaal001=pmdj004 AND t5.imaal002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t6 ON t6.oocqlent='"||g_enterprise||"' AND t6.oocql001='221' AND t6.oocql002=pmdj006 AND t6.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t7 ON t7.oocqlent='"||g_enterprise||"' AND t7.oocql001='265' AND t7.oocql002=pmdj015 AND t7.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t8 ON t8.oocalent='"||g_enterprise||"' AND t8.oocal001=pmdj008 AND t8.oocal002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t9 ON t9.oocqlent='"||g_enterprise||"' AND t9.oocql001='263' AND t9.oocql002=pmdj014 AND t9.oocql003='"||g_dlang||"' ",
 
                     " WHERE pmdjent=? AND pmdjdocno=?"
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before
         {<point name="b_fill.body.fill_sql"/>}
         #end add-point
         IF NOT cl_null(g_wc2_table1) THEN
            LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY pmdj_t.pmdjseq"
         
         #add-point:單身填充控制
         {<point name="b_fill.sql"/>}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE apmt440_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR apmt440_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_pmdi_m.pmdidocno
                                               
      FOREACH b_fill_cs INTO g_pmdj_d[l_ac].pmdj001,g_pmdj_d[l_ac].pmdjsite,g_pmdj_d[l_ac].pmdjseq,g_pmdj_d[l_ac].pmdj002, 
          g_pmdj_d[l_ac].pmdj003,g_pmdj_d[l_ac].pmdj031,g_pmdj_d[l_ac].pmdj032,g_pmdj_d[l_ac].pmdj004, 
          g_pmdj_d[l_ac].pmdj005,g_pmdj_d[l_ac].pmdj006,g_pmdj_d[l_ac].pmdj007,g_pmdj_d[l_ac].pmdj015, 
          g_pmdj_d[l_ac].pmdj008,g_pmdj_d[l_ac].pmdj009,g_pmdj_d[l_ac].pmdj010,g_pmdj_d[l_ac].pmdj011, 
          g_pmdj_d[l_ac].pmdj016,g_pmdj_d[l_ac].pmdj012,g_pmdj_d[l_ac].pmdj013,g_pmdj_d[l_ac].pmdj014, 
          g_pmdj_d[l_ac].pmdj030,g_pmdj_d[l_ac].pmdj002_desc,g_pmdj_d[l_ac].pmdj002_desc_desc,g_pmdj_d[l_ac].pmdj031_desc, 
          g_pmdj_d[l_ac].pmdj032_desc,g_pmdj_d[l_ac].pmdj004_desc,g_pmdj_d[l_ac].pmdj006_desc,g_pmdj_d[l_ac].pmdj015_desc, 
          g_pmdj_d[l_ac].pmdj008_desc,g_pmdj_d[l_ac].pmdj014_desc
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF
            EXIT FOREACH
         END IF
         
         LET l_ac = l_ac + 1
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
   #判斷是否填充
   IF apmt440_fill_chk(2) THEN
      IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
         LET g_sql = "SELECT  UNIQUE pmdkseq,pmdk001,pmdk002,pmdk003,pmdk004  FROM pmdk_t",   
                     " INNER JOIN pmdi_t ON pmdidocno = pmdkdocno ",
 
                     "",
                     
                     
                     " WHERE pmdkent=? AND pmdkdocno=?"   
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before
         {<point name="b_fill.body2.fill_sql"/>}
         #end add-point
         IF NOT cl_null(g_wc2_table2) THEN
            LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY pmdk_t.pmdkseq,pmdk_t.pmdk001,pmdk_t.pmdk002"
         
         #add-point:單身填充控制
         {<point name="b_fill.sql2"/>}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE apmt440_pb2 FROM g_sql
         DECLARE b_fill_cs2 CURSOR FOR apmt440_pb2
      END IF
    
      LET l_ac = 1
      
      OPEN b_fill_cs2 USING g_enterprise,g_pmdi_m.pmdidocno
                                               
      FOREACH b_fill_cs2 INTO g_pmdj2_d[l_ac].pmdkseq,g_pmdj2_d[l_ac].pmdk001,g_pmdj2_d[l_ac].pmdk002, 
          g_pmdj2_d[l_ac].pmdk003,g_pmdj2_d[l_ac].pmdk004
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill2.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
         
      END FOREACH
   END IF
 
 
   
   #add-point:browser_fill段其他table處理
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_pmdj_d.deleteElement(g_pmdj_d.getLength())
   CALL g_pmdj2_d.deleteElement(g_pmdj2_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE apmt440_pb
   FREE apmt440_pb2
 
 
   
   LET li_idx = l_ac
   
   #遮罩相關處理
   FOR l_ac = 1 TO g_pmdj_d.getLength()
      LET g_pmdj_d_mask_o[l_ac].* =  g_pmdj_d[l_ac].*
      CALL apmt440_pmdj_t_mask()
      LET g_pmdj_d_mask_n[l_ac].* =  g_pmdj_d[l_ac].*
   END FOR
   
   LET g_pmdj2_d_mask_o.* =  g_pmdj2_d.*
   FOR l_ac = 1 TO g_pmdj2_d.getLength()
      LET g_pmdj2_d_mask_o[l_ac].* =  g_pmdj2_d[l_ac].*
      CALL apmt440_pmdk_t_mask()
      LET g_pmdj2_d_mask_n[l_ac].* =  g_pmdj2_d[l_ac].*
   END FOR
 
   
   LET l_ac = li_idx
   
   CALL cl_ap_performance_next_end()
 
END FUNCTION
]]>
  </section>
