  <point name="input.a.pmdi005" order="0" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[                        CALL apmt440_pmdi005_desc()
            IF NOT cl_null(g_pmdi_m.pmdi005) THEN 
               IF p_cmd = 'a' OR ( p_cmd = 'u' AND (g_pmdi_m.pmdi005 != g_pmdi_m_t.pmdi005 OR cl_null(g_pmdi_m_t.pmdi005))) THEN
#此段落由子樣板a19產生
               #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
                  INITIALIZE g_chkparam.* TO NULL
                  
                  #設定g_chkparam.*的參數
                  LET g_chkparam.arg1 = g_site
                  LET g_chkparam.arg2 = g_pmdi_m.pmdi005
   
                     
                  #呼叫檢查存在並帶值的library
                  IF cl_chk_exist("v_ooaj002") THEN
                     #呼叫幣別取位應用元件對單價作取位(依單頭幣別做取位基準),更新单身及子作业中的单价
                     LET l_pmdjseq = 0
                     LET l_pmdj011 = 0
                     DECLARE pmdi005_cs CURSOR FOR 
                         SELECT pmdjseq,pmdj011 FROM pmdj_t WHERE pmdjent = g_enterprise AND pmdjdocno = g_pmdi_m.pmdidocno
                     FOREACH pmdi005_cs INTO l_pmdjseq,l_pmdj011
                        CALL s_curr_round(g_site,g_pmdi_m.pmdi005,l_pmdj011,'1') RETURNING l_pmdj011
                        UPDATE pmdj_t SET pmdj011 = l_pmdj011 
                          WHERE pmdjent = g_enterprise AND pmdjdocno = g_pmdi_m.pmdidocno
                            AND pmdjseq = l_pmdjseq
                     END FOREACH
                     CALL apmt440_b_fill() 
                     #更新子作业中的单价
                     LET l_pmdkseq = 0
                     LET l_pmdk003 = 0
                     LET l_pmdk001 = 0
                     LET l_pmdk002 = 0
                     DECLARE pmdi005_cs2 CURSOR FOR 
                         SELECT pmdkseq,pmdk001,pmdk002,pmdk003 FROM pmdk_t WHERE pmdkent = g_enterprise AND pmdkdocno = g_pmdi_m.pmdidocno
                     FOREACH pmdi005_cs2 INTO l_pmdkseq,l_pmdk001,l_pmdk002,l_pmdk003
                        CALL s_curr_round(g_site,g_pmdi_m.pmdi005,l_pmdk003,'1') RETURNING l_pmdk003
                        UPDATE pmdk_t SET pmdk003 = l_pmdk003 
                          WHERE pmdkent = g_enterprise AND pmdkdocno = g_pmdi_m.pmdidocno
                            AND pmdkseq = l_pmdkseq AND pmdk001 = l_pmdk001 AND pmdk002 = l_pmdk002
                     END FOREACH   
                     CALL apmt440_pmdk_fill(l_pmdkseq)
   
                  ELSE
                     #檢查失敗時後續處理
                     LET g_pmdi_m.pmdi005 = g_pmdi_m_t.pmdi005
                     CALL apmt440_pmdi005_desc()
                     NEXT FIELD CURRENT
                  END IF
               END IF
               IF p_cmd = 'u' AND (g_pmdi_m.pmdi005 != g_pmdi_m_t.pmdi005 OR cl_null(g_pmdi_m_t.pmdi005)) THEN
                  CALL apmt440_pmdj011_upd()
               END IF   
            END IF 

