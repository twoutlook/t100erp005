  <point name="function.apmt440_get_last_price" order="1" ver="16" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 取得上次单价
# Memo...........:
# Usage..........: CALL apmt440_get_last_price(p_pmdi004,p_pmdi005,p_pmdi006,p_pmdi015,p_pmdj001,p_pmdj02,p_pmdj003,p_pmdj006,p_pmdj007,p_pmdj008)
#                  RETURNING r_success,r_last_price
# Input parameter: 1.p_pmdi004      LIKE pmdi_t.pmdi004     #供应商编号
#                : 2.p_pmdi005      LIKE pmdi_t.pmdi005     #币别
#                : 3.p_pmdi006      LIKE pmdi_t.pmdi006     #税别
#                : 4.p_pmdi015      LIKE pmdi_t.pmdi015     #生效日期
#                : 5.p_pmdj001      LIKE pmdj_t.pmdj001     #委外否
#                : 6.p_pmdj002      LIKE pmdj_t.pmdj002     #料号
#                : 7.p_pmdj003      LIKE pmdj_t.pmdj003     #产品特征
#                : 8.p_pmdj006      LIKE pmdj_t.pmdj006     #作业编号
#                : 9.p_pmdj007      LIKE pmdj_t.pmdj007     #製程序
#                : 10.p_pmdj008     LIKE pmdj_t.pmdj008     #单位
# Return code....: 1.r_success      LIKE type_t.num5        #执行成功否
#                : 2.r_last_price   LIKE pmdj_t.pmdj010     #上次单价
# Date & Author..: 2013/12/30 By chenjing
# Modify.........:
################################################################################
PRIVATE FUNCTION apmt440_get_last_price(p_pmdi004,p_pmdi005,p_pmdi006,p_pmdi015,p_pmdj001,p_pmdj002,p_pmdj003,p_pmdj006,p_pmdj007,p_pmdj008)
DEFINE p_pmdi004      LIKE pmdi_t.pmdi004     #供应商编号
DEFINE p_pmdi005      LIKE pmdi_t.pmdi005     #币别
DEFINE p_pmdi006      LIKE pmdi_t.pmdi006     #税别
DEFINE p_pmdi015      LIKE pmdi_t.pmdi015     #生效日期
DEFINE p_pmdj001      LIKE pmdj_t.pmdj001     #委外否
DEFINE p_pmdj002      LIKE pmdj_t.pmdj002     #料号
DEFINE p_pmdj003      LIKE pmdj_t.pmdj003     #产品特征
DEFINE p_pmdj006      LIKE pmdj_t.pmdj006     #作业编号
DEFINE p_pmdj007      LIKE pmdj_t.pmdj007     #製程序
DEFINE p_pmdj008      LIKE pmdj_t.pmdj008     #单位
DEFINE r_success      LIKE type_t.num5        #执行成功否
DEFINE r_last_price   LIKE pmdj_t.pmdj010     #上次单价
DEFINE l_sql          STRING
DEFINE l_sql1         STRING
DEFINE l_sql2         STRING
DEFINE l_sql3         STRING
DEFINE l_pmdjdocno    LIKE pmdj_t.pmdjdocno
DEFINE l_pmdjseq      LIKE pmdj_t.pmdjseq
DEFINE l_pmdj008      LIKE pmdj_t.pmdj008
DEFINE l_pmdj009      LIKE pmdj_t.pmdj009
DEFINE l_pmdj011      LIKE pmdj_t.pmdj011
DEFINE l_success      LIKE type_t.num5
DEFINE l_rate         LIKE inaj_t.inaj014

 WHENEVER ERROR CONTINUE
 LET r_success = FALSE
 
 #检查供应商编号不可为空
 IF cl_null(p_pmdi004) THEN
    INITIALIZE g_errparam TO NULL
    LET g_errparam.code = ''
    LET g_errparam.extend = ''
    LET g_errparam.popup = TRUE
    CALL cl_err()

    RETURN r_success,''
 END IF
 
 #检查币别不可为空
 IF cl_null(p_pmdi005) THEN
    INITIALIZE g_errparam TO NULL
    LET g_errparam.code = ''
    LET g_errparam.extend = ''
    LET g_errparam.popup = TRUE
    CALL cl_err()

    RETURN r_success,''
 END IF

 #检查税别不可为空
 IF cl_null(p_pmdi006) THEN
    INITIALIZE g_errparam TO NULL
    LET g_errparam.code = ''
    LET g_errparam.extend = ''
    LET g_errparam.popup = TRUE
    CALL cl_err()

    RETURN r_success,''
 END IF

 #检查生效日期不可为空
 IF cl_null(p_pmdi015) THEN
    INITIALIZE g_errparam TO NULL
    LET g_errparam.code = ''
    LET g_errparam.extend = ''
    LET g_errparam.popup = TRUE
    CALL cl_err()

    RETURN r_success,''
 END IF
 
 #检查料号不可为空
 IF cl_null(p_pmdj002) THEN
    INITIALIZE g_errparam TO NULL
    LET g_errparam.code = ''
    LET g_errparam.extend = ''
    LET g_errparam.popup = TRUE
    CALL cl_err()

    RETURN r_success,''
 END IF
 
 #检查单位不可为空
 IF cl_null(p_pmdj008) THEN
    INITIALIZE g_errparam TO NULL
    LET g_errparam.code = ''
    LET g_errparam.extend = ''
    LET g_errparam.popup = TRUE
    CALL cl_err()

    RETURN r_success,''
 END IF
 
 #判断委外否是否有值，若没值则给'N'
 IF cl_null(p_pmdj001) THEN
    LET p_pmdj001 = 'N'
 END IF 
 LET l_sql1 = "SELECT MAX(pmdi015) FROM pmdi_t,pmdj_t"
 LET l_sql2 = " WHERE pmdient=pmdjent AND pmdidocno=pmdjdocno AND pmdient='",g_enterprise,"' AND pmdisite= '",g_site,"' ",
              "   AND pmdi004='",p_pmdi004,"' AND pmdi005='",p_pmdi005,"' AND pmdi006='",p_pmdi006,"'",
              "   AND pmdj001='",p_pmdj001,"' AND pmdj002='",p_pmdj002,"' AND pmdi015<='",p_pmdi015,"'"

 IF p_pmdj003 IS NOT NULL THEN
    LET l_sql2 = l_sql2," AND pmdj003='",p_pmdj003,"'"
 ELSE
    LET l_sql2 = l_sql2," AND pmdj003 IS NULL"
 END IF 
 IF p_pmdj001 = 'Y' THEN
#    IF p_pmdj006 IS NOT NULL THEN
       LET l_sql2 = l_sql2," AND pmdj006='",p_pmdj006,"'"
#    ELSE
#       LET l_sql2 = l_sql2," AND pmdj006 IS NULL"
#    END IF 
#    IF p_pmdj007 IS NOT NULL THEN
       LET l_sql2 = l_sql2," AND pmdj007='",p_pmdj007,"'"
#    ELSE
#       LET l_sql2 = l_sql2," AND pmdj007 IS NULL"
#    END IF    
 END IF 
 
 LET l_sql3 = "SELECT pmdjdocno,pmdjseq,pmdj008,pmdj009,pmdj011 FROM pmdi_t,pmdj_t"
 
 LET l_sql = l_sql3,l_sql2," AND pmdi015=(",l_sql1,l_sql2,")"
 PREPARE apmt440_price_pre FROM l_sql
 DECLARE apmt440_price_cs CURSOR FOR apmt440_price_pre
 FOREACH apmt440_price_cs INTO l_pmdjdocno,l_pmdjseq,l_pmdj008,l_pmdj009,l_pmdj011
    IF l_pmdj009 = 'Y' THEN
       SELECT MAX(pmdk003) INTO l_pmdj011 FROM pmdk_t WHERE pmdkent = g_enterprise AND pmdkdocno=l_pmdjdocno AND pmdkseq=l_pmdjseq
    END IF 
    IF l_pmdj008 != p_pmdj008 THEN
#       CALL s_aimi190_get_convert(p_pmdj002,l_pmdj008,p_pmdj008) RETURNING l_success,l_rate
#       IF l_success THEN
#          LET l_pmdj011 = l_pmdj011 * l_rate
#       END IF 
       CALL s_aooi250_convert_qty(p_pmdj002,l_pmdj008,p_pmdj008,l_pmdj011) 
            RETURNING l_success,l_pmdj011
    END IF
    IF r_last_price IS NULL THEN
       LET r_last_price = l_pmdj011
    ELSE
       IF r_last_price < l_pmdj011 THEN
          LET r_last_price = l_pmdj011
       END IF
    END IF
    LET l_pmdjdocno = ''
    LET l_pmdjseq = ''
    LET l_pmdj008 = ''
    LET l_pmdj009 = ''
    LET l_pmdj011 = ''
 END FOREACH
 LET r_success = TRUE
 RETURN r_success,r_last_price
END FUNCTION]]>
  </point>
