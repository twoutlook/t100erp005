  <point name="function.apmt440_pmdi004_chk" order="4" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION apmt440_pmdi004_chk(p_pmdi004)
DEFINE p_pmdi004    LIKE pmdi_t.pmdi004
DEFINE l_success    LIKE type_t.num5
DEFINE l_flag       LIKE type_t.num5
DEFINE r_success    LIKE type_t.num5      
   LET r_success = TRUE
   
   IF NOT cl_null(p_pmdi004) THEN
      #檢核輸入的供應商是否在單據別限制範圍內，若不在限制內則不允許跟此供應商採購
      CALL s_control_chk_doc('2',g_pmdi_m.pmdidocno,p_pmdi004,'','','','') RETURNING l_success,l_flag
      IF NOT l_success THEN
         LET r_success = FALSE
         RETURN r_success
      ELSE
         IF NOT l_flag THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = 'apm-00239'
            LET g_errparam.extend = p_pmdi004
            LET g_errparam.popup = TRUE
            CALL cl_err()

            LET r_success = FALSE
            RETURN r_success
         END IF
      END IF
      
      #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
      INITIALIZE g_chkparam.* TO NULL
      
      #設定g_chkparam.*的參數
      LET g_chkparam.arg1 = p_pmdi004

         
      #呼叫檢查存在並帶值的library
      IF cl_chk_exist("v_pmaa001_1") THEN
         #檢查成功時後續處理             
      ELSE
         #檢查失敗時後續處理
         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF
   RETURN r_success
