  <point name="statechange.b_update" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[         CALL s_transaction_begin()
      IF lc_state = 'Y' THEN
         CALL s_apmt440_conf_chk(g_pmdi_m.pmdidocno) RETURNING l_success
         IF NOT l_success THEN
            RETURN
         ELSE
            IF NOT cl_ask_confirm('aim-00108') THEN
               RETURN
            ELSE
               CALL s_apmt440_conf_upd(g_pmdi_m.pmdidocno) RETURNING l_success
               IF NOT l_success THEN
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','0')
               END IF
            END IF
         END IF
      END IF
      IF lc_state = 'N' THEN
         CALL s_apmt440_unconf_chk(g_pmdi_m.pmdidocno) RETURNING l_success
         IF NOT l_success THEN
            RETURN
         ELSE
            IF NOT cl_ask_confirm('aim-00110') THEN
               RETURN
            ELSE
               CALL s_apmt440_unconf_upd(g_pmdi_m.pmdidocno) RETURNING l_success
               IF NOT l_success THEN
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','0')
               END IF
            END IF
         END IF
      END IF
      IF lc_state = 'X' THEN
         CALL s_apmt440_invalid_chk(g_pmdi_m.pmdidocno) RETURNING l_success
         IF NOT l_success THEN
            RETURN
         ELSE
            IF NOT cl_ask_confirm('aim-00109') THEN
               RETURN
            ELSE
               CALL s_apmt440_invalid_upd(g_pmdi_m.pmdidocno) RETURNING l_success
               IF NOT l_success THEN
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','0')
               END IF
            END IF
         END IF
      END IF]]>
  </point>
