  <section id="apmt440.modify" ver="30" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION apmt440_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   DEFINE l_wc2_table1          STRING
   DEFINE l_wc2_table2   STRING
 
 
 
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point    
   #add-point:modify段define(客製用)
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point    
   
   #保存單頭舊值
   LET g_pmdi_m_t.* = g_pmdi_m.*
   LET g_pmdi_m_o.* = g_pmdi_m.*
   
   IF g_pmdi_m.pmdidocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_pmdidocno_t = g_pmdi_m.pmdidocno
 
   CALL s_transaction_begin()
   
   OPEN apmt440_cl USING g_enterprise,g_pmdi_m.pmdidocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN apmt440_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE apmt440_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE apmt440_master_referesh USING g_pmdi_m.pmdidocno INTO g_pmdi_m.pmdi001,g_pmdi_m.pmdisite, 
       g_pmdi_m.pmdidocno,g_pmdi_m.pmdidocdt,g_pmdi_m.pmdi002,g_pmdi_m.pmdi004,g_pmdi_m.pmdi003,g_pmdi_m.pmdistus, 
       g_pmdi_m.pmdi005,g_pmdi_m.pmdi006,g_pmdi_m.pmdi007,g_pmdi_m.pmdi008,g_pmdi_m.pmdi009,g_pmdi_m.pmdi011, 
       g_pmdi_m.pmdi019,g_pmdi_m.pmdi030,g_pmdi_m.pmdi013,g_pmdi_m.pmdi014,g_pmdi_m.pmdi010,g_pmdi_m.pmdi012, 
       g_pmdi_m.pmdi031,g_pmdi_m.pmdi032,g_pmdi_m.pmdi020,g_pmdi_m.pmdi015,g_pmdi_m.pmdi016,g_pmdi_m.pmdi017, 
       g_pmdi_m.pmdi018,g_pmdi_m.pmdiownid,g_pmdi_m.pmdiowndp,g_pmdi_m.pmdicrtid,g_pmdi_m.pmdicrtdp, 
       g_pmdi_m.pmdicrtdt,g_pmdi_m.pmdimodid,g_pmdi_m.pmdimoddt,g_pmdi_m.pmdicnfid,g_pmdi_m.pmdicnfdt, 
       g_pmdi_m.pmdi002_desc,g_pmdi_m.pmdi004_desc,g_pmdi_m.pmdi003_desc,g_pmdi_m.pmdi005_desc,g_pmdi_m.pmdi009_desc, 
       g_pmdi_m.pmdi011_desc,g_pmdi_m.pmdi019_desc,g_pmdi_m.pmdiownid_desc,g_pmdi_m.pmdiowndp_desc,g_pmdi_m.pmdicrtid_desc, 
       g_pmdi_m.pmdicrtdp_desc,g_pmdi_m.pmdimodid_desc,g_pmdi_m.pmdicnfid_desc
   
   #檢查是否允許此動作
   IF NOT apmt440_action_chk() THEN
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_pmdi_m_mask_o.* =  g_pmdi_m.*
   CALL apmt440_pmdi_t_mask()
   LET g_pmdi_m_mask_n.* =  g_pmdi_m.*
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
   #LET l_wc2_table2 = g_wc2_table2
   #LET l_wc2_table2 = " 1=1"
 
 
 
   
   CALL apmt440_show()
   #add-point:modify段show之後
   {<point name="modify.after_show"/>}
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
   #LET  g_wc2_table2 = l_wc2_table2 
 
 
 
    
   WHILE TRUE
      LET g_pmdidocno_t = g_pmdi_m.pmdidocno
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_pmdi_m.pmdimodid = g_user 
LET g_pmdi_m.pmdimoddt = cl_get_current()
LET g_pmdi_m.pmdimodid_desc = cl_get_username(g_pmdi_m.pmdimodid)
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      CALL apmt440_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE pmdi_t SET (pmdimodid,pmdimoddt) = (g_pmdi_m.pmdimodid,g_pmdi_m.pmdimoddt)
          WHERE pmdient = g_enterprise AND pmdidocno = g_pmdidocno_t
 
      END IF
    
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_pmdi_m.* = g_pmdi_m_t.*
         CALL apmt440_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_pmdi_m.pmdidocno != g_pmdidocno_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE pmdj_t SET pmdjdocno = g_pmdi_m.pmdidocno
 
          WHERE pmdjent = g_enterprise AND pmdjdocno = g_pmdidocno_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "pmdj_t" 
               LET g_errparam.code   = "std-00009" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "pmdj_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
         #更新單身key值
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update2" mark="Y"/>}
         #end add-point
         
         UPDATE pmdk_t
            SET pmdkdocno = g_pmdi_m.pmdidocno
 
          WHERE pmdkent = g_enterprise AND
                pmdkdocno = g_pmdidocno_t
 
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update2"/>}
         #end add-point
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "pmdk_t" 
               LET g_errparam.code   = "std-00009" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "pmdk_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update2"/>}
         #end add-point
 
 
         
 
         
         #UPDATE 多語言table key值
         
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL apmt440_set_act_visible()   
   CALL apmt440_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " pmdient = '" ||g_enterprise|| "' AND",
                      " pmdidocno = '", g_pmdi_m.pmdidocno, "' "
 
   #填到對應位置
   CALL apmt440_browser_fill("")
 
   CLOSE apmt440_cl
   
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL apmt440_msgcentre_notify('')
 
END FUNCTION   
]]>
  </section>
