  <point name="function.apmq860_comp_visible" order="1" ver="2" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[#p_cmd 1.畫面初始化
#      2.依資料開關欄位
PRIVATE FUNCTION apmq860_comp_visible(p_cmd)
   DEFINE p_cmd          LIKE type_t.chr1
   

   CASE p_cmd
      WHEN '1'
         
         #單頭
         CALL cl_set_comp_visible("lbl_pmds200,pmds200,b_pmds200",TRUE)
         CALL cl_set_comp_visible("lbl_pmds006,pmds006,b_pmds006",TRUE)
         #單身1
         CALL cl_set_comp_visible("b_pmds011",TRUE)
         CALL cl_set_comp_visible("b_pmds052",TRUE)
         CALL cl_set_comp_visible("b_pmds081",TRUE)
         #單身2
         CALL cl_set_comp_visible("b_pmdt207",TRUE)
         CALL cl_set_comp_visible("b_pmdt028",TRUE)
         CALL cl_set_comp_visible("b_pmdt025",TRUE)
         CALL cl_set_comp_visible("b_pmdt026",TRUE)
         CALL cl_set_comp_visible("b_pmdt203,b_pmdt203_desc",TRUE)
         CALL cl_set_comp_visible("b_pmdt204,b_pmdt204_desc",TRUE)
         CALL cl_set_comp_visible("b_pmdt205,b_pmdt205_desc",TRUE)
         CALL cl_set_comp_visible("b_pmdt208,b_pmdt208_desc",TRUE)
         CALL cl_set_comp_visible("b_pmdt209,",TRUE)
         CALL cl_set_comp_visible("b_pmdt210",TRUE)
         CALL cl_set_comp_visible("b_pmdt211,b_pmdt211_desc",TRUE)
         CALL cl_set_comp_visible("b_pmdt212",TRUE)
         CALL cl_set_comp_visible("b_pmdt213",TRUE)
         CALL cl_set_comp_visible("b_pmdt053",TRUE)
         CALL cl_set_comp_visible("b_pmdt055",TRUE)
         CALL cl_set_comp_visible("b_pmdt060",TRUE)
         CALL cl_set_comp_visible("b_pmdt061",TRUE)
         CALL cl_set_comp_visible("l_pmdo001,l_pmdo001_desc,l_pmdo001_desc_desc",TRUE)
         CALL cl_set_comp_visible("l_pmdo002,l_pmdo002_desc",TRUE)
         #單身3
         CALL cl_set_comp_visible("b_pmdu013",TRUE)
         CALL cl_set_comp_visible("b_pmdu014",TRUE)
         CALL cl_set_comp_visible("b_pmdu015",TRUE)
         IF g_argv[1] = '1' THEN
            CALL cl_set_comp_visible("lbl_pmds006,pmds006,b_pmds006",FALSE)
            CALL cl_set_comp_visible("b_pmds081",FALSE)            
            CALL cl_set_comp_visible("b_pmdt028",FALSE)
         ELSE
            CALL cl_set_comp_visible("lbl_pmds200,pmds200,b_pmds200",FALSE)
            CALL cl_set_comp_visible("b_pmds011",FALSE)
            CALL cl_set_comp_visible("b_pmds052",FALSE)
            IF g_argv[1] = '6' THEN
               CALL cl_set_comp_visible("b_pmds081",FALSE)
            END IF
            CALL cl_set_comp_visible("b_pmdt207",FALSE)
            CALL cl_set_comp_visible("b_pmdt025",FALSE)
            CALL cl_set_comp_visible("b_pmdt026",FALSE)
            CALL cl_set_comp_visible("b_pmdt203,b_pmdt203_desc",FALSE)
            CALL cl_set_comp_visible("b_pmdt204,b_pmdt204_desc",FALSE)
            CALL cl_set_comp_visible("b_pmdt205,b_pmdt205_desc",FALSE)
            CALL cl_set_comp_visible("b_pmdt208,b_pmdt208_desc",FALSE)
            CALL cl_set_comp_visible("b_pmdt209,",FALSE)
            CALL cl_set_comp_visible("b_pmdt210",FALSE)
            CALL cl_set_comp_visible("b_pmdt211,b_pmdt211_desc",FALSE)
            CALL cl_set_comp_visible("b_pmdt212",FALSE)
            CALL cl_set_comp_visible("b_pmdt213",FALSE)
            CALL cl_set_comp_visible("b_pmdt053",FALSE)
            CALL cl_set_comp_visible("b_pmdt055",FALSE)
            CALL cl_set_comp_visible("b_pmdt060",FALSE)
            CALL cl_set_comp_visible("b_pmdt061",FALSE)
            CALL cl_set_comp_visible("b_pmdu013",FALSE)
            CALL cl_set_comp_visible("b_pmdu014",FALSE)
            CALL cl_set_comp_visible("b_pmdu015",FALSE)
            CALL cl_set_comp_visible("l_pmdo001,l_pmdo001_desc,l_pmdo001_desc_desc",FALSE)
            CALL cl_set_comp_visible("l_pmdo002,l_pmdo002_desc",FALSE)
         END IF
      WHEN '2'
         CALL cl_set_comp_visible("b_pmdt207",TRUE)
         IF g_detail_idx > 0 THEN
            IF cl_null(g_pmds_d[g_detail_idx].pmds200) THEN
               CALL cl_set_comp_visible("b_pmdt207",FALSE)
            END IF
         END IF
   END CASE
END FUNCTION]]>
