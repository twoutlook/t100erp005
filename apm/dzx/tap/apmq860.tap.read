      CALL cl_ui_init()
   
      #程式初始化
      CALL apmq860_init()   
 
      #進入選單 Menu (="N")
      CALL apmq860_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_apmq860
      
   END IF 
   
   CLOSE apmq860_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
END MAIN
 
 
]]>
