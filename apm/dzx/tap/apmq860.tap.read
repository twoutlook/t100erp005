  <point name="function.apmq860_get_acc" order="2" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#依單據編號 找出歸屬的作業其設定的ACC內容
PRIVATE FUNCTION apmq860_get_acc(p_pmdsdocno)
   DEFINE p_pmdsdocno     LIKE pmds_t.pmdsdocno
   DEFINE l_pmds000       LIKE pmds_t.pmds000
   DEFINE l_prog          LIKE type_t.chr10
   DEFINE r_acc           LIKE type_t.chr10
   
   LET r_acc = ''
   LET l_pmds000 = ''
   SELECT pmds000 INTO l_pmds000 FROM pmds_t
    WHERE pmdsent = g_enterprise
      AND pmdsdocno = p_pmdsdocno
      
   CASE l_pmds000
      WHEN '1'
         LET l_prog = 'apmt860'
      WHEN '2'
         LET l_prog = 'apmt861'
      WHEN '3'
         LET l_prog = 'apmt862'
      WHEN '4'
         LET l_prog = 'apmt863'
      WHEN '5'
         LET l_prog = 'apmt870'
      WHEN '6'
         LET l_prog = 'apmt880'
      WHEN '7'
         LET l_prog = 'apmt890'
      OTHERWISE
         RETURN r_acc
   END CASE
   
   SELECT gzcb004 INTO r_acc FROM gzcb_t
    WHERE gzcb001 = '24'
      AND gzcb002 = l_prog
      
   RETURN r_acc
END FUNCTION]]>
  </point>
