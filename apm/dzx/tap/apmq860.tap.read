  <point name="menu.output" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[               LET l_wc = ''
               LET l_flag = 'N'
               FOR l_i = 1 TO g_pmds_d.getLength()
                  IF g_pmds_d[l_i].sel = 'Y' THEN
                     IF cl_null(l_wc) THEN
                        LET l_wc = "'",g_pmds_d[l_i].pmdsdocno CLIPPED,"'"
                     ELSE
                        LET l_wc = l_wc CLIPPED,",'",g_pmds_d[l_i].pmdsdocno CLIPPED,"'"
                     END IF
                     LET l_flag = 'Y'
                  END IF
               END FOR

               IF l_flag = 'Y' THEN
                  LET l_wc = "pmdsdocno IN (",l_wc,")"
                  CASE g_argv[1]
                    WHEN '1'
                       CALL apmr860_g01(l_wc,tm.multi_stock,'Y')
                    WHEN '5'
                       CALL apmr870_g01(l_wc,tm.multi_stock,'Y')
                    WHEN '6'
                       CALL apmr880_g01(l_wc,tm.multi_stock,'Y')   #150410 by lori522612 add 第三個參數值
                    WHEN '7'
                       CALL apmr890_g01(l_wc,tm.multi_stock,'Y')
                  END CASE
               ELSE
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.extend = ""
                  LET g_errparam.code   = "axr-00159"
                  LET g_errparam.popup  = TRUE
                  CALL cl_err()
               END IF

               CONTINUE DIALOG]]>
