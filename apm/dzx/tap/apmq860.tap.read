  <section id="apmq860.b_fill" ver="4" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION apmq860_b_fill()
   DEFINE ls_wc           STRING
   DEFINE l_pid           LIKE type_t.chr50
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準
   {<point name="b_fill.define" edit="s"/>}
   #end add-point
   #add-point:b_fill段define-客製
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
 
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
 
   CALL g_pmds_d.clear()
 
   #add-point:陣列清空
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1
 
   # b_fill段sql組成及FOREACH撰寫
   #應用 qs04 樣板自動產生(Version:4)
   #+ b_fill段資料取得(包含sql組成及FOREACH段撰寫)
   LET ls_sql_rank = "SELECT  UNIQUE '',pmdssite,'',pmdsdocno,pmds200,pmdsdocdt,pmds011,pmds002,'',pmds003, 
       '',pmds006,pmds007,'',pmds008,'',pmds009,'',pmds010,pmds052,pmds081,pmds045  ,DENSE_RANK() OVER( ORDER BY pmds_t.pmdsdocno) AS RANK FROM pmds_t", 

 
#table2
                     " LEFT JOIN pmdt_t ON pmdtent = pmdsent AND pmdsdocno = pmdtdocno",
#table3
                     " LEFT JOIN pmdu_t ON pmduent = pmdsent AND pmdsdocno = pmdudocno",
#table4
                     " LEFT JOIN xrcd_t ON xrcdent = pmdsent AND pmdsdocno = xrcddocno",
 
                     "",
                     " WHERE pmdsent= ? AND 1=1 AND ", ls_wc
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("pmds_t"),
                     " ORDER BY pmds_t.pmdsdocno"
 
   #add-point:b_fill段rank_sql_after
   {<point name="b_fill.rank_sql_after"/>}
   #end add-point
 
   LET g_sql = "SELECT COUNT(*) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql  #總筆數
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT '',pmdssite,'',pmdsdocno,pmds200,pmdsdocdt,pmds011,pmds002,'',pmds003,'',pmds006, 
       pmds007,'',pmds008,'',pmds009,'',pmds010,pmds052,pmds081,pmds045",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE apmq860_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR apmq860_pb
 
   OPEN b_fill_curs USING g_enterprise
 
   FOREACH b_fill_curs INTO g_pmds_d[l_ac].sel,g_pmds_d[l_ac].pmdssite,g_pmds_d[l_ac].pmdssite_desc, 
       g_pmds_d[l_ac].pmdsdocno,g_pmds_d[l_ac].pmds200,g_pmds_d[l_ac].pmdsdocdt,g_pmds_d[l_ac].pmds011, 
       g_pmds_d[l_ac].pmds002,g_pmds_d[l_ac].pmds002_desc,g_pmds_d[l_ac].pmds003,g_pmds_d[l_ac].pmds003_desc, 
       g_pmds_d[l_ac].pmds006,g_pmds_d[l_ac].pmds007,g_pmds_d[l_ac].pmds007_desc,g_pmds_d[l_ac].pmds008, 
       g_pmds_d[l_ac].pmds008_desc,g_pmds_d[l_ac].pmds009,g_pmds_d[l_ac].pmds009_desc,g_pmds_d[l_ac].pmds010, 
       g_pmds_d[l_ac].pmds052,g_pmds_d[l_ac].pmds081,g_pmds_d[l_ac].pmds045
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
 
      
 
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL apmq860_detail_show("'1'")
 
      CALL apmq860_pmds_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
 
   END FOREACH
 
 
 
 
 
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   CALL g_pmds_d.deleteElement(g_pmds_d.getLength())
 
   #add-point:陣列長度調整
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
 
   LET g_error_show = 0
 
   LET g_detail_cnt = g_pmds_d.getLength()
   LET l_ac = g_cnt
   LET g_cnt = 0
 
   #應用 qs06 樣板自動產生(Version:2)
   #+ b_fill段CURSOR釋放
   CLOSE b_fill_curs
   FREE apmq860_pb
 
 
 
 
 
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL apmq860_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL apmq860_detail_action_trans()
 
   LET l_ac = 1
   CALL apmq860_b_fill2()
 
