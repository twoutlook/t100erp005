  <section id="apmq832.b_fill" ver="6" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION apmq832_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2           STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE pmcpsite,'',pmcp006,'',pmcpdocno,pmcpseq,pmcp002,pmcp001,'','',pmcp004, 
       pmcp005,'',pmcp009,'',pmcp010,pmcp011,pmcp013,pmcp012,'',pmcp007,pmcp008,'',pmcp014 FROM pmcp_t", 

               "",
               " WHERE pmcpent= ? AND 1=1 AND ", p_wc2 
    
   LET g_sql = g_sql, #,cl_get_extra_cond('zzuser', 'zzgrup') 
                      " ORDER BY pmcp_t.pmcpdocno,pmcp_t.pmcpseq"
  
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
  
   PREPARE apmq832_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR apmq832_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_pmcp_d.clear()
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_pmcp_d[l_ac].pmcpsite,g_pmcp_d[l_ac].pmcpsite_desc,g_pmcp_d[l_ac].pmcp006, 
       g_pmcp_d[l_ac].pmcp006_desc,g_pmcp_d[l_ac].pmcpdocno,g_pmcp_d[l_ac].pmcpseq,g_pmcp_d[l_ac].pmcp002, 
       g_pmcp_d[l_ac].pmcp001,g_pmcp_d[l_ac].pmcp001_desc,g_pmcp_d[l_ac].pmcp001_desc1,g_pmcp_d[l_ac].pmcp004, 
       g_pmcp_d[l_ac].pmcp005,g_pmcp_d[l_ac].pmcp005_desc,g_pmcp_d[l_ac].pmcp009,g_pmcp_d[l_ac].pmcp009_desc, 
       g_pmcp_d[l_ac].pmcp010,g_pmcp_d[l_ac].pmcp011,g_pmcp_d[l_ac].pmcp013,g_pmcp_d[l_ac].pmcp012,g_pmcp_d[l_ac].pmcp012_desc, 
       g_pmcp_d[l_ac].pmcp007,g_pmcp_d[l_ac].pmcp008,g_pmcp_d[l_ac].pmcp008_desc,g_pmcp_d[l_ac].pmcp014 

      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL apmq832_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec THEN
      IF g_error_show = 1 THEN
         CALL cl_err("pmcp_t",9035,1)
      END IF
   END IF 
   LET g_error_show = 0
   
 
  
   
   CALL g_pmcp_d.deleteElement(g_pmcp_d.getLength())   
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_pmcp_d.getLength()
 
   END FOR
   
   IF g_cnt > g_pmcp_d.getLength() THEN
      LET l_ac = g_pmcp_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = g_pmcp_d.getLength() 
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE apmq832_pb
   
END FUNCTION
]]>
</section>
  <section id="apmq832.default_search" ver="2" status="" src="s">
<![CDATA[#+ 外部參數搜尋
PRIVATE FUNCTION apmq832_default_search()
   DEFINE li_idx  LIKE type_t.num5
   DEFINE li_cnt  LIKE type_t.num5
   DEFINE ls_wc   STRING
   #add-point:default_search段define
   {<point name="default_search.define"/>}
   #end add-point  
   
   IF NOT cl_null(g_argv[1]) THEN
      LET ls_wc = ls_wc, " pmcpdocno = '", g_argv[1], "' AND "
   END IF
   
   IF NOT cl_null(g_argv[02]) THEN
      LET ls_wc = ls_wc, " pmcpseq = ", g_argv[02], " AND "
   END IF
 
 
   #add-point:default_search段after sql
   {<point name="default_search.after_sql"/>}
   #end add-point  
   
   IF NOT cl_null(ls_wc) THEN
