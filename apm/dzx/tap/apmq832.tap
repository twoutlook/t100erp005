  <point name="b_fill.sql_after" cite_std="N" status="u" ver="1" src="s" new="N" order="" mark_hard="N">
<![CDATA[   LET g_sql = "SELECT  UNIQUE pmcpsite,'',pmcp006,'',pmcpdocno,pmcpseq,pmcp002,pmcp001,'','',pmcp004,pmcp005, 
       '',pmcp009,'',pmcp010,pmcp011,pmcp013,pmcp012,'',pmcp007,pmcp008,'',pmcp014 FROM pmco_t,pmcp_t",
               "",
               " WHERE pmcpent= ? AND 1=1 AND (pmcpsite = '",g_site,"' OR pmcpsite IN (SELECT ooed004 FROM ooed_t ",                                                                     
               "  START WITH ooed005= '",g_site,"' AND ooed001='2' AND ooed006<=to_date('",g_today,"','YYYY/MM/DD')",
               "    AND (ooed007>=to_date('",g_today,"','YYYY/MM/DD') OR ooed007 IS NULL) ",                                                                      
               "  CONNECT BY  nocycle PRIOR ooed004=ooed005 AND ooed001='2' AND ooed006<=to_date('",g_today,"','YYYY/MM/DD')",
               " AND (ooed007>=to_date('",g_today,"','YYYY/MM/DD') OR ooed007 IS NULL ))) AND ", p_wc2,
               " AND pmcoent = pmcpent AND pmcodocno =pmcpdocno AND pmcostus = 'Y'"
    
   LET g_sql = g_sql, " ORDER BY pmcp_t.pmcpdocno,pmcp_t.pmcpseq"]]>
</point>
