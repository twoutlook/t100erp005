       pmdt206          LIKE pmdt_t.pmdt206,    #預約收貨單號
       pmdt207          LIKE pmdt_t.pmdt207,    #預約收貨項次
       pmdt208          LIKE pmdt_t.pmdt208,    #採購渠道
