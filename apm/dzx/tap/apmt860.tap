  <section id="apmt860.ui_dialog" ver="200" status="" src="s" readonly="">
    <![CDATA[#+ 功能選單
PRIVATE FUNCTION apmt860_ui_dialog()
   DEFINE li_idx     LIKE type_t.num10
   DEFINE ls_wc      STRING
   DEFINE lb_first   BOOLEAN
   DEFINE la_wc      DYNAMIC ARRAY OF RECORD
          tableid    STRING,
          wc         STRING
          END RECORD
