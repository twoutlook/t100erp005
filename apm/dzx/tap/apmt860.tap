   LET g_pmdt_d[l_ac].pmdt053  = l_pmdt020          #允收數量
   LET g_pmdt_d[l_ac].pmdt063  = l_pmdt.pmdt063     #庫存管理特徵
   LET g_pmdt_d[l_ac].pmdt200  = l_pmdt.pmdt200     #商品條碼
