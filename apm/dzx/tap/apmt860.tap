      FOREACH b_fill_cs INTO g_pmdt_d[l_ac].pmdtsite,g_pmdt_d[l_ac].pmdtseq,g_pmdt_d[l_ac].pmdt206,g_pmdt_d[l_ac].pmdt207, 
