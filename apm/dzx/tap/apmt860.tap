     FROM pmdl_t,pmab_t,pmdn_t, pmdo_t
     LEFT OUTER JOIN (SELECT pmdtent,pmdt001,pmdt002,pmdt003,pmdt004,COUNT (pmdt001) cnt
                        FROM pmds_t,pmdt_t
                       WHERE pmdsent = pmdtent
