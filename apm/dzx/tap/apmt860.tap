                             g_pmdt_d[l_ac].pmdt001,g_pmds_m.pmdssite,
                             g_pmds_m.pmds037,g_pmdt_d[l_ac].pmdt024,
                             g_pmdt_d[l_ac].pmdt036,g_pmdt_d[l_ac].pmdt046)
      RETURNING g_pmdt_d[l_ac].pmdt038,g_pmdt_d[l_ac].pmdt047,g_pmdt_d[l_ac].pmdt039
   
   #商品編號
