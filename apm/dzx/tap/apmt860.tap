            LET g_pmds_m.pmds011 = "1"
      LET g_pmds_m.pmds021 = "N"
      LET g_pmds_m.pmds048 = "1"
      LET g_pmds_m.pmds049 = "1"
      LET g_pmds_m.pmds014 = "1"
