          pmdn053, pmdnorga, pmdo006, pmdn227  #150710-00016#7 Add By Ken 150713 加帶補貨規格說明
     INTO l_pmdt.pmdt005, l_pmdt.pmdt006, l_pmdt.pmdt007, l_pmdt.pmdt016,
