#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION apmi130_pmam001_ref()
        INITIALIZE g_ref_fields TO NULL
        LET g_ref_fields[1] = g_pmam_m.pmam001
        CALL ap_ref_array2(g_ref_fields,"SELECT pmaml003,pmaml004 FROM pmaml_t WHERE pmamlent='"||g_enterprise||"' AND pmaml001=? AND pmaml002 = '"||g_dlang||"'","") RETURNING g_rtn_fields
        LET g_pmam_m.pmaml003 = '', g_rtn_fields[1] , ''
        LET g_pmam_m.pmaml004 = '', g_rtn_fields[1] , ''
        DISPLAY BY NAME g_pmam_m.pmaml003,g_pmam_m.pmaml004
        
END FUNCTION]]>
