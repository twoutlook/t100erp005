      #end add-point
         
      IF SQLCA.sqlcode THEN
         CALL cl_err("pmdz_t",SQLCA.sqlcode,0) 
         CALL s_transaction_end('N','0')
         RETURN
      END IF    
 
      #add-point:單身刪除後
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
            
                                                               
 
 
 
                                                               
      CLEAR FORM
      CALL g_pmdz_d.clear() 
 
     
      #CALL apmt480_01_ui_browser_refresh()  
      CALL apmt480_01_ui_headershow()  
      CALL apmt480_01_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL apmt480_01_browser_fill("")
