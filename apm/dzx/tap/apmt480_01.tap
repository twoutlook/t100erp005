  <section id="apmt480_01.b_fill" ver="2" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION apmt480_01_b_fill()
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_pmdz_d.clear()    #g_pmdz_d 單頭及單身 
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF apmt480_01_fill_chk(1) THEN
   
      LET g_sql = "SELECT  UNIQUE pmdzsite,pmdzseq1,pmdz001,pmdz002,pmdz003,pmdz004,pmdz005 FROM pmdz_t", 
             
                  " INNER JOIN pmdy_t ON pmdydocno = pmdzdocno ",
                  " AND pmdyseq = pmdzseq ",
 
                  #"",
                  
                  "",
                  " WHERE"
      #add-point:b_fill段sql_before
      {<point name="b_fill.body.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table1) THEN
         LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
      END IF
      
      #子單身的WC
      
      
      LET g_sql = g_sql, " ORDER BY pmdz_t.pmdzseq1"
      
      #add-point:單身填充控制
      {<point name="b_fill.sql"/>}
      #end add-point
      
      PREPARE apmt480_01_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR apmt480_01_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_pmdy_m.pmdydocno
                                               ,g_pmdy_m.pmdyseq
 
                                               
      FOREACH b_fill_cs INTO g_pmdz_d[l_ac].pmdzsite,g_pmdz_d[l_ac].pmdzseq1,g_pmdz_d[l_ac].pmdz001, 
          g_pmdz_d[l_ac].pmdz002,g_pmdz_d[l_ac].pmdz003,g_pmdz_d[l_ac].pmdz004,g_pmdz_d[l_ac].pmdz005 

         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               CALL cl_err( '', 9035, 1)
            END IF
            EXIT FOREACH
         END IF
         
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
 
   
   #add-point:browser_fill段其他table處理
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_pmdz_d.deleteElement(g_pmdz_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE apmt480_01_pb
 
   
END FUNCTION
]]>
</section>
