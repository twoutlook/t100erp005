  <point name="browser_fill.before_prepare" cite_std="N" status="" ver="1" src="s" new="N" order="" mark_hard="N">
<![CDATA[   IF g_wc2 <> " 1=1" AND NOT cl_null(g_wc2) THEN 
      #依照pmdydocno,pmdyseq Browser欄位定義(取代原本bs_sql功能)
      LET l_sql_rank = "SELECT DISTINCT '',pmdydocno,pmdyseq,DENSE_RANK() OVER(ORDER BY pmdydocno, 
          pmdyseq ",g_order,") AS RANK ",
                        " FROM pmdy_t ",
                              " ",
                              " LEFT JOIN pmdz_t ON pmdzent = pmdyent AND pmdydocno = pmdzdocno AND pmdyseq = pmdzseq ",
 
 
                              " ",
                              " ",
                       " ",
                       " WHERE pmdyent = '" ||g_enterprise|| "' AND ",g_wc," AND ",g_wc2
   ELSE
      #單身無輸入資料
      LET l_sql_rank = "SELECT DISTINCT '',pmdydocno,pmdyseq,DENSE_RANK() OVER(ORDER BY pmdydocno, 
          pmdyseq ",g_order,") AS RANK ",
                       " FROM pmdy_t ",
                            "  ",
                            "  ",
                       " WHERE pmdyent = '" ||g_enterprise|| "' AND ", g_wc
   END IF
   
   #定義翻頁CURSOR
   LET g_sql= "SELECT '',pmdydocno,pmdyseq FROM (",l_sql_rank,") WHERE ",
              " RANK >= ",1," AND RANK<",1+g_max_browse,
              " ORDER BY ",l_searchcol," ",g_order]]>
