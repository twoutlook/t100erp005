  <section id="apmt480_01.modify" ver="3" status="" src="s">
<![CDATA[#+ 資料修改
PRIVATE FUNCTION apmt480_01_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_pmdy_m.pmdydocno IS NULL
   OR g_pmdy_m.pmdyseq IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE apmt480_01_master_referesh USING g_pmdy_m.pmdydocno,g_pmdy_m.pmdyseq INTO g_pmdy_m.pmdydocno, 
       g_pmdy_m.pmdyseq,g_pmdy_m.pmdy002,g_pmdy_m.pmdy003,g_pmdy_m.pmdy004,g_pmdy_m.pmdy006,g_pmdy_m.pmdy007, 
       g_pmdy_m.pmdysite,g_pmdy_m.pmdy008
 
   ERROR ""
  
   LET g_pmdydocno_t = g_pmdy_m.pmdydocno
   LET g_pmdyseq_t = g_pmdy_m.pmdyseq
 
   CALL s_transaction_begin()
   
   OPEN apmt480_01_cl USING g_enterprise,g_pmdy_m.pmdydocno
                                                       ,g_pmdy_m.pmdyseq
 
   IF STATUS THEN
      CALL cl_err("OPEN apmt480_01_cl:", STATUS, 1)
      CLOSE apmt480_01_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH apmt480_01_cl INTO g_pmdy_m.pmdydocno,g_pmdy_m.pmdyseq,g_pmdy_m.pmdx005,g_pmdy_m.pmdy002,g_pmdy_m.imaal003, 
       g_pmdy_m.imaal004,g_pmdy_m.pmdy003,g_pmdy_m.pmdy004,g_pmdy_m.pmdy006,g_pmdy_m.pmdy006_desc,g_pmdy_m.pmdy007, 
       g_pmdy_m.pmdysite,g_pmdy_m.pmdy008
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_pmdy_m.pmdydocno,SQLCA.sqlcode,0)
      CLOSE apmt480_01_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   CALL apmt480_01_show()
   WHILE TRUE
      LET g_pmdydocno_t = g_pmdy_m.pmdydocno
      LET g_pmdyseq_t = g_pmdy_m.pmdyseq
 
      
      #寫入修改者/修改日期資訊(單頭)
      
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL apmt480_01_input("u")     #欄位更改
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_pmdy_m.* = g_pmdy_m_t.*
         CALL apmt480_01_show()
         CALL cl_err('',9001,0)
         CALL s_transaction_end('N','0')
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_pmdy_m.pmdydocno != g_pmdydocno_t 
      OR g_pmdy_m.pmdyseq != g_pmdyseq_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE pmdz_t SET pmdzdocno = g_pmdy_m.pmdydocno
                                      ,pmdzseq = g_pmdy_m.pmdyseq
 
          WHERE pmdzent = g_enterprise AND pmdzdocno = g_pmdydocno_t
            AND pmdzseq = g_pmdyseq_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("pmdz_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("pmdz_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_pmdy_m.pmdydocno,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE apmt480_01_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_pmdy_m.pmdydocno,'U')
 
END FUNCTION   
]]>
</section>
