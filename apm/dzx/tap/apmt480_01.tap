      LET g_pmdyseq_t = g_pmdy_m.pmdyseq
 
 
      DELETE FROM pmdy_t
       WHERE pmdyent = g_enterprise AND pmdydocno = g_pmdy_m.pmdydocno
         AND pmdyseq = g_pmdy_m.pmdyseq
 
       
      #add-point:單頭刪除中
      {<point name="delete.head.m_delete"/>}
      #end add-point
       
      IF SQLCA.sqlcode THEN
         CALL cl_err(g_pmdy_m.pmdydocno,SQLCA.sqlcode,0) 
         CALL s_transaction_end('N','0')
         RETURN
      END IF
      
      #add-point:單頭刪除後
      {<point name="delete.head.a_delete"/>}
      #end add-point
  
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM pmdz_t
       WHERE pmdzent = g_enterprise AND pmdzdocno = g_pmdy_m.pmdydocno
         AND pmdzseq = g_pmdy_m.pmdyseq
 
 
      #add-point:單身刪除中
