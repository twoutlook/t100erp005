   OR g_pmdy_m.pmdyseq IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE apmt480_01_master_referesh USING g_pmdy_m.pmdydocno,g_pmdy_m.pmdyseq INTO g_pmdy_m.pmdydocno, 
       g_pmdy_m.pmdyseq,g_pmdy_m.pmdy002,g_pmdy_m.pmdy003,g_pmdy_m.pmdy004,g_pmdy_m.pmdy006,g_pmdy_m.pmdy007, 
       g_pmdy_m.pmdysite,g_pmdy_m.pmdy008
   
   
 
