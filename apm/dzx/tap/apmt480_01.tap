  <point name="function.apmt480_01_pmdz001_round" cite_std="N" status="" ver="1" src="s" new="Y" order="1" mark_hard="N">
<![CDATA[
PRIVATE FUNCTION apmt480_01_pmdz001_round()
DEFINE l_success   LIKE type_t.num5,
       l_ooca002   LIKE ooca_t.ooca002,
       l_ooca004   LIKE ooca_t.ooca004

   IF NOT cl_null(g_pmdy_m.pmdy008) AND NOT cl_null(g_pmdz_d[l_ac].pmdz001) THEN 
      CALL s_aooi250_get_msg(g_pmdy_m.pmdy008) RETURNING l_success,l_ooca002,l_ooca004
      IF l_success = TRUE THEN 
         CALL s_num_round(l_ooca004,g_pmdz_d[l_ac].pmdz001,l_ooca002) RETURNING g_pmdz_d[l_ac].pmdz001
      END IF
   END IF
END FUNCTION]]>
</point>
  <point name="function.apmt480_01_pmdz001" cite_std="N" status="" ver="1" src="s" new="Y" order="2" mark_hard="N">
<![CDATA[
PRIVATE FUNCTION apmt480_01_pmdz001()
DEFINE l_pmdy009   LIKE pmdy_t.pmdy009
DEFINE l_max       LIKE pmdy_t.pmdy009
DEFINE r_success   LIKE type_t.num5
   
   LET r_success = TRUE
   SELECT MAX(pmdz001) INTO l_max 
     FROM pmdz_t
    WHERE pmdzent = g_enterprise
      AND pmdzdocno = g_pmdy_m.pmdydocno
      AND pmdzseq = g_pmdy_m.pmdyseq
      AND pmdzseq1 <> g_pmdz_d[l_ac].pmdzseq1
   IF cl_null(l_max) THEN LET l_max = 0 END IF
   IF l_max >= g_pmdz_d[l_ac].pmdz001 THEN 
      CALL cl_err(g_pmdz_d[l_ac].pmdz001,'axm-00224',1)
      LET r_success = FALSE
      RETURN r_success
   END IF
   
   SELECT pmdy009 INTO l_pmdy009 
     FROM pmdy_t
    WHERE pmdyent = g_enterprise
      AND pmdydocno = g_pmdy_m.pmdydocno
      AND pmdyseq = g_pmdy_m.pmdyseq
   IF cl_null(l_pmdy009) THEN 
      LET l_pmdy009 = g_pmdy009
   END IF
   IF l_pmdy009 < g_pmdz_d[l_ac].pmdz001 THEN 
      CALL cl_err(g_pmdz_d[l_ac].pmdz001,'axm-00223',1)
      LET r_success = FALSE
      RETURN r_success
   END IF

   CALL apmt480_01_pmdz001_round() 
   RETURN r_success
   
END FUNCTION]]>
</point>
  <point name="function.apmt480_01_pmdy_show" cite_std="N" status="" ver="1" src="s" new="Y" order="3" mark_hard="N">
<![CDATA[
PRIVATE FUNCTION apmt480_01_pmdy_show()
DEFINE l_pmdxstus  LIKE pmdx_t.pmdxstus

   LET g_pmdy_m.pmdydocno = g_pmdydocno
   LET g_pmdy_m.pmdyseq = g_pmdyseq 
   LET g_pmdy_m.pmdy002 = g_pmdy002 
   LET g_pmdy_m.pmdy003 = g_pmdy003 
   LET g_pmdy_m.pmdy004 = g_pmdy004 
   LET g_pmdy_m.pmdy006 = g_pmdy006 
   LET g_pmdy_m.pmdy007 = g_pmdy007 
   LET g_pmdy_m.pmdy008 = g_pmdy008 
   LET g_pmdy_m.pmdysite = g_site
   LET l_pmdxstus = ''
   SELECT pmdx005,pmdxstus INTO g_pmdy_m.pmdx005,l_pmdxstus FROM pmdx_t 
    WHERE pmdxent = g_enterprise
      AND pmdxdocno = g_pmdydocno
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_pmdy_m.pmdy002
   CALL ap_ref_array2(g_ref_fields,"SELECT imaal003,imaal004 FROM imaal_t WHERE imaalent='"||g_enterprise||"' AND imaal001=? AND imaal002='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_pmdy_m.imaal003 = '', g_rtn_fields[1] , ''
   LET g_pmdy_m.imaal004 = '', g_rtn_fields[2] , ''
   DISPLAY BY NAME g_pmdy_m.imaal003,g_pmdy_m.imaal004
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_pmdy_m.pmdy006
   CALL ap_ref_array2(g_ref_fields,"SELECT oocql004 FROM oocql_t WHERE oocqlent='"||g_enterprise||"' AND oocql001='221' AND oocql002=? AND oocql003='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_pmdy_m.pmdy006_desc = '', g_rtn_fields[1] , ''
   DISPLAY BY NAME g_pmdy_m.pmdy006_desc

   DISPLAY BY NAME g_pmdy_m.pmdydocno,g_pmdy_m.pmdyseq,g_pmdy_m.pmdy008,g_pmdy_m.pmdy002,g_pmdy_m.imaal003, 
       g_pmdy_m.imaal004,g_pmdy_m.pmdy003,g_pmdy_m.pmdy004,g_pmdy_m.pmdy006,g_pmdy_m.pmdy006_desc,g_pmdy_m.pmdy007, 
       g_pmdy_m.pmdysite,g_pmdy_m.pmdx005
       
    
    CALL cl_set_act_visible("modify",TRUE)
    IF l_pmdxstus != 'N' THEN
       CALL cl_set_act_visible("modify",FALSE)
    END IF       
END FUNCTION]]>
</point>
