   CALL s_transaction_begin()
 
   OPEN apmt480_01_cl USING g_enterprise,g_pmdy_m.pmdydocno
                                                       ,g_pmdy_m.pmdyseq
 
   IF STATUS THEN
      CALL cl_err("OPEN apmt480_01_cl:", STATUS, 1)
      CLOSE apmt480_01_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   FETCH apmt480_01_cl INTO g_pmdy_m.pmdydocno,g_pmdy_m.pmdyseq,g_pmdy_m.pmdx005,g_pmdy_m.pmdy002,g_pmdy_m.imaal003, 
       g_pmdy_m.imaal004,g_pmdy_m.pmdy003,g_pmdy_m.pmdy004,g_pmdy_m.pmdy006,g_pmdy_m.pmdy006_desc,g_pmdy_m.pmdy007, 
       g_pmdy_m.pmdysite,g_pmdy_m.pmdy008              #鎖住將被更改或取消的資料
