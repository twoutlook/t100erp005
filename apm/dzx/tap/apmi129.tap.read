  <point name="function.apmi129_get_price" order="13" ver="6" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 通過捨入取得價格
# Date & Author..: 2014/02/26 By chenjing
# Modify.........:
################################################################################
PRIVATE FUNCTION apmi129_get_price(p_price,p_w,p_s,p_r)
   DEFINE   p_price     LIKE pmaw_t.pmaw019
   DEFINE   p_w          LIKE gzcb_t.gzcb002
   DEFINE   p_r          LIKE type_t.num5
   DEFINE   p_s          LIKE type_t.num5
   DEFINE   l_price      LIKE pmaw_t.pmaw019 
   DEFINE   l_num_s      LIKE pmaw_t.pmaw019
   DEFINE   l_num        LIKE type_t.num20
   DEFINE   l_num1       LIKE pmaw_t.pmaw019 
   DEFINE   l_num2       LIKE type_t.num20
   
   
   IF NOT cl_null(p_price) AND NOT cl_null(p_w) THEN
      #add by lixh 20150603  #配合SCC碼調整
      SELECT gzcbl004 INTO p_w FROM gzcbl_t
       WHERE gzcbl001 = '2078'
         AND gzcbl002 = p_w
         AND gzcbl003 = g_lang   
      #add by lixh 20150603       
      IF cl_null(p_s) AND cl_null(p_r) THEN
         LET l_price = p_price 
         RETURN l_price
      END IF 
      LET l_num_s = p_price / p_w      
      CALL s_num_round(3,l_num_s,'') RETURNING l_num
      LET l_num1 = (p_price /p_w - l_num)*10
      CALL s_num_round(3,l_num1,'') RETURNING l_num2 
      IF NOT cl_null(p_s) AND NOT cl_null(p_r) THEN
         IF l_num2 > = p_r THEN
            LET l_price = (l_num+1) * p_w
         ELSE
            LET l_price = l_num * p_w         
         END IF
      END IF
      IF p_s >= 0 AND cl_null(p_r) THEN
         LET l_price = l_num * p_w       
      END IF
      IF cl_null(p_s) AND p_r >= 0 THEN
         LET l_price = (l_num+1) * p_w
      END IF
   END IF
   RETURN l_price
   
END FUNCTION]]>
  </point>
  <point name="function.apmi129_pmaw011_chk" order="14" ver="6" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 該料號有被其他料參考就不可以再修改
# Date & Author..: 2014/02/22
# Modify.........:
################################################################################
PRIVATE FUNCTION apmi129_pmaw011_chk()
DEFINE   l_cnt       LIKE type_t.num5

   LET l_cnt = 0
   IF NOT cl_null(g_pmaw_m.pmaw001) AND NOT cl_null(g_pmaw_m.pmaw002) AND NOT cl_null(g_pmaw_d[l_ac].pmaw011) 
      AND g_pmaw_d[l_ac].pmaw012 IS NOT NULL AND NOT cl_null(g_pmaw_d[l_ac].pmaw013) THEN
      SELECT COUNT(pmaw001) INTO l_cnt FROM pmaw_t
       WHERE pmawent = g_enterprise
         AND pmaw001 = g_pmaw_m.pmaw001
         AND pmaw002 = g_pmaw_m.pmaw002
         AND pmaw013 = g_pmaw_d_t.pmaw013
         AND pmaw015 = g_pmaw_d_t.pmaw011
         AND pmaw016 = g_pmaw_d_t.pmaw012
         AND (pmaw012 != g_pmaw_d_t.pmaw012 OR pmaw011 != g_pmaw_d_t.pmaw011)                        
         AND pmawstus = 'Y' 
         
      IF l_cnt > 0 THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'axm-00082'
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()

         LET g_pmaw_d[l_ac].pmaw011 = g_pmaw_d_t.pmaw011
         LET g_pmaw_d[l_ac].pmaw012 = g_pmaw_d_t.pmaw012
         LET g_pmaw_d[l_ac].pmaw013 = g_pmaw_d_t.pmaw013
         LET g_pmaw_d[l_ac].pmaw015 = g_pmaw_d_t.pmaw015
         LET g_pmaw_d[l_ac].pmaw016 = g_pmaw_d_t.pmaw016 
         CALL apmi129_pmaw011_desc()
         CALL apmi129_pmaw015_desc()
         CALL apmi129_pmaw013_desc()
         DISPLAY BY NAME g_pmaw_d[l_ac].pmaw011,g_pmaw_d[l_ac].pmaw012,g_pmaw_d[l_ac].pmaw013,g_pmaw_d[l_ac].pmaw015,g_pmaw_d[l_ac].pmaw016                                  
         RETURN FALSE
      END IF  
   END IF 
   RETURN TRUE   
END FUNCTION]]>
