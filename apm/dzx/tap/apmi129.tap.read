   DEFINE   l_pmaw034       LIKE pmaw_t.pmaw034
   DEFINE   l_chk           LIKE type_t.num5
 
   IF p_pmaw015 IS NULL OR p_pmaw016 IS NULL THEN
      RETURN 
   END IF 
   LET l_chk = FALSE 
   #dorislai-20150628-modify----(S)        
#   SELECT pmaw015,pmaw016 INTO  l_pmaw015,l_pmaw016 FROM pmaw_t 
#    WHERE pmawent = g_enterprise
