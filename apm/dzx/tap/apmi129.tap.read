# Memo...........:
# Usage..........: CALL apmi129_pmaw031_chk()
# Return code....: 回傳TRUE/FALSE
# Date & Author..: 2014/06/14 By dorislai
# Modify.........:
################################################################################
PRIVATE FUNCTION apmi129_pmaw031_chk()
   DEFINE   l_cnt       LIKE type_t.num5
   DEFINE   l_msg       STRING
   LET l_cnt = 0
   IF NOT cl_null(g_pmaw_m.pmaw001) AND NOT cl_null(g_pmaw_m.pmaw002) AND NOT cl_null(g_pmaw_d[l_ac].pmaw031)  AND NOT cl_null(g_pmaw_d[l_ac].pmaw013) THEN
      SELECT COUNT(pmaw001) INTO l_cnt FROM pmaw_t
       WHERE pmawent = g_enterprise
         AND pmaw001 = g_pmaw_m.pmaw001
         AND pmaw002 = g_pmaw_m.pmaw002
         AND pmaw013 = g_pmaw_d_t.pmaw013
         AND pmaw033 = g_pmaw_d_t.pmaw031
         AND pmaw031 != g_pmaw_d_t.pmaw031                
         AND pmawstus = 'Y' 
         
      IF l_cnt > 0 THEN
         CALL cl_getmsg('axm-00648',g_lang) RETURNING l_msg      
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'apm-00958'
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         LET g_errparam.replace[1] = l_msg
         CALL cl_err()

         LET g_pmaw_d[l_ac].pmaw013 = g_pmaw_d_t.pmaw013
         LET g_pmaw_d[l_ac].pmaw033 = g_pmaw_d_t.pmaw031   
         DISPLAY BY NAME g_pmaw_d[l_ac].pmaw013,g_pmaw_d[l_ac].pmaw013_desc
         DISPLAY BY NAME g_pmaw_d[l_ac].pmaw031,g_pmaw_d[l_ac].pmaw031_desc
         CALL apmi129_pmaw013_desc()
         CALL apmi129_pmaw031_desc()
         
         RETURN FALSE
      END IF  
   END IF 
   RETURN TRUE
END FUNCTION]]>
