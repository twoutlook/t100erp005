  <point name="function.apmi129_auto_gene" order="10" ver="6" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 整批產生採購價格表
# Date & Author..: 2014/02/22 By chenjing
# Modify.........:
################################################################################
PRIVATE FUNCTION apmi129_auto_gene(l_type)
   DEFINE   g_pmaw001_s       LIKE pmaw_t.pmaw001
   DEFINE   g_pmaw002_s       LIKE pmaw_t.pmaw002 
   DEFINE   l_s_imaa001_t     LIKE imaa_t.imaa001
   DEFINE   l_e_imaa001_t     LIKE imaa_t.imaa001
   DEFINE   l_wc              STRING
   DEFINE   l_wc_1            STRING  
   DEFINE   l_type            LIKE type_t.chr1   

   #畫面開啟 (identifier)
   OPEN WINDOW w_apmi129_s01 WITH FORM cl_ap_formpath("apm","apmi129_s01")
   INITIALIZE g_pmaw_s.* TO NULL
   LET g_errshow = 1
   LET l_s_imaa001_t = NULL
   LET l_e_imaa001_t = NULL   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
   
      #螢幕上取條件
      CONSTRUCT BY NAME l_wc ON s_imaa001,e_imaa001

         AFTER FIELD s_imaa001
            LET g_s_imaa001 = GET_FLDBUF(s_imaa001)   
            

         ON ACTION controlp INFIELD s_imaa001

	        INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'i'
	        LET g_qryparam.reqry = FALSE
            LET g_qryparam.arg1 = "ALL"
	        LET l_s_imaa001_t = g_s_imaa001
            CALL q_imaf001_8()                       #呼叫開窗
            LET g_s_imaa001 = g_qryparam.return1
            IF cl_null(g_s_imaa001) THEN 
               LET g_s_imaa001 = l_s_imaa001_t
            END IF    
            DISPLAY g_s_imaa001 TO s_imaa001  #顯示到畫面上

            NEXT FIELD s_imaa001
            
         AFTER FIELD e_imaa001
            LET g_e_imaa001 = GET_FLDBUF(e_imaa001)            

           
         ON ACTION controlp INFIELD e_imaa001
	        INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'i'
	        LET g_qryparam.reqry = FALSE
            LET g_qryparam.arg1 = "ALL"
            LET l_e_imaa001_t = g_e_imaa001
            CALL q_imaf001_8()                         #呼叫開窗
            LET g_e_imaa001 = g_qryparam.return1
            IF cl_null(g_e_imaa001) THEN
               LET g_e_imaa001 = l_e_imaa001_t 
            END IF
            DISPLAY g_e_imaa001 TO e_imaa001  #顯示到畫面上

            NEXT FIELD e_imaa001
      END CONSTRUCT
      
      CONSTRUCT BY NAME l_wc_1 ON imaa003,imaf141

         AFTER FIELD imaa003

            

         ON ACTION controlp INFIELD imaa003

	    INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c'
	    LET g_qryparam.reqry = FALSE
            CALL q_imca001_1()                         #呼叫開窗
            DISPLAY g_qryparam.return1 TO imaa003  #顯示到畫面上

            NEXT FIELD imaa003

         AFTER FIELD imaf111
         

           
         ON ACTION controlp INFIELD imaf141
	        INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c'
	        LET g_qryparam.reqry = FALSE
	        LET g_qryparam.arg1 = "ALL"
            CALL q_imce141_2()                         #呼叫開窗
            DISPLAY g_qryparam.return1 TO imaf141  #顯示到畫面上
            NEXT FIELD imaf141
            
      END CONSTRUCT  
      
      INPUT BY NAME g_pmaw_s.pmaw001,g_pmaw_s.pmaw002,g_pmaw_s.chk1,
                    g_pmaw_s.pmaw019_1,g_pmaw_s.pmaw019_w,g_pmaw_s.pmaw019_s,g_pmaw_s.pmaw019_r,g_pmaw_s.chk2,
                    g_pmaw_s.pmaw020_mon,g_pmaw_s.pmaw020_rate,g_pmaw_s.pmaw020_w,g_pmaw_s.pmaw020_s,
                    g_pmaw_s.pmaw020_r,g_pmaw_s.pmaw021,g_pmaw_s.pmaw022

         ATTRIBUTE(WITHOUT DEFAULTS)     

         BEFORE INPUT 
            INITIALIZE g_pmaw_s.* TO NULL
            CALL cl_set_combo_scc('pmaw019_1','2071') 
            CALL cl_set_combo_scc('pmaw019_w','2078')  
            CALL cl_set_combo_scc('pmaw020_w','2078')
            
            LET g_pmaw_s.pmaw019_1 = '1'
            LET g_pmaw_s.chk1 = 'N'
            LET g_pmaw_s.chk2 = 'Y'
            CALL cl_set_comp_entry("pmaw020_mon",TRUE)
            CALL cl_set_comp_entry("pmaw020_rate",TRUE)   
            IF l_type = 'a' THEN
               LET g_pmaw_s.pmaw001 = g_pmaw_m.pmaw001
               LET g_pmaw_s.pmaw002 = g_pmaw_m.pmaw002
               CALL apmi129_pmaw001_ref() 
               CALL apmi129_pmaw002_ref() 
               CALL cl_set_comp_entry("pmaw001,pmaw002",FALSE)  
            ELSE
               CALL cl_set_comp_entry("pmaw001,pmaw002",TRUE)            
            END IF             
         AFTER FIELD pmaw001
           CALL apmi129_pmaw001_ref() 
            IF NOT cl_null(g_pmaw_s.pmaw001) THEN
               INITIALIZE g_chkparam.* TO NULL
               LET g_chkparam.arg1 = g_pmaw_s.pmaw001
               IF NOT cl_chk_exist("v_ooal002_14") THEN
                  LET g_pmaw_s.pmaw001 = g_pmaw001_s
                  CALL apmi129_pmaw001_ref()
                  NEXT FIELD CURRENT
               END IF
            END IF

         AFTER FIELD pmaw002
            CALL apmi129_pmaw002_ref() 
            IF NOT cl_null(g_pmaw_s.pmaw002) THEN
               INITIALIZE g_chkparam.* TO NULL
               #LET g_chkparam.arg1 = 'ALL'
               LET g_chkparam.arg1 = g_site   #add by lixh 20150327
               LET g_chkparam.arg2 = g_pmaw_s.pmaw002
               IF NOT cl_chk_exist("v_ooaj002") THEN
                  LET g_pmaw_s.pmaw002 = g_pmaw002_s
                  CALL apmi129_pmaw002_ref()
                  NEXT FIELD CURRENT
               END IF
            END IF
         AFTER FIELD pmaw019_s
            IF NOT cl_ap_chk_Range(g_pmaw_s.pmaw019_s,"0","1","9","1","azz-00087",1) THEN
               NEXT FIELD pmaw019_s
            END IF   
            IF NOT cl_null(g_pmaw_s.pmaw019_s) AND NOT cl_null(g_pmaw_s.pmaw019_r) THEN 
               IF g_pmaw_s.pmaw019_s = g_pmaw_s.pmaw019_r THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = 'apm-00326'
                  LET g_errparam.extend = g_pmaw_s.pmaw019_s
                  LET g_errparam.popup = TRUE
                  CALL cl_err()
 
               END IF
            END IF               
            IF NOT cl_null(g_pmaw_s.pmaw019_s) THEN
               LET g_pmaw_s.pmaw019_r = g_pmaw_s.pmaw019_s + 1
               IF g_pmaw_s.pmaw019_r =10 THEN 
                  LET g_pmaw_s.pmaw019_r = '' 
               END IF   
               DISPLAY g_pmaw_s.pmaw019_r TO pmaw019_r
            END IF
            CALL apmi129_pmaw_desc(g_pmaw_s.pmaw019_s,g_pmaw_s.pmaw019_r)
         AFTER FIELD pmaw019_r
            IF NOT cl_ap_chk_Range(g_pmaw_s.pmaw019_r,"0","1","9","1","azz-00087",1) THEN
               NEXT FIELD pmaw019_r
            END IF   
            IF NOT cl_null(g_pmaw_s.pmaw019_s) AND NOT cl_null(g_pmaw_s.pmaw019_r) THEN 
               IF g_pmaw_s.pmaw019_s = g_pmaw_s.pmaw019_r THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = 'apm-00326'
                  LET g_errparam.extend = g_pmaw_s.pmaw019_s
                  LET g_errparam.popup = TRUE
                  CALL cl_err()
 
               END IF
            END IF               
            IF NOT cl_null(g_pmaw_s.pmaw019_r) THEN
               LET g_pmaw_s.pmaw019_s = g_pmaw_s.pmaw019_r - 1
               IF g_pmaw_s.pmaw019_s = -1 THEN 
                  LET g_pmaw_s.pmaw019_s = '' 
