  <point name="function.apmi129_pmaw031_pmaw032_chk_unit" order="36" ver="6" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 系列或產品分類的單位校驗
# Memo...........:
# Usage..........: CALL apmi129_pmaw031_pmaw032_chk_unit()
#                  RETURNING TRUE/FALSE
# Input parameter: 回傳 TRUE/FALSE
# Return code....: 
# Date & Author..: 2015-06-19 By dorislai
# Modify.........:
################################################################################
PRIVATE FUNCTION apmi129_pmaw031_pmaw032_chk_unit()
   DEFINE  l_sql                 STRING                               #抓取sql語法
   DEFINE  l_chk                 LIKE  type_t.num5                    #判斷是否有抓到符合的值
   DEFINE  l_imaa001             LIKE  type_t.chr100                  #接收抓取到的料件編號值 

   #先依欄位抓取sql語法
      #---用系列去抓料件編號
   IF NOT cl_null(g_pmaw_d[l_ac].pmaw031) THEN
      LET l_sql = "SELECT imaa001 FROM imaa_t ",
                  "WHERE imaaent = '"||g_enterprise||"' AND imaa127 = '"||g_pmaw_d[l_ac].pmaw031||"'",
                  "ORDER BY imaa001"
   END IF
      #----用產品分類去抓料件編號
   IF NOT cl_null(g_pmaw_d[l_ac].pmaw032) THEN
      LET l_sql = "SELECT imaa001 FROM imaa_t ",
                  "WHERE imaaent = '"||g_enterprise||"' AND imaa009 = '"||g_pmaw_d[l_ac].pmaw032||"'",
                  "ORDER BY imaa001"
   END IF
               
   
   PREPARE apmi129_get_imaa001_chk_pre FROM l_sql
   DECLARE apmi129_get_imaa001_chk_curs CURSOR FOR apmi129_get_imaa001_chk_pre
   LET l_imaa001 =''
   LET l_chk = FALSE  #判斷是否有抓到符合的值
   #一筆一筆比對是否有符合任一 料件編號中的可使用單位
   FOREACH apmi129_get_imaa001_chk_curs INTO l_imaa001 
      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_pmaw_d[l_ac].pmaw013
      #以COUNT來計算，並判斷是否有比對到符合的資料
      CALL ap_ref_array2(g_ref_fields,"SELECT COUNT(*) FROM imao_t WHERE imaoent='"||g_enterprise||"' AND imao002 = ? AND imao001 = '"||l_imaa001||"'","") RETURNING g_rtn_fields
      IF g_rtn_fields[1] > 0 THEN
         LET l_chk = TRUE 
         EXIT FOREACH
      END IF 
   END FOREACH
   FREE  apmi129_get_imaa001_chk_pre  #已使用完畢，釋放
   #若料件編號中無相對應的可使用單位，跳出錯誤訊息提示
   IF NOT l_chk THEN
      RETURN FALSE
   END IF              
   RETURN TRUE
END FUNCTION]]>
