    <![CDATA[################################################################################
# Descriptions...: 採購價格表整批產生
# Date & Author..: 2014/02/22 By chenjing
# Modify.........:
################################################################################
PRIVATE FUNCTION apmi129_auto_gene_pmaw(p_wc)
DEFINE    p_wc        STRING  
DEFINE    l_sql       STRING
DEFINE    l_imaa001   LIKE imaa_t.imaa001
DEFINE    l_pmaw      RECORD LIKE pmaw_t.*
DEFINE    l_pmaw001   LIKE pmaw_t.pmaw001
DEFINE    l_pmaw002   LIKE pmaw_t.pmaw002
DEFINE    l_pmaw011   LIKE pmaw_t.pmaw011
DEFINE    l_pmaw012   LIKE pmaw_t.pmaw012
DEFINE    l_pmaw013   LIKE pmaw_t.pmaw013
DEFINE    l_pmaw031   LIKE pmaw_t.pmaw031
DEFINE    l_pmaw032   LIKE pmaw_t.pmaw032
DEFINE    l_pmaw033   LIKE pmaw_t.pmaw033
DEFINE    l_pmaw034   LIKE pmaw_t.pmaw034
DEFINE    l_flag      LIKE type_t.chr1

DEFINE    l_cnt       LIKE type_t.num5
DEFINE    r_success   LIKE type_t.chr1
DEFINE    l_price022  LIKE pmaw_t.pmaw022
DEFINE    l_time      DATETIME YEAR TO SECOND

   IF cl_null(p_wc) THEN LET p_wc = "1=1" END IF
   LET l_sql = " SELECT imaa001 FROM imaa_t,imaf_t",
               "  WHERE imaaent = imafent ",
               "    AND imaa001 = imaf001" , 
               "    AND imaaent = '",g_enterprise,"'",               
               "    AND imafsite = 'ALL'",
               "    AND imaa001 = imaf001",
               "    AND imaastus = 'Y'" ,
               "    AND ", p_wc
               
   IF cl_null(g_s_imaa001) AND NOT cl_null(g_e_imaa001) THEN 
      LET l_sql = l_sql, " AND imaa001 = '",g_e_imaa001,"'"
   END IF 
   IF cl_null(g_e_imaa001) AND NOT cl_null(g_s_imaa001) THEN 
      LET l_sql = l_sql, " AND imaa001 = '",g_s_imaa001,"'"
   END IF
   IF NOT cl_null(g_s_imaa001) AND NOT cl_null(g_e_imaa001) THEN 
      LET l_sql = l_sql, "    AND imaa001 BETWEEN '",g_s_imaa001,"'", " AND '",g_e_imaa001,"'"
   END IF               
   PREPARE apmi129_s01_pre FROM l_sql
   DECLARE apmi129_s01_cur CURSOR FOR apmi129_s01_pre
   LET r_success = 'Y'
   CALL s_transaction_begin()
   FOREACH apmi129_s01_cur INTO l_imaa001
      SELECT COUNT(pmaw001) INTO l_cnt FROM pmaw_t 
       WHERE pmawent = g_enterprise
         AND pmaw011 = l_imaa001
         AND pmaw001 = g_pmaw_s.pmaw001
         AND pmaw002 = g_pmaw_s.pmaw002
         
      IF cl_null(l_cnt) THEN LET l_cnt = 0 END IF
      IF l_cnt > 0  AND g_pmaw_s.chk1 = 'Y' THEN
         CONTINUE FOREACH
      END IF  
      IF g_pmaw_s.pmaw019_1 = '1' OR g_pmaw_s.pmaw019_1 = '2' THEN
         SELECT imai021 INTO g_pmaw_s.pmaw019 FROM imai_t
          WHERE imaient = g_enterprise
            AND imaisite = g_site
            AND imai001 = l_imaa001 
            AND imai035 IN (SELECT MAX(imai035) FROM imai_t WHERE imaient = g_enterprise AND imai001 = l_imaa001) 
         IF SQLCA.sqlcode = 100 OR cl_null(g_pmaw_s.pmaw019) THEN
            IF g_pmaw_s.chk2 = 'Y' THEN
               CONTINUE FOREACH
            ELSE
               LET l_flag = 'Y'
               LET g_pmaw_s.pmaw019 = 0       
            END IF 
         ELSE 
            LET l_flag = 'Y'            
         END IF
      END IF 
      IF g_pmaw_s.pmaw019_1 = '3' THEN
         SELECT imai021 INTO g_pmaw_s.pmaw019 FROM imai_t
          WHERE imaient = g_enterprise
            AND imai001 = l_imaa001 
            AND imai035 IN (SELECT MAX(imai035) FROM imai_t WHERE imaient = g_enterprise AND imai001 = l_imaa001) 
         IF SQLCA.sqlcode = 100 OR cl_null(g_pmaw_s.pmaw019) THEN
            IF g_pmaw_s.chk2 = 'Y' THEN
               CONTINUE FOREACH
            ELSE
               LET l_flag = 'Y'
               LET g_pmaw_s.pmaw019 = 0       
            END IF 
         ELSE 
            LET l_flag = 'Y'  
         END IF
      END IF 
      CALL apmi129_get_price(g_pmaw_s.pmaw019,g_pmaw_s.pmaw019_w,g_pmaw_s.pmaw019_s,g_pmaw_s.pmaw019_r)
           RETURNING g_pmaw_s.pmaw019      
      IF NOT cl_null(g_pmaw_s.pmaw020_mon) THEN 
         LET g_pmaw_s.pmaw020 = g_pmaw_s.pmaw019 + g_pmaw_s.pmaw020_mon    
      END IF 
      IF NOT cl_null(g_pmaw_s.pmaw020_rate) THEN 
         LET g_pmaw_s.pmaw020 = g_pmaw_s.pmaw019*(1+g_pmaw_s.pmaw020_rate/100)
      END IF
      IF cl_null(g_pmaw_s.pmaw020) THEN 
         LET g_pmaw_s.pmaw020 = 0 
      END IF         

     CALL apmi129_get_price(g_pmaw_s.pmaw020,g_pmaw_s.pmaw020_w,g_pmaw_s.pmaw020_s,g_pmaw_s.pmaw020_r)
           RETURNING g_pmaw_s.pmaw020       
 
      IF l_cnt > 0  AND g_pmaw_s.chk1 = 'N' THEN  
         #UPDATE pmaw_t SET pmaw017 = g_pmaw_s.pmaw020_mon,
         #                  pmaw018 = g_pmaw_s.pmaw020_rate,
         UPDATE pmaw_t SET pmaw019 = g_pmaw_s.pmaw019,
                           pmaw020 = g_pmaw_s.pmaw020,
                           pmaw021 = g_pmaw_s.pmaw021,
                           pmaw022 = g_pmaw_s.pmaw022
          WHERE pmawent = g_enterprise
            AND pmaw011 = l_imaa001
            AND pmaw001 = g_pmaw_s.pmaw001
            AND pmaw002 = g_pmaw_s.pmaw002
            
         IF SQLCA.sqlcode OR SQLCA.sqlerrd[3] = 0 THEN 
            LET r_success = 'N' 
         END IF  
         LET l_sql = "SELECT pmaw011,pmaw012,pmaw013 FROM pmaw_t " , 
                     "  WHERE pmawent = '",g_enterprise,"'",
                     "    AND pmaw011 = '",l_imaa001,"'",
                     "    AND pmaw001 = '",g_pmaw_s.pmaw001,"'",
                     "    AND pmaw002 = '",g_pmaw_s.pmaw002,"'"
         PREPARE apmi129_s01_pre01 FROM l_sql
         DECLARE apmi129_s01_cur01 CURSOR FOR apmi129_s01_pre01
         FOREACH apmi129_s01_cur01 INTO l_pmaw011,l_pmaw012,l_pmaw031,l_pmaw032,l_pmaw013
            CALL apmi129_pmaw_upd(l_pmaw011,l_pmaw012,l_pmaw031,l_pmaw032,l_pmaw013,g_pmaw_s.pmaw019,g_pmaw_s.pmaw020)
         END FOREACH   
      END IF  

      IF l_cnt = 0 THEN 
         LET l_pmaw.pmaw001 = g_pmaw_s.pmaw001
         LET l_pmaw.pmaw002 = g_pmaw_s.pmaw002
         LET l_pmaw.pmaw011 = l_imaa001
         LET l_pmaw.pmaw012 = ' '
         LET l_pmaw.pmaw014 = 'N'
         LET l_pmaw.pmaw015 = l_imaa001
         LET l_pmaw.pmaw016 = ' '
         #LET l_pmaw.pmaw017 = g_pmaw_s.pmaw020_mon
         #LET l_pmaw.pmaw018 = g_pmaw_s.pmaw020_rate
         LET l_pmaw.pmaw017 = ''
         LET l_pmaw.pmaw018 = ''
         LET l_pmaw.pmaw019 = g_pmaw_s.pmaw019  
         LET l_pmaw.pmaw020 = g_pmaw_s.pmaw020
         LET l_pmaw.pmaw021 = g_pmaw_s.pmaw021
         LET l_pmaw.pmaw022 = g_pmaw_s.pmaw022
         LET l_pmaw.pmaw031 = ' '
         LET l_pmaw.pmaw032 = ' '
         LET l_pmaw.pmaw033 = ' '
         LET l_pmaw.pmaw034 = ' '
         LET l_pmaw.pmawstus = 'Y' 
       
         SELECT imaf113 INTO l_pmaw.pmaw013 FROM imaf_t
          WHERE imafent = g_enterprise
            AND imafsite = 'ALL'
            AND imaf001 = l_imaa001
         IF l_pmaw.pmaw013 IS NULL THEN
            LET l_pmaw.pmaw013 = ' '
         END IF         
         LET l_pmaw.pmawownid = g_user
         LET l_pmaw.pmawowndp = g_dept
         LET l_pmaw.pmawcrtid = g_user  
         LET l_pmaw.pmawcrtdp = g_dept
         LET l_time = cl_get_current()
         LET l_pmaw.pmawmodid = "" 
         LET l_pmaw.pmawmoddt = "" 
         
         INSERT INTO pmaw_t
                           (pmawent,
                            pmaw001,pmaw002,
                            pmaw011,pmaw012,pmaw013,pmaw031,pmaw032,
                            pmawstus,pmaw014,pmaw015,pmaw016,pmaw033,pmaw034,
                            pmaw017,pmaw018,pmaw019,pmaw020,pmaw021,pmaw022,pmaw100, 
                            pmawownid,pmawowndp,pmawcrtid,pmawcrtdp,pmawcrtdt,pmawmodid,pmawmoddt) 
                     VALUES(g_enterprise,
