            LET l_chk = TRUE
         END IF
      END IF
      IF NOT cl_null(g_pmaw_d[l_ac].pmaw032) THEN
         SELECT pmaw013,pmaw034 INTO l_pmaw013,l_pmaw034 FROM pmaw_t
          WHERE pmawent = g_enterprise
            AND pmaw001 = g_pmaw_m.pmaw001
            AND pmaw002 = g_pmaw_m.pmaw002
            AND pmaw011 = p_pmaw015
            AND pmaw012 = p_pmaw016
            AND pmaw013 = g_pmaw_d[l_ac].pmaw013
            AND pmaw014 = 'Y'
          IF l_pmaw013 = g_pmaw_d[l_ac].pmaw013 AND l_pmaw034 = g_pmaw_d[l_ac].pmaw032 THEN
