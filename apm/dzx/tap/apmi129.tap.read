            AND pmaw011 = p_pmaw015
            AND pmaw012 = p_pmaw016
            AND pmaw013 = g_pmaw_d[l_ac].pmaw013
            AND pmaw014 = 'Y'
         IF l_pmaw013 = g_pmaw_d[l_ac].pmaw013 AND l_pmaw015 = g_pmaw_d[l_ac].pmaw011 AND l_pmaw016 = g_pmaw_d[l_ac].pmaw012 THEN
            LET l_chk = TRUE
