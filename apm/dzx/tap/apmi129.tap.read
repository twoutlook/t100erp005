            LET l_chk = FALSE #避免輸入料件編號出現二次開窗
            IF NOT cl_null(g_pmaw_d[l_ac].pmaw011) THEN
		         INITIALIZE g_qryparam.* TO NULL
               LET g_qryparam.state = 'i'
		         LET g_qryparam.reqry = FALSE
               LET g_qryparam.default1 = g_pmaw_d[l_ac].pmaw013             #給予default值
               LET g_qryparam.arg1 = g_pmaw_d[l_ac].pmaw011
               CALL q_imao002()                                             #呼叫開窗
               LET g_pmaw_d[l_ac].pmaw013 = g_qryparam.return1              #將開窗取得的值回傳到變數
               LET l_chk = TRUE
            END IF
            
            #若系列或產品分類有值，抓取aimm200中的單位開窗
            IF (NOT cl_null(g_pmaw_d[l_ac].pmaw031)) OR cl_null(g_pmaw_d[l_ac].pmaw032) = FALSE THEN
               #先判斷是哪個欄位有值，給予sql查詢條件
                  #---用系列去抓料件編號
               IF NOT cl_null(g_pmaw_d[l_ac].pmaw031)THEN
                  LET l_sql = "SELECT imaa001 FROM imaa_t ",
                              "WHERE imaaent = '"||g_enterprise||"' AND imaa127 = '"||g_pmaw_d[l_ac].pmaw031||"'",
                              "ORDER BY imaa001" 
               END IF
                  #---用產品分類抓料件編號
               IF NOT cl_null(g_pmaw_d[l_ac].pmaw032)THEN
                  LET l_sql = "SELECT imaa001 FROM imaa_t ",
                              "WHERE imaaent = '"||g_enterprise||"' AND imaa009 = '"||g_pmaw_d[l_ac].pmaw032||"'",
                              "ORDER BY imaa001" 
               END IF
                              
               PREPARE apmi129_get_imaa001_open_pre FROM l_sql
               DECLARE apmi129_get_imaa001_open_curs CURSOR FOR apmi129_get_imaa001_open_pre
               #先清空值，避免殘留舊值
               LET l_imaa001 = ''
               LET l_str = ''
               LET l_str_cnt = 1
               #一筆一筆比對是否有符合任一 料件編號中的可使用單位
               FOREACH apmi129_get_imaa001_open_curs INTO l_imaa001
                  #抓取到第一筆資料時，開始組字串
                  IF l_str_cnt = 1 THEN
                     LET l_str = "'",l_imaa001,"'"
                  #抓取道第二筆資料時(含第二筆)，接著第一筆或先前的字串繼續串字串
                  ELSE
                     LET l_str = l_str||",'"||l_imaa001||"'"
                  END IF
                  LET l_str_cnt = l_str_cnt + 1 #筆數增加，避免重複判斷是第一筆資料
               END FOREACH
               FREE apmi129_get_imaa001_open_pre#已使用完畢，釋放
               
               INITIALIZE g_qryparam.* TO NULL
               LET g_qryparam.state = 'i'
               LET g_qryparam.reqry = FALSE
               #依是否抓到料號，來決定g_qryparam.where值
               IF cl_null(l_str) THEN   #沒抓到對應的料號
                  LET g_qryparam.where = "imao001 = ''"
               ELSE
                  LET g_qryparam.where = "imao001 IN ("||l_str||")"#增加q_imao002_01的條件判斷
               END IF
               CALL q_imao002_01()
               LET g_pmaw_d[l_ac].pmaw013 = g_qryparam.return1
            #若料號、系列、產品分類皆沒值，則開窗出全部的單位值
            ELSE
               IF l_chk = FALSE THEN
                  INITIALIZE g_qryparam.* TO NULL
                  LET g_qryparam.state = 'i'
                  LET g_qryparam.reqry = FALSE
                  LET g_qryparam.default1 = g_pmaw_d[l_ac].pmaw013   #給予default值
                  #給予arg
                  CALL q_ooca001_1()                          #呼叫開窗
                  LET g_pmaw_d[l_ac].pmaw013 = g_qryparam.return1
               END IF
            END IF
            #dorislai-20150614-modify----(E)

            DISPLAY BY NAME g_pmaw_d[l_ac].pmaw013      #顯示到畫面上
            CALL apmi129_pmaw013_desc()
