  <section id="apmi129.insert" ver="13" status="" src="s" readonly="">
    <![CDATA[#+ 資料新增
PRIVATE FUNCTION apmi129_insert()
   #add-point:insert段define
   {<point name="insert.define" edit="s"/>}
   #end add-point
   #add-point:insert段define
   {<point name="insert.define_customerization" edit="c"/>}
   #end add-point   
   
   #add-point:insert段before
   {<point name="insert.before"/>}
   #end add-point    
   
   #清除相關資料
   CLEAR FORM                    
   CALL g_pmaw_d.clear()
   CALL g_pmaw2_d.clear()
 
 
   INITIALIZE g_pmaw_m.* LIKE pmaw_t.*             #DEFAULT 設定
   LET g_pmaw001_t = NULL
   LET g_pmaw002_t = NULL
 
   LET g_master_insert = FALSE
   CALL s_transaction_begin()
   WHILE TRUE
     
      #單頭預設值
      
     
      #add-point:單頭預設值
      {<point name="insert.default"/>}
      #end add-point 
 
      CALL apmi129_input("a")
      
      #add-point:單頭輸入後
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG AND NOT g_master_insert THEN
         LET INT_FLAG = 0
         LET g_pmaw_m.* = g_pmaw_m_t.*
         CALL apmi129_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
         DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
         INITIALIZE g_pmaw_m.* TO NULL
         INITIALIZE g_pmaw_d TO NULL
         INITIALIZE g_pmaw2_d TO NULL
 
         CALL apmi129_show()
         RETURN
      END IF
    
      #CALL g_pmaw_d.clear()
      #CALL g_pmaw2_d.clear()
 
      
      #add-point:單頭輸入後2
      {<point name="insert.after_insert2"/>}
      #end add-point
 
      LET g_rec_b = 0
      EXIT WHILE
      
   END WHILE
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("modify,modify_detail,delete,reproduce", TRUE)
   CALL apmi129_set_act_visible()
   CALL apmi129_set_act_no_visible()
 
   #將新增的資料併入搜尋條件中
   LET g_state = "insert"
   
   LET g_pmaw001_t = g_pmaw_m.pmaw001
   LET g_pmaw002_t = g_pmaw_m.pmaw002
 
   
   #組合新增資料的條件
   LET g_add_browse = " pmawent = '" ||g_enterprise|| "' AND",
                      " pmaw001 = '", g_pmaw_m.pmaw001, "' "
                      ," AND pmaw002 = '", g_pmaw_m.pmaw002, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL apmi129_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   CALL apmi129_idx_chk()
   
   #撈取異動後的資料(主要是帶出reference)
   EXECUTE apmi129_master_referesh USING g_pmaw_m.pmaw001,g_pmaw_m.pmaw002 INTO g_pmaw_m.pmaw001,g_pmaw_m.pmaw002, 
       g_pmaw_m.pmaw001_desc,g_pmaw_m.pmaw002_desc
   
   #遮罩相關處理
   LET g_pmaw_m_mask_o.* =  g_pmaw_m.*
   CALL apmi129_pmaw_t_mask()
   LET g_pmaw_m_mask_n.* =  g_pmaw_m.*
   
   #將資料顯示到畫面上
   DISPLAY BY NAME g_pmaw_m.pmaw001,g_pmaw_m.pmaw001_desc,g_pmaw_m.pmaw002,g_pmaw_m.pmaw002_desc
   
   #功能已完成,通報訊息中心
   CALL apmi129_msgcentre_notify('insert')
   
END FUNCTION
