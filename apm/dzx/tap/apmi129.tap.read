  <point name="function.apmi129_pmaw015_chk" order="6" ver="6" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 參考料號欄位檢查
# Date & Author..: 2014/02/21 By chenjing
# Modify.........:
################################################################################
PRIVATE FUNCTION apmi129_pmaw015_chk()
   DEFINE l_pmaw015     LIKE pmaw_t.pmaw015
   DEFINE l_pmawstus    LIKE pmaw_t.pmawstus
   DEFINE l_errno       LIKE gzze_t.gzze001
   DEFINE l_sql         STRING
   IF g_pmaw_d[l_ac].pmaw014 = 'Y' THEN 
                    
   
      LET l_sql = " SELECT pmaw001,pmawstus FROM pmaw_t ",              
                  "  WHERE pmawent = '",g_enterprise,"'" ,
                  "    AND pmaw001 = '",g_pmaw_m.pmaw001,"'",
                  "    AND pmaw002 = '",g_pmaw_m.pmaw002,"'",
                  "    AND (pmaw011 <> '",g_pmaw_d[l_ac].pmaw011 ,"' OR pmaw012 <> '",g_pmaw_d[l_ac].pmaw012,"')"
#      LET l_sql = " SELECT pmaw015,pmawstus FROM pmaw_t ",              
#                  "  WHERE pmawent = '",g_enterprise,"'" ,
#                  "    AND pmaw001 = '",g_pmaw_m.pmaw001,"'",
#                  "    AND pmaw002 = '",g_pmaw_m.pmaw002,"'",
#                  "    AND (pmaw011 <> '",g_pmaw_d[l_ac].pmaw011 ,"' OR pmaw012 <> '",g_pmaw_d[l_ac].pmaw012,"')"
      IF NOT cl_null(g_pmaw_d[l_ac].pmaw015) THEN
         LET l_sql = l_sql," AND pmaw011 = '",g_pmaw_d[l_ac].pmaw015,"'" 
      END IF   
      IF g_pmaw_d[l_ac].pmaw016 IS NOT NULL THEN 
         LET l_sql = l_sql," AND pmaw012 = '",g_pmaw_d[l_ac].pmaw016,"'"
      END IF  
      IF NOT cl_null(g_pmaw_d[l_ac].pmaw013) THEN 
         LET l_sql = l_sql," AND pmaw013 = '",g_pmaw_d[l_ac].pmaw013,"'"
      END IF    
      PREPARE apmi129_pmaw001 FROM l_sql
      EXECUTE apmi129_pmaw001 INTO l_pmaw015,l_pmawstus
      LET l_errno = ''
      IF cl_null(l_pmaw015) THEN         
         LET l_errno = 'apm-00319'
      END IF
      IF l_pmawstus != 'Y' THEN
         LET l_errno = 'apm-00320'
      END IF
   END IF
   RETURN l_errno
END FUNCTION]]>
  </point>
  <point name="function.apmi129_pmaw_price" order="7" ver="6" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 帶出標準定價和一般採購價
# Date & Author..: 2014/02/21 By chenjing
# Modify.........:
################################################################################
PRIVATE FUNCTION apmi129_pmaw_price()
   DEFINE    l_pmaw019     LIKE pmaw_t.pmaw019
   DEFINE    l_pmaw020     LIKE pmaw_t.pmaw020
   #dorislai-20150614-modify----(S)
#   IF g_pmaw_d[l_ac].pmaw014 = 'Y' AND NOT cl_null(g_pmaw_d[l_ac].pmaw013) AND NOT cl_null(g_pmaw_d[l_ac].pmaw015) AND g_pmaw_d[l_ac].pmaw016 IS NOT NULL THEN
#      SELECT pmaw019,pmaw020 INTO l_pmaw019,l_pmaw020
#        FROM pmaw_t       
#       WHERE pmawent = g_enterprise
#         AND pmaw001 = g_pmaw_m.pmaw001
#         AND pmaw002 = g_pmaw_m.pmaw002
#         AND pmaw011 = g_pmaw_d[l_ac].pmaw015
#         AND pmaw012 = g_pmaw_d[l_ac].pmaw016
#         AND pmaw013 = g_pmaw_d[l_ac].pmaw013
   IF g_pmaw_d[l_ac].pmaw014 = 'Y' AND NOT cl_null(g_pmaw_d[l_ac].pmaw013)  THEN
      IF cl_null(g_pmaw_d[l_ac].pmaw015) AND cl_null(g_pmaw_d[l_ac].pmaw033) AND cl_null(g_pmaw_d[l_ac].pmaw034) THEN
         RETURN
      END IF
      
      IF NOT cl_null(g_pmaw_d[l_ac].pmaw015) AND g_pmaw_d[l_ac].pmaw016 IS NOT NULL THEN
         SELECT pmaw019,pmaw020 INTO l_pmaw019,l_pmaw020
           FROM pmaw_t       
          WHERE pmawent = g_enterprise
            AND pmaw001 = g_pmaw_m.pmaw001
            AND pmaw002 = g_pmaw_m.pmaw002
            AND pmaw011 = g_pmaw_d[l_ac].pmaw015
            AND pmaw012 = g_pmaw_d[l_ac].pmaw016
            AND pmaw013 = g_pmaw_d[l_ac].pmaw013
       END IF
       
       IF NOT cl_null(g_pmaw_d[l_ac].pmaw033) THEN
          SELECT pmaw019,pmaw020 INTO l_pmaw019,l_pmaw020
            FROM pmaw_t       
           WHERE pmawent = g_enterprise
             AND pmaw001 = g_pmaw_m.pmaw001
             AND pmaw002 = g_pmaw_m.pmaw002
             AND pmaw031 = g_pmaw_d[l_ac].pmaw033
             AND pmaw013 = g_pmaw_d[l_ac].pmaw013
       END IF
       IF NOT cl_null(g_pmaw_d[l_ac].pmaw034) THEN
          SELECT pmaw019,pmaw020 INTO l_pmaw019,l_pmaw020
            FROM pmaw_t       
           WHERE pmawent = g_enterprise
             AND pmaw001 = g_pmaw_m.pmaw001
             AND pmaw002 = g_pmaw_m.pmaw002
             AND pmaw032 = g_pmaw_d[l_ac].pmaw034
             AND pmaw013 = g_pmaw_d[l_ac].pmaw013
             
       END IF
       #dorislai-20150614-add----(E)
       IF NOT cl_null(g_pmaw_d[l_ac].pmaw017) THEN
          LET g_pmaw_d[l_ac].pmaw019 = l_pmaw019 + g_pmaw_d[l_ac].pmaw017 
          LET g_pmaw_d[l_ac].pmaw020 = l_pmaw020 + g_pmaw_d[l_ac].pmaw017 
       END IF
       IF NOT cl_null(g_pmaw_d[l_ac].pmaw018) THEN  
          LET g_pmaw_d[l_ac].pmaw019 = l_pmaw019 * (1+g_pmaw_d[l_ac].pmaw018/100)
          LET g_pmaw_d[l_ac].pmaw020 = l_pmaw020 * (1+g_pmaw_d[l_ac].pmaw018/100)        
       END IF   
       IF cl_null(g_pmaw_d[l_ac].pmaw017) AND cl_null(g_pmaw_d[l_ac].pmaw018) THEN
          LET g_pmaw_d[l_ac].pmaw019 = l_pmaw019  
          LET g_pmaw_d[l_ac].pmaw020 = l_pmaw020   
       END IF
       DISPLAY BY NAME g_pmaw_d[l_ac].pmaw019,g_pmaw_d[l_ac].pmaw020      
   END IF  
END FUNCTION]]>
  </point>
