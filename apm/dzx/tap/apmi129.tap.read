  <section id="apmi129.input.body" ver="25" status="" src="s" readonly="">
    <![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_pmaw_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,WITHOUT DEFAULTS, #MAXCOUNT = g_max_rec,
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂單身ACTION
         
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_pmaw_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL apmi129_b_fill(g_wc2) #test 
            #如果一直都在單頭則控制筆數位置
            IF g_loc = 'm' THEN
               CALL FGL_SET_ARR_CURR(g_detail_idx)
            END IF
            LET g_loc = 'm' 
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            #add-point:modify段before row
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            LET g_curr_diag = ui.DIALOG.getCurrent()
            CALL apmi129_idx_chk()
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN apmi129_cl USING g_enterprise,g_pmaw_m.pmaw001,g_pmaw_m.pmaw002                          
               IF STATUS THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "OPEN apmi129_cl:" 
                  LET g_errparam.code   =  STATUS 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  CLOSE apmi129_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            IF g_rec_b >= l_ac 
               AND g_pmaw_d[l_ac].pmaw011 IS NOT NULL
               AND g_pmaw_d[l_ac].pmaw012 IS NOT NULL
               AND g_pmaw_d[l_ac].pmaw013 IS NOT NULL
               AND g_pmaw_d[l_ac].pmaw031 IS NOT NULL
               AND g_pmaw_d[l_ac].pmaw032 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_pmaw_d_t.* = g_pmaw_d[l_ac].*  #BACKUP
               LET g_pmaw_d_o.* = g_pmaw_d[l_ac].*  #BACKUP
               CALL apmi129_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL apmi129_set_no_entry_b(l_cmd)
               OPEN apmi129_bcl USING g_enterprise,g_pmaw_m.pmaw001,
                                                g_pmaw_m.pmaw002,
 
                                                g_pmaw_d_t.pmaw011
                                                ,g_pmaw_d_t.pmaw012
                                                ,g_pmaw_d_t.pmaw013
                                                ,g_pmaw_d_t.pmaw031
                                                ,g_pmaw_d_t.pmaw032
 
               IF STATUS THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "OPEN apmi129_bcl:" 
                  LET g_errparam.code   =  STATUS 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  LET l_lock_sw='Y'
               ELSE
                  FETCH apmi129_bcl INTO g_pmaw_d[l_ac].pmawstus,g_pmaw_d[l_ac].pmaw011,g_pmaw_d[l_ac].pmaw012, 
                      g_pmaw_d[l_ac].pmaw031,g_pmaw_d[l_ac].pmaw032,g_pmaw_d[l_ac].pmaw013,g_pmaw_d[l_ac].pmaw014, 
                      g_pmaw_d[l_ac].pmaw015,g_pmaw_d[l_ac].pmaw016,g_pmaw_d[l_ac].pmaw033,g_pmaw_d[l_ac].pmaw034, 
                      g_pmaw_d[l_ac].pmaw017,g_pmaw_d[l_ac].pmaw018,g_pmaw_d[l_ac].pmaw019,g_pmaw_d[l_ac].pmaw020, 
                      g_pmaw_d[l_ac].pmaw021,g_pmaw_d[l_ac].pmaw022,g_pmaw_d[l_ac].pmaw100,g_pmaw2_d[l_ac].pmaw011, 
                      g_pmaw2_d[l_ac].pmaw012,g_pmaw2_d[l_ac].pmaw013,g_pmaw2_d[l_ac].pmawownid,g_pmaw2_d[l_ac].pmawowndp, 
                      g_pmaw2_d[l_ac].pmawcrtid,g_pmaw2_d[l_ac].pmawcrtdp,g_pmaw2_d[l_ac].pmawcrtdt, 
                      g_pmaw2_d[l_ac].pmawmodid,g_pmaw2_d[l_ac].pmawmoddt
                  IF SQLCA.sqlcode THEN
                      INITIALIZE g_errparam TO NULL 
                      LET g_errparam.extend = g_pmaw_d_t.pmaw011 
                      LET g_errparam.code   = SQLCA.sqlcode 
                      LET g_errparam.popup  = TRUE 
                      CALL cl_err()
                      LET l_lock_sw = "Y"
                  END IF
                  
                  #遮罩相關處理
                  LET g_pmaw_d_mask_o[l_ac].* =  g_pmaw_d[l_ac].*
                  CALL apmi129_pmaw_t_mask()
                  LET g_pmaw_d_mask_n[l_ac].* =  g_pmaw_d[l_ac].*
                  
                  CALL apmi129_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
        
         BEFORE INSERT
            
            INITIALIZE g_pmaw_d_t.* TO NULL
            INITIALIZE g_pmaw_d_o.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_pmaw_d[l_ac].* TO NULL
            #公用欄位預設值
            #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_pmaw2_d[l_ac].pmawownid = g_user
      LET g_pmaw2_d[l_ac].pmawowndp = g_dept
      LET g_pmaw2_d[l_ac].pmawcrtid = g_user
      LET g_pmaw2_d[l_ac].pmawcrtdp = g_dept 
      LET g_pmaw2_d[l_ac].pmawcrtdt = cl_get_current()
      LET g_pmaw2_d[l_ac].pmawmodid = g_user
      LET g_pmaw2_d[l_ac].pmawmoddt = cl_get_current()
      LET g_pmaw_d[l_ac].pmawstus = ''
 
  
            #一般欄位預設值
                  LET g_pmaw_d[l_ac].pmawstus = "Y"
      LET g_pmaw_d[l_ac].pmaw014 = "N"
      LET g_pmaw_d[l_ac].pmaw022 = "10"
 
            
            #add-point:modify段before備份
            {<point name="input.body.before_insert.before_bak"/>}
            #end add-point
            LET g_pmaw_d_t.* = g_pmaw_d[l_ac].*     #新輸入資料
            LET g_pmaw_d_o.* = g_pmaw_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL apmi129_set_entry_b(l_cmd)
            #add-point:set_entry_b後
            {<point name="input.body.before_insert.set_entry_b"/>}
            #end add-point
            CALL apmi129_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_pmaw_d[li_reproduce_target].* = g_pmaw_d[li_reproduce].*
               LET g_pmaw2_d[li_reproduce_target].* = g_pmaw2_d[li_reproduce].*
 
               LET g_pmaw_d[g_pmaw_d.getLength()].pmaw011 = NULL
               LET g_pmaw_d[g_pmaw_d.getLength()].pmaw012 = NULL
               LET g_pmaw_d[g_pmaw_d.getLength()].pmaw013 = NULL
               LET g_pmaw_d[g_pmaw_d.getLength()].pmaw031 = NULL
               LET g_pmaw_d[g_pmaw_d.getLength()].pmaw032 = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
 
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM pmaw_t 
             WHERE pmawent = g_enterprise AND pmaw001 = g_pmaw_m.pmaw001
               AND pmaw002 = g_pmaw_m.pmaw002
 
               AND pmaw011 = g_pmaw_d[l_ac].pmaw011
               AND pmaw012 = g_pmaw_d[l_ac].pmaw012
               AND pmaw013 = g_pmaw_d[l_ac].pmaw013
               AND pmaw031 = g_pmaw_d[l_ac].pmaw031
               AND pmaw032 = g_pmaw_d[l_ac].pmaw032
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               CALL s_transaction_begin()
               #add-point:單身新增前
               {<point name="input.body.b_insert" mark="Y"/>}
               #end add-point
               INSERT INTO pmaw_t
                           (pmawent,
                            pmaw001,pmaw002,
                            pmaw011,pmaw012,pmaw013,pmaw031,pmaw032
                            ,pmawstus,pmaw014,pmaw015,pmaw016,pmaw033,pmaw034,pmaw017,pmaw018,pmaw019,pmaw020,pmaw021,pmaw022,pmaw100,pmawownid,pmawowndp,pmawcrtid,pmawcrtdp,pmawcrtdt,pmawmodid,pmawmoddt) 
                     VALUES(g_enterprise,
                            g_pmaw_m.pmaw001,g_pmaw_m.pmaw002,
                            g_pmaw_d[l_ac].pmaw011,g_pmaw_d[l_ac].pmaw012,g_pmaw_d[l_ac].pmaw013,g_pmaw_d[l_ac].pmaw031, 
                                g_pmaw_d[l_ac].pmaw032
                            ,g_pmaw_d[l_ac].pmawstus,g_pmaw_d[l_ac].pmaw014,g_pmaw_d[l_ac].pmaw015,g_pmaw_d[l_ac].pmaw016, 
                                g_pmaw_d[l_ac].pmaw033,g_pmaw_d[l_ac].pmaw034,g_pmaw_d[l_ac].pmaw017, 
                                g_pmaw_d[l_ac].pmaw018,g_pmaw_d[l_ac].pmaw019,g_pmaw_d[l_ac].pmaw020, 
                                g_pmaw_d[l_ac].pmaw021,g_pmaw_d[l_ac].pmaw022,g_pmaw_d[l_ac].pmaw100, 
                                g_pmaw2_d[l_ac].pmawownid,g_pmaw2_d[l_ac].pmawowndp,g_pmaw2_d[l_ac].pmawcrtid, 
                                g_pmaw2_d[l_ac].pmawcrtdp,g_pmaw2_d[l_ac].pmawcrtdt,g_pmaw2_d[l_ac].pmawmodid, 
                                g_pmaw2_d[l_ac].pmawmoddt)
               #add-point:單身新增中
               {<point name="input.body.m_insert"/>}
               #end add-point
               LET p_cmd = 'u'
               LET g_master_insert = TRUE
            ELSE    
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = 'INSERT' 
               LET g_errparam.code   = "std-00006" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               INITIALIZE g_pmaw_d[l_ac].* TO NULL
