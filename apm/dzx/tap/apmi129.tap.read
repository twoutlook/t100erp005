# Usage..........: CALL apmi129_pmaw014_chk()
#                  RETURNING TRUE/FALSE
# Input parameter: 
# Return code....: 回傳 TRUE/FALSE
# Date & Author..: 20150629 By dorislai
# Modify.........:
################################################################################
PRIVATE FUNCTION apmi129_pmaw014_chk()
   DEFINE l_cnt  LIKE  type_t.num5
   IF g_pmaw_d[l_ac].pmaw014 = 'Y'  THEN
      LET l_cnt = 1 
      SELECT COUNT(*) INTO l_cnt 
        FROM pmaw_t 
       WHERE pmawent = g_enterprise 
         AND pmaw001 = g_pmaw_m.pmaw001  
         AND pmaw002 = g_pmaw_m.pmaw002
         AND pmawstus = 'Y'
         AND pmaw013 = COALESCE(g_pmaw_d[l_ac].pmaw013,' ')
         AND pmaw014 = 'N'
         AND NOT (     pmaw011 = COALESCE(g_pmaw_d[l_ac].pmaw011,' ')  
                   AND pmaw012 = COALESCE(g_pmaw_d[l_ac].pmaw012,' ')  
                   AND pmaw031 = COALESCE(g_pmaw_d[l_ac].pmaw031,' ')  
                   AND pmaw032 = COALESCE(g_pmaw_d[l_ac].pmaw032,' '))
      IF cl_null(l_cnt) OR l_cnt = 0
      THEN 
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'apm-00964'
         LET g_errparam.extend = ""
         LET g_errparam.replace[1] = g_pmaw_m.pmaw001
         LET g_errparam.replace[2] = g_pmaw_m.pmaw002
         LET g_errparam.popup = TRUE
         CALL cl_err()
         RETURN FALSE
      END IF
      RETURN TRUE
   END IF
END FUNCTION]]>
