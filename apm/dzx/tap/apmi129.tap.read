  <section id="apmi129.detail_reproduce" ver="7" status="" src="s" readonly="">
    <![CDATA[#+ 單身自動複製
PRIVATE FUNCTION apmi129_detail_reproduce()
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE pmaw_t.*
 
 
   #add-point:delete段define
   {<point name="detail_reproduce.define" edit="s"/>}
   #end add-point    
   #add-point:delete段define
   {<point name="detail_reproduce.define_customerization" edit="c"/>}
   #end add-point 
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE apmi129_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE apmi129_detail AS ",
                "SELECT * FROM pmaw_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO apmi129_detail SELECT * FROM pmaw_t 
                                         WHERE pmawent = g_enterprise AND pmaw001 = g_pmaw001_t
                                         AND pmaw002 = g_pmaw002_t
 
   
   #將key修正為調整後   
   UPDATE apmi129_detail 
      #更新key欄位
      SET pmaw001 = g_pmaw_m.pmaw001
          , pmaw002 = g_pmaw_m.pmaw002
 
      #更新共用欄位
      #應用 a13 樣板自動產生(Version:3)
       , pmawownid = g_user 
       , pmawowndp = g_dept
       , pmawcrtid = g_user
       , pmawcrtdp = g_dept 
       , pmawcrtdt = ld_date
       , pmawmodid = g_user
       , pmawmoddt = ld_date
      #, pmawstus = "Y" 
 
 
                                       
   #將資料塞回原table   
   INSERT INTO pmaw_t SELECT * FROM apmi129_detail
   
