          g_pmdb_d[l_ac].pmdb031,g_pmdb_d[l_ac].pmdb050,g_pmdb_d[l_ac].pmdb032,g_pmdb_d[l_ac].pmdb051, 
          g_pmdb_d[l_ac].pmdb033,g_pmdb_d[l_ac].pmdb049,g_pmdb2_d[l_ac].pmdbseq,g_pmdb2_d[l_ac].pmdb012, 
