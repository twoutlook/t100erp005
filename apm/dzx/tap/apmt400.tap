               EXIT DIALOG
 
 
         
         ON ACTION first
            LET g_action_choice = "fetch"
            CALL apmt400_fetch('F')
            LET g_current_row = g_current_idx
            LET g_curr_diag = ui.DIALOG.getCurrent()
            CALL apmt400_idx_chk()
            
         ON ACTION previous
            LET g_action_choice = "fetch"
            CALL apmt400_fetch('P')
            LET g_current_row = g_current_idx
            LET g_curr_diag = ui.DIALOG.getCurrent()
            CALL apmt400_idx_chk()
            
         ON ACTION jump
            LET g_action_choice = "fetch"
            CALL apmt400_fetch('/')
            LET g_current_row = g_current_idx
            LET g_curr_diag = ui.DIALOG.getCurrent()
            CALL apmt400_idx_chk()
            
         ON ACTION next
            LET g_action_choice = "fetch"
            CALL apmt400_fetch('N')
            LET g_current_row = g_current_idx
            LET g_curr_diag = ui.DIALOG.getCurrent()
            CALL apmt400_idx_chk()
            
         ON ACTION last
