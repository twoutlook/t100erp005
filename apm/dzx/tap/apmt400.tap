      #end add-point
      
      #單頭多語言刪除
      
      
      #單身多語言刪除
      
 
 
   
      #add-point:多語言刪除
      {<point name="delete.lang.delete"/>}
      #end add-point
   
   END IF
 
   CALL s_transaction_end('Y','0')
   CLOSE apmt400_cl
 
   #功能已完成,通報訊息中心
   CALL apmt400_msgcentre_notify('delete')
    
