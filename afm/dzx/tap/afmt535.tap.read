  <point name="menu.open_reback" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[               CALL s_transaction_begin()
               #傳票還原
               IF g_fmml_m.fmmldocno IS NULL THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.extend = ""
                  LET g_errparam.code   = "std-00003"
                  LET g_errparam.popup  = FALSE
                  CALL cl_err()
                  CALL s_transaction_end('N','0')
                  CONTINUE DIALOG
               END IF
               
               #無傳票 不可還原
               IF g_fmml_m.fmml007 IS NULL THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.extend = ""
                  LET g_errparam.code   = "anm-00186"
                  LET g_errparam.popup  = TRUE
                  CALL cl_err()
                  CALL s_transaction_end('N','0')
                  CONTINUE DIALOG
               END IF

               IF NOT cl_ask_confirm('aap-00239') THEN
                  CALL s_transaction_end('N','0')
                  EXIT DIALOG
               ELSE
                  #傳票已經確認不可還原
                  SELECT glapstus INTO l_stus
                    FROM glap_t
                   WHERE glapent = g_enterprise
                     AND glapdocno = g_fmml_m.fmml007
                  
                  IF l_stus = 'Y' THEN
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code = 'axr-00076'
                     LET g_errparam.extend = ''
                     LET g_errparam.popup = TRUE
                     CALL s_transaction_end('N','0')
                     CALL cl_err()
                  ELSE
                     #取得傳票日期
                     SELECT glapdocdt INTO l_glapdocdt
                       FROM glap_t
                      WHERE glapent = g_enterprise
                        AND glapld = g_fmml_m.fmml003
                        AND glapdocno = g_fmml_m.fmml007
                     #刪除單頭
                     DELETE FROM glap_t
                      WHERE glapent   = g_enterprise
                        AND glapld = g_fmml_m.fmml003
                        AND glapdocno = g_fmml_m.fmml007
                     #刪除單身
                     DELETE FROM glaq_t
                      WHERE glaqent = g_enterprise
                        AND glaqld = g_fmml_m.fmml003
                        AND glaqdocno = g_fmml_m.fmml007

                     #更新月結這邊的傳票號碼要給空
                     UPDATE fmml_t SET fmml007 = ''
                      WHERE fmmlent = g_enterprise
                        AND fmmldocno = g_fmml_m.fmmldocno
                     #更新最大號
                     SELECT (CASE WHEN glca002 = '2' THEN 'Y' ELSE 'N' END) INTO l_glca002
                       FROM glca_t
                      WHERE glcaent = g_enterprise
                        AND glcald  = g_fmml_m.fmml003 AND glca001 = 'FM'

                     LET g_prog = 'aglt310'
                     IF NOT s_aooi200_fin_del_docno(g_fmml_m.fmml003,g_fmml_m.fmml007,l_glapdocdt) THEN
                        CALL s_transaction_end('N','0')
                        CONTINUE DIALOG
                     END IF
                     LET g_prog = 'afmt535'
                     LET g_fmml_m.fmml007 = ''
                     DISPLAY BY NAME g_fmml_m.fmml007
                     
                     #分錄底稿
                     UPDATE glga_t SET glga007 = ''
                      WHERE glgaent = g_enterprise AND glgald = g_fmml_m.fmml003
                        AND glgadocno=g_fmml_m.fmmldocno AND glga100 = 'FM' AND glga101 = 'M20'

                     CALL s_transaction_end('Y','0')
                  END IF
               END IF]]>
