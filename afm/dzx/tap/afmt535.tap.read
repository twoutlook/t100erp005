  <section id="afmt535.insert" ver="5" status="" src="s" readonly="">
    <![CDATA[#+ 資料新增
PRIVATE FUNCTION afmt535_insert()
   #add-point:insert段define
   {<point name="insert.define" edit="s"/>}
   #end add-point    
   #add-point:insert段define(客製用)
   {<point name="insert.define_customerization" edit="c"/>}
   #end add-point    
   
   #清畫面欄位內容
   CLEAR FORM                    
   CALL g_fmmm_d.clear()   
   CALL g_fmmm2_d.clear()  
   CALL g_fmmm3_d.clear()  
   CALL g_fmmm4_d.clear()  
 
 
   INITIALIZE g_fmml_m.* LIKE fmml_t.*             #DEFAULT 設定
   
   LET g_fmmldocno_t = NULL
 
   
   LET g_master_insert = FALSE
   
   #add-point:insert段before
   {<point name="insert.before"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   WHILE TRUE
      #公用欄位給值(單頭)
      #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_fmml_m.fmmlownid = g_user
      LET g_fmml_m.fmmlowndp = g_dept
      LET g_fmml_m.fmmlcrtid = g_user
      LET g_fmml_m.fmmlcrtdp = g_dept 
      LET g_fmml_m.fmmlcrtdt = cl_get_current()
      LET g_fmml_m.fmmlmodid = g_user
      LET g_fmml_m.fmmlmoddt = cl_get_current()
      LET g_fmml_m.fmmlstus = 'N'
 
 
 
      #append欄位給值
      
     
      #一般欄位給值
      
  
      #add-point:單頭預設值
      {<point name="insert.default"/>}
      #end add-point 
      
      #保存單頭舊值(用於資料輸入錯誤還原預設值時使用)
      LET g_fmml_m_t.* = g_fmml_m.*
      LET g_fmml_m_o.* = g_fmml_m.*
      
      #顯示狀態(stus)圖片
            #應用 a21 樣板自動產生(Version:2)
	  #根據當下狀態碼顯示圖片
      CASE g_fmml_m.fmmlstus 
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
 
 
    
      CALL afmt535_input("a")
      
      #add-point:單頭輸入後
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
      END IF
      
      IF NOT g_master_insert THEN
         DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
         DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
         INITIALIZE g_fmml_m.* TO NULL
         INITIALIZE g_fmmm_d TO NULL
         INITIALIZE g_fmmm2_d TO NULL
         INITIALIZE g_fmmm3_d TO NULL
         INITIALIZE g_fmmm4_d TO NULL
 
         #add-point:取消新增後
         {<point name="insert.cancel"/>}
         #end add-point 
         CALL afmt535_show()
         RETURN
      END IF
      
      LET INT_FLAG = 0
      #CALL g_fmmm_d.clear()
      #CALL g_fmmm2_d.clear()
      #CALL g_fmmm3_d.clear()
      #CALL g_fmmm4_d.clear()
 
 
      LET g_rec_b = 0
      CALL s_transaction_end('Y','0')
      EXIT WHILE
        
   END WHILE
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL afmt535_set_act_visible()   
   CALL afmt535_set_act_no_visible()
   
   #將新增的資料併入搜尋條件中
   LET g_fmmldocno_t = g_fmml_m.fmmldocno
 
   
   #組合新增資料的條件
   LET g_add_browse = " fmmlent = '" ||g_enterprise|| "' AND",
                      " fmmldocno = '", g_fmml_m.fmmldocno, "' "
 
                      
   #add-point:組合新增資料的條件後
   {<point name="insert.after.add_browse"/>}
   #end add-point
      
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL afmt535_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   CLOSE afmt535_cl
   
   CALL afmt535_idx_chk()
   
   #撈取異動後的資料(主要是帶出reference)
   EXECUTE afmt535_master_referesh USING g_fmml_m.fmmldocno INTO g_fmml_m.fmml001,g_fmml_m.fmml003,g_fmml_m.fmml004, 
       g_fmml_m.fmmldocno,g_fmml_m.fmmldocdt,g_fmml_m.fmml005,g_fmml_m.fmml006,g_fmml_m.fmml007,g_fmml_m.fmmlstus, 
