  <point name="menu.open_aapp330" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[               IF g_fmml_m.fmmldocno IS NULL THEN
                 INITIALIZE g_errparam TO NULL 
                 LET g_errparam.extend = "" 
                 LET g_errparam.code   = "std-00003" 
                 LET g_errparam.popup  = TRUE 
                 CALL cl_err()
                 CONTINUE DIALOG
               END IF
               
               #未確認單據不可拋轉
               IF g_fmml_m.fmmlstus = 'N' THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = 'anm-00185'
                  LET g_errparam.extend = ''
                  LET g_errparam.popup = TRUE
                  CALL cl_err()
                  EXIT DIALOG
               END IF
               
               #無傳票號碼才可拋轉
               IF cl_null(g_fmml_m.fmml007) THEN                                   
                  #產生單別/日期
                  CALL axrp330_01(g_fmml_m.fmml003) RETURNING l_slip,l_date                  
                  #若取消產生時，就不往下走拋轉了
                  IF cl_null(l_slip) THEN
                     CONTINUE DIALOG
                  END IF                  
                  
                  CALL s_transaction_begin()

                  CALL cl_err_collect_init()
                  IF s_afmt535_do_voucher_chk(g_fmml_m.fmmldocno,'') THEN
                     IF g_glaa.glaa121 = 'Y' THEN 
                        LET l_wc =" glgbdocno = '",g_fmml_m.fmmldocno,"' "
                        CALL s_pre_voucher_ins_glap('FM','M20',g_fmml_m.fmml003,g_fmml_m.fmmldocdt,l_slip,'1',l_wc) 
                             RETURNING g_sub_success,g_fmml_m.fmml007,g_fmml_m.fmml007
                     ELSE
                        LET l_wc =" fmmldocno = '",g_fmml_m.fmmldocno,"' "
                        CALL s_voucher_gen_fm('8',g_fmml_m.fmml003,l_date,l_slip,'0',l_wc,'N','afmt535')
                             RETURNING g_sub_success,g_fmml_m.fmml007,g_fmml_m.fmml007
                     END IF
                  END IF
                  CALL cl_err_collect_show()

                  IF g_sub_success THEN
                     UPDATE glga_t SET glga007 = g_fmml_m.fmml007
                      WHERE glgaent = g_enterprise AND glgald = g_fmml_m.fmml003
                        AND glgadocno=g_fmml_m.fmmldocno AND glga100 = 'FM' AND glga101 = 'M20'
