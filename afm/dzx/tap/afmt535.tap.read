            CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
            CALL afmt535_set_act_visible()   
            CALL afmt535_set_act_no_visible()
            IF NOT (g_fmml_m.fmmldocno IS NULL
 
              ) THEN
