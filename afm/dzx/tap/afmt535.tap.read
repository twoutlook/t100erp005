  <section id="afmt535.b_fill" ver="4" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION afmt535_b_fill()
   DEFINE p_wc2      STRING
   DEFINE li_idx     LIKE type_t.num10
   #add-point:b_fill段define
   {<point name="b_fill.define" edit="s"/>}
   #end add-point     
   #add-point:b_fill段define(客製用)
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point     
 
   CALL g_fmmm_d.clear()    #g_fmmm_d 單頭及單身 
   CALL g_fmmm2_d.clear()
   CALL g_fmmm3_d.clear()
   CALL g_fmmm4_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF afmt535_fill_chk(1) THEN
      #切換上下筆時不重組SQL
      IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
         LET g_sql = "SELECT  UNIQUE fmmmseq,fmmm002,fmmm003,fmmm004,fmmm005,fmmm007,fmmm008,fmmm009, 
             fmmm010,fmmmseq,fmmm013,fmmm014,fmmmseq,fmmm015,fmmm016,fmmmseq,fmmm006,fmmm043,fmmm020, 
             fmmm021,fmmm022,fmmm023,fmmm024,fmmm025,fmmm026,fmmm027,fmmm028,fmmm029,fmmm030,fmmm031, 
             fmmm032,fmmm033,fmmm034,fmmm035,fmmm036,fmmm037,fmmm038,fmmm039,fmmm040,fmmm041,fmmm042 , 
             t1.ooefl003 ,t2.fmmcl003 FROM fmmm_t",   
                     " INNER JOIN fmml_t ON fmmldocno = fmmm001 ",
 
                     #"",
                     
                     "",
                                    " LEFT JOIN ooefl_t t1 ON t1.ooeflent='"||g_enterprise||"' AND t1.ooefl001=fmmm002 AND t1.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN fmmcl_t t2 ON t2.fmmclent='"||g_enterprise||"' AND t2.fmmcl001=fmmm005 AND t2.fmmcl002='"||g_dlang||"' ",
 
                     " WHERE fmmment=? AND fmmm001=?"
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before
         {<point name="b_fill.body.fill_sql"/>}
         #end add-point
         IF NOT cl_null(g_wc2_table1) THEN
            LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY fmmm_t.fmmmseq"
         
         #add-point:單身填充控制
         {<point name="b_fill.sql"/>}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE afmt535_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR afmt535_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_fmml_m.fmmldocno
                                               
      FOREACH b_fill_cs INTO g_fmmm_d[l_ac].fmmmseq,g_fmmm_d[l_ac].fmmm002,g_fmmm_d[l_ac].fmmm003,g_fmmm_d[l_ac].fmmm004, 
          g_fmmm_d[l_ac].fmmm005,g_fmmm_d[l_ac].fmmm007,g_fmmm_d[l_ac].fmmm008,g_fmmm_d[l_ac].fmmm009, 
          g_fmmm_d[l_ac].fmmm010,g_fmmm2_d[l_ac].fmmmseq,g_fmmm2_d[l_ac].fmmm013,g_fmmm2_d[l_ac].fmmm014, 
          g_fmmm3_d[l_ac].fmmmseq,g_fmmm3_d[l_ac].fmmm015,g_fmmm3_d[l_ac].fmmm016,g_fmmm4_d[l_ac].fmmmseq, 
