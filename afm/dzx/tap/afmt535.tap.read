  <section id="afmt535.modify" ver="3" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION afmt535_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   DEFINE l_wc2_table1          STRING
 
 
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point    
   #add-point:modify段define(客製用)
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point    
   
   #保存單頭舊值
   LET g_fmml_m_t.* = g_fmml_m.*
   LET g_fmml_m_o.* = g_fmml_m.*
   
   IF g_fmml_m.fmmldocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_fmmldocno_t = g_fmml_m.fmmldocno
 
   CALL s_transaction_begin()
   
   OPEN afmt535_cl USING g_enterprise,g_fmml_m.fmmldocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN afmt535_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE afmt535_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE afmt535_master_referesh USING g_fmml_m.fmmldocno INTO g_fmml_m.fmml001,g_fmml_m.fmml003,g_fmml_m.fmml004, 
       g_fmml_m.fmmldocno,g_fmml_m.fmmldocdt,g_fmml_m.fmml005,g_fmml_m.fmml006,g_fmml_m.fmml007,g_fmml_m.fmmlstus, 
       g_fmml_m.fmmlownid,g_fmml_m.fmmlowndp,g_fmml_m.fmmlcrtid,g_fmml_m.fmmlcrtdp,g_fmml_m.fmmlcrtdt, 
       g_fmml_m.fmmlmodid,g_fmml_m.fmmlmoddt,g_fmml_m.fmmlcnfid,g_fmml_m.fmmlcnfdt,g_fmml_m.fmml001_desc, 
       g_fmml_m.fmml003_desc,g_fmml_m.fmml004_desc,g_fmml_m.fmmlownid_desc,g_fmml_m.fmmlowndp_desc,g_fmml_m.fmmlcrtid_desc, 
       g_fmml_m.fmmlcrtdp_desc,g_fmml_m.fmmlmodid_desc,g_fmml_m.fmmlcnfid_desc
   
   #檢查是否允許此動作
   IF NOT afmt535_action_chk() THEN
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_fmml_m_mask_o.* =  g_fmml_m.*
   CALL afmt535_fmml_t_mask()
   LET g_fmml_m_mask_n.* =  g_fmml_m.*
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
 
   
   CALL afmt535_show()
   #add-point:modify段show之後
   {<point name="modify.after_show"/>}
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
 
 
    
   WHILE TRUE
      LET g_fmmldocno_t = g_fmml_m.fmmldocno
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_fmml_m.fmmlmodid = g_user 
LET g_fmml_m.fmmlmoddt = cl_get_current()
LET g_fmml_m.fmmlmodid_desc = cl_get_username(g_fmml_m.fmmlmodid)
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      CALL afmt535_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE fmml_t SET (fmmlmodid,fmmlmoddt) = (g_fmml_m.fmmlmodid,g_fmml_m.fmmlmoddt)
          WHERE fmmlent = g_enterprise AND fmmldocno = g_fmmldocno_t
 
      END IF
    
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_fmml_m.* = g_fmml_m_t.*
         CALL afmt535_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_fmml_m.fmmldocno != g_fmmldocno_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE fmmm_t SET fmmm001 = g_fmml_m.fmmldocno
 
          WHERE fmmment = g_enterprise AND fmmm001 = g_fmmldocno_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "fmmm_t" 
               LET g_errparam.code   = "std-00009" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "fmmm_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
         
         
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL afmt535_set_act_visible()   
   CALL afmt535_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " fmmlent = '" ||g_enterprise|| "' AND",
                      " fmmldocno = '", g_fmml_m.fmmldocno, "' "
 
   #填到對應位置
