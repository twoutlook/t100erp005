                      g_fmmm_d[l_ac].fmmm009,g_fmmm_d[l_ac].fmmm010,g_fmmm2_d[l_ac].fmmmseq,g_fmmm2_d[l_ac].fmmm013, 
                      g_fmmm2_d[l_ac].fmmm014,g_fmmm3_d[l_ac].fmmmseq,g_fmmm3_d[l_ac].fmmm015,g_fmmm3_d[l_ac].fmmm016, 
                      g_fmmm4_d[l_ac].fmmmseq,g_fmmm4_d[l_ac].fmmm006,g_fmmm4_d[l_ac].fmmm043,g_fmmm4_d[l_ac].fmmm020, 
                      g_fmmm4_d[l_ac].fmmm021,g_fmmm4_d[l_ac].fmmm022,g_fmmm4_d[l_ac].fmmm023,g_fmmm4_d[l_ac].fmmm024, 
                      g_fmmm4_d[l_ac].fmmm025,g_fmmm4_d[l_ac].fmmm026,g_fmmm4_d[l_ac].fmmm027,g_fmmm4_d[l_ac].fmmm028, 
