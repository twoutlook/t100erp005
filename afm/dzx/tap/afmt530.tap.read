  <section id="afmt530.insert" ver="6" status="" src="s" readonly="">
    <![CDATA[#+ 資料新增
PRIVATE FUNCTION afmt530_insert()
   #add-point:insert段define
   {<point name="insert.define" edit="s"/>}
   #end add-point    
   #add-point:insert段define(客製用)
   {<point name="insert.define_customerization" edit="c"/>}
   #end add-point
   
   CLEAR FORM #清畫面欄位內容
   INITIALIZE g_fmmj_m.* LIKE fmmj_t.*             #DEFAULT 設定
   LET g_fmmjdocno_t = NULL
 
   
   #add-point:insert段before
   {<point name="insert.before"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   
   WHILE TRUE
      
      #公用欄位給值
      #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_fmmj_m.fmmjownid = g_user
      LET g_fmmj_m.fmmjowndp = g_dept
      LET g_fmmj_m.fmmjcrtid = g_user
      LET g_fmmj_m.fmmjcrtdp = g_dept 
      LET g_fmmj_m.fmmjcrtdt = cl_get_current()
      LET g_fmmj_m.fmmjmodid = g_user
      LET g_fmmj_m.fmmjmoddt = cl_get_current()
      LET g_fmmj_m.fmmjstus = 'N'
 
 
 
      #append欄位給值
      
     
      #一般欄位給值
            LET g_fmmj_m.fmmj010 = "N"
      LET g_fmmj_m.fmmj005 = "0"
      LET g_fmmj_m.fmmj007 = "0"
      LET g_fmmj_m.fmmj008 = "0"
      LET g_fmmj_m.fmmj015 = "0"
      LET g_fmmj_m.fmmj016 = "0"
      LET g_fmmj_m.fmmj017 = "0"
      LET g_fmmj_m.fmmj019 = "N"
      LET g_fmmj_m.fmmj022 = "360"
      LET g_fmmj_m.fmmj023 = "N"
 
 
      #add-point:單頭預設值
      {<point name="insert.default"/>}
      #end add-point   
     
      #顯示狀態(stus)圖片
            #應用 a21 樣板自動產生(Version:2)
	  #根據當下狀態碼顯示圖片
      CASE g_fmmj_m.fmmjstus 
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
 
 
     
      #資料輸入
      CALL afmt530_input("a")
      
      #add-point:單頭輸入後
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG THEN
         #取消
         LET INT_FLAG = 0
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         DISPLAY g_current_cnt TO FORMONLY.h_count     #總筆數
         DISPLAY g_current_idx TO FORMONLY.h_index     #當下筆數
         INITIALIZE g_fmmj_m.* TO NULL
         CALL afmt530_show()
         RETURN
      END IF
 
      LET g_rec_b = 0
      EXIT WHILE
   END WHILE
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL afmt530_set_act_visible()
   CALL afmt530_set_act_no_visible()
 
   #將新增的資料併入搜尋條件中
   LET g_state = "insert"
   
   LET g_fmmjdocno_t = g_fmmj_m.fmmjdocno
 
   
   #組合新增資料的條件
   LET g_add_browse = " fmmjent = '" ||g_enterprise|| "' AND",
                      " fmmjdocno = '", g_fmmj_m.fmmjdocno, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL afmt530_browser_fill("","")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
 
   #撈取異動後的資料(主要是帶出reference)
