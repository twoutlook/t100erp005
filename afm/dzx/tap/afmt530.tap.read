  <point name="input.a.fmmj001" order="" ver="2" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[            #投資審核單號
            IF NOT cl_null(g_fmmj_m.fmmj001) THEN
               IF p_cmd = 'a' OR (p_cmd = 'u' AND (g_fmmj_m.fmmj001 != g_fmmj_m_t.fmmj001 OR g_fmmj_m_t.fmmj001 IS NULL )) THEN
                  CALL s_afm_fmmgdocno_chk(g_fmmj_m.fmmj001) RETURNING g_sub_success,g_errno
                  IF NOT g_sub_success THEN
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code = g_errno
                     LET g_errparam.extend = ''
                     LET g_errparam.popup = TRUE
                     CALL cl_err()
                     LET g_fmmj_m.fmmj001 = g_fmmj_m_t.fmmj001
                     CALL s_afmt530_fmmgdocno_carry(g_fmmj_m.fmmj001)
                      RETURNING g_fmmj_m.fmmj002,g_fmmj_m.fmmj003,g_fmmj_m.fmmj004,g_fmmj_m.fmmj006
                     LET g_fmmj_m.fmmj002_desc = s_desc_fmme001_desc(g_fmmj_m.fmmj002)
                     LET g_fmmj_m.fmmj003_desc = s_desc_fmma001_desc(g_fmmj_m.fmmj003)
                     DISPLAY BY NAME g_fmmj_m.fmmj002,g_fmmj_m.fmmj002_desc,g_fmmj_m.fmmj003,g_fmmj_m.fmmj003_desc,
                                     g_fmmj_m.fmmj004,g_fmmj_m.fmmj006                
                     NEXT FIELD CURRENT
                  END IF  
                  CALL s_afmt530_fmmgdocno_carry(g_fmmj_m.fmmj001)
                   RETURNING g_fmmj_m.fmmj002,g_fmmj_m.fmmj003,g_fmmj_m.fmmj004,g_fmmj_m.fmmj006
                  LET g_fmmj_m.fmmj002_desc = s_desc_fmme001_desc(g_fmmj_m.fmmj002)
                  LET g_fmmj_m.fmmj003_desc = s_desc_fmma001_desc(g_fmmj_m.fmmj003)
                  DISPLAY BY NAME g_fmmj_m.fmmj002,g_fmmj_m.fmmj002_desc,g_fmmj_m.fmmj003,g_fmmj_m.fmmj003_desc,
                                  g_fmmj_m.fmmj004,g_fmmj_m.fmmj006
                  #取得投資組織所屬法人主帳套之本幣對交易幣之匯率						
						IF NOT cl_null(g_fmmj_m.fmmjsite) AND NOT cl_null(g_fmmj_m.fmmj006) AND NOT cl_null(g_fmmj_m.fmmjdocdt) THEN
                     CALL s_aooi160_get_exrate('2',g_glaa.glaald,g_fmmj_m.fmmjdocdt,g_fmmj_m.fmmj006,g_glaa.glaa001,0,g_glaa.glaa025)
                          RETURNING g_fmmj_m.fmmj009		
                     DISPLAY BY NAME g_fmmj_m.fmmj009                          
                  END IF 
                  CALL afmt530_set_no_required()
                  CALL afmt530_set_required()                  
               END IF
            END IF  ]]>
