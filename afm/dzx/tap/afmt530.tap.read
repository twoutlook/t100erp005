  <point name="function.afmt530_set_info" order="1" ver="2" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 帶出投資組織相關資訊
# Memo...........:
# Usage..........: CALL afmt530_set_info(p_ooef001)
# Modify.........:
################################################################################
PRIVATE FUNCTION afmt530_set_info(p_ooef001)
DEFINE p_ooef001      LIKE ooef_t.ooef001

   #取得投資組織所屬法人與法人之主帳套
   CALL s_fin_orga_get_comp_ld(p_ooef001) RETURNING g_sub_success,g_errno,g_glaa.glaacomp,g_glaa.glaald
   CALL s_ld_sel_glaa(g_glaa.glaald,'glaa001|glaa024|glaa025|glaa026')
        RETURNING g_sub_success,g_glaa.glaa001,g_glaa.glaa024,g_glaa.glaa025,g_glaa.glaa026
END FUNCTION]]>
  </point>
  <point name="function.afmt530_set_required" order="2" ver="2" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION afmt530_set_required()
DEFINE l_fmma002  LIKE fmma_t.fmma002

   IF g_fmmj_m.fmmj011 = '3' THEN
      CALL cl_set_comp_required("fmmj012,fmmj013",TRUE)
   END IF
   SELECT fmma002 INTO l_fmma002
     FROM fmma_t
    WHERE fmmaent = g_enterprise
      AND fmma001 = g_fmmj_m.fmmj003
   #投資類型之計息方式為1:計息,票面利率必輸
   IF l_fmma002 = '1' THEN
      CALL cl_set_comp_required("fmmj018",TRUE)
   END IF

END FUNCTION]]>
  </point>
