  <section id="afmt530.main" ver="5" status="" src="s" readonly="">
    <![CDATA[#應用 a26 樣板自動產生(Version:4)
#+ 作業開始(主程式類型)
MAIN
   #add-point:main段define
   {<point name="main.define" edit="s"/>}
   #end add-point   
   #add-point:main段define(客製用)
   {<point name="main.define_customerization" edit="c"/>}
   #end add-point   
 
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("afm","")
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT fmmjsite,'',fmmjdocno,fmmjdocdt,fmmj001,fmmj002,'',fmmj003,'',fmmj004, 
       fmmj006,fmmj010,fmmjstus,fmmj005,fmmj007,fmmj008,fmmj009,fmmj011,fmmj012,fmmj013,fmmj014,fmmj015, 
       fmmj016,fmmj017,fmmj018,fmmj019,fmmj022,fmmj020,fmmj021,fmmj023,fmmjownid,'',fmmjowndp,'',fmmjcrtid, 
       '',fmmjcrtdp,'',fmmjcrtdt,fmmjmodid,'',fmmjmoddt,fmmjcnfid,'',fmmjcnfdt", 
                      " FROM fmmj_t",
                      " WHERE fmmjent= ? AND fmmjdocno=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE afmt530_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE t0.fmmjsite,t0.fmmjdocno,t0.fmmjdocdt,t0.fmmj001,t0.fmmj002,t0.fmmj003, 
       t0.fmmj004,t0.fmmj006,t0.fmmj010,t0.fmmjstus,t0.fmmj005,t0.fmmj007,t0.fmmj008,t0.fmmj009,t0.fmmj011, 
       t0.fmmj012,t0.fmmj013,t0.fmmj014,t0.fmmj015,t0.fmmj016,t0.fmmj017,t0.fmmj018,t0.fmmj019,t0.fmmj022, 
       t0.fmmj020,t0.fmmj021,t0.fmmj023,t0.fmmjownid,t0.fmmjowndp,t0.fmmjcrtid,t0.fmmjcrtdp,t0.fmmjcrtdt, 
       t0.fmmjmodid,t0.fmmjmoddt,t0.fmmjcnfid,t0.fmmjcnfdt,t1.ooefl003 ,t2.fmmel003 ,t3.fmmal003 ,t4.ooag011 , 
       t5.ooefl003 ,t6.ooag011 ,t7.ooefl003 ,t8.ooag011 ,t9.ooag011",
               " FROM fmmj_t t0",
                              " LEFT JOIN ooefl_t t1 ON t1.ooeflent='"||g_enterprise||"' AND t1.ooefl001=t0.fmmjsite AND t1.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN fmmel_t t2 ON t2.fmmelent='"||g_enterprise||"' AND t2.fmmel001=t0.fmmj002 AND t2.fmmel002='"||g_dlang||"' ",
               " LEFT JOIN fmmal_t t3 ON t3.fmmalent='"||g_enterprise||"' AND t3.fmmal001=t0.fmmj003 AND t3.fmmal002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t4 ON t4.ooagent='"||g_enterprise||"' AND t4.ooag001=t0.fmmjownid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=t0.fmmjowndp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent='"||g_enterprise||"' AND t6.ooag001=t0.fmmjcrtid  ",
               " LEFT JOIN ooefl_t t7 ON t7.ooeflent='"||g_enterprise||"' AND t7.ooefl001=t0.fmmjcrtdp AND t7.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t8 ON t8.ooagent='"||g_enterprise||"' AND t8.ooag001=t0.fmmjmodid  ",
               " LEFT JOIN ooag_t t9 ON t9.ooagent='"||g_enterprise||"' AND t9.ooag001=t0.fmmjcnfid  ",
 
               " WHERE t0.fmmjent = '" ||g_enterprise|| "' AND t0.fmmjdocno = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE afmt530_master_referesh FROM g_sql
 
    
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_afmt530 WITH FORM cl_ap_formpath("afm",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL afmt530_init()   
 
      #進入選單 Menu (="N")
      CALL afmt530_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_afmt530
      
   END IF 
   
   CLOSE afmt530_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
END MAIN
 
 
]]>
