  <point name="menu.open_aapp330" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[               IF g_fmmw_m.fmmwdocno IS NULL THEN
                 INITIALIZE g_errparam TO NULL
                 LET g_errparam.extend = ""
                 LET g_errparam.code   = "std-00003"
                 LET g_errparam.popup  = TRUE
                 CALL cl_err()
                 CONTINUE DIALOG
               END IF

               #未確認單據不可拋轉
               IF g_fmmw_m.fmmwstus = 'N' THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = 'anm-00185'
                  LET g_errparam.extend = ''
                  LET g_errparam.popup = TRUE
                  CALL cl_err()
                  EXIT DIALOG
               END IF
               
                #無傳票號碼才可拋轉
               IF cl_null(g_fmmw_m.fmmw004) THEN
                  #產生單別/日期
                  CALL axrp330_01(g_fmmw_m.fmmw001) RETURNING l_slip,l_date
                  #若取消產生時，就不往下走拋轉了
                  IF cl_null(l_slip) THEN
                     CONTINUE DIALOG
                  END IF
                  CALL s_transaction_begin()

                  CALL cl_err_collect_init()
                  IF g_glaa.glaa121 = 'Y' THEN
                     LET l_wc =" glgbdocno = '",g_fmmw_m.fmmwdocno,"' "
                     CALL s_pre_voucher_ins_glap('FM','M50',g_fmmw_m.fmmw001,g_fmmw_m.fmmwdocdt,l_slip,'1',l_wc)
                          RETURNING g_sub_success,g_fmmw_m.fmmw004,g_fmmw_m.fmmw004
                  ELSE
                     LET l_wc =" fmmwdocno = '",g_fmmw_m.fmmwdocno,"' "
                     CALL s_voucher_gen_fm('6',g_fmmw_m.fmmw001,l_date,l_slip,'0',l_wc,'N','afmt585')
                          RETURNING g_sub_success,g_fmmw_m.fmmw004,g_fmmw_m.fmmw004
                  END IF
                  CALL cl_err_collect_show()

                  IF g_sub_success THEN
                     UPDATE glga_t SET glga007 = g_fmmw_m.fmmw004
                      WHERE glgaent = g_enterprise AND glgald = g_fmmw_m.fmmw001
                        AND glgadocno=g_fmmw_m.fmmwdocno AND glga100 = 'FM' AND glga101 = 'M50'
                     CALL s_transaction_end('Y','0')
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code = 'adz-00217'
                     LET g_errparam.popup = TRUE
                     CALL cl_err()
                  ELSE
                    IF cl_null(g_fmmw_m.fmmw004) THEN
                       INITIALIZE g_errparam TO NULL
                       LET g_errparam.code = SQLCA.sqlcode
                       LET g_errparam.extend = ''
                       LET g_errparam.code   = 'axc-00530'
                       LET g_errparam.popup = TRUE
                       CALL cl_err()
                    ELSE
                       INITIALIZE g_errparam TO NULL
                       LET g_errparam.code = SQLCA.sqlcode
                       LET g_errparam.extend = ''
                       LET g_errparam.code   = 'axr-00120'
                       LET g_errparam.popup = TRUE
                       CALL cl_err()
                    END IF
                    CALL s_transaction_end('N','0')
                  END IF

                  #重新顯示傳票號碼
                  SELECT fmmw004 INTO g_fmmw_m.fmmw004
                    FROM fmmw_t
                   WHERE fmmwent = g_enterprise
                     AND fmmwdocno = g_fmmw_m.fmmwdocno
                  DISPLAY BY NAME g_fmmw_m.fmmw004
               ELSE
                  LET l_glap001 = ''
                  SELECT glap001 INTO l_glap001
                    FROM glap_t
                   WHERE glapent = g_enterprise AND glapld = g_fmmw_m.fmmw001
                     AND glapdocno = g_fmmw_m.fmmw004
                  IF NOT l_glap001 MATCHES '[1235]' THEN
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code = 'sub-00029'
                     LET g_errparam.extend = g_fmmw_m.fmmwdocno
                     LET g_errparam.popup = TRUE
