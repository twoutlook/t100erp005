  <section id="afmt585.update_b" ver="2" status="" src="s" readonly="">
    <![CDATA[#+ 修改單身後其他table連動
PRIVATE FUNCTION afmt585_update_b(ps_table,ps_keys,ps_keys_bak,ps_page)
   DEFINE ps_table         STRING
   DEFINE ps_page          STRING
   DEFINE ps_keys          DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ps_keys_bak      DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ls_group         STRING
   DEFINE li_idx           LIKE type_t.num10 
   DEFINE lb_chk           BOOLEAN
   DEFINE l_new_key        DYNAMIC ARRAY OF STRING
   DEFINE l_old_key        DYNAMIC ARRAY OF STRING
   DEFINE l_field_key      DYNAMIC ARRAY OF STRING
   #add-point:update_b段define
   {<point name="update_b.define" edit="s"/>}
   #end add-point   
   #add-point:update_b段define(客製用)
   {<point name="update_b.define_customerization" edit="c"/>}
   #end add-point   
   
   LET g_update = TRUE   
   
   #判斷key是否有改變
   LET lb_chk = TRUE
   FOR li_idx = 1 TO ps_keys.getLength()
      IF ps_keys[li_idx] <> ps_keys_bak[li_idx] THEN
         LET lb_chk = FALSE
         EXIT FOR
      END IF
   END FOR
   
   #不需要做處理
   IF lb_chk THEN
      RETURN
   END IF
   
   #判斷是否是同一群組的table
   LET ls_group = "'1','2','3','4',"
   IF ls_group.getIndexOf(ps_page,1) > 0 AND ps_table <> "fmmx_t" THEN
      #add-point:update_b段修改前
      {<point name="update_b.before_update" mark="Y"/>}
      #end add-point 
      
      #將遮罩欄位還原
      CALL afmt585_fmmx_t_mask_restore('restore_mask_o')
               
      UPDATE fmmx_t 
         SET (fmmxdocno,
              fmmxseq
              ,fmmx001,fmmx002,fmmx003,fmmx004,fmmx005,fmmx100,fmmx101,fmmx111,fmmx112,fmmx029,fmmx030,fmmx133,fmmx006,fmmx007,fmmx008,fmmx009,fmmx010,fmmx011,fmmx012,fmmx013,fmmx014,fmmx015,fmmx016,fmmx017,fmmx018,fmmx019,fmmx020,fmmx021,fmmx022,fmmx023,fmmx024,fmmx025,fmmx026,fmmx027,fmmx028,fmmx121,fmmx122,fmmx131,fmmx132) 
              = 
             (ps_keys[1],ps_keys[2]
              ,g_fmmx_d[g_detail_idx].fmmx001,g_fmmx_d[g_detail_idx].fmmx002,g_fmmx_d[g_detail_idx].fmmx003, 
                  g_fmmx_d[g_detail_idx].fmmx004,g_fmmx_d[g_detail_idx].fmmx005,g_fmmx_d[g_detail_idx].fmmx100, 
                  g_fmmx_d[g_detail_idx].fmmx101,g_fmmx_d[g_detail_idx].fmmx111,g_fmmx_d[g_detail_idx].fmmx112, 
                  g_fmmx2_d[g_detail_idx].fmmx029,g_fmmx2_d[g_detail_idx].fmmx030,g_fmmx2_d[g_detail_idx].fmmx133, 
                  g_fmmx2_d[g_detail_idx].fmmx006,g_fmmx2_d[g_detail_idx].fmmx007,g_fmmx2_d[g_detail_idx].fmmx008, 
                  g_fmmx2_d[g_detail_idx].fmmx009,g_fmmx2_d[g_detail_idx].fmmx010,g_fmmx2_d[g_detail_idx].fmmx011, 
                  g_fmmx2_d[g_detail_idx].fmmx012,g_fmmx2_d[g_detail_idx].fmmx013,g_fmmx2_d[g_detail_idx].fmmx014, 
                  g_fmmx2_d[g_detail_idx].fmmx015,g_fmmx2_d[g_detail_idx].fmmx016,g_fmmx2_d[g_detail_idx].fmmx017, 
                  g_fmmx2_d[g_detail_idx].fmmx018,g_fmmx2_d[g_detail_idx].fmmx019,g_fmmx2_d[g_detail_idx].fmmx020, 
                  g_fmmx2_d[g_detail_idx].fmmx021,g_fmmx2_d[g_detail_idx].fmmx022,g_fmmx2_d[g_detail_idx].fmmx023, 
                  g_fmmx2_d[g_detail_idx].fmmx024,g_fmmx2_d[g_detail_idx].fmmx025,g_fmmx2_d[g_detail_idx].fmmx026, 
                  g_fmmx2_d[g_detail_idx].fmmx027,g_fmmx2_d[g_detail_idx].fmmx028,g_fmmx3_d[g_detail_idx].fmmx121, 
                  g_fmmx3_d[g_detail_idx].fmmx122,g_fmmx4_d[g_detail_idx].fmmx131,g_fmmx4_d[g_detail_idx].fmmx132)  

         WHERE fmmxent = g_enterprise AND fmmxdocno = ps_keys_bak[1] AND fmmxseq = ps_keys_bak[2]
      #add-point:update_b段修改中
      {<point name="update_b.m_update"/>}
      #end add-point   
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "fmmx_t" 
            LET g_errparam.code   = "std-00009" 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            CALL s_transaction_end('N','0')
         WHEN SQLCA.sqlcode #其他錯誤
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "fmmx_t" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            CALL s_transaction_end('N','0')
         OTHERWISE
            
      END CASE
      
      #將遮罩欄位進行遮蔽
      CALL afmt585_fmmx_t_mask_restore('restore_mask_n')
               
      #add-point:update_b段修改後
      {<point name="update_b.after_update"/>}
      #end add-point  
   END IF
   
 
   
 
   
   #add-point:update_b段other
   {<point name="update_b.other"/>}
   #end add-point  
   
END FUNCTION
]]>
