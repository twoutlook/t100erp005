  <point name="menu.open_reback" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[               CALL s_transaction_begin()
               #傳票還原
               IF g_fmmw_m.fmmwdocno IS NULL THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.extend = ""
                  LET g_errparam.code   = "std-00003"
                  LET g_errparam.popup  = FALSE
                  CALL cl_err()
                  CALL s_transaction_end('N','0')
                  CONTINUE DIALOG
               END IF

               #無傳票 不可還原
               IF g_fmmw_m.fmmw004 IS NULL THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.extend = ""
                  LET g_errparam.code   = "anm-00186"
                  LET g_errparam.popup  = TRUE
                  CALL cl_err()
                  CALL s_transaction_end('N','0')
                  CONTINUE DIALOG
               END IF
               
               #傳票還原單據日期不可小於等於過帳日期             
               CALL s_afm_close_day_chk(g_fmmw_m.fmmw001,g_fmmw_m.fmmw004) RETURNING g_sub_success,g_errno
               IF NOT g_sub_success THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = g_errno
                  LET g_errparam.extend = ''
                  LET g_errparam.popup = TRUE
                  CALL cl_err()
                  CALL s_transaction_end('N','0')
                  CONTINUE DIALOG
               END IF
               
               IF NOT cl_ask_confirm('aap-00239') THEN
                  CALL s_transaction_end('N','0')
                  EXIT DIALOG
               ELSE
                  #傳票已經確認不可還原
                  SELECT glapstus INTO l_stus
                    FROM glap_t
                   WHERE glapent = g_enterprise
                     AND glapdocno = g_fmmw_m.fmmw004

                  IF l_stus = 'Y' THEN
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code = 'axr-00076'
                     LET g_errparam.extend = ''
                     LET g_errparam.popup = TRUE
                     CALL s_transaction_end('N','0')
                     CALL cl_err()
                  ELSE
                   #取得傳票日期
                     SELECT glapdocdt INTO l_glapdocdt
                       FROM glap_t
                      WHERE glapent = g_enterprise
                        AND glapld = g_fmmw_m.fmmw001
                        AND glapdocno = g_fmmw_m.fmmw004
                        
                     #刪除單頭
                     DELETE FROM glap_t
                      WHERE glapent   = g_enterprise
                        AND glapld = g_fmmw_m.fmmw001
                        AND glapdocno = g_fmmw_m.fmmw004
                     #刪除單身
                     DELETE FROM glaq_t
                      WHERE glaqent = g_enterprise
                        AND glaqld = g_fmmw_m.fmmw001
                        AND glaqdocno = g_fmmw_m.fmmw004
                    #更新月結這邊的傳票號碼要給空
                     UPDATE fmmw_t SET fmmw004 = ''
                      WHERE fmmwent = g_enterprise
                        AND fmmwdocno = g_fmmw_m.fmmwdocno              
                     LET g_prog = 'aglt310'          
                     IF NOT s_aooi200_fin_del_docno(g_fmmw_m.fmmw001,g_fmmw_m.fmmw004,l_glapdocdt) THEN
                        CALL s_transaction_end('N','0')
                        CONTINUE DIALOG
                     END IF
                     LET g_prog = 'afmt585'
                     LET g_fmmw_m.fmmw004 = ''
                     DISPLAY BY NAME g_fmmw_m.fmmw004

                     #分錄底稿
                     UPDATE glga_t SET glga007 = ''
                      WHERE glgaent = g_enterprise AND glgald = g_fmmw_m.fmmw001
                        AND glgadocno=g_fmmw_m.fmmwdocno AND glga100 = 'FM' AND glga101 = 'M50'

                     CALL s_transaction_end('Y','0')
                  END IF
               END IF]]>
