  <section id="afmt585.insert" ver="8" status="" src="s" readonly="">
    <![CDATA[#+ 資料新增
PRIVATE FUNCTION afmt585_insert()
   #add-point:insert段define
   {<point name="insert.define" edit="s"/>}
   #end add-point    
   #add-point:insert段define(客製用)
   {<point name="insert.define_customerization" edit="c"/>}
   #end add-point    
   
   #清畫面欄位內容
   CLEAR FORM                    
   CALL g_fmmx_d.clear()   
   CALL g_fmmx2_d.clear()  
   CALL g_fmmx3_d.clear()  
   CALL g_fmmx4_d.clear()  
 
 
   INITIALIZE g_fmmw_m.* LIKE fmmw_t.*             #DEFAULT 設定
   
   LET g_fmmwdocno_t = NULL
 
   
   LET g_master_insert = FALSE
   
   #add-point:insert段before
   {<point name="insert.before"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   WHILE TRUE
      #公用欄位給值(單頭)
      #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_fmmw_m.fmmwownid = g_user
      LET g_fmmw_m.fmmwowndp = g_dept
      LET g_fmmw_m.fmmwcrtid = g_user
      LET g_fmmw_m.fmmwcrtdp = g_dept 
      LET g_fmmw_m.fmmwcrtdt = cl_get_current()
      LET g_fmmw_m.fmmwmodid = g_user
      LET g_fmmw_m.fmmwmoddt = cl_get_current()
      LET g_fmmw_m.fmmwstus = 'N'
 
 
 
      #append欄位給值
      
     
      #一般欄位給值
            LET g_fmmw_m.fmmw002 = "0"
      LET g_fmmw_m.fmmw003 = "0"
 
  
      #add-point:單頭預設值
      {<point name="insert.default"/>}
      #end add-point 
      
      #保存單頭舊值(用於資料輸入錯誤還原預設值時使用)
      LET g_fmmw_m_t.* = g_fmmw_m.*
      LET g_fmmw_m_o.* = g_fmmw_m.*
      
      #顯示狀態(stus)圖片
            #應用 a21 樣板自動產生(Version:2)
	  #根據當下狀態碼顯示圖片
      CASE g_fmmw_m.fmmwstus 
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
 
 
    
      CALL afmt585_input("a")
      
      #add-point:單頭輸入後
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
      END IF
      
      IF NOT g_master_insert THEN
         DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
         DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
         INITIALIZE g_fmmw_m.* TO NULL
         INITIALIZE g_fmmx_d TO NULL
         INITIALIZE g_fmmx2_d TO NULL
         INITIALIZE g_fmmx3_d TO NULL
         INITIALIZE g_fmmx4_d TO NULL
 
         #add-point:取消新增後
         {<point name="insert.cancel"/>}
         #end add-point 
         CALL afmt585_show()
         RETURN
      END IF
      
      LET INT_FLAG = 0
      #CALL g_fmmx_d.clear()
      #CALL g_fmmx2_d.clear()
      #CALL g_fmmx3_d.clear()
      #CALL g_fmmx4_d.clear()
 
 
      LET g_rec_b = 0
      CALL s_transaction_end('Y','0')
      EXIT WHILE
        
   END WHILE
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL afmt585_set_act_visible()   
   CALL afmt585_set_act_no_visible()
   
   #將新增的資料併入搜尋條件中
   LET g_fmmwdocno_t = g_fmmw_m.fmmwdocno
 
   
   #組合新增資料的條件
   LET g_add_browse = " fmmwent = '" ||g_enterprise|| "' AND",
                      " fmmwdocno = '", g_fmmw_m.fmmwdocno, "' "
 
                      
   #add-point:組合新增資料的條件後
   {<point name="insert.after.add_browse"/>}
   #end add-point
      
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL afmt585_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   CLOSE afmt585_cl
   
   CALL afmt585_idx_chk()
   
   #撈取異動後的資料(主要是帶出reference)
   EXECUTE afmt585_master_referesh USING g_fmmw_m.fmmwdocno INTO g_fmmw_m.fmmwsite,g_fmmw_m.fmmw001, 
       g_fmmw_m.fmmwcomp,g_fmmw_m.fmmwdocno,g_fmmw_m.fmmwdocdt,g_fmmw_m.fmmw002,g_fmmw_m.fmmw003,g_fmmw_m.fmmw005, 
       g_fmmw_m.fmmw004,g_fmmw_m.fmmwstus,g_fmmw_m.fmmwownid,g_fmmw_m.fmmwowndp,g_fmmw_m.fmmwcrtid,g_fmmw_m.fmmwcrtdp, 
       g_fmmw_m.fmmwcrtdt,g_fmmw_m.fmmwmodid,g_fmmw_m.fmmwmoddt,g_fmmw_m.fmmwcnfid,g_fmmw_m.fmmwcnfdt, 
       g_fmmw_m.fmmwownid_desc,g_fmmw_m.fmmwowndp_desc,g_fmmw_m.fmmwcrtid_desc,g_fmmw_m.fmmwcrtdp_desc, 
       g_fmmw_m.fmmwmodid_desc,g_fmmw_m.fmmwcnfid_desc
   
   #遮罩相關處理
   LET g_fmmw_m_mask_o.* =  g_fmmw_m.*
   CALL afmt585_fmmw_t_mask()
   LET g_fmmw_m_mask_n.* =  g_fmmw_m.*
   
   #將資料顯示到畫面上
   DISPLAY BY NAME g_fmmw_m.fmmwsite,g_fmmw_m.fmmwsite_desc,g_fmmw_m.fmmw001,g_fmmw_m.fmmw001_desc,g_fmmw_m.fmmwcomp, 
       g_fmmw_m.fmmwcomp_desc,g_fmmw_m.fmmwdocno,g_fmmw_m.fmmwdocdt,g_fmmw_m.fmmw002,g_fmmw_m.fmmw003, 
       g_fmmw_m.fmmw005,g_fmmw_m.fmmw005_desc,g_fmmw_m.fmmw004,g_fmmw_m.fmmwstus,g_fmmw_m.fmmwownid, 
       g_fmmw_m.fmmwownid_desc,g_fmmw_m.fmmwowndp,g_fmmw_m.fmmwowndp_desc,g_fmmw_m.fmmwcrtid,g_fmmw_m.fmmwcrtid_desc, 
       g_fmmw_m.fmmwcrtdp,g_fmmw_m.fmmwcrtdp_desc,g_fmmw_m.fmmwcrtdt,g_fmmw_m.fmmwmodid,g_fmmw_m.fmmwmodid_desc, 
       g_fmmw_m.fmmwmoddt,g_fmmw_m.fmmwcnfid,g_fmmw_m.fmmwcnfid_desc,g_fmmw_m.fmmwcnfdt
   
   #add-point:新增結束後
   {<point name="insert.after"/>}
   #end add-point 
   
   #功能已完成,通報訊息中心
   CALL afmt585_msgcentre_notify('insert')
   
END FUNCTION
]]>
  </section>
