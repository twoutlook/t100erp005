  <section id="afmt585.b_fill" ver="2" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION afmt585_b_fill()
   DEFINE p_wc2      STRING
   DEFINE li_idx     LIKE type_t.num10
   #add-point:b_fill段define
   {<point name="b_fill.define" edit="s"/>}
   #end add-point     
   #add-point:b_fill段define(客製用)
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point     
 
   CALL g_fmmx_d.clear()    #g_fmmx_d 單頭及單身 
   CALL g_fmmx2_d.clear()
   CALL g_fmmx3_d.clear()
   CALL g_fmmx4_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF afmt585_fill_chk(1) THEN
      #切換上下筆時不重組SQL
      IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
         LET g_sql = "SELECT  UNIQUE fmmxseq,fmmx001,fmmx002,fmmx003,fmmx004,fmmx005,fmmx100,fmmx101, 
             fmmx111,fmmx112,fmmxseq,fmmx029,fmmx030,fmmx133,fmmx006,fmmx007,fmmx008,fmmx009,fmmx010, 
             fmmx011,fmmx012,fmmx013,fmmx014,fmmx015,fmmx016,fmmx017,fmmx018,fmmx019,fmmx020,fmmx021, 
             fmmx022,fmmx023,fmmx024,fmmx025,fmmx026,fmmx027,fmmx028,fmmxseq,fmmx121,fmmx122,fmmxseq, 
             fmmx131,fmmx132  FROM fmmx_t",   
                     " INNER JOIN fmmw_t ON fmmwdocno = fmmxdocno ",
 
                     #"",
                     
                     "",
                     
                     " WHERE fmmxent=? AND fmmxdocno=?"
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before
         {<point name="b_fill.body.fill_sql"/>}
         #end add-point
         IF NOT cl_null(g_wc2_table1) THEN
            LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY fmmx_t.fmmxseq"
         
         #add-point:單身填充控制
         {<point name="b_fill.sql"/>}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE afmt585_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR afmt585_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_fmmw_m.fmmwdocno
                                               
      FOREACH b_fill_cs INTO g_fmmx_d[l_ac].fmmxseq,g_fmmx_d[l_ac].fmmx001,g_fmmx_d[l_ac].fmmx002,g_fmmx_d[l_ac].fmmx003, 
          g_fmmx_d[l_ac].fmmx004,g_fmmx_d[l_ac].fmmx005,g_fmmx_d[l_ac].fmmx100,g_fmmx_d[l_ac].fmmx101, 
          g_fmmx_d[l_ac].fmmx111,g_fmmx_d[l_ac].fmmx112,g_fmmx2_d[l_ac].fmmxseq,g_fmmx2_d[l_ac].fmmx029, 
          g_fmmx2_d[l_ac].fmmx030,g_fmmx2_d[l_ac].fmmx133,g_fmmx2_d[l_ac].fmmx006,g_fmmx2_d[l_ac].fmmx007, 
          g_fmmx2_d[l_ac].fmmx008,g_fmmx2_d[l_ac].fmmx009,g_fmmx2_d[l_ac].fmmx010,g_fmmx2_d[l_ac].fmmx011, 
          g_fmmx2_d[l_ac].fmmx012,g_fmmx2_d[l_ac].fmmx013,g_fmmx2_d[l_ac].fmmx014,g_fmmx2_d[l_ac].fmmx015, 
          g_fmmx2_d[l_ac].fmmx016,g_fmmx2_d[l_ac].fmmx017,g_fmmx2_d[l_ac].fmmx018,g_fmmx2_d[l_ac].fmmx019, 
          g_fmmx2_d[l_ac].fmmx020,g_fmmx2_d[l_ac].fmmx021,g_fmmx2_d[l_ac].fmmx022,g_fmmx2_d[l_ac].fmmx023, 
          g_fmmx2_d[l_ac].fmmx024,g_fmmx2_d[l_ac].fmmx025,g_fmmx2_d[l_ac].fmmx026,g_fmmx2_d[l_ac].fmmx027, 
          g_fmmx2_d[l_ac].fmmx028,g_fmmx3_d[l_ac].fmmxseq,g_fmmx3_d[l_ac].fmmx121,g_fmmx3_d[l_ac].fmmx122, 
          g_fmmx4_d[l_ac].fmmxseq,g_fmmx4_d[l_ac].fmmx131,g_fmmx4_d[l_ac].fmmx132
