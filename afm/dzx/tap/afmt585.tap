  <section id="afmt585.modify" ver="4" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION afmt585_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   DEFINE l_wc2_table1          STRING
 
 
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point    
   #add-point:modify段define(客製用)
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point    
   
   #保存單頭舊值
   LET g_fmmw_m_t.* = g_fmmw_m.*
   LET g_fmmw_m_o.* = g_fmmw_m.*
   
   IF g_fmmw_m.fmmwdocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_fmmwdocno_t = g_fmmw_m.fmmwdocno
 
   CALL s_transaction_begin()
   
   OPEN afmt585_cl USING g_enterprise,g_fmmw_m.fmmwdocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN afmt585_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE afmt585_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE afmt585_master_referesh USING g_fmmw_m.fmmwdocno INTO g_fmmw_m.fmmwsite,g_fmmw_m.fmmw001, 
       g_fmmw_m.fmmwcomp,g_fmmw_m.fmmwdocno,g_fmmw_m.fmmwdocdt,g_fmmw_m.fmmw002,g_fmmw_m.fmmw003,g_fmmw_m.fmmw005, 
       g_fmmw_m.fmmw004,g_fmmw_m.fmmwstus,g_fmmw_m.fmmwownid,g_fmmw_m.fmmwowndp,g_fmmw_m.fmmwcrtid,g_fmmw_m.fmmwcrtdp, 
       g_fmmw_m.fmmwcrtdt,g_fmmw_m.fmmwmodid,g_fmmw_m.fmmwmoddt,g_fmmw_m.fmmwcnfid,g_fmmw_m.fmmwcnfdt, 
       g_fmmw_m.fmmwownid_desc,g_fmmw_m.fmmwowndp_desc,g_fmmw_m.fmmwcrtid_desc,g_fmmw_m.fmmwcrtdp_desc, 
       g_fmmw_m.fmmwmodid_desc,g_fmmw_m.fmmwcnfid_desc
   
   #檢查是否允許此動作
   IF NOT afmt585_action_chk() THEN
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_fmmw_m_mask_o.* =  g_fmmw_m.*
   CALL afmt585_fmmw_t_mask()
   LET g_fmmw_m_mask_n.* =  g_fmmw_m.*
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
 
   
   CALL afmt585_show()
   #add-point:modify段show之後
   {<point name="modify.after_show"/>}
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
 
 
    
   WHILE TRUE
      LET g_fmmwdocno_t = g_fmmw_m.fmmwdocno
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_fmmw_m.fmmwmodid = g_user 
LET g_fmmw_m.fmmwmoddt = cl_get_current()
LET g_fmmw_m.fmmwmodid_desc = cl_get_username(g_fmmw_m.fmmwmodid)
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      CALL afmt585_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE fmmw_t SET (fmmwmodid,fmmwmoddt) = (g_fmmw_m.fmmwmodid,g_fmmw_m.fmmwmoddt)
          WHERE fmmwent = g_enterprise AND fmmwdocno = g_fmmwdocno_t
 
      END IF
    
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_fmmw_m.* = g_fmmw_m_t.*
         CALL afmt585_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_fmmw_m.fmmwdocno != g_fmmwdocno_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE fmmx_t SET fmmxdocno = g_fmmw_m.fmmwdocno
 
          WHERE fmmxent = g_enterprise AND fmmxdocno = g_fmmwdocno_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "fmmx_t" 
               LET g_errparam.code   = "std-00009" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "fmmx_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
         
         
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL afmt585_set_act_visible()   
   CALL afmt585_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " fmmwent = '" ||g_enterprise|| "' AND",
                      " fmmwdocno = '", g_fmmw_m.fmmwdocno, "' "
 
   #填到對應位置
   CALL afmt585_browser_fill("")
 
   CLOSE afmt585_cl
   
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL afmt585_msgcentre_notify('modify')
 
END FUNCTION   
]]>
  </section>
