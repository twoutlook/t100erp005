            END IF
 
 
 
         #應用 a43 樣板自動產生(Version:2)
         ON ACTION open_aapt300_14
            LET g_action_choice="open_aapt300_14"
            IF cl_auth_chk_act("open_aapt300_14") THEN
               
               #add-point:ON ACTION open_aapt300_14
               {<point name="menu.open_aapt300_14" />}
               #END add-point
               
            END IF
 
 
 
         #應用 a43 樣板自動產生(Version:2)
         ON ACTION open_afmt585_02
            LET g_action_choice="open_afmt585_02"
            IF cl_auth_chk_act("open_afmt585_02") THEN
               
               #add-point:ON ACTION open_afmt585_02
               {<point name="menu.open_afmt585_02" />}
               #END add-point
               
            END IF
 
 
 
         #應用 a43 樣板自動產生(Version:2)
         ON ACTION delete
            LET g_action_choice="delete"
            IF cl_auth_chk_act("delete") THEN
               CALL afmt585_delete()
               #add-point:ON ACTION delete
               {<point name="menu.delete" />}
               #END add-point
               
            END IF
 
 
 
         #應用 a43 樣板自動產生(Version:2)
         ON ACTION open_reback
            LET g_action_choice="open_reback"
