{<section id="afmt535.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION afmt535_b_fill()
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   {<point name="b_fill.define_customerization" edit="c" mark="Y"/>}
   #end add-point     
   DEFINE p_wc2      STRING
   DEFINE li_idx     LIKE type_t.num10
   #add-point:b_fill段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   {<point name="b_fill.define"/>} 
   #end add-point     
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   {<point name="b_fill.pre_function"/>}
   #end add-point
   
   #清空第一階單身
   CALL g_fmmm_d.clear()
   CALL g_fmmm2_d.clear()
   CALL g_fmmm3_d.clear()
   CALL g_fmmm4_d.clear()
 
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF afmt535_fill_chk(1) THEN
      #切換上下筆時不重組SQL
      IF (g_action_choice = "query" OR cl_null(g_action_choice))
      #add-point:b_fill段long_sql_if name="b_fill.long_sql_if"
      {<point name="b_fill.long_sql_if"/>}
      #end add-point
      THEN
         LET g_sql = "SELECT  DISTINCT fmmmseq,fmmm002,fmmm003,fmmm004,fmmm005,fmmm007,fmmm008,fmmm009, 
             fmmm010,fmmmseq,fmmm013,fmmm014,fmmmseq,fmmm015,fmmm016,fmmmseq,fmmm006,fmmm046,fmmm043, 
             fmmm044,fmmm045,fmmm020,fmmm021,fmmm022,fmmm023,fmmm024,fmmm025,fmmm026,fmmm027,fmmm028, 
             fmmm029,fmmm030,fmmm031,fmmm032,fmmm033,fmmm034,fmmm035,fmmm036,fmmm037,fmmm038,fmmm039, 
             fmmm040,fmmm041,fmmm042 ,t1.ooefl003 ,t2.fmmcl003 FROM fmmm_t",   
                     " INNER JOIN fmml_t ON fmmlent = " ||g_enterprise|| " AND fmmldocno = fmmm001 ",
 
                     #"",
                     
                     "",
                     #下層單身所需的join條件
 
                                    " LEFT JOIN ooefl_t t1 ON t1.ooeflent="||g_enterprise||" AND t1.ooefl001=fmmm002 AND t1.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN fmmcl_t t2 ON t2.fmmclent="||g_enterprise||" AND t2.fmmcl001=fmmm005 AND t2.fmmcl002='"||g_dlang||"' ",
 
                     " WHERE fmmment=? AND fmmm001=?"
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before name="b_fill.body.fill_sql"
         {<point name="b_fill.body.fill_sql"/>}
         #end add-point
         IF NOT cl_null(g_wc2_table1) THEN
            LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY fmmm_t.fmmmseq"
         
         #add-point:單身填充控制 name="b_fill.sql"
         {<point name="b_fill.sql"/>}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE afmt535_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR afmt535_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
   #  OPEN b_fill_cs USING g_enterprise,g_fmml_m.fmmldocno   #(ver:78)
                                               
      FOREACH b_fill_cs USING g_enterprise,g_fmml_m.fmmldocno INTO g_fmmm_d[l_ac].fmmmseq,g_fmmm_d[l_ac].fmmm002, 
          g_fmmm_d[l_ac].fmmm003,g_fmmm_d[l_ac].fmmm004,g_fmmm_d[l_ac].fmmm005,g_fmmm_d[l_ac].fmmm007, 
          g_fmmm_d[l_ac].fmmm008,g_fmmm_d[l_ac].fmmm009,g_fmmm_d[l_ac].fmmm010,g_fmmm2_d[l_ac].fmmmseq, 
          g_fmmm2_d[l_ac].fmmm013,g_fmmm2_d[l_ac].fmmm014,g_fmmm3_d[l_ac].fmmmseq,g_fmmm3_d[l_ac].fmmm015, 
          g_fmmm3_d[l_ac].fmmm016,g_fmmm4_d[l_ac].fmmmseq,g_fmmm4_d[l_ac].fmmm006,g_fmmm4_d[l_ac].fmmm046, 
          g_fmmm4_d[l_ac].fmmm043,g_fmmm4_d[l_ac].fmmm044,g_fmmm4_d[l_ac].fmmm045,g_fmmm4_d[l_ac].fmmm020, 
          g_fmmm4_d[l_ac].fmmm021,g_fmmm4_d[l_ac].fmmm022,g_fmmm4_d[l_ac].fmmm023,g_fmmm4_d[l_ac].fmmm024, 
          g_fmmm4_d[l_ac].fmmm025,g_fmmm4_d[l_ac].fmmm026,g_fmmm4_d[l_ac].fmmm027,g_fmmm4_d[l_ac].fmmm028, 
          g_fmmm4_d[l_ac].fmmm029,g_fmmm4_d[l_ac].fmmm030,g_fmmm4_d[l_ac].fmmm031,g_fmmm4_d[l_ac].fmmm032, 
          g_fmmm4_d[l_ac].fmmm033,g_fmmm4_d[l_ac].fmmm034,g_fmmm4_d[l_ac].fmmm035,g_fmmm4_d[l_ac].fmmm036, 
          g_fmmm4_d[l_ac].fmmm037,g_fmmm4_d[l_ac].fmmm038,g_fmmm4_d[l_ac].fmmm039,g_fmmm4_d[l_ac].fmmm040, 
          g_fmmm4_d[l_ac].fmmm041,g_fmmm4_d[l_ac].fmmm042,g_fmmm_d[l_ac].fmmm002_desc,g_fmmm_d[l_ac].fmmm005_desc  
            #(ver:78)
         IF SQLCA.SQLCODE THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code = SQLCA.SQLCODE 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充 name="b_fill.fill"
         {<point name="b_fill.fill"/>}
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code = 9035 
               LET g_errparam.popup = TRUE 
               CALL cl_err()
            END IF
            EXIT FOREACH
         END IF
         
         LET l_ac = l_ac + 1
      END FOREACH
      LET g_error_show = 0
   
   END IF
    
 
   
   #add-point:browser_fill段其他table處理 name="browser_fill.other_fill"
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_fmmm_d.deleteElement(g_fmmm_d.getLength())
   CALL g_fmmm2_d.deleteElement(g_fmmm2_d.getLength())
   CALL g_fmmm3_d.deleteElement(g_fmmm3_d.getLength())
   CALL g_fmmm4_d.deleteElement(g_fmmm4_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE afmt535_pb
 
   
   LET li_idx = l_ac
   
   #遮罩相關處理
   FOR l_ac = 1 TO g_fmmm_d.getLength()
      LET g_fmmm_d_mask_o[l_ac].* =  g_fmmm_d[l_ac].*
      CALL afmt535_fmmm_t_mask()
      LET g_fmmm_d_mask_n[l_ac].* =  g_fmmm_d[l_ac].*
   END FOR
   
   LET g_fmmm2_d_mask_o.* =  g_fmmm2_d.*
   FOR l_ac = 1 TO g_fmmm2_d.getLength()
      LET g_fmmm2_d_mask_o[l_ac].* =  g_fmmm2_d[l_ac].*
      CALL afmt535_fmmm_t_mask()
      LET g_fmmm2_d_mask_n[l_ac].* =  g_fmmm2_d[l_ac].*
   END FOR
   LET g_fmmm3_d_mask_o.* =  g_fmmm3_d.*
   FOR l_ac = 1 TO g_fmmm3_d.getLength()
      LET g_fmmm3_d_mask_o[l_ac].* =  g_fmmm3_d[l_ac].*
      CALL afmt535_fmmm_t_mask()
      LET g_fmmm3_d_mask_n[l_ac].* =  g_fmmm3_d[l_ac].*
   END FOR
   LET g_fmmm4_d_mask_o.* =  g_fmmm4_d.*
   FOR l_ac = 1 TO g_fmmm4_d.getLength()
      LET g_fmmm4_d_mask_o[l_ac].* =  g_fmmm4_d[l_ac].*
      CALL afmt535_fmmm_t_mask()
      LET g_fmmm4_d_mask_n[l_ac].* =  g_fmmm4_d[l_ac].*
   END FOR
 
   
   LET l_ac = li_idx
   
   CALL cl_ap_performance_next_end()
 
END FUNCTION

