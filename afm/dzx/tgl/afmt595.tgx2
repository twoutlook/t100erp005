   {<point name="delete.pre_function"/>}
   #end add-point
   
   IF g_fmne_m.fmnedocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code = "std-00003" 
      LET g_errparam.popup = FALSE 
      CALL cl_err()
      RETURN
   END IF
   
   
   
   CALL s_transaction_begin()
 
   OPEN afmt595_cl USING g_enterprise,g_fmne_m.fmnedocno
   IF SQLCA.SQLCODE THEN   #(ver:78)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN afmt595_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE   #(ver:78)
      LET g_errparam.popup = TRUE 
      CLOSE afmt595_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE afmt595_master_referesh USING g_fmne_m.fmnedocno INTO g_fmne_m.fmnesite,g_fmne_m.fmne001, 
       g_fmne_m.fmnecomp,g_fmne_m.fmnedocno,g_fmne_m.fmnedocdt,g_fmne_m.fmne002,g_fmne_m.fmne003,g_fmne_m.fmne005, 
       g_fmne_m.fmne004,g_fmne_m.fmnestus,g_fmne_m.fmneownid,g_fmne_m.fmneowndp,g_fmne_m.fmnecrtid,g_fmne_m.fmnecrtdp, 
       g_fmne_m.fmnecrtdt,g_fmne_m.fmnemodid,g_fmne_m.fmnemoddt,g_fmne_m.fmnepstdt,g_fmne_m.fmnecnfid, 
       g_fmne_m.fmnecnfdt,g_fmne_m.fmnepstid,g_fmne_m.fmneownid_desc,g_fmne_m.fmneowndp_desc,g_fmne_m.fmnecrtid_desc, 
       g_fmne_m.fmnecrtdp_desc,g_fmne_m.fmnemodid_desc,g_fmne_m.fmnecnfid_desc,g_fmne_m.fmnepstid_desc 

   
   
   #檢查是否允許此動作
   IF NOT afmt595_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_fmne_m_mask_o.* =  g_fmne_m.*
   CALL afmt595_fmne_t_mask()
   LET g_fmne_m_mask_n.* =  g_fmne_m.*
   
   CALL afmt595_show()
   
   #add-point:delete段before ask name="delete.before_ask"
   {<point name="delete.before_ask"/>}
   #end add-point 
 
   IF cl_ask_del_master() THEN              #確認一下
   
      #add-point:單頭刪除前 name="delete.head.b_delete"
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point   
      
      #應用 a47 樣板自動產生(Version:4)
      #刪除相關文件
      CALL afmt595_set_pk_array()
      #add-point:相關文件刪除前 name="delete.befroe.related_document_remove"
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
  
  
      #資料備份
      LET g_fmnedocno_t = g_fmne_m.fmnedocno
 
 
      DELETE FROM fmne_t
       WHERE fmneent = g_enterprise AND fmnedocno = g_fmne_m.fmnedocno
 
       
      #add-point:單頭刪除中 name="delete.head.m_delete"
      {<point name="delete.head.m_delete"/>}
      #end add-point
       
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = g_fmne_m.fmnedocno,":",SQLERRMESSAGE  
         LET g_errparam.code = SQLCA.SQLCODE 
         LET g_errparam.popup = FALSE 
         CALL s_transaction_end('N','0')
         CALL cl_err()
         RETURN
      END IF
      
      #add-point:單頭刪除後 name="delete.head.a_delete"
      {<point name="delete.head.a_delete"/>}
      #end add-point
  
      #add-point:單身刪除前 name="delete.body.b_delete"
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM fmnf_t
       WHERE fmnfent = g_enterprise AND fmnfdocno = g_fmne_m.fmnedocno
 
 
      #add-point:單身刪除中 name="delete.body.m_delete"
      {<point name="delete.body.m_delete"/>}
      #end add-point
         
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "fmnf_t:",SQLERRMESSAGE 
         LET g_errparam.code = SQLCA.SQLCODE 
         LET g_errparam.popup = FALSE 
         CALL s_transaction_end('N','0')
         CALL cl_err()
         RETURN
      END IF    
 
      #add-point:單身刪除後 name="delete.body.a_delete"
      {<point name="delete.body.a_delete"/>}
