   LET g_fmdf001_t = g_fmdf_m.fmdf001
   LET g_fmdf002_t = g_fmdf_m.fmdf002
 
    
   LET g_fmdf_m.fmdfld = ""
   LET g_fmdf_m.fmdfdocno = ""
   LET g_fmdf_m.fmdf001 = ""
   LET g_fmdf_m.fmdf002 = ""
 
 
