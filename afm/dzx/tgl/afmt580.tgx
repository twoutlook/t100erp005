   LET g_forupd_sql = " SELECT fmmvsite,'',fmmv001,fmmv002,fmmvdocdt,'',fmmv003,fmmvdocno,fmmv015,fmmv004,fmmv013,'',fmmv007,fmmv012,'',fmmv005,fmmv011,'',fmmv006,fmmv014,fmmv010,fmmv016,fmmv017,fmmv018,fmmv019,fmmv020,fmmvstus,fmmvownid,'',fmmvowndp,'',fmmvcrtid,'',fmmvcrtdp,'',fmmvcrtdt,fmmvmodid,'',fmmvmoddt,fmmvcnfid,'',fmmvcnfdt", 
