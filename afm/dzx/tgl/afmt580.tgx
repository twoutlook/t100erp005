{<section id="afmt580.fetch" >}
#+ 指定PK後抓取單頭其他資料
PRIVATE FUNCTION afmt580_fetch(p_fl)
   #add-point:fetch段define(客製用) name="fetch.define_customerization"
   {<point name="fetch.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE p_fl       LIKE type_t.chr1
   DEFINE ls_msg     STRING
   #add-point:fetch段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="fetch.define"
   {<point name="fetch.define"/>}
   #end add-point  
   
   #add-point:Function前置處理  name="fetch.pre_function"
   {<point name="fetch.pre_function"/>}
   #end add-point
   
   #根據傳入的條件決定抓取的資料
   CASE p_fl
      WHEN "F" 
         LET g_current_idx = 1
      WHEN "P"
         IF g_current_idx > 1 THEN               
            LET g_current_idx = g_current_idx - 1
         END IF 
      WHEN "N"
         IF g_current_idx < g_header_cnt THEN
            LET g_current_idx =  g_current_idx + 1
         END IF        
      WHEN "L" 
         #LET g_current_idx = g_header_cnt        
         LET g_current_idx = g_browser.getLength()    
      WHEN "/"
         #詢問要指定的筆數
         IF (NOT g_no_ask) THEN      
            CALL cl_getmsg("fetch", g_lang) RETURNING ls_msg
            LET INT_FLAG = 0
 
            PROMPT ls_msg CLIPPED,": " FOR g_jump
               #交談指令共用ACTION
               &include "common_action.4gl"
            END PROMPT
            
            IF INT_FLAG THEN
               LET INT_FLAG = 0
               EXIT CASE  
            END IF           
         END IF
         IF g_jump > 0 THEN
            LET g_current_idx = g_jump
         END IF
         LET g_no_ask = FALSE     
   END CASE
 
   #筆數顯示
   LET g_browser_idx = g_current_idx 
   IF g_browser_cnt > 0 THEN
      DISPLAY g_browser_idx TO FORMONLY.b_index #當下筆數
      DISPLAY g_browser_cnt TO FORMONLY.b_count #總筆數
      DISPLAY g_browser_idx TO FORMONLY.h_index #當下筆數
      DISPLAY g_browser_cnt TO FORMONLY.h_count #總筆數
   ELSE
      DISPLAY '' TO FORMONLY.b_index #當下筆數
      DISPLAY '' TO FORMONLY.b_count #總筆數
      DISPLAY '' TO FORMONLY.h_index #當下筆數
      DISPLAY '' TO FORMONLY.h_count #總筆數
   END IF
   
   #該樣板不需此段落#樹狀管控(action)
   #該樣板不需此段落IF g_browser[g_current_idx].b_expcode <> "1" THEN
   #該樣板不需此段落   INITIALIZE g_fmmv_m.* TO NULL
   #該樣板不需此段落   DISPLAY BY NAME g_fmmv_m.*
   #該樣板不需此段落   CALL cl_set_act_visible("statechange,modify,delete,reproduce", FALSE)
   #該樣板不需此段落   RETURN
   #該樣板不需此段落ELSE
   #該樣板不需此段落   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   #該樣板不需此段落END IF
   
   #該樣板不需此段落CALL afmt580_browser_fill(g_wc,p_fl)
   
   #避免超出browser資料筆數上限
   IF g_current_idx > g_browser.getLength() THEN
      LET g_browser_idx = g_browser.getLength()
      LET g_current_idx = g_browser.getLength() 
   END IF
   
   # 設定browse索引
   CALL g_curr_diag.setCurrentRow("s_browse", g_current_idx)
   CALL cl_navigator_setting(g_browser_idx, g_browser_cnt) 
 
   #代表沒有資料, 無需做後續資料撈取之動作
   IF g_current_idx = 0 THEN
      RETURN
   END IF
 
   #根據選定的筆數給予key欄位值
   LET g_fmmv_m.fmmvdocno = g_browser[g_current_idx].b_fmmvdocno

                       
   #讀取單頭所有欄位資料
   EXECUTE afmt580_master_referesh USING g_fmmv_m.fmmvdocno INTO g_fmmv_m.fmmvsite,g_fmmv_m.fmmv001,g_fmmv_m.fmmv002,g_fmmv_m.fmmvdocdt,g_fmmv_m.fmmv003,g_fmmv_m.fmmvdocno,g_fmmv_m.fmmv015,g_fmmv_m.fmmv004,g_fmmv_m.fmmv013,g_fmmv_m.fmmv007,g_fmmv_m.fmmv012,g_fmmv_m.fmmv005,g_fmmv_m.fmmv011,g_fmmv_m.fmmv006,g_fmmv_m.fmmv014,g_fmmv_m.fmmv010,g_fmmv_m.fmmv016,g_fmmv_m.fmmv017,g_fmmv_m.fmmv018,g_fmmv_m.fmmv019,g_fmmv_m.fmmv020,g_fmmv_m.fmmvstus,g_fmmv_m.fmmvownid,g_fmmv_m.fmmvowndp,g_fmmv_m.fmmvcrtid,g_fmmv_m.fmmvcrtdp,g_fmmv_m.fmmvcrtdt,g_fmmv_m.fmmvmodid,g_fmmv_m.fmmvmoddt,g_fmmv_m.fmmvcnfid,g_fmmv_m.fmmvcnfdt,g_fmmv_m.fmmvownid_desc,g_fmmv_m.fmmvowndp_desc,g_fmmv_m.fmmvcrtid_desc,g_fmmv_m.fmmvcrtdp_desc,g_fmmv_m.fmmvmodid_desc,g_fmmv_m.fmmvcnfid_desc
   
   #遮罩相關處理
   LET g_fmmv_m_mask_o.* =  g_fmmv_m.*
   CALL afmt580_fmmv_t_mask()
   LET g_fmmv_m_mask_n.* =  g_fmmv_m.*
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL afmt580_set_act_visible()
   CALL afmt580_set_act_no_visible()
 
   #add-point:fetch段action控制 name="fetch.action_control"
   {<point name="fetch.action_control"/>}
   #end add-point  
   
   
   
   #保存單頭舊值
   LET g_fmmv_m_t.* = g_fmmv_m.*
   LET g_fmmv_m_o.* = g_fmmv_m.*
   
   LET g_data_owner = g_fmmv_m.fmmvownid      
   LET g_data_dept  = g_fmmv_m.fmmvowndp
   
   #重新顯示
   CALL afmt580_show()
   #應用 a56 樣板自動產生(Version:3)
   #檢查此單據是否需顯示BPM簽核狀況按鈕 
   IF cl_bpm_chk() THEN
      CALL cl_set_act_visible("bpm_status",TRUE)
   ELSE
      CALL cl_set_act_visible("bpm_status",FALSE)
   END IF
 



 
