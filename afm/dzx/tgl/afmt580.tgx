   LET g_sql = " SELECT DISTINCT t0.fmmvsite,t0.fmmv001,t0.fmmv002,t0.fmmvdocdt,t0.fmmv003,t0.fmmvdocno,t0.fmmv015,t0.fmmv004,t0.fmmv013,t0.fmmv007,t0.fmmv012,t0.fmmv005,t0.fmmv011,t0.fmmv006,t0.fmmv014,t0.fmmv010,t0.fmmv016,t0.fmmv017,t0.fmmv018,t0.fmmv019,t0.fmmv020,t0.fmmvstus,t0.fmmvownid,t0.fmmvowndp,t0.fmmvcrtid,t0.fmmvcrtdp,t0.fmmvcrtdt,t0.fmmvmodid,t0.fmmvmoddt,t0.fmmvcnfid,t0.fmmvcnfdt,t1.ooag011 ,t2.ooefl003 ,t3.ooag011 ,t4.ooefl003 ,t5.ooag011 ,t6.ooag011",
               " FROM fmmv_t t0",
                              " LEFT JOIN ooag_t t1 ON t1.ooagent="||g_enterprise||" AND t1.ooag001=t0.fmmvownid  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent="||g_enterprise||" AND t2.ooefl001=t0.fmmvowndp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent="||g_enterprise||" AND t3.ooag001=t0.fmmvcrtid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent="||g_enterprise||" AND t4.ooefl001=t0.fmmvcrtdp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=t0.fmmvmodid  ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent="||g_enterprise||" AND t6.ooag001=t0.fmmvcnfid  ",

               " WHERE t0.fmmvent = " ||g_enterprise|| " AND t0.fmmvdocno = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
