{<section id="afmt530.modify" >}
#+ 資料修改
PRIVATE FUNCTION afmt530_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   {<point name="modify.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   {<point name="modify.define"/>}
   #end add-point
   
   #add-point:Function前置處理 name="modify.pre_function"
   {<point name="modify.pre_function"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_fmmj_m.fmmjdocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_fmmjdocno_t = g_fmmj_m.fmmjdocno
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN afmt530_cl USING g_enterprise,g_fmmj_m.fmmjdocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN afmt530_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CLOSE afmt530_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE afmt530_master_referesh USING g_fmmj_m.fmmjdocno INTO g_fmmj_m.fmmjsite,g_fmmj_m.fmmjdocno, 
       g_fmmj_m.fmmjdocdt,g_fmmj_m.fmmj027,g_fmmj_m.fmmj001,g_fmmj_m.fmmj002,g_fmmj_m.fmmj003,g_fmmj_m.fmmj004, 
       g_fmmj_m.fmmj006,g_fmmj_m.fmmj010,g_fmmj_m.fmmjstus,g_fmmj_m.fmmj005,g_fmmj_m.fmmj007,g_fmmj_m.fmmj008, 
       g_fmmj_m.fmmj011,g_fmmj_m.fmmj012,g_fmmj_m.fmmj013,g_fmmj_m.fmmj014,g_fmmj_m.fmmj025,g_fmmj_m.fmmj026, 
       g_fmmj_m.fmmj009,g_fmmj_m.fmmj028,g_fmmj_m.fmmj015,g_fmmj_m.fmmj029,g_fmmj_m.fmmj016,g_fmmj_m.fmmj030, 
       g_fmmj_m.fmmj017,g_fmmj_m.fmmj031,g_fmmj_m.fmmj018,g_fmmj_m.fmmj019,g_fmmj_m.fmmj022,g_fmmj_m.fmmj020, 
       g_fmmj_m.fmmj021,g_fmmj_m.fmmj024,g_fmmj_m.fmmj023,g_fmmj_m.fmmjownid,g_fmmj_m.fmmjowndp,g_fmmj_m.fmmjcrtid, 
       g_fmmj_m.fmmjcrtdp,g_fmmj_m.fmmjcrtdt,g_fmmj_m.fmmjmodid,g_fmmj_m.fmmjmoddt,g_fmmj_m.fmmjcnfid, 
       g_fmmj_m.fmmjcnfdt,g_fmmj_m.fmmjsite_desc,g_fmmj_m.fmmj002_desc,g_fmmj_m.fmmj003_desc,g_fmmj_m.fmmjownid_desc, 
       g_fmmj_m.fmmjowndp_desc,g_fmmj_m.fmmjcrtid_desc,g_fmmj_m.fmmjcrtdp_desc,g_fmmj_m.fmmjmodid_desc, 
       g_fmmj_m.fmmjcnfid_desc
 
   #檢查是否允許此動作
   IF NOT afmt530_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_fmmj_m_mask_o.* =  g_fmmj_m.*
   CALL afmt530_fmmj_t_mask()
   LET g_fmmj_m_mask_n.* =  g_fmmj_m.*
   
   
 
   #顯示資料
   CALL afmt530_show()
   
   WHILE TRUE
      LET g_fmmj_m.fmmjdocno = g_fmmjdocno_t
 
      
      #寫入修改者/修改日期資訊
      LET g_fmmj_m.fmmjmodid = g_user 
LET g_fmmj_m.fmmjmoddt = cl_get_current()
LET g_fmmj_m.fmmjmodid_desc = cl_get_username(g_fmmj_m.fmmjmodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
      #end add-point
 
      #資料輸入
      CALL afmt530_input("u")     
 
      #add-point:modify段修改後 name="modify.after_input"
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         LET g_fmmj_m.* = g_fmmj_m_t.*
         CALL afmt530_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE fmmj_t SET (fmmjmodid,fmmjmoddt) = (g_fmmj_m.fmmjmodid,g_fmmj_m.fmmjmoddt)
       WHERE fmmjent = g_enterprise AND fmmjdocno = g_fmmjdocno_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL afmt530_set_act_visible()
   CALL afmt530_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " fmmjent = " ||g_enterprise|| " AND",
                      " fmmjdocno = '", g_fmmj_m.fmmjdocno, "' "
 
   #填到對應位置
   CALL afmt530_browser_fill(g_wc,"")
 
   CLOSE afmt530_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL afmt530_msgcentre_notify('modify')
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   

