{<section id="afmt530.fetch" >}
#+ 指定PK後抓取單頭其他資料
PRIVATE FUNCTION afmt530_fetch(p_fl)
   #add-point:fetch段define(客製用) name="fetch.define_customerization"
   {<point name="fetch.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE p_fl       LIKE type_t.chr1
   DEFINE ls_msg     STRING
   #add-point:fetch段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="fetch.define"
   {<point name="fetch.define"/>}
   #end add-point  
   
   #add-point:Function前置處理  name="fetch.pre_function"
   {<point name="fetch.pre_function"/>}
   #end add-point
   
   #根據傳入的條件決定抓取的資料
   CASE p_fl
      WHEN "F" 
         LET g_current_idx = 1
      WHEN "P"
         IF g_current_idx > 1 THEN               
            LET g_current_idx = g_current_idx - 1
         END IF 
      WHEN "N"
         IF g_current_idx < g_header_cnt THEN
            LET g_current_idx =  g_current_idx + 1
         END IF        
      WHEN "L" 
         #LET g_current_idx = g_header_cnt        
         LET g_current_idx = g_browser.getLength()    
      WHEN "/"
         #詢問要指定的筆數
         IF (NOT g_no_ask) THEN      
            CALL cl_getmsg("fetch", g_lang) RETURNING ls_msg
            LET INT_FLAG = 0
 
            PROMPT ls_msg CLIPPED,": " FOR g_jump
               #交談指令共用ACTION
               &include "common_action.4gl"
            END PROMPT
            
            IF INT_FLAG THEN
               LET INT_FLAG = 0
               EXIT CASE  
            END IF           
         END IF
         IF g_jump > 0 THEN
            LET g_current_idx = g_jump
         END IF
         LET g_no_ask = FALSE     
   END CASE
 
   #筆數顯示
   LET g_browser_idx = g_current_idx 
   IF g_browser_cnt > 0 THEN
      DISPLAY g_browser_idx TO FORMONLY.b_index #當下筆數
      DISPLAY g_browser_cnt TO FORMONLY.b_count #總筆數
      DISPLAY g_browser_idx TO FORMONLY.h_index #當下筆數
      DISPLAY g_browser_cnt TO FORMONLY.h_count #總筆數
   ELSE
      DISPLAY '' TO FORMONLY.b_index #當下筆數
      DISPLAY '' TO FORMONLY.b_count #總筆數
      DISPLAY '' TO FORMONLY.h_index #當下筆數
      DISPLAY '' TO FORMONLY.h_count #總筆數
   END IF
   
   
   
   #避免超出browser資料筆數上限
   IF g_current_idx > g_browser.getLength() THEN
      LET g_browser_idx = g_browser.getLength()
      LET g_current_idx = g_browser.getLength() 
   END IF
   
   # 設定browse索引
   CALL g_curr_diag.setCurrentRow("s_browse", g_current_idx)
   CALL cl_navigator_setting(g_browser_idx, g_browser_cnt) 
 
   #代表沒有資料, 無需做後續資料撈取之動作
   IF g_current_idx = 0 THEN
      RETURN
   END IF
 
   #根據選定的筆數給予key欄位值
   LET g_fmmj_m.fmmjdocno = g_browser[g_current_idx].b_fmmjdocno
 
                       
   #讀取單頭所有欄位資料
   EXECUTE afmt530_master_referesh USING g_fmmj_m.fmmjdocno INTO g_fmmj_m.fmmjsite,g_fmmj_m.fmmjdocno, 
       g_fmmj_m.fmmjdocdt,g_fmmj_m.fmmj027,g_fmmj_m.fmmj001,g_fmmj_m.fmmj002,g_fmmj_m.fmmj003,g_fmmj_m.fmmj004, 
       g_fmmj_m.fmmj006,g_fmmj_m.fmmj010,g_fmmj_m.fmmjstus,g_fmmj_m.fmmj005,g_fmmj_m.fmmj007,g_fmmj_m.fmmj008, 
       g_fmmj_m.fmmj011,g_fmmj_m.fmmj012,g_fmmj_m.fmmj013,g_fmmj_m.fmmj014,g_fmmj_m.fmmj025,g_fmmj_m.fmmj026, 
       g_fmmj_m.fmmj009,g_fmmj_m.fmmj028,g_fmmj_m.fmmj015,g_fmmj_m.fmmj029,g_fmmj_m.fmmj016,g_fmmj_m.fmmj030, 
       g_fmmj_m.fmmj017,g_fmmj_m.fmmj031,g_fmmj_m.fmmj018,g_fmmj_m.fmmj019,g_fmmj_m.fmmj022,g_fmmj_m.fmmj020, 
       g_fmmj_m.fmmj021,g_fmmj_m.fmmj024,g_fmmj_m.fmmj023,g_fmmj_m.fmmjownid,g_fmmj_m.fmmjowndp,g_fmmj_m.fmmjcrtid, 
       g_fmmj_m.fmmjcrtdp,g_fmmj_m.fmmjcrtdt,g_fmmj_m.fmmjmodid,g_fmmj_m.fmmjmoddt,g_fmmj_m.fmmjcnfid, 
       g_fmmj_m.fmmjcnfdt,g_fmmj_m.fmmjsite_desc,g_fmmj_m.fmmj002_desc,g_fmmj_m.fmmj003_desc,g_fmmj_m.fmmjownid_desc, 
       g_fmmj_m.fmmjowndp_desc,g_fmmj_m.fmmjcrtid_desc,g_fmmj_m.fmmjcrtdp_desc,g_fmmj_m.fmmjmodid_desc, 
       g_fmmj_m.fmmjcnfid_desc
   
   #遮罩相關處理
   LET g_fmmj_m_mask_o.* =  g_fmmj_m.*
   CALL afmt530_fmmj_t_mask()
   LET g_fmmj_m_mask_n.* =  g_fmmj_m.*
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL afmt530_set_act_visible()
   CALL afmt530_set_act_no_visible()
 
   #add-point:fetch段action控制 name="fetch.action_control"
   {<point name="fetch.action_control"/>}
   #end add-point  
   
   
   
   #保存單頭舊值
   LET g_fmmj_m_t.* = g_fmmj_m.*
   LET g_fmmj_m_o.* = g_fmmj_m.*
   
   LET g_data_owner = g_fmmj_m.fmmjownid      
   LET g_data_dept  = g_fmmj_m.fmmjowndp
   
   #重新顯示
   CALL afmt530_show()
   #應用 a56 樣板自動產生(Version:3)
   #檢查此單據是否需顯示BPM簽核狀況按鈕 
   IF cl_bpm_chk() THEN
      CALL cl_set_act_visible("bpm_status",TRUE)
   ELSE
      CALL cl_set_act_visible("bpm_status",FALSE)
   END IF
 
 
 
 
 
END FUNCTION

