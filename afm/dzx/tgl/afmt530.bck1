{<section id="afmt530.delete" >}
#+ 資料刪除 
PRIVATE FUNCTION afmt530_delete()
   #add-point:delete段define(客製用) name="delete.define_customerization"
   {<point name="delete.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="delete.define"
   {<point name="delete.define"/>}
   #end add-point  
   
   #add-point:Function前置處理  name="delete.pre_function"
   {<point name="delete.pre_function"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_fmmj_m.fmmjdocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
    
   LET g_fmmjdocno_t = g_fmmj_m.fmmjdocno
 
   
   
 
   OPEN afmt530_cl USING g_enterprise,g_fmmj_m.fmmjdocno
 
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN afmt530_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CLOSE afmt530_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE afmt530_master_referesh USING g_fmmj_m.fmmjdocno INTO g_fmmj_m.fmmjsite,g_fmmj_m.fmmjdocno, 
       g_fmmj_m.fmmjdocdt,g_fmmj_m.fmmj027,g_fmmj_m.fmmj001,g_fmmj_m.fmmj002,g_fmmj_m.fmmj003,g_fmmj_m.fmmj004, 
       g_fmmj_m.fmmj006,g_fmmj_m.fmmj010,g_fmmj_m.fmmjstus,g_fmmj_m.fmmj005,g_fmmj_m.fmmj007,g_fmmj_m.fmmj008, 
       g_fmmj_m.fmmj011,g_fmmj_m.fmmj012,g_fmmj_m.fmmj013,g_fmmj_m.fmmj014,g_fmmj_m.fmmj025,g_fmmj_m.fmmj026, 
       g_fmmj_m.fmmj009,g_fmmj_m.fmmj028,g_fmmj_m.fmmj015,g_fmmj_m.fmmj029,g_fmmj_m.fmmj016,g_fmmj_m.fmmj030, 
       g_fmmj_m.fmmj017,g_fmmj_m.fmmj031,g_fmmj_m.fmmj018,g_fmmj_m.fmmj019,g_fmmj_m.fmmj022,g_fmmj_m.fmmj020, 
       g_fmmj_m.fmmj021,g_fmmj_m.fmmj024,g_fmmj_m.fmmj023,g_fmmj_m.fmmjownid,g_fmmj_m.fmmjowndp,g_fmmj_m.fmmjcrtid, 
       g_fmmj_m.fmmjcrtdp,g_fmmj_m.fmmjcrtdt,g_fmmj_m.fmmjmodid,g_fmmj_m.fmmjmoddt,g_fmmj_m.fmmjcnfid, 
       g_fmmj_m.fmmjcnfdt,g_fmmj_m.fmmjsite_desc,g_fmmj_m.fmmj002_desc,g_fmmj_m.fmmj003_desc,g_fmmj_m.fmmjownid_desc, 
       g_fmmj_m.fmmjowndp_desc,g_fmmj_m.fmmjcrtid_desc,g_fmmj_m.fmmjcrtdp_desc,g_fmmj_m.fmmjmodid_desc, 
       g_fmmj_m.fmmjcnfid_desc
   
   
   #檢查是否允許此動作
   IF NOT afmt530_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_fmmj_m_mask_o.* =  g_fmmj_m.*
   CALL afmt530_fmmj_t_mask()
   LET g_fmmj_m_mask_n.* =  g_fmmj_m.*
   
   #將最新資料顯示到畫面上
   CALL afmt530_show()
   
   IF cl_ask_delete() THEN
 
      #add-point:單頭刪除前 name="delete.head.b_delete"
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point
 
      #應用 a47 樣板自動產生(Version:4)
      #刪除相關文件
      CALL afmt530_set_pk_array()
      #add-point:相關文件刪除前 name="delete.befroe.related_document_remove"
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
 
 
      DELETE FROM fmmj_t 
       WHERE fmmjent = g_enterprise AND fmmjdocno = g_fmmj_m.fmmjdocno 
 
 
      #add-point:單頭刪除中 name="delete.head.m_delete"
      {<point name="delete.head.m_delete"/>}
      #end add-point
         
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "fmmj_t:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = FALSE 
         CALL s_transaction_end('N','0')
         CALL cl_err()
      END IF
  
      
      
      #add-point:單頭刪除後 name="delete.head.a_delete"
      {<point name="delete.head.a_delete"/>}
      #end add-point
      
       
 
      #修改歷程記錄(刪除)
      IF NOT cl_log_modified_record('','') THEN 
         CLOSE afmt530_cl
         CALL s_transaction_end('N','0')
         RETURN
      END IF
      
      CLEAR FORM
      CALL afmt530_ui_browser_refresh()
      
      #確保畫面上保有資料
      IF g_browser_cnt > 0 THEN
         #CALL afmt530_browser_fill(g_wc,"")
         CALL afmt530_fetch("P")
      ELSE
         CLEAR FORM
      END IF
      CALL s_transaction_end('Y','0')
   ELSE    
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE afmt530_cl
 
   #功能已完成,通報訊息中心
   CALL afmt530_msgcentre_notify('delete')
 
   #add-point:單頭刪除完成後 name="delete.a_delete"
   {<point name="delete.a_delete"/>}
   #end add-point
 
END FUNCTION

