                  g_fmmx_d[g_detail_idx].fmmx108,g_fmmx2_d[g_detail_idx].fmmx133,g_fmmx2_d[g_detail_idx].fmmx032, 
                  g_fmmx2_d[g_detail_idx].fmmx031,g_fmmx2_d[g_detail_idx].fmmx006,g_fmmx2_d[g_detail_idx].fmmx007, 
                  g_fmmx2_d[g_detail_idx].fmmx008,g_fmmx2_d[g_detail_idx].fmmx009,g_fmmx2_d[g_detail_idx].fmmx010, 
                  g_fmmx2_d[g_detail_idx].fmmx011,g_fmmx2_d[g_detail_idx].fmmx012,g_fmmx2_d[g_detail_idx].fmmx013, 
                  g_fmmx2_d[g_detail_idx].fmmx014,g_fmmx2_d[g_detail_idx].fmmx015,g_fmmx2_d[g_detail_idx].fmmx016, 
                  g_fmmx2_d[g_detail_idx].fmmx017,g_fmmx2_d[g_detail_idx].fmmx018,g_fmmx2_d[g_detail_idx].fmmx019, 
                  g_fmmx2_d[g_detail_idx].fmmx020,g_fmmx2_d[g_detail_idx].fmmx021,g_fmmx2_d[g_detail_idx].fmmx022, 
                  g_fmmx2_d[g_detail_idx].fmmx023,g_fmmx2_d[g_detail_idx].fmmx024,g_fmmx2_d[g_detail_idx].fmmx025, 
                  g_fmmx2_d[g_detail_idx].fmmx026,g_fmmx2_d[g_detail_idx].fmmx027,g_fmmx2_d[g_detail_idx].fmmx028, 
                  g_fmmx3_d[g_detail_idx].fmmx121,g_fmmx3_d[g_detail_idx].fmmx122,g_fmmx4_d[g_detail_idx].fmmx131, 
                  g_fmmx4_d[g_detail_idx].fmmx132) 
         WHERE fmmxent = g_enterprise AND fmmxdocno = ps_keys_bak[1] AND fmmxseq = ps_keys_bak[2]
      #add-point:update_b段修改中 name="update_b.m_update"
      {<point name="update_b.m_update"/>}
      #end add-point   
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "fmmx_t" 
            LET g_errparam.code = "std-00009" 
