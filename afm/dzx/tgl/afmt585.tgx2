                  g_fmmx_d[g_detail_idx].fmmx033,g_fmmx_d[g_detail_idx].fmmx101,g_fmmx_d[g_detail_idx].fmmx111, 
                  g_fmmx_d[g_detail_idx].fmmx112,g_fmmx_d[g_detail_idx].fmmx030,g_fmmx_d[g_detail_idx].fmmx102, 
                  g_fmmx_d[g_detail_idx].fmmx103,g_fmmx_d[g_detail_idx].fmmx104,g_fmmx_d[g_detail_idx].fmmx029, 
