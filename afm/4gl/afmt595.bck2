                  g_fmnf2_d[g_detail_idx].fmnf025,g_fmnf2_d[g_detail_idx].fmnf026,g_fmnf2_d[g_detail_idx].fmnf027, 
                  g_fmnf2_d[g_detail_idx].fmnf028,g_fmnf3_d[g_detail_idx].fmnf121,g_fmnf3_d[g_detail_idx].fmnf122, 
                  g_fmnf3_d[g_detail_idx].fmnf123,g_fmnf4_d[g_detail_idx].fmnf131,g_fmnf4_d[g_detail_idx].fmnf132, 
