{<section id="afmt595.action_chk" >}
#+ 修改/刪除前行為檢查(是否可允許此動作), 若有其他行為須管控也可透過此段落
PRIVATE FUNCTION afmt595_action_chk()
   #add-point:action_chk段define(客製用) name="action_chk.define_customerization"
   
   #end add-point
   #add-point:action_chk段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="action_chk.define"
   
   #end add-point
   
   #add-point:action_chk段action_chk name="action_chk.action_chk"
   #160818-00017#13 2016/08/23 By 07900 add -s--
   SELECT fmnestus INTO g_fmne_m.fmnestus
     FROM fmne_t
    WHERE fmneent = g_enterprise
      AND fmnedocno = g_fmne_m.fmnedocno

   IF (g_action_choice="modify" OR g_action_choice="delete" OR g_action_choice="modify_detail")  THEN
     LET g_errno = NULL
     CASE g_fmne_m.fmnestus
        
        WHEN 'W'
           LET g_errno = 'sub-00180'
        WHEN 'X'
           LET g_errno = 'sub-00229'
        WHEN 'Y'
           LET g_errno = 'sub-00178'
        WHEN 'S'
           LET g_errno = 'sub-00230'
     END CASE

     IF NOT cl_null(g_errno) THEN
        INITIALIZE g_errparam TO NULL
        LET g_errparam.code = g_errno
        LET g_errparam.extend = g_fmne_m.fmnedocno
        LET g_errparam.popup = TRUE
        CALL cl_err()
        LET g_errno = NULL
        CALL afmt595_set_act_visible()
        CALL afmt595_set_act_no_visible()
        CALL afmt595_show()
        RETURN FALSE
     END IF
   END IF
   
   #160818-00017#13 2016/08/23 By 07900 add -e--
   #end add-point
      
   RETURN TRUE
   
END FUNCTION
 
