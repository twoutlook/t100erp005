################################################################################
# Descriptions...: 檢核帳戶存在+有效+幣別
# Memo...........: #150820-00011#6
# Usage..........: CALL afmt580_fmmv013_chk(p_fmmvsite,p_fmmv013)
# Input parameter: p_fmmv013   帳戶
# Date & Author..: 15/09/14 By RayHuang
# Modify.........: 
################################################################################
PRIVATE FUNCTION afmt580_fmmv013_chk(p_fmmvsite,p_fmmv013)
DEFINE p_fmmvsite  LIKE fmmv_t.fmmvsite
   DEFINE p_fmmv013   LIKE fmmv_t.fmmv013
   DEFINE r_success   LIKE type_t.num5
   DEFINE r_errno     LIKE gzze_t.gzze001
   DEFINE l_nmaa      RECORD
                      nmaastus   LIKE nmaa_t.nmaastus,
                      nmaa002    LIKE nmaa_t.nmaa002,
                      nmas003    LIKE nmas_t.nmas003
                      END RECORD
   DEFINE l_sql       STRING
   DEFINE l_count     LIKE type_t.num5   
                     
   LET r_success = TRUE
   LET r_errno   = ''
   LET l_sql = NULL
   LET l_count = 0
   IF NOT cl_null(p_fmmv013) THEN
      #存在
      LET l_sql = "SELECT COUNT(*) FROM nmaa_t,nmas_t ",
                  " WHERE nmaaent = ",g_enterprise,
                  "   AND nmas001 = nmaa001 ",
                  "   AND nmaa002 = '",p_fmmvsite,"' " ,
                  "   AND nmas002 = '",p_fmmv013,"' " ,
                  "   AND nmas003 = '",g_fmmv_m.fmmv004,"' "
      PREPARE sel_nmaap01 FROM l_sql            
      
      LET l_count = NULL
      EXECUTE sel_nmaap01 INTO l_count
      IF cl_null(l_count) THEN LET l_count = 0 END IF
      IF l_count = 0 THEN
         LET r_success = FALSE
         LET r_errno = 'anm-00125'
         RETURN r_success,r_errno
      END IF
      #存在+有效
      LET l_sql = l_sql CLIPPED," AND nmaastus = 'Y' "
      PREPARE sel_nmaap02 FROM l_sql  
      LET l_count = NULL
      EXECUTE sel_nmaap02 INTO l_count
      IF cl_null(l_count) THEN LET l_count = 0 END IF
      IF l_count = 0 THEN
         LET r_success = FALSE
         LET r_errno = 'ais-00080'
         RETURN r_success,r_errno
      END IF
      FREE sel_nmaap01
      FREE sel_nmaap02      
   END IF
   RETURN r_success,r_errno
END FUNCTION

