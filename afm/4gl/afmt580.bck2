{<section id="afmt580.set_no_entry" >}
#+ 單頭欄位關閉設定
PRIVATE FUNCTION afmt580_set_no_entry(p_cmd)
   #add-point:set_no_entry段define(客製用) name="set_no_entry.define_customerization"
   
   #end add-point
   DEFINE p_cmd LIKE type_t.chr1
   #add-point:set_no_entry段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="set_no_entry.define"
   DEFINE l_success  LIKE type_t.num5     #151130-00015#2 
   DEFINE l_slip     LIKE type_t.chr10  #151130-00015#2
   DEFINE l_dfin0033  LIKE type_t.chr80 #151130-00015#2
   #end add-point     
   
   #add-point:Function前置處理  name="set_no_entry.pre_function"
   
   #end add-point
   
   IF p_cmd = 'u' AND g_chkey = 'N' THEN
      CALL cl_set_comp_entry("fmmvdocno",FALSE)
      #根據azzi850使用者身分開關特定欄位
      IF NOT cl_null(g_no_entry) THEN
         CALL cl_set_comp_entry(g_no_entry,FALSE)
      END IF
      #add-point:set_no_entry段欄位控制 name="set_no_entry.field_control"
      CALL cl_set_comp_entry("fmmvsite,fmmv001",FALSE)
      #end add-point 
   END IF
   
   #add-point:set_no_entry段欄位控制後 name="set_no_entry.after_control"
   CASE 
      WHEN g_fmmv_m.fmmv007 = 1
         CALL cl_set_comp_entry("fmmv010",FALSE)
      WHEN g_fmmv_m.fmmv007 = 2
          CALL cl_set_comp_required("fmmv011,fmmv012,fmmv013",FALSE)
      WHEN g_fmmv_m.fmmv007 = 3
         CALL cl_set_comp_entry("fmmv010",FALSE)
   END CASE
   #151130-00015#2 -begin -add by XZG 20151222
      IF NOT cl_null(g_fmmv_m.fmmvdocno) THEN  
            #获取单别
            CALL s_aooi200_fin_get_slip(g_fmmv_m.fmmvdocno) RETURNING l_success,l_slip
            #是否可改日期
            CALL s_fin_get_doc_para(g_glaa.glaald,g_glaa.glaacomp,l_slip,'D-FIN-0033') RETURNING l_dfin0033
            IF l_dfin0033 = "N"  THEN 
               CALL cl_set_comp_entry("fmmvdocdt",FALSE)
            ELSE 
               CALL cl_set_comp_entry("fmmvdocdt",TRUE)
            END IF          
         END IF 
      #151130-00015#2  -end
   #end add-point 
 
END FUNCTION
 
