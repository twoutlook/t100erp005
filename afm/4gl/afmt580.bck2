{<section id="afmt580.state_change" >}
   #應用 a09 樣板自動產生(Version:17)
#+ 確認碼變更 
PRIVATE FUNCTION afmt580_statechange()
   #add-point:statechange段define(客製用) name="statechange.define_customerization"
   
   #end add-point  
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="statechange.define"
   DEFINE l_count LIKE type_t.num5
   DEFINE l_cnt1   LIKE type_t.num10    #150924-00006#3
   #end add-point  
   
   #add-point:Function前置處理 name="statechange.before"
   
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_fmmv_m.fmmvdocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN afmt580_cl USING g_enterprise,g_fmmv_m.fmmvdocno
   IF STATUS THEN
      CLOSE afmt580_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN afmt580_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE afmt580_master_referesh USING g_fmmv_m.fmmvdocno INTO g_fmmv_m.fmmvsite,g_fmmv_m.fmmv001, 
       g_fmmv_m.fmmv002,g_fmmv_m.fmmvdocdt,g_fmmv_m.fmmv003,g_fmmv_m.fmmvdocno,g_fmmv_m.fmmv015,g_fmmv_m.fmmv004, 
       g_fmmv_m.fmmv013,g_fmmv_m.fmmv007,g_fmmv_m.fmmv012,g_fmmv_m.fmmv005,g_fmmv_m.fmmv011,g_fmmv_m.fmmv006, 
       g_fmmv_m.fmmv014,g_fmmv_m.fmmv010,g_fmmv_m.fmmv016,g_fmmv_m.fmmv017,g_fmmv_m.fmmv018,g_fmmv_m.fmmv019, 
       g_fmmv_m.fmmv020,g_fmmv_m.fmmvstus,g_fmmv_m.fmmvownid,g_fmmv_m.fmmvowndp,g_fmmv_m.fmmvcrtid,g_fmmv_m.fmmvcrtdp, 
       g_fmmv_m.fmmvcrtdt,g_fmmv_m.fmmvmodid,g_fmmv_m.fmmvmoddt,g_fmmv_m.fmmvcnfid,g_fmmv_m.fmmvcnfdt, 
       g_fmmv_m.fmmvownid_desc,g_fmmv_m.fmmvowndp_desc,g_fmmv_m.fmmvcrtid_desc,g_fmmv_m.fmmvcrtdp_desc, 
       g_fmmv_m.fmmvmodid_desc,g_fmmv_m.fmmvcnfid_desc
   
 
   #檢查是否允許此動作
   IF NOT afmt580_action_chk() THEN
      CLOSE afmt580_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_fmmv_m.fmmvsite,g_fmmv_m.fmmvsite_desc,g_fmmv_m.fmmv001,g_fmmv_m.fmmv002,g_fmmv_m.fmmvdocdt, 
       g_fmmv_m.b_fmmv0012,g_fmmv_m.fmmv003,g_fmmv_m.fmmvdocno,g_fmmv_m.fmmv015,g_fmmv_m.fmmv004,g_fmmv_m.fmmv013, 
       g_fmmv_m.fmmv013_desc,g_fmmv_m.fmmv007,g_fmmv_m.fmmv012,g_fmmv_m.fmmv012_desc,g_fmmv_m.fmmv005, 
       g_fmmv_m.fmmv011,g_fmmv_m.fmmv011_desc,g_fmmv_m.fmmv006,g_fmmv_m.fmmv014,g_fmmv_m.fmmv010,g_fmmv_m.fmmv016, 
       g_fmmv_m.fmmv017,g_fmmv_m.fmmv018,g_fmmv_m.fmmv019,g_fmmv_m.fmmv020,g_fmmv_m.fmmvstus,g_fmmv_m.fmmvownid, 
       g_fmmv_m.fmmvownid_desc,g_fmmv_m.fmmvowndp,g_fmmv_m.fmmvowndp_desc,g_fmmv_m.fmmvcrtid,g_fmmv_m.fmmvcrtid_desc, 
       g_fmmv_m.fmmvcrtdp,g_fmmv_m.fmmvcrtdp_desc,g_fmmv_m.fmmvcrtdt,g_fmmv_m.fmmvmodid,g_fmmv_m.fmmvmodid_desc, 
       g_fmmv_m.fmmvmoddt,g_fmmv_m.fmmvcnfid,g_fmmv_m.fmmvcnfid_desc,g_fmmv_m.fmmvcnfdt
 
   CASE g_fmmv_m.fmmvstus
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
      WHEN "X"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_fmmv_m.fmmvstus
            
            WHEN "Y"
               HIDE OPTION "confirmed"
            WHEN "N"
               HIDE OPTION "unconfirmed"
            WHEN "X"
               HIDE OPTION "invalid"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      CALL cl_set_act_visible("signing,withdraw",FALSE)
      CALL cl_set_act_visible("unconfirmed,invalid,confirmed",TRUE)
      CALL cl_set_act_visible("closed",FALSE)

      CASE g_fmmv_m.fmmvstus
         WHEN "N"
            CALL cl_set_act_visible("unconfirmed",FALSE)
            #需提交至BPM時，則顯示「提交」功能並隱藏「確認」功能
            IF cl_bpm_chk() THEN
                CALL cl_set_act_visible("signing",TRUE)
                CALL cl_set_act_visible("confirmed",FALSE)
            END IF
         WHEN "R"   #保留修改的功能(如作廢)，隱藏其他應用功能(如: 確認、未確認、留置、過帳…)
            CALL cl_set_act_visible("confirmed,unconfirmed",FALSE)
         WHEN "D"   #保留修改的功能(如作廢)，隱藏其他應用功能(如: 確認、未確認、留置、過帳…)
            CALL cl_set_act_visible("confirmed,unconfirmed",FALSE)
         WHEN "X"
            CALL s_transaction_end('N','0')        #150401-00001#13
            RETURN
         WHEN "Y"
            CALL cl_set_act_visible("invalid,confirmed",FALSE)
         WHEN "W"    #只能顯示抽單;其餘應用功能皆隱藏
             CALL cl_set_act_visible("withdraw",TRUE)
             CALL cl_set_act_visible("unconfirmed,invalid,confirmed",FALSE)
         WHEN "A"     #只能顯示確認; 其餘應用功能皆隱藏
             CALL cl_set_act_visible("confirmed ",TRUE)
             CALL cl_set_act_visible("unconfirmed,invalid",FALSE)
      END CASE
      #end add-point
      
      
	  
      ON ACTION confirmed
         IF cl_auth_chk_act("confirmed") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.confirmed"
            
            #end add-point
         END IF
         EXIT MENU
      ON ACTION unconfirmed
         IF cl_auth_chk_act("unconfirmed") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.unconfirmed"
            
            #end add-point
         END IF
         EXIT MENU
      ON ACTION invalid
         IF cl_auth_chk_act("invalid") THEN
            LET lc_state = "X"
            #add-point:action控制 name="statechange.invalid"
            
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "Y" 
      AND lc_state <> "N"
      AND lc_state <> "X"
      ) OR 
      g_fmmv_m.fmmvstus = lc_state OR cl_null(lc_state) THEN
      CLOSE afmt580_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   #確認
   IF lc_state = 'Y' THEN
      IF g_fmmv_m.fmmv007 = '2' AND cl_null(g_fmmv_m.fmmv010) THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'ais-00233'
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()
         CALL s_transaction_end('N','0')        #150401-00001#13
         RETURN                    
      END IF   
          
      IF NOT cl_ask_confirm('aim-00108') THEN   #是否執行確認？
         CALL s_transaction_end('N','0')       #150401-00001#13
         RETURN
      ELSE  
         IF NOT cl_null(g_fmmv_m.fmmv010) THEN
            CALL afmt580_ins_fmmj() RETURNING g_sub_success,g_fmmv_m.fmmv010
            IF NOT g_sub_success THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = g_fmmv_m.fmmv010
               LET g_errparam.extend = "ins fmmj_t"
               LET g_errparam.popup = TRUE
               CALL cl_err()
               CALL s_transaction_end('N','0')  #150401-00001#13
               RETURN
            END IF      
         END IF            
         LET g_fmmv_m.fmmvcnfdt = g_today
         LET g_fmmv_m.fmmvcnfid = g_user
         UPDATE fmmv_t
         SET (fmmvcnfdt,fmmvcnfid,fmmv010)
           = (g_fmmv_m.fmmvcnfdt ,g_fmmv_m.fmmvcnfid,g_fmmv_m.fmmv010)
          WHERE fmmvent = g_enterprise AND fmmvdocno = g_fmmv_m.fmmvdocno
          CALL afmt580_conf_upd('+') RETURNING g_sub_success,g_errno #151117-00001#6 
          IF NOT g_sub_success THEN
             INITIALIZE g_errparam TO NULL
             LET g_errparam.code = g_errno
             LET g_errparam.extend = ""
             LET g_errparam.popup = TRUE
             CALL cl_err()
             CALL s_transaction_end('N','0')  #150401-00001#13
             RETURN
         END IF      
      END IF
   END IF
   #取消確認
   IF lc_state = 'N' THEN
      LET l_count = 0
      SELECT COUNT(fmmx002) INTO l_count
        FROM fmmw_t,fmmx_t
       WHERE fmmwent = fmmxent
         AND fmmwdocno = fmmxdocno
         AND fmmwstus <> 'X'
         AND fmmwent = g_enterprise
         AND fmmx002 = g_fmmv_m.fmmvdocno
      IF cl_null(l_count)THEN LET l_count = 0 END IF
      IF l_count > 0 THEN
         INITIALIZE g_errparam.* TO NULL
         LET g_errparam.code = 'afm-00124'
         LET g_errparam.extend = g_fmmv_m.fmmvdocno
         LET g_errparam.popup = TRUE
         CALL cl_err()
         CALL s_transaction_end('N','0')        #150401-00001#13
         RETURN
      END IF                      
      IF NOT cl_ask_confirm('aim-00110') THEN   #是否執行取消確認？
         CALL s_transaction_end('N','0')        #150401-00001#13
         RETURN
      ELSE    
         IF NOT cl_null(g_fmmv_m.fmmv010) THEN 
            #不可跳號則不刪除改作廢
            LET g_prog = 'afmt530'
            IF NOT s_aooi200_fin_del_docno(g_glaa.glaald,g_fmmv_m.fmmv010,g_fmmv_m.fmmvdocdt) THEN
               LET g_prog = 'afmt580'
               UPDATE fmmj_t SET fmmjstus = 'X'
                WHERE fmmjent = g_enterprise
                  AND fmmjdocno = g_fmmv_m.fmmv010
            ELSE      
               DELETE FROM fmmj_t
                WHERE fmmjent = g_enterprise
                  AND fmmjdocno = g_fmmv_m.fmmv010               
              #重複刪除兩次最大號，導致錯誤
#               LET g_prog = 'afmt530'
#               CALL s_aooi200_fin_del_docno(g_glaa.glaald,g_fmmv_m.fmmv010,g_fmmv_m.fmmvdocdt)
#                   RETURNING g_sub_success
#               LET g_prog = 'afmt580'
#               IF NOT g_sub_success THEN
#                  INITIALIZE g_errparam TO NULL
#                  LET g_errparam.code = g_fmmv_m.fmmv010
#                  LET g_errparam.extend = "del fmmj_t"
#                  LET g_errparam.popup = TRUE
#                  CALL cl_err()
#                  CALL s_transaction_end('N','0')        #150401-00001#13
#                  RETURN 
#               END IF    
            END IF  
            #取單別
            LET g_prog = 'afmt530'
            CALL s_aooi200_fin_get_slip(g_fmmv_m.fmmv010) RETURNING g_sub_success,g_fmmv_m.fmmv010
            LET g_prog = 'afmt580'
            UPDATE fmmv_t SET fmmv010 = g_fmmv_m.fmmv010 
             WHERE fmmvent = g_enterprise 
               AND fmmvdocno = g_fmmv_m.fmmvdocno
            IF SQLCA.SQLcode OR SQLCA.SQLERRD[3]=0 THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = SQLCA.sqlcode
               LET g_errparam.extend = g_fmmv_m.fmmv010 CLIPPED,"/upd fmmv_t"
               LET g_errparam.popup = TRUE
               CALL cl_err()
               CALL s_transaction_end('N','0')        #150401-00001#13
               RETURN            
            END IF                     
         END IF         
         LET g_fmmv_m.fmmvcnfdt = ''
         LET g_fmmv_m.fmmvcnfid = ''
         UPDATE fmmv_t
          SET (fmmvcnfdt,fmmvcnfid)
            = (g_fmmv_m.fmmvcnfdt ,g_fmmv_m.fmmvcnfid)
           WHERE fmmvent = g_enterprise AND fmmvdocno = g_fmmv_m.fmmvdocno
           #151117-00001#6  ---s---
           CALL afmt580_conf_upd('-') RETURNING g_sub_success,g_errno
           IF NOT g_sub_success THEN
              INITIALIZE g_errparam TO NULL
              LET g_errparam.code = g_errno
              LET g_errparam.extend = ""
              LET g_errparam.popup = TRUE
              CALL cl_err()
              CALL s_transaction_end('N','0')  
              RETURN
           END IF                 
           #151117-00001#6 ---e---
      END IF
   END IF
   #作廢
   IF lc_state = 'X' THEN
      #詢問
      IF NOT cl_ask_confirm('aim-00109') THEN   #是否執作廢?
         CALL s_transaction_end('N','0')        #150401-00001#13
         RETURN
      END IF
   END IF
   #end add-point
   
   LET g_fmmv_m.fmmvmodid = g_user
   LET g_fmmv_m.fmmvmoddt = cl_get_current()
   LET g_fmmv_m.fmmvstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE fmmv_t 
      SET (fmmvstus,fmmvmodid,fmmvmoddt) 
        = (g_fmmv_m.fmmvstus,g_fmmv_m.fmmvmodid,g_fmmv_m.fmmvmoddt)     
    WHERE fmmvent = g_enterprise AND fmmvdocno = g_fmmv_m.fmmvdocno
 
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE afmt580_master_referesh USING g_fmmv_m.fmmvdocno INTO g_fmmv_m.fmmvsite,g_fmmv_m.fmmv001, 
          g_fmmv_m.fmmv002,g_fmmv_m.fmmvdocdt,g_fmmv_m.fmmv003,g_fmmv_m.fmmvdocno,g_fmmv_m.fmmv015,g_fmmv_m.fmmv004, 
          g_fmmv_m.fmmv013,g_fmmv_m.fmmv007,g_fmmv_m.fmmv012,g_fmmv_m.fmmv005,g_fmmv_m.fmmv011,g_fmmv_m.fmmv006, 
          g_fmmv_m.fmmv014,g_fmmv_m.fmmv010,g_fmmv_m.fmmv016,g_fmmv_m.fmmv017,g_fmmv_m.fmmv018,g_fmmv_m.fmmv019, 
          g_fmmv_m.fmmv020,g_fmmv_m.fmmvstus,g_fmmv_m.fmmvownid,g_fmmv_m.fmmvowndp,g_fmmv_m.fmmvcrtid, 
          g_fmmv_m.fmmvcrtdp,g_fmmv_m.fmmvcrtdt,g_fmmv_m.fmmvmodid,g_fmmv_m.fmmvmoddt,g_fmmv_m.fmmvcnfid, 
          g_fmmv_m.fmmvcnfdt,g_fmmv_m.fmmvownid_desc,g_fmmv_m.fmmvowndp_desc,g_fmmv_m.fmmvcrtid_desc, 
          g_fmmv_m.fmmvcrtdp_desc,g_fmmv_m.fmmvmodid_desc,g_fmmv_m.fmmvcnfid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_fmmv_m.fmmvsite,g_fmmv_m.fmmvsite_desc,g_fmmv_m.fmmv001,g_fmmv_m.fmmv002,g_fmmv_m.fmmvdocdt, 
          g_fmmv_m.b_fmmv0012,g_fmmv_m.fmmv003,g_fmmv_m.fmmvdocno,g_fmmv_m.fmmv015,g_fmmv_m.fmmv004, 
          g_fmmv_m.fmmv013,g_fmmv_m.fmmv013_desc,g_fmmv_m.fmmv007,g_fmmv_m.fmmv012,g_fmmv_m.fmmv012_desc, 
          g_fmmv_m.fmmv005,g_fmmv_m.fmmv011,g_fmmv_m.fmmv011_desc,g_fmmv_m.fmmv006,g_fmmv_m.fmmv014, 
          g_fmmv_m.fmmv010,g_fmmv_m.fmmv016,g_fmmv_m.fmmv017,g_fmmv_m.fmmv018,g_fmmv_m.fmmv019,g_fmmv_m.fmmv020, 
          g_fmmv_m.fmmvstus,g_fmmv_m.fmmvownid,g_fmmv_m.fmmvownid_desc,g_fmmv_m.fmmvowndp,g_fmmv_m.fmmvowndp_desc, 
          g_fmmv_m.fmmvcrtid,g_fmmv_m.fmmvcrtid_desc,g_fmmv_m.fmmvcrtdp,g_fmmv_m.fmmvcrtdp_desc,g_fmmv_m.fmmvcrtdt, 
          g_fmmv_m.fmmvmodid,g_fmmv_m.fmmvmodid_desc,g_fmmv_m.fmmvmoddt,g_fmmv_m.fmmvcnfid,g_fmmv_m.fmmvcnfid_desc, 
          g_fmmv_m.fmmvcnfdt
   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
   #150820-00011#6---s
   LET g_success = 'Y'
   IF lc_state = 'Y' THEN 
      IF g_fmmv_m.fmmv007 MATCHES '[13]' THEN
         CALL afmt580_nmbc_insert()
         IF g_success = 'Y' THEN
            CALL afmt580_glbc_insert()
            IF g_success = 'Y' THEN
               CALL s_transaction_end('Y','1')
            ELSE
               CALL s_transaction_end('N','1')
               RETURN
            END IF 
         ELSE
            CALL s_transaction_end('N','1')
            RETURN
         END IF
      END IF
      #150924-00006#3-----s
      #維護費用則新增銀存收支異動檔
      LET l_cnt1 = 0
      SELECT COUNT(*) INTO l_cnt1 FROM fmnd_t 
       WHERE fmndent=g_enterprise AND fmnddocno= g_fmmv_m.fmmvdocno 
         AND fmnd008 IS NOT NULL   
         
      IF l_cnt1 > 0 THEN
         CALL afmt580_01_nmbc_insert()
         IF g_success = 'Y' THEN
            CALL afmt580_01_glbc_insert()
            IF g_success = 'Y' THEN
               CALL s_transaction_end('Y','1')
            ELSE
               CALL s_transaction_end('N','1')
               RETURN
            END IF 
         ELSE
            CALL s_transaction_end('N','1')
            RETURN
         END IF
      END IF      
      #150924-00006#3-----e
   END IF
   IF lc_state = 'N' THEN
      #150924-00006#3-----s
      LET l_cnt1 = 0
      SELECT COUNT(*) INTO l_cnt1 FROM fmnd_t 
       WHERE fmndent=g_enterprise AND fmnddocno= g_fmmv_m.fmmvdocno
         AND fmnd008 IS NOT NULL
      #150924-00006#3-----e
      IF g_fmmv_m.fmmv007 MATCHES '[13]' OR l_cnt1 > 0 THEN #150924-00006#3 add l_cnt1
         CALL afmt580_nmbc_delete()      
         IF g_success = 'Y' THEN
            CALL afmt580_glbc_delete()
            IF g_success = 'Y' THEN
               CALL s_transaction_end('Y','1')
            ELSE
               CALL s_transaction_end('N','1')
               RETURN
            END IF
         ELSE
            CALL s_transaction_end('N','1')
            RETURN
         END IF
      END IF
   END IF
   #150820-00011#6---e
   #end add-point
 
   #add-point:statechange段結束前 name="statechange.after"
   
   #end add-point  
 
   CLOSE afmt580_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL afmt580_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION
 
 
 
 
