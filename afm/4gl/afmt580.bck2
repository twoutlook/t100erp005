################################################################################
# Descriptions...: 購買單號預設單頭資料
# Memo...........:
# Usage..........:
# Input parameter:
# Date & Author..: 151027 By albireo
# Modify.........:
################################################################################
PRIVATE FUNCTION afmt580_fmmv001_carry()
DEFINE l_sdat   LIKE type_t.dat
DEFINE l_edat   LIKE type_t.dat
DEFINE l_fmmj  RECORD
              fmmj021   LIKE fmmj_t.fmmj021,
              fmmj024   LIKE fmmj_t.fmmj024,
              fmmjdocdt LIKE fmmj_t.fmmjdocdt,
              fmmj006   LIKE fmmj_t.fmmj006,
              fmmj023   LIKE fmmj_t.fmmj023
               END RECORD

#2，在錄入購買單號後帶出利息起止日期：
#起期日期：相同投資單號的前一張收息單（最大截止日期）的截止日期+1，不存在于取投資單的fmmj024,再沒有取fmmjdocdt
# 截止日期：收息日期fmmvdocdt與投資fmmj021(不為空時）較小的日期
#3，幣別帶預設值 fmmj006
#4，投資單的利息轉本金fmmj023<>Y時，收息方式帶預設值=3:銀行帳戶收取，可修改，且不可=2:利息自動轉本金； 投資購買單號fmmv010灰掉，不可維護
#5，金額fmmv005，帶預設值，方式同afmt590的fmmy006

INITIALIZE l_fmmj.* TO NULL
SELECT fmmj021,fmmj024,fmmjdocdt,fmmj006,fmmj023
  INTO l_fmmj.*
  FROM fmmj_t
 WHERE fmmjent = g_enterprise
   AND fmmjdocno = g_fmmv_m.fmmv001

#2-----------s
#起
LET l_sdat = NULL
SELECT MAX(fmmv003)+1 INTO l_sdat FROM l_dat
 WHERE fmmvent = g_enterprise
  AND fmmv001 = g_fmmv_m.fmmv001
IF cl_null(l_sdat)THEN
   IF NOT cl_null(l_fmmj.fmmj024)THEN
      LET l_sdat = l_fmmj.fmmj024
   ELSE
      LET l_sdat = l_fmmj.fmmjdocdt
   END IF
END IF

#止
LET l_edat = NULL
IF NOT cl_null(l_fmmj.fmmj021)THEN
   IF l_fmmj.fmmj021 < g_fmmv_m.fmmvdocdt THEN
      LET l_edat = l_fmmj.fmmj021
   ELSE
      LET l_edat = g_fmmv_m.fmmvdocdt
   END IF
ELSE
   LET l_edat = g_fmmv_m.fmmvdocdt
END IF

IF l_edat < l_sdat THEN
   LET l_edat = l_sdat
END IF

LET g_fmmv_m.fmmv002 = l_sdat
LET g_fmmv_m.fmmv003 = l_edat
DISPLAY BY NAME g_fmmv_m.fmmv002,g_fmmv_m.fmmv003
#2-----------e

#3-----------s
LET g_fmmv_m.fmmv004 = l_fmmj.fmmj006
DISPLAY BY NAME g_fmmv_m.fmmv004
#取得投資組織所屬法人主帳套之本幣對交易幣之匯率                       
IF NOT cl_null(g_fmmv_m.fmmvsite) AND NOT cl_null(g_fmmv_m.fmmv004) AND NOT cl_null(g_fmmv_m.fmmvdocdt) THEN
   CALL s_aooi160_get_exrate('2',g_glaa.glaald,g_fmmv_m.fmmvdocdt,g_fmmv_m.fmmv004,g_glaa.glaa001,0,g_glaa.glaa025)
   RETURNING g_fmmv_m.fmmv006
   DISPLAY BY NAME g_fmmv_m.fmmv006
END IF
#------------e

#4-----------s
LET g_fmmv_m.fmmv007 = NULL
IF l_fmmj.fmmj023 = 'Y' THEN
ELSE
   LET g_fmmv_m.fmmv007 = '3'
END IF

CASE
  WHEN g_fmmv_m.fmmv007 = '1' 
     CALL cl_set_comp_required("fmmv011,fmmv012,fmmv013",TRUE)
     IF NOT cl_null(g_fmmv_m.fmmvsite) AND NOT cl_null(g_fmmv_m.fmmv001) THEN
        SELECT fmmf003 INTO g_fmmv_m.fmmv013
          FROM fmmj_t,fmmf_t
         WHERE fmmjent = fmmfent AND fmmj002 = fmmf002 AND fmmf001 = g_fmmv_m.fmmvsite
           AND fmmjent =g_enterprise AND fmmjdocno = g_fmmv_m.fmmv001
        CALL s_desc_get_nmas002_desc(g_fmmv_m.fmmv013) RETURNING g_fmmv_m.fmmv013_desc                       
     END IF
     LET g_fmmv_m.fmmv010 = ''
     CALL cl_set_comp_required("fmmv011,fmmv012,fmmv013",TRUE)
     CALL cl_set_comp_entry("fmmv010",FALSE)
  WHEN g_fmmv_m.fmmv007 = '2' 
     CALL cl_set_comp_required("fmmv011,fmmv012,fmmv013",FALSE)
     CALL cl_set_comp_entry("fmmv010",TRUE)
  WHEN g_fmmv_m.fmmv007 = '3'
     CALL cl_set_comp_required("fmmv011,fmmv012,fmmv013",TRUE)
     CALL cl_set_comp_entry("fmmv010",FALSE)
     LET g_fmmv_m.fmmv010 = ''
END CASE 

#------------e

#5-----
CALL afmt580_get_interest()

END FUNCTION
################################################################################
# Descriptions...: 取利息金額預設值
# Date & Author..: 151027 by albireo
# Modify.........:
################################################################################
PRIVATE FUNCTION afmt580_get_interest()
DEFINE l_fmmjdocdt      LIKE fmmj_t.fmmjdocdt
DEFINE l_fmmj005        LIKE fmmj_t.fmmj005
DEFINE l_fmmj006        LIKE fmmj_t.fmmj006
DEFINE l_fmmj017        LIKE fmmj_t.fmmj017
DEFINE l_fmmj018        LIKE fmmj_t.fmmj018
DEFINE l_fmmj022        LIKE fmmj_t.fmmj022
DEFINE l_fmmv005        LIKE fmmv_t.fmmv005
DEFINE l_interest_sum   LIKE fmmy_t.fmmy003  #全部利息
DEFINE l_interest_part  LIKE fmmy_t.fmmy003  #部分利息
DEFINE l_days           LIKE type_t.num5     #實際占用天數
DEFINE l_glaald         LIKE glaa_t.glaald
DEFINE l_ooef017        LIKE ooef_t.ooef017

   #afmt580帶利息收入的預設值，邏輯如下：
   #最後一期利息=全部應收到利息收入
   #  全部應收到利息收入=總面值fmmj017*票面年利率fmmj018/計算天數fmmj022*實際佔用天數
   #  實際佔用天數=企日-起日+1
   IF cl_null(g_fmmv_m.fmmv001) OR cl_null(g_fmmv_m.fmmv002) OR cl_null(g_fmmv_m.fmmv003) THEN
      RETURN
   END IF
  
   SELECT ooef017 INTO l_ooef017
     FROM ooef_t
    WHERE ooefent = g_enterprise AND ooef001 = g_fmmv_m.fmmvsite
   CALL s_fin_get_major_ld(l_ooef017) RETURNING l_glaald
   
   SELECT fmmj005,fmmj006,fmmj017,fmmj018,fmmj022,
          fmmjdocdt
     INTO l_fmmj005,l_fmmj006,l_fmmj017,l_fmmj018,l_fmmj022,
          l_fmmjdocdt
     FROM fmmj_t
    WHERE fmmjent = g_enterprise AND fmmjdocno = g_fmmv_m.fmmv001
   
   #全部應收到利息收入=總面值fmmj017 *票面年利率fmmj018/計算天數fmmj022*實際佔用天數
   LET l_days = g_fmmv_m.fmmv003 - g_fmmv_m.fmmv002 + 1
   LET l_interest_sum = l_fmmj017 * (l_fmmj018/100) / l_fmmj022 * l_days
   CALL s_curr_round_ld(1,l_glaald,l_fmmj006,l_interest_sum,2) RETURNING g_sub_success,g_errno,l_interest_sum
#   #收息的币别在收息单没控卡与购买的币别一致。当初规划是在套期保值业务里处理这种不一致的状况。所以这时就当币别一致好了
#   SELECT SUM(fmmv005) INTO l_fmmv005
#     FROM fmmv_t
#    WHERE fmmvent = g_enterprise AND fmmv001 = g_fmmy_m.fmmy001
#      AND fmmvdocdt <= g_fmmy_m.fmmydocdt AND fmmvstus = 'Y' 
#   IF cl_null(l_fmmv005) THEN LET l_fmmv005 = 0 END IF
#   LET l_interest_part = l_fmmv005/l_fmmj005*g_fmmy_m.fmmy003
#   IF cl_null(l_interest_part) THEN LET l_interest_part = 0 END IF
#                                           #已收到利息收入=SUM(fmmv005)/購買數量fmmj005*出售數量fmmy003
   LET g_fmmv_m.fmmv005 = l_interest_sum 
   
   CALL s_curr_round_ld(1,l_glaald,l_fmmj006,g_fmmv_m.fmmv005,2) RETURNING g_sub_success,g_errno,g_fmmv_m.fmmv005
   IF g_fmmv_m.fmmv005 < 0 THEN
      LET g_fmmv_m.fmmv005 = 0
   END IF
   DISPLAY BY NAME g_fmmv_m.fmmv005
END FUNCTION

################################################################################
# Descriptions...: 確認取消確認回寫fmmt及產生fmmn
# Memo...........:
# Usage..........: CALL afmt580_conf_upd(p_type)
# Date & Author..: 2015/11/20 By Hans
# Modify.........:
################################################################################
PRIVATE FUNCTION afmt580_conf_upd(p_type)
DEFINE p_type       LIKE type_t.chr1   #+-
DEFINE r_success    LIKE type_t.num5
DEFINE l_sql        STRING
DEFINE l_fmmn       RECORD LIKE fmmn_t.*
DEFINE l_fmmt016    LIKE fmmt_t.fmmt016
DEFINE r_errno         LIKE gzze_t.gzze001


  LET r_success = TRUE LET r_errno = NULL
  
  CASE      
     WHEN g_fmmv_m.fmmv015 = '3' 
        #afmt560 投資收益- 已收原幣金額 
        SELECT SUM(fmmt012-fmmt018),SUM(fmmt014-fmmt019)
          INTO g_fmmv_m.fmmv016,g_fmmv_m.fmmv017
          FROM fmmt_t,fmms_t
         WHERE fmmtent = fmmsent AND fmmtent = g_enterprise 
           AND fmmt012 > fmmt018 
           AND fmmtdocno = fmmsdocno AND fmmt002 = g_fmmv_m.fmmv001
           AND fmmsstus = 'Y' AND fmmt017 = g_fmmv_m.fmmv015
        IF cl_null(g_fmmv_m.fmmv016) THEN LET g_fmmv_m.fmmv016 = 0 END IF
        IF cl_null(g_fmmv_m.fmmv017) THEN LET g_fmmv_m.fmmv017 = 0 END IF
        LET g_fmmv_m.fmmv018 = g_fmmv_m.fmmv005 - g_fmmv_m.fmmv016
        LET g_fmmv_m.fmmv019 = g_fmmv_m.fmmv018 * g_fmmv_m.fmmv006
        CALL s_curr_round_ld('1',g_glaa.glaald,g_glaa.glaa001,g_fmmv_m.fmmv019 ,2) 
           RETURNING g_sub_success,g_errno,g_fmmv_m.fmmv019 
        LET g_fmmv_m.fmmv020 = g_fmmv_m.fmmv014 - g_fmmv_m.fmmv017 - g_fmmv_m.fmmv019
        CALL s_curr_round_ld('1',g_glaa.glaald,g_glaa.glaa001,g_fmmv_m.fmmv020,2) 
           RETURNING g_sub_success,g_errno,g_fmmv_m.fmmv020
        
     OTHERWISE
        SELECT SUM(fmmt016) INTO l_fmmt016 #本幣投資收益
          FROM fmmt_t
         WHERE fmmtent = g_enterprise
           AND fmmt002 = g_fmmv_m.fmmv001
           AND fmmt017 = g_fmmv_m.fmmv015
        IF cl_null(l_fmmt016)THEN LET l_fmmt016 = 0 END IF
        LET g_fmmv_m.fmmv016 = g_fmmv_m.fmmv005
        LET g_fmmv_m.fmmv017 = l_fmmt016        #本期計提金額
        LET g_fmmv_m.fmmv018 = '0'
        LET g_fmmv_m.fmmv019 = '0'
        LET g_fmmv_m.fmmv020 = g_fmmv_m.fmmv014 - g_fmmv_m.fmmv017 - g_fmmv_m.fmmv019  #收息本幣 - 沖銷利息本幣
        CALL s_curr_round_ld('1',g_glaa.glaald,g_glaa.glaa001,g_fmmv_m.fmmv020,2) 
           RETURNING g_sub_success,g_errno,g_fmmv_m.fmmv020                          
  END CASE
  IF p_type = '+' THEN
     UPDATE fmmv_t SET
            fmmv016 = g_fmmv_m.fmmv016,fmmv017 = g_fmmv_m.fmmv017,fmmv018 = g_fmmv_m.fmmv018,
            fmmv019 = g_fmmv_m.fmmv019,fmmv020 = g_fmmv_m.fmmv020
      WHERE fmmvent = g_enterprise
        AND fmmvdocno = g_fmmv_m.fmmvdocno
        
     CALL s_afm_ins_fmmt('+',g_fmmv_m.fmmv015,g_fmmv_m.fmmv001,g_fmmv_m.fmmvdocno,g_fmmv_m.fmmv016,g_fmmv_m.fmmv017)
        RETURNING r_success,r_errno
   ELSE
     UPDATE fmmv_t SET
            fmmv016 = '',fmmv017='',fmmv018='',
            fmmv019 = '',fmmv020='' 
      WHERE fmmvent = g_enterprise
        AND fmmvdocno = g_fmmv_m.fmmvdocno   
      CALL s_afm_ins_fmmt('-',g_fmmv_m.fmmv015,g_fmmv_m.fmmv001,g_fmmv_m.fmmvdocno,g_fmmv_m.fmmv016,g_fmmv_m.fmmv017)
         RETURNING r_success,r_errno        
   END IF
   
   RETURN r_success,r_errno
END FUNCTION

 
