   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON fmmvdocno
                          FROM s_browse[1].b_fmmvdocno
 
         BEFORE CONSTRUCT
               DISPLAY afmt580_filter_parser('fmmvdocno') TO s_browse[1].b_fmmvdocno
      
