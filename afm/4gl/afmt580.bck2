################################################################################
# Descriptions...: 填充累計計息狀態
# Memo...........:
# Usage..........: CALL afmt580_b_fill2()
# Date & Author..: 2015/05/04 By Hans
# Modify.........:
################################################################################
PRIVATE FUNCTION afmt580_b_fill2()
DEFINE l_sql STRING
DEFINE l_idx         LIKE type_t.num10
DEFINE l_year        LIKE type_t.num10
DEFINE l_mon         LIKE type_t.num10
DEFINE l_date        LIKE type_t.num10
DEFINE l_fmmvdocno   LIKE fmmv_t.fmmvdocno
DEFINE l_date2       LIKE fmmv_t.fmmvdocdt
DEFINE l_fmmv002     LIKE fmmv_t.fmmv002
DEFINE l_fmmv003     LIKE fmmv_t.fmmv003
DEFINE l_fmmv005     LIKE fmmv_t.fmmv005
DEFINE l_glaa003     LIKE glaa_t.glaa003

   LET l_year = YEAR(g_fmmv_m.fmmvdocdt)
   LET l_mon  = MONTH(g_fmmv_m.fmmvdocdt)
   LET l_date = l_year*12 + l_mon  #將日期轉換成 年*12+月
   CALL s_ld_sel_glaa(g_glaa.glaald,'glaa003') RETURNING g_sub_success,l_glaa003
   DELETE FROM afmt580_tmp
   
   #計提
   LET l_sql = " INSERT INTO afmt580_tmp                                                ",
               " SELECT fmmsdocno,fmms001 ,fmms002,fmmt005,fmmt006,fmmt010, 0,0,0,0,'1' ",                                                  
               "    FROM fmms_t,fmmt_t                                                  ",
               "   WHERE fmmsent = fmmtent  AND fmmsent = '",g_enterprise,"'            ",
               "     AND fmmtdocno = fmmsdocno                                          ",
               "     AND fmmt002 = '",g_fmmv_m.fmmv001,"'                               ",
               "     AND fmmsstus = 'Y'                                                 ",
               "     AND (fmms001*12+fmms002) <= '",l_date,"'                           "
               
   PREPARE  afmt580_prep01 FROM l_sql
   EXECUTE  afmt580_prep01 
   #收息
   LET l_sql = "  SELECT fmmvdocno,fmmvdocdt,fmmv002,fmmv003,fmmv005             ",  
               "    FROM fmmv_t                                                  ",
               "   WHERE fmmvent = '",g_enterprise,"'                            ",
               "     AND fmmv001 = '",g_fmmv_m.fmmv001,"'                        ",
               "     AND fmmvstus = 'Y'                                          ",
               "   ORDER BY fmmvdocdt                                            "               
                                                   
   PREPARE afmt580_prep02 FROM l_sql
   DECLARE afmt580_curs02 CURSOR FOR afmt580_prep02
  
   FOREACH afmt580_curs02 INTO l_fmmvdocno,l_date2,l_fmmv002,l_fmmv003,l_fmmv005
       IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam.* TO NULL
         LET g_errparam.code = SQLCA.SQLCODE
         LET g_errparam.extend = 'FOREACH:fmmv_t'
         CALL cl_err()
      END IF
      CALL s_fin_date_get_period_value(l_glaa003,g_glaa.glaald,l_date2)
         RETURNING g_sub_success,l_year,l_mon   
      INSERT INTO afmt580_tmp
      VALUES (l_fmmvdocno,l_year,l_mon,l_fmmv002,l_fmmv003,0,0,l_fmmv005,0,0,'2')               
   END FOREACH   
   
   LET l_sql = " SELECT * FROM afmt580_tmp ORDER BY l_flag,fmms001,fmms002" 
   PREPARE afmt580_prep03 FROM l_sql
   DECLARE afmt580_CURS03 CURSOR FOR afmt580_prep03 
   CALL g_memo_afmt580.clear()   
   LET l_idx = 1   
   FOREACH afmt580_CURS03 INTO g_memo_afmt580[l_idx].*
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam.* TO NULL
         LET g_errparam.code = SQLCA.SQLCODE
         LET g_errparam.extend = 'FOREACH:fmmv_t'
         CALL cl_err()
      END IF
      
      #累計計提金額/累計收息金額
      IF l_idx = 1 THEN 
         LET g_memo_afmt580[l_idx].l_fmmt0102 = g_memo_afmt580[l_idx].l_fmmt0101  
         LET g_memo_afmt580[l_idx].l_fmmt0104 = g_memo_afmt580[l_idx].l_fmmt0103            
      ELSE
         LET g_memo_afmt580[l_idx].l_fmmt0102 = g_memo_afmt580[l_idx].l_fmmt0102 + g_memo_afmt580[l_idx-1].l_fmmt0101
         LET g_memo_afmt580[l_idx].l_fmmt0104 = g_memo_afmt580[l_idx].l_fmmt0103 + g_memo_afmt580[l_idx-1].l_fmmt0104         
      END IF
      
      LET g_memo_afmt580[l_idx].l_fmmt0105 = g_memo_afmt580[l_idx].l_fmmt0102 - g_memo_afmt580[l_idx].l_fmmt0104
   LET l_idx = l_idx + 1   
   
   END FOREACH
   CALL g_memo_afmt580.deleteElement(g_memo_afmt580.getLength())
   
   DISPLAY ARRAY g_memo_afmt580 TO s_detail1.* ATTRIBUTES(COUNT=g_rec_b2)
      BEFORE DISPLAY
         EXIT DISPLAY
   END DISPLAY




END FUNCTION

################################################################################
# Descriptions...: 銀行收支單號/項次檢查
# Memo...........:
# Usage..........: CALL afmt580_fmmv008_chk(p_nmbadocno,p_nmbbseq)
# Date & Author..: 2015/05/06 By Hans
# Modify.........:
################################################################################
PUBLIC FUNCTION afmt580_fmmv008_chk(p_nmbadocno,p_nmbbseq)
DEFINE l_nmbasuts      LIKE nmba_t.nmbastus
DEFINE l_count         LIKE type_t.num5
DEFINE r_success       LIKE type_t.chr1
DEFINE r_errno         LIKE gzze_t.gzze001
DEFINE p_nmbadocno     LIKE nmba_t.nmbadocno
DEFINE p_nmbbseq       LIKE nmbb_t.nmbbseq

   LET r_success = TRUE
   LET r_errno = NULL   

   SELECT nmbastus  INTO l_nmbasuts
     FROM nmba_t
    WHERE nmbaent = g_enterprise
      AND nmbacomp = g_glaa.glaacomp
      AND nmbadocno = p_nmbadocno
      
     CASE
      WHEN SQLCA.SQLCODE = 100   LET r_errno = 'anm-00058' LET r_success = FALSE
      WHEN l_nmbasuts = 'N'      LET r_errno = 'abm-00079' LET r_success = FALSE 
   END CASE
   
   IF NOT cl_null(p_nmbbseq) AND NOT cl_null(p_nmbadocno) THEN 
      SELECT COUNT(*)  INTO l_count
        FROM nmbb_t
       WHERE nmbbent = g_enterprise
         AND nmbbdocno = p_nmbadocno
         AND nmbbcomp  = g_glaa.glaacomp
         AND nmbbseq   = p_nmbbseq
         AND nmbb004   = g_fmmv_m.fmmv004
      
      IF cl_null(l_count) THEN LET l_count = 0 END IF
      IF l_count = 0 THEN LET r_errno = 'anm-00193' LET r_success = FALSE END IF
     
   END IF       
      
   RETURN r_success,r_errno
END FUNCTION

