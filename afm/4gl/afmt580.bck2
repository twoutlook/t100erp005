   #add-point:modify段define(客製用) name="modify.define_customerization"
   
   #end add-point
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   
   #end add-point
   
   #add-point:Function前置處理 name="modify.pre_function"
   
   #end add-point
   
   #先確定key值無遺漏
   IF g_fmmv_m.fmmvdocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_fmmvdocno_t = g_fmmv_m.fmmvdocno
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN afmt580_cl USING g_enterprise,g_fmmv_m.fmmvdocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN afmt580_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CLOSE afmt580_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE afmt580_master_referesh USING g_fmmv_m.fmmvdocno INTO g_fmmv_m.fmmvsite,g_fmmv_m.fmmv001, 
       g_fmmv_m.fmmv002,g_fmmv_m.fmmvdocdt,g_fmmv_m.fmmv003,g_fmmv_m.fmmvdocno,g_fmmv_m.fmmv015,g_fmmv_m.fmmv004, 
       g_fmmv_m.fmmv013,g_fmmv_m.fmmv007,g_fmmv_m.fmmv012,g_fmmv_m.fmmv005,g_fmmv_m.fmmv011,g_fmmv_m.fmmv006, 
       g_fmmv_m.fmmv014,g_fmmv_m.fmmv010,g_fmmv_m.fmmv016,g_fmmv_m.fmmv017,g_fmmv_m.fmmv018,g_fmmv_m.fmmv019, 
       g_fmmv_m.fmmv020,g_fmmv_m.fmmvstus,g_fmmv_m.fmmvownid,g_fmmv_m.fmmvowndp,g_fmmv_m.fmmvcrtid,g_fmmv_m.fmmvcrtdp, 
       g_fmmv_m.fmmvcrtdt,g_fmmv_m.fmmvmodid,g_fmmv_m.fmmvmoddt,g_fmmv_m.fmmvcnfid,g_fmmv_m.fmmvcnfdt, 
       g_fmmv_m.fmmvownid_desc,g_fmmv_m.fmmvowndp_desc,g_fmmv_m.fmmvcrtid_desc,g_fmmv_m.fmmvcrtdp_desc, 
       g_fmmv_m.fmmvmodid_desc,g_fmmv_m.fmmvcnfid_desc
 
   #檢查是否允許此動作
   IF NOT afmt580_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_fmmv_m_mask_o.* =  g_fmmv_m.*
   CALL afmt580_fmmv_t_mask()
   LET g_fmmv_m_mask_n.* =  g_fmmv_m.*
   
   
 
   #顯示資料
   CALL afmt580_show()
   
   WHILE TRUE
      LET g_fmmv_m.fmmvdocno = g_fmmvdocno_t
 
      
      #寫入修改者/修改日期資訊
      LET g_fmmv_m.fmmvmodid = g_user 
LET g_fmmv_m.fmmvmoddt = cl_get_current()
LET g_fmmv_m.fmmvmodid_desc = cl_get_username(g_fmmv_m.fmmvmodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      
      #end add-point
 
      #資料輸入
      CALL afmt580_input("u")     
 
      #add-point:modify段修改後 name="modify.after_input"
      
      #end add-point
      
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         LET g_fmmv_m.* = g_fmmv_m_t.*
         CALL afmt580_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE fmmv_t SET (fmmvmodid,fmmvmoddt) = (g_fmmv_m.fmmvmodid,g_fmmv_m.fmmvmoddt)
       WHERE fmmvent = g_enterprise AND fmmvdocno = g_fmmvdocno_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL afmt580_set_act_visible()
   CALL afmt580_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " fmmvent = " ||g_enterprise|| " AND",
                      " fmmvdocno = '", g_fmmv_m.fmmvdocno, "' "
 
   #填到對應位置
   CALL afmt580_browser_fill(g_wc,"")
 
   CLOSE afmt580_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL afmt580_msgcentre_notify('modify')
   
   LET g_worksheet_hidden = 0
   
