{<section id="afmt580.delete" >}
#+ 資料刪除 
PRIVATE FUNCTION afmt580_delete()
   #add-point:delete段define(客製用) name="delete.define_customerization"
   
   #end add-point
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="delete.define"
   
   #end add-point  
   
   #add-point:Function前置處理  name="delete.pre_function"
   
   #end add-point
   
   #先確定key值無遺漏
   IF g_fmmv_m.fmmvdocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
    
   LET g_fmmvdocno_t = g_fmmv_m.fmmvdocno
 
   
   
 
   OPEN afmt580_cl USING g_enterprise,g_fmmv_m.fmmvdocno
 
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN afmt580_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CLOSE afmt580_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE afmt580_master_referesh USING g_fmmv_m.fmmvdocno INTO g_fmmv_m.fmmvsite,g_fmmv_m.fmmv001, 
       g_fmmv_m.fmmv002,g_fmmv_m.fmmvdocdt,g_fmmv_m.fmmv003,g_fmmv_m.fmmvdocno,g_fmmv_m.fmmv015,g_fmmv_m.fmmv004, 
       g_fmmv_m.fmmv013,g_fmmv_m.fmmv007,g_fmmv_m.fmmv012,g_fmmv_m.fmmv005,g_fmmv_m.fmmv011,g_fmmv_m.fmmv006, 
       g_fmmv_m.fmmv014,g_fmmv_m.fmmv010,g_fmmv_m.fmmv016,g_fmmv_m.fmmv017,g_fmmv_m.fmmv018,g_fmmv_m.fmmv019, 
       g_fmmv_m.fmmv020,g_fmmv_m.fmmvstus,g_fmmv_m.fmmvownid,g_fmmv_m.fmmvowndp,g_fmmv_m.fmmvcrtid,g_fmmv_m.fmmvcrtdp, 
       g_fmmv_m.fmmvcrtdt,g_fmmv_m.fmmvmodid,g_fmmv_m.fmmvmoddt,g_fmmv_m.fmmvcnfid,g_fmmv_m.fmmvcnfdt, 
       g_fmmv_m.fmmvownid_desc,g_fmmv_m.fmmvowndp_desc,g_fmmv_m.fmmvcrtid_desc,g_fmmv_m.fmmvcrtdp_desc, 
       g_fmmv_m.fmmvmodid_desc,g_fmmv_m.fmmvcnfid_desc
   
   
   #檢查是否允許此動作
   IF NOT afmt580_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_fmmv_m_mask_o.* =  g_fmmv_m.*
   CALL afmt580_fmmv_t_mask()
   LET g_fmmv_m_mask_n.* =  g_fmmv_m.*
   
   #將最新資料顯示到畫面上
   CALL afmt580_show()
   
   IF cl_ask_delete() THEN
 
      #add-point:單頭刪除前 name="delete.head.b_delete"
      
      #end add-point
 
      #應用 a47 樣板自動產生(Version:4)
      #刪除相關文件
      CALL afmt580_set_pk_array()
      #add-point:相關文件刪除前 name="delete.befroe.related_document_remove"
      
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
 
 
      DELETE FROM fmmv_t 
       WHERE fmmvent = g_enterprise AND fmmvdocno = g_fmmv_m.fmmvdocno 
 
 
      #add-point:單頭刪除中 name="delete.head.m_delete"
      
      #end add-point
         
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "fmmv_t:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = FALSE 
         CALL s_transaction_end('N','0')
         CALL cl_err()
      END IF
  
      
      
      #add-point:單頭刪除後 name="delete.head.a_delete"
      IF NOT s_aooi200_fin_del_docno(g_glaa.glaald,g_fmmv_m.fmmvdocno ,g_fmmv_m.fmmvdocdt) THEN
         CALL s_transaction_end('N','0')
         RETURN
      END IF
      DELETE FROM fmnd_t
       WHERE fmndent = g_enterprise AND fmnddocno = g_fmmv_m.fmmvdocno
      #end add-point
      
       
 
      #修改歷程記錄(刪除)
      IF NOT cl_log_modified_record('','') THEN 
         CLOSE afmt580_cl
         CALL s_transaction_end('N','0')
         RETURN
      END IF
      
      CLEAR FORM
      CALL afmt580_ui_browser_refresh()
      
      #確保畫面上保有資料
      IF g_browser_cnt > 0 THEN
         #CALL afmt580_browser_fill(g_wc,"")
         CALL afmt580_fetch("P")
      ELSE
         CLEAR FORM
      END IF
      CALL s_transaction_end('Y','0')
   ELSE    
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE afmt580_cl
 
   #功能已完成,通報訊息中心
   CALL afmt580_msgcentre_notify('delete')
 
   #add-point:單頭刪除完成後 name="delete.a_delete"
   
   #end add-point
 
END FUNCTION
 
