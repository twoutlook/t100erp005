        LET g_errparam.popup = TRUE
        CALL cl_err()
        LET g_errno = NULL
        CALL afmt580_set_act_visible()
        CALL afmt580_set_act_no_visible()
        CALL afmt580_show()
        RETURN FALSE
     END IF
   END IF
   #160818-00017#13 2016/08/23 By 07900 --add -e- 
   #end add-point
   
   RETURN TRUE
   
