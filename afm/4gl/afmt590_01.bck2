PRIVATE FUNCTION afmt590_01_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   
   #end add-point
   DEFINE  l_cmd                  LIKE type_t.chr1
   DEFINE  l_ac_t                 LIKE type_t.num10               #未取消的ARRAY CNT 
   DEFINE  l_n                    LIKE type_t.num10               #檢查重複用  
   DEFINE  l_cnt                  LIKE type_t.num10               #檢查重複用  
   DEFINE  l_lock_sw              LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert         LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete         LIKE type_t.num5                #可刪除否  
   DEFINE  l_count                LIKE type_t.num10
   DEFINE  l_i                    LIKE type_t.num10
   DEFINE  ls_return              STRING
   DEFINE  l_var_keys             DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys           DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                 DYNAMIC ARRAY OF STRING
   DEFINE  l_fields               DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak         DYNAMIC ARRAY OF STRING
   DEFINE  li_reproduce           LIKE type_t.num10
   DEFINE  li_reproduce_target    LIKE type_t.num10
   DEFINE  lb_reproduce           BOOLEAN
   DEFINE  l_insert               BOOLEAN
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   DEFINE l_num1    LIKE type_t.num20_6
   DEFINE l_num2    LIKE type_t.num20_6
   DEFINE l_glaa001 LIKE glaa_t.glaa001
   #end add-point 
   
   #add-point:Function前置處理  name="modify.pre_function"
   
   #end add-point
   
   LET g_action_choice = ""
   
   LET g_qryparam.state = "i"
 
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   
   #add-point:modify開始前 name="modify.define_sql"
   
   #end add-point
   
   LET INT_FLAG = FALSE
   LET lb_reproduce = FALSE
   LET l_insert = FALSE
 
   #關閉被遮罩相關欄位輸入, 無法確定USER是否會需要輸入此欄位
   #因此先行關閉, 若有需要可於下方add-point中自行開啟
   CALL cl_mask_set_no_entry()
 
   #add-point:modify段修改前 name="modify.before_input"
   
   #end add-point
 
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #Page1 預設值產生於此處
      INPUT ARRAY g_fmmz_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_detail_cnt,WITHOUT DEFAULTS, #MAXCOUNT = g_max_rec,
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_fmmz_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL afmt590_01_b_fill(g_wc2)
            LET g_detail_cnt = g_fmmz_d.getLength()
         
         BEFORE ROW
            #add-point:modify段before row name="input.body.before_row2"
            
            #end add-point  
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_cmd = ''
            LET l_ac_t = l_ac 
            LET l_ac = g_detail_idx
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            DISPLAY g_fmmz_d.getLength() TO FORMONLY.cnt
         
            CALL s_transaction_begin()
            LET g_detail_cnt = g_fmmz_d.getLength()
            
            IF g_detail_cnt >= l_ac 
               AND g_fmmz_d[l_ac].fmmzdocno IS NOT NULL
               AND g_fmmz_d[l_ac].fmmzseq IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_fmmz_d_t.* = g_fmmz_d[l_ac].*  #BACKUP
               LET g_fmmz_d_o.* = g_fmmz_d[l_ac].*  #BACKUP
               IF NOT afmt590_01_lock_b("fmmz_t") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH afmt590_01_bcl INTO g_fmmz_d[l_ac].fmmzdocno,g_fmmz_d[l_ac].fmmzseq,g_fmmz_d[l_ac].fmmz001, 
                      g_fmmz_d[l_ac].fmmz005,g_fmmz_d[l_ac].fmmz008,g_fmmz_d[l_ac].fmmz010,g_fmmz_d[l_ac].fmmz009, 
                      g_fmmz_d[l_ac].fmmz002,g_fmmz_d[l_ac].fmmz003,g_fmmz_d[l_ac].fmmz004,g_fmmz_d[l_ac].fmmz011 
 
                  IF SQLCA.sqlcode THEN
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = g_fmmz_d_t.fmmzdocno,":",SQLERRMESSAGE  
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     LET l_lock_sw = "Y"
                  END IF
                  
                  #遮罩相關處理
                  LET g_fmmz_d_mask_o[l_ac].* =  g_fmmz_d[l_ac].*
                  CALL afmt590_01_fmmz_t_mask()
                  LET g_fmmz_d_mask_n[l_ac].* =  g_fmmz_d[l_ac].*
                  
                  CALL afmt590_01_detail_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            CALL afmt590_01_set_entry_b(l_cmd)
            CALL afmt590_01_set_no_entry_b(l_cmd)
            #add-point:modify段before row name="input.body.before_row"
            CALL afmt590_01_set_entry_b(l_cmd)
            CALL afmt590_01_set_no_entry_b(l_cmd)
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
 
            #其他table進行lock
            
 
        
         BEFORE INSERT
            
            LET l_insert = TRUE
            IF s_transaction_chk("N",0) THEN
               CALL s_transaction_begin()
            END IF
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_fmmz_d_t.* TO NULL
            INITIALIZE g_fmmz_d_o.* TO NULL
            INITIALIZE g_fmmz_d[l_ac].* TO NULL 
            #公用欄位給值(單身)
            
            #自定義預設值(單身1)
                  LET g_fmmz_d[l_ac].fmmzseq = "0"
      LET g_fmmz_d[l_ac].fmmz003 = "0"
      LET g_fmmz_d[l_ac].fmmz004 = "0"
      LET g_fmmz_d[l_ac].fmmz011 = "0"
 
            #add-point:modify段before備份 name="input.body.before_bak"
            LET g_fmmz_d[l_ac].fmmzdocno  = g_fmmz_m.l_fmmzdocno
            LET g_fmmz_d[l_ac].fmmz005 = "Y"
            LET g_fmmz_d[l_ac].fmmzseq = NULL
            SELECT MAX(fmmzseq)+1 INTO g_fmmz_d[l_ac].fmmzseq FROM fmmz_t
             WHERE fmmzent = g_enterprise
               AND fmmzdocno  = g_fmmz_m.l_fmmzdocno
            IF cl_null(g_fmmz_d[l_ac].fmmzseq)THEN LET g_fmmz_d[l_ac].fmmzseq = 1 END IF
            
            DISPLAY BY NAME  g_fmmz_d[l_ac].fmmzseq
            #end add-point
            LET g_fmmz_d_t.* = g_fmmz_d[l_ac].*     #新輸入資料
            LET g_fmmz_d_o.* = g_fmmz_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_fmmz_d[li_reproduce_target].* = g_fmmz_d[li_reproduce].*
 
               LET g_fmmz_d[g_fmmz_d.getLength()].fmmzdocno = NULL
               LET g_fmmz_d[g_fmmz_d.getLength()].fmmzseq = NULL
 
            END IF
            
 
            CALL afmt590_01_set_entry_b(l_cmd)
            CALL afmt590_01_set_no_entry_b(l_cmd)
            #add-point:modify段before insert name="input.body.before_insert"
            CALL afmt590_01_set_entry_b(l_cmd)
            CALL afmt590_01_set_no_entry_b(l_cmd)
            #end add-point  
  
         AFTER INSERT
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(1) INTO l_count FROM fmmz_t 
             WHERE fmmzent = g_enterprise AND fmmzdocno = g_fmmz_d[l_ac].fmmzdocno
                                       AND fmmzseq = g_fmmz_d[l_ac].fmmzseq
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前 name="input.body.b_insert"
               
               #end add-point
            
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_fmmz_d[g_detail_idx].fmmzdocno
               LET gs_keys[2] = g_fmmz_d[g_detail_idx].fmmzseq
               CALL afmt590_01_insert_b('fmmz_t',gs_keys,"'1'")
                           
               #add-point:單身新增後 name="input.body.a_insert"
               
               #end add-point
            ELSE    
               INITIALIZE g_fmmz_d[l_ac].* TO NULL
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = 'INSERT' 
               LET g_errparam.code   = "std-00006" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "fmmz_t:",SQLERRMESSAGE 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL afmt590_01_b_fill(g_wc2)
               #資料多語言用-增/改
               
               #add-point:input段-after_insert name="input.body.a_insert2"
               
               #end add-point
               ##ERROR 'INSERT O.K'
               LET g_detail_cnt = g_detail_cnt + 1
               
               LET g_wc2 = g_wc2, " OR (fmmzdocno = '", g_fmmz_d[l_ac].fmmzdocno, "' "
                                  ," AND fmmzseq = '", g_fmmz_d[l_ac].fmmzseq, "' "
 
                                  ,")"
            END IF                
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' THEN
               LET l_cmd='d'
            ELSE
               #add-point:單身刪除ask前 name="input.body.b_delete_ask"
               
               #end add-point   
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "" 
                  LET g_errparam.code   = -263 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  CANCEL DELETE
               END IF
               
               #add-point:單身刪除前 name="input.body.b_delete"
               
               #end add-point   
               
               DELETE FROM fmmz_t
                WHERE fmmzent = g_enterprise AND 
                      fmmzdocno = g_fmmz_d_t.fmmzdocno
                      AND fmmzseq = g_fmmz_d_t.fmmzseq
 
                      
               #add-point:單身刪除中 name="input.body.m_delete"
               
               #end add-point  
                      
               IF SQLCA.sqlcode THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "fmmz_t:",SQLERRMESSAGE 
                  LET g_errparam.code   = SQLCA.sqlcode 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  CANCEL DELETE   
               ELSE
                  LET g_detail_cnt = g_detail_cnt-1
                  
 
                  #add-point:單身刪除後 name="input.body.a_delete"
                  
                  #end add-point
                  #修改歷程記錄(刪除)
                  CALL afmt590_01_set_pk_array()
                  IF NOT cl_log_modified_record('','') THEN 
                  ELSE
                  END IF
               END IF 
               CLOSE afmt590_01_bcl
