    LET g_sql = g_sql CLIPPED," AND (t0.fmmz008 IN (",g_sql_bank,") OR TRIM(t0.fmmz008) IS NULL)" #160122-00001#21 mod by 07675
   #160122-00001#21--add---end
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("fmmz_t"),
                      " ORDER BY t0.fmmzdocno,t0.fmmzseq"
   
   #add-point:b_fill段sql之後 name="b_fill.sql_after"
   
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"fmmz_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE afmt590_01_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR afmt590_01_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_fmmz_d.clear()
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_fmmz_d[l_ac].fmmzdocno,g_fmmz_d[l_ac].fmmzseq,g_fmmz_d[l_ac].fmmz001,g_fmmz_d[l_ac].fmmz005, 
