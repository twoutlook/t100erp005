   END FOR
   
   IF g_cnt > g_fmmz_d.getLength() THEN
      LET l_ac = g_fmmz_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_fmmz_d.getLength()
      LET g_fmmz_d_mask_o[l_ac].* =  g_fmmz_d[l_ac].*
      CALL afmt590_01_fmmz_t_mask()
      LET g_fmmz_d_mask_n[l_ac].* =  g_fmmz_d[l_ac].*
   END FOR
   
 
   
   LET l_ac = g_cnt
 
