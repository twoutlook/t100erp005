   LET g_sql = "SELECT  DISTINCT t0.fmmzdocno,t0.fmmzseq,t0.fmmz001,t0.fmmz005,t0.fmmz008,t0.fmmz010, 
       t0.fmmz009,t0.fmmz002,t0.fmmz003,t0.fmmz004,t0.fmmz011  FROM fmmz_t t0",
               "",
               
               " WHERE t0.fmmzent= ?  AND  1=1 AND (", p_wc2, ") " 
