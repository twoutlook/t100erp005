################################################################################
PRIVATE FUNCTION afmt145_fmcu007_chk()
DEFINE l_nmajstus               LIKE nmaj_t.nmajstus
DEFINE l_nmaj002                LIKE nmaj_t.nmaj002

   LET g_errno = ''

   SELECT nmajstus,nmaj002 INTO l_nmajstus,l_nmaj002
     FROM nmaj_t
    WHERE nmajent = g_enterprise
      AND nmaj001 = g_fmcu_d[l_ac].fmcu007

   CASE
      WHEN SQLCA.SQLCODE = 100    LET g_errno = 'anm-00017'
      WHEN l_nmajstus = 'N'       LET g_errno = 'anm-00016'
      WHEN l_nmaj002 <> '1'       LET g_errno = 'anm-00072' #输入的资料不为存入存提码
   END CASE

END FUNCTION

################################################################################
# Descriptions...: 傳票拋轉檢查
# Memo...........:
# Usage..........: CALL afmt145_fmct002_chk()
# Input parameter: 
# Date & Author..: 2015/12/22 By yangtt
# Modify.........:
################################################################################
PRIVATE FUNCTION afmt145_fmct002_chk()
   DEFINE r_success       LIKE type_t.num5
   
   LET r_success = TRUE
   
   IF g_fmct_m.fmctdocno IS NULL THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      LET  r_success = FALSE
      RETURN r_success
   END IF
   
   #未確認單據不可拋轉
   IF g_fmct_m.fmctstus <> 'Y' THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = g_fmct_m.fmctdocno
      LET g_errparam.code   = "anm-00185" 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      LET  r_success = FALSE
      RETURN r_success
   END IF
   
   IF NOT cl_null(g_fmct_m.fmct002) THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = g_fmct_m.fmctdocno 
      LET g_errparam.code   = "anm-00198" 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      LET  r_success = FALSE
      RETURN r_success
   END IF
   
   RETURN r_success
END FUNCTION

