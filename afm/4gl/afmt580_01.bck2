   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   
   #add-point:modify開始前 name="modify.define_sql"
   #150820-00011#6---s
   LET g_glaa005 = ''
   SELECT glaa005 INTO g_glaa005
     FROM glaa_t
    WHERE glaald = g_glaald AND glaaent = g_enterprise
   #150820-00011#6---e
   #end add-point
   
   LET INT_FLAG = FALSE
   LET lb_reproduce = FALSE
   LET l_insert = FALSE
 
   #關閉被遮罩相關欄位輸入, 無法確定USER是否會需要輸入此欄位
   #因此先行關閉, 若有需要可於下方add-point中自行開啟
   CALL cl_mask_set_no_entry()
 
   #add-point:modify段修改前 name="modify.before_input"
   
   #end add-point
 
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #Page1 預設值產生於此處
      INPUT ARRAY g_fmnd_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_detail_cnt,WITHOUT DEFAULTS, #MAXCOUNT = g_max_rec,
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_fmnd_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL afmt580_01_b_fill(g_wc2)
            LET g_detail_cnt = g_fmnd_d.getLength()
         
         BEFORE ROW
            #add-point:modify段before row name="input.body.before_row2"
    
            #end add-point  
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_cmd = ''
            LET l_ac_t = l_ac 
            LET l_ac = g_detail_idx
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            DISPLAY g_fmnd_d.getLength() TO FORMONLY.cnt
         
            CALL s_transaction_begin()
            LET g_detail_cnt = g_fmnd_d.getLength()
            
            IF g_detail_cnt >= l_ac 
               AND g_fmnd_d[l_ac].fmnddocno IS NOT NULL
               AND g_fmnd_d[l_ac].fmndseq IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_fmnd_d_t.* = g_fmnd_d[l_ac].*  #BACKUP
               LET g_fmnd_d_o.* = g_fmnd_d[l_ac].*  #BACKUP
               IF NOT afmt580_01_lock_b("fmnd_t") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH afmt580_01_bcl INTO g_fmnd_d[l_ac].fmnddocno,g_fmnd_d[l_ac].fmndseq,g_fmnd_d[l_ac].fmnd001, 
                      g_fmnd_d[l_ac].fmnd005,g_fmnd_d[l_ac].fmnd008,g_fmnd_d[l_ac].fmnd010,g_fmnd_d[l_ac].fmnd009, 
                      g_fmnd_d[l_ac].fmnd002,g_fmnd_d[l_ac].fmnd004,g_fmnd_d[l_ac].fmnd003,g_fmnd_d[l_ac].fmnd011 
 
                  IF SQLCA.sqlcode THEN
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = g_fmnd_d_t.fmnddocno,":",SQLERRMESSAGE  
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     LET l_lock_sw = "Y"
                  END IF
                  
                  #遮罩相關處理
                  LET g_fmnd_d_mask_o[l_ac].* =  g_fmnd_d[l_ac].*
                  CALL afmt580_01_fmnd_t_mask()
                  LET g_fmnd_d_mask_n[l_ac].* =  g_fmnd_d[l_ac].*
                  
                  CALL afmt580_01_detail_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            CALL afmt580_01_set_entry_b(l_cmd)
            CALL afmt580_01_set_no_entry_b(l_cmd)
            #add-point:modify段before row name="input.body.before_row"
            CALL afmt580_01_set_entry_b(l_cmd)
            CALL afmt580_01_set_no_entry_b(l_cmd)
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
 
            #其他table進行lock
            
 
        
         BEFORE INSERT
            
            LET l_insert = TRUE
            IF s_transaction_chk("N",0) THEN
               CALL s_transaction_begin()
            END IF
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_fmnd_d_t.* TO NULL
            INITIALIZE g_fmnd_d_o.* TO NULL
            INITIALIZE g_fmnd_d[l_ac].* TO NULL 
            #公用欄位給值(單身)
            
            #自定義預設值(單身1)
                  LET g_fmnd_d[l_ac].fmnd005 = "Y"
      LET g_fmnd_d[l_ac].fmnd004 = "1"
      LET g_fmnd_d[l_ac].fmnd003 = "0"
      LET g_fmnd_d[l_ac].fmnd011 = "0"
 
            #add-point:modify段before備份 name="input.body.before_bak"
            
            #end add-point
            LET g_fmnd_d_t.* = g_fmnd_d[l_ac].*     #新輸入資料
            LET g_fmnd_d_o.* = g_fmnd_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_fmnd_d[li_reproduce_target].* = g_fmnd_d[li_reproduce].*
 
               LET g_fmnd_d[g_fmnd_d.getLength()].fmnddocno = NULL
               LET g_fmnd_d[g_fmnd_d.getLength()].fmndseq = NULL
 
            END IF
            
 
            CALL afmt580_01_set_entry_b(l_cmd)
            CALL afmt580_01_set_no_entry_b(l_cmd)
            #add-point:modify段before insert name="input.body.before_insert"
            LET g_fmnd_d[l_ac].fmnddocno = g_fmmvdocno
            #預帶項次
            LET g_fmnd_d[l_ac].fmndseq = NULL
            SELECT MAX(fmndseq) INTO g_fmnd_d[l_ac].fmndseq FROM fmnd_t
             WHERE fmndent = g_enterprise
               AND fmnddocno = g_fmnd_d[l_ac].fmnddocno
            IF cl_null(g_fmnd_d[l_ac].fmndseq) THEN
               LET g_fmnd_d[l_ac].fmndseq = 0
            END IF
            LET g_fmnd_d[l_ac].fmndseq = g_fmnd_d[l_ac].fmndseq + 1  
            LET g_fmnd_d[l_ac].fmnd005 = 'Y'  

            CALL cl_set_comp_entry("fmnd008",TRUE)
            CALL cl_set_comp_entry("fmnd002,fmnd003,fmnd004",TRUE)
            IF g_fmnd_d[l_ac].fmnd005 = 'Y' THEN
               #CALL cl_set_comp_entry("fmnd008",FALSE)   ##151106-00002#2 改為必輸
            ELSE
               CALL cl_set_comp_entry("fmnd002,fmnd003,fmnd004",FALSE)
            END IF 
            #end add-point  
  
         AFTER INSERT
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(1) INTO l_count FROM fmnd_t 
             WHERE fmndent = g_enterprise AND fmnddocno = g_fmnd_d[l_ac].fmnddocno
                                       AND fmndseq = g_fmnd_d[l_ac].fmndseq
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前 name="input.body.b_insert"
               
               #end add-point
            
                              INITIALIZE gs_keys TO NULL 
