{<section id="afmt580_01.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION afmt580_01_b_fill(p_wc2)              #BODY FILL UP
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   
   #end add-point
   DEFINE p_wc2    STRING
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   
   #end add-point
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前 name="b_fill.sql_before"
   LET p_wc2 = " fmnddocno = '",g_fmmvdocno,"'"
   #end add-point
 
   LET g_sql = "SELECT  DISTINCT t0.fmnddocno,t0.fmndseq,t0.fmnd001,t0.fmnd005,t0.fmnd008,t0.fmnd010, 
       t0.fmnd009,t0.fmnd002,t0.fmnd004,t0.fmnd003,t0.fmnd011  FROM fmnd_t t0",
               "",
               
               " WHERE t0.fmndent= ?  AND  1=1 AND (", p_wc2, ") " 
   #add-point:b_fill段sql wc name="b_fill.sql_wc"
   #160122-00001#21--add---str
#   LET g_sql = g_sql CLIPPED," AND (t0.fmnd008 IN(SELECT UNIQUE nmll001 FROM nmll_t WHERE nmllent = ",g_enterprise," AND nmll002 = '",g_user,"')
#                                OR t0.fmnd008 IS NULL)"
   LET g_sql = g_sql CLIPPED," AND (t0.fmnd008 IN(",g_sql_bank,") OR TRIM(t0.fmnd008) IS NULL)"#160122-00001#21 mod by 07675
   #160122-00001#21--add---end
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("fmnd_t"),
                      " ORDER BY t0.fmnddocno,t0.fmndseq"
   
   #add-point:b_fill段sql之後 name="b_fill.sql_after"
   
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"fmnd_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE afmt580_01_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR afmt580_01_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_fmnd_d.clear()
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_fmnd_d[l_ac].fmnddocno,g_fmnd_d[l_ac].fmndseq,g_fmnd_d[l_ac].fmnd001,g_fmnd_d[l_ac].fmnd005, 
       g_fmnd_d[l_ac].fmnd008,g_fmnd_d[l_ac].fmnd010,g_fmnd_d[l_ac].fmnd009,g_fmnd_d[l_ac].fmnd002,g_fmnd_d[l_ac].fmnd004, 
       g_fmnd_d[l_ac].fmnd003,g_fmnd_d[l_ac].fmnd011
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充 name="b_fill.fill"
      
      #end add-point
      
      CALL afmt580_01_detail_show()      
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
 
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   LET g_error_show = 0
   
 
   
   CALL g_fmnd_d.deleteElement(g_fmnd_d.getLength())   
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_fmnd_d.getLength()
 
      #add-point:b_fill段key值相關欄位 name="b_fill.keys.fill"
      
      #end add-point
   END FOR
   
   IF g_cnt > g_fmnd_d.getLength() THEN
      LET l_ac = g_fmnd_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_fmnd_d.getLength()
      LET g_fmnd_d_mask_o[l_ac].* =  g_fmnd_d[l_ac].*
      CALL afmt580_01_fmnd_t_mask()
      LET g_fmnd_d_mask_n[l_ac].* =  g_fmnd_d[l_ac].*
   END FOR
   
 
   
   LET l_ac = g_cnt
 
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_fmnd_d.getLength()
   DISPLAY g_detail_idx TO FORMONLY.idx
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE afmt580_01_pb
   
END FUNCTION
 
