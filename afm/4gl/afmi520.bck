{<section id="afmi520.fetch" >}
#+ 指定PK後抓取單頭其他資料
PRIVATE FUNCTION afmi520_fetch(p_fl)
   #add-point:fetch段define(客製用) name="fetch.define_customerization"
   
   #end add-point
   DEFINE p_fl       LIKE type_t.chr1
   DEFINE ls_msg     STRING
   #add-point:fetch段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="fetch.define"
   
   #end add-point  
   
   #add-point:Function前置處理  name="fetch.pre_function"
   
   #end add-point
   
   #根據傳入的條件決定抓取的資料
   CASE p_fl
      WHEN "F" 
         LET g_current_idx = 1
      WHEN "P"
         IF g_current_idx > 1 THEN               
            LET g_current_idx = g_current_idx - 1
         END IF 
      WHEN "N"
         IF g_current_idx < g_header_cnt THEN
            LET g_current_idx =  g_current_idx + 1
         END IF        
      WHEN "L" 
         #LET g_current_idx = g_header_cnt        
         LET g_current_idx = g_browser.getLength()    
      WHEN "/"
         #詢問要指定的筆數
         IF (NOT g_no_ask) THEN      
            CALL cl_getmsg("fetch", g_lang) RETURNING ls_msg
            LET INT_FLAG = 0
 
            PROMPT ls_msg CLIPPED,": " FOR g_jump
               #交談指令共用ACTION
               &include "common_action.4gl"
            END PROMPT
            
            IF INT_FLAG THEN
               LET INT_FLAG = 0
               EXIT CASE  
            END IF           
         END IF
         IF g_jump > 0 THEN
            LET g_current_idx = g_jump
         END IF
         LET g_no_ask = FALSE     
   END CASE
 
   #筆數顯示
   LET g_browser_idx = g_current_idx 
   IF g_browser_cnt > 0 THEN
      DISPLAY g_browser_idx TO FORMONLY.b_index #當下筆數
      DISPLAY g_browser_cnt TO FORMONLY.b_count #總筆數
      DISPLAY g_browser_idx TO FORMONLY.h_index #當下筆數
      DISPLAY g_browser_cnt TO FORMONLY.h_count #總筆數
   ELSE
      DISPLAY '' TO FORMONLY.b_index #當下筆數
      DISPLAY '' TO FORMONLY.b_count #總筆數
      DISPLAY '' TO FORMONLY.h_index #當下筆數
      DISPLAY '' TO FORMONLY.h_count #總筆數
   END IF
   
   
   
   #避免超出browser資料筆數上限
   IF g_current_idx > g_browser.getLength() THEN
      LET g_browser_idx = g_browser.getLength()
      LET g_current_idx = g_browser.getLength() 
   END IF
   
   # 設定browse索引
   CALL g_curr_diag.setCurrentRow("s_browse", g_current_idx)
   CALL cl_navigator_setting(g_browser_idx, g_browser_cnt) 
 
   #代表沒有資料, 無需做後續資料撈取之動作
   IF g_current_idx = 0 THEN
      RETURN
   END IF
 
   #根據選定的筆數給予key欄位值
   LET g_fmmb_m.fmmb001 = g_browser[g_current_idx].b_fmmb001
   LET g_fmmb_m.fmmb002 = g_browser[g_current_idx].b_fmmb002
 
                       
   #讀取單頭所有欄位資料
   EXECUTE afmi520_master_referesh USING g_fmmb_m.fmmb001,g_fmmb_m.fmmb002 INTO g_fmmb_m.fmmb001,g_fmmb_m.fmmb002, 
       g_fmmb_m.fmmbstus,g_fmmb_m.fmmbownid,g_fmmb_m.fmmbowndp,g_fmmb_m.fmmbcrtid,g_fmmb_m.fmmbcrtdp, 
       g_fmmb_m.fmmbcrtdt,g_fmmb_m.fmmbmodid,g_fmmb_m.fmmbmoddt,g_fmmb_m.fmmb003,g_fmmb_m.fmmb004,g_fmmb_m.fmmb005, 
       g_fmmb_m.fmmb006,g_fmmb_m.fmmb012,g_fmmb_m.fmmb008,g_fmmb_m.fmmb013,g_fmmb_m.fmmb009,g_fmmb_m.fmmb010, 
       g_fmmb_m.fmmb011,g_fmmb_m.fmmb014,g_fmmb_m.fmmb015,g_fmmb_m.fmmb016,g_fmmb_m.fmmbownid_desc,g_fmmb_m.fmmbowndp_desc, 
       g_fmmb_m.fmmbcrtid_desc,g_fmmb_m.fmmbcrtdp_desc,g_fmmb_m.fmmbmodid_desc
   
   #遮罩相關處理
   LET g_fmmb_m_mask_o.* =  g_fmmb_m.*
   CALL afmi520_fmmb_t_mask()
   LET g_fmmb_m_mask_n.* =  g_fmmb_m.*
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL afmi520_set_act_visible()
   CALL afmi520_set_act_no_visible()
 
   #add-point:fetch段action控制 name="fetch.action_control"
   
   #end add-point  
   
   
   
   #保存單頭舊值
   LET g_fmmb_m_t.* = g_fmmb_m.*
   LET g_fmmb_m_o.* = g_fmmb_m.*
   
   LET g_data_owner = g_fmmb_m.fmmbownid      
   LET g_data_dept  = g_fmmb_m.fmmbowndp
   
   #重新顯示
   CALL afmi520_show()
   
 
END FUNCTION
 
