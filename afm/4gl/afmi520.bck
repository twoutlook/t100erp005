################################################################################
# Descriptions...: 資金類型檢核
# Memo...........:
# Usage..........: CALLafmi520_fmmb001_chk()
# Date & Author..: 2015/04/25 By Hans
# Modify.........:
################################################################################
PRIVATE FUNCTION afmi520_fmmb001_chk()
DEFINE l_fmma001      LIKE fmma_t.fmma001
DEFINE l_fmmastus     LIKE fmma_t.fmmastus
DEFINE r_success LIKE type_t.num5
DEFINE r_errno   LIKE gzze_t.gzze001

   LET r_success = TRUE
   LET r_errno = NULL
    
    SELECT fmma001,fmmastus INTO l_fmma001,l_fmmastus
      FROM fmma_t
     WHERE fmmaent = g_enterprise
       AND fmma001 = g_fmmb_m.fmmb001
              
   CASE
      WHEN SQLCA.SQLCODE = 100   LET r_errno = 'afm-00070' LET r_success = FALSE
      WHEN l_fmmastus = 'N'      LET r_errno = 'sub-01302'  LET r_success = FALSE   #'agl-00012'-> 'sub-01302' #160318-00005#11 mod  
   END CASE        
    
   RETURN r_success,r_errno



END FUNCTION

