            DISPLAY g_qryparam.return1 TO fmmb001  #顯示到畫面上
            NEXT FIELD fmmb001
            #END add-point
 
 
         #Ctrlp:construct.c.fmmb002
         #應用 a03 樣板自動產生(Version:3)
         ON ACTION controlp INFIELD fmmb002
            #add-point:ON ACTION controlp INFIELD fmmb002 name="construct.c.fmmb002"
            #開窗c段
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c'
            LET g_qryparam.reqry = FALSE
            CALL q_authorised_ld()
            DISPLAY g_qryparam.return1 TO fmmb002  #顯示到畫面上
            NEXT FIELD fmmb002



