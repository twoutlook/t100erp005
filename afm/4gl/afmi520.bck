         AFTER FIELD fmmb001
            
            #add-point:AFTER FIELD fmmb001 name="construct.a.fmmb001"
            
            #END add-point
            
 
 
         #Ctrlp:construct.c.fmmb001
         #應用 a03 樣板自動產生(Version:3)
         ON ACTION controlp INFIELD fmmb001
            #add-point:ON ACTION controlp INFIELD fmmb001 name="construct.c.fmmb001"
