                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
         
         #----<<fmmbcnfdt>>----
         
         #----<<fmmbpstdt>>----
 
 
 
      
         #一般欄位
                  #應用 a01 樣板自動產生(Version:2)
         BEFORE FIELD fmmb001
            #add-point:BEFORE FIELD fmmb001 name="construct.b.fmmb001"
            
            #END add-point
 
 
