      #add-point:menu前 name="statechange.before_menu"
      
      #end add-point
      
      
	  
      ON ACTION inactive
         IF cl_auth_chk_act("inactive") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.inactive"
            
            #end add-point
         END IF
         EXIT MENU
      ON ACTION active
         IF cl_auth_chk_act("active") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.active"
            
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      ) OR 
      g_fmmb_m.fmmbstus = lc_state OR cl_null(lc_state) THEN
      CLOSE afmi520_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   
   #end add-point
   
   LET g_fmmb_m.fmmbmodid = g_user
   LET g_fmmb_m.fmmbmoddt = cl_get_current()
   LET g_fmmb_m.fmmbstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE fmmb_t 
      SET (fmmbstus,fmmbmodid,fmmbmoddt) 
        = (g_fmmb_m.fmmbstus,g_fmmb_m.fmmbmodid,g_fmmb_m.fmmbmoddt)     
    WHERE fmmbent = g_enterprise AND fmmb001 = g_fmmb_m.fmmb001
      AND fmmb002 = g_fmmb_m.fmmb002
    
