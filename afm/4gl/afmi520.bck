PRIVATE FUNCTION afmi520_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   
   #end add-point
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   
   #end add-point
   
   #add-point:Function前置處理 name="modify.pre_function"
   
   #end add-point
   
   #先確定key值無遺漏
   IF g_fmmb_m.fmmb001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_fmmb001_t = g_fmmb_m.fmmb001
   LET g_fmmb002_t = g_fmmb_m.fmmb002
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN afmi520_cl USING g_enterprise,g_fmmb_m.fmmb001,g_fmmb_m.fmmb002
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN afmi520_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CLOSE afmi520_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE afmi520_master_referesh USING g_fmmb_m.fmmb001,g_fmmb_m.fmmb002 INTO g_fmmb_m.fmmb001,g_fmmb_m.fmmb002, 
