{<section id="afmi520.delete" >}
#+ 資料刪除 
PRIVATE FUNCTION afmi520_delete()
   #add-point:delete段define(客製用) name="delete.define_customerization"
   
   #end add-point
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="delete.define"
   
   #end add-point  
   
   #add-point:Function前置處理  name="delete.pre_function"
   
   #end add-point
   
   #先確定key值無遺漏
   IF g_fmmb_m.fmmb001 IS NULL
   OR g_fmmb_m.fmmb002 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
    
   LET g_fmmb001_t = g_fmmb_m.fmmb001
   LET g_fmmb002_t = g_fmmb_m.fmmb002
 
   
   
 
   OPEN afmi520_cl USING g_enterprise,g_fmmb_m.fmmb001,g_fmmb_m.fmmb002
 
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN afmi520_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CLOSE afmi520_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE afmi520_master_referesh USING g_fmmb_m.fmmb001,g_fmmb_m.fmmb002 INTO g_fmmb_m.fmmb001,g_fmmb_m.fmmb002, 
       g_fmmb_m.fmmbstus,g_fmmb_m.fmmbownid,g_fmmb_m.fmmbowndp,g_fmmb_m.fmmbcrtid,g_fmmb_m.fmmbcrtdp, 
       g_fmmb_m.fmmbcrtdt,g_fmmb_m.fmmbmodid,g_fmmb_m.fmmbmoddt,g_fmmb_m.fmmb003,g_fmmb_m.fmmb004,g_fmmb_m.fmmb005, 
       g_fmmb_m.fmmb006,g_fmmb_m.fmmb012,g_fmmb_m.fmmb008,g_fmmb_m.fmmb013,g_fmmb_m.fmmb009,g_fmmb_m.fmmb010, 
       g_fmmb_m.fmmb011,g_fmmb_m.fmmb014,g_fmmb_m.fmmb015,g_fmmb_m.fmmb016,g_fmmb_m.fmmbownid_desc,g_fmmb_m.fmmbowndp_desc, 
       g_fmmb_m.fmmbcrtid_desc,g_fmmb_m.fmmbcrtdp_desc,g_fmmb_m.fmmbmodid_desc
   
   
   #檢查是否允許此動作
   IF NOT afmi520_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_fmmb_m_mask_o.* =  g_fmmb_m.*
   CALL afmi520_fmmb_t_mask()
   LET g_fmmb_m_mask_n.* =  g_fmmb_m.*
   
   #將最新資料顯示到畫面上
   CALL afmi520_show()
   
   IF cl_ask_delete() THEN
 
      #add-point:單頭刪除前 name="delete.head.b_delete"
      
      #end add-point
 
      #應用 a47 樣板自動產生(Version:4)
      #刪除相關文件
      CALL afmi520_set_pk_array()
      #add-point:相關文件刪除前 name="delete.befroe.related_document_remove"
      
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
 
 
      DELETE FROM fmmb_t 
       WHERE fmmbent = g_enterprise AND fmmb001 = g_fmmb_m.fmmb001 
         AND fmmb002 = g_fmmb_m.fmmb002 
 
 
      #add-point:單頭刪除中 name="delete.head.m_delete"
      
      #end add-point
         
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "fmmb_t:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = FALSE 
         CALL s_transaction_end('N','0')
         CALL cl_err()
      END IF
  
      
      
      #add-point:單頭刪除後 name="delete.head.a_delete"
      
      #end add-point
      
       
 
      #修改歷程記錄(刪除)
      IF NOT cl_log_modified_record('','') THEN 
         CLOSE afmi520_cl
         CALL s_transaction_end('N','0')
         RETURN
      END IF
      
      CLEAR FORM
      CALL afmi520_ui_browser_refresh()
      
      #確保畫面上保有資料
      IF g_browser_cnt > 0 THEN
         #CALL afmi520_browser_fill(g_wc,"")
         CALL afmi520_fetch("P")
      ELSE
         CLEAR FORM
      END IF
      CALL s_transaction_end('Y','0')
   ELSE    
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE afmi520_cl
 
   #功能已完成,通報訊息中心
   CALL afmi520_msgcentre_notify('delete')
 
   #add-point:單頭刪除完成後 name="delete.a_delete"
   
   #end add-point
 
END FUNCTION
 
