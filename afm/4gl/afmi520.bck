         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE afmi520_master_referesh USING g_fmmb_m.fmmb001,g_fmmb_m.fmmb002 INTO g_fmmb_m.fmmb001, 
          g_fmmb_m.fmmb002,g_fmmb_m.fmmbstus,g_fmmb_m.fmmbownid,g_fmmb_m.fmmbowndp,g_fmmb_m.fmmbcrtid, 
          g_fmmb_m.fmmbcrtdp,g_fmmb_m.fmmbcrtdt,g_fmmb_m.fmmbmodid,g_fmmb_m.fmmbmoddt,g_fmmb_m.fmmb003, 
          g_fmmb_m.fmmb004,g_fmmb_m.fmmb005,g_fmmb_m.fmmb006,g_fmmb_m.fmmb012,g_fmmb_m.fmmb008,g_fmmb_m.fmmb013, 
          g_fmmb_m.fmmb009,g_fmmb_m.fmmb010,g_fmmb_m.fmmb011,g_fmmb_m.fmmb014,g_fmmb_m.fmmb015,g_fmmb_m.fmmb016, 
          g_fmmb_m.fmmbownid_desc,g_fmmb_m.fmmbowndp_desc,g_fmmb_m.fmmbcrtid_desc,g_fmmb_m.fmmbcrtdp_desc, 
          g_fmmb_m.fmmbmodid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_fmmb_m.fmmb001,g_fmmb_m.fmmb001_desc,g_fmmb_m.fmmb002,g_fmmb_m.fmmb002_desc, 
          g_fmmb_m.fmmbstus,g_fmmb_m.fmmbownid,g_fmmb_m.fmmbownid_desc,g_fmmb_m.fmmbowndp,g_fmmb_m.fmmbowndp_desc, 
          g_fmmb_m.fmmbcrtid,g_fmmb_m.fmmbcrtid_desc,g_fmmb_m.fmmbcrtdp,g_fmmb_m.fmmbcrtdp_desc,g_fmmb_m.fmmbcrtdt, 
          g_fmmb_m.fmmbmodid,g_fmmb_m.fmmbmodid_desc,g_fmmb_m.fmmbmoddt,g_fmmb_m.fmmb003,g_fmmb_m.fmmb003_desc, 
          g_fmmb_m.fmmb004,g_fmmb_m.fmmb004_desc,g_fmmb_m.fmmb005,g_fmmb_m.fmmb005_desc,g_fmmb_m.fmmb006, 
          g_fmmb_m.fmmb006_desc,g_fmmb_m.fmmb012,g_fmmb_m.fmmb012_desc,g_fmmb_m.fmmb008,g_fmmb_m.fmmb008_desc, 
