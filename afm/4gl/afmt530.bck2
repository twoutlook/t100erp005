PRIVATE FUNCTION afmt530_set_required()
DEFINE l_fmma002  LIKE fmma_t.fmma002

   IF g_fmmj_m.fmmj011 = '3' OR g_fmmj_m.fmmj011 = '2' THEN  #150820-00011#4 add 支付方式2
      CALL cl_set_comp_required("fmmj012,fmmj013",TRUE)
   END IF
   SELECT fmma002 INTO l_fmma002
     FROM fmma_t
    WHERE fmmaent = g_enterprise
      AND fmma001 = g_fmmj_m.fmmj003
   #投資類型之計息方式為1:計息,票面利率必輸
   IF l_fmma002 = '1' THEN
      CALL cl_set_comp_required("fmmj018",TRUE)
   END IF
   #-150611-00020#2--(s)
   #面值不為空時,首次利息支付日必輸
   IF g_fmmj_m.fmmj017 > 0 THEN
      CALL cl_set_comp_required("fmmj024",TRUE)
      IF cl_null(g_fmmj_m.fmmj024) THEN
         LET g_fmmj_m.fmmj024 = g_fmmj_m.fmmj021
      END IF
   END IF
   #-150611-00020#2--(e)

END FUNCTION

PRIVATE FUNCTION afmt530_set_no_required()
   CALL cl_set_comp_required("fmmj012,fmmj013,fmmj018,fmmj024",FALSE)   #150611-00020#2 add fmmj024
END FUNCTION

################################################################################
# Descriptions...: 重新推算金額
# Date & Author..: 15/10/30 By 03538(#151029-00012#1)
# Modify.........:
################################################################################
PRIVATE FUNCTION afmt530_refresh_amt()
   #影響投資本幣金額
   IF INFIELD(fmmjdocdt) OR INFIELD(fmmj001) OR INFIELD(fmmj009) OR
      INFIELD(fmmj005) OR INFIELD(fmmj007) THEN
      LET g_fmmj_m.fmmj028 = g_fmmj_m.fmmj008 * g_fmmj_m.fmmj009
      IF cl_null(g_fmmj_m.fmmj028) THEN LET g_fmmj_m.fmmj028 = 0 END IF         
      CALL s_curr_round_ld('1',g_glaa.glaald,g_glaa.glaa001,g_fmmj_m.fmmj028,2) RETURNING g_sub_success,g_errno,g_fmmj_m.fmmj028
      DISPLAY BY NAME g_fmmj_m.fmmj028
   END IF
   #影響未發放股利本幣金額/未領取利息本幣金額(兩者會相互影響,因此寫在一起)
   IF INFIELD(fmmjdocdt) OR INFIELD(fmmj001) OR INFIELD(fmmj009) OR
      INFIELD(fmmj015) OR INFIELD(fmmj016) THEN
      LET g_fmmj_m.fmmj029 = g_fmmj_m.fmmj015 * g_fmmj_m.fmmj009
      IF cl_null(g_fmmj_m.fmmj029) THEN LET g_fmmj_m.fmmj029 = 0 END IF         
      CALL s_curr_round_ld('1',g_glaa.glaald,g_glaa.glaa001,g_fmmj_m.fmmj029,2) RETURNING g_sub_success,g_errno,g_fmmj_m.fmmj029
      DISPLAY BY NAME g_fmmj_m.fmmj029
      LET g_fmmj_m.fmmj030 = g_fmmj_m.fmmj016 * g_fmmj_m.fmmj009
      IF cl_null(g_fmmj_m.fmmj030) THEN LET g_fmmj_m.fmmj030 = 0 END IF         
      CALL s_curr_round_ld('1',g_glaa.glaald,g_glaa.glaa001,g_fmmj_m.fmmj030,2) RETURNING g_sub_success,g_errno,g_fmmj_m.fmmj030
      DISPLAY BY NAME g_fmmj_m.fmmj030      
   END IF   
   #影響面值本幣金額
   IF INFIELD(fmmjdocdt) OR INFIELD(fmmj001) OR INFIELD(fmmj009) OR INFIELD(fmmj005) OR INFIELD(fmmj007) OR
      INFIELD(fmmj017) THEN
      LET g_fmmj_m.fmmj031 = g_fmmj_m.fmmj017 * g_fmmj_m.fmmj009
      IF cl_null(g_fmmj_m.fmmj031) THEN LET g_fmmj_m.fmmj031 = 0 END IF         
      CALL s_curr_round_ld('1',g_glaa.glaald,g_glaa.glaa001,g_fmmj_m.fmmj031,2) RETURNING g_sub_success,g_errno,g_fmmj_m.fmmj031
      DISPLAY BY NAME g_fmmj_m.fmmj031
   END IF    

END FUNCTION

