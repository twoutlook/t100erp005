# Descriptions...: 質押/解除質押
# Memo...........:
# Usage..........: CALL afmt530_update_fmmj010(p_fmmj010)
# Parameter......: p_fmmj010     目的質押狀態:質押設定Y/解除質押設定:N
# Date & Author..: 160725 By 03538(#160518-00035#12)
# Modify.........:
################################################################################
PRIVATE FUNCTION afmt530_update_fmmj010(p_fmmj010)
DEFINE p_fmmj010      LIKE fmmj_t.fmmj010

   IF NOT cl_null(g_fmmj_m.fmmjdocno) THEN
      #確認狀態下可執行
      SELECT fmmjstus,fmmj010 INTO g_fmmj_m.fmmjstus,g_fmmj_m.fmmj010
        FROM fmmj_t
       WHERE fmmjent = g_enterprise 
         AND fmmjdocno = g_fmmj_m.fmmjdocno  
      DISPLAY BY NAME g_fmmj_m.fmmjstus,g_fmmj_m.fmmj010
      IF g_fmmj_m.fmmjstus = 'Y' AND g_fmmj_m.fmmj010 <> p_fmmj010 THEN
         CALL s_transaction_begin()
         OPEN afmt530_cl USING g_enterprise,g_fmmj_m.fmmjdocno
         IF STATUS THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "OPEN afmt530_cl:" 
            LET g_errparam.code   = 'afm-00101'
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            CLOSE afmt530_cl
            CALL s_transaction_end('N','0')
            RETURN
         ELSE
            UPDATE fmmj_t SET fmmj010 = p_fmmj010
             WHERE fmmjent = g_enterprise 
               AND fmmjdocno = g_fmmj_m.fmmjdocno    
	           IF SQLCA.sqlcode THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "fmmj_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()                    
               CALL s_transaction_end('N','0')                                  
            ELSE
               CALL s_transaction_end('Y','0')
               LET g_fmmj_m.fmmj010 = p_fmmj010
               DISPLAY BY NAME g_fmmj_m.fmmj010           
               CALL afmt530_set_act_visible()
               CALL afmt530_set_act_no_visible()               
            END IF
         END IF
      END IF
   END IF
END FUNCTION

 
