PRIVATE FUNCTION afmt530_update_fmmj027()

   #應先查出單據
   IF cl_null(g_fmmj_m.fmmjdocno) THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = "std-00003"
      LET g_errparam.extend = ""
      LET g_errparam.popup = FALSE
      CALL cl_err()

      RETURN
   END IF
   
   CALL s_transaction_begin()

   INPUT BY NAME g_fmmj_m.fmmj027 ATTRIBUTE(WITHOUT DEFAULTS)
     BEFORE INPUT
        OPEN afmt530_cl USING g_enterprise,g_fmmj_m.fmmjdocno
        IF STATUS THEN
           INITIALIZE g_errparam TO NULL 
           LET g_errparam.extend = "OPEN afmt530_cl:" 
           LET g_errparam.code   = STATUS 
           LET g_errparam.popup  = TRUE 
           CALL cl_err()
           CLOSE afmt530_cl
           CALL s_transaction_end('N','0')
           RETURN
        END IF
        DISPLAY BY NAME g_fmmj_m.fmmj027
        
        NEXT FIELD fmmj027

      AFTER INPUT
         
         UPDATE fmmj_t SET fmmj027 = g_fmmj_m.fmmj027
          WHERE fmmjent = g_enterprise 
			   AND fmmjdocno = g_fmmj_m.fmmjdocno
          
		  IF SQLCA.sqlcode THEN
           INITIALIZE g_errparam TO NULL 
           LET g_errparam.extend = "fmmj_t" 
           LET g_errparam.code   = SQLCA.sqlcode 
           LET g_errparam.popup  = TRUE 
           CALL cl_err()
           CALL s_transaction_end('N','0')
        ELSE
           CALL s_transaction_end('Y','0')        
        END IF
         
      ON ACTION accept
         ACCEPT INPUT
 
      ON ACTION cancel
         LET INT_FLAG = 1
         EXIT INPUT
   END INPUT
   
   IF INT_FLAG THEN
      LET INT_FLAG = 0
      LET g_fmmj_m.* = g_fmmj_m_t.*
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 9001
      LET g_errparam.extend = ''
      LET g_errparam.popup = FALSE
      CALL cl_err()
      DISPLAY BY NAME g_fmmj_m.fmmj027
   END IF
   
END FUNCTION

