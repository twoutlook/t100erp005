       g_fmmj_m.fmmj017,g_fmmj_m.fmmj031,g_fmmj_m.fmmj018,g_fmmj_m.fmmj019,g_fmmj_m.fmmj022,g_fmmj_m.fmmj020, 
       g_fmmj_m.fmmj021,g_fmmj_m.fmmj024,g_fmmj_m.fmmj023,g_fmmj_m.fmmjownid,g_fmmj_m.fmmjowndp,g_fmmj_m.fmmjcrtid, 
       g_fmmj_m.fmmjcrtdp,g_fmmj_m.fmmjcrtdt,g_fmmj_m.fmmjmodid,g_fmmj_m.fmmjmoddt,g_fmmj_m.fmmjcnfid, 
       g_fmmj_m.fmmjcnfdt,g_fmmj_m.fmmjsite_desc,g_fmmj_m.fmmj002_desc,g_fmmj_m.fmmj003_desc,g_fmmj_m.fmmjownid_desc, 
       g_fmmj_m.fmmjowndp_desc,g_fmmj_m.fmmjcrtid_desc,g_fmmj_m.fmmjcrtdp_desc,g_fmmj_m.fmmjmodid_desc, 
       g_fmmj_m.fmmjcnfid_desc
   
   
   #檢查是否允許此動作
