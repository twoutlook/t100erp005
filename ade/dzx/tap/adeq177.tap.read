  <section id="adeq177.fetch" ver="2" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充2
PRIVATE FUNCTION adeq177_fetch()
   DEFINE li_ac           LIKE type_t.num10
   #add-point:fetch段define-標準
   {<point name="fetch.define" edit="s"/>}
   #end add-point
   #add-point:fetch段define-客製
   {<point name="fetch.define_customerization" edit="c"/>}
   #end add-point
   
   CALL g_debv2_d.clear()
 
   CALL g_debv3_d.clear()
 
 
   #add-point:陣列清空
   {<point name="fetch.array_clear" />}
   #end add-point
   
   LET li_ac = l_ac 
   
   #為避免影響執行效能，若是按上下筆就不重組SQL
   IF g_action_choice <> "fetch" OR cl_null(g_action_choice) THEN
      LET g_sql = "SELECT  UNIQUE dedo005,'',dedo006,'',dedo007,dedo008,dedo012,dedo017,dedo018,dedo019, 
          dedo013,dedo014,dedo015,dedo016,dedo022 FROM dedo_t",    
                  "",
                  " WHERE dedoent=? AND dedosite=? AND dedo020=? AND dedo021=?"
  
      IF NOT cl_null(g_wc2_table2) THEN
         LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
      END IF
  
      LET g_sql = g_sql, " ORDER BY dedo_t.dedo005,dedo_t.dedo006" 
                         
      #add-point:單身填充前
      {<point name="fetch.before_fill2" />}
      #end add-point
 
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料   
      PREPARE adeq177_pb2 FROM g_sql
      DECLARE b_fill_curs2 CURSOR FOR adeq177_pb2
   END IF
 
   OPEN b_fill_curs2 USING g_enterprise,g_debv_d[g_detail_idx].debvsite
                                  ,g_debv_d[g_detail_idx].debv002
 
                                  ,g_debv_d[g_detail_idx].debv003
 
 
   LET l_ac = 1
   FOREACH b_fill_curs2 INTO g_debv2_d[l_ac].dedo005,g_debv2_d[l_ac].dedo005_desc,g_debv2_d[l_ac].dedo006, 
       g_debv2_d[l_ac].dedo006_desc,g_debv2_d[l_ac].dedo007,g_debv2_d[l_ac].dedo008,g_debv2_d[l_ac].dedo012, 
       g_debv2_d[l_ac].dedo017,g_debv2_d[l_ac].dedo018,g_debv2_d[l_ac].dedo019,g_debv2_d[l_ac].dedo013, 
       g_debv2_d[l_ac].dedo014,g_debv2_d[l_ac].dedo015,g_debv2_d[l_ac].dedo016,g_debv2_d[l_ac].dedo022 

      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      
 
      #add-point:b_fill段資料填充
      {<point name="fetch.fill2"/>}
      #end add-point
      
      CALL adeq177_detail_show("'2'")      
 
      CALL adeq177_dedo_t_mask()
 
      IF l_ac > g_max_rec THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend =  "" 
         LET g_errparam.code   =  9035 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   #為避免影響執行效能，若是按上下筆就不重組SQL
