  <section id="adeq177.b_fill" ver="2" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION adeq177_b_fill()
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準
   {<point name="b_fill.define" edit="s"/>}
   #end add-point
   #add-point:b_fill段define-客製
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   LET ls_sql_rank = "SELECT  UNIQUE debvsite,'',debv001,debv002,debv003,debv012,debv013,debv014,debv015, 
       debv016,debv017,debv029,debv030,debv032,debv033,debv031,debv018,debv019,debv020,debv034,debv027, 
       debv028,debv025,debv026  ,DENSE_RANK() OVER( ORDER BY debv_t.debvsite,debv_t.debv002,debv_t.debv003) AS RANK FROM debv_t", 

 
                     " LEFT JOIN dedo_t ON dedoent = debvent AND debvsite = dedosite AND debv002 = dedo020 AND debv003 = dedo021",
 
                     " LEFT JOIN dedp_t ON dedpent = debvent AND debvsite = dedpsite AND debv002 = dedp008 AND debv003 = dedp009",
 
 
                     "",
                     " WHERE debvent= ? AND 1=1 AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("debv_t"),
                     " ORDER BY debv_t.debvsite,debv_t.debv002,debv_t.debv003"
 
   #add-point:b_fill段rank_sql_after
   {<point name="b_fill.rank_sql_after"/>}
   #end add-point
 
   LET g_sql = "SELECT COUNT(*) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT debvsite,'',debv001,debv002,debv003,debv012,debv013,debv014,debv015,debv016,debv017, 
       debv029,debv030,debv032,debv033,debv031,debv018,debv019,debv020,debv034,debv027,debv028,debv025, 
       debv026",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE adeq177_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR adeq177_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_debv_d.clear()
 
   #add-point:陣列清空
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1   
 
   FOREACH b_fill_curs INTO g_debv_d[l_ac].debvsite,g_debv_d[l_ac].debvsite_desc,g_debv_d[l_ac].debv001, 
       g_debv_d[l_ac].debv002,g_debv_d[l_ac].debv003,g_debv_d[l_ac].debv012,g_debv_d[l_ac].debv013,g_debv_d[l_ac].debv014, 
       g_debv_d[l_ac].debv015,g_debv_d[l_ac].debv016,g_debv_d[l_ac].debv017,g_debv_d[l_ac].debv029,g_debv_d[l_ac].debv030, 
       g_debv_d[l_ac].debv032,g_debv_d[l_ac].debv033,g_debv_d[l_ac].debv031,g_debv_d[l_ac].debv018,g_debv_d[l_ac].debv019, 
       g_debv_d[l_ac].debv020,g_debv_d[l_ac].debv034,g_debv_d[l_ac].debv027,g_debv_d[l_ac].debv028,g_debv_d[l_ac].debv025, 
       g_debv_d[l_ac].debv026
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #LET g_debv_d[l_ac].statepic = cl_get_actipic(g_debv_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL adeq177_detail_show("'1'")      
 
      CALL adeq177_debv_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
   LET g_error_show = 0
   
 
   
   CALL g_debv_d.deleteElement(g_debv_d.getLength())   
 
   #add-point:陣列長度調整
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = g_debv_d.getLength()
#  DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE adeq177_pb
 
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL adeq177_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL adeq177_detail_action_trans()
 
   IF g_debv_d.getLength() > 0 THEN
      LET l_ac = 1
      CALL adeq177_fetch()
   END IF
   
      CALL adeq177_filter_show('debvsite','b_debvsite')
   CALL adeq177_filter_show('debv001','b_debv001')
   CALL adeq177_filter_show('debv002','b_debv002')
   CALL adeq177_filter_show('debv003','b_debv003')
   CALL adeq177_filter_show('debv012','b_debv012')
