   CALL adeq177_filter_show('debv030','b_debv030')
   CALL adeq177_filter_show('debv032','b_debv032')
   CALL adeq177_filter_show('debv033','b_debv033')
   CALL adeq177_filter_show('debv031','b_debv031')
   CALL adeq177_filter_show('debv018','b_debv018')
   CALL adeq177_filter_show('debv019','b_debv019')
   CALL adeq177_filter_show('debv020','b_debv020')
   CALL adeq177_filter_show('debv034','b_debv034')
   CALL adeq177_filter_show('debv027','b_debv027')
   CALL adeq177_filter_show('debv028','b_debv028')
