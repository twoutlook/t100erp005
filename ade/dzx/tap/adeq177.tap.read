  <point name="fetch.before_fill2" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[     #效能调整
      LET g_sql = "SELECT UNIQUE dedo005, t1.mmanl003, dedo006, t2.oocql004, dedo007, dedo008, ",
                  "              dedo012,dedo017,dedo018,dedo019, ",  
                  "              dedo013,dedo014,dedo015,dedo016,dedo022  ",  #150705-00001#8 Add By Ken 150706 加上客單價(dedo022)
                  "  FROM dedo_t ",   
                  "       LEFT JOIN mmanl_t t1 ON t1.mmanlent = dedoent AND t1.mmanl001 = dedo005 AND t1.mmanl002 = '",g_dlang,"' ",
                  "       LEFT JOIN (SELECT oocqlent,oocql002,oocql004 ",
                  "                    FROM oocql_t,oocq_t ",
                  "                   WHERE oocqlent = oocqent AND oocql001 = oocq004 AND oocql003 = '",g_dlang,"' ",
                  "                     AND oocq001 = '2049'   AND oocq002 = '09') t2 ",
                  "              ON t2.oocqlent = dedoent AND t2.oocql002 = dedo006 ",
                  " WHERE dedoent = ? AND dedosite = ? AND dedo002 = ? "
  
      IF NOT cl_null(g_wc2_table2) THEN
         LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
      END IF
  
      LET g_sql = g_sql, " ORDER BY dedo_t.dedo005,dedo_t.dedo006" ]]>
