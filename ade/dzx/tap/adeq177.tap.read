  <point name="b_fill.fill" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[                 LET g_sql = " SELECT dedp006,dedp005,ooial003,SUM(dedp007) ",
                  "   FROM (SELECT UNIQUE debvent,debvsite,debv002,debv003,  ",
                  "            dedp006,dedp005,dedp007,ooial003 ",
                  "          FROM debv_t",
                  "               LEFT JOIN dedo_t ON dedoent = debvent AND dedosite = debvsite ",
                  "                      AND dedo020 = debv002 AND dedo021 = debv003",
                  "               LEFT JOIN  dedp_t on dedpent = debvent AND dedpsite = debvsite ",
                  "                      AND dedp008 = debv002 AND dedp009 = debv003 ",
                  "               LEFT JOIN ooial_t t1 ON t1.ooialent = dedpent ",
                  "                       AND t1.ooial001 = dedp005 ",
                  "                       AND t1.ooial002 = '",g_dlang,"' ",
                  "         WHERE debvent = ? AND ", ls_wc,")",
                  " GROUP BY dedp006,dedp005,ooial003",
                  " ORDER BY dedp006,dedp005"
      #150302-00004#8 150312 by lori522612 add 效能調整---(E)    
      PREPARE adeq177_pb1 FROM g_sql
      DECLARE b_fill_curs1 CURSOR FOR adeq177_pb1]]>
  </point>
  <point name="b_fill.sql_after" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   #效能調整
   LET g_sql = "SELECT UNIQUE       debvsite,    t1.ooefl003, debv001,    debv002,  debv003, ",
               "       debv012,     debv013,     debv014,     debv015, ",
               "       debv016,     debv017,     debv029,     debv030,    debv032,  debv033,  debv031,",   
               "       debv018,     debv019,     debv020,     debv034,    debv027 ",  #150705-00001#8 Add By Ken 150706 加上客單價(debv034)
               "       debv028,     debv025,     debv026 ",
               "  FROM debv_t",  
               "       LEFT JOIN ooefl_t t1 ON t1.ooeflent = debvent AND t1.ooefl001 = debvsite AND t1.ooefl002 = '",g_dlang,"' ",
               "        LEFT JOIN dedo_t ON dedoent = debvent AND debvsite = dedosite AND debv002 = dedo020 AND debv003 = dedo021",
               "        LEFT JOIN dedp_t ON dedpent = debvent AND debvsite = dedpsite AND debv002 = dedp008 AND debv003 = dedp009",
               " WHERE debvent = ? ",
               "   AND ", ls_wc,cl_sql_add_filter("debv_t")
    
   LET g_sql = g_sql, cl_sql_add_filter("debv_t"),
                      " ORDER BY debv_t.debvsite,debv_t.debv002 "]]>
  </point>
