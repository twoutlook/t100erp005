   CALL adeq177_filter_show('debv014','b_debv014')
   CALL adeq177_filter_show('debv015','b_debv015')
   CALL adeq177_filter_show('debv016','b_debv016')
   CALL adeq177_filter_show('debv017','b_debv017')
