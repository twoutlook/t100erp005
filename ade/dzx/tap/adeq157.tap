         CALL cl_err()
 
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
 
 
   
   #add-point:單身填充後
   {<point name="fetch.after_fill" />}
   #end add-point 
   
   CALL g_decq2_d.deleteElement(g_decq2_d.getLength())   
 
   #單身總筆數顯示
   LET g_detail_cnt2 = g_decq2_d.getLength()
   DISPLAY g_detail_cnt2 TO FORMONLY.cnt
