      PREPARE adeq157_pb3 FROM g_sql
      DECLARE b_fill_curs3 CURSOR FOR adeq157_pb3
   END IF
 
   OPEN b_fill_curs3 USING g_enterprise,g_decq_d[g_detail_idx].decqsite
                                  ,g_decq_d[g_detail_idx].decq025
 
                                  ,g_decq_d[g_detail_idx].decq026
 
 
   LET l_ac = 1
   FOREACH b_fill_curs3 INTO g_decq3_d[l_ac].decs006,g_decq3_d[l_ac].decs005,g_decq3_d[l_ac].decs005_desc, 
       g_decq3_d[l_ac].decs007
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      
 
      #add-point:b_fill段資料填充
      {<point name="fetch.fill3"/>}
