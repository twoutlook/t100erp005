  <section id="adeq157.b_fill" ver="7" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION adeq157_b_fill()
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準
   {<point name="b_fill.define" edit="s"/>}
   #end add-point
   #add-point:b_fill段define-客製
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   LET ls_sql_rank = "SELECT  UNIQUE decqsite,'',decq001,decq025,decq026,decq005,decq006,decq007,decq008, 
       decq009,decq010,decq011,decq012,decq023,decq024,decq013,decq014,decq015,decq016,decq027,decq019, 
       decq020,decq017,decq018  ,DENSE_RANK() OVER( ORDER BY decq_t.decqsite,decq_t.decq025,decq_t.decq026) AS RANK FROM decq_t", 

 
                     " LEFT JOIN decr_t ON decrent = decqent AND decqsite = decrsite AND decq025 = decr020 AND decq026 = decr021",
 
                     " LEFT JOIN decs_t ON decsent = decqent AND decqsite = decssite AND decq025 = decs008 AND decq026 = decs009",
 
 
                     "",
                     " WHERE decqent= ? AND 1=1 AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("decq_t"),
                     " ORDER BY decq_t.decqsite,decq_t.decq025,decq_t.decq026"
 
   #add-point:b_fill段rank_sql_after
   {<point name="b_fill.rank_sql_after"/>}
   #end add-point
 
   LET g_sql = "SELECT COUNT(*) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT decqsite,'',decq001,decq025,decq026,decq005,decq006,decq007,decq008,decq009,decq010, 
       decq011,decq012,decq023,decq024,decq013,decq014,decq015,decq016,decq027,decq019,decq020,decq017, 
       decq018",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE adeq157_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR adeq157_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_decq_d.clear()
 
   #add-point:陣列清空
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1   
 
   FOREACH b_fill_curs INTO g_decq_d[l_ac].decqsite,g_decq_d[l_ac].decqsite_desc,g_decq_d[l_ac].decq001, 
       g_decq_d[l_ac].decq025,g_decq_d[l_ac].decq026,g_decq_d[l_ac].decq005,g_decq_d[l_ac].decq006,g_decq_d[l_ac].decq007, 
       g_decq_d[l_ac].decq008,g_decq_d[l_ac].decq009,g_decq_d[l_ac].decq010,g_decq_d[l_ac].decq011,g_decq_d[l_ac].decq012, 
       g_decq_d[l_ac].decq023,g_decq_d[l_ac].decq024,g_decq_d[l_ac].decq013,g_decq_d[l_ac].decq014,g_decq_d[l_ac].decq015, 
       g_decq_d[l_ac].decq016,g_decq_d[l_ac].decq027,g_decq_d[l_ac].decq019,g_decq_d[l_ac].decq020,g_decq_d[l_ac].decq017, 
       g_decq_d[l_ac].decq018
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #LET g_decq_d[l_ac].statepic = cl_get_actipic(g_decq_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL adeq157_detail_show("'1'")      
 
      CALL adeq157_decq_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
   LET g_error_show = 0
   
 
   
   CALL g_decq_d.deleteElement(g_decq_d.getLength())   
 
   #add-point:陣列長度調整
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = g_decq_d.getLength()
#  DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE adeq157_pb
 
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL adeq157_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL adeq157_detail_action_trans()
 
   IF g_decq_d.getLength() > 0 THEN
      LET l_ac = 1
      CALL adeq157_fetch()
   END IF
   
      CALL adeq157_filter_show('decqsite','b_decqsite')
   CALL adeq157_filter_show('decq001','b_decq001')
   CALL adeq157_filter_show('decq025','b_decq025')
   CALL adeq157_filter_show('decq026','b_decq026')
   CALL adeq157_filter_show('decq005','b_decq005')
   CALL adeq157_filter_show('decq006','b_decq006')
   CALL adeq157_filter_show('decq007','b_decq007')
   CALL adeq157_filter_show('decq008','b_decq008')
   CALL adeq157_filter_show('decq009','b_decq009')
   CALL adeq157_filter_show('decq010','b_decq010')
   CALL adeq157_filter_show('decq011','b_decq011')
   CALL adeq157_filter_show('decq012','b_decq012')
   CALL adeq157_filter_show('decq023','b_decq023')
   CALL adeq157_filter_show('decq024','b_decq024')
   CALL adeq157_filter_show('decq013','b_decq013')
   CALL adeq157_filter_show('decq014','b_decq014')
   CALL adeq157_filter_show('decq015','b_decq015')
   CALL adeq157_filter_show('decq016','b_decq016')
   CALL adeq157_filter_show('decq027','b_decq027')
   CALL adeq157_filter_show('decq019','b_decq019')
   CALL adeq157_filter_show('decq020','b_decq020')
   CALL adeq157_filter_show('decq017','b_decq017')
   CALL adeq157_filter_show('decq018','b_decq018')
   CALL adeq157_filter_show('decr005','b_decr005')
   CALL adeq157_filter_show('decr006','b_decr006')
   CALL adeq157_filter_show('decr007','b_decr007')
   CALL adeq157_filter_show('decr008','b_decr008')
   CALL adeq157_filter_show('decr012','b_decr012')
   CALL adeq157_filter_show('decr017','b_decr017')
   CALL adeq157_filter_show('decr018','b_decr018')
   CALL adeq157_filter_show('decr019','b_decr019')
   CALL adeq157_filter_show('decr013','b_decr013')
   CALL adeq157_filter_show('decr014','b_decr014')
   CALL adeq157_filter_show('decr015','b_decr015')
   CALL adeq157_filter_show('decr016','b_decr016')
   CALL adeq157_filter_show('decr022','b_decr022')
   CALL adeq157_filter_show('decs006','b_decs006')
   CALL adeq157_filter_show('decs005','b_decs005')
   CALL adeq157_filter_show('decs007','b_decs007')
 
 
   #add-point:b_fill段結束前
   {<point name="b_fill.after"/>}
   #end add-point
 
END FUNCTION
]]>
