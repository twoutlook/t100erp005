  <point name="b_fill.fill" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[      LET g_sql = " SELECT decs006,decs005,ooial003,SUM(decs007) ",
                  "   FROM (SELECT UNIQUE decqent,decqsite,decq025,decq026, ",
                  "                       decs006,decs005,decs007,ooial003 ",
                  "          FROM decq_t",
                  "               LEFT JOIN decr_t ON decrent = decqent AND decrsite = decqsite AND decr020 = decq025 AND decr021 = decq026 ",
                  "               LEFT JOIN (SELECT decsent,decssite,decs008,decs009,decs006,decs005,decs007,t1.ooial003 ",
                  "                            FROM decs_t ",
                  "                                 LEFT JOIN ooial_t t1 ON t1.ooialent = decsent ",
                  "                                                     AND t1.ooial001 = decs005 ",
                  "                                                     AND t1.ooial002 = '",g_dlang,"') ",
                  "                      ON decsent = decqent AND decssite = decqsite AND decs008 = decq025 AND decs009 = decq026 ", 
                  "         WHERE decqent = ? AND ", ls_wc,")",
                  " GROUP BY decs006,decs005,ooial003",
                  " ORDER BY decs006,decs005"
      PREPARE adeq157_pb1 FROM g_sql
      DECLARE b_fill_curs1 CURSOR FOR adeq157_pb1      ]]>
  </point>
