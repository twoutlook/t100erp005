   CALL g_decq3_d.clear()
 
 
   #add-point:陣列清空
   {<point name="fetch.array_clear" />}
   #end add-point
   
   LET li_ac = l_ac 
   
   #為避免影響執行效能，若是按上下筆就不重組SQL
   IF g_action_choice <> "fetch" OR cl_null(g_action_choice) THEN
      LET g_sql = "SELECT  UNIQUE decr005,'',decr006,'',decr007,decr008,decr012,decr017,decr018,decr019, 
          decr013,decr014,decr015,decr016,decr022 FROM decr_t",    
                  "",
                  " WHERE decrent=? AND decrsite=? AND decr020=? AND decr021=?"
  
      IF NOT cl_null(g_wc2_table2) THEN
         LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
      END IF
  
      LET g_sql = g_sql, " ORDER BY decr_t.decr005,decr_t.decr006" 
                         
      #add-point:單身填充前
      {<point name="fetch.before_fill2" />}
      #end add-point
 
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料   
      PREPARE adeq157_pb2 FROM g_sql
      DECLARE b_fill_curs2 CURSOR FOR adeq157_pb2
   END IF
 
   OPEN b_fill_curs2 USING g_enterprise,g_decq_d[g_detail_idx].decqsite
                                  ,g_decq_d[g_detail_idx].decq025
 
                                  ,g_decq_d[g_detail_idx].decq026
 
 
   LET l_ac = 1
   FOREACH b_fill_curs2 INTO g_decq2_d[l_ac].decr005,g_decq2_d[l_ac].decr005_desc,g_decq2_d[l_ac].decr006, 
       g_decq2_d[l_ac].decr006_desc,g_decq2_d[l_ac].decr007,g_decq2_d[l_ac].decr008,g_decq2_d[l_ac].decr012, 
