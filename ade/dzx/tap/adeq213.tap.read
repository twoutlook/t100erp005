  <point name="function.adeq213_create_tmp" order="1" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[# 创建临时表   #20150619 add lujh
PRIVATE FUNCTION adeq213_create_tmp()
   
   DROP TABLE adeq213_tmp;
   CREATE TEMP TABLE adeq213_tmp(
   sel               LIKE type_t.chr1, 
   deagsite          LIKE deag_t.deagsite, 
   deagsite_desc     LIKE type_t.chr500, 
   deagdocdt         LIKE deag_t.deagdocdt, 
   deagdocno         LIKE deag_t.deagdocno, 
   deag004           LIKE deag_t.deag004, 
   deag004_desc      LIKE type_t.chr500, 
   pcab002           LIKE pcab_t.pcab002, 
   pcab002_desc      LIKE type_t.chr500, 
   deaf012           LIKE deaf_t.deaf012, 
   deaf005           LIKE deaf_t.deaf005, 
   deaf005_desc      LIKE type_t.chr500, 
   deaf006           LIKE deaf_t.deaf006, 
   deaf007           LIKE deaf_t.deaf007, 
   deaf015           LIKE deaf_t.deaf015, 
   deaf008           LIKE deaf_t.deaf008, 
   deaf009           LIKE deaf_t.deaf009, 
   deaf016           LIKE deaf_t.deaf016, 
   deaf017           LIKE deaf_t.deaf017, 
   deaf017_desc      LIKE type_t.chr500, 
   deaf018           LIKE deaf_t.deaf018, 
   deaf010           LIKE deaf_t.deaf010, 
   apca113           LIKE apca_t.apca113, 
   deafdocno         LIKE deaf_t.deafdocno   
   );
   
END FUNCTION]]>
  </point>
  <point name="function.adeq213_get_data" order="2" ver="2" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[# 抓取资料   #20150619 add lujh
PRIVATE FUNCTION adeq213_get_data()
   DEFINE l_tmp           RECORD
                          sel               LIKE type_t.chr1, 
                          deagsite          LIKE deag_t.deagsite, 
                          deagsite_desc     LIKE type_t.chr500, 
                          deagdocdt         LIKE deag_t.deagdocdt, 
                          deagdocno         LIKE deag_t.deagdocno, 
                          deag004           LIKE deag_t.deag004, 
                          deag004_desc      LIKE type_t.chr500, 
                          pcab002           LIKE pcab_t.pcab002, 
                          pcab002_desc      LIKE type_t.chr500, 
                          deaf012           LIKE deaf_t.deaf012, 
                          deaf005           LIKE deaf_t.deaf005, 
                          deaf005_desc      LIKE type_t.chr500, 
                          deaf006           LIKE deaf_t.deaf006, 
                          deaf007           LIKE deaf_t.deaf007, 
                          deaf015           LIKE deaf_t.deaf015, 
                          deaf008           LIKE deaf_t.deaf008, 
                          deaf009           LIKE deaf_t.deaf009, 
                          deaf016           LIKE deaf_t.deaf016, 
                          deaf017           LIKE deaf_t.deaf017, 
                          deaf017_desc      LIKE type_t.chr500, 
                          deaf018           LIKE deaf_t.deaf018, 
                          deaf010           LIKE deaf_t.deaf010, 
                          apca113           LIKE apca_t.apca113, 
                          deafdocno         LIKE deaf_t.deafdocno  
                          END RECORD
   DEFINE l_sql           STRING
   DEFINE l_pcab001       LIKE pcab_t.pcab001
   DEFINE l_pcab002       LIKE pcab_t.pcab002
   DEFINE l_pcab002_t     LIKE pcab_t.pcab002
   DEFINE ls_wc           STRING
   DEFINE l_where         STRING
   DEFINE l_where1        STRING
   DEFINE ls_sql_rank     STRING
   DEFINE l_cnt           LIKE type_t.num5
   DEFINE l_ooag001_str   STRING
   DEFINE l_ooag001_str_1 STRING
   DEFINE l_wc_ooag001    STRING  
   DEFINE l_ooag001       LIKE ooag_t.ooag001
   
   
   DELETE FROM adeq213_tmp

   LET g_wc_deag004 = cl_replace_str(g_wc_deag004,'deag004','pcab001')
   LET g_wc_apcadocdt = cl_replace_str(g_wc_deagdocdt,'deagdocdt','apcadocdt')
   LET l_wc_ooag001 = cl_replace_str(g_wc_pcab002,'pcab002','ooag001')
   
   IF cl_null(g_wc_deag004) THEN
      LET g_wc_deag004 = " 1=1 "
   END IF
   
   IF cl_null(g_wc_deagdocdt) THEN
      LET g_wc_deagdocdt = " 1=1 "
   END IF
   
   IF cl_null(g_wc_apcadocdt) THEN
      LET g_wc_apcadocdt = " 1=1 "
   END IF
   
   IF cl_null(g_wc_pcab002) THEN
      LET g_wc_pcab002 = " 1=1 "
   END IF
   
   IF cl_null(l_wc_ooag001) THEN
      LET l_wc_ooag001 = " 1=1 "
   END IF
   
   CALL s_aooi500_sql_where(g_prog,'deafsite') RETURNING l_where
   CALL s_aooi500_sql_where(g_prog,'apcasite') RETURNING l_where1
   
   LET l_ooag001_str = ''
   
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter CLIPPED," AND ",l_where CLIPPED," AND ",g_wc_deagdocdt
   
   #循环apci202收银员
   LET l_sql = "SELECT pcab001,pcab002 ",
               "  FROM pcab_t ",
               " WHERE pcabent = '",g_enterprise,"'",
               "   AND pcabstus = 'Y' ",
               "   AND ",g_wc_deag004,
               "   AND ",g_wc_pcab002               
   
   PREPARE adeq213_pcab_pre FROM l_sql
   DECLARE adeq213_pcab_cs CURSOR FOR adeq213_pcab_pre
   
   #抓取收银员差错资料
   LET ls_sql_rank = "SELECT  UNIQUE 'N',deagsite,ooefl003 deagsite_desc,deagdocdt,deagdocno,deag004 ,",
                     "               pcab003 deag004_desc,pcab002,t3.ooag011 pcab002_desc,deaf012,",
                     "               deaf005,t1.ooial003 deaf005_desc,deaf006,deaf007,deaf015,",  
                     "               deaf008,deaf009,deaf016,deaf017,t2.ooial003 deaf017_desc,deaf018,",
                     "               deaf010,0,deafdocno,   ",  
                     " DENSE_RANK() OVER( ORDER BY deag_t.deagsite,deag_t.deagdocdt,deag_t.deagdocno,deaf_t.deaf005) AS RANK ", 
                     " FROM deag_t,deaf_t ",
                     " LEFT JOIN ooefl_t ON deafent=ooeflent  AND deafsite=ooefl001 AND ooefl002= '"||g_dlang||"'  ",
                     " LEFT JOIN pcab_t ON deafent=pcabent  AND deaf004=pcab001                                    ",
                     " LEFT JOIN ooial_t t1 ON deafent=t1.ooialent  AND deaf005=t1.ooial001 AND t1.ooial002= '"||g_dlang||"'",
                     " LEFT JOIN ooial_t t2 ON deafent=t2.ooialent  AND deaf017=t2.ooial001 AND t2.ooial002= '"||g_dlang||"'",
                     " LEFT JOIN ooag_t t3 ON t3.ooagent='"||g_enterprise||"' AND t3.ooag001 = pcab002  ", 
                     " WHERE deafent= '",g_enterprise,"'",
                     "   AND deagent = deafent ", 
                     "   AND deagdocno = deafdocno ",
                     "   AND deagstus = 'Y'  ",
                     "   AND deag004 = ? ",
                     #"   AND deaf009 IS NOT NULL "   #150703-00013#1 Mark By Ken 150703 把原本只要顯示有差錯調整的條件拿掉，改全部顯示
                     "   AND 1=1 AND ", ls_wc 
                     
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("deaf_t"),
                     " ORDER BY deag_t.deagsite,deag_t.deagdocdt,deag_t.deagdocno,deaf_t.deaf005,pcab_t.pcab002 "
                     
   PREPARE adeq213_deag_pre FROM ls_sql_rank
   DECLARE adeq213_deag_cs CURSOR FOR adeq213_deag_pre
   
   #抓取员工报支单资料
   LET l_sql = "SELECT  UNIQUE 'N',apcasite,ooefl003,apcadocdt,'','','',",
               "               apca057,t1.ooag011,'','','',0,0,0,",  
               "               0,'','','','','', '',0,''  ",   
               " FROM apca_t ",
               " LEFT JOIN ooefl_t ON apcaent=ooeflent  AND apcasite=ooefl001 AND ooefl002= '"||g_dlang||"'  ",
               " LEFT JOIN ooag_t t1 ON t1.ooagent='"||g_enterprise||"' AND t1.ooag001 = apca057  ", 
               " WHERE apcaent= '",g_enterprise,"'",
               "   AND apca001 = '15' ",
               "   AND apcastus = 'Y' ",
               "   AND apca057 = ? ",
               "   AND ",l_where1,
               "   AND ",g_wc_apcadocdt
               
   PREPARE adeq213_apca_pre FROM l_sql
   DECLARE adeq213_apca_cs CURSOR FOR adeq213_apca_pre
   
   FOREACH adeq213_pcab_cs INTO l_pcab001,l_pcab002
   
      #计算收银员差错资料笔数
      LET l_sql = "SELECT COUNT(*) FROM (",ls_sql_rank,")"
      PREPARE adeq213_cnt_pre FROM l_sql
      EXECUTE adeq213_cnt_pre USING l_pcab001 INTO l_cnt
      FREE adeq213_cnt_pre   
   
      IF l_cnt > 0 THEN   #有收银员差错资料,将差错资料插入临时表
         FOREACH adeq213_deag_cs USING l_pcab001 INTO l_tmp.*

            INSERT INTO adeq213_tmp VALUES(l_tmp.*)
            
            IF cl_null(l_ooag001_str_1) THEN 
               LET l_ooag001_str_1 = "'",l_pcab002,"'"
            ELSE
               LET l_ooag001_str_1 = l_ooag001_str_1,",","'",l_pcab002,"'"
            END IF

         END FOREACH
      ELSE   #若没有收银员差错资料,查询员工报支单的资料
         FOREACH adeq213_apca_cs USING l_pcab002 INTO l_tmp.*
         
            SELECT pcab001,pcab003 INTO l_tmp.deag004,l_tmp.deag004_desc
              FROM pcab_t
             WHERE pcabent = g_enterprise
               AND pcab002 = l_pcab002
              
            INSERT INTO adeq213_tmp VALUES(l_tmp.*)

            IF cl_null(l_ooag001_str) THEN 
               LET l_ooag001_str = "'",l_pcab002,"'"
            ELSE
               LET l_ooag001_str = l_ooag001_str,",","'",l_pcab002,"'"
            END IF
         END FOREACH 
      END IF
   END FOREACH 

   #循环员工编号
   LET l_sql = "SELECT ooag001 ",
               "  FROM ooag_t ",
               " WHERE ooagent = '",g_enterprise,"'",
               "   AND ooagstus = 'Y' ",
               "   AND ",l_wc_ooag001
   IF NOT cl_null(l_ooag001_str) THEN 
      LET l_sql = l_sql," AND ooag001 NOT IN (",l_ooag001_str,")"
   END IF
   
   IF NOT cl_null(l_ooag001_str_1) THEN 
      LET l_sql = l_sql," AND ooag001 NOT IN (",l_ooag001_str_1,")"
   END IF
               
   PREPARE adeq213_ooag_pre FROM l_sql
   DECLARE adeq213_ooag_cs CURSOR FOR adeq213_ooag_pre   

   FOREACH adeq213_ooag_cs INTO l_ooag001
      FOREACH adeq213_apca_cs USING l_ooag001 INTO l_tmp.*
         
         SELECT pcab001,pcab003 INTO l_tmp.deag004,l_tmp.deag004_desc
           FROM pcab_t
          WHERE pcabent = g_enterprise
            AND pcab002 = l_ooag001
           
         INSERT INTO adeq213_tmp VALUES(l_tmp.*)
       
      END FOREACH 
   END FOREACH 


END FUNCTION]]>
  </point>
  <point name="function.adeq213_b_fill1" order="3" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[# 單身陣列填充   #20150619 add lujh
PRIVATE FUNCTION adeq213_b_fill1()
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準
   DEFINE l_where         STRING
   DEFINE l_docdt_t       LIKE deag_t.deagdocdt  
   DEFINE l_pcab002_t     LIKE pcab_t.pcab002  
   #end add-point
   #add-point:b_fill段define-客製

   #end add-point
 
   #add-point:b_fill段sql_before
   
   CALL adeq213_create_tmp()
   CALL adeq213_get_data()
   #end add-point
 
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT '','','','','','','','','',deaf012,deaf005,'',deaf006,deaf007,deaf015,deaf008, 
       deaf009,deaf016,deaf017,'',deaf018,deaf010,'',deafdocno",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after
         
   LET g_sql = "SELECT * FROM adeq213_tmp "
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE adeq213_pb1 FROM g_sql
   DECLARE b_fill_curs1 CURSOR FOR adeq213_pb1
   
   OPEN b_fill_curs1 
 
   CALL g_deaf_d.clear()
 
   #add-point:陣列清空

   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1   
 
   FOREACH b_fill_curs1 INTO g_deaf_d[l_ac].sel,g_deaf_d[l_ac].deagsite,g_deaf_d[l_ac].deagsite_desc, 
       g_deaf_d[l_ac].deagdocdt,g_deaf_d[l_ac].deagdocno,g_deaf_d[l_ac].deag004,g_deaf_d[l_ac].deag004_desc, 
       g_deaf_d[l_ac].pcab002,g_deaf_d[l_ac].pcab002_desc,g_deaf_d[l_ac].deaf012,g_deaf_d[l_ac].deaf005, 
       g_deaf_d[l_ac].deaf005_desc,g_deaf_d[l_ac].deaf006,g_deaf_d[l_ac].deaf007,g_deaf_d[l_ac].deaf015, 
       g_deaf_d[l_ac].deaf008,g_deaf_d[l_ac].deaf009,g_deaf_d[l_ac].deaf016,g_deaf_d[l_ac].deaf017,g_deaf_d[l_ac].deaf017_desc, 
       g_deaf_d[l_ac].deaf018,g_deaf_d[l_ac].deaf010,g_deaf_d[l_ac].apca113,g_deaf_d[l_ac].deafdocno 

      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #LET g_deaf_d[l_ac].statepic = cl_get_actipic(g_deaf_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充
      #20150618--add--str--lujh
      IF cl_null(l_docdt_t) OR cl_null(l_pcab002_t) OR l_docdt_t <> g_deaf_d[l_ac].deagdocdt OR l_pcab002_t <> g_deaf_d[l_ac].pcab002 THEN 
         SELECT SUM(apca113) INTO g_deaf_d[l_ac].apca113
           FROM apca_t
          WHERE apcaent = g_enterprise
            AND apca001 = '15'
            AND apca057 = g_deaf_d[l_ac].pcab002
            AND apcadocdt = g_deaf_d[l_ac].deagdocdt
            AND apcastus = 'Y'
            
         IF cl_null(g_deaf_d[l_ac].apca113) THEN 
            LET g_deaf_d[l_ac].apca113 = 0
         END IF
      END IF
      #20150618--add--end--lujh
      
      LET l_docdt_t = g_deaf_d[l_ac].deagdocdt
      LET l_pcab002_t = g_deaf_d[l_ac].pcab002
      #end add-point
 
      CALL adeq213_detail_show("'1'")      
 
      CALL adeq213_deaf_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
   LET g_error_show = 0
   
 
   
   CALL g_deaf_d.deleteElement(g_deaf_d.getLength())   
 
   #add-point:陣列長度調整

   #end add-point
   
   #add-point:b_fill段資料填充(其他單身)

   #end add-point
 
   LET g_detail_cnt = g_deaf_d.getLength()
#  DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs1
   FREE adeq213_pb1
 
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL adeq213_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL adeq213_detail_action_trans()
 
   IF g_deaf_d.getLength() > 0 THEN
      LET l_ac = 1
      CALL adeq213_fetch()
   END IF
   
   CALL adeq213_filter_show('deagsite','b_deagsite')
   CALL adeq213_filter_show('deagdocdt','b_deagdocdt')
   CALL adeq213_filter_show('deagdocno','b_deagdocno')
   CALL adeq213_filter_show('deag004','b_deag004')
   CALL adeq213_filter_show('pcab002','b_pcab002')
   CALL adeq213_filter_show('deaf012','b_deaf012')
   CALL adeq213_filter_show('deaf005','b_deaf005')
   CALL adeq213_filter_show('deaf006','b_deaf006')
   CALL adeq213_filter_show('deaf007','b_deaf007')
   CALL adeq213_filter_show('deaf015','b_deaf015')
   CALL adeq213_filter_show('deaf008','b_deaf008')
   CALL adeq213_filter_show('deaf009','b_deaf009')
   CALL adeq213_filter_show('deaf016','b_deaf016')
   CALL adeq213_filter_show('deaf017','b_deaf017')
   CALL adeq213_filter_show('deaf018','b_deaf018')
   CALL adeq213_filter_show('deaf010','b_deaf010')
   CALL adeq213_filter_show('apca113','b_apca113')
   CALL adeq213_filter_show('deafdocno','b_deafdocno')
 
 
   #add-point:b_fill段結束前
   DROP TABLE adeq213_tmp;    
   #end add-point
 
END FUNCTION]]>
  </point>
