  <section id="adeq621.b_fill" ver="9" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION adeq621_b_fill()
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準
   {<point name="b_fill.define" edit="s"/>}
   #end add-point
   #add-point:b_fill段define-客製
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   LET ls_sql_rank = "SELECT  UNIQUE '',debusite,'',debu002,debu003,debu040,debu007,debu009,debu010, 
       debu011,'',debu012,'',debu014,'',debu016,'',debu018,'',debu019,debu020,debu021,debu022,debu023, 
       debu024,debu017,debu025,debu026,debu027,debu039,debu028,debu029,debu030,debu008,debu004,''  , 
       DENSE_RANK() OVER( ORDER BY debu_t.debusite,debu_t.debu002,debu_t.debu003,debu_t.debu004,debu_t.debu007, 
       debu_t.debu008,debu_t.debu015,debu_t.debu016,debu_t.debu018) AS RANK FROM debu_t",
 
 
                     "",
                     " WHERE debuent= ? AND 1=1 AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("debu_t"),
                     " ORDER BY debu_t.debusite,debu_t.debu002,debu_t.debu003,debu_t.debu004,debu_t.debu007,debu_t.debu008,debu_t.debu015,debu_t.debu016,debu_t.debu018"
 
   #add-point:b_fill段rank_sql_after
   {<point name="b_fill.rank_sql_after"/>}
   #end add-point
 
   LET g_sql = "SELECT COUNT(*) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT '',debusite,'',debu002,debu003,debu040,debu007,debu009,debu010,debu011,'',debu012, 
       '',debu014,'',debu016,'',debu018,'',debu019,debu020,debu021,debu022,debu023,debu024,debu017,debu025, 
       debu026,debu027,debu039,debu028,debu029,debu030,debu008,debu004,''",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE adeq621_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR adeq621_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_debu_d.clear()
 
   #add-point:陣列清空
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1   
 
   FOREACH b_fill_curs INTO g_debu_d[l_ac].sel,g_debu_d[l_ac].debusite,g_debu_d[l_ac].debusite_desc, 
       g_debu_d[l_ac].debu002,g_debu_d[l_ac].debu003,g_debu_d[l_ac].debu040,g_debu_d[l_ac].debu007,g_debu_d[l_ac].debu009, 
       g_debu_d[l_ac].debu010,g_debu_d[l_ac].debu011,g_debu_d[l_ac].debu011_desc,g_debu_d[l_ac].debu012, 
       g_debu_d[l_ac].debu012_desc,g_debu_d[l_ac].debu014,g_debu_d[l_ac].debu014_desc,g_debu_d[l_ac].debu016, 
       g_debu_d[l_ac].debu016_desc,g_debu_d[l_ac].debu018,g_debu_d[l_ac].debu018_desc,g_debu_d[l_ac].debu019, 
       g_debu_d[l_ac].debu020,g_debu_d[l_ac].debu021,g_debu_d[l_ac].debu022,g_debu_d[l_ac].debu023,g_debu_d[l_ac].debu024, 
       g_debu_d[l_ac].debu017,g_debu_d[l_ac].debu025,g_debu_d[l_ac].debu026,g_debu_d[l_ac].debu027,g_debu_d[l_ac].debu039, 
       g_debu_d[l_ac].debu028,g_debu_d[l_ac].debu029,g_debu_d[l_ac].debu030,g_debu_d[l_ac].debu008,g_debu_d[l_ac].debu004, 
       g_debu_d[l_ac].debu004_desc
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #LET g_debu_d[l_ac].statepic = cl_get_actipic(g_debu_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL adeq621_detail_show("'1'")      
 
      CALL adeq621_debu_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
   LET g_error_show = 0
   
 
   
   CALL g_debu_d.deleteElement(g_debu_d.getLength())   
 
   #add-point:陣列長度調整
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = g_debu_d.getLength()
#  DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE adeq621_pb
 
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL adeq621_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL adeq621_detail_action_trans()
 
   IF g_debu_d.getLength() > 0 THEN
      LET l_ac = 1
      CALL adeq621_fetch()
   END IF
   
      CALL adeq621_filter_show('debusite','b_debusite')
   CALL adeq621_filter_show('debu002','b_debu002')
   CALL adeq621_filter_show('debu003','b_debu003')
   CALL adeq621_filter_show('debu040','b_debu040')
   CALL adeq621_filter_show('debu007','b_debu007')
   CALL adeq621_filter_show('debu009','b_debu009')
   CALL adeq621_filter_show('debu010','b_debu010')
   CALL adeq621_filter_show('debu011','b_debu011')
   CALL adeq621_filter_show('debu012','b_debu012')
   CALL adeq621_filter_show('debu014','b_debu014')
   CALL adeq621_filter_show('debu016','b_debu016')
   CALL adeq621_filter_show('debu018','b_debu018')
   CALL adeq621_filter_show('debu019','b_debu019')
   CALL adeq621_filter_show('debu020','b_debu020')
   CALL adeq621_filter_show('debu021','b_debu021')
