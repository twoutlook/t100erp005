  <section id="adeq621.filter" ver="7" status="" src="s" readonly="">
    <![CDATA[#+ filter過濾功能
PRIVATE FUNCTION adeq621_filter()
   DEFINE  ls_result   STRING
   #add-point:filter段define-標準
   {<point name="filter.define" edit="s"/>}
   #end add-point
   #add-point:filter段define-客製
   {<point name="filter.define_customerization" edit="c"/>}
   #end add-point
   
   LET l_ac = 1
   LET g_detail_idx = 1
   LET g_detail_idx2 = 1
 
   LET INT_FLAG = 0
 
   LET g_qryparam.state = 'c'
 
   LET g_wc_filter_t = g_wc_filter
   LET g_wc_t = g_wc
   
   CALL gfrm_curr.setFieldHidden("formonly.sel", TRUE)
   CALL gfrm_curr.setFieldHidden("formonly.statepic", TRUE)
 
   LET g_wc = cl_replace_str(g_wc, g_wc_filter, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON debusite,debu002,debu003,debu040,debu007,debu009,debu010,debu011,debu012, 
          debu014,debu016,debu018,debu019,debu020,debu021,debu022,debu023,debu024,debu017,debu025,debu026, 
          debu027,debu039,debu028,debu029,debu030,debu008,debu004
                          FROM s_detail1[1].b_debusite,s_detail1[1].b_debu002,s_detail1[1].b_debu003, 
                              s_detail1[1].b_debu040,s_detail1[1].b_debu007,s_detail1[1].b_debu009,s_detail1[1].b_debu010, 
                              s_detail1[1].b_debu011,s_detail1[1].b_debu012,s_detail1[1].b_debu014,s_detail1[1].b_debu016, 
