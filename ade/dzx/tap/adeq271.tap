  <point name="b_fill.sql_after" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[ LET g_sql = "SELECT deds019,deds020,dedssite,ooefl_t.ooefl003,deds001,deds002,ooial_t.ooial003,
       deds004,deds005,deds006,deds007, 
       deds008,nmabl_t.nmabl003,deds009,deds012,deds013,deds014,deds015,deds011,mmanl_t.mmanl003",
       " FROM  deds_t",
"  LEFT JOIN ooefl_t ON ooefl_t.ooeflent=deds_t.dedsent AND ooefl_t.ooefl001=deds_t.dedssite AND ooefl_t.ooefl002 = '",g_dlang,"' ",
"  LEFT JOIN ooial_t ON ooial_t.ooialent=deds_t.dedsent AND ooial_t.ooial001=deds_t.deds002  AND ooial_t.ooial002='",g_dlang,"' ",
"  LEFT JOIN nmabl_t ON nmabl_t.nmablent=deds_t.dedsent AND nmabl_t.nmabl001=deds_t.deds008  AND nmabl_t.nmabl002='",g_dlang,"' ",
"  LEFT JOIN mmanl_t ON mmanl_t.mmanlent=deds_t.dedsent AND mmanl_t.mmanl001=deds_t.deds011  AND mmanl_t.mmanl002='",g_dlang,"' ",
" WHERE dedsent = ? ",
       "   AND ", ls_wc,cl_sql_add_filter("deds_t")
     LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("deds_t"),
                     " ORDER BY deds_t.dedssite,deds_t.deds002,deds_t.deds005,deds_t.deds011,deds_t.deds019,deds_t.deds020"
      ]]>
