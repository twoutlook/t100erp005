       g_deds_d[l_ac].deds005,g_deds_d[l_ac].deds006,g_deds_d[l_ac].deds007,g_deds_d[l_ac].deds008,g_deds_d[l_ac].deds008_desc, 
       g_deds_d[l_ac].deds009,g_deds_d[l_ac].deds012,g_deds_d[l_ac].deds013,g_deds_d[l_ac].deds014,g_deds_d[l_ac].deds015, 
       g_deds_d[l_ac].deds011,g_deds_d[l_ac].deds011_desc
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #LET g_deds_d[l_ac].statepic = cl_get_actipic(g_deds_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL adeq271_detail_show("'1'")      
 
      CALL adeq271_deds_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
   LET g_error_show = 0
   
 
   
   CALL g_deds_d.deleteElement(g_deds_d.getLength())   
 
   #add-point:陣列長度調整
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = g_deds_d.getLength()
#  DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
