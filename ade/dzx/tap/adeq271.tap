   CLOSE b_fill_curs
   FREE adeq271_pb
 
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL adeq271_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL adeq271_detail_action_trans()
 
