  <point name="function.adeq415_upd_gcbj" order="19" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION adeq415_upd_gcbj(p_act,p_dialog)
   DEFINE p_act       LIKE type_t.chr20
   DEFINE p_dialog    ui.Dialog
   DEFINE l_i         LIKE type_t.num5 
   DEFINE l_gcbj009   LIKE gcbj_t.gcbj009
   DEFINE l_gcbj010   LIKE gcbj_t.gcbj010
   DEFINE l_gcbj011   LIKE gcbj_t.gcbj011
   DEFINE l_gcbj006   LIKE gcbj_t.gcbj006
   DEFINE l_gcbj007   LIKE gcbj_t.gcbj007
   #DEFINE l_gcao005   LIKE gcao_t.gcao005
   DEFINE l_success   LIKE type_t.num5
   
   IF p_act = "lost_sel" THEN
      CALL adeq415_open_s01() RETURNING l_success,l_gcbj006,l_gcbj007
   END IF
   
   FOR l_i = 1 TO g_deah2_d.getLength()
      #LET l_gcao005 = '7'
      CASE p_act 
         WHEN "verify_all"
            LET g_deah2_d[l_i].gcbj008 = '2'
            LET l_gcbj009 = g_site
            LET l_gcbj010 = cl_get_current()
            LET l_gcbj011 = g_user
         WHEN "unverify_all"
            LET g_deah2_d[l_i].gcbj008 = '1'
            LET l_gcbj009 = NULL
            LET l_gcbj010 = NULL
            LET l_gcbj011 = NULL
            #LET l_gcao005 = g_deah2_d[l_i].gcbj003
         WHEN "verify_sel"
            IF p_dialog.isRowSelected("s_detail2", l_i) THEN
               LET g_deah2_d[l_i].gcbj008 = '2'
               LET l_gcbj009 = g_site
               LET l_gcbj010 = cl_get_current()
               LET l_gcbj011 = g_user
            ELSE
               CONTINUE FOR
            END IF
         WHEN "unverify_sel"
            IF p_dialog.isRowSelected("s_detail2", l_i) THEN
               LET g_deah2_d[l_i].gcbj008 = '1'
               LET l_gcbj009 = NULL
               LET l_gcbj010 = NULL
               LET l_gcbj011 = NULL
               #LET l_gcao005 = g_deah2_d[l_i].gcbj003
            ELSE
               CONTINUE FOR
            END IF
         WHEN "lost_sel"
            IF p_dialog.isRowSelected("s_detail2", l_i) THEN
               LET g_deah2_d[l_i].gcbj008 = '3'
               LET l_gcbj009 = g_site
               LET l_gcbj010 = cl_get_current()
               LET l_gcbj011 = g_user
            ELSE
               CONTINUE FOR
            END IF
         OTHERWISE
            RETURN         
      END CASE
      
      IF p_act = "lost_sel" THEN
         IF l_success THEN
            LET g_deah2_d[l_i].gcbj006 = l_gcbj006
            LET g_deah2_d[l_i].gcbj007 = l_gcbj007
         END IF
      ELSE
         LET g_deah2_d[l_i].gcbj006 = NULL
         LET g_deah2_d[l_i].gcbj007 = NULL
      END IF
      
      UPDATE gcbj_t
         SET gcbj008 = g_deah2_d[l_i].gcbj008,
             gcbj006 = g_deah2_d[l_i].gcbj006,
             gcbj007 = g_deah2_d[l_i].gcbj007,
             gcbj009 = l_gcbj009,
             gcbj010 = l_gcbj010,
             gcbj011 = l_gcbj011
       WHERE gcbjent = g_enterprise
         AND gcbjdocno = g_deah_d[g_detail_idx].deahdocno
         AND gcbjseq = g_deah_d[g_detail_idx].deahseq
         AND gcbjseq1 = g_deah2_d[l_i].gcbjseq1
       
      IF SQLCA.sqlcode THEN
         CALL cl_err("upd gcbj_t",SQLCA.sqlcode,1)  
         EXIT FOR
      END IF 
      
#      UPDATE gcao_t
#         SET gcao005 = l_gcao005
#       WHERE gcaoent = g_enterprise
#         AND gcao001 = g_deah2_d[l_i].gcbj001
#         
#      IF SQLCA.sqlcode THEN
#         CALL cl_err("upd gcao_t",SQLCA.sqlcode,1)  
#         EXIT FOR
#      END IF 
      
      CALL adeq415_cnt_gcbjcnt(l_i)
   END FOR
END FUNCTION]]>
  </point>
  <point name="function.adeq415_open_s01" order="20" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION adeq415_open_s01()
   DEFINE l_gcbj006        LIKE gcbj_t.gcbj006
   DEFINE l_gcbj007        LIKE gcbj_t.gcbj007
   DEFINE l_gcbj006_desc   LIKE type_t.chr500
   DEFINE r_success        LIKE type_t.num5
   
   LET r_success = TRUE
   
   OPEN WINDOW w_adeq415_s01 WITH FORM cl_ap_formpath("ade","adeq415_s01")
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
      INPUT l_gcbj006,l_gcbj007 FROM gcbj006,gcbj007
         ON ACTION controlp INFIELD gcbj006
            #此段落由子樣板a08產生
            #開窗c段
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'i'
            LET g_qryparam.reqry = FALSE
            LET g_qryparam.arg1 = '2104'
            CALL q_oocq002()                           #呼叫開窗
            LET l_gcbj006 = g_qryparam.return1
            DISPLAY l_gcbj006 TO gcbj006  #顯示到畫面上
            
            INITIALIZE g_ref_fields TO NULL
            LET g_ref_fields[1] = l_gcbj006
            CALL ap_ref_array2(g_ref_fields,"SELECT oocql004 FROM oocql_t WHERE oocqlent='"||g_enterprise||"' AND oocql001 = '2104' AND oocql002=? AND oocql003='"||g_dlang||"'","") RETURNING g_rtn_fields
            LET l_gcbj006_desc = '', g_rtn_fields[1] , '' 
            DISPLAY l_gcbj006_desc TO gcbj006_desc
            
            NEXT FIELD gcbj006                     #返回原欄位
            
         AFTER FIELD gcbj006
            IF NOT s_azzi650_chk_exist('2104',l_gcbj006) THEN
               LET l_gcbj006 = NULL
               NEXT FIELD gcbj006
            END IF
            INITIALIZE g_ref_fields TO NULL
            LET g_ref_fields[1] = l_gcbj006
            CALL ap_ref_array2(g_ref_fields,"SELECT oocql004 FROM oocql_t WHERE oocqlent='"||g_enterprise||"' AND oocql001 = '2104' AND oocql002=? AND oocql003='"||g_dlang||"'","") RETURNING g_rtn_fields
            LET l_gcbj006_desc = '', g_rtn_fields[1] , '' 
            DISPLAY l_gcbj006_desc TO gcbj006_desc
      END INPUT
      
      ON ACTION accept
         ACCEPT DIALOG
        
      ON ACTION cancel
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION close
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION exit
         LET INT_FLAG = TRUE 
         EXIT DIALOG
   
   END DIALOG
   CLOSE WINDOW w_adeq415_s01
   IF INT_FLAG THEN
      LET r_success = FALSE
      LET INT_FLAG = FALSE 
   END IF
   
   RETURN r_success,l_gcbj006,l_gcbj007
END FUNCTION]]>
  </point>
  <point name="function.adeq415_update_gcao" order="21" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION adeq415_update_gcao(p_act,p_deahdocno,p_deahseq)
   DEFINE p_act       LIKE type_t.chr20
   DEFINE p_deahdocno LIKE deah_t.deahdocno
   DEFINE p_deahseq   LIKE deah_t.deahseq
   DEFINE l_i         LIKE type_t.num5 
   DEFINE l_gcao005   LIKE gcao_t.gcao005
   DEFINE l_gcbj      RECORD
             gcbj001  LIKE gcbj_t.gcbj001,
             gcbj003  LIKE gcbj_t.gcbj003,
             gcbj008  LIKE gcbj_t.gcbj008
             END RECORD
   
   DECLARE adeq415_cl1 CURSOR FOR 
   SELECT gcbj001,gcbj003,gcbj008
     FROM gcbj_t
    WHERE gcbjent = g_enterprise
      AND gcbjdocno = p_deahdocno
      AND gcbjseq = p_deahseq
   
   FOREACH adeq415_cl1 INTO l_gcbj.gcbj001,l_gcbj.gcbj003,l_gcbj.gcbj008
      IF p_act = 'conf' THEN
         IF l_gcbj.gcbj008 = '2' THEN
            LET l_gcao005 = '7'
         ELSE
            LET l_gcao005 = l_gcbj.gcbj003
         END IF
      ELSE
         LET l_gcao005 = l_gcbj.gcbj003
      END IF
      UPDATE gcao_t
         SET gcao005 = l_gcao005
       WHERE gcaoent = g_enterprise
         AND gcao001 = l_gcbj.gcbj001
         
      IF SQLCA.sqlcode THEN
         CALL cl_err("upd gcao_t",SQLCA.sqlcode,1)  
         EXIT FOREACH
      END IF 
   END FOREACH
END FUNCTION]]>
