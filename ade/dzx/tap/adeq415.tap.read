  <point name="function.adeq415_cnt_deahrt" order="4" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION adeq415_cnt_deahrt(p_ac)
   DEFINE p_ac        LIKE type_t.num5
   DEFINE l_cnt       LIKE type_t.num5
   DEFINE l_deah015   LIKE deah_t.deah015
   
   IF cl_null(p_ac) THEN
      LET p_ac = l_ac
   END IF
   
   SELECT COUNT(*)
     INTO l_cnt
     FROM gcbj_t
    WHERE gcbjent = g_enterprise
      AND gcbjdocno = g_deah_d[p_ac].deahdocno
      AND gcbjseq = g_deah_d[p_ac].deahseq
      AND gcbj008 = '2'
      
   IF cl_null(l_cnt) THEN
      LET l_cnt = 0
   END IF
   LET g_deah_d[p_ac].deahrt = l_cnt
   LET l_cnt = g_deah_d[p_ac].deah007 - l_cnt
   LET g_deah_d[p_ac].deahdif = l_cnt
   LET g_deah_d[p_ac].deah015 = g_deah_d[p_ac].deahdif * g_deah_d[p_ac].deah004
   IF cl_null(g_deah_d[p_ac].deah015) THEN
      LET g_deah_d[p_ac].deah015 = 0
   END IF
   
   SELECT deah015
     INTO l_deah015
     FROM deah_t
    WHERE deahent = g_enterprise
      AND deahdocno = g_deah_d[p_ac].deahdocno
      AND deahseq = g_deah_d[p_ac].deahseq
      
   IF cl_null(l_deah015) OR l_deah015 <> g_deah_d[p_ac].deah015 THEN
      UPDATE deah_t
         SET deah015 = g_deah_d[p_ac].deah015
       WHERE deahent = g_enterprise
         AND deahdocno = g_deah_d[p_ac].deahdocno
         AND deahseq = g_deah_d[p_ac].deahseq
   END IF
END FUNCTION]]>
  </point>
