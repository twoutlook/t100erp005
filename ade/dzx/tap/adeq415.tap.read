  <point name="function.adeq415_cnt_gcbjcnt" order="9" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION adeq415_cnt_gcbjcnt(p_ac)
   DEFINE p_ac      LIKE type_t.num5
   DEFINE l_cnt     LIKE type_t.num5
   
   SELECT COUNT(*)
     INTO l_cnt
     FROM gcbj_t,deah_t
    WHERE gcbjent = g_enterprise
      AND gcbj001 = g_deah2_d[p_ac].gcbj001
      AND gcbj008 = '2'
      AND deahent = gcbjent
      AND deahdocno = gcbjdocno
      AND deahseq = gcbjseq
      AND deah014 IS NOT NULL
      
   IF cl_null(l_cnt) THEN
      LET l_cnt = 0
   END IF  
   LET g_deah2_d[p_ac].gcbjcnt = l_cnt
END FUNCTION]]>
