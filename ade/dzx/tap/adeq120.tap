  <section id="adeq120.fetch" ver="23" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充2
PRIVATE FUNCTION adeq120_fetch()
   DEFINE li_ac           LIKE type_t.num10
   #add-point:fetch段define-標準
   {<point name="fetch.define" edit="s"/>}
   #end add-point
   #add-point:fetch段define-客製
   {<point name="fetch.define_customerization" edit="c"/>}
   #end add-point
   
   CALL g_debg2_d.clear()
 
   CALL g_debg3_d.clear()
 
 
   #add-point:陣列清空
   {<point name="fetch.array_clear" />}
   #end add-point
   
   LET li_ac = l_ac 
   
   #為避免影響執行效能，若是按上下筆就不重組SQL
   IF g_action_choice <> "fetch" OR cl_null(g_action_choice) THEN
      LET g_sql = "SELECT  UNIQUE debh008,'',debh009,'',debh010,debh011,debh012,debh013,debh014,debh015, 
          debh020,debh021,debh022,debh016,debh017,debh018,debh019 FROM debh_t",    
                  "",
                  " WHERE debhent=? AND debhsite=? AND debh002=? AND debh005=? AND debh006=? AND debh007=?"
  
      IF NOT cl_null(g_wc2_table2) THEN
         LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
      END IF
  
      LET g_sql = g_sql, " ORDER BY debh_t.debh008,debh_t.debh009" 
                         
      #add-point:單身填充前
      {<point name="fetch.before_fill2" />}
      #end add-point
 
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料   
      PREPARE adeq120_pb2 FROM g_sql
      DECLARE b_fill_curs2 CURSOR FOR adeq120_pb2
   END IF
 
   OPEN b_fill_curs2 USING g_enterprise,g_debg_d[g_detail_idx].debgsite
                                  ,g_debg_d[g_detail_idx].debg002
 
                                  ,g_debg_d[g_detail_idx].debg005
 
                                  ,g_debg_d[g_detail_idx].debg007
 
                                  ,g_debg_d[g_detail_idx].debg008
 
 
   LET l_ac = 1
   FOREACH b_fill_curs2 INTO g_debg2_d[l_ac].debh008,g_debg2_d[l_ac].debh008_desc,g_debg2_d[l_ac].debh009, 
       g_debg2_d[l_ac].debh009_desc,g_debg2_d[l_ac].debh010,g_debg2_d[l_ac].debh011,g_debg2_d[l_ac].debh012, 
