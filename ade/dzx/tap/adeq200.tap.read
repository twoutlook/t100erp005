  <section id="adeq200.filter" ver="10" status="" src="s" readonly="">
    <![CDATA[#+ filter過濾功能
PRIVATE FUNCTION adeq200_filter()
   DEFINE  ls_result   STRING
   #add-point:filter段define-標準
   {<point name="filter.define" edit="s"/>}
   #end add-point
   #add-point:filter段define-客製
   {<point name="filter.define_customerization" edit="c"/>}
   #end add-point
   
   LET l_ac = 1
   LET g_detail_idx = 1
   LET g_detail_idx2 = 1
 
   LET INT_FLAG = 0
 
   LET g_qryparam.state = 'c'
 
   LET g_wc_filter_t = g_wc_filter
   LET g_wc_t = g_wc
   
   CALL gfrm_curr.setFieldHidden("formonly.sel", TRUE)
   CALL gfrm_curr.setFieldHidden("formonly.statepic", TRUE)
 
   LET g_wc = cl_replace_str(g_wc, g_wc_filter, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON debz002,debz003,debz004,debzsite,debz005,debz007,debz014,debz008,debz010, 
          debz011,debz012,debz009,debz013
                          FROM s_detail1[1].b_debz002,s_detail1[1].b_debz003,s_detail1[1].b_debz004, 
                              s_detail1[1].b_debzsite,s_detail1[1].b_debz005,s_detail1[1].b_debz007, 
                              s_detail1[1].b_debz014,s_detail1[1].b_debz008,s_detail1[1].b_debz010,s_detail1[1].b_debz011, 
                              s_detail1[1].b_debz012,s_detail1[1].b_debz009,s_detail1[1].b_debz013
 
         BEFORE CONSTRUCT
                     DISPLAY adeq200_filter_parser('debz002') TO s_detail1[1].b_debz002
            DISPLAY adeq200_filter_parser('debz003') TO s_detail1[1].b_debz003
            DISPLAY adeq200_filter_parser('debz004') TO s_detail1[1].b_debz004
            DISPLAY adeq200_filter_parser('debzsite') TO s_detail1[1].b_debzsite
            DISPLAY adeq200_filter_parser('debz005') TO s_detail1[1].b_debz005
            DISPLAY adeq200_filter_parser('debz007') TO s_detail1[1].b_debz007
            DISPLAY adeq200_filter_parser('debz014') TO s_detail1[1].b_debz014
            DISPLAY adeq200_filter_parser('debz008') TO s_detail1[1].b_debz008
            DISPLAY adeq200_filter_parser('debz010') TO s_detail1[1].b_debz010
            DISPLAY adeq200_filter_parser('debz011') TO s_detail1[1].b_debz011
            DISPLAY adeq200_filter_parser('debz012') TO s_detail1[1].b_debz012
