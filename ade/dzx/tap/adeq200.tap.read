               "                    FROM ooef_t,oodbl_t WHERE ooefent = oodblent AND ooef019 = oodbl001) t4 ",
               "              ON t4.oodblent = debzent AND t4.ooef001 = debzsite AND t4.oodbl002 = debz008 AND t4.oodbl003 = '",g_dlang,"' ",
               " WHERE debzent = ? ",
               "   AND ", ls_wc,cl_sql_add_filter("debz_t")
    
   LET g_sql = g_sql, cl_sql_add_filter("debz_t"),
                      " ORDER BY debz_t.debzsite,debz_t.debz002,debz_t.debz005,debz_t.debz007,debz_t.debz008"   
   #150302-00004#8 150312 by lori522612 add 效能調整---(E)      ]]>
