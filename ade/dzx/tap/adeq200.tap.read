  <section id="adeq200.b_fill" ver="18" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION adeq200_b_fill()
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準
   {<point name="b_fill.define" edit="s"/>}
   #end add-point
   #add-point:b_fill段define-客製
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   LET ls_sql_rank = "SELECT  UNIQUE '',debz002,debz003,debz004,debzsite,'',debz005,debz007,'',debz014, 
       '',debz008,'',debz010,debz011,debz012,debz009,debz013  ,DENSE_RANK() OVER( ORDER BY debz_t.debzsite, 
       debz_t.debz002,debz_t.debz005,debz_t.debz007,debz_t.debz008,debz_t.debz014,debz_t.debz018) AS RANK FROM debz_t", 

 
 
                     "",
                     " WHERE debzent= ? AND 1=1 AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("debz_t"),
                     " ORDER BY debz_t.debzsite,debz_t.debz002,debz_t.debz005,debz_t.debz007,debz_t.debz008,debz_t.debz014,debz_t.debz018"
 
   #add-point:b_fill段rank_sql_after
   {<point name="b_fill.rank_sql_after"/>}
   #end add-point
 
   LET g_sql = "SELECT COUNT(*) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT '',debz002,debz003,debz004,debzsite,'',debz005,debz007,'',debz014,'',debz008, 
       '',debz010,debz011,debz012,debz009,debz013",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE adeq200_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR adeq200_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_debz_d.clear()
 
   #add-point:陣列清空
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1   
 
   FOREACH b_fill_curs INTO g_debz_d[l_ac].sel,g_debz_d[l_ac].debz002,g_debz_d[l_ac].debz003,g_debz_d[l_ac].debz004, 
       g_debz_d[l_ac].debzsite,g_debz_d[l_ac].debzsite_desc,g_debz_d[l_ac].debz005,g_debz_d[l_ac].debz007, 
       g_debz_d[l_ac].debz007_desc,g_debz_d[l_ac].debz014,g_debz_d[l_ac].debz014_desc,g_debz_d[l_ac].debz008, 
       g_debz_d[l_ac].debz008_desc,g_debz_d[l_ac].debz010,g_debz_d[l_ac].debz011,g_debz_d[l_ac].debz012, 
       g_debz_d[l_ac].debz009,g_debz_d[l_ac].debz013
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #LET g_debz_d[l_ac].statepic = cl_get_actipic(g_debz_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL adeq200_detail_show("'1'")      
 
      CALL adeq200_debz_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
   LET g_error_show = 0
   
 
   
   CALL g_debz_d.deleteElement(g_debz_d.getLength())   
 
   #add-point:陣列長度調整
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = g_debz_d.getLength()
#  DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE adeq200_pb
 
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL adeq200_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL adeq200_detail_action_trans()
 
   IF g_debz_d.getLength() > 0 THEN
      LET l_ac = 1
      CALL adeq200_fetch()
   END IF
   
      CALL adeq200_filter_show('debz002','b_debz002')
   CALL adeq200_filter_show('debz003','b_debz003')
   CALL adeq200_filter_show('debz004','b_debz004')
   CALL adeq200_filter_show('debzsite','b_debzsite')
   CALL adeq200_filter_show('debz005','b_debz005')
   CALL adeq200_filter_show('debz007','b_debz007')
   CALL adeq200_filter_show('debz014','b_debz014')
   CALL adeq200_filter_show('debz008','b_debz008')
   CALL adeq200_filter_show('debz010','b_debz010')
   CALL adeq200_filter_show('debz011','b_debz011')
   CALL adeq200_filter_show('debz012','b_debz012')
   CALL adeq200_filter_show('debz009','b_debz009')
   CALL adeq200_filter_show('debz013','b_debz013')
 
 
   #add-point:b_fill段結束前
   {<point name="b_fill.after"/>}
   #end add-point
 
END FUNCTION
]]>
  </section>
