  <point name="function.adeq113_deba2_b_fill" order="1" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 門店匯總顯示
# Memo...........:
# Usage..........: CALL adeq113_deba2_b_fill()
# Input parameter: 
# Return code....: 
# Date & Author..: 2015/07/07 By Ken
# Modify.........:
################################################################################
PRIVATE FUNCTION adeq113_deba2_b_fill()
DEFINE l_sql   STRING
DEFINE l_cnt   LIKE type_t.num5 
 
   CALL g_deba2.clear()
   
   LET l_sql = "SELECT UNIQUE  ",
               "       debasite,   t2.ooefl003,   SUBSTR(t0.imaa009,1,6) imaa009  , ",
               "       rtaxl003,  SUM(deba026), ",
               "       SUM((CASE WHEN deba049='1' THEN deba026 ELSE 0 END)) deba0261, ",
               "       SUM((CASE WHEN deba049='2' THEN deba022 ELSE 0 END)) deba0262, ",
               "       SUM((CASE WHEN deba049='3' THEN deba026 ELSE 0 END)) deba0493, ",
               "       SUM((CASE WHEN deba049='4' THEN deba026 ELSE 0 END)) deba0494, ",
               "       SUM(deba022) ,SUM(deba027) ",
               "  FROM deba_t ",
               "  LEFT OUTER JOIN imaa_t t0 ON debaent = t0.imaaent AND deba009 = t0.imaa001 ",
               "  LEFT OUTER JOIN rtaxl_t t1 ON t1.rtaxlent = t0.imaaent ",
               "                      AND t1.rtaxl001 = substr(t0.imaa009,1,6) AND t1.rtaxl002 = '",g_dlang,"' ",
               "  LEFT OUTER JOIN ooefl_t t2 ON  t2.ooeflent = debaent  AND t2.ooefl001 = debasite  AND t2.ooefl002 = '",g_dlang,"' ",
               "  LEFT OUTER JOIN ooga_t t3 ON t3.oogaent = debaent AND t3.ooga001 = deba002 ",
               " WHERE debaent = ? ",            
               #"   AND debasite IN (" ,g_debasite_tmp.subString(3,g_debasite_tmp.getLength()) ,")",
               #"   AND SUBSTR(t0.imaa009,1,6) IN (" ,g_imaa009_tmp.subString(3,g_imaa009_tmp.getLength()) ,")"
               "   AND debasite IN (SELECT debasite FROM debasite_tmp) ",
               "   AND SUBSTR(t0.imaa009,1,6) IN (SELECT imaa009 FROM imaa009_tmp) " 
               
   LET l_sql = l_sql, 
               " GROUP BY debasite,t2.ooefl003,SUBSTR(t0.imaa009,1,6),rtaxl003 ",
               " ORDER BY debasite,imaa009 "
   DISPLAY "l_sql:",l_sql
   
   PREPARE sel_deba2_pre FROM l_sql
   DECLARE sel_deba2_cs  CURSOR FOR sel_deba2_pre
   LET g_cnt = l_ac
   LET l_ac = 1
   FOREACH sel_deba2_cs USING g_enterprise 
                       INTO g_deba2[l_ac].debasite,          g_deba2[l_ac].debasite_desc,   g_deba2[l_ac].l_imaa009_1,
                            g_deba2[l_ac].l_imaa009_1_desc,  g_deba2[l_ac].deba026,         g_deba2[l_ac].l_deba026_11,
                            g_deba2[l_ac].l_deba026_12,      g_deba2[l_ac].l_deba026_13,    g_deba2[l_ac].l_deba026_14,
                            g_deba2[l_ac].deba022,           g_deba2[l_ac].deba027                            
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "FOREACH:"
         LET g_errparam.popup = TRUE
         CALL cl_err()

         EXIT FOREACH
      END IF
      
      LET l_ac = l_ac + 1
   END FOREACH
   
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CALL g_deba2.deleteElement(g_deba2.getLength())
   
   DISPLAY ARRAY g_deba2 TO s_detail2.* ATTRIBUTES(COUNT=g_detail_cnt)  
   
      BEFORE DISPLAY 
         EXIT DISPLAY

   END DISPLAY

   FREE sel_deba2_cs
END FUNCTION]]>
  </point>
