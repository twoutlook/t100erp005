  <point name="global.variable" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[DEFINE g_deba2               DYNAMIC ARRAY OF RECORD
       debasite              LIKE deba_t.debasite,
       debasite_desc         LIKE type_t.chr100,
       l_imaa009_1           LIKE imaa_t.imaa009,
       l_imaa009_1_desc      LIKE type_t.chr100,
       deba026               LIKE deba_t.deba026,
       l_deba026_11          LIKE deba_t.deba026, 
       l_deba026_12          LIKE deba_t.deba026, 
       l_deba026_13          LIKE deba_t.deba026, 
       l_deba026_14          LIKE deba_t.deba026, 
       deba022               LIKE deba_t.deba022, 
       deba027               LIKE deba_t.deba027 
