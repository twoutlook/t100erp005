  <point name="function.adeq113_create_tmp" order="3" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 建立暫存表
# Memo...........:
# Usage..........: CALL adeq113_create_tmp()
#                  RETURNING r_success
# Input parameter: 
# Return code....: r_success     TRUE/FALSE
# Date & Author..: 2015/07/07 By Ken
# Modify.........:
################################################################################
PRIVATE FUNCTION adeq113_create_tmp()
DEFINE r_success         LIKE type_t.num5

   DROP TABLE imaa009_tmp
   DROP TABLE debasite_tmp
   
   LET r_success = TRUE
   
   CREATE TEMP TABLE debasite_tmp(
      debasite       LIKE deba_t.debasite) 

   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend = "Create debasite_tmp"
      LET g_errparam.popup = TRUE
      CALL cl_err()
      LET r_success = FALSE
      RETURN r_success
   END IF

   CREATE TEMP TABLE imaa009_tmp(
      imaa009       LIKE imaa_t.imaa009)  

   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend = "Create imaa009_tmp"
      LET g_errparam.popup = TRUE
      CALL cl_err()
      LET r_success = FALSE
      RETURN r_success
   END IF
   
   RETURN r_success
END FUNCTION]]>
  </point>
