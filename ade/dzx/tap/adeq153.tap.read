PRIVATE FUNCTION adeq153_b_fill()
   {<Local define>}
   DEFINE ls_wc           STRING
   {</Local define>}
   #add-point:b_fill段define
   DEFINE l_where         STRING
   {<point name="b_fill.define"/>}
   #end add-point

   #add-point:b_fill段sql_before
   CALL s_aooi500_sql_where(g_prog,'dectsite') RETURNING l_where
   {<point name="b_fill.sql_before"/>}
   #end add-point

   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF

   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   LET ls_wc = ls_wc CLIPPED," AND ",l_where #aooi500控管
   LET g_sql = "SELECT UNIQUE dectsite,t1.ooefl003,dect001,dect034,dect035, ",
               "              dect007,t2.pmaal004,dect008,t3.staal003, ",
               "              dect009,t4.oodbl004, ", 
               "              SUM(dect010),SUM(dect011),SUM(dect012),SUM(dect013),SUM(dect014),SUM(dect015), ", 
               "              SUM(dect016),SUM(dect031),SUM(dect032),SUM(dect033),SUM(dect017),SUM(dect017)/SUM(dect016)*100 dect018, ",
               #"             SUM(dect019),SUM(dect036),", #客單價dect036
               "              SUM(dect019),'', ",   
               "              SUM(dect020),SUM(dect021),SUM(dect022),SUM(dect023),SUM(dect024), ", 
               "              SUM(dect025),SUM(dect026),SUM(dect027),SUM(dect028),SUM(dect029),SUM(dect030)  ", 
               "  FROM dect_t ",
               "       LEFT JOIN decu_t ON decuent = dectent AND decusite = dectsite AND decu025 = dect034 ", 
               "                       AND decu026 = dect035 AND decu005 = dect005   AND decu009 = dect009 ", 
               "       LEFT JOIN ooefl_t t1 ON t1.ooeflent = dectent AND t1.ooefl001 = dectsite AND t1.ooefl002 = '",g_dlang,"' ",
               "       LEFT JOIN pmaal_t t2 ON t2.pmaalent = dectent AND t2.pmaal001 = dect007 AND t2.pmaal002 = '",g_dlang,"' ",
               "       LEFT JOIN staal_t t3 ON t3.staalent = dectent AND t3.staal001 = dect008 AND t3.staal002 = '",g_dlang,"' ",
               "       LEFT JOIN (SELECT oodblent,ooef001,oodbl002,oodbl003,oodbl004 ",
               "                    FROM ooef_t,oodbl_t WHERE ooefent = oodblent AND ooef019 = oodbl001) t4",
