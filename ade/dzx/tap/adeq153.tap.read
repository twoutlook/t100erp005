  <point name="function.adeq153_fetch" order="5" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION adeq153_fetch()
   {<Local define>}
   DEFINE li_ac           LIKE type_t.num5
   {</Local define>}
   #add-point:fetch段define
   {<point name="fetch.define"/>}
   DEFINE l_dectsite      LIKE dect_t.dectsite 
   DEFINE l_dect034       LIKE dect_t.dect034 
   DEFINE l_dect035       LIKE dect_t.dect035 
   DEFINE l_dect005       LIKE dect_t.dect005 
   DEFINE l_dect006       LIKE dect_t.dect006 
   DEFINE l_dect007       LIKE dect_t.dect007 
   DEFINE l_dect009       LIKE dect_t.dect009    
   #end add-point

   CALL g_dect2_d.clear()



   LET li_ac = l_ac
   
   LET g_sql = "SELECT UNIQUE dectsite,dect034,dect035,dect005,dect006,dect007,dect009 ", 
               "  FROM dect_t ", 
               " WHERE dectent = ? ", 
               "   AND dectsite = ?", 
               "   AND dect034 = ? ",
               "   AND dect035 = ? ", 
               "   AND dect007 = ? ", 
               "   AND NVL(dect008,' ') = NVL(?,' ') ", 
               "   AND dect009 = ? "  
   LET g_sql = g_sql," ORDER BY dectsite,dect034,dect035,dect005,dect006,dect007,dect009 " 
   PREPARE adeq153_dect_prep FROM g_sql 
   DECLARE adeq153_dect_curs CURSOR FOR adeq153_dect_prep 

 
   LET g_sql = "SELECT UNIQUE decu010,t1.mmanl003,decu011,t2.oocql004,",
               "              SUM(decu012),SUM(decu013),SUM(decu017), ", 
               "              SUM(decu022),SUM(decu023),SUM(decu024), ",
               "              SUM(decu018),SUM(decu018)/SUM(decu017)*100 decu019, ", 
               "              SUM(decu020),SUM(decu021),SUM(decu027),", #客單價 decu027
               "              SUM(decu014),SUM(decu015),SUM(decu016) ", 
               "  FROM decu_t",
               "       LEFT JOIN mmanl_t t1 ON t1.mmanlent = decuent AND t1.mmanl001 = decu010 AND t1.mmanl002 = '",g_dlang,"' ",
               "       LEFT JOIN (SELECT oocqlent,oocql002,oocql004",
               "                    FROM oocql_t,oocq_t ",
               "                   WHERE oocqlent = oocqent AND oocql001 = oocq004 AND oocql003 = '",g_dlang,"' ",
               "                     AND oocq001 = '2049'   AND oocq002 = '09') t2 ",
               "              ON t2.oocqlent = decuent AND t2.oocql002 = decu011 ",
               " WHERE decuent = ? ", 
               "   AND decusite = ?",
               "   AND decu025 = ? ",
               "   AND decu026 = ? ", 
               "   AND decu005 = ? ", 
               "   AND decu006 = ? ", 
               "   AND decu007 = ? ", 
               "   AND decu009 = ?"

   IF NOT cl_null(g_wc2_table2) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
   END IF

   LET g_sql = g_sql, " GROUP BY decu010,t1.mmanl003,decu011,t2.oocql004 ", 
                      " ORDER BY decu_t.decu010,decu_t.decu011 " 
 
   #add-point:單身填充前
   {<point name="fetch.before_fill" />}
   #end add-point

   PREPARE adeq153_pb2 FROM g_sql
   DECLARE b_fill_curs2 CURSOR FOR adeq153_pb2

   LET l_ac = 1
   FOREACH adeq153_dect_curs USING g_enterprise,g_dect_d[g_detail_idx].dectsite,
                                                g_dect_d[g_detail_idx].dect034,g_dect_d[g_detail_idx].dect035, 
                                                g_dect_d[g_detail_idx].dect007,g_dect_d[g_detail_idx].dect008, 
                                                g_dect_d[g_detail_idx].dect009 
                              INTO l_dectsite,l_dect034,l_dect035,l_dect005,l_dect006,l_dect007,l_dect009 
      IF SQLCA.sqlcode THEN 
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = 'foreach:'
         LET g_errparam.popup = TRUE
         CALL cl_err()
 
         EXIT FOREACH 
      END IF  
      OPEN b_fill_curs2 USING g_enterprise,l_dectsite,l_dect034,l_dect035,l_dect005,l_dect006,l_dect007,l_dect009 

      FOREACH b_fill_curs2 INTO g_dect2_d[l_ac].decu010,g_dect2_d[l_ac].decu010_desc,   
                                g_dect2_d[l_ac].decu011,g_dect2_d[l_ac].decu011_desc,  
                                g_dect2_d[l_ac].decu012,g_dect2_d[l_ac].decu013,g_dect2_d[l_ac].decu017,
                                g_dect2_d[l_ac].decu022,g_dect2_d[l_ac].decu023,g_dect2_d[l_ac].decu024,
                                g_dect2_d[l_ac].decu018,g_dect2_d[l_ac].decu019,g_dect2_d[l_ac].decu020, 
                                g_dect2_d[l_ac].decu021,g_dect2_d[l_ac].decu027,g_dect2_d[l_ac].decu014, #客單價 decu027
                                g_dect2_d[l_ac].decu015,g_dect2_d[l_ac].decu016
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "FOREACH:"
            LET g_errparam.popup = TRUE
            CALL cl_err()

            EXIT FOREACH
         END IF
         # 是否有串查功能
   
   
         #add-point:b_fill段資料填充
         {<point name="fetch.fill2"/>}
        
         #end add-point
   
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
   
      END FOREACH
   END FOREACH 



   #DISPLAY g_detail_cnt TO FORMONLY.cnt

   #add-point:單身填充後
   {<point name="fetch.after_fill" />}
   #end add-point

   CALL g_dect2_d.deleteElement(g_dect2_d.getLength())



   LET l_ac = li_ac

END FUNCTION]]>
  </point>
