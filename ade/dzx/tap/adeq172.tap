    <![CDATA[
PRIVATE FUNCTION adeq172_fetch()
   {<Local define>}
   DEFINE li_ac           LIKE type_t.num5
   {</Local define>}
   #add-point:fetch段define
   {<point name="fetch.define"/>}
   #end add-point

   CALL g_dedq2_d.clear()



   LET li_ac = l_ac
   

   LET g_sql = "SELECT UNIQUE dedr010,t1.mmanl003,dedr011,t2.oocql004, ",
               "              SUM(dedr012),SUM(dedr013),SUM(dedr017),  ",
               "              SUM(dedr022),SUM(dedr023),SUM(dedr024),  ", 
               "              SUM(dedr018),SUM(dedr018)/SUM(dedr017)*100 dedr019, ", #s983961--add 毛利率 
               "              SUM(dedr020),SUM(dedr021),SUM(dedr027),SUM(dedr014),  ",   #150705-00001#9 Add By Ken 150706 加客單價
               "              SUM(dedr015),SUM(dedr016) ", 
               "  FROM dedr_t",
               "       LEFT JOIN mmanl_t t1 ON t1.mmanlent = dedrent AND t1.mmanl001 = dedr010 AND t1.mmanl002 = '",g_dlang,"' ",
               "       LEFT JOIN (SELECT oocqlent,oocql002,oocql004",
               "                    FROM oocql_t,oocq_t ",
               "                   WHERE oocqlent = oocqent AND oocql001 = oocq004 AND oocql003 = '",g_dlang,"' ",
               "                     AND oocq001 = '2049'   AND oocq002 = '09') t2 ",
               "              ON t2.oocqlent = dedrent AND t2.oocql002 = dedr011 ",
               " WHERE dedrent = ? ", 
               "   AND dedrsite = ?",
               "   AND dedr025 = ? ", 
               "   AND dedr026 = ? ", 
               "   AND dedr006 = ? ", 
               "   AND dedr009 = ?"

   IF NOT cl_null(g_wc2_table2) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
   END IF

   LET g_sql = g_sql, " GROUP BY dedr010,t1.mmanl003,dedr011,t2.oocql004 ", 
                      " ORDER BY dedr_t.dedr010,dedr_t.dedr011 " 
 
   #add-point:單身填充前
   {<point name="fetch.before_fill" />}
   #end add-point

   PREPARE adeq172_pb2 FROM g_sql
   DECLARE b_fill_curs2 CURSOR FOR adeq172_pb2

   OPEN b_fill_curs2 USING g_enterprise,g_dedq_d[g_detail_idx].dedqsite
                                       ,g_dedq_d[g_detail_idx].dedq034  #15/06/25 983961--add
                                       ,g_dedq_d[g_detail_idx].dedq035
                                       ,g_dedq_d[g_detail_idx].dedq006
                                       ,g_dedq_d[g_detail_idx].dedq009


   LET l_ac = 1
   FOREACH b_fill_curs2 INTO g_dedq2_d[l_ac].dedr010,g_dedq2_d[l_ac].dedr010_desc,   
                             g_dedq2_d[l_ac].dedr011,g_dedq2_d[l_ac].dedr011_desc,  
                             g_dedq2_d[l_ac].dedr012,g_dedq2_d[l_ac].dedr013,g_dedq2_d[l_ac].dedr017, 
                             g_dedq2_d[l_ac].dedr022,g_dedq2_d[l_ac].dedr023,g_dedq2_d[l_ac].dedr024, 
                             g_dedq2_d[l_ac].dedr018,g_dedq2_d[l_ac].dedr019,g_dedq2_d[l_ac].dedr020, 
                             g_dedq2_d[l_ac].dedr021,g_dedq2_d[l_ac].dedr027,g_dedq2_d[l_ac].dedr014, #150705-00001#9 Add By Ken 150706 加客單價
                             g_dedq2_d[l_ac].dedr015,g_dedq2_d[l_ac].dedr016
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "FOREACH:"
         LET g_errparam.popup = TRUE
         CALL cl_err()

         EXIT FOREACH
      END IF

      # 是否有串查功能


      #add-point:b_fill段資料填充
      {<point name="fetch.fill2"/>}
         
      #end add-point

      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF

   END FOREACH



   #DISPLAY g_detail_cnt TO FORMONLY.cnt

   #add-point:單身填充後
   {<point name="fetch.after_fill" />}
   #end add-point

   CALL g_dedq2_d.deleteElement(g_dedq2_d.getLength())



   LET l_ac = li_ac

END FUNCTION]]>
