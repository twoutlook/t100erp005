   LET g_sql = "SELECT UNIQUE dedqsite,    t1.ooefl003, dedq001,     dedq034,     dedq035, ",
               "              dedq006,     t2.oocql004, dedq009,     t3.oodbl004, ", 
               "              SUM(dedq010),SUM(dedq011),SUM(dedq012),SUM(dedq013),SUM(dedq014),SUM(dedq015), ", 
