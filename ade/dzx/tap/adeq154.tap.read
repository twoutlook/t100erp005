                  " WHERE decient=? AND decisite=? AND deci005=? AND deci009=? AND deci010=? AND deci011=? AND deci027=? AND deci028=?"
  
      IF NOT cl_null(g_wc2_table2) THEN
         LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
      END IF
  
      LET g_sql = g_sql, " ORDER BY deci_t.deci012,deci_t.deci013" ]]>
