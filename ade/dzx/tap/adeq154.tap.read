  <section id="adeq154.b_fill" ver="6" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION adeq154_b_fill()
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準
   {<point name="b_fill.define" edit="s"/>}
   #end add-point
   #add-point:b_fill段define-客製
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   LET ls_sql_rank = "SELECT  UNIQUE dechsite,'',dech001,dech005,'',dech031,dech032,dech006,dech007, 
       dech008,dech009,'',dech010,dech011,'',dech012,dech013,dech014,dech015,dech016,dech017,dech018, 
       dech019,dech021,dech029,dech030,dech020,dech022,dech023,dech024,dech033,dech027,dech028,dech026, 
       dech025  ,DENSE_RANK() OVER( ORDER BY dech_t.dechsite,dech_t.dech005,dech_t.dech009,dech_t.dech010, 
       dech_t.dech011,dech_t.dech031,dech_t.dech032) AS RANK FROM dech_t",
 
                     " LEFT JOIN deci_t ON decient = dechent AND dechsite = decisite AND dech005 = deci005 AND dech009 = deci009 AND dech010 = deci010 AND dech011 = deci011 AND dech031 = deci027 AND dech032 = deci028",
 
                     " LEFT JOIN decj_t ON decjent = dechent AND dechsite = decjsite AND dech005 = decj005 AND dech009 = decj009 AND dech010 = decj010 AND dech011 = decj011 AND dech031 = decj015 AND dech032 = decj016",
 
 
                     "",
                     " WHERE dechent= ? AND 1=1 AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("dech_t"),
                     " ORDER BY dech_t.dechsite,dech_t.dech005,dech_t.dech009,dech_t.dech010,dech_t.dech011,dech_t.dech031,dech_t.dech032"
 
   #add-point:b_fill段rank_sql_after
   {<point name="b_fill.rank_sql_after"/>}
   #end add-point
 
   LET g_sql = "SELECT COUNT(*) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT dechsite,'',dech001,dech005,'',dech031,dech032,dech006,dech007,dech008,dech009, 
       '',dech010,dech011,'',dech012,dech013,dech014,dech015,dech016,dech017,dech018,dech019,dech021, 
       dech029,dech030,dech020,dech022,dech023,dech024,dech033,dech027,dech028,dech026,dech025",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE adeq154_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR adeq154_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_dech_d.clear()
 
   #add-point:陣列清空
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1   
 
   FOREACH b_fill_curs INTO g_dech_d[l_ac].dechsite,g_dech_d[l_ac].dechsite_desc,g_dech_d[l_ac].dech001, 
       g_dech_d[l_ac].dech005,g_dech_d[l_ac].dech005_desc,g_dech_d[l_ac].dech031,g_dech_d[l_ac].dech032, 
       g_dech_d[l_ac].dech006,g_dech_d[l_ac].dech007,g_dech_d[l_ac].dech008,g_dech_d[l_ac].dech009,g_dech_d[l_ac].dech009_desc, 
       g_dech_d[l_ac].dech010,g_dech_d[l_ac].dech011,g_dech_d[l_ac].dech011_desc,g_dech_d[l_ac].dech012, 
       g_dech_d[l_ac].dech013,g_dech_d[l_ac].dech014,g_dech_d[l_ac].dech015,g_dech_d[l_ac].dech016,g_dech_d[l_ac].dech017, 
       g_dech_d[l_ac].dech018,g_dech_d[l_ac].dech019,g_dech_d[l_ac].dech021,g_dech_d[l_ac].dech029,g_dech_d[l_ac].dech030, 
       g_dech_d[l_ac].dech020,g_dech_d[l_ac].dech022,g_dech_d[l_ac].dech023,g_dech_d[l_ac].dech024,g_dech_d[l_ac].dech033, 
       g_dech_d[l_ac].dech027,g_dech_d[l_ac].dech028,g_dech_d[l_ac].dech026,g_dech_d[l_ac].dech025
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #LET g_dech_d[l_ac].statepic = cl_get_actipic(g_dech_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL adeq154_detail_show("'1'")      
 
      CALL adeq154_dech_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
   LET g_error_show = 0
   
 
   
   CALL g_dech_d.deleteElement(g_dech_d.getLength())   
 
   #add-point:陣列長度調整
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = g_dech_d.getLength()
#  DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE adeq154_pb
 
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL adeq154_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL adeq154_detail_action_trans()
 
   IF g_dech_d.getLength() > 0 THEN
      LET l_ac = 1
      CALL adeq154_fetch()
   END IF
   
      CALL adeq154_filter_show('dechsite','b_dechsite')
   CALL adeq154_filter_show('dech001','b_dech001')
