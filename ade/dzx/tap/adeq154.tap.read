  <section id="adeq154.fetch" ver="4" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充2
PRIVATE FUNCTION adeq154_fetch()
   DEFINE li_ac           LIKE type_t.num10
   #add-point:fetch段define-標準
   {<point name="fetch.define" edit="s"/>}
   #end add-point
   #add-point:fetch段define-客製
   {<point name="fetch.define_customerization" edit="c"/>}
   #end add-point
   
   CALL g_dech2_d.clear()
 
   CALL g_dech3_d.clear()
 
 
   #add-point:陣列清空
   {<point name="fetch.array_clear" />}
   #end add-point
   
   LET li_ac = l_ac 
   
   #為避免影響執行效能，若是按上下筆就不重組SQL
   IF g_action_choice <> "fetch" OR cl_null(g_action_choice) THEN
      LET g_sql = "SELECT  UNIQUE deci012,'',deci013,'',deci015,deci019,deci024,deci025,deci026,deci020, 
          deci021,deci022,deci023,deci029 FROM deci_t",    
                  "",
                  " WHERE decient=? AND decisite=? AND deci005=? AND deci009=? AND deci010=? AND deci011=? AND deci027=? AND deci028=?"
  
      IF NOT cl_null(g_wc2_table2) THEN
         LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
      END IF
  
      LET g_sql = g_sql, " ORDER BY deci_t.deci012,deci_t.deci013" 
                         
      #add-point:單身填充前
      {<point name="fetch.before_fill2" />}
      #end add-point
 
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料   
      PREPARE adeq154_pb2 FROM g_sql
      DECLARE b_fill_curs2 CURSOR FOR adeq154_pb2
   END IF
 
   OPEN b_fill_curs2 USING g_enterprise,g_dech_d[g_detail_idx].dechsite
                                  ,g_dech_d[g_detail_idx].dech005
 
                                  ,g_dech_d[g_detail_idx].dech009
 
                                  ,g_dech_d[g_detail_idx].dech010
 
                                  ,g_dech_d[g_detail_idx].dech011
 
                                  ,g_dech_d[g_detail_idx].dech031
 
                                  ,g_dech_d[g_detail_idx].dech032
 
 
   LET l_ac = 1
   FOREACH b_fill_curs2 INTO g_dech2_d[l_ac].deci012,g_dech2_d[l_ac].deci012_desc,g_dech2_d[l_ac].deci013, 
       g_dech2_d[l_ac].deci013_desc,g_dech2_d[l_ac].deci015,g_dech2_d[l_ac].deci019,g_dech2_d[l_ac].deci024, 
       g_dech2_d[l_ac].deci025,g_dech2_d[l_ac].deci026,g_dech2_d[l_ac].deci020,g_dech2_d[l_ac].deci021, 
       g_dech2_d[l_ac].deci022,g_dech2_d[l_ac].deci023,g_dech2_d[l_ac].deci029
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      
 
      #add-point:b_fill段資料填充
      {<point name="fetch.fill2"/>}
      #end add-point
      
      CALL adeq154_detail_show("'2'")      
 
      CALL adeq154_deci_t_mask()
 
      IF l_ac > g_max_rec THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend =  "" 
         LET g_errparam.code   =  9035 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   #為避免影響執行效能，若是按上下筆就不重組SQL
   IF g_action_choice <> "fetch" OR cl_null(g_action_choice) THEN
      LET g_sql = "SELECT  UNIQUE decj013,decj012,'',decj014 FROM decj_t",    
                  "",
                  " WHERE decjent=? AND decjsite=? AND decj005=? AND decj009=? AND decj010=? AND decj011=? AND decj015=? AND decj016=?"
  
      IF NOT cl_null(g_wc2_table3) THEN
         LET g_sql = g_sql CLIPPED," AND ",g_wc2_table3 CLIPPED
      END IF
  
      LET g_sql = g_sql, " ORDER BY decj_t.decj012" 
                         
      #add-point:單身填充前
      {<point name="fetch.before_fill3" />}
      #end add-point
 
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料   
      PREPARE adeq154_pb3 FROM g_sql
      DECLARE b_fill_curs3 CURSOR FOR adeq154_pb3
   END IF
 
   OPEN b_fill_curs3 USING g_enterprise,g_dech_d[g_detail_idx].dechsite
                                  ,g_dech_d[g_detail_idx].dech005
 
                                  ,g_dech_d[g_detail_idx].dech009
 
                                  ,g_dech_d[g_detail_idx].dech010
 
                                  ,g_dech_d[g_detail_idx].dech011
 
                                  ,g_dech_d[g_detail_idx].dech031
 
                                  ,g_dech_d[g_detail_idx].dech032
 
 
   LET l_ac = 1
   FOREACH b_fill_curs3 INTO g_dech3_d[l_ac].decj013,g_dech3_d[l_ac].decj012,g_dech3_d[l_ac].decj012_desc, 
       g_dech3_d[l_ac].decj014
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      
 
      #add-point:b_fill段資料填充
      {<point name="fetch.fill3"/>}
      #end add-point
      
      CALL adeq154_detail_show("'3'")      
 
      CALL adeq154_decj_t_mask()
 
      IF l_ac > g_max_rec THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend =  "" 
         LET g_errparam.code   =  9035 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
 
 
   
   #add-point:單身填充後
   {<point name="fetch.after_fill" />}
   #end add-point 
   
   CALL g_dech2_d.deleteElement(g_dech2_d.getLength())   
 
   #單身總筆數顯示
   LET g_detail_cnt2 = g_dech2_d.getLength()
   DISPLAY g_detail_cnt2 TO FORMONLY.cnt
   IF g_detail_cnt2 > 0 THEN
      LET g_detail_idx2 = 1
      DISPLAY g_detail_idx2 TO FORMONLY.idx
   ELSE
      LET g_detail_idx2 = 0
      DISPLAY ' ' TO FORMONLY.idx
   END IF
 
   CALL g_dech3_d.deleteElement(g_dech3_d.getLength())   
 
   #單身總筆數顯示
   LET g_detail_cnt2 = g_dech3_d.getLength()
   DISPLAY g_detail_cnt2 TO FORMONLY.cnt
   IF g_detail_cnt2 > 0 THEN
      LET g_detail_idx2 = 1
      DISPLAY g_detail_idx2 TO FORMONLY.idx
   ELSE
      LET g_detail_idx2 = 0
      DISPLAY ' ' TO FORMONLY.idx
   END IF
 
 
   #add-point:陣列筆數調整
   {<point name="fetch.array_deleteElement" />}
   #end add-point
 
   LET l_ac = li_ac
   
END FUNCTION
