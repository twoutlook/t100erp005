  <point name="fetch.after_fill" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   LET l_ac = 1
   FOREACH b_fill_curs1 USING g_enterprise INTO g_dech4_d[l_ac].decj013_1,g_dech4_d[l_ac].decj012_1,
                                                g_dech4_d[l_ac].decj012_1_desc,   #150302-00004#8 150312 by lori522612 add
                                                g_dech4_d[l_ac].decj014_1
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "FOREACH:"
         LET g_errparam.popup = TRUE
         CALL cl_err()

         EXIT FOREACH
      END IF
      
      #CALL adeq145_decj009_ref2()   #150302-00004#8 150312 by lori522612 mark

      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
   END FOREACH]]>
  </point>
  <point name="fetch.before_fill2" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[      LET g_sql = "SELECT  UNIQUE deci012,t1.mmanl003,deci013,t2.oocql004,deci015,deci019,deci024,deci025,deci026,deci020, 
          deci021,deci022,deci023,deci029 FROM deci_t",    
          "       LEFT JOIN mmanl_t t1 ON t1.mmanlent = decient AND t1.mmanl001 = deci012 AND t1.mmanl002 = '",g_dlang,"' ",
