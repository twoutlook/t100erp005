  <point name="b_fill.fill" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
              LET g_sql = " SELECT decj013,decj012,ooial003,SUM(decj014) ",
                  "   FROM  (SELECT UNIQUE dechent,dechsite,dech005,dech009,dech010,dech011,dech031,dech032, ",
                  "                        decj013,decj012,decj014,t1.ooial003 ",
                  "            FROM dech_t",
                  "                 LEFT JOIN deci_t ON decient = dechent AND dechsite = decisite ",
                  "                                 AND dech005 = deci005 AND dech009 = deci009 ",
                  "                                 AND dech010 = deci010 AND dech011 = deci011 ",
                  "                                 AND dech031 = deci027 AND dech032 = deci028 ",
                  "                 LEFT JOIN decj_t ON decjent = dechent AND dechsite = decjsite ",
                  "                                 AND dech005 = decj005 AND dech009 = decj009 ",
                  "                                 AND dech010 = decj010 AND dech011 = decj011 ",
                  "                                 AND dech031 = decj015 AND dech032 = decj016 ",
                  "                LEFT JOIN ooial_t t1 ON t1.ooialent = dechent AND t1.ooial001 = decj012 ",
                  "                                    AND t1.ooial002 = '",g_dlang,"' ",
                  "           WHERE dechent = ? AND decj012 IS NOT NULL AND ", ls_wc,")",
                  " GROUP BY decj013,decj012,ooial003 ",
                  " ORDER BY decj013,decj012 "

      PREPARE adeq154_pb1 FROM g_sql
      DECLARE b_fill_curs1 CURSOR FOR adeq154_pb1]]>
  </point>
  <point name="b_fill.sql_after" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[    LET g_sql = "SELECT dechsite,t1.ooefl003,dech001,dech005,t2.inayl003,dech031,dech032,dech006,dech007,dech008,dech009, 
       t3.rtaxl003,dech010,dech011,t4.oodbl004,dech012,dech013,dech014,dech015,dech016,dech017,dech018,dech019,dech021, 
       dech029,dech030,dech020,dech022,dech023,dech024,dech033,dech027,dech028,dech026,dech025",
               " FROM dech_t ",
       "       LEFT JOIN ooefl_t t1 ON t1.ooeflent = dechent AND t1.ooefl001 = dechsite AND t1.ooefl002 = '",g_dlang,"' ",
       # "       LEFT JOIN ooefl_t t2 ON t2.ooeflent = dechent AND t2.ooefl001 = dech005  AND t2.ooefl002 = '",g_dlang,"' ",       
       "       LEFT JOIN inayl_t t2 ON t2.inaylent = dechent AND t2.inayl001 = dech005  AND t2.inayl002 = '",g_dlang,"' ",   
       "       LEFT JOIN rtaxl_t t3 ON t3.rtaxlent = dechent AND t3.rtaxl001 = dech009 AND t3.rtaxl002 = '",g_dlang,"' ",
      
       "       LEFT JOIN ooef_t t5 ON t5.ooefent = dechent AND t5.ooef001 = dechsite ",
       "       LEFT JOIN oodbl_t t4 ON t4.oodblent = dechent AND t4.oodbl002 = dech011 AND t4.oodbl001 =t5.ooef019 AND t4.oodbl003 = '",g_dlang,"' ",
     
       " WHERE dechent = ? ",
       "   AND ", ls_wc,cl_sql_add_filter("dech_t")
       
       LET g_sql = g_sql, cl_sql_add_filter("dech_t"),
                      " ORDER BY dech_t.dechsite,dech_t.dech005,dech_t.dech009,dech_t.dech010,dech_t.dech011,dech_t.dech031,dech_t.dech032"]]>
  </point>
  <point name="b_fill.sql_before" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   CALL s_aooi500_sql_where(g_prog,'dechsite') RETURNING l_where
   LET g_wc = g_wc," AND ",l_where]]>
