  <section id="adeq174.b_fill" ver="2" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION adeq174_b_fill()
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準
   {<point name="b_fill.define" edit="s"/>}
   #end add-point
   #add-point:b_fill段define-客製
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   LET ls_sql_rank = "SELECT  UNIQUE dedfsite,'',dedf001,dedf031,dedf032,dedf005,'',dedf006,dedf007, 
       dedf008,dedf009,'',dedf010,dedf011,'',dedf012,dedf013,dedf014,dedf015,dedf016,dedf017,dedf018, 
       dedf019,dedf021,dedf029,dedf030,dedf020,dedf022,dedf023,dedf024,dedf033,dedf027,dedf028,dedf026, 
       dedf025  ,DENSE_RANK() OVER( ORDER BY dedf_t.dedfsite,dedf_t.dedf005,dedf_t.dedf009,dedf_t.dedf010, 
       dedf_t.dedf011,dedf_t.dedf031,dedf_t.dedf032) AS RANK FROM dedf_t",
 
                     " LEFT JOIN dedg_t ON dedgent = dedfent AND dedfsite = dedgsite AND dedf005 = dedg005 AND dedf009 = dedg009 AND dedf010 = dedg010 AND dedf011 = dedg011 AND dedf031 = dedg027 AND dedf032 = dedg028",
 
                     " LEFT JOIN dedh_t ON dedhent = dedfent AND dedfsite = dedhsite AND dedf005 = dedh005 AND dedf009 = dedh009 AND dedf010 = dedh010 AND dedf011 = dedh011 AND dedf031 = dedh015 AND dedf032 = dedh016",
 
 
                     "",
                     " WHERE dedfent= ? AND 1=1 AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("dedf_t"),
                     " ORDER BY dedf_t.dedfsite,dedf_t.dedf005,dedf_t.dedf009,dedf_t.dedf010,dedf_t.dedf011,dedf_t.dedf031,dedf_t.dedf032"
 
   #add-point:b_fill段rank_sql_after
   {<point name="b_fill.rank_sql_after"/>}
   #end add-point
 
   LET g_sql = "SELECT COUNT(*) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT dedfsite,'',dedf001,dedf031,dedf032,dedf005,'',dedf006,dedf007,dedf008,dedf009, 
       '',dedf010,dedf011,'',dedf012,dedf013,dedf014,dedf015,dedf016,dedf017,dedf018,dedf019,dedf021, 
       dedf029,dedf030,dedf020,dedf022,dedf023,dedf024,dedf033,dedf027,dedf028,dedf026,dedf025",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE adeq174_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR adeq174_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_dedf_d.clear()
 
   #add-point:陣列清空
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1   
 
   FOREACH b_fill_curs INTO g_dedf_d[l_ac].dedfsite,g_dedf_d[l_ac].dedfsite_desc,g_dedf_d[l_ac].dedf001, 
       g_dedf_d[l_ac].dedf031,g_dedf_d[l_ac].dedf032,g_dedf_d[l_ac].dedf005,g_dedf_d[l_ac].dedf005_desc, 
       g_dedf_d[l_ac].dedf006,g_dedf_d[l_ac].dedf007,g_dedf_d[l_ac].dedf008,g_dedf_d[l_ac].dedf009,g_dedf_d[l_ac].dedf009_desc, 
       g_dedf_d[l_ac].dedf010,g_dedf_d[l_ac].dedf011,g_dedf_d[l_ac].dedf011_desc,g_dedf_d[l_ac].dedf012, 
       g_dedf_d[l_ac].dedf013,g_dedf_d[l_ac].dedf014,g_dedf_d[l_ac].dedf015,g_dedf_d[l_ac].dedf016,g_dedf_d[l_ac].dedf017, 
       g_dedf_d[l_ac].dedf018,g_dedf_d[l_ac].dedf019,g_dedf_d[l_ac].dedf021,g_dedf_d[l_ac].dedf029,g_dedf_d[l_ac].dedf030, 
       g_dedf_d[l_ac].dedf020,g_dedf_d[l_ac].dedf022,g_dedf_d[l_ac].dedf023,g_dedf_d[l_ac].dedf024,g_dedf_d[l_ac].dedf033, 
       g_dedf_d[l_ac].dedf027,g_dedf_d[l_ac].dedf028,g_dedf_d[l_ac].dedf026,g_dedf_d[l_ac].dedf025
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #LET g_dedf_d[l_ac].statepic = cl_get_actipic(g_dedf_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL adeq174_detail_show("'1'")      
 
      CALL adeq174_dedf_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
   LET g_error_show = 0
   
 
   
   CALL g_dedf_d.deleteElement(g_dedf_d.getLength())   
 
   #add-point:陣列長度調整
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = g_dedf_d.getLength()
#  DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE adeq174_pb
 
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL adeq174_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL adeq174_detail_action_trans()
 
   IF g_dedf_d.getLength() > 0 THEN
      LET l_ac = 1
      CALL adeq174_fetch()
   END IF
   
      CALL adeq174_filter_show('dedfsite','b_dedfsite')
