  <section id="adeq174.fetch" ver="2" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充2
PRIVATE FUNCTION adeq174_fetch()
   DEFINE li_ac           LIKE type_t.num10
   #add-point:fetch段define-標準
   {<point name="fetch.define" edit="s"/>}
   #end add-point
   #add-point:fetch段define-客製
   {<point name="fetch.define_customerization" edit="c"/>}
   #end add-point
   
   CALL g_dedf2_d.clear()
 
   CALL g_dedf3_d.clear()
 
 
   #add-point:陣列清空
   {<point name="fetch.array_clear" />}
   #end add-point
   
   LET li_ac = l_ac 
   
   #為避免影響執行效能，若是按上下筆就不重組SQL
   IF g_action_choice <> "fetch" OR cl_null(g_action_choice) THEN
      LET g_sql = "SELECT  UNIQUE dedg012,'',dedg013,'',dedg014,dedg015,dedg019,dedg024,dedg025,dedg026, 
          dedg020,dedg021,dedg022,dedg023,dedg029 FROM dedg_t",    
                  "",
                  " WHERE dedgent=? AND dedgsite=? AND dedg005=? AND dedg009=? AND dedg010=? AND dedg011=? AND dedg027=? AND dedg028=?"
  
      IF NOT cl_null(g_wc2_table2) THEN
         LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
      END IF
  
      LET g_sql = g_sql, " ORDER BY dedg_t.dedg012,dedg_t.dedg013" 
                         
      #add-point:單身填充前
      {<point name="fetch.before_fill2" />}
      #end add-point
 
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料   
      PREPARE adeq174_pb2 FROM g_sql
      DECLARE b_fill_curs2 CURSOR FOR adeq174_pb2
   END IF
 
   OPEN b_fill_curs2 USING g_enterprise,g_dedf_d[g_detail_idx].dedfsite
                                  ,g_dedf_d[g_detail_idx].dedf005
 
