  <point name="fetch.after_fill" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   LET l_ac = 1
   FOREACH b_fill_curs1 USING g_enterprise INTO g_dedf4_d[l_ac].dedh013_1,g_dedf4_d[l_ac].dedh012_1,
                                                g_dedf4_d[l_ac].dedh012_1_desc,
