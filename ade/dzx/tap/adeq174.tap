  <point name="b_fill.fill" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[      LET g_sql = " SELECT dedh013,dedh012,ooial003,SUM(dedh014) ",
                  "   FROM  (SELECT UNIQUE dedfent,dedfsite,dedf005,dedf009,dedf010,dedf011,dedf031,dedf032, ",
                  "                        dedh013,dedh012,dedh014,t1.ooial003 ",
                  "            FROM dedf_t",
                  "                 LEFT JOIN dedg_t ON dedfent = dedgent AND dedfsite = dedgsite ",
                  "                                 AND dedf005 = dedg005 AND dedf009  = dedg009 ",
                  "                                 AND dedf010 = dedg010 AND dedf011  = dedg011 ",
                  "                                 AND dedf031 = dedg027 AND dedf032  = dedg028 ",
                  "                 LEFT JOIN dedh_t ON dedfent = dedhent AND dedfsite = dedhsite ",
                  "                                 AND dedf005 = dedh005 AND dedf009  = dedh009 ",
                  "                                 AND dedf010 = dedh010 AND dedf011  = dedh011 ",
                  "                                 AND dedf031 = dedh015 AND dedf032  = dedh016 ",
                  "                LEFT JOIN ooial_t t1 ON t1.ooialent = dedfent AND t1.ooial001 = dedh012 ",
                  "                                    AND t1.ooial002 = '",g_dlang,"' ",
                  "           WHERE dedfent = ? AND dedh012 IS NOT NULL AND ", ls_wc,")",
                  " GROUP BY dedh013,dedh012,ooial003 ",
                  " ORDER BY dedh013,dedh012 "

      PREPARE adeq174_pb1 FROM g_sql
      DECLARE b_fill_curs1 CURSOR FOR adeq174_pb1]]>
  </point>
  <point name="b_fill.sql_after" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   LET g_sql = "SELECT dedfsite,  t1.ooefl003,  dedf001,  dedf031,  dedf032,   ",
               "       dedf005,   t2.inayl003,  dedf006,  dedf007,  dedf008,   ",
               "       dedf009,   t3.rtaxl003,  dedf010,  dedf011,  '',        ",
               "       dedf012,   dedf013,      dedf014,  dedf015,  dedf016,   ",
               "       dedf017,   dedf018,      dedf019,  dedf021,  dedf029,   ",
               "       dedf030,   dedf020,      dedf022,  dedf023,  dedf024,   ",
               "       dedf033,   dedf027,      dedf028,  dedf026,  dedf025",   #150705-00001#5 Add By Ken 150706 加客單價(dedef033)
               " FROM dedf_t ",
       "       LEFT JOIN ooefl_t t1 ON t1.ooeflent = dedfent AND t1.ooefl001 = dedfsite AND t1.ooefl002 = '",g_dlang,"' ",

       "       LEFT JOIN inayl_t t2 ON t2.inaylent = dedfent AND t2.inayl001 = dedf005  AND t2.inayl002 = '",g_dlang,"' ",         
       "       LEFT JOIN rtaxl_t t3 ON t3.rtaxlent = dedfent AND t3.rtaxl001 = dedf009 AND t3.rtaxl002 = '",g_dlang,"' ",
      
       "       LEFT JOIN ooef_t t5 ON t5.ooefent = dedfent AND t5.ooef001 = dedfsite ",
       "       LEFT JOIN oodbl_t t4 ON t4.oodblent = dedfent AND t4.oodbl002 = dedf011 AND t4.oodbl001 =t5.ooef019 AND t4.oodbl003 = '",g_dlang,"' ",
     
       " WHERE dedfent = ? ",
       "   AND ", ls_wc,cl_sql_add_filter("dedf_t")
       
       LET g_sql = g_sql, cl_sql_add_filter("dedf_t"),
                      " ORDER BY dedf_t.dedfsite,dedf_t.dedf005,dedf_t.dedf009,dedf_t.dedf010,dedf_t.dedf011,dedf_t.dedf031,dedf_t.dedf032"
   ]]>
  </point>
  <point name="b_fill.sql_before" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   CALL s_aooi500_sql_where(g_prog,'dedfsite') RETURNING l_where
   LET g_wc = g_wc," AND ",l_where]]>
