#+ 單身陣列填充
PRIVATE FUNCTION adeq620_b_fill()
   #add-point:b_fill段define-客製 name="b_fill.define_customerization"
   {<point name="b_fill.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準  (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   {<point name="b_fill.define"/>}
   #end add-point
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter, cl_sql_auth_filter()   #(ver:40) add cl_sql_auth_filter()
 
   LET ls_sql_rank = "SELECT  UNIQUE '',dembsite,'',demb001,demb002,demb003,demb004,demb005,'',demb048, 
       '',demb006,demb007,demb008,'',demb009,'',demb011,demb010,'','',demb012,demb013,'',demb014,'', 
       demb015,'',demb016,'',demb017,demb019,demb018,'',demb020,demb021,demb022,demb023,demb024,demb025, 
       demb026,demb027,demb028,demb029,demb030,demb031,demb033,demb034,demb035,demb036,demb037,demb038, 
       demb039,demb040,demb041,demb042,demb043,demb044,demb045,demb046,demb047  ,DENSE_RANK() OVER( ORDER BY demb_t.dembsite, 
       demb_t.demb002,demb_t.demb005,demb_t.demb006,demb_t.demb007,demb_t.demb008,demb_t.demb009,demb_t.demb010, 
       demb_t.demb014,demb_t.demb016) AS RANK FROM demb_t",
 
 
                     "",
                     " WHERE dembent= ? AND 1=1 AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("demb_t"),
                     " ORDER BY demb_t.dembsite,demb_t.demb002,demb_t.demb005,demb_t.demb006,demb_t.demb007,demb_t.demb008,demb_t.demb009,demb_t.demb010,demb_t.demb014,demb_t.demb016"
 
   #add-point:b_fill段rank_sql_after name="b_fill.rank_sql_after"
   {<point name="b_fill.rank_sql_after"/>}
   #end add-point
 
   LET g_sql = "SELECT COUNT(1) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   #add-point:b_fill段rank_sql_after_count name="b_fill.rank_sql_after_count"
   {<point name="b_fill.rank_sql_after_count"/>}
   #end add-point
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT '',dembsite,'',demb001,demb002,demb003,demb004,demb005,'',demb048,'',demb006, 
       demb007,demb008,'',demb009,'',demb011,demb010,'','',demb012,demb013,'',demb014,'',demb015,'', 
       demb016,'',demb017,demb019,demb018,'',demb020,demb021,demb022,demb023,demb024,demb025,demb026, 
       demb027,demb028,demb029,demb030,demb031,demb033,demb034,demb035,demb036,demb037,demb038,demb039, 
       demb040,demb041,demb042,demb043,demb044,demb045,demb046,demb047",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE adeq620_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR adeq620_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_demb_d.clear()
 
   #add-point:陣列清空 name="b_fill.array_clear"
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1   
 
   FOREACH b_fill_curs INTO g_demb_d[l_ac].sel,g_demb_d[l_ac].dembsite,g_demb_d[l_ac].dembsite_desc, 
       g_demb_d[l_ac].demb001,g_demb_d[l_ac].demb002,g_demb_d[l_ac].demb003,g_demb_d[l_ac].demb004,g_demb_d[l_ac].demb005, 
       g_demb_d[l_ac].demb005_desc,g_demb_d[l_ac].demb048,g_demb_d[l_ac].demb048_desc,g_demb_d[l_ac].demb006, 
       g_demb_d[l_ac].demb007,g_demb_d[l_ac].demb008,g_demb_d[l_ac].demb008_desc,g_demb_d[l_ac].demb009, 
       g_demb_d[l_ac].demb009_desc,g_demb_d[l_ac].demb011,g_demb_d[l_ac].demb010,g_demb_d[l_ac].demb010_desc, 
       g_demb_d[l_ac].demb010_desc_1,g_demb_d[l_ac].demb012,g_demb_d[l_ac].demb013,g_demb_d[l_ac].demb013_desc, 
       g_demb_d[l_ac].demb014,g_demb_d[l_ac].demb014_desc,g_demb_d[l_ac].demb015,g_demb_d[l_ac].demb015_desc, 
       g_demb_d[l_ac].demb016,g_demb_d[l_ac].demb016_desc,g_demb_d[l_ac].demb017,g_demb_d[l_ac].demb019, 
       g_demb_d[l_ac].demb018,g_demb_d[l_ac].demb018_desc,g_demb_d[l_ac].demb020,g_demb_d[l_ac].demb021, 
       g_demb_d[l_ac].demb022,g_demb_d[l_ac].demb023,g_demb_d[l_ac].demb024,g_demb_d[l_ac].demb025,g_demb_d[l_ac].demb026, 
       g_demb_d[l_ac].demb027,g_demb_d[l_ac].demb028,g_demb_d[l_ac].demb029,g_demb_d[l_ac].demb030,g_demb_d[l_ac].demb031, 
       g_demb_d[l_ac].demb033,g_demb_d[l_ac].demb034,g_demb_d[l_ac].demb035,g_demb_d[l_ac].demb036,g_demb_d[l_ac].demb037, 
       g_demb_d[l_ac].demb038,g_demb_d[l_ac].demb039,g_demb_d[l_ac].demb040,g_demb_d[l_ac].demb041,g_demb_d[l_ac].demb042, 
       g_demb_d[l_ac].demb043,g_demb_d[l_ac].demb044,g_demb_d[l_ac].demb045,g_demb_d[l_ac].demb046,g_demb_d[l_ac].demb047 

      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code = SQLCA.SQLCODE 
         LET g_errparam.popup = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #LET g_demb_d[l_ac].statepic = cl_get_actipic(g_demb_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充 name="b_fill.fill"
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL adeq620_detail_show("'1'")      
 
      CALL adeq620_demb_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "" 
            LET g_errparam.code = 9035 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
   LET g_error_show = 0
   
 
   
   CALL g_demb_d.deleteElement(g_demb_d.getLength())   
 
   #add-point:陣列長度調整 name="b_fill.array_deleteElement"
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
   
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = g_demb_d.getLength()
#  DISPLAY g_detail_cnt TO FORMONLY.h_count
