   {<point name="fetch.before_function" />}
   #end add-point
 
   CALL g_dedi2_d.clear()

   CALL g_dedi3_d.clear()

   CALL g_dedi4_d.clear()


   #add-point:陣列清空 name="fetch.array_clear"
   {<point name="fetch.array_clear" />}
   #end add-point
   
   LET li_ac = l_ac 
   
   #為避免影響執行效能，若是按上下筆就不重組SQL
   IF g_action_choice <> "fetch" OR cl_null(g_action_choice) THEN
      LET g_sql = "SELECT  UNIQUE dedj008,'',dedj009,'',dedj010,dedj011,dedj015,dedj020,dedj021,dedj022,dedj016,dedj017,dedj018,dedj019,dedj012,dedj013,dedj014,dedj001 FROM dedj_t",    
                  "",
                  " WHERE dedjent=? AND dedjsite=? AND dedj005=? AND dedj006=? AND dedj007=? AND dedj023=? AND dedj024=?"
  
      IF NOT cl_null(g_wc2_table2) THEN
         LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
      END IF
  
      LET g_sql = g_sql, " ORDER BY dedj_t.dedj008,dedj_t.dedj009" 
                         
      #add-point:單身填充前 name="fetch.before_fill2"
      {<point name="fetch.before_fill2" />}
      #end add-point
 
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料   
      PREPARE adeq175_pb2 FROM g_sql
      DECLARE b_fill_curs2 CURSOR FOR adeq175_pb2
   END IF

#(ver:42) mark ---start---
