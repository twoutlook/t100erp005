#+ 單身陣列填充
PRIVATE FUNCTION adeq175_b_fill()
   #add-point:b_fill段define-客製 name="b_fill.define_customerization"
   {<point name="b_fill.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準  (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   {<point name="b_fill.define"/>}
   #end add-point
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter, cl_sql_auth_filter()   #(ver:40) add cl_sql_auth_filter()
 
   LET ls_sql_rank = "SELECT  UNIQUE dedisite,'',dedi001,dedi027,dedi028,dedi005,'',dedi007,'',dedi008,'',dedi009,dedi010,dedi011,dedi012,dedi013,dedi014,dedi015,dedi016,dedi025,dedi026,dedi017,dedi018,dedi019,dedi020,dedi023,dedi024,dedi021,dedi022,dedi006  ,DENSE_RANK() OVER( ORDER BY dedi_t.dedisite,dedi_t.dedi005,dedi_t.dedi007,dedi_t.dedi008,dedi_t.dedi027,dedi_t.dedi028) AS RANK FROM dedi_t",

                     " LEFT JOIN dedj_t ON dedjent = dedient AND dedisite = dedjsite AND dedi005 = dedj005 AND dedi007 = dedj006 AND dedi008 = dedj007 AND dedi027 = dedj023 AND dedi028 = dedj024",

                     " LEFT JOIN dedk_t ON dedkent = dedient AND dedisite = dedksite AND dedi005 = dedk005 AND dedi007 = dedk007 AND dedi008 = dedk008 AND dedi027 = dedk012 AND dedi028 = dedk013",


                     "",
                     " WHERE dedient= ? AND 1=1 AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("dedi_t"),
                     " ORDER BY dedi_t.dedisite,dedi_t.dedi005,dedi_t.dedi007,dedi_t.dedi008,dedi_t.dedi027,dedi_t.dedi028"
 
   #add-point:b_fill段rank_sql_after name="b_fill.rank_sql_after"
   {<point name="b_fill.rank_sql_after"/>}
   #end add-point
 
   LET g_sql = "SELECT COUNT(1) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   #add-point:b_fill段rank_sql_after_count name="b_fill.rank_sql_after_count"
   {<point name="b_fill.rank_sql_after_count"/>}
   #end add-point
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT dedisite,'',dedi001,dedi027,dedi028,dedi005,'',dedi007,'',dedi008,'',dedi009,dedi010,dedi011,dedi012,dedi013,dedi014,dedi015,dedi016,dedi025,dedi026,dedi017,dedi018,dedi019,dedi020,dedi023,dedi024,dedi021,dedi022,dedi006",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE adeq175_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR adeq175_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_dedi_d.clear()

   #add-point:陣列清空 name="b_fill.array_clear"
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1   
 
   FOREACH b_fill_curs INTO g_dedi_d[l_ac].dedisite,g_dedi_d[l_ac].dedisite_desc,g_dedi_d[l_ac].dedi001,g_dedi_d[l_ac].dedi027,g_dedi_d[l_ac].dedi028,g_dedi_d[l_ac].dedi005,g_dedi_d[l_ac].dedi005_desc,g_dedi_d[l_ac].dedi007,g_dedi_d[l_ac].dedi007_desc,g_dedi_d[l_ac].dedi008,g_dedi_d[l_ac].dedi008_desc,g_dedi_d[l_ac].dedi009,g_dedi_d[l_ac].dedi010,g_dedi_d[l_ac].dedi011,g_dedi_d[l_ac].dedi012,g_dedi_d[l_ac].dedi013,g_dedi_d[l_ac].dedi014,g_dedi_d[l_ac].dedi015,g_dedi_d[l_ac].dedi016,g_dedi_d[l_ac].dedi025,g_dedi_d[l_ac].dedi026,g_dedi_d[l_ac].dedi017,g_dedi_d[l_ac].dedi018,g_dedi_d[l_ac].dedi019,g_dedi_d[l_ac].dedi020,g_dedi_d[l_ac].dedi023,g_dedi_d[l_ac].dedi024,g_dedi_d[l_ac].dedi021,g_dedi_d[l_ac].dedi022,g_dedi_d[l_ac].dedi006
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code = SQLCA.SQLCODE 
         LET g_errparam.popup = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #LET g_dedi_d[l_ac].statepic = cl_get_actipic(g_dedi_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充 name="b_fill.fill"
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL adeq175_detail_show("'1'")      
 
      CALL adeq175_dedi_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "" 
            LET g_errparam.code = 9035 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
   LET g_error_show = 0
   

   
   CALL g_dedi_d.deleteElement(g_dedi_d.getLength())   

   #add-point:陣列長度調整 name="b_fill.array_deleteElement"
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
   
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = g_dedi_d.getLength()
#  DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE adeq175_pb
 
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL adeq175_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL adeq175_detail_action_trans()
 
   IF g_dedi_d.getLength() > 0 THEN
      LET l_ac = 1
      CALL adeq175_fetch()
   END IF
   
      CALL adeq175_filter_show('dedisite','b_dedisite')
   CALL adeq175_filter_show('dedi001','b_dedi001')
   CALL adeq175_filter_show('dedi027','b_dedi027')
   CALL adeq175_filter_show('dedi028','b_dedi028')
