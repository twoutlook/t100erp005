{<section id="adeq211.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION adeq211_b_fill()
   #add-point:b_fill段define-客製 name="b_fill.define_customerization"
   {<point name="b_fill.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準  (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   {<point name="b_fill.define"/>}
   #end add-point
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter, cl_sql_auth_filter()   #(ver:40) add cl_sql_auth_filter()
 
   LET ls_sql_rank = "SELECT  UNIQUE '',rtjfsite,'',rtjf025,rtjf030,'',rtjfdocno,rtjfseq,rtjfseq1,rtjf001,rtjf002,'',rtjf003,rtjf004,rtjf005,rtjf006,rtjf007,rtjf008,rtjf009,rtjf010,rtjf011,rtjf012,rtjf013,rtjf014,rtjf015,rtjf016,rtjf017,rtjf018,rtjf019,rtjf020,rtjf021,rtjf022,rtjf023,rtjf024,rtjf026,rtjf027,'',rtjf028,rtjf029,'',rtjf104,'','','','','',rtjf106,'',rtjf107,'','','',''  ,DENSE_RANK() OVER( ORDER BY rtjf_t.rtjfdocno,rtjf_t.rtjfseq,rtjf_t.rtjfseq1) AS RANK FROM rtjf_t",


                     "",
                     " WHERE rtjfent= ? AND 1=1 AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("rtjf_t"),
                     " ORDER BY rtjf_t.rtjfdocno,rtjf_t.rtjfseq,rtjf_t.rtjfseq1"
 
   #add-point:b_fill段rank_sql_after name="b_fill.rank_sql_after"
   {<point name="b_fill.rank_sql_after"/>}
   #end add-point
 
   LET g_sql = "SELECT COUNT(1) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   #add-point:b_fill段rank_sql_after_count name="b_fill.rank_sql_after_count"
   {<point name="b_fill.rank_sql_after_count"/>}
   #end add-point
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT '',rtjfsite,'',rtjf025,rtjf030,'',rtjfdocno,rtjfseq,rtjfseq1,rtjf001,rtjf002,'',rtjf003,rtjf004,rtjf005,rtjf006,rtjf007,rtjf008,rtjf009,rtjf010,rtjf011,rtjf012,rtjf013,rtjf014,rtjf015,rtjf016,rtjf017,rtjf018,rtjf019,rtjf020,rtjf021,rtjf022,rtjf023,rtjf024,rtjf026,rtjf027,'',rtjf028,rtjf029,'',rtjf104,'','','','','',rtjf106,'',rtjf107,'','','',''",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE adeq211_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR adeq211_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_rtjf_d.clear()

   #add-point:陣列清空 name="b_fill.array_clear"
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1   
 
   FOREACH b_fill_curs INTO g_rtjf_d[l_ac].sel,g_rtjf_d[l_ac].rtjfsite,g_rtjf_d[l_ac].rtjfsite_desc,g_rtjf_d[l_ac].rtjf025,g_rtjf_d[l_ac].rtjf030,g_rtjf_d[l_ac].rtjf030_desc,g_rtjf_d[l_ac].rtjfdocno,g_rtjf_d[l_ac].rtjfseq,g_rtjf_d[l_ac].rtjfseq1,g_rtjf_d[l_ac].rtjf001,g_rtjf_d[l_ac].rtjf002,g_rtjf_d[l_ac].rtjf002_desc,g_rtjf_d[l_ac].rtjf003,g_rtjf_d[l_ac].rtjf004,g_rtjf_d[l_ac].rtjf005,g_rtjf_d[l_ac].rtjf006,g_rtjf_d[l_ac].rtjf007,g_rtjf_d[l_ac].rtjf008,g_rtjf_d[l_ac].rtjf009,g_rtjf_d[l_ac].rtjf010,g_rtjf_d[l_ac].rtjf011,g_rtjf_d[l_ac].rtjf012,g_rtjf_d[l_ac].rtjf013,g_rtjf_d[l_ac].rtjf014,g_rtjf_d[l_ac].rtjf015,g_rtjf_d[l_ac].rtjf016,g_rtjf_d[l_ac].rtjf017,g_rtjf_d[l_ac].rtjf018,g_rtjf_d[l_ac].rtjf019,g_rtjf_d[l_ac].rtjf020,g_rtjf_d[l_ac].rtjf021,g_rtjf_d[l_ac].rtjf022,g_rtjf_d[l_ac].rtjf023,g_rtjf_d[l_ac].rtjf024,g_rtjf_d[l_ac].rtjf026,g_rtjf_d[l_ac].rtjf027,g_rtjf_d[l_ac].rtjf027_desc,g_rtjf_d[l_ac].rtjf028,g_rtjf_d[l_ac].rtjf029,g_rtjf_d[l_ac].rtjf029_desc,g_rtjf_d[l_ac].rtjf104,g_rtjf_d[l_ac].rtja105,g_rtjf_d[l_ac].rtja101,g_rtjf_d[l_ac].rtja101_desc,g_rtjf_d[l_ac].rtja102,g_rtjf_d[l_ac].rtja102_desc,g_rtjf_d[l_ac].rtjf106,g_rtjf_d[l_ac].rtjf106_desc,g_rtjf_d[l_ac].rtjf107,g_rtjf_d[l_ac].rtja032,g_rtjf_d[l_ac].rtja033,g_rtjf_d[l_ac].rtja034,g_rtjf_d[l_ac].rtja035
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code = SQLCA.SQLCODE 
         LET g_errparam.popup = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #LET g_rtjf_d[l_ac].statepic = cl_get_actipic(g_rtjf_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充 name="b_fill.fill"
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL adeq211_detail_show("'1'")      
 
      CALL adeq211_rtjf_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "" 
            LET g_errparam.code = 9035 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
   LET g_error_show = 0
   

   
   CALL g_rtjf_d.deleteElement(g_rtjf_d.getLength())   

   #add-point:陣列長度調整 name="b_fill.array_deleteElement"
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
   
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = g_rtjf_d.getLength()
#  DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE adeq211_pb
 
   #調整單身index指標，避免翻頁後指到空白筆數
