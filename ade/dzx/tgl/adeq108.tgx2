{<section id="adeq108.fetch" >}
#+ 單身陣列填充2
PRIVATE FUNCTION adeq108_fetch()
   #add-point:fetch段define-客製 name="fetch.define_customerization"
   {<point name="fetch.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE li_ac           LIKE type_t.num10
   #add-point:fetch段define-標準  (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="fetch.define"
   {<point name="fetch.define"/>}
   #end add-point
   
   #add-point:FUNCTION前置處理 name="fetch.before_function"
   {<point name="fetch.before_function" />}
   #end add-point
 
   CALL g_debw2_d.clear()
 
 
   #add-point:陣列清空 name="fetch.array_clear"
   {<point name="fetch.array_clear" />}
   #end add-point
   
   LET li_ac = l_ac 
   
   #為避免影響執行效能，若是按上下筆就不重組SQL
   IF g_action_choice <> "fetch" OR cl_null(g_action_choice) THEN
      LET g_sql = "SELECT  UNIQUE debx010,'',debx011,'',debx012,debx013,debx017,debx014,debx022,debx023, 
          debx024,debx018,debx019,debx020,debx021,debx025 FROM debx_t",    
                  "",
                  " WHERE debxent=? AND debxsite=? AND debx002=? AND debx005=?"
  
      IF NOT cl_null(g_wc2_table2) THEN
         LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
      END IF
  
      LET g_sql = g_sql, " ORDER BY debx_t.debx010,debx_t.debx011" 
                         
      #add-point:單身填充前 name="fetch.before_fill2"
      {<point name="fetch.before_fill2" />}
      #end add-point
 
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料   
      PREPARE adeq108_pb2 FROM g_sql
      DECLARE b_fill_curs2 CURSOR FOR adeq108_pb2
   END IF
 
#(ver:42) mark ---start---
#  OPEN b_fill_curs2 USING g_enterprise,g_debw_d[g_detail_idx].debwsite
#                                 ,g_debw_d[g_detail_idx].debw002
 
#                                 ,g_debw_d[g_detail_idx].debw005
 
 
#(ver:42) mark --- end ---
 
   LET l_ac = 1
   FOREACH b_fill_curs2 USING g_enterprise,g_debw_d[g_detail_idx].debwsite   #(ver:42)
                                     ,g_debw_d[g_detail_idx].debw002   #(ver:42)
 
                                     ,g_debw_d[g_detail_idx].debw005   #(ver:42)
 
 
        INTO g_debw2_d[l_ac].debx010,g_debw2_d[l_ac].debx010_desc,g_debw2_d[l_ac].debx011,g_debw2_d[l_ac].debx011_desc, 
            g_debw2_d[l_ac].debx012,g_debw2_d[l_ac].debx013,g_debw2_d[l_ac].debx017,g_debw2_d[l_ac].debx014, 
            g_debw2_d[l_ac].debx022,g_debw2_d[l_ac].debx023,g_debw2_d[l_ac].debx024,g_debw2_d[l_ac].debx018, 
            g_debw2_d[l_ac].debx019,g_debw2_d[l_ac].debx020,g_debw2_d[l_ac].debx021,g_debw2_d[l_ac].debx025  
              #(ver:42)
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code = SQLCA.SQLCODE 
         LET g_errparam.popup = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      
 
      #add-point:b_fill段資料填充 name="fetch.fill2"
      {<point name="fetch.fill2"/>}
      #end add-point
      
      CALL adeq108_detail_show("'2'")      
 
      CALL adeq108_debx_t_mask()
 
      IF l_ac > g_max_rec THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code = 9035 
         LET g_errparam.popup = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
 
 
   
   #add-point:單身填充後 name="fetch.after_fill"
   {<point name="fetch.after_fill" />}
   #end add-point 
   
