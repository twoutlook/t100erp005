{<section id="adeq108.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION adeq108_b_fill()
   #add-point:b_fill段define-客製 name="b_fill.define_customerization"
   {<point name="b_fill.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準  (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   {<point name="b_fill.define"/>}
   #end add-point
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter, cl_sql_auth_filter()   #(ver:40) add cl_sql_auth_filter()
 
   LET ls_sql_rank = "SELECT  UNIQUE '',debwsite,'',debw002,debw001,debw003,debw004,debw005,'',debw010, 
       debw011,debw012,debw013,debw014,debw015,debw016,debw031,debw032,debw033,debw017,debw018,debw019, 
       debw034,debw020,debw021,debw022,debw023,debw024,debw025,debw026,debw027,debw028,debw029,debw030  , 
       DENSE_RANK() OVER( ORDER BY debw_t.debwsite,debw_t.debw002,debw_t.debw005) AS RANK FROM debw_t", 

 
                     " LEFT JOIN debx_t ON debxent = debwent AND debwsite = debxsite AND debw002 = debx002 AND debw005 = debx005",
 
 
                     "",
                     " WHERE debwent= ? AND 1=1 AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("debw_t"),
                     " ORDER BY debw_t.debwsite,debw_t.debw002,debw_t.debw005"
 
   #add-point:b_fill段rank_sql_after name="b_fill.rank_sql_after"
   {<point name="b_fill.rank_sql_after"/>}
   #end add-point
 
   LET g_sql = "SELECT COUNT(1) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   #add-point:b_fill段rank_sql_after_count name="b_fill.rank_sql_after_count"
   {<point name="b_fill.rank_sql_after_count"/>}
   #end add-point
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT '',debwsite,'',debw002,debw001,debw003,debw004,debw005,'',debw010,debw011,debw012, 
       debw013,debw014,debw015,debw016,debw031,debw032,debw033,debw017,debw018,debw019,debw034,debw020, 
       debw021,debw022,debw023,debw024,debw025,debw026,debw027,debw028,debw029,debw030",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE adeq108_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR adeq108_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_debw_d.clear()
 
   #add-point:陣列清空 name="b_fill.array_clear"
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1   
 
   FOREACH b_fill_curs INTO g_debw_d[l_ac].sel,g_debw_d[l_ac].debwsite,g_debw_d[l_ac].debwsite_desc, 
       g_debw_d[l_ac].debw002,g_debw_d[l_ac].debw001,g_debw_d[l_ac].debw003,g_debw_d[l_ac].debw004,g_debw_d[l_ac].debw005, 
       g_debw_d[l_ac].debw005_desc,g_debw_d[l_ac].debw010,g_debw_d[l_ac].debw011,g_debw_d[l_ac].debw012, 
       g_debw_d[l_ac].debw013,g_debw_d[l_ac].debw014,g_debw_d[l_ac].debw015,g_debw_d[l_ac].debw016,g_debw_d[l_ac].debw031, 
       g_debw_d[l_ac].debw032,g_debw_d[l_ac].debw033,g_debw_d[l_ac].debw017,g_debw_d[l_ac].debw018,g_debw_d[l_ac].debw019, 
       g_debw_d[l_ac].debw034,g_debw_d[l_ac].debw020,g_debw_d[l_ac].debw021,g_debw_d[l_ac].debw022,g_debw_d[l_ac].debw023, 
       g_debw_d[l_ac].debw024,g_debw_d[l_ac].debw025,g_debw_d[l_ac].debw026,g_debw_d[l_ac].debw027,g_debw_d[l_ac].debw028, 
       g_debw_d[l_ac].debw029,g_debw_d[l_ac].debw030
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code = SQLCA.SQLCODE 
         LET g_errparam.popup = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #LET g_debw_d[l_ac].statepic = cl_get_actipic(g_debw_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充 name="b_fill.fill"
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL adeq108_detail_show("'1'")      
 
      CALL adeq108_debw_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "" 
            LET g_errparam.code = 9035 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
   LET g_error_show = 0
   
 
   
   CALL g_debw_d.deleteElement(g_debw_d.getLength())   
 
   #add-point:陣列長度調整 name="b_fill.array_deleteElement"
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
   
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = g_debw_d.getLength()
#  DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE adeq108_pb
 
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL adeq108_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL adeq108_detail_action_trans()
 
   IF g_debw_d.getLength() > 0 THEN
      LET l_ac = 1
      CALL adeq108_fetch()
   END IF
   
      CALL adeq108_filter_show('debwsite','b_debwsite')
   CALL adeq108_filter_show('debw002','b_debw002')
   CALL adeq108_filter_show('debw001','b_debw001')
