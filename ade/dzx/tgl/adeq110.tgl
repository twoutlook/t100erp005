#                                 ,g_debc_d[g_detail_idx].debc010
 
#                                 ,g_debc_d[g_detail_idx].debc011
 
 
#(ver:42) mark --- end ---
 
   LET l_ac = 1
   FOREACH b_fill_curs2 USING g_enterprise,g_debc_d[g_detail_idx].debcsite   #(ver:42)
                                     ,g_debc_d[g_detail_idx].debc002   #(ver:42)
 
                                     ,g_debc_d[g_detail_idx].debc005   #(ver:42)
 
                                     ,g_debc_d[g_detail_idx].debc009   #(ver:42)
 
                                     ,g_debc_d[g_detail_idx].debc010   #(ver:42)
 
                                     ,g_debc_d[g_detail_idx].debc011   #(ver:42)
 
 
        INTO g_debc2_d[l_ac].debd012,g_debc2_d[l_ac].debd012_desc,g_debc2_d[l_ac].debd013,g_debc2_d[l_ac].debd013_desc, 
            g_debc2_d[l_ac].debd014,g_debc2_d[l_ac].debd015,g_debc2_d[l_ac].debd019,g_debc2_d[l_ac].debd024, 
            g_debc2_d[l_ac].debd025,g_debc2_d[l_ac].debd026,g_debc2_d[l_ac].debd020,g_debc2_d[l_ac].debd021, 
            g_debc2_d[l_ac].debd022,g_debc2_d[l_ac].debd023,g_debc2_d[l_ac].debd027   #(ver:42)
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code = SQLCA.SQLCODE 
         LET g_errparam.popup = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      
 
      #add-point:b_fill段資料填充 name="fetch.fill2"
      {<point name="fetch.fill2"/>}
      #end add-point
      
      CALL adeq110_detail_show("'2'")      
 
      CALL adeq110_debd_t_mask()
 
      IF l_ac > g_max_rec THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code = 9035 
         LET g_errparam.popup = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   #為避免影響執行效能，若是按上下筆就不重組SQL
   IF g_action_choice <> "fetch" OR cl_null(g_action_choice) THEN
      LET g_sql = "SELECT  UNIQUE debe013,debe012,'',debe014 FROM debe_t",    
                  "",
                  " WHERE debeent=? AND debesite=? AND debe002=? AND debe005=? AND debe009=? AND debe010=? AND debe011=?"
  
      IF NOT cl_null(g_wc2_table3) THEN
         LET g_sql = g_sql CLIPPED," AND ",g_wc2_table3 CLIPPED
      END IF
  
      LET g_sql = g_sql, " ORDER BY debe_t.debe012" 
                         
      #add-point:單身填充前 name="fetch.before_fill3"
      {<point name="fetch.before_fill3" />}
      #end add-point
 
