      PREPARE adeq110_pb3 FROM g_sql
      DECLARE b_fill_curs3 CURSOR FOR adeq110_pb3
   END IF
 
#(ver:42) mark ---start---
#  OPEN b_fill_curs3 USING g_enterprise,g_debc_d[g_detail_idx].debcsite
#                                 ,g_debc_d[g_detail_idx].debc002
 
#                                 ,g_debc_d[g_detail_idx].debc005
 
#                                 ,g_debc_d[g_detail_idx].debc009
 
#                                 ,g_debc_d[g_detail_idx].debc010
 
#                                 ,g_debc_d[g_detail_idx].debc011
 
 
#(ver:42) mark --- end ---
 
   LET l_ac = 1
   FOREACH b_fill_curs3 USING g_enterprise,g_debc_d[g_detail_idx].debcsite   #(ver:42)
                                     ,g_debc_d[g_detail_idx].debc002   #(ver:42)
 
                                     ,g_debc_d[g_detail_idx].debc005   #(ver:42)
 
                                     ,g_debc_d[g_detail_idx].debc009   #(ver:42)
 
                                     ,g_debc_d[g_detail_idx].debc010   #(ver:42)
 
                                     ,g_debc_d[g_detail_idx].debc011   #(ver:42)
 
 
        INTO g_debc3_d[l_ac].debe013,g_debc3_d[l_ac].debe012,g_debc3_d[l_ac].debe012_desc,g_debc3_d[l_ac].debe014  
              #(ver:42)
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code = SQLCA.SQLCODE 
         LET g_errparam.popup = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      
 
      #add-point:b_fill段資料填充 name="fetch.fill3"
      {<point name="fetch.fill3"/>}
      #end add-point
      
      CALL adeq110_detail_show("'3'")      
 
      CALL adeq110_debe_t_mask()
 
      IF l_ac > g_max_rec THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code = 9035 
         LET g_errparam.popup = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
 
 
   
   #add-point:單身填充後 name="fetch.after_fill"
   {<point name="fetch.after_fill" />}
   #end add-point 
   
   CALL g_debc2_d.deleteElement(g_debc2_d.getLength())   
 
   #單身總筆數顯示
   LET g_detail_cnt2 = g_debc2_d.getLength()
   DISPLAY g_detail_cnt2 TO FORMONLY.cnt
