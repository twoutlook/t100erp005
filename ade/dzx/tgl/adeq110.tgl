   LET li_ac = l_ac 
   
   #為避免影響執行效能，若是按上下筆就不重組SQL
   IF g_action_choice <> "fetch" OR cl_null(g_action_choice) THEN
      LET g_sql = "SELECT  UNIQUE debd012,'',debd013,'',debd014,debd015,debd019,debd024,debd025,debd026, 
          debd020,debd021,debd022,debd023,debd027 FROM debd_t",    
                  "",
                  " WHERE debdent=? AND debdsite=? AND debd002=? AND debd005=? AND debd009=? AND debd010=? AND debd011=?"
  
      IF NOT cl_null(g_wc2_table2) THEN
         LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
      END IF
  
      LET g_sql = g_sql, " ORDER BY debd_t.debd012,debd_t.debd013" 
                         
      #add-point:單身填充前 name="fetch.before_fill2"
      {<point name="fetch.before_fill2" />}
      #end add-point
 
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料   
      PREPARE adeq110_pb2 FROM g_sql
      DECLARE b_fill_curs2 CURSOR FOR adeq110_pb2
   END IF
 
#(ver:42) mark ---start---
#  OPEN b_fill_curs2 USING g_enterprise,g_debc_d[g_detail_idx].debcsite
#                                 ,g_debc_d[g_detail_idx].debc002
 
#                                 ,g_debc_d[g_detail_idx].debc005
 
