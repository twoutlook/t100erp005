{<section id="adeq110.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION adeq110_b_fill()
   #add-point:b_fill段define-客製 name="b_fill.define_customerization"
   {<point name="b_fill.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準  (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   {<point name="b_fill.define"/>}
   #end add-point
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter, cl_sql_auth_filter()   #(ver:40) add cl_sql_auth_filter()
 
   LET ls_sql_rank = "SELECT  UNIQUE '',debcsite,'',debc001,debc002,debc003,debc004,debc005,'',debc006, 
       debc007,debc008,debc009,'',debc010,'',debc011,'',debc012,debc013,debc014,debc015,debc016,debc017, 
       debc018,debc019,debc021,debc029,debc030,debc020,debc022,debc023,debc024,debc031,debc027,debc028, 
       debc026,debc025  ,DENSE_RANK() OVER( ORDER BY debc_t.debcsite,debc_t.debc002,debc_t.debc005,debc_t.debc009, 
       debc_t.debc010,debc_t.debc011) AS RANK FROM debc_t",
 
                     " LEFT JOIN debd_t ON debdent = debcent AND debcsite = debdsite AND debc002 = debd002 AND debc005 = debd005 AND debc009 = debd009 AND debc010 = debd010 AND debc011 = debd011",
 
                     " LEFT JOIN debe_t ON debeent = debcent AND debcsite = debesite AND debc002 = debe002 AND debc005 = debe005 AND debc009 = debe009 AND debc010 = debe010 AND debc011 = debe011",
 
 
                     "",
                     " WHERE debcent= ? AND 1=1 AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("debc_t"),
                     " ORDER BY debc_t.debcsite,debc_t.debc002,debc_t.debc005,debc_t.debc009,debc_t.debc010,debc_t.debc011"
 
   #add-point:b_fill段rank_sql_after name="b_fill.rank_sql_after"
   {<point name="b_fill.rank_sql_after"/>}
   #end add-point
 
   LET g_sql = "SELECT COUNT(1) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   #add-point:b_fill段rank_sql_after_count name="b_fill.rank_sql_after_count"
   {<point name="b_fill.rank_sql_after_count"/>}
   #end add-point
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT '',debcsite,'',debc001,debc002,debc003,debc004,debc005,'',debc006,debc007,debc008, 
       debc009,'',debc010,'',debc011,'',debc012,debc013,debc014,debc015,debc016,debc017,debc018,debc019, 
       debc021,debc029,debc030,debc020,debc022,debc023,debc024,debc031,debc027,debc028,debc026,debc025", 

               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE adeq110_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR adeq110_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_debc_d.clear()
 
   #add-point:陣列清空 name="b_fill.array_clear"
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1   
 
   FOREACH b_fill_curs INTO g_debc_d[l_ac].sel,g_debc_d[l_ac].debcsite,g_debc_d[l_ac].debcsite_desc, 
       g_debc_d[l_ac].debc001,g_debc_d[l_ac].debc002,g_debc_d[l_ac].debc003,g_debc_d[l_ac].debc004,g_debc_d[l_ac].debc005, 
       g_debc_d[l_ac].debc005_desc,g_debc_d[l_ac].debc006,g_debc_d[l_ac].debc007,g_debc_d[l_ac].debc008, 
       g_debc_d[l_ac].debc009,g_debc_d[l_ac].debc009_desc,g_debc_d[l_ac].debc010,g_debc_d[l_ac].debc010_desc, 
       g_debc_d[l_ac].debc011,g_debc_d[l_ac].debc011_desc,g_debc_d[l_ac].debc012,g_debc_d[l_ac].debc013, 
       g_debc_d[l_ac].debc014,g_debc_d[l_ac].debc015,g_debc_d[l_ac].debc016,g_debc_d[l_ac].debc017,g_debc_d[l_ac].debc018, 
       g_debc_d[l_ac].debc019,g_debc_d[l_ac].debc021,g_debc_d[l_ac].debc029,g_debc_d[l_ac].debc030,g_debc_d[l_ac].debc020, 
       g_debc_d[l_ac].debc022,g_debc_d[l_ac].debc023,g_debc_d[l_ac].debc024,g_debc_d[l_ac].debc031,g_debc_d[l_ac].debc027, 
       g_debc_d[l_ac].debc028,g_debc_d[l_ac].debc026,g_debc_d[l_ac].debc025
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code = SQLCA.SQLCODE 
         LET g_errparam.popup = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #LET g_debc_d[l_ac].statepic = cl_get_actipic(g_debc_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充 name="b_fill.fill"
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL adeq110_detail_show("'1'")      
 
      CALL adeq110_debc_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "" 
            LET g_errparam.code = 9035 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
   LET g_error_show = 0
   
 
   
   CALL g_debc_d.deleteElement(g_debc_d.getLength())   
 
   #add-point:陣列長度調整 name="b_fill.array_deleteElement"
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
   
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = g_debc_d.getLength()
#  DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE adeq110_pb
 
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL adeq110_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL adeq110_detail_action_trans()
 
   IF g_debc_d.getLength() > 0 THEN
      LET l_ac = 1
      CALL adeq110_fetch()
   END IF
   
      CALL adeq110_filter_show('debcsite','b_debcsite')
   CALL adeq110_filter_show('debc001','b_debc001')
   CALL adeq110_filter_show('debc002','b_debc002')
   CALL adeq110_filter_show('debc003','b_debc003')
   CALL adeq110_filter_show('debc004','b_debc004')
   CALL adeq110_filter_show('debc005','b_debc005')
   CALL adeq110_filter_show('debc006','b_debc006')
   CALL adeq110_filter_show('debc007','b_debc007')
   CALL adeq110_filter_show('debc008','b_debc008')
