################################################################################
PRIVATE FUNCTION adet402_deaf011_chk(p_deagdocno)
DEFINE p_deagdocno       LIKE deag_t.deagdocno
DEFINE l_cnt             LIKE type_t.num5
DEFINE r_success         LIKE type_t.num5

   LET r_success = TRUE
   LET l_cnt = 0
   
   SELECT COUNT(*) INTO l_cnt
     FROM deaf_t
    WHERE deafent = g_enterprise
      AND deafdocno = p_deagdocno
      AND (deaf011 = 'Y' OR deaf021 = 'Y' OR deaf022 = 'Y')
      
   IF l_cnt > 0 THEN
      LET r_success = FALSE
   END IF
   
   RETURN r_success
   
END FUNCTION

