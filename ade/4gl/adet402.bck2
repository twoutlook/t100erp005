   #   LET l_flag = 'Y'
   #END IF
   #150630-00007#1 Mark By Ken 150702(E)
   
   IF l_deag006 != g_deag_m.deag006 OR ( l_deag006 IS NULL AND g_deag_m.deag006 IS NOT NULL) THEN
      LET l_flag = 'Y'
   END IF 

