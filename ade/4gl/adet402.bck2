                                     nmbaownid,nmbaowndp,nmbacrtdp,nmbacrtid, nmbacrtdt,
                                     nmbastus, nmbacomp)
                         VALUES(l_nmba.nmbaent,  l_nmba.nmbasite, l_nmba.nmbadocno, l_nmba.nmbadocdt,
                                l_nmba.nmba001,  l_nmba.nmba002,  l_nmba.nmba003,  l_nmba.nmba004,   l_nmba.nmba006,  
                                l_nmba.nmbaownid,l_nmba.nmbaowndp,l_nmba.nmbacrtdp,l_nmba.nmbacrtid, l_nmba.nmbacrtdt,
                                l_nmba.nmbastus, l_nmba.nmbacomp)     
