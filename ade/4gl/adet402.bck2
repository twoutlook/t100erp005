#                  LET l_nmba.nmba003   = 'adet402'     #160326-00001#34  by 08172
                  LET l_nmba.nmba003   = '2'    #160326-00001#34  by 08172
                  LET l_nmba.nmba004   = g_deag_m.deagsite
                  LET l_nmba.nmba006   = 'Y'   #150625-00010#1 Add By Ken 150629
