################################################################################
# Descriptions...: 收銀資料單身匯總顯示
# Memo...........:
# Usage..........: CALL adet402_deah3_fill()
# Date & Author..: 2014/01/22 By dongsz
# Modify.........:
################################################################################
PRIVATE FUNCTION adet402_deah3_fill()
DEFINE l_cnt   LIKE type_t.num5 
 
   CALL g_deah3.clear()
   
   LET g_sql = " SELECT deah001,ooial003,deah005,ooail003,SUM(deah008) FROM deah_t ",
               " LEFT JOIN ooial_t ON deahent = ooialent AND deah001 = ooial001 AND ooial002 = '",g_dlang,"' ",
               " LEFT JOIN ooail_t ON deahent = ooailent AND deah005 = ooail001 AND ooail002 = '",g_dlang,"' ", 
               "  WHERE deahent = '",g_enterprise,"' AND deahdocno = '",g_deag_m.deagdocno,"' ",
               "  GROUP BY deah001,ooial003,deah005,ooail003 ",
               "  ORDER BY deah001 "
   PREPARE sel_deah_pre FROM g_sql
   DECLARE sel_deah_cs  CURSOR FOR sel_deah_pre
   LET g_cnt = l_ac
   LET l_ac = 1
   FOREACH sel_deah_cs INTO g_deah3[l_ac].deah001,g_deah3[l_ac].deah001_desc,g_deah3[l_ac].deah005,g_deah3[l_ac].deah005_desc,g_deah3[l_ac].deah008
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "FOREACH:"
         LET g_errparam.popup = TRUE
         CALL cl_err()

         EXIT FOREACH
      END IF
      
      LET l_ac = l_ac + 1
   END FOREACH
   
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CALL g_deah3.deleteElement(g_deah3.getLength())
   
   DISPLAY ARRAY g_deah3 TO s_detail3.* ATTRIBUTES(COUNT=g_rec_b)  
   
      BEFORE DISPLAY 
         EXIT DISPLAY

   END DISPLAY

   FREE sel_deah_pre
END FUNCTION
