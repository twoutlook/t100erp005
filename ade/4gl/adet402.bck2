PRIVATE FUNCTION adet402_chk_pcab()
   DEFINE l_cnt       LIKE type_t.num5
   DEFINE r_success   LIKE type_t.num5
   
   LET r_success = TRUE
   
   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt FROM pcab_t,pcac_t
    WHERE pcabent = pcacent AND pcab001 = pcac001
      AND pcabent = g_enterprise
      AND pcab001 = g_deag_m.deag004
      AND pcac002 = g_deag_m.deagsite
      AND pcacstus = 'Y'
   IF l_cnt = 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'ade-00026'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()
      LET r_success = FALSE      
      RETURN r_success
   END IF
   
   IF NOT cl_null(g_deag_m.deag003) THEN
      LET l_cnt = 0
      SELECT COUNT(*) INTO l_cnt FROM pcab_t,pcac_t
       WHERE pcabent = pcacent AND pcab001 = pcac001
         AND pcabent = g_enterprise
         AND pcab001 = g_deag_m.deag004
         AND pcac002 = g_deag_m.deagsite
         AND pcacstus = 'Y'
         AND (pcac003 = g_deag_m.deag003 OR pcac003 = ' ')
      IF l_cnt = 0 THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'ade-00027'
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()
         LET r_success = FALSE      
         RETURN r_success
      END IF
   END IF
   
   RETURN r_success
END FUNCTION
