                     LET g_errparam.extend = 'INSERT INTO nmba_t'
                     LET g_errparam.popup = TRUE
                     CALL cl_err()
                     LET r_success = FALSE
                     RETURN r_success
                  END IF
      
                  INITIALIZE l_nmbb.* TO NULL
                  LET l_nmbb.nmbbent  = l_nmba.nmbaent      
                  LET l_nmbb.nmbbcomp = l_nmba.nmbacomp
                  LET l_nmbb.nmbbdocno= l_nmba.nmbadocno           
                  LET l_nmbb.nmbborga = g_deag_m.deagsite
                  LET l_nmbb.nmbblegl = g_deag_m.deagsite
                  LET l_nmbb.nmbbseq  = 1
                  LET l_nmbb.nmbb001  = '1'
                  LET l_nmbb.nmbb002  = ''       
                  #LET l_nmbb.nmbb003  = ' '  #mark by geza 20160310
                  #产生短款追回单的时候，nmbb003给NULL
                  LET l_nmbb.nmbb003  = ''  #add by geza 20160310
                  LET l_nmbb.nmbb004  = l_ooef016
                  LET l_nmbb.nmbb005  = 1
                  LET l_nmbb.nmbb006  = l_money * -1
                  LET l_nmbb.nmbb007  = l_money * -1
                  LET l_nmbb.nmbb010  = '' 
                  #150619-00013#1 Add By Ken 150622(S)
                  LET l_pcab002 = ''
                  LET l_pcab005 = ''
                  SELECT pcab002,pcab005 INTO l_pcab002,l_pcab005
                    FROM pcab_t
                   WHERE pcabent = g_enterprise
                     AND pcab001 = g_deag_m.deag004
                     
                  IF NOT cl_null(l_pcab002) THEN
                     LET l_nmbb.nmbb053 = l_pcab002
                  ELSE
                     LET l_nmbb.nmbb053 = g_deag_m.deag004                
                  END IF
                  IF NOT cl_null(l_pcab005) THEN
                     LET l_nmbb.nmbb054 = l_pcab005
                  ELSE                     
                     SELECT ooag003 INTO l_nmbb.nmbb054 FROM ooag_t
                      WHERE ooagent = g_enterprise
                        AND ooag001 = g_deag_m.deag004                       
                  END IF
                  #150619-00013#1 Add By Ken 150622(E)

            
                  INSERT INTO nmbb_t(nmbbent,  nmbbcomp, nmbbdocno, nmbborga, nmbblegl,
                                     nmbbseq,  nmbb001,  nmbb002,   nmbb003,  nmbb004, 
                                     nmbb005,  nmbb006,  nmbb007,   nmbb010,  nmbb053, 
