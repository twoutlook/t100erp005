                         VALUES(l_nmbb.nmbbent,  l_nmbb.nmbbcomp, l_nmbb.nmbbdocno, l_nmbb.nmbborga, l_nmbb.nmbblegl,
                                l_nmbb.nmbbseq,  l_nmbb.nmbb001,  l_nmbb.nmbb002,   l_nmbb.nmbb003,  l_nmbb.nmbb004, 
