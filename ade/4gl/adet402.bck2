                                 " LEFT JOIN ooefl_t t1 ON t1.ooeflent='"||g_enterprise||"' AND t1.ooefl001=t0.deagsite AND t1.ooefl002='"||g_dlang||"' ",
 
                  " WHERE t0.deagent = '" ||g_enterprise|| "' AND ",l_wc," AND ",l_wc2, cl_sql_add_filter("deag_t")
   ELSE
      #單身無輸入搜尋條件   
