################################################################################
# Descriptions...: 批次產生收銀繳款單
# Memo...........:
# Usage..........: CALL adet402_deag_ins(p_wc,p_deagdocdt)
#                  RETURNING r_success
# Input parameter: p_wc           營運組織
#                : p_deagdocdt    營業日期
# Return code....: r_success      TURE/FALSE
# Date & Author..: 2015/07/04 By Ken
# Modify.........:
################################################################################
PRIVATE FUNCTION adet402_deag_ins(p_wc,p_deagdocdt)
   DEFINE p_wc             STRING
   DEFINE p_deagdocdt      LIKE deag_t.deagdocdt
   DEFINE r_success        LIKE type_t.num5
   DEFINE l_deag        RECORD
            deagent        LIKE deag_t.deagent,
            deagsite       LIKE deag_t.deagsite,
            deagunit       LIKE deag_t.deagunit,
            deagdocno      LIKE deag_t.deagdocno,
            deagdocdt      LIKE deag_t.deagdocdt,
            deag001        LIKE deag_t.deag001,
            deag002        LIKE deag_t.deag002,
            deag003        LIKE deag_t.deag003,
            deag004        LIKE deag_t.deag004,
            deagownid      LIKE deag_t.deagownid,
            deagowndp      LIKE deag_t.deagowndp,
            deagcrtdp      LIKE deag_t.deagcrtdp,
            deagcrtid      LIKE deag_t.deagcrtid,
            deagcrtdt      LIKE deag_t.deagcrtdt,
            deagstus       LIKE deag_t.deagstus
                       END RECORD
   DEFINE l_dt             DATETIME YEAR TO SECOND                     
   DEFINE l_sql            STRING
   DEFINE l_wc             STRING
   DEFINE l_where          STRING
   DEFINE l_rtjfsite       LIKE rtjf_t.rtjfsite
   DEFINE l_rtjf025        LIKE rtjf_t.rtjf025
   DEFINE l_rtjf030        LIKE rtjf_t.rtjf030
   DEFINE l_rtjf028        LIKE rtjf_t.rtjf028
   DEFINE l_rtjf029        LIKE rtjf_t.rtjf029
   DEFINE l_rtjfsite_t     LIKE rtjf_t.rtjfsite
   DEFINE l_cnt            LIKE type_t.num5
   DEFINE l_success        LIKE type_t.num5
   DEFINE l_date           LIKE type_t.dat
   
   LET r_success = TRUE
   
   LET l_sql = ''
   LET l_cnt = 0   
   LET l_dt = cl_get_current()
   LET l_date = p_deagdocdt + 1
   
   LET l_sql = " SELECT COUNT(*) ",
               "   FROM deag_t ",
               "  WHERE deagent = ",g_enterprise,
               "    AND deagsite = ? ",
               "    AND deagdocdt = ? ",
               "    AND deag004 = ? ",
               "    AND deagstus != 'X' " 
               
   IF g_deag002_flag = 'Y' THEN
      LET l_sql = l_sql CLIPPED, " AND deag002 = ? "
   END IF   
   IF g_deag003_flag = 'Y' THEN
      LET l_sql = l_sql CLIPPED, " AND deag003 = ? "
   END IF  

   PREPARE adet402_sel_deag_pre FROM l_sql 
  
   LET l_wc = cl_replace_str(p_wc,'deagsite','rtjfsite') 
   CALL s_aooi500_sql_where(g_prog,'rtjfsite') RETURNING l_where
   
   LET l_sql = " SELECT UNIQUE rtjfsite,rtjf025,rtjf030 "
   
   IF g_deag002_flag = 'Y' THEN
      LET l_sql = l_sql CLIPPED, ",rtjf028"
   ELSE
      LET l_sql = l_sql CLIPPED, ",''"
   END IF
   
   IF g_deag003_flag = 'Y' THEN
      LET l_sql = l_sql CLIPPED, ",rtjf029"
   ELSE
      LET l_sql = l_sql CLIPPED, ",''"
   END IF
   
   LET l_sql = l_sql CLIPPED,
               "   FROM rtjf_t ",
               "  WHERE rtjfent = ",g_enterprise,
               "    AND ",l_wc,
               "    AND ",l_where,
               "    AND (rtjf025 = ?  ",
               "         OR (EXISTS (SELECT 1 FROM rtja_t ",
               "                      WHERE rtjaent = rtjfent ",
               "                        AND rtjadocno = rtjfdocno ",
               "                        AND rtjacrtdt <  ? ) ",
               "            AND rtjf033 IS NULL)) ", 
               "  ORDER BY rtjfsite"   
               
   PREPARE adet402_ins_deag_pre FROM l_sql
   DECLARE adet402_ins_deag_cs CURSOR FOR adet402_ins_deag_pre   
   
   FOREACH adet402_ins_deag_cs USING p_deagdocdt,l_date INTO l_rtjfsite,l_rtjf025,l_rtjf030,l_rtjf028,l_rtjf029               
      #組織的單據設定有問題,不重複進入foreach中
      IF NOT cl_null(l_rtjfsite_t) AND l_rtjfsite = l_rtjfsite_t THEN
         CONTINUE FOREACH
      END IF
      
      LET l_cnt = 0
      CASE 
         WHEN g_deag002_flag = 'Y' AND g_deag003_flag = 'Y'
            IF cl_null(l_rtjfsite) OR cl_null(l_rtjf025) OR cl_null(l_rtjf030) OR
               cl_null(l_rtjf028)  OR cl_null(l_rtjf029) THEN
               #營運組織:%1 ,收銀日期:%2 ,班別:%3 ,收銀機號:%4 ,收銀員:%5 資訊不足,不進行批次產生
               INITIALIZE g_errparam TO NULL
               LET g_errparam.replace[1] = l_rtjfsite
               LET g_errparam.replace[2] = l_rtjf025
               LET g_errparam.replace[3] = l_rtjf028
               LET g_errparam.replace[4] = l_rtjf029
