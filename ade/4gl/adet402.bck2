#計算差異金額
PRIVATE FUNCTION adet402_compute_deaf008()
   DEFINE l_deaf006    LIKE deaf_t.deaf006
   DEFINE l_deaf007    LIKE deaf_t.deaf007
   DEFINE l_deaf015    LIKE deaf_t.deaf015
   DEFINE l_deaf020    LIKE deaf_t.deaf020     #150629-00035#4 By benson 2015/07/10
   
   IF l_ac = 0 THEN RETURN END IF
   
   LET l_deaf006 = g_deah5_d[l_ac].deaf006
   LET l_deaf007 = g_deah5_d[l_ac].deaf007
   LET l_deaf015 = g_deah5_d[l_ac].deaf015 
   LET l_deaf020 = g_deah5_d[l_ac].deaf020     #150629-00035#4 By benson 2015/07/10
   
   IF cl_null(l_deaf006) THEN LET l_deaf006 = 0 END IF
   IF cl_null(l_deaf007) THEN LET l_deaf007 = 0 END IF
   IF cl_null(l_deaf015) THEN LET l_deaf015 = 0 END IF
   IF cl_null(l_deaf020) THEN LET l_deaf020 = 0 END IF   #150629-00035#4 By benson 2015/07/10
   
   #LET g_deah5_d[l_ac].deaf008 = l_deaf006 - l_deaf007 - l_deaf015              #150629-00035#4 By benson 2015/07/10
   LET g_deah5_d[l_ac].deaf008 = l_deaf006 - l_deaf007 - l_deaf015 - l_deaf020   #150629-00035#4 By benson 2015/07/10
   IF g_deah5_d[l_ac].deaf008 = 0 THEN 
      LET g_deah5_d[l_ac].deaf009 = ''
   ELSE
      IF g_deah5_d[l_ac].deaf008 * g_deah5_d_o.deaf008 < 0 THEN
         LET g_deah5_d[l_ac].deaf009 = ''
      END IF
   END IF
   #150603-00050#2 Add-S By Ken 150605
   IF (g_deah5_d[l_ac].deaf008 < 0) AND (g_deah5_d[l_ac].deaf012 = '10') THEN
      IF s_math_abs(g_deah5_d[l_ac].deaf008) <= g_deaf008 THEN
         LET g_deah5_d[l_ac].deaf009 = '7'
      END IF
   END IF
   #150603-00050#2 Add-E By Ken 150605   
   
   LET g_deah5_d_o.deaf008 = g_deah5_d[l_ac].deaf008
   
END FUNCTION
