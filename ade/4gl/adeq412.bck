{<section id="adeq412.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION adeq412_b_fill()
   #add-point:b_fill段define-客製 name="b_fill.define_customerization"
   
   #end add-point
   DEFINE ls_wc           STRING
   DEFINE l_pid           LIKE type_t.chr50
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準  (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   DEFINE l_where           STRING
   DEFINE l_str           STRING
   #end add-point
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   
   #end add-point
 
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
 
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
 
   CALL g_deaf_d.clear()
 
   #add-point:陣列清空 name="b_fill.array_clear"
   CALL s_aooi500_sql_where(g_prog,'deafsite') RETURNING l_where
   LET g_wc2 = g_wc2," AND ",l_where
   LET g_wc2 = g_wc2.trim() 
   IF g_sel1 = 'Y' THEN 
      LET l_str = " AND deagstus ='N'"
   END IF 
   IF g_sel2 = 'Y' THEN 
      IF cl_null(l_str) THEN 
         LET l_str = "AND  deaf008 <> 0"
      ELSE
         LET l_str = l_str," AND ","deaf008 <> 0"
      END IF 
   END IF 
   #LET g_wc = " 1=1"      #160126-00002#1 160126 By pomelo mark
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter,l_str
   
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1
 
   # b_fill段sql組成及FOREACH撰寫
   #應用 qs04 樣板自動產生(Version:9)
   #+ b_fill段資料取得(包含sql組成及FOREACH段撰寫)
   LET ls_sql_rank = "SELECT  UNIQUE deafdocno,'',deafdocdt,deafsite,'',deaf001,deaf002,'',deaf003,'', 
       deaf004,'',deaf005,'',deaf006,deaf007,deaf008,deaf009,deaf010,deaf011,''  ,DENSE_RANK() OVER( ORDER BY deaf_t.deafdocno, 
       deaf_t.deaf005) AS RANK FROM deaf_t",
 
 
                     "",
                     " WHERE deafent= ? AND 1=1 AND ", ls_wc
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("deaf_t"),
                     " ORDER BY deaf_t.deafdocno,deaf_t.deaf005"
 
   #add-point:b_fill段rank_sql_after name="b_fill.rank_sql_after"
   #150826-00013#1 效能調整 20150910 add by beckxie---S
   LET ls_sql_rank = "SELECT  UNIQUE deafdocno,deagstus,deafdocdt,deafsite,",
                     "              (SELECT ooefl003",
                     "                 FROM ooefl_t",
                     "                WHERE ooefl001=deafsite AND  ooeflent=deafent ",
                     "                  AND ooefl002='"||g_lang||"') deafsite_desc,", 
                     "              deaf001,deaf002,",
                     "              (SELECT oogd002",
                     "                 FROM oogd_t",
                     "                WHERE oogdsite=deafsite AND  oogdent=deafent ",
                     "                  AND oogd001 = deaf002) deaf002_desc,",
                     "              deaf003,",
                     "              (SELECT pcaal003",
                     "                 FROM pcaal_t",
                     "                WHERE pcaalsite=deafsite AND  pcaalent=deafent",
                     "                  AND pcaal001 = deaf003 AND pcaal002='"||g_lang||"') deaf003_desc,", 
                     "              deaf004,",
                     "              (SELECT pcab003",
                     "                 FROM pcab_t",
                     "                WHERE pcabent=deafent AND pcab001 = deaf004) deaf004_desc,",
                     "              deaf005,",
                     "              (SELECT ooial003",
                     "                 FROM ooial_t",
                     "                WHERE ooialent=deafent AND ooial001 = deaf005",
                     "                  AND ooial002='"||g_lang||"') deaf005_desc,",
                     "              deaf006,deaf007,deaf008,deaf009,deaf010,deaf011,'',",
                     "              DENSE_RANK() OVER( ORDER BY deaf_t.deafdocno,deaf_t.deaf005) AS RANK ",
                     "FROM deag_t,deaf_t", 
                     " WHERE deagent = deafent  AND deafdocno = deagdocno ",
                     " AND deafent= ? AND 1=1 AND ", ls_wc
                     
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("deaf_t"),
                      " ORDER BY deaf_t.deafdocno,deaf_t.deaf005"
   #150826-00013#1 效能調整 20150910 add by beckxie---E
   #end add-point
 
   LET g_sql = "SELECT COUNT(1) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql  #總筆數
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   #add-point:b_fill段rank_sql_after_count name="b_fill.rank_sql_after_count"
   
   #end add-point
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT deafdocno,'',deafdocdt,deafsite,'',deaf001,deaf002,'',deaf003,'',deaf004,'',deaf005, 
       '',deaf006,deaf007,deaf008,deaf009,deaf010,deaf011,''",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   #150826-00013#1 效能調整 20150910 add by beckxie---S
   #LET g_sql = "SELECT  UNIQUE deafdocno,deagstus,deafdocdt,deafsite,ooefl003,deaf001,deaf002,oogd002,deaf003,pcaal003,deaf004,pcab003,deaf005, 
   #    ooial003,deaf006,deaf007,deaf008,deaf009,deaf010,deaf011,'' FROM deag_t,deaf_t",
   #         
   #         " LEFT JOIN ooefl_t   ON ooefl001=deafsite AND  ooeflent=deafent AND ooefl002='"||g_lang||"' ", 
   #         " LEFT JOIN oogd_t   ON oogdsite=deafsite AND  oogdent=deafent AND oogd001 = deaf002 ", 
   #         " LEFT JOIN pcaal_t   ON pcaalsite=deafsite AND  pcaalent=deafent AND pcaal001 = deaf003 AND pcaal002='"||g_lang||"' ", 
   #         " LEFT JOIN pcab_t   ON  pcabent=deafent AND pcab001 = deaf004 ", 
   #         " LEFT JOIN ooial_t   ON ooialent=deafent AND ooial001 = deaf005 AND ooial002='"||g_lang||"' ", 
   #         " WHERE deagent = deafent  AND deafdocno = deagdocno ",
   #            
   #            "",
   #            " AND deafent= ? AND 1=1 AND ", ls_wc
   #
   #LET g_sql = g_sql, cl_sql_add_filter("deaf_t"),
   #                   " ORDER BY deaf_t.deafdocno,deaf_t.deaf005"
   #150826-00013#1 效能調整 20150910 add by beckxie---E
   #150826-00013#1 效能調整 20150910 add by beckxie---S
   LET g_sql = "SELECT deafdocno,    deagstus,   deafdocdt,    deafsite,deafsite_desc,",
               "         deaf001,     deaf002,deaf002_desc,     deaf003,deaf003_desc,",
               "         deaf004,deaf004_desc,     deaf005,deaf005_desc,deaf006,",
               "         deaf007,     deaf008,     deaf009,     deaf010,deaf011,''",
               " FROM (",ls_sql_rank,")",
               " WHERE RANK >= ",g_pagestart,
               " AND RANK < ",g_pagestart + g_num_in_page
   #150826-00013#1 效能調整 20150910 add by beckxie---E
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE adeq412_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR adeq412_pb
 
   OPEN b_fill_curs USING g_enterprise
 
   FOREACH b_fill_curs INTO g_deaf_d[l_ac].deafdocno,g_deaf_d[l_ac].deagstus,g_deaf_d[l_ac].deafdocdt, 
       g_deaf_d[l_ac].deafsite,g_deaf_d[l_ac].deafsite_desc,g_deaf_d[l_ac].deaf001,g_deaf_d[l_ac].deaf002, 
       g_deaf_d[l_ac].deaf002_desc,g_deaf_d[l_ac].deaf003,g_deaf_d[l_ac].deaf003_desc,g_deaf_d[l_ac].deaf004, 
       g_deaf_d[l_ac].deaf004_desc,g_deaf_d[l_ac].deaf005,g_deaf_d[l_ac].deaf005_desc,g_deaf_d[l_ac].deaf006, 
       g_deaf_d[l_ac].deaf007,g_deaf_d[l_ac].deaf008,g_deaf_d[l_ac].deaf009,g_deaf_d[l_ac].deaf010,g_deaf_d[l_ac].deaf011, 
       g_deaf_d[l_ac].sel
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
 
      
 
      #add-point:b_fill段資料填充 name="b_fill.fill"
      
      #end add-point
 
      CALL adeq412_detail_show("'1'")
 
      CALL adeq412_deaf_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
 
   END FOREACH
 
 
 
 
 
   #應用 qs05 樣板自動產生(Version:3)
   #+ b_fill段其他table資料取得(包含sql組成及資料填充)
 
 
 
 
 
 
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   
   #end add-point
 
   CALL g_deaf_d.deleteElement(g_deaf_d.getLength())
 
   #add-point:陣列長度調整 name="b_fill.array_deleteElement"
   
   #end add-point
 
   LET g_error_show = 0
 
   LET g_detail_cnt = g_deaf_d.getLength()
   LET l_ac = g_cnt
   LET g_cnt = 0
 
   #應用 qs06 樣板自動產生(Version:3)
   #+ b_fill段CURSOR釋放
   CLOSE b_fill_curs
   FREE adeq412_pb
 
 
 
 
 
 
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL adeq412_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL adeq412_detail_action_trans()
 
   LET l_ac = 1
   IF g_deaf_d.getLength() > 0 THEN
      CALL adeq412_b_fill2()
   END IF
 
      CALL adeq412_filter_show('deagstus','b_deagstus')
   CALL adeq412_filter_show('deafdocdt','b_deafdocdt')
   CALL adeq412_filter_show('deafsite','b_deafsite')
   CALL adeq412_filter_show('deaf001','b_deaf001')
   CALL adeq412_filter_show('deaf002','b_deaf002')
   CALL adeq412_filter_show('deaf003','b_deaf003')
   CALL adeq412_filter_show('deaf004','b_deaf004')
   CALL adeq412_filter_show('deaf005','b_deaf005')
   CALL adeq412_filter_show('deaf006','b_deaf006')
   CALL adeq412_filter_show('deaf007','b_deaf007')
   CALL adeq412_filter_show('deaf008','b_deaf008')
   CALL adeq412_filter_show('deaf009','b_deaf009')
   CALL adeq412_filter_show('deaf010','b_deaf010')
   CALL adeq412_filter_show('deaf011','b_deaf011')
 
 
END FUNCTION
 
