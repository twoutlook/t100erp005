#+ filter過濾功能
PRIVATE FUNCTION adeq412_filter()
   #add-point:filter段define-客製 name="filter.define_customerization"
   
   #end add-point
   DEFINE  ls_result   STRING
   #add-point:filter段define-標準  (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="filter.define"
   
   #end add-point
 
   #add-point:FUNCTION前置處理 name="filter.before_function"
   
   #end add-point
 
   LET INT_FLAG = 0
 
   LET g_qryparam.state = 'c'
   LET g_detail_idx  = 1
   LET g_detail_idx2 = 1
 
   LET g_wc_filter_t = g_wc_filter
   LET g_wc_t = g_wc
 
   CALL gfrm_curr.setFieldHidden("formonly.sel", TRUE)
   CALL gfrm_curr.setFieldHidden("formonly.b_statepic", TRUE)
 
   
 
   LET g_wc = cl_replace_str(g_wc, g_wc_filter, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   #應用 qs08 樣板自動產生(Version:5)
   #+ filter段DIALOG段的組成
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON deagstus,deafdocdt,deafsite,deaf001,deaf002,deaf003,deaf004,deaf005,deaf006, 
          deaf007,deaf008,deaf009,deaf010,deaf011
                          FROM s_detail1[1].b_deagstus,s_detail1[1].b_deafdocdt,s_detail1[1].b_deafsite, 
                              s_detail1[1].b_deaf001,s_detail1[1].b_deaf002,s_detail1[1].b_deaf003,s_detail1[1].b_deaf004, 
                              s_detail1[1].b_deaf005,s_detail1[1].b_deaf006,s_detail1[1].b_deaf007,s_detail1[1].b_deaf008, 
                              s_detail1[1].b_deaf009,s_detail1[1].b_deaf010,s_detail1[1].b_deaf011
 
         BEFORE CONSTRUCT
                     DISPLAY adeq412_filter_parser('deagstus') TO s_detail1[1].b_deagstus
            DISPLAY adeq412_filter_parser('deafdocdt') TO s_detail1[1].b_deafdocdt
            DISPLAY adeq412_filter_parser('deafsite') TO s_detail1[1].b_deafsite
            DISPLAY adeq412_filter_parser('deaf001') TO s_detail1[1].b_deaf001
            DISPLAY adeq412_filter_parser('deaf002') TO s_detail1[1].b_deaf002
            DISPLAY adeq412_filter_parser('deaf003') TO s_detail1[1].b_deaf003
            DISPLAY adeq412_filter_parser('deaf004') TO s_detail1[1].b_deaf004
            DISPLAY adeq412_filter_parser('deaf005') TO s_detail1[1].b_deaf005
            DISPLAY adeq412_filter_parser('deaf006') TO s_detail1[1].b_deaf006
            DISPLAY adeq412_filter_parser('deaf007') TO s_detail1[1].b_deaf007
            DISPLAY adeq412_filter_parser('deaf008') TO s_detail1[1].b_deaf008
            DISPLAY adeq412_filter_parser('deaf009') TO s_detail1[1].b_deaf009
            DISPLAY adeq412_filter_parser('deaf010') TO s_detail1[1].b_deaf010
            DISPLAY adeq412_filter_parser('deaf011') TO s_detail1[1].b_deaf011
 
 
         #單身公用欄位開窗相關處理
         
 
         #單身一般欄位開窗相關處理
                  #----<<b_deafdocno>>----
         #----<<b_deagstus>>----
         #Ctrlp:construct.c.filter.page1.b_deagstus
#         #應用 a03 樣板自動產生(Version:3)
         ON ACTION controlp INFIELD b_deagstus
            #add-point:ON ACTION controlp INFIELD b_deagstus name="construct.c.filter.page1.b_deagstus"
 
