                  #end add-point
                  "  LEFT JOIN deau_t ON deauent = deaxent AND deaxdocno = deaudocno", "  ", 
                  #add-point:browser_fill段sql(deau_t1) name="browser_fill.join.deau_t1"
                  
                  #end add-point
 
 
 
