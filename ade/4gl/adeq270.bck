{<section id="adeq270.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION adeq270_b_fill()
   #add-point:b_fill段define-客製 name="b_fill.define_customerization"
   
   #end add-point
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準  (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   DEFINE l_where         STRING
   #end add-point
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   CALL s_aooi500_sql_where(g_prog,'dedtsite') RETURNING l_where
   IF cl_null(g_wc) THEN
      LET g_wc = l_where
   ELSE
      LET g_wc = g_wc CLIPPED," AND ",l_where
   END IF
   #end add-point
 
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   LET ls_sql_rank = "SELECT  UNIQUE dedt018,dedt019,dedt020,dedtsite,'',dedt005,'',dedt007,'',dedt014, 
       '',dedt008,'',dedt010,dedt011,dedt012,dedt009,dedt013  ,DENSE_RANK() OVER( ORDER BY dedt_t.dedtsite, 
       dedt_t.dedt005,dedt_t.dedt007,dedt_t.dedt008,dedt_t.dedt014,dedt_t.dedt018,dedt_t.dedt019,dedt_t.dedt020) AS RANK FROM dedt_t", 
 
 
 
                     "",
                     " WHERE dedtent= ? AND 1=1 AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("dedt_t"),
                     " ORDER BY dedt_t.dedtsite,dedt_t.dedt005,dedt_t.dedt007,dedt_t.dedt008,dedt_t.dedt014,dedt_t.dedt018,dedt_t.dedt019,dedt_t.dedt020"
 
   #add-point:b_fill段rank_sql_after name="b_fill.rank_sql_after"
   #150826-00013#2 20150911 s983961--add(s) 效能調整
   LET ls_sql_rank = "SELECT  dedt018,dedt019,dedt020,dedtsite, ",
                     "        (SELECT ooefl003",
                     "           FROM ooefl_t",
                     "          WHERE ooeflent = dedtent AND ooefl001 = dedtsite AND ooefl002 = '",g_dlang,"') dedtsite_desc, ",
                     "        dedt005, ",
                     "        (SELECT mhael023 FROM mhael_t ",   
                     "          WHERE mhaelent = dedtent AND mhaelsite=dedtsite AND mhael001=dedt005 ",
                     "            AND mhael022='"||g_dlang||"') dedt005_desc, ",
                     "        dedt007, ",
                     "        (SELECT rtaxl003",
                     "           FROM rtaxl_t",
                     "          WHERE rtaxlent = dedtent AND rtaxl001 = dedt007 AND rtaxl002 = '",g_dlang,"') dedt007_desc, ",
                     "        dedt014, ",
                     "        (SELECT inayl003",
                     "           FROM inayl_t",
                     "          WHERE inaylent = dedtent AND inayl001 = dedt014 AND inayl002 = '",g_dlang,"') dedt014_desc, ",
                     "        dedt008, ",
                     "        (SELECT oodbl004",
                     "           FROM oodbl_t",
                     "          WHERE oodblent = dedtent AND oodbl002 = dedt008 AND oodbl001 = ooef019 AND oodbl003 = '",g_dlang,"') dedt008_desc, ",
                     "        dedt010,dedt011, ",
                     "        dedt012,dedt009,dedt013,",
                     " DENSE_RANK() OVER( ORDER BY dedt_t.dedtsite,dedt_t.dedt005,dedt_t.dedt007, ",
                     "                             dedt_t.dedt008,dedt_t.dedt014,dedt_t.dedt018,  ",
                     "                             dedt_t.dedt019,dedt_t.dedt020) AS RANK         ",
                     "  FROM dedt_t",
                     "  LEFT JOIN ooef_t  ON ooef_t.ooefent = dedt_t.dedtent AND ooef_t.ooef001 = dedt_t.dedtsite ",
                     " WHERE dedtent= ? AND 1=1 AND ", ls_wc
   
   #LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("dedt_t"),
   #            " ORDER BY dedt_t.dedtsite,dedt_t.dedt005,dedt_t.dedt007,dedt_t.dedt008,dedt_t.dedt014,dedt_t.dedt018,dedt_t.dedt019,dedt_t.dedt020"   
   #150826-00013#2 20150911 s983961--add(e) 效能調整            
   #end add-point
 
   LET g_sql = "SELECT COUNT(1) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   #add-point:b_fill段rank_sql_after_count name="b_fill.rank_sql_after_count"
   
   #end add-point
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT dedt018,dedt019,dedt020,dedtsite,'',dedt005,'',dedt007,'',dedt014,'',dedt008, 
       '',dedt010,dedt011,dedt012,dedt009,dedt013",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   #150826-00013#2 20150910 s983961--mark and mod(s) 效能調整
   #LET g_sql = "SELECT dedt018,dedt019,dedt020,dedtsite,ooefl_t.ooefl003,dedt005,dedt007,rtaxl_t.rtaxl003,
   #    dedt014,inayl_t.inayl003,dedt008,oodbl_t.oodbl004, 
   #    dedt010,dedt011,dedt012,dedt009,dedt013",
   #            "  FROM dedt_t",
   #            "  LEFT JOIN ooefl_t ON ooefl_t.ooeflent=dedt_t.dedtent AND ooefl_t.ooefl001=dedt_t.dedtsite AND ooefl_t.ooefl002 = '",g_dlang,"' ",               
   #            "  LEFT JOIN rtaxl_t ON rtaxl_t.rtaxlent=dedt_t.dedtent AND rtaxl_t.rtaxl001 =dedt_t.dedt007 AND rtaxl_t.rtaxl002 = '",g_dlang,"' ",   
   #            "  LEFT JOIN inayl_t ON inayl_t.inaylent = dedt_t.dedtent AND inayl_t.inayl001=dedt_t.dedt014 AND inayl_t.inayl002 = '",g_dlang,"' ", 
   #            "  LEFT JOIN ooef_t  ON ooef_t.ooefent = dedt_t.dedtent AND ooef_t.ooef001 = dedt_t.dedtsite ",
   #            "  LEFT JOIN oodbl_t ON oodbl_t.oodblent=dedt_t.dedtent  AND oodbl_t.oodbl002 =dedt_t.dedt008  AND oodbl_t.oodbl001=ooef_t.ooef019 AND oodbl_t.oodbl003 = '",g_dlang,"' ",  
   #            " WHERE dedtent = ? ",
   #    "   AND ", ls_wc,cl_sql_add_filter("dedt_t")
   #LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("dedt_t"),
   #  " ORDER BY dedt_t.dedtsite,dedt_t.dedt005,dedt_t.dedt007,dedt_t.dedt008,dedt_t.dedt014,dedt_t.dedt018,dedt_t.dedt019,dedt_t.dedt020"
   
   LET g_sql = "SELECT dedt018,dedt019,dedt020,dedtsite,dedtsite_desc,dedt005,dedt005_desc,dedt007,dedt007_desc,
                       dedt014,dedt014_desc,dedt008,dedt008_desc,dedt010,dedt011,dedt012,dedt009,dedt013",
               "  FROM (",ls_sql_rank,")",
              "  WHERE RANK >= ",g_pagestart,
                "  AND RANK < ",g_pagestart + g_num_in_page
   #150826-00013#2 20150910 s983961--mark and mod(e) 效能調整
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE adeq270_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR adeq270_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_dedt_d.clear()
 
   #add-point:陣列清空 name="b_fill.array_clear"
   
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
