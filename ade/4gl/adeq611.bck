   #            "              t2.oocql004, deba014,     t3.pmaal004, deba016,     t4.rtaxl003, ",
   #            "              deba018,     t5.oodbl004, deba020,     t6.oocal003, deba021, ",
   #            "              deba022,     deba023,     deba024,     deba025,     deba026, ",
