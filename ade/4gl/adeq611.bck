   #150826-00013#1 效能調整 20150910 add by beckxie---S
   LET g_sql = "SELECT      'N',    debasite,debasite_desc,deba002     ,deba003,",
               "        deba004,     deba010,      deba009,deba011     ,deba012,",
               "        deba013,deba013_desc,      deba014,deba014_desc,deba016,",
