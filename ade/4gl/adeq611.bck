                     "              deba010,     deba009,     deba011,     deba012,     deba013, ",
                     "              (SELECT oocql004",
                     "                 FROM oocql_t",
                     "                WHERE oocqlent = debaent AND oocql001 = '2002'",
                     "                  AND oocql002 = deba013 AND oocql003 = '",g_dlang,"') deba013_desc,",
                     "              deba014,",
                     "              (SELECT pmaal004",
                     "                 FROM pmaal_t",
                     "                WHERE pmaalent = debaent AND pmaal001 = deba014",
                     "                  AND pmaal002 = '",g_dlang,"') deba014_desc,",
                     "              deba016,",
                     "              (SELECT rtaxl003",
                     "                 FROM rtaxl_t",
                     "                WHERE rtaxlent = debaent AND rtaxl001 = deba016",
                     "                  AND rtaxl002= '",g_dlang,"') deba016_desc,",
                     "              deba018,t5.oodbl004 deba018_desc, deba020,",
                     "              (SELECT oocal003",
                     "                 FROM oocal_t",
                     "                WHERE oocalent = debaent AND oocal001 = deba020 ",
                     "                  AND oocal002 = '",g_dlang,"') deba020_desc,",
                     "              deba021, ",
                     "              deba022,     deba023,     deba024,     deba025,     deba026, ",
                     "              deba019,     deba027,     deba028,     deba029,     deba044, ",
                     "              deba030,     deba031,     deba032,     deba005,     deba017 ",
                     " ,DENSE_RANK() OVER( ORDER BY deba_t.debasite,deba_t.deba002,deba_t.deba005,",
                     " deba_t.deba009,deba_t.deba017,deba_t.deba018,deba_t.deba020,deba_t.deba043) AS RANK",
                     "  FROM deba_t ", 
                     "       LEFT JOIN (SELECT oodblent,ooef001,oodbl002,oodbl003,oodbl004 ",
                     "                    FROM ooef_t,oodbl_t WHERE ooefent = oodblent AND ooef019 = oodbl001) t5 ",
                     "              ON t5.oodblent = debaent AND t5.ooef001 = debasite ",
                     "             AND t5.oodbl002 = deba018 AND t5.oodbl003 = '",g_dlang,"' ",
                     " WHERE debaent = ? ",
                     "   AND ", ls_wc,cl_sql_add_filter("deba_t")
    
