{<section id="adeq176.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION adeq176_b_fill()
   #add-point:b_fill段define-客製 name="b_fill.define_customerization"
   
   #end add-point
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準  (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   DEFINE l_where         STRING
   #end add-point
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   CALL s_aooi500_sql_where(g_prog,'dedlsite') RETURNING l_where
   IF cl_null(g_wc) THEN
      LET g_wc = l_where
   ELSE
      LET g_wc = g_wc CLIPPED," AND ",l_where
   END IF
   #end add-point
 
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   LET ls_sql_rank = "SELECT  UNIQUE dedlsite,'',dedl001,dedl024,dedl025,dedl005,'',dedl006,dedl007, 
       dedl008,dedl009,dedl010,dedl011,dedl012,dedl013,dedl022,dedl023,dedl014,dedl015,dedl016,dedl017, 
       dedl020,dedl021,dedl018,dedl019  ,DENSE_RANK() OVER( ORDER BY dedl_t.dedlsite,dedl_t.dedl005, 
       dedl_t.dedl024,dedl_t.dedl025) AS RANK FROM dedl_t",
 
                     " LEFT JOIN dedm_t ON dedment = dedlent AND dedlsite = dedmsite AND dedl005 = dedm005 AND dedl024 = dedm021 AND dedl025 = dedm022",
 
                     " LEFT JOIN dedn_t ON dednent = dedlent AND dedlsite = dednsite AND dedl005 = dedn005 AND dedl024 = dedn009 AND dedl025 = dedn010",
 
 
                     "",
                     " WHERE dedlent= ? AND 1=1 AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("dedl_t"),
                     " ORDER BY dedl_t.dedlsite,dedl_t.dedl005,dedl_t.dedl024,dedl_t.dedl025"
 
   #add-point:b_fill段rank_sql_after name="b_fill.rank_sql_after"
   LET ls_sql_rank = "SELECT UNIQUE dedlsite, ",
                     "                   (SELECT ooefl003 FROM ooefl_t ",
                     "                     WHERE ooeflent = dedlent ",
                     "                       AND ooefl001 = dedlsite ",
                     "                       AND ooefl002 ='"||g_dlang||"') dedlsite_desc,",
