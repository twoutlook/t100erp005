################################################################################
# Descriptions...: 料件分群碼重帶
# Date & Author..: 2014/02/17  By chenjing
# Modify.........:
################################################################################
PRIVATE FUNCTION aimm206_get_imcg()
#DEFINE l_imcg            RECORD LIKE imcg_t.*  #161124-00048#2   2016/12/07 By 08734 mark
#161124-00048#2   2016/12/07 By 08734 add(S)
DEFINE l_imcg RECORD  #料件據點品管分群檔
       imcgent LIKE imcg_t.imcgent, #企业编号
       imcgownid LIKE imcg_t.imcgownid, #资料所有者
       imcgowndp LIKE imcg_t.imcgowndp, #资料所有部门
       imcgcrtid LIKE imcg_t.imcgcrtid, #资料录入者
       imcgcrtdt LIKE imcg_t.imcgcrtdt, #资料创建日
       imcgcrtdp LIKE imcg_t.imcgcrtdp, #资料录入部门
       imcgmodid LIKE imcg_t.imcgmodid, #资料更改者
       imcgmoddt LIKE imcg_t.imcgmoddt, #最近更改日
       imcgstus LIKE imcg_t.imcgstus, #状态码
       imcgsite LIKE imcg_t.imcgsite, #营运据点
       imcg111 LIKE imcg_t.imcg111, #品管分群
       imcg112 LIKE imcg_t.imcg112, #品管员
       imcg113 LIKE imcg_t.imcg113, #检验单位
       imcg114 LIKE imcg_t.imcg114, #进料检验否
       imcg115 LIKE imcg_t.imcg115, #检验程度
       imcg116 LIKE imcg_t.imcg116, #检验水准
       imcg117 LIKE imcg_t.imcg117, #检验级数
       imcg118 LIKE imcg_t.imcg118, #库存失效提前通知天数
       imcg119 LIKE imcg_t.imcg119, #检验标准工时
       imcg120 LIKE imcg_t.imcg120 #使用品检判定等级功能
END RECORD
#161124-00048#2   2016/12/07 By 08734 add(E)
  # SELECT * INTO l_imcg.*  #161124-00048#2   2016/12/07 By 08734 mark
   SELECT imcgent,imcgownid,imcgowndp,imcgcrtid,imcgcrtdt,imcgcrtdp,imcgmodid,imcgmoddt,imcgstus,imcgsite,imcg111,imcg112,imcg113,imcg114,imcg115,imcg116,imcg117,imcg118,imcg119,imcg120  #161124-00048#2   2016/12/07 By 08734 add
     INTO l_imcg.*
     FROM imcg_t
    WHERE imcg111 = g_imae_m.imae111
      AND imcgent = g_enterprise
      AND imcgsite = g_site
   IF g_site = 'ALL' THEN
      LET g_imae_m.imae111 = l_imcg.imcg111
      LET g_imae_m.imae112 = l_imcg.imcg112
      LET g_imae_m.imae113 = l_imcg.imcg113
      LET g_imae_m.imae114 = l_imcg.imcg114
      LET g_imae_m.imae115 = l_imcg.imcg115
      LET g_imae_m.imae116 = l_imcg.imcg116
      LET g_imae_m.imae117 = l_imcg.imcg117
      LET g_imae_m.imae118 = l_imcg.imcg118
      LET g_imae_m.imae119 = l_imcg.imcg119
      LET g_imae_m.imae120 = l_imcg.imcg120
   ELSE
      IF NOT aimm206_chk_ooeh('imae111') THEN
         LET g_imae_m.imae111 = l_imcg.imcg111
      END IF 
      IF NOT aimm206_chk_ooeh('imae112') THEN
         LET g_imae_m.imae112 = l_imcg.imcg112
      END IF 
      IF NOT aimm206_chk_ooeh('imae113') THEN
         LET g_imae_m.imae113 = l_imcg.imcg113
      END IF 
      IF NOT aimm206_chk_ooeh('imae114') THEN
         LET g_imae_m.imae114 = l_imcg.imcg114
      END IF 
      IF NOT aimm206_chk_ooeh('imae115') THEN
         LET g_imae_m.imae115 = l_imcg.imcg115
      END IF 
      IF NOT aimm206_chk_ooeh('imae116') THEN
         LET g_imae_m.imae116 = l_imcg.imcg116
      END IF 
      IF NOT aimm206_chk_ooeh('imae117') THEN
         LET g_imae_m.imae117 = l_imcg.imcg117
      END IF 
      IF NOT aimm206_chk_ooeh('imae118') THEN
         LET g_imae_m.imae118 = l_imcg.imcg118
      END IF 
      IF NOT aimm206_chk_ooeh('imae119') THEN
         LET g_imae_m.imae119 = l_imcg.imcg119
      END IF 
      IF NOT aimm206_chk_ooeh('imae120') THEN
         LET g_imae_m.imae120 = l_imcg.imcg120
      END IF 
   END IF
   
   IF cl_null(g_imae_m.imae113) THEN
      LET g_imae_m.imae113 = g_imae_m.imaa006
   END IF
   
   #---#161110-00032#1-----add---begin---
   IF g_imae_m.imae116 = '1' THEN
      CALL cl_set_combo_scc_part("imae117",'5053','1,2,3')
   END IF
   IF g_imae_m.imae116 = '2' THEN
     CALL cl_set_combo_scc_part("imae117",'5053','1,2,3,4')
   END IF
   IF g_imae_m.imae116 = '3' OR g_imae_m.imae116 = '4' THEN
      CALL cl_set_combo_scc_part("imae117",'5053','1,2,3,4,5,6,7')
   END IF
   #--#161110-00032#1------add---end-----
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_imae_m.imae111
   CALL ap_ref_array2(g_ref_fields,"SELECT oocql004 FROM oocql_t WHERE oocqlent='"||g_enterprise||"' AND oocql001='205' AND oocql002=? AND oocql003='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_imae_m.imae111_desc =  g_rtn_fields[1]
   DISPLAY BY NAME g_imae_m.imae111_desc

   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_imae_m.imae112
   CALL ap_ref_array2(g_ref_fields,"SELECT ooag011 FROM ooag_t WHERE ooagent='"||g_enterprise||"' AND ooag001=? ","") RETURNING g_rtn_fields
   LET g_imae_m.imae112_desc =  g_rtn_fields[1] 
   DISPLAY BY NAME g_imae_m.imae112_desc

   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_imae_m.imae113
   CALL ap_ref_array2(g_ref_fields,"SELECT oocal003 FROM oocal_t WHERE oocalent='"||g_enterprise||"' AND oocal001=? AND oocal002='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_imae_m.imae113_desc =  g_rtn_fields[1]
   DISPLAY BY NAME g_imae_m.imae113_desc

   DISPLAY BY NAME g_imae_m.imae111,g_imae_m.imae112,g_imae_m.imae113, 
       g_imae_m.imae114,g_imae_m.imae115,g_imae_m.imae116,g_imae_m.imae117,g_imae_m.imae120
END FUNCTION
#+ display only 欄位抓取資料
PRIVATE FUNCTION aimm206_imaa_desc()
   IF cl_null(g_imae_m.imae001) THEN
      LET g_imae_m.imaa002 = ""
      LET g_imae_m.imaa003 = ""
      LET g_imae_m.imaa004 = ""
      LET g_imae_m.imaa005 = ""
      LET g_imae_m.imaa006 = ""
      LET g_imae_m.imaa009 = ""
      LET g_imae_m.imaa010 = ""
      LET g_imae_m.imaa003_desc = ""
      LET g_imae_m.imaa005_desc = ""
      LET g_imae_m.imaa006_desc = ""
      LET g_imae_m.imaa009_desc = ""
      LET g_imae_m.imaa010_desc = ""
      LET g_imae_m.l_s1 = ""
      LET g_imae_m.imaal003 = ""
      LET g_imae_m.imaal004 = ""
      LET g_imae_m.imaal005 = ""
               
   ELSE
      SELECT imaa002,imaa009,imaa003,imaa004,imaa005,imaa006,imaa010
        INTO g_imae_m.imaa002,g_imae_m.imaa009,g_imae_m.imaa003,
             g_imae_m.imaa004,g_imae_m.imaa005,g_imae_m.imaa006,
             g_imae_m.imaa010
        FROM imaa_t
        WHERE imaa001 = g_imae_m.imae001
          AND imaaent = g_enterprise

       SELECT imaestus INTO g_imae_m.l_s1 FROM imae_t
        WHERE imaeent = g_enterprise  AND imae001 = g_imae_m.imae001 AND imaesite = g_site
      END IF    
      
      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_imae_m.imae001
      CALL ap_ref_array2(g_ref_fields," SELECT imaal003,imaal004,imaal005 FROM imaal_t WHERE imaalent = '"||g_enterprise||"' AND imaal001 = ? AND imaal002 = '"||g_dlang||"'","") RETURNING g_rtn_fields 
      LET g_imae_m.imaal003 = g_rtn_fields[1] 
      LET g_imae_m.imaal004 = g_rtn_fields[2] 
      LET g_imae_m.imaal005 = g_rtn_fields[3] 
      DISPLAY BY NAME g_imae_m.imaal003,g_imae_m.imaal004,g_imae_m.imaal005
      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_imae_m.imaa009
      CALL ap_ref_array2(g_ref_fields,"SELECT rtaxl003 FROM rtaxl_t WHERE rtaxlent='"||g_enterprise||"' AND rtaxl001=? AND rtaxl002='"||g_dlang||"'","") RETURNING g_rtn_fields
      LET g_imae_m.imaa009_desc = g_rtn_fields[1]
      DISPLAY BY NAME g_imae_m.imaa009_desc

      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_imae_m.imaa003
      CALL ap_ref_array2(g_ref_fields,"SELECT oocql004 FROM oocql_t WHERE oocqlent='"||g_enterprise||"' AND oocql001='200' AND oocql002=? AND oocql003='"||g_dlang||"'","") RETURNING g_rtn_fields
      LET g_imae_m.imaa003_desc =  g_rtn_fields[1]
      DISPLAY BY NAME g_imae_m.imaa003_desc

      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_imae_m.imaa006
      CALL ap_ref_array2(g_ref_fields,"SELECT oocal003 FROM oocal_t WHERE oocalent='"||g_enterprise||"' AND oocal001=? AND oocal002='"||g_dlang||"'","") RETURNING g_rtn_fields
      LET g_imae_m.imaa006_desc = g_rtn_fields[1] 
      DISPLAY BY NAME g_imae_m.imaa006_desc

      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_imae_m.imaa010
      CALL ap_ref_array2(g_ref_fields,"SELECT oocql004 FROM oocql_t WHERE oocqlent='"||g_enterprise||"' AND oocql001='210' AND oocql002=? AND oocql003='"||g_dlang||"'","") RETURNING g_rtn_fields
      LET g_imae_m.imaa010_desc = g_rtn_fields[1] 
      DISPLAY BY NAME g_imae_m.imaa010_desc

      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_imae_m.imaa005
      CALL ap_ref_array2(g_ref_fields,"SELECT imeal003 FROM imeal_t WHERE imealent='"||g_enterprise||"' AND imeal001=? AND imeal002='"||g_dlang||"'","") RETURNING g_rtn_fields
      LET g_imae_m.imaa005_desc =  g_rtn_fields[1] 
      DISPLAY BY NAME g_imae_m.imaa005_desc 
     
      DISPLAY BY NAME g_imae_m.imaa002,g_imae_m.imaa009,g_imae_m.imaa003,g_imae_m.imaa004,
                      g_imae_m.imaa005,g_imae_m.imaa006,g_imae_m.imaa010,g_imae_m.l_s1
      DISPLAY BY NAME g_imae_m.imaal003,g_imae_m.imaal004,g_imae_m.imaal005,
                      g_imae_m.imaa009_desc,g_imae_m.imaa003_desc,g_imae_m.imaa006_desc,g_imae_m.imaa010_desc
