{<section id="aimm218.reproduce" >}
#+ 資料複製
PRIVATE FUNCTION aimm218_reproduce()
   #add-point:reproduce段define name="reproduce.define_customerization"
   
   #end add-point
   DEFINE l_newno     LIKE imaf_t.imafsite 
   DEFINE l_oldno     LIKE imaf_t.imafsite 
 
   DEFINE l_master    RECORD LIKE imaf_t.*
   DEFINE l_detail    RECORD LIKE imaf_t.*
 
   DEFINE l_cnt       LIKE type_t.num10
   #add-point:reproduce段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="reproduce.define"
   RETURN
   #end add-point
   
   #add-point:Function前置處理  name="reproduce.pre_function"
   
   #end add-point
   
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF     
 
   IF g_imaf_m.imafsite IS NULL
 
      THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
   
   LET g_imafsite_t = g_imaf_m.imafsite
 
   
   LET g_imaf_m.imafsite = ""
 
   LET g_master_insert = FALSE
   CALL aimm218_set_entry('a')
   CALL aimm218_set_no_entry('a')
   
   CALL cl_set_head_visible("","YES")
   CALL s_transaction_begin()
   
   #add-point:複製輸入前 name="reproduce.head.b_input"
   
   #end add-point
   
   #清空key欄位的desc
      LET g_imaf_m.imafsite_desc = ''
   DISPLAY BY NAME g_imaf_m.imafsite_desc
 
   
   CALL aimm218_input("r")
    
   IF INT_FLAG AND NOT g_master_insert THEN
      DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
      DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
      INITIALIZE g_imaf_m.* TO NULL
      INITIALIZE g_imaf_d TO NULL
      INITIALIZE g_imaf2_d TO NULL
 
      CALL aimm218_show()
      LET INT_FLAG = 0
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = '' 
      LET g_errparam.code   = 9001 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN 
   END IF
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("modify,modify_detail,delete,reproduce", TRUE)
   CALL aimm218_set_act_visible()
   CALL aimm218_set_act_no_visible()
 
   #將新增的資料併入搜尋條件中
   LET g_state = "insert"
   
   LET g_imafsite_t = g_imaf_m.imafsite
 
   
   #組合新增資料的條件
   LET g_add_browse = " imafent = " ||g_enterprise|| " AND",
                      " imafsite = '", g_imaf_m.imafsite, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL aimm218_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   LET g_curr_diag = ui.DIALOG.getCurrent()
   CALL aimm218_idx_chk()
   
   #add-point:完成複製段落後 name="reproduce.after_reproduce"
   
   #end add-point
   
   #功能已完成,通報訊息中心
   CALL aimm218_msgcentre_notify('reproduce')
   
END FUNCTION
 
