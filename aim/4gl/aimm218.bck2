   CALL aimm218_set_act_visible()
   CALL aimm218_set_act_no_visible()
 
   #將新增的資料併入搜尋條件中
   LET g_state = "insert"
   
   LET g_imafsite_t = g_imaf_m.imafsite
 
   
   #組合新增資料的條件
   LET g_add_browse = " imafent = " ||g_enterprise|| " AND",
                      " imafsite = '", g_imaf_m.imafsite, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL aimm218_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   CALL aimm218_idx_chk()
   
   #撈取異動後的資料(主要是帶出reference)
   EXECUTE aimm218_master_referesh USING g_imaf_m.imafsite INTO g_imaf_m.imafsite,g_imaf_m.imafsite_desc 
 
   
   #遮罩相關處理
   LET g_imaf_m_mask_o.* =  g_imaf_m.*
   CALL aimm218_imaf_t_mask()
   LET g_imaf_m_mask_n.* =  g_imaf_m.*
   
   #將資料顯示到畫面上
   DISPLAY BY NAME g_imaf_m.imafsite,g_imaf_m.imafsite_desc
   
   #功能已完成,通報訊息中心
   CALL aimm218_msgcentre_notify('insert')
   
