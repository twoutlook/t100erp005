      CALL s_desc_get_locator_desc(g_site,g_imaf_d[l_ac].imaf091,g_imaf_d[l_ac].imaf092) RETURNING g_imaf_d[l_ac].imaf092_desc
   END IF
   IF NOT aimm218_chk_ooeh('imaf101') THEN
      LET g_imaf_d[l_ac].imaf101 = l_imcc.imcc101
   END IF
   IF NOT aimm218_chk_ooeh('imaf102') THEN
      LET g_imaf_d[l_ac].imaf102 = l_imcc.imcc102
   END IF
   IF cl_null(g_imaf_d[l_ac].imaf101) THEN
