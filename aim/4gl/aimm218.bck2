################################################################################
# Descriptions...: 獲取不可修改欄位
# Memo...........:
# Usage..........: CALL aimm218_get_ooeh002()
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By lixh
# Modify.........:
################################################################################
PRIVATE FUNCTION aimm218_get_ooeh002()
DEFINE l_field   STRING
DEFINE l_sql     STRING
DEFINE l_ooeh002 LIKE ooeh_t.ooeh002

   LET l_sql = " SELECT ooeh002 FROM ooeh_t ",
               "  WHERE ooehent = '",g_enterprise,"'",
               "    AND ooeh001 = '1' ",
               "    AND ooehstus = 'Y'"
               
   PREPARE aimm218_ooeh002 FROM l_sql
   DECLARE aimm218_ooeh002_cur CURSOR FOR aimm218_ooeh002
   LET l_field = ''
   FOREACH aimm218_ooeh002_cur INTO l_ooeh002
      IF cl_null(l_field) THEN 
         LET l_field = l_field,l_ooeh002
      ELSE
         LET l_field = l_field,",",l_ooeh002
      END IF      
   END FOREACH
   RETURN l_field
END FUNCTION

