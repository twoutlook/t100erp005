   END IF
   IF cl_null(g_imaf_d[l_ac].imaf102) THEN
      LET g_imaf_d[l_ac].imaf102 = "0"
   END IF
   IF cl_null(g_imaf_d[l_ac].imaf053) THEN
      SELECT imaa006 INTO g_imaf_d[l_ac].imaf053 FROM imaa_t
       WHERE imaaent = g_enterprise
         AND imaa001 = g_imaf_d[l_ac].imaf001
      IF cl_null(g_imaf_d[l_ac].imaf053) THEN
         LET g_imaf_d[l_ac].imaf053_desc = ""
