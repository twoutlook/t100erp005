################################################################################
# Descriptions...: 重新带料件分群资料
# Memo...........:
# Usage..........: CALL aimm203_get_imcd()
# Date & Author..: 2014/2/14 By lixiang
# Modify.........:
################################################################################
PRIVATE FUNCTION aimm203_get_imcd()
DEFINE l_imcd    RECORD LIKE imcd_t.*

   INITIALIZE l_imcd.* TO NULL
   SELECT * INTO l_imcd.* FROM imcd_t
    WHERE imcdent = g_enterprise
      AND imcdsite = g_site
      AND imcd111 = g_imaf_m.imaf111
   #集團層的不需要考慮aooi090,據點級的資料需aooi090設定的欄位   
   IF g_site = 'ALL' THEN  
      LET g_imaf_m.imaf112 = l_imcd.imcd112
      LET g_imaf_m.imaf113 = l_imcd.imcd113
      LET g_imaf_m.imaf114 = l_imcd.imcd114
      LET g_imaf_m.imaf115 = l_imcd.imcd115
      LET g_imaf_m.imaf116 = l_imcd.imcd116
      LET g_imaf_m.imaf117 = l_imcd.imcd117
      LET g_imaf_m.imaf118 = l_imcd.imcd118
      LET g_imaf_m.imaf121 = l_imcd.imcd121
      LET g_imaf_m.imaf122 = l_imcd.imcd122
      LET g_imaf_m.imaf123 = l_imcd.imcd123
      LET g_imaf_m.imaf124 = l_imcd.imcd124
      LET g_imaf_m.imaf125 = l_imcd.imcd125
      LET g_imaf_m.imaf126 = l_imcd.imcd126
      LET g_imaf_m.imaf127 = l_imcd.imcd127
      LET g_imaf_m.imaf128 = l_imcd.imcd128
      LET g_imaf_m.imaf130 = l_imcd.imcd130  #160617-00004#3 
   ELSE
      IF NOT aimm203_chk_ooeh('imaf112') THEN
         LET g_imaf_m.imaf112 = l_imcd.imcd112
      END IF
      IF NOT aimm203_chk_ooeh('imaf113') THEN
         LET g_imaf_m.imaf113 = l_imcd.imcd113
      END IF
      IF NOT aimm203_chk_ooeh('imaf114') THEN
         LET g_imaf_m.imaf114 = l_imcd.imcd114
      END IF
      IF NOT aimm203_chk_ooeh('imaf115') THEN
         LET g_imaf_m.imaf115 = l_imcd.imcd115
      END IF
      IF NOT aimm203_chk_ooeh('imaf116') THEN
         LET g_imaf_m.imaf116 = l_imcd.imcd116
      END IF
      IF NOT aimm203_chk_ooeh('imaf117') THEN
         LET g_imaf_m.imaf117 = l_imcd.imcd117
      END IF
      IF NOT aimm203_chk_ooeh('imaf118') THEN
         LET g_imaf_m.imaf118 = l_imcd.imcd118
      END IF
      IF NOT aimm203_chk_ooeh('imaf121') THEN
         LET g_imaf_m.imaf121 = l_imcd.imcd121
      END IF
      IF NOT aimm203_chk_ooeh('imaf122') THEN
         LET g_imaf_m.imaf122 = l_imcd.imcd122
      END IF
      IF NOT aimm203_chk_ooeh('imaf123') THEN
         LET g_imaf_m.imaf123 = l_imcd.imcd123
      END IF
      IF NOT aimm203_chk_ooeh('imaf124') THEN
         LET g_imaf_m.imaf124 = l_imcd.imcd124
      END IF
      IF NOT aimm203_chk_ooeh('imaf125') THEN
         LET g_imaf_m.imaf125 = l_imcd.imcd125
      END IF
      IF NOT aimm203_chk_ooeh('imaf126') THEN
         LET g_imaf_m.imaf126 = l_imcd.imcd126
      END IF
      IF NOT aimm203_chk_ooeh('imaf127') THEN
         LET g_imaf_m.imaf127 = l_imcd.imcd127
      END IF
      IF NOT aimm203_chk_ooeh('imaf128') THEN
         LET g_imaf_m.imaf128 = l_imcd.imcd128
      END IF
      #160617-00004#3--s
      IF NOT aimm203_chk_ooeh('imaf130') THEN
         LET g_imaf_m.imaf130 = l_imcd.imcd130
      END IF
      #160617-00004#3--e
   END IF
   
   #預測料號若無值時(ex.分群那邊沒設這個欄位)，預設料件編號
   IF cl_null(g_imaf_m.imaf125) THEN
      LET g_imaf_m.imaf125 = g_imaf_m.imaf001
   END IF
   IF cl_null(g_imaf_m.imaf112) THEN
      LET g_imaf_m.imaf112 = g_imaf_m.imaa006
   END IF
   IF cl_null(g_imaf_m.imaf113) THEN
      LET g_imaf_m.imaf113 = g_imaf_m.imaa006
   END IF
   
   DISPLAY BY NAME g_imaf_m.imaf001,g_imaf_m.imaf111,g_imaf_m.imaf112,g_imaf_m.imaf113,g_imaf_m.imaf114,g_imaf_m.imaf115, 
     g_imaf_m.imaf116,g_imaf_m.imaf117,g_imaf_m.imaf118,g_imaf_m.imaf121,g_imaf_m.imaf122, 
     g_imaf_m.imaf123,g_imaf_m.imaf124,g_imaf_m.imaf125,g_imaf_m.imaf126,g_imaf_m.imaf127,g_imaf_m.imaf128,
     g_imaf_m.imaf130 #160617-00004#3
     
   CALL aimm203_imaf112_ref(g_imaf_m.imaf112) RETURNING g_imaf_m.imaf112_desc
   DISPLAY BY NAME g_imaf_m.imaf112_desc
   
   CALL aimm203_imaf113_ref(g_imaf_m.imaf113) RETURNING g_imaf_m.imaf113_desc
   DISPLAY BY NAME g_imaf_m.imaf113_desc

   CALL aimm203_imaf123_ref(g_imaf_m.imaf123) RETURNING g_imaf_m.imaf123_desc
   DISPLAY BY NAME g_imaf_m.imaf123_desc
   
   CALL aimm203_imaf125_ref(g_imaf_m.imaf125) RETURNING g_imaf_m.imaf125_desc
   DISPLAY BY NAME g_imaf_m.imaf125_desc
END FUNCTION
################################################################################
# Descriptions...: 檢查aooi090是否設置對應欄位
# Memo...........:
# Usage..........: CALL aimm203_chk_ooeh(p_ooeh002)
# Date & Author..: 2014/02/114 By lixiang
# Modify.........:
################################################################################
PRIVATE FUNCTION aimm203_chk_ooeh(p_ooeh002)
DEFINE p_ooeh002        LIKE ooeh_t.ooeh002
DEFINE r_success        LIKE type_t.num5
DEFINE l_n              LIKE type_t.num5

   LET r_success = TRUE
   LET l_n = 0
   SELECT COUNT(*) INTO l_n FROM ooeh_t
    WHERE ooehent = g_enterprise
      AND ooeh001 = '1'
      AND ooeh002 = p_ooeh002
   IF l_n = 0 THEN
      LET r_success = FALSE
      RETURN r_success
   END IF
   RETURN r_success
   
END FUNCTION

 
