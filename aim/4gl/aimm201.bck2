{<section id="aimm201.set_no_entry" >}
#+ 單頭欄位關閉設定
PRIVATE FUNCTION aimm201_set_no_entry(p_cmd)
   #add-point:set_no_entry段define(客製用) name="set_no_entry.define_customerization"
   
   #end add-point
   DEFINE p_cmd LIKE type_t.chr1
   #add-point:set_no_entry段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="set_no_entry.define"
   DEFINE l_n   LIKE type_t.num10 #150815---earl---add
   #end add-point     
   
   #add-point:Function前置處理  name="set_no_entry.pre_function"
   
   #end add-point
   
   IF p_cmd = 'u' AND g_chkey = 'N' THEN
      CALL cl_set_comp_entry("imaf001",FALSE)
      #根據azzi850使用者身分開關特定欄位
      IF NOT cl_null(g_no_entry) THEN
         CALL cl_set_comp_entry(g_no_entry,FALSE)
      END IF
      #add-point:set_no_entry段欄位控制 name="set_no_entry.field_control"
 
      #end add-point 
   END IF
   
   #add-point:set_no_entry段欄位控制後 name="set_no_entry.after_control"
   IF g_imaf_m.imaf012 <> '3' THEN
      CALL cl_set_comp_entry("imaf021,imaf022,imaf023",FALSE)
      LET g_imaf_m.imaf021 = 0
      LET g_imaf_m.imaf022 = 0
      LET g_imaf_m.imaf023 = 0
   END IF
   IF g_imaf_m.imaf012 <> '4' THEN
      CALL cl_set_comp_entry("imaf024,imaf025",FALSE)
      LET g_imaf_m.imaf024 = 0
      LET g_imaf_m.imaf025 = 0
   END IF
   IF g_imaf_m.imaf034 <> 'Y' THEN
      CALL cl_set_comp_entry("imaf035",FALSE)
      LET g_imaf_m.imaf035 = ''
      LET g_imaf_m.imaf035_desc = ''
   END IF
   #若g_site<>'ALL'，則呼叫s_aooi090_set_no_entry，設定為集團一致的欄位不可輸入
   IF g_site <> 'ALL' THEN
      CALL s_aooi090_set_no_entry('1')  #'1' 據點進銷存資料
   END IF
   
   #判斷據點參數若不使用參考單位時，則參考單位不可以維護(據點參數:S-BAS-0028)
   IF g_site <> 'ALL' AND cl_get_para(g_enterprise,g_site,'S-BAS-0028') = 'N' THEN
      CALL cl_set_comp_entry("imaf015",FALSE)
   END IF
   #160617-00004#1--s
   ##add--2015/05/18 By shiun--(S)
   #IF cl_null(g_imaf_m.imaf177) OR g_imaf_m.imaf177 = 'N' THEN
   #   CALL cl_set_comp_entry("imaf178,imaf179",FALSE)
   #   LET g_imaf_m.imaf178 = ''
   #   LET g_imaf_m.imaf178_desc = ''
   #   LET g_imaf_m.imaf179 = ''
   #   DISPLAY BY NAME g_imaf_m.imaf178,g_imaf_m.imaf178_desc,g_imaf_m.imaf179
   #END IF
   ##add--2015/05/18 By shiun--(E)
   #160617-00004#1---e
   
   #150815---earl---add---s
   #檢查該料件是否已存在於庫存明細檔中
   LET l_n = 0
   SELECT COUNT(*) INTO l_n
     FROM inag_t
    WHERE inagent = g_enterprise
      AND inag001 = g_imaf_m.imaf001 #料號
      
   IF l_n <> 0 THEN
      CALL cl_set_comp_entry("imaf015",FALSE)
   END IF
   #150815---earl---add---e
   
   #end add-point 
 
END FUNCTION
 
