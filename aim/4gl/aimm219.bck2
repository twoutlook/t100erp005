{<section id="aimm219.construct" >}
#+ QBE資料查詢
PRIVATE FUNCTION aimm219_construct()
   #add-point:cs段define name="cs.define_customerization"
   
   #end add-point    
   DEFINE ls_return   STRING
   DEFINE ls_result   STRING 
   DEFINE ls_wc       STRING 
   #add-point:cs段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="cs.define"
   
   #end add-point 
   
   #add-point:Function前置處理  name="cs.pre_function"
   
   #end add-point
   
   #清除畫面上相關資料
   CLEAR FORM                 
   INITIALIZE g_imae_m.* TO NULL
   CALL g_imae_d.clear()
   CALL g_imae2_d.clear()
 
   INITIALIZE g_wc TO NULL
   INITIALIZE g_wc2 TO NULL
   LET g_action_choice = ""
    
   LET g_qryparam.state = 'c'
   
   #add-point:cs段construct外 name="cs.head.before"
   
   #end add-point 
   
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
      
      #單頭
      CONSTRUCT BY NAME g_wc ON imaesite
 
         BEFORE CONSTRUCT
            #add-point:cs段more_construct name="cs.head.before_construct"
            IF cl_null(g_imae_m.imaesite) THEN
               LET g_imae_m.imaesite = g_site 
               CALL s_desc_get_department_desc(g_imae_m.imaesite) RETURNING g_imae_m.imaesite_desc               
            END IF
            DISPLAY BY NAME g_imae_m.imaesite,g_imae_m.imaesite_desc
            #end add-point 
            
                 #應用 a01 樣板自動產生(Version:2)
         BEFORE FIELD imaesite
            #add-point:BEFORE FIELD imaesite name="construct.b.imaesite"
            
            #END add-point
 
 
         #應用 a02 樣板自動產生(Version:2)
         AFTER FIELD imaesite
            
            #add-point:AFTER FIELD imaesite name="construct.a.imaesite"
            
            #END add-point
            
 
 
         #Ctrlp:construct.c.imaesite
         #應用 a03 樣板自動產生(Version:3)
         ON ACTION controlp INFIELD imaesite
            #add-point:ON ACTION controlp INFIELD imaesite name="construct.c.imaesite"
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.return1 = g_site
            DISPLAY g_qryparam.return1 TO imaesite   #顯示到畫面上
            NEXT FIELD imaesite 
            #END add-point
 
 
 
         
      END CONSTRUCT
 
      CONSTRUCT g_wc2_table1 ON imaestus,imae001,imae011,imae012,imae016,imae017,imae018,imae041,imae042, 
          imae051,imae052,imae078,imae079,imae071,imae072,imae073,imae074,imae075,imae081,imae082,imae083, 
          imae085,imae101,imae102,imae103,imae104,imaeownid,imaeowndp,imaecrtid,imaecrtdp,imaecrtdt, 
          imaemodid,imaemoddt,imaecnfid,imaecnfdt
           FROM s_detail1[1].imaestus,s_detail1[1].imae001,s_detail1[1].imae011,s_detail1[1].imae012, 
               s_detail1[1].imae016,s_detail1[1].imae017,s_detail1[1].imae018,s_detail1[1].imae041,s_detail1[1].imae042, 
               s_detail1[1].imae051,s_detail1[1].imae052,s_detail1[1].imae078,s_detail1[1].imae079,s_detail1[1].imae071, 
               s_detail1[1].imae072,s_detail1[1].imae073,s_detail1[1].imae074,s_detail1[1].imae075,s_detail1[1].imae081, 
               s_detail1[1].imae082,s_detail1[1].imae083,s_detail1[1].imae085,s_detail1[1].imae101,s_detail1[1].imae102, 
               s_detail1[1].imae103,s_detail1[1].imae104,s_detail2[1].imaeownid,s_detail2[1].imaeowndp, 
               s_detail2[1].imaecrtid,s_detail2[1].imaecrtdp,s_detail2[1].imaecrtdt,s_detail2[1].imaemodid, 
               s_detail2[1].imaemoddt,s_detail2[1].imaecnfid,s_detail2[1].imaecnfdt
                      
         BEFORE CONSTRUCT
            #add-point:cs段more_construct name="cs.body.before_construct"
            
            #end add-point 
            
         #單身公用欄位開窗相關處理
         #應用 a11 樣板自動產生(Version:3)
         #共用欄位查詢處理  
         ##----<<imaecrtdt>>----
         AFTER FIELD imaecrtdt
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
 
         #----<<imaemoddt>>----
         AFTER FIELD imaemoddt
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
         
         #----<<imaecnfdt>>----
