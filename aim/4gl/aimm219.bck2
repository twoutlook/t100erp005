{<section id="aimm219.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION aimm219_b_fill(p_wc2)
   #add-point:b_fill段define name="b_fill.define_customerization"
   
   #end add-point
   DEFINE p_wc2      STRING
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   
   #end add-point     
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   #先清空單身變數內容
   CALL g_imae_d.clear()
   CALL g_imae2_d.clear()
 
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   
   #end add-point
   
   LET g_sql = "SELECT  DISTINCT imaestus,imae001,imae011,imae012,imae016,imae017,imae018,imae041,imae042, 
       imae051,imae052,imae078,imae079,imae071,imae072,imae073,imae074,imae075,imae081,imae082,imae083, 
       imae085,imae101,imae102,imae103,imae104,imae001,imaeownid,imaeowndp,imaecrtid,imaecrtdp,imaecrtdt, 
       imaemodid,imaemoddt,imaecnfid,imaecnfdt,t1.imaal003 ,t2.imaal004 ,t3.oocql004 ,t4.ooag011 ,t5.oocal003 , 
       t6.inayl003 ,t7.inab003 ,t8.oocal003 ,t9.inayl003 ,t10.inab003 ,t11.inayl003 ,t12.inab003 ,t14.ooag011 , 
       t15.ooefl003 ,t16.ooag011 ,t17.ooefl003 ,t18.ooag011 ,t19.ooag011 FROM imae_t",   
               "",
               
                              " LEFT JOIN imaal_t t1 ON t1.imaalent="||g_enterprise||" AND t1.imaal001=imae001 AND t1.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t2 ON t2.imaalent="||g_enterprise||" AND t2.imaal001=imae001 AND t2.imaal002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t3 ON t3.oocqlent="||g_enterprise||" AND t3.oocql001='204' AND t3.oocql002=imae011 AND t3.oocql003='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t4 ON t4.ooagent="||g_enterprise||" AND t4.ooag001=imae012  ",
               " LEFT JOIN oocal_t t5 ON t5.oocalent="||g_enterprise||" AND t5.oocal001=imae016 AND t5.oocal002='"||g_dlang||"' ",
               " LEFT JOIN inayl_t t6 ON t6.inaylent="||g_enterprise||" AND t6.inayl001=imae041 AND t6.inayl002='"||g_dlang||"' ",
               " LEFT JOIN inab_t t7 ON t7.inabent="||g_enterprise||" AND t7.inab001=imae041 AND t7.inab002=imae042  ",
               " LEFT JOIN oocal_t t8 ON t8.oocalent="||g_enterprise||" AND t8.oocal001=imae081 AND t8.oocal002='"||g_dlang||"' ",
               " LEFT JOIN inayl_t t9 ON t9.inaylent="||g_enterprise||" AND t9.inayl001=imae101 AND t9.inayl002='"||g_dlang||"' ",
               " LEFT JOIN inab_t t10 ON t10.inabent="||g_enterprise||" AND t10.inab001=imae101 AND t10.inab002=imae102  ",
               " LEFT JOIN inayl_t t11 ON t11.inaylent="||g_enterprise||" AND t11.inayl001=imae103 AND t11.inayl002='"||g_dlang||"' ",
               " LEFT JOIN inab_t t12 ON t12.inabent="||g_enterprise||" AND t12.inab001=imae103 AND t12.inab002=imae104  ",
               " LEFT JOIN ooag_t t14 ON t14.ooagent="||g_enterprise||" AND t14.ooag001=imaeownid  ",
               " LEFT JOIN ooefl_t t15 ON t15.ooeflent="||g_enterprise||" AND t15.ooefl001=imaeowndp AND t15.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t16 ON t16.ooagent="||g_enterprise||" AND t16.ooag001=imaecrtid  ",
               " LEFT JOIN ooefl_t t17 ON t17.ooeflent="||g_enterprise||" AND t17.ooefl001=imaecrtdp AND t17.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t18 ON t18.ooagent="||g_enterprise||" AND t18.ooag001=imaemodid  ",
               " LEFT JOIN ooag_t t19 ON t19.ooagent="||g_enterprise||" AND t19.ooag001=imaecnfid  ",
 
               " WHERE imaeent= ? AND imaesite=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("imae_t")
   END IF
   
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF aimm219_fill_chk(1) THEN
      IF (g_action_choice = "query" OR cl_null(g_action_choice))
         #add-point:b_fill段long_sql_if name="b_fill.long_sql_if"
         
         #end add-point
      THEN
         LET g_sql = g_sql, " ORDER BY imae_t.imae001"
         #add-point:b_fill段fill_before name="b_fill.fill_before"
         
         #end add-point
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE aimm219_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR aimm219_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_imae_m.imaesite
                                               
      FOREACH b_fill_cs INTO g_imae_d[l_ac].imaestus,g_imae_d[l_ac].imae001,g_imae_d[l_ac].imae011,g_imae_d[l_ac].imae012, 
          g_imae_d[l_ac].imae016,g_imae_d[l_ac].imae017,g_imae_d[l_ac].imae018,g_imae_d[l_ac].imae041, 
          g_imae_d[l_ac].imae042,g_imae_d[l_ac].imae051,g_imae_d[l_ac].imae052,g_imae_d[l_ac].imae078, 
