PRIVATE FUNCTION aimm204_imaa_desc()
   IF cl_null(g_imaf_m.imaf001) THEN
      LET g_imaf_m.imaa002 = ""
      LET g_imaf_m.imaa003 = ""
      LET g_imaf_m.imaa004 = ""
      LET g_imaf_m.imaa005 = ""
      LET g_imaf_m.imaa006 = ""
      LET g_imaf_m.imaa009 = ""
      LET g_imaf_m.imaa010 = ""
      LET g_imaf_m.imaa003_desc = ""
      LET g_imaf_m.imaa006_desc = ""
      LET g_imaf_m.imaa009_desc = ""
      LET g_imaf_m.imaa010_desc = ""
      LET g_imaf_m.l_s1 = ""
      LET g_imaf_m.imaal003 = ""
      LET g_imaf_m.imaal004 = ""
      LET g_imaf_m.imaal005 = ""
      LET g_imaf_m.imai021 = ""
      LET g_imaf_m.imai022 = ""
      LET g_imaf_m.imai023 = ""
      LET g_imaf_m.imai024 = ""
      LET g_imaf_m.imai025 = ""
               
   ELSE
      SELECT imaa002,imaa009,imaa003,imaa004,imaa005,imaa006,imaa010
        INTO g_imaf_m.imaa002,g_imaf_m.imaa009,g_imaf_m.imaa003,
             g_imaf_m.imaa004,g_imaf_m.imaa005,g_imaf_m.imaa006,
             g_imaf_m.imaa010
        FROM imaa_t
        WHERE imaa001 = g_imaf_m.imaf001
          AND imaaent = g_enterprise
    
      SELECT imafstus INTO g_imaf_m.l_s1 FROM imaf_t
       WHERE imafent = g_enterprise
         AND imafsite = g_site
         AND imaf001 = g_imaf_m.imaf001
         
      SELECT imai021,imai022,imai023,imai024,imai025 INTO g_imaf_m.imai021,g_imaf_m.imai022,g_imaf_m.imai023,g_imaf_m.imai024,g_imaf_m.imai025
        FROM imai_t
       WHERE imai001 = g_imaf_m.imaf001
         AND imaient =  g_enterprise
         AND imaisite = g_site
      END IF    
      
      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_imaf_m.imaf001
      CALL ap_ref_array2(g_ref_fields," SELECT imaal003,imaal004,imaal005 FROM imaal_t WHERE imaalent = '"||g_enterprise||"' AND imaal001 = ? AND imaal002 = '"||g_dlang||"'","") RETURNING g_rtn_fields 
      LET g_imaf_m.imaal003 = g_rtn_fields[1] 
      LET g_imaf_m.imaal004 = g_rtn_fields[2] 
      LET g_imaf_m.imaal005 = g_rtn_fields[3] 
      DISPLAY BY NAME g_imaf_m.imaal003,g_imaf_m.imaal004,g_imaf_m.imaal005
      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_imaf_m.imaa009
      CALL ap_ref_array2(g_ref_fields,"SELECT rtaxl003 FROM rtaxl_t WHERE rtaxlent='"||g_enterprise||"' AND rtaxl001=? AND rtaxl002='"||g_dlang||"'","") RETURNING g_rtn_fields
      LET g_imaf_m.imaa009_desc = g_rtn_fields[1] 
      DISPLAY BY NAME g_imaf_m.imaa009_desc

      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_imaf_m.imaa003
      CALL ap_ref_array2(g_ref_fields,"SELECT oocql004 FROM oocql_t WHERE oocqlent='"||g_enterprise||"' AND oocql001='200' AND oocql002=? AND oocql003='"||g_dlang||"'","") RETURNING g_rtn_fields
      LET g_imaf_m.imaa003_desc =  g_rtn_fields[1] 
      DISPLAY BY NAME g_imaf_m.imaa003_desc
      
      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_imaf_m.imaa005
      CALL ap_ref_array2(g_ref_fields,"SELECT imeal003 FROM imeal_t WHERE imealent='"||g_enterprise||"' AND imeal001=? AND imeal002='"||g_dlang||"'","") RETURNING g_rtn_fields
      LET g_imaf_m.imaa005_desc =  g_rtn_fields[1] 
      DISPLAY BY NAME g_imaf_m.imaa005_desc

      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_imaf_m.imaa006
      CALL ap_ref_array2(g_ref_fields,"SELECT oocal003 FROM oocal_t WHERE oocalent='"||g_enterprise||"' AND oocal001=? AND oocal002='"||g_dlang||"'","") RETURNING g_rtn_fields
      LET g_imaf_m.imaa006_desc =  g_rtn_fields[1] 
      DISPLAY BY NAME g_imaf_m.imaa006_desc

      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_imaf_m.imaa010
      CALL ap_ref_array2(g_ref_fields,"SELECT oocql004 FROM oocql_t WHERE oocqlent='"||g_enterprise||"' AND oocql001='210' AND oocql002=? AND oocql003='"||g_dlang||"'","") RETURNING g_rtn_fields
      LET g_imaf_m.imaa010_desc = g_rtn_fields[1] 
