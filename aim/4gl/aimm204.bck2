                      g_imaf_m.imaa009_desc,g_imaf_m.imaa003_desc,g_imaf_m.imaa006_desc,g_imaf_m.imaa010_desc
      DISPLAY BY NAME g_imaf_m.imai021,g_imaf_m.imai022,g_imaf_m.imai023,g_imaf_m.imai024,g_imaf_m.imai025                  
