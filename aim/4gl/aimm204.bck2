PRIVATE FUNCTION aimm204_imaf141_chk()
DEFINE l_imce141    LIKE imce_t.imce141
DEFINE l_imcestus   LIKE imce_t.imcestus  

   IF g_site = 'ALL' THEN
      LET l_imce141 = ''
      LET l_imcestus = ''
      SELECT imce141 ,imcestus INTO l_imce141,l_imcestus
        FROM imce_t
       WHERE imceent = g_enterprise
         AND imcesite = g_site
         AND imce141 = g_imaf_m.imaf141
      IF cl_null(l_imce141) THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'aim-00048'
         LET g_errparam.extend = l_imce141
         LET g_errparam.popup = TRUE
         CALL cl_err()
         RETURN FALSE
      END IF
      IF l_imcestus != 'Y' THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code =  'sub-01302'  #160318-00005#20 mod 'aim-00049'
         LET g_errparam.extend = l_imce141
         #160318-00005#20  --add--str
         LET g_errparam.replace[1] ='aimi104'
         LET g_errparam.replace[2] = cl_get_progname('aimi104',g_lang,"2")
         LET g_errparam.exeprog    ='aimi104'
         #160318-00005#20 --add--end
         LET g_errparam.popup = TRUE
         CALL cl_err()
         RETURN FALSE
      END IF
   ELSE
      IF NOT s_azzi650_chk_exist('203',g_imaf_m.imaf141) THEN
         RETURN FALSE
      END IF
   END IF

   RETURN TRUE
END FUNCTION
