      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE aimi110_master_referesh USING g_imcp_m.imcpsite,g_imcp_m.imcp011 INTO g_imcp_m.imcp011,g_imcp_m.imcp012, 
       g_imcp_m.imcp013,g_imcp_m.imcp014,g_imcp_m.imcpsite,g_imcp_m.imcpstus,g_imcp_m.imcpownid,g_imcp_m.imcpowndp, 
       g_imcp_m.imcpcrtid,g_imcp_m.imcpcrtdp,g_imcp_m.imcpcrtdt,g_imcp_m.imcpmodid,g_imcp_m.imcpmoddt, 
       g_imcp_m.imcp021,g_imcp_m.imcp022,g_imcp_m.imcp023,g_imcp_m.imcp024,g_imcp_m.imcp025,g_imcp_m.imcp031, 
       g_imcp_m.imcp032,g_imcp_m.imcp033,g_imcp_m.imcp034,g_imcp_m.imcpownid_desc,g_imcp_m.imcpowndp_desc, 
       g_imcp_m.imcpcrtid_desc,g_imcp_m.imcpcrtdp_desc,g_imcp_m.imcpmodid_desc
   
 
   #檢查是否允許此動作
   IF NOT aimi110_action_chk() THEN
      CLOSE aimi110_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_imcp_m.imcp011,g_imcp_m.oocql004,g_imcp_m.oocql005,g_imcp_m.imcp012,g_imcp_m.imcp013, 
       g_imcp_m.imcp014,g_imcp_m.imcpsite,g_imcp_m.imcpstus,g_imcp_m.imcpownid,g_imcp_m.imcpownid_desc, 
       g_imcp_m.imcpowndp,g_imcp_m.imcpowndp_desc,g_imcp_m.imcpcrtid,g_imcp_m.imcpcrtid_desc,g_imcp_m.imcpcrtdp, 
       g_imcp_m.imcpcrtdp_desc,g_imcp_m.imcpcrtdt,g_imcp_m.imcpmodid,g_imcp_m.imcpmodid_desc,g_imcp_m.imcpmoddt, 
       g_imcp_m.imcp021,g_imcp_m.imcp022,g_imcp_m.imcp023,g_imcp_m.imcp024,g_imcp_m.imcp025,g_imcp_m.imcp031, 
       g_imcp_m.imcp032,g_imcp_m.imcp033,g_imcp_m.imcp034
 
   CASE g_imcp_m.imcpstus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   {<point name="statechange.after_refresh"/>}
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_imcp_m.imcpstus
            
            WHEN "N"
               HIDE OPTION "inactive"
            WHEN "Y"
               HIDE OPTION "active"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      {<point name="statechange.before_menu"/>}
      #end add-point
      
      
	  
      ON ACTION inactive
         IF cl_auth_chk_act("inactive") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.inactive"
            {<point name="statechange.inactive"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION active
         IF cl_auth_chk_act("active") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.active"
            {<point name="statechange.active"/>}
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      {<point name="statechange.more_control"/>}
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      ) OR 
      g_imcp_m.imcpstus = lc_state OR cl_null(lc_state) THEN
      CLOSE aimi110_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   {<point name="statechange.b_update"/>}
   #end add-point
   
   LET g_imcp_m.imcpmodid = g_user
   LET g_imcp_m.imcpmoddt = cl_get_current()
   LET g_imcp_m.imcpstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE imcp_t 
      SET (imcpstus,imcpmodid,imcpmoddt) 
        = (g_imcp_m.imcpstus,g_imcp_m.imcpmodid,g_imcp_m.imcpmoddt)     
    WHERE imcpent = g_enterprise AND imcpsite = g_imcp_m.imcpsite
      AND imcp011 = g_imcp_m.imcp011
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE aimi110_master_referesh USING g_imcp_m.imcpsite,g_imcp_m.imcp011 INTO g_imcp_m.imcp011, 
          g_imcp_m.imcp012,g_imcp_m.imcp013,g_imcp_m.imcp014,g_imcp_m.imcpsite,g_imcp_m.imcpstus,g_imcp_m.imcpownid, 
          g_imcp_m.imcpowndp,g_imcp_m.imcpcrtid,g_imcp_m.imcpcrtdp,g_imcp_m.imcpcrtdt,g_imcp_m.imcpmodid, 
          g_imcp_m.imcpmoddt,g_imcp_m.imcp021,g_imcp_m.imcp022,g_imcp_m.imcp023,g_imcp_m.imcp024,g_imcp_m.imcp025, 
          g_imcp_m.imcp031,g_imcp_m.imcp032,g_imcp_m.imcp033,g_imcp_m.imcp034,g_imcp_m.imcpownid_desc, 
          g_imcp_m.imcpowndp_desc,g_imcp_m.imcpcrtid_desc,g_imcp_m.imcpcrtdp_desc,g_imcp_m.imcpmodid_desc 

      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_imcp_m.imcp011,g_imcp_m.oocql004,g_imcp_m.oocql005,g_imcp_m.imcp012,g_imcp_m.imcp013, 
          g_imcp_m.imcp014,g_imcp_m.imcpsite,g_imcp_m.imcpstus,g_imcp_m.imcpownid,g_imcp_m.imcpownid_desc, 
          g_imcp_m.imcpowndp,g_imcp_m.imcpowndp_desc,g_imcp_m.imcpcrtid,g_imcp_m.imcpcrtid_desc,g_imcp_m.imcpcrtdp, 
          g_imcp_m.imcpcrtdp_desc,g_imcp_m.imcpcrtdt,g_imcp_m.imcpmodid,g_imcp_m.imcpmodid_desc,g_imcp_m.imcpmoddt, 
          g_imcp_m.imcp021,g_imcp_m.imcp022,g_imcp_m.imcp023,g_imcp_m.imcp024,g_imcp_m.imcp025,g_imcp_m.imcp031, 
          g_imcp_m.imcp032,g_imcp_m.imcp033,g_imcp_m.imcp034
   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
   {<point name="statechange.a_update"/>}
   #end add-point
 
   #add-point:statechange段結束前 name="statechange.after"
   {<point name="statechange.after"/>}
   #end add-point  
 
   CLOSE aimi110_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aimi110_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION
 
 
 

{</section>}

