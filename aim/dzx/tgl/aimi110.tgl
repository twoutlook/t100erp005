{<section id="aimi110.modify" >}
#+ 資料修改
PRIVATE FUNCTION aimi110_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   {<point name="modify.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   {<point name="modify.define"/>}
   #end add-point
   
   #add-point:Function前置處理 name="modify.pre_function"
   {<point name="modify.pre_function"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_imcp_m.imcpsite IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_imcpsite_t = g_imcp_m.imcpsite
   LET g_imcp011_t = g_imcp_m.imcp011
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN aimi110_cl USING g_enterprise,g_imcp_m.imcpsite,g_imcp_m.imcp011
   IF SQLCA.SQLCODE THEN    #(ver:49)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aimi110_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE
      LET g_errparam.popup = TRUE 
      CLOSE aimi110_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aimi110_master_referesh USING g_imcp_m.imcpsite,g_imcp_m.imcp011 INTO g_imcp_m.imcp011,g_imcp_m.imcp012, 
       g_imcp_m.imcp013,g_imcp_m.imcp014,g_imcp_m.imcpsite,g_imcp_m.imcpstus,g_imcp_m.imcpownid,g_imcp_m.imcpowndp, 
       g_imcp_m.imcpcrtid,g_imcp_m.imcpcrtdp,g_imcp_m.imcpcrtdt,g_imcp_m.imcpmodid,g_imcp_m.imcpmoddt, 
       g_imcp_m.imcp021,g_imcp_m.imcp022,g_imcp_m.imcp023,g_imcp_m.imcp024,g_imcp_m.imcp025,g_imcp_m.imcp031, 
       g_imcp_m.imcp032,g_imcp_m.imcp033,g_imcp_m.imcp034,g_imcp_m.imcpownid_desc,g_imcp_m.imcpowndp_desc, 
       g_imcp_m.imcpcrtid_desc,g_imcp_m.imcpcrtdp_desc,g_imcp_m.imcpmodid_desc
 
   #檢查是否允許此動作
   IF NOT aimi110_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_imcp_m_mask_o.* =  g_imcp_m.*
   CALL aimi110_imcp_t_mask()
   LET g_imcp_m_mask_n.* =  g_imcp_m.*
   
   
 
   #顯示資料
   CALL aimi110_show()
   
   WHILE TRUE
      LET g_imcp_m.imcpsite = g_imcpsite_t
      LET g_imcp_m.imcp011 = g_imcp011_t
 
      
      #寫入修改者/修改日期資訊
      LET g_imcp_m.imcpmodid = g_user 
LET g_imcp_m.imcpmoddt = cl_get_current()
LET g_imcp_m.imcpmodid_desc = cl_get_username(g_imcp_m.imcpmodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
      #end add-point
 
      #資料輸入
      CALL aimi110_input("u")     
 
      #add-point:modify段修改後 name="modify.after_input"
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         LET g_imcp_m.* = g_imcp_m_t.*
         CALL aimi110_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE imcp_t SET (imcpmodid,imcpmoddt) = (g_imcp_m.imcpmodid,g_imcp_m.imcpmoddt)
       WHERE imcpent = g_enterprise AND imcpsite = g_imcpsite_t
         AND imcp011 = g_imcp011_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL aimi110_set_act_visible()
   CALL aimi110_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " imcpent = " ||g_enterprise|| " AND",
                      " imcpsite = '", g_imcp_m.imcpsite, "' "
                      ," AND imcp011 = '", g_imcp_m.imcp011, "' "
 
   #填到對應位置
   CALL aimi110_browser_fill(g_wc,"")
 
   CLOSE aimi110_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aimi110_msgcentre_notify('modify')
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   

