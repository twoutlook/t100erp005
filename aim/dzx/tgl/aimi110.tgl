   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_imcp_m.imcpsite IS NULL
      OR g_imcp_m.imcp011 IS NULL
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN aimi110_cl USING g_enterprise,g_imcp_m.imcpsite,g_imcp_m.imcp011
   IF STATUS THEN
      CLOSE aimi110_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
