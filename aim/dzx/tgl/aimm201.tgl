                   g_imaf_m.imaf016,g_imaf_m.imaf017,g_imaf_m.imaf018,g_imaf_m.imafownid,g_imaf_m.imafowndp, 
                   g_imaf_m.imafcrtid,g_imaf_m.imafcrtdp,g_imaf_m.imafcrtdt,g_imaf_m.imafmodid,g_imaf_m.imafmoddt, 
                   g_imaf_m.imaf021,g_imaf_m.imaf022,g_imaf_m.imaf023,g_imaf_m.imaf024,g_imaf_m.imaf025, 
                   g_imaf_m.imaf026,g_imaf_m.imaf027,g_imaf_m.imaf031,g_imaf_m.imaf032,g_imaf_m.imaf033, 
                   g_imaf_m.imaf034,g_imaf_m.imaf035,g_imaf_m.imaf096)
                WHERE imafent = g_enterprise AND imafsite = g_site AND imaf001 = g_imaf001_t #
 
               #add-point:單頭修改中 name="input.head.m_update"
               {<point name="input.head.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL s_transaction_end('N','0')
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "imaf_t" 
                     LET g_errparam.code   = "std-00009" 
                     LET g_errparam.popup  = TRUE 
