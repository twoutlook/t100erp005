               LET l_count = 1  
 
               #確定新增的資料不存在(不重複)
               SELECT COUNT(1) INTO l_count FROM imaf_t
                WHERE imafent = g_enterprise AND imafsite = g_site AND imaf001 = g_imaf_m.imaf001
 
               IF l_count = 0 THEN
               
                  #add-point:單頭新增前 name="input.head.b_insert"
