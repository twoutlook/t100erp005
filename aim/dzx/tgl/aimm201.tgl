               " LEFT JOIN imaal_t t8 ON t8.imaalent="||g_enterprise||" AND t8.imaal001=t0.imaf035 AND t8.imaal002='"||g_dlang||"' ",
 
               " WHERE t0.imafent = " ||g_enterprise|| " AND t0.imafsite = ? AND t0.imaf001 = ?"
