   #LOCK CURSOR (identifier)
   #add-point:SQL_define name="main.define_sql"
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT imaf001,'','','','','','','','','','','','','','','','',imaf012,imaf013, 
       imaf014,imaf156,imaf015,'',imaf016,'',imaf017,'',imaf018,'','','','','','','','','','','','', 
