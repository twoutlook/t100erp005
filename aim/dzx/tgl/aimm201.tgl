       imaf024,imaf025,imaf026,imaf027,imaf031,imaf032,imaf033,imaf034,imaf035,'','','','','','','', 
       '','',imaf096,'','','','','','','','','','','','','','','','','','','','','','','','','','','', 
       '','','','','','',''", 
                      " FROM imaf_t",
                      " WHERE imafent= ? AND imafsite= ? AND imaf001=? FOR UPDATE"
   #add-point:SQL_define name="main.after_define_sql"
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE aimm201_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.imaf001,t0.imaf012,t0.imaf013,t0.imaf014,t0.imaf156,t0.imaf015,t0.imaf016, 
       t0.imaf017,t0.imaf018,t0.imafownid,t0.imafowndp,t0.imafcrtid,t0.imafcrtdp,t0.imafcrtdt,t0.imafmodid, 
