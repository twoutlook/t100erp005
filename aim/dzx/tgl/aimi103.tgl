               #將遮罩欄位還原
               CALL aimi103_imcd_t_mask_restore('restore_mask_o')
               
               UPDATE imcd_t SET (imcd111,imcd112,imcd113,imcd114,imcd115,imcd116,imcd117,imcd118,imcdstus, 
                   imcdownid,imcdowndp,imcdcrtid,imcdcrtdp,imcdcrtdt,imcdmodid,imcdmoddt,imcd121,imcd122, 
                   imcd123,imcd124,imcd125,imcd126,imcd127,imcd130,imcd128) = (g_imcd_m.imcd111,g_imcd_m.imcd112, 
                   g_imcd_m.imcd113,g_imcd_m.imcd114,g_imcd_m.imcd115,g_imcd_m.imcd116,g_imcd_m.imcd117, 
                   g_imcd_m.imcd118,g_imcd_m.imcdstus,g_imcd_m.imcdownid,g_imcd_m.imcdowndp,g_imcd_m.imcdcrtid, 
                   g_imcd_m.imcdcrtdp,g_imcd_m.imcdcrtdt,g_imcd_m.imcdmodid,g_imcd_m.imcdmoddt,g_imcd_m.imcd121, 
                   g_imcd_m.imcd122,g_imcd_m.imcd123,g_imcd_m.imcd124,g_imcd_m.imcd125,g_imcd_m.imcd126, 
                   g_imcd_m.imcd127,g_imcd_m.imcd130,g_imcd_m.imcd128)
                WHERE imcdent = g_enterprise AND imcdsite = g_site AND imcd111 = g_imcd111_t #
 
               #add-point:單頭修改中 name="input.head.m_update"
               {<point name="input.head.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL s_transaction_end('N','0')
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "imcd_t" 
                     LET g_errparam.code   = "std-00009" 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     NEXT FIELD CURRENT
                  WHEN SQLCA.SQLCODE #其他錯誤
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "imcd_t:",SQLERRMESSAGE 
                     LET g_errparam.code = SQLCA.SQLCODE
                     LET g_errparam.popup = TRUE 
                     CALL s_transaction_end('N','0')
                     CALL cl_err()
                     NEXT FIELD CURRENT
                  OTHERWISE
                     
                     #資料多語言用-增/改
                              INITIALIZE l_var_keys TO NULL 
         INITIALIZE l_field_keys TO NULL 
         INITIALIZE l_vars TO NULL 
         INITIALIZE l_fields TO NULL 
         IF '202' = g_master_multi_table_t.oocql001 AND
         g_imcd_m.imcd111 = g_master_multi_table_t.oocql002 AND
         g_imcd_m.oocql004 = g_master_multi_table_t.oocql004 AND 
         g_imcd_m.oocql005 = g_master_multi_table_t.oocql005  THEN
         ELSE 
            LET l_var_keys[01] = g_enterprise
            LET l_field_keys[01] = 'oocqlent'
            LET l_var_keys_bak[01] = g_enterprise
            LET l_var_keys[02] = '202'
            LET l_field_keys[02] = 'oocql001'
            LET l_var_keys_bak[02] = g_master_multi_table_t.oocql001
            LET l_var_keys[03] = g_imcd_m.imcd111
            LET l_field_keys[03] = 'oocql002'
            LET l_var_keys_bak[03] = g_master_multi_table_t.oocql002
            LET l_var_keys[04] = g_dlang
            LET l_field_keys[04] = 'oocql003'
            LET l_var_keys_bak[04] = g_dlang
            LET l_vars[01] = g_imcd_m.oocql004
            LET l_fields[01] = 'oocql004'
            LET l_vars[02] = g_imcd_m.oocql005
            LET l_fields[02] = 'oocql005'
            CALL cl_multitable(l_var_keys,l_field_keys,l_vars,l_fields,l_var_keys_bak,'oocql_t')
         END IF 
 
                     
                     #將遮罩欄位進行遮蔽
                     CALL aimi103_imcd_t_mask_restore('restore_mask_n')
                     
                     #add-point:單頭修改後 name="input.head.a_update"
                     {<point name="input.head.a_update"/>}
                     #end add-point
                     #修改歷程記錄(單頭修改)
                     LET g_log1 = util.JSON.stringify(g_imcd_m_t)
                     LET g_log2 = util.JSON.stringify(g_imcd_m)
                     IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                        CALL s_transaction_end('N','0')
                     ELSE
                        CALL s_transaction_end('Y','0')
                     END IF
               END CASE
               
            END IF
           #controlp
      END INPUT
      
      #add-point:input段more input  name="input.more_input"
      {<point name="input.more_input"/>}
      #end add-point
    
      BEFORE DIALOG
