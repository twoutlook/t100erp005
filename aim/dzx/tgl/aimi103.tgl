      LET g_imcd_m.imcdmodid = g_user
      LET g_imcd_m.imcdmoddt = cl_get_current()
      LET g_imcd_m.imcdstus = 'Y'
 
 
 
 
      #append欄位給值
      
     
      #一般欄位給值
            LET g_imcd_m.imcd114 = "0"
      LET g_imcd_m.imcd115 = "0"
      LET g_imcd_m.imcd116 = "1"
      LET g_imcd_m.imcd117 = "0"
      LET g_imcd_m.imcd118 = "0"
      LET g_imcd_m.imcdstus = "Y"
      LET g_imcd_m.imcd121 = "0"
      LET g_imcd_m.imcd122 = "0"
      LET g_imcd_m.imcd124 = "0"
      LET g_imcd_m.imcd126 = "N"
      LET g_imcd_m.imcd127 = "N"
      LET g_imcd_m.imcd130 = "0"
      LET g_imcd_m.imcd128 = "0"
 
 
      #add-point:單頭預設值 name="insert.default"
      {<point name="insert.default"/>}
      #end add-point   
     
      #顯示狀態(stus)圖片
            #應用 a21 樣板自動產生(Version:3)
	  #根據當下狀態碼顯示圖片
      CASE g_imcd_m.imcdstus 
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
         
      END CASE
 
 
 
     
      #資料輸入
      CALL aimi103_input("a")
      
      #add-point:單頭輸入後 name="insert.after_insert"
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG THEN
         #取消
         LET INT_FLAG = 0
         DISPLAY g_current_cnt TO FORMONLY.h_count     #總筆數
         DISPLAY g_current_idx TO FORMONLY.h_index     #當下筆數
         INITIALIZE g_imcd_m.* TO NULL
         CALL aimi103_show()
         CALL s_transaction_end('N','0')
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         RETURN
      END IF
 
      LET g_rec_b = 0
      EXIT WHILE
   END WHILE
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL aimi103_set_act_visible()
   CALL aimi103_set_act_no_visible()
 
   #將新增的資料併入搜尋條件中
   LET g_state = "insert"
   
   LET g_imcd111_t = g_imcd_m.imcd111
 
   
   #組合新增資料的條件
   LET g_add_browse = " imcdent = " ||g_enterprise|| " AND imcdsite = '" ||g_site|| "' AND",
                      " imcd111 = '", g_imcd_m.imcd111, "' "
 
