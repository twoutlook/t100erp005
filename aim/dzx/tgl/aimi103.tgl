{<section id="aimi103.modify" >}
#+ 資料修改
PRIVATE FUNCTION aimi103_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   {<point name="modify.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   {<point name="modify.define"/>}
   #end add-point
   
   #add-point:Function前置處理 name="modify.pre_function"
   {<point name="modify.pre_function"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_imcd_m.imcd111 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_imcd111_t = g_imcd_m.imcd111
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN aimi103_cl USING g_enterprise, g_site,g_imcd_m.imcd111
   IF SQLCA.SQLCODE THEN    #(ver:49)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aimi103_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE
      LET g_errparam.popup = TRUE 
      CLOSE aimi103_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aimi103_master_referesh USING g_site,g_imcd_m.imcd111 INTO g_imcd_m.imcd111,g_imcd_m.imcd112, 
       g_imcd_m.imcd113,g_imcd_m.imcd114,g_imcd_m.imcd115,g_imcd_m.imcd116,g_imcd_m.imcd117,g_imcd_m.imcd118, 
       g_imcd_m.imcdstus,g_imcd_m.imcdownid,g_imcd_m.imcdowndp,g_imcd_m.imcdcrtid,g_imcd_m.imcdcrtdp, 
       g_imcd_m.imcdcrtdt,g_imcd_m.imcdmodid,g_imcd_m.imcdmoddt,g_imcd_m.imcd121,g_imcd_m.imcd122,g_imcd_m.imcd123, 
       g_imcd_m.imcd124,g_imcd_m.imcd125,g_imcd_m.imcd126,g_imcd_m.imcd127,g_imcd_m.imcd130,g_imcd_m.imcd128, 
       g_imcd_m.imcd112_desc,g_imcd_m.imcd113_desc,g_imcd_m.imcdownid_desc,g_imcd_m.imcdowndp_desc,g_imcd_m.imcdcrtid_desc, 
       g_imcd_m.imcdcrtdp_desc,g_imcd_m.imcdmodid_desc,g_imcd_m.imcd123_desc,g_imcd_m.imcd125_desc
 
   #檢查是否允許此動作
   IF NOT aimi103_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_imcd_m_mask_o.* =  g_imcd_m.*
   CALL aimi103_imcd_t_mask()
   LET g_imcd_m_mask_n.* =  g_imcd_m.*
   
   
 
   #顯示資料
   CALL aimi103_show()
   
   WHILE TRUE
      LET g_imcd_m.imcd111 = g_imcd111_t
 
      
      #寫入修改者/修改日期資訊
      LET g_imcd_m.imcdmodid = g_user 
LET g_imcd_m.imcdmoddt = cl_get_current()
LET g_imcd_m.imcdmodid_desc = cl_get_username(g_imcd_m.imcdmodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
      #end add-point
 
      #資料輸入
      CALL aimi103_input("u")     
 
      #add-point:modify段修改後 name="modify.after_input"
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         LET g_imcd_m.* = g_imcd_m_t.*
         CALL aimi103_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE imcd_t SET (imcdmodid,imcdmoddt) = (g_imcd_m.imcdmodid,g_imcd_m.imcdmoddt)
       WHERE imcdent = g_enterprise AND imcdsite = g_site AND imcd111 = g_imcd111_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL aimi103_set_act_visible()
   CALL aimi103_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " imcdent = " ||g_enterprise|| " AND imcdsite = '" ||g_site|| "' AND",
                      " imcd111 = '", g_imcd_m.imcd111, "' "
 
   #填到對應位置
   CALL aimi103_browser_fill(g_wc,"")
 
   CLOSE aimi103_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aimi103_msgcentre_notify('modify')
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   

{</section>}

{<section id="aimi103.input" >}
