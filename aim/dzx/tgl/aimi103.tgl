               " LEFT JOIN imaal_t t9 ON t9.imaalent="||g_enterprise||" AND t9.imaal001=t0.imcd125 AND t9.imaal002='"||g_dlang||"' ",
 
               " WHERE t0.imcdent = " ||g_enterprise|| " AND t0.imcdsite = ? AND t0.imcd111 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
