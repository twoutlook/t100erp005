                      " WHERE imcdent= ? AND imcdsite= ? AND imcd111=? FOR UPDATE"
   #add-point:SQL_define name="main.after_define_sql"
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE aimi103_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.imcd111,t0.imcd112,t0.imcd113,t0.imcd114,t0.imcd115,t0.imcd116,t0.imcd117, 
       t0.imcd118,t0.imcdstus,t0.imcdownid,t0.imcdowndp,t0.imcdcrtid,t0.imcdcrtdp,t0.imcdcrtdt,t0.imcdmodid, 
       t0.imcdmoddt,t0.imcd121,t0.imcd122,t0.imcd123,t0.imcd124,t0.imcd125,t0.imcd126,t0.imcd127,t0.imcd130, 
       t0.imcd128,t1.oocal003 ,t2.oocal003 ,t3.ooag011 ,t4.ooefl003 ,t5.ooag011 ,t6.ooefl003 ,t7.ooag011 , 
