   LET g_forupd_sql = " SELECT imcd111,'','',imcd112,'',imcd113,'',imcd114,imcd115,imcd116,imcd117,imcd118, 
       imcdstus,imcdownid,'',imcdowndp,'',imcdcrtid,'',imcdcrtdp,'',imcdcrtdt,imcdmodid,'',imcdmoddt, 
