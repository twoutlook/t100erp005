   LET g_wc = cl_replace_str(g_wc, g_wc_filter_t, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON imbfdocno,imbf141
                          FROM s_browse[1].b_imbfdocno,s_browse[1].b_imbf141
 
         BEFORE CONSTRUCT
               DISPLAY aimt304_filter_parser('imbfdocno') TO s_browse[1].b_imbfdocno
