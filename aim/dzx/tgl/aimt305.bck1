{<section id="aimt305.fetch" >}
#+ 指定PK後抓取單頭其他資料
PRIVATE FUNCTION aimt305_fetch(p_fl)
   #add-point:fetch段define(客製用) name="fetch.define_customerization"
   {<point name="fetch.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE p_fl       LIKE type_t.chr1
   DEFINE ls_msg     STRING
   #add-point:fetch段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="fetch.define"
   {<point name="fetch.define"/>}
   #end add-point  
   
   #add-point:Function前置處理  name="fetch.pre_function"
   {<point name="fetch.pre_function"/>}
   #end add-point
   
   #根據傳入的條件決定抓取的資料
   CASE p_fl
      WHEN "F" 
         LET g_current_idx = 1
      WHEN "P"
         IF g_current_idx > 1 THEN               
            LET g_current_idx = g_current_idx - 1
         END IF 
      WHEN "N"
         IF g_current_idx < g_header_cnt THEN
            LET g_current_idx =  g_current_idx + 1
         END IF        
      WHEN "L" 
         #LET g_current_idx = g_header_cnt        
         LET g_current_idx = g_browser.getLength()    
      WHEN "/"
         #詢問要指定的筆數
         IF (NOT g_no_ask) THEN      
            CALL cl_getmsg("fetch", g_lang) RETURNING ls_msg
            LET INT_FLAG = 0
 
            PROMPT ls_msg CLIPPED,": " FOR g_jump
               #交談指令共用ACTION
               &include "common_action.4gl"
            END PROMPT
            
            IF INT_FLAG THEN
               LET INT_FLAG = 0
               EXIT CASE  
            END IF           
         END IF
         IF g_jump > 0 THEN
            LET g_current_idx = g_jump
         END IF
         LET g_no_ask = FALSE     
   END CASE
 
   #筆數顯示
   LET g_browser_idx = g_current_idx 
   IF g_browser_cnt > 0 THEN
      DISPLAY g_browser_idx TO FORMONLY.b_index #當下筆數
      DISPLAY g_browser_cnt TO FORMONLY.b_count #總筆數
      DISPLAY g_browser_idx TO FORMONLY.h_index #當下筆數
      DISPLAY g_browser_cnt TO FORMONLY.h_count #總筆數
   ELSE
      DISPLAY '' TO FORMONLY.b_index #當下筆數
      DISPLAY '' TO FORMONLY.b_count #總筆數
      DISPLAY '' TO FORMONLY.h_index #當下筆數
      DISPLAY '' TO FORMONLY.h_count #總筆數
   END IF
   
   
   
   #避免超出browser資料筆數上限
   IF g_current_idx > g_browser.getLength() THEN
      LET g_browser_idx = g_browser.getLength()
      LET g_current_idx = g_browser.getLength() 
   END IF
   
   # 設定browse索引
   CALL g_curr_diag.setCurrentRow("s_browse", g_current_idx)
   CALL cl_navigator_setting(g_browser_idx, g_browser_cnt) 
 
   #代表沒有資料, 無需做後續資料撈取之動作
   IF g_current_idx = 0 THEN
      RETURN
   END IF
 
   #根據選定的筆數給予key欄位值
   LET g_imbe_m.imbedocno = g_browser[g_current_idx].b_imbedocno
 
                       
   #讀取單頭所有欄位資料
   EXECUTE aimt305_master_referesh USING g_site,g_imbe_m.imbedocno INTO g_imbe_m.imbedocno,g_imbe_m.imbe011, 
       g_imbe_m.imbe012,g_imbe_m.imbeownid,g_imbe_m.imbeowndp,g_imbe_m.imbecrtid,g_imbe_m.imbecrtdp, 
       g_imbe_m.imbecrtdt,g_imbe_m.imbemodid,g_imbe_m.imbemoddt,g_imbe_m.imbe013,g_imbe_m.imbe014,g_imbe_m.imbe015, 
       g_imbe_m.imbe023,g_imbe_m.imbe016,g_imbe_m.imbe017,g_imbe_m.imbe018,g_imbe_m.imbe019,g_imbe_m.imbe020, 
       g_imbe_m.imbe021,g_imbe_m.imbe031,g_imbe_m.imbe037,g_imbe_m.imbe032,g_imbe_m.imbe033,g_imbe_m.imbe034, 
       g_imbe_m.imbe035,g_imbe_m.imbe022,g_imbe_m.imbe036,g_imbe_m.imbe041,g_imbe_m.imbe042,g_imbe_m.imbe051, 
       g_imbe_m.imbe052,g_imbe_m.imbe062,g_imbe_m.imbe064,g_imbe_m.imbe077,g_imbe_m.imbe078,g_imbe_m.imbe079, 
       g_imbe_m.imbe080,g_imbe_m.imbe071,g_imbe_m.imbe072,g_imbe_m.imbe073,g_imbe_m.imbe074,g_imbe_m.imbe075, 
       g_imbe_m.imbe081,g_imbe_m.imbe082,g_imbe_m.imbe083,g_imbe_m.imbe084,g_imbe_m.imbe085,g_imbe_m.imbe091, 
       g_imbe_m.imbe092,g_imbe_m.imbe101,g_imbe_m.imbe102,g_imbe_m.imbe103,g_imbe_m.imbe104,g_imbe_m.imbe011_desc, 
       g_imbe_m.imbe012_desc,g_imbe_m.imbeownid_desc,g_imbe_m.imbeowndp_desc,g_imbe_m.imbecrtid_desc, 
       g_imbe_m.imbecrtdp_desc,g_imbe_m.imbemodid_desc,g_imbe_m.imbe016_desc,g_imbe_m.imbe032_desc,g_imbe_m.imbe033_desc, 
       g_imbe_m.imbe035_desc,g_imbe_m.imbe081_desc
   
   #遮罩相關處理
   LET g_imbe_m_mask_o.* =  g_imbe_m.*
   CALL aimt305_imbe_t_mask()
   LET g_imbe_m_mask_n.* =  g_imbe_m.*
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL aimt305_set_act_visible()
   CALL aimt305_set_act_no_visible()
 
   #add-point:fetch段action控制 name="fetch.action_control"
   {<point name="fetch.action_control"/>}
   #end add-point  
   
   
   
   #保存單頭舊值
   LET g_imbe_m_t.* = g_imbe_m.*
   LET g_imbe_m_o.* = g_imbe_m.*
   
   LET g_data_owner = g_imbe_m.imbeownid      
   LET g_data_dept  = g_imbe_m.imbeowndp
   
   #重新顯示
   CALL aimt305_show()
   
 
