{<section id="aimt305.modify" >}
#+ 資料修改
PRIVATE FUNCTION aimt305_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   {<point name="modify.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   {<point name="modify.define"/>}
   #end add-point
   
   #add-point:Function前置處理 name="modify.pre_function"
   {<point name="modify.pre_function"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_imbe_m.imbedocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_imbedocno_t = g_imbe_m.imbedocno
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN aimt305_cl USING g_enterprise, g_site,g_imbe_m.imbedocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aimt305_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CLOSE aimt305_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aimt305_master_referesh USING g_site,g_imbe_m.imbedocno INTO g_imbe_m.imbedocno,g_imbe_m.imbe011, 
       g_imbe_m.imbe012,g_imbe_m.imbeownid,g_imbe_m.imbeowndp,g_imbe_m.imbecrtid,g_imbe_m.imbecrtdp, 
       g_imbe_m.imbecrtdt,g_imbe_m.imbemodid,g_imbe_m.imbemoddt,g_imbe_m.imbe013,g_imbe_m.imbe014,g_imbe_m.imbe015, 
       g_imbe_m.imbe023,g_imbe_m.imbe016,g_imbe_m.imbe017,g_imbe_m.imbe018,g_imbe_m.imbe019,g_imbe_m.imbe020, 
       g_imbe_m.imbe021,g_imbe_m.imbe031,g_imbe_m.imbe037,g_imbe_m.imbe032,g_imbe_m.imbe033,g_imbe_m.imbe034, 
       g_imbe_m.imbe035,g_imbe_m.imbe022,g_imbe_m.imbe036,g_imbe_m.imbe041,g_imbe_m.imbe042,g_imbe_m.imbe051, 
       g_imbe_m.imbe052,g_imbe_m.imbe062,g_imbe_m.imbe064,g_imbe_m.imbe077,g_imbe_m.imbe078,g_imbe_m.imbe079, 
       g_imbe_m.imbe080,g_imbe_m.imbe071,g_imbe_m.imbe072,g_imbe_m.imbe073,g_imbe_m.imbe074,g_imbe_m.imbe075, 
       g_imbe_m.imbe081,g_imbe_m.imbe082,g_imbe_m.imbe083,g_imbe_m.imbe084,g_imbe_m.imbe085,g_imbe_m.imbe091, 
       g_imbe_m.imbe092,g_imbe_m.imbe101,g_imbe_m.imbe102,g_imbe_m.imbe103,g_imbe_m.imbe104,g_imbe_m.imbe011_desc, 
       g_imbe_m.imbe012_desc,g_imbe_m.imbeownid_desc,g_imbe_m.imbeowndp_desc,g_imbe_m.imbecrtid_desc, 
       g_imbe_m.imbecrtdp_desc,g_imbe_m.imbemodid_desc,g_imbe_m.imbe016_desc,g_imbe_m.imbe032_desc,g_imbe_m.imbe033_desc, 
       g_imbe_m.imbe035_desc,g_imbe_m.imbe081_desc
 
   #檢查是否允許此動作
   IF NOT aimt305_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_imbe_m_mask_o.* =  g_imbe_m.*
   CALL aimt305_imbe_t_mask()
   LET g_imbe_m_mask_n.* =  g_imbe_m.*
   
   
 
   #顯示資料
   CALL aimt305_show()
   
   WHILE TRUE
      LET g_imbe_m.imbedocno = g_imbedocno_t
 
      
      #寫入修改者/修改日期資訊
      LET g_imbe_m.imbemodid = g_user 
LET g_imbe_m.imbemoddt = cl_get_current()
LET g_imbe_m.imbemodid_desc = cl_get_username(g_imbe_m.imbemodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
      #end add-point
 
      #資料輸入
      CALL aimt305_input("u")     
 
      #add-point:modify段修改後 name="modify.after_input"
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         LET g_imbe_m.* = g_imbe_m_t.*
         CALL aimt305_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE imbe_t SET (imbemodid,imbemoddt) = (g_imbe_m.imbemodid,g_imbe_m.imbemoddt)
       WHERE imbeent = g_enterprise AND imbesite = g_site AND imbedocno = g_imbedocno_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL aimt305_set_act_visible()
   CALL aimt305_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " imbeent = " ||g_enterprise|| " AND imbesite = '" ||g_site|| "' AND",
                      " imbedocno = '", g_imbe_m.imbedocno, "' "
 
   #填到對應位置
   CALL aimt305_browser_fill(g_wc,"")
 
   CLOSE aimt305_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aimt305_msgcentre_notify('modify')
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   

