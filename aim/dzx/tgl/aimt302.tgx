   #end add-point
   
   #先確定key值無遺漏
   IF g_imbf_m.imbfdocno IS NULL

   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
    
   LET g_imbfdocno_t = g_imbf_m.imbfdocno

   
   LET g_master_multi_table_t.imbaldocno = g_imbf_m.imbfdocno
LET g_master_multi_table_t.imbal002 = g_imbf_m.imbal002
LET g_master_multi_table_t.imbal003 = g_imbf_m.imbal003
LET g_master_multi_table_t.imbal004 = g_imbf_m.imbal004

 
   OPEN aimt302_cl USING g_enterprise, g_site,g_imbf_m.imbfdocno
   IF SQLCA.SQLCODE THEN    #(ver:49)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aimt302_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE
      LET g_errparam.popup = TRUE 
      CLOSE aimt302_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aimt302_master_referesh USING g_site,g_imbf_m.imbfdocno INTO g_imbf_m.imbfdocno,g_imbf_m.imbf051,g_imbf_m.imbf052,g_imbf_m.imbf053,g_imbf_m.imbf054,g_imbf_m.imbf055,g_imbf_m.imbf057,g_imbf_m.imbf058,g_imbf_m.imbf059,g_imbf_m.imbfownid,g_imbf_m.imbfowndp,g_imbf_m.imbfcrtid,g_imbf_m.imbfcrtdp,g_imbf_m.imbfcrtdt,g_imbf_m.imbfmodid,g_imbf_m.imbfmoddt,g_imbf_m.imbf061,g_imbf_m.imbf062,g_imbf_m.imbf063,g_imbf_m.imbf064,g_imbf_m.imbf071,g_imbf_m.imbf072,g_imbf_m.imbf073,g_imbf_m.imbf074,g_imbf_m.imbf081,g_imbf_m.imbf082,g_imbf_m.imbf083,g_imbf_m.imbf084,g_imbf_m.imbf091,g_imbf_m.imbf092,g_imbf_m.imbf177,g_imbf_m.imbf178,g_imbf_m.imbf179,g_imbf_m.imbf101,g_imbf_m.imbf102,g_imbf_m.imbf094,g_imbf_m.imbf095,g_imbf_m.imbf096,g_imbf_m.imbf051_desc,g_imbf_m.imbf052_desc,g_imbf_m.imbf053_desc,g_imbf_m.imbfownid_desc,g_imbf_m.imbfowndp_desc,g_imbf_m.imbfcrtid_desc,g_imbf_m.imbfcrtdp_desc,g_imbf_m.imbfmodid_desc,g_imbf_m.imbf178_desc
   
   
   #檢查是否允許此動作
   IF NOT aimt302_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_imbf_m_mask_o.* =  g_imbf_m.*
   CALL aimt302_imbf_t_mask()
   LET g_imbf_m_mask_n.* =  g_imbf_m.*
   
   #將最新資料顯示到畫面上
   CALL aimt302_show()
   
   IF cl_ask_delete() THEN
 
      #add-point:單頭刪除前 name="delete.head.b_delete"
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point
 
