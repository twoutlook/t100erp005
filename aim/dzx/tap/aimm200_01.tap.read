               SELECT imeb005 INTO l_imeb005 FROM imeb_t WHERE imebent = g_enterprise
                  AND imeb001=p_imaa005 AND imeb004=g_imak_d[l_ac].imak002
               IF l_imeb005 = '4' THEN
                  FOR l_i = l_ac + 1 TO g_rec_b
