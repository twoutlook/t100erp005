  <point name="function.aimm200_01_imak002_chk" order="2" ver="4" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: imak002 對imak003的控管

################################################################################
PRIVATE FUNCTION aimm200_01_imak002_chk()
   DEFINE l_imeb005    LIKE imeb_t.imeb005
   DEFINE l_imeb009    LIKE imeb_t.imeb009
   
   IF NOT cl_null(g_imak_d[l_ac].imak002) THEN
      SELECT imeb005,imeb009 INTO l_imeb005,l_imeb009 FROM imeb_t 
       WHERE imebent = g_enterprise
         AND imeb001 = g_imaa005
         AND imeb004 = g_imak_d[l_ac].imak002
      IF l_imeb005 = '4' THEN
         CALL cl_set_comp_entry("imck003",FALSE)               
      END IF
      IF cl_null(g_imak_d[l_ac].imak003) THEN
         LET g_imak_d[l_ac].imak003 = l_imeb009
         CALL aimm200_01_imak_desc()
      END IF
   END IF      
END FUNCTION]]>
  </point>
  <point name="dialog.aimm200_01_input" order="3" ver="4" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 被主程式嵌入的單身輸入模式
# Memo...........: 
# Usage..........: CALL aimm200_01_input()
# Input parameter: None
# Return code....: None
# Date & Author..: 2014/09/24 By lixiang
# Modify.........:
################################################################################
DIALOG aimm200_01_input()
   DEFINE l_ac_t          LIKE type_t.num5        #未取消的ARRAY CNT   
   DEFINE l_count         LIKE type_t.num5
   DEFINE l_insert        LIKE type_t.num5
   DEFINE l_cmd           LIKE type_t.chr5
   DEFINE l_lock_sw       LIKE type_t.chr1
   DEFINE l_forupd_sql    STRING
   DEFINE l_n             LIKE type_t.num5                #檢查重複用  
   DEFINE l_cnt           LIKE type_t.num5
   DEFINE l_imak          RECORD LIKE imak_t.*
   DEFINE l_imeb005       LIKE imeb_t.imeb005
   DEFINE l_errno         LIKE gzze_t.gzze001
   DEFINE l_success       LIKE type_t.num5
   DEFINE l_i             LIKE type_t.num5
   DEFINE l_cn            LIKE type_t.num5
   
   INPUT ARRAY g_imak_d FROM s_detail1_aimm200_01.*
          ATTRIBUTE(COUNT = g_d_cnt_m20001,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = FALSE,
                  DELETE ROW = FALSE,
                  APPEND ROW = FALSE)

         BEFORE INPUT
            WHENEVER ERROR CALL cl_err_msg_log
            LET l_forupd_sql = " SELECT imak001,imak002,'',imak003 ",
                               "   FROM imak_t ",
                               "  WHERE imakent = '",g_enterprise,"' ",
                               "    AND imak001 = ? AND imak002 = ? FOR UPDATE"
            LET l_forupd_sql = cl_sql_forupd(l_forupd_sql)
            PREPARE aimm200_01_dialog_b FROM l_forupd_sql
            DECLARE aimm200_01_dialog_bcl CURSOR FOR aimm200_01_dialog_b
            IF g_appoint_idx > 0 THEN
               CALL DIALOG.setCurrentRow("s_detail1_aimm200_01",g_appoint_idx)
            END IF
            SELECT imaa005 INTO g_imaa005 FROM imaa_t WHERE imaaent = g_enterprise AND imaa001 = g_imaa001_d
            
          BEFORE ROW 
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET g_detail_idx = l_ac
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
         
            CALL s_transaction_begin()
   
            LET g_rec_b = g_imak_d.getLength()
            
            IF g_rec_b >= l_ac THEN
               LET l_cmd='u'
			      LET g_imak_d_t.* = g_imak_d[l_ac].*  #BACKUP
			      
			      #判斷是否可以進入產品特徵欄位
               LET l_cn = 0
               FOR l_i = l_ac TO g_imak_d.getLength()
                   SELECT imeb005 INTO l_imeb005 FROM imeb_t
                      WHERE imebent = g_enterprise
                        AND imeb001 = g_imaa005
                        AND imeb003 = '1'
                        AND imeb004 =  g_imak_d[l_i].imak002
                  IF l_imeb005 <> '4' THEN
                     LET l_cn = l_i
                     EXIT FOR
                  END IF
               END FOR
               IF l_cn = 0 THEN
                  EXIT DIALOG
               ELSE
                  LET l_ac = l_cn
               END IF
               IF l_ac > g_rec_b THEN
                  EXIT DIALOG
               ELSE
                  CALL fgl_set_arr_curr(l_ac)
               END IF
               
			      
			      CALL aimm200_01_set_entry_b()
			      CALL aimm200_01_set_no_entry_b()
			   
			      OPEN aimm200_01_dialog_bcl USING g_imaa001_d,g_imak_d[l_ac].imak002
               IF SQLCA.sqlcode THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = SQLCA.sqlcode
                  LET g_errparam.extend = "aimm200_01_dialog_bcl"
                  LET g_errparam.popup = TRUE
                  CALL cl_err()

                  LET l_lock_sw='Y'
               ELSE
                  FETCH aimm200_01_dialog_bcl INTO g_imak_d[l_ac].imak001,g_imak_d[l_ac].imak002,g_imak_d[l_ac].imak002_desc,g_imak_d[l_ac].imak003
                   
                  IF SQLCA.sqlcode THEN
                     LET l_lock_sw = "Y"
                  END IF
				      CALL aimm200_01_imak_desc()
                  CALL cl_show_fld_cont()
               END IF
               
            ELSE
               LET l_cmd='a'
            END IF
        
         BEFORE INSERT
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_imak_d[l_ac].* TO NULL          
            
            LET g_imak_d[l_ac].imak001 = g_imaa001_d
            
            LET g_imak_d_t.* = g_imak_d[l_ac].*     #新輸入資料

            CALL cl_show_fld_cont()
            
            
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 9001
               LET g_errparam.extend = ''
               LET g_errparam.popup = FALSE
               CALL cl_err()

               LET INT_FLAG = 0
               #CANCEL INSERT
            END IF
               
            LET l_imak.imakent = g_enterprise        
            LET l_imak.imak001 = g_imaa001_d
            LET l_imak.imak002 = g_imak_d[l_ac].imak002
            LET l_imak.imak003 = g_imak_d[l_ac].imak003
         
            SELECT COUNT(*) INTO l_count FROM imak_t 
             WHERE imakent = g_enterprise
               AND imak001 = g_imaa001_d
               AND imak002 = g_imak_d[l_ac].imak002
      
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               INSERT INTO imak_t (imakent,imak001,imak002,imak003)
                  VALUES (g_enterprise,l_imak.imak001,l_imak.imak002,l_imak.imak003)
            ELSE    
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = "std-00006"
               LET g_errparam.extend = 'INSERT'
               LET g_errparam.popup = TRUE
               CALL cl_err()

               INITIALIZE g_imak_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               #CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = SQLCA.sqlcode
               LET g_errparam.extend = "imak_t"
               LET g_errparam.popup = TRUE
               CALL cl_err()
  
               CALL s_transaction_end('N','0')                    
               #CANCEL INSERT
            ELSE
               CALL s_transaction_end('Y','0')
               ERROR 'INSERT O.K'
               LET g_rec_b = g_rec_b + 1
            END IF
              
         BEFORE DELETE                            #是否取消單身
            IF NOT cl_null(g_imaa001_d) AND NOT cl_null(g_imak_d[l_ac].imak002)THEN

               IF cl_ask_del_detail() THEN
                  
                  IF l_lock_sw = "Y" THEN
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code =  -263
                     LET g_errparam.extend = ""
                     LET g_errparam.popup = TRUE
                     CALL cl_err()
                  
                     #CANCEL DELETE
                  END IF
                  
                 DELETE FROM imak_t
                   WHERE imakent = g_enterprise 
                     AND imak001 = g_imaa001_d
                     AND imak002 = g_imak_d[l_ac].imak002
                  
                  IF SQLCA.sqlcode THEN
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code = SQLCA.sqlcode
                     LET g_errparam.extend = "imak_t"
                     LET g_errparam.popup = TRUE
                     CALL cl_err()
                     
                     CALL s_transaction_end('N','0')
                     #CANCEL DELETE   
                  ELSE
                     CALL s_transaction_end('Y','0')
                  END IF 
                  CLOSE aimm200_01_dialog_bcl
                  LET l_count = g_imak_d.getLength()
               END IF
            END IF 
            
        AFTER DELETE
            CALL aimm200_01_b_fill(g_imaa001_d)
            
        ON ROW CHANGE
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 9001
               LET g_errparam.extend = ''
               LET g_errparam.popup = FALSE
               CALL cl_err()

               LET INT_FLAG = 0
               LET g_imak_d[l_ac].* = g_imak_d_t.*
               CLOSE aimm200_01_dialog_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG
            END IF

            IF l_lock_sw = 'Y' THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = -263
               LET g_errparam.extend = g_imak_d[l_ac].imak002
               LET g_errparam.popup = TRUE
               CALL cl_err()

               LET g_imak_d[l_ac].* = g_imak_d_t.*
            ELSE
               UPDATE imak_t SET (imak002,imak003)= (g_imak_d[l_ac].imak002,g_imak_d[l_ac].imak003)
                WHERE imakent = g_enterprise
                  AND imak001 = g_imaa001_d 
                  AND imak002 = g_imak_d_t.imak002

               IF SQLCA.sqlcode THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = SQLCA.sqlcode
                  LET g_errparam.extend = "imak_t"
                  LET g_errparam.popup = TRUE
                  CALL cl_err()

                  LET g_imak_d[l_ac].* = g_imak_d_t.*
                  CALL s_transaction_end('N','0')
               ELSE
                  CALL s_transaction_end('Y','0')
               END IF
            END IF
            
         AFTER ROW
            CLOSE aimm200_01_dialog_bcl
            
         AFTER FIELD imak001
            IF  NOT cl_null(g_imaa001_d) AND NOT cl_null(g_imak_d[l_ac].imak002) THEN 
               IF l_cmd = 'a' OR ( l_cmd = 'u' AND ( l_cmd = 'u' AND (g_imaa001_d != g_imak_d_t.imak001 OR g_imak_d[l_ac].imak002 != g_imak_d_t.imak002))) THEN 
                  IF NOT ap_chk_notDup("","SELECT COUNT(*) FROM imak_t WHERE "||"imakent = '" ||g_enterprise|| "' AND "||"imak001 = '"||g_imaa001_d ||"' AND "|| "imak002 = '"||g_imak_d[l_ac].imak002 ||"'",'std-00004',1) THEN 
                     NEXT FIELD CURRENT
                  END IF
               END IF
            END IF
            
         AFTER FIELD imak002
           CALL aimm200_01_imak_desc()
           IF  NOT cl_null(g_imak_d[l_ac].imak002)  THEN 
               IF l_cmd = 'a' OR ( l_cmd = 'u' AND ( l_cmd = 'u' AND (g_imak_d[l_ac].imak002 != g_imak_d_t.imak002 ))) THEN 
                  IF NOT ap_chk_notDup(g_imak_d[l_ac].imak002,"SELECT COUNT(*) FROM imak_t WHERE "||"imakent = '" ||g_enterprise|| "' AND "||"imak002 = '"||g_imak_d[l_ac].imak002 ||"' AND "|| "imak001 = '"||g_imaa001_d ||"'",'std-00004',1) THEN 
                     LET g_imak_d[l_ac].imak002 = g_imak_d_t.imak002
                     CALL aimm200_01_imak_desc()
                     NEXT FIELD CURRENT
                  END IF
               END IF
            END IF
                
            IF NOT cl_null(g_imak_d[l_ac].imak002) THEN
               IF NOT ap_chk_isExist(g_imak_d[l_ac].imak002,"SELECT COUNT(*) FROM imeb_t WHERE imebent = '" ||g_enterprise|| "' AND imeb001 = '" ||g_imaa005||"' AND imeb003 = '1' AND imeb004 = ? ","aim-00158",1) THEN
                  LET g_imak_d[l_ac].imak002 = g_imak_d_t.imak002
                  CALL aimm200_01_imak_desc()
                  NEXT FIELD imak002
               END IF
               CALL aimm200_01_imak002_chk()

            END IF 
            IF NOT cl_null(g_imak_d[l_ac].imak002) AND NOT cl_null(g_imak_d[l_ac].imak003) THEN
               CALL s_aimi092_chk_eigenvalue(g_imaa005,g_imak_d[l_ac].imak002,g_imak_d[l_ac].imak003) RETURNING l_success,l_errno,g_imak_d[l_ac].imak003
               IF NOT cl_null(l_errno) THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = l_errno
                  LET g_errparam.extend = g_imak_d[l_ac].imak003
                  LET g_errparam.popup = TRUE
                  CALL cl_err()

                  CALL aimm200_01_imak_desc()
                  NEXT FIELD imak002
               END IF
               IF l_cmd = 'a' THEN
                  SELECT imeb009 INTO g_imak_d[l_ac].imak003
                    FROM imeb_t
                   WHERE imebent = g_enterprise
                     AND imeb001 = g_imaa005
                     AND imeb003 = '1'
                     AND imeb004 = g_imak_d[l_ac].imak002
                  DISPLAY BY NAME g_imak_d[l_ac].imak003
               END IF              
            END IF
            CALL aimm200_01_set_entry_b()
            CALL aimm200_01_set_no_entry_b()

            
         AFTER FIELD imak003
            IF NOT cl_null(g_imak_d[l_ac].imak002) AND NOT cl_null(g_imak_d[l_ac].imak003) THEN
               CALL s_aimi092_chk_eigenvalue(g_imaa005,g_imak_d[l_ac].imak002,g_imak_d[l_ac].imak003) RETURNING l_success,l_errno,g_imak_d[l_ac].imak003
               IF NOT cl_null(l_errno) THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = l_errno
                  LET g_errparam.extend = g_imak_d[l_ac].imak003
