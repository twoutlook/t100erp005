                         AND imeb001=p_imaa005 AND imeb004=g_imak_d[l_i].imak002
                      IF l_imeb005 != '4' THEN
                        LET l_ac = l_i
                        LET l_flag = 'N'
                        EXIT DIALOG
                     END IF
                  END FOR
                  LET l_ac = g_rec_b
                  FOR l_i = g_rec_b - 1 TO 1 STEP -1
                      SELECT imeb005 INTO l_imeb005 FROM imeb_t WHERE imebent = g_enterprise
                         AND imeb001=p_imaa005 AND imeb004=g_imak_d[l_i].imak002
                      IF l_imeb005 != '4' THEN
                        LET l_ac = l_i
                        LET l_flag = 'N'
                        EXIT DIALOG
                     END IF
                  END FOR
               END IF 
            END IF                
            
         BEFORE INSERT
            CALL s_transaction_begin()
            LET l_cmd = 'a'
            INITIALIZE g_imak_d_t.* TO NULL
            INITIALIZE g_imak_d[l_ac].* TO NULL 
            
            LET g_imak_d_t.* = g_imak_d[l_ac].*     #新輸入資料
