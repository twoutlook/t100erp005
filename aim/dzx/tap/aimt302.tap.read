   OPEN aimt302_cl USING g_enterprise, g_site,g_imbf_m.imbfdocno
 
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aimt302_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE aimt302_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aimt302_master_referesh USING g_site,g_imbf_m.imbfdocno INTO g_imbf_m.imbfdocno,g_imbf_m.imbf051, 
       g_imbf_m.imbf052,g_imbf_m.imbf053,g_imbf_m.imbf054,g_imbf_m.imbf055,g_imbf_m.imbf057,g_imbf_m.imbf058, 
