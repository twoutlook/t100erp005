  <point name="function.aimt302_chk_ooag" order="2" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[# 員工編號檢查
PRIVATE FUNCTION aimt302_chk_ooag()
DEFINE l_n       LIKE type_t.num5

    LET g_errno = ""
    LET l_n = 0
    SELECT COUNT(*) INTO l_n
      FROM ooag_t
     WHERE ooag001 = g_imbf_m.imbf052
       AND ooagent = g_enterprise
    IF l_n = 0 THEN
       LET g_errno = 'aoo-00074'
    ELSE
       LET l_n =0
       SELECT COUNT(*) INTO l_n
         FROM ooag_t
        WHERE ooag001 = g_imbf_m.imbf052
          AND ooagent = g_enterprise
          AND ooagstus = 'Y'
       IF l_n = 0 THEN
          LET g_errno = 'aoo-00071'
       END IF
    END IF
END FUNCTION]]>
  </point>
  <point name="function.aimt302_chk_imcc" order="3" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[# 庫存分群碼檢查
PRIVATE FUNCTION aimt302_chk_imcc()
DEFINE l_n       LIKE type_t.num5

    LET g_errno = ""
    LET l_n = 0
    SELECT COUNT(*) INTO l_n
      FROM imcc_t
     WHERE imcc051 = g_imbf_m.imbf051
       AND imccsite = g_site
       AND imccent = g_enterprise
    IF l_n = 0 THEN
       IF g_prog = 'aimt302' THEN
          LET g_errno = 'aim-00044'
       ELSE
          LET g_errno = 'aim-00137'
       END IF
    ELSE
       LET l_n =0
       SELECT COUNT(*) INTO l_n
         FROM imcc_t
        WHERE imcc051 = g_imbf_m.imbf051
          AND imccent = g_enterprise
          AND imccsite = g_site
          AND imccstus = 'Y'
       IF l_n = 0 THEN
          IF g_prog = 'aimt302' THEN
             LET g_errno = 'aim-00045'
          ELSE
             LET g_errno = 'aim-00138'
          END IF
       END IF
    END IF
END FUNCTION]]>
  </point>
  <point name="function.aimt302_imaa_desc" order="4" ver="7" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[# 不可錄字段顯示
PRIVATE FUNCTION aimt302_imaa_desc()
DEFINE l_ooag002           LIKE ooag_t.ooag002


   SELECT imba000,imbadocdt,imba001,imba002,imba009,imba003,imba004,imba005,imba006,imba010,imbastus,imba900,imba901
     INTO g_imbf_m.imba000,g_imbf_m.imbadocdt,g_imbf_m.imba001,g_imbf_m.imba002,g_imbf_m.imba009,g_imbf_m.imba003,
          g_imbf_m.imba004,g_imbf_m.imba005,g_imbf_m.imba006,
          g_imbf_m.imba010,g_imbf_m.s1,g_imbf_m.imba900,g_imbf_m.imba901
     FROM imba_t
    WHERE imbadocno = g_imbf_m.imbfdocno
      AND imbaent = g_enterprise
   DISPLAY BY NAME g_imbf_m.imba000,g_imbf_m.imbadocdt,g_imbf_m.imba001,g_imbf_m.imba002,g_imbf_m.imba009,g_imbf_m.imba003,g_imbf_m.imba004,
                   g_imbf_m.imba005,g_imbf_m.imba006,g_imbf_m.imba010,g_imbf_m.s1,g_imbf_m.imba900,g_imbf_m.imba901

   SELECT imbal002,imbal003,imbal004
     INTO g_imbf_m.imbal002,g_imbf_m.imbal003,g_imbf_m.imbal004
     FROM imbal_t
    WHERE imbalent = g_enterprise
      AND imbaldocno = g_imbf_m.imbfdocno
      AND imbal001 = g_dlang
   DISPLAY BY NAME g_imbf_m.imbal002,g_imbf_m.imbal003,g_imbf_m.imbal004

   SELECT rtaxl003 INTO g_imbf_m.imba009_desc1
     FROM rtaxl_t
    WHERE rtaxl001 = g_imbf_m.imba009
      AND rtaxl002 = g_dlang
      AND rtaxlent = g_enterprise
   DISPLAY BY NAME g_imbf_m.imba009_desc1

   
   SELECT oocql004  INTO g_imbf_m.imba003_desc
     FROM oocql_t
    WHERE oocql001 = '200'
      AND oocql002 = g_imbf_m.imba003
      AND oocql003 = g_dlang
      AND oocqlent = g_enterprise
   DISPLAY BY NAME g_imbf_m.imba003_desc
   
   LET g_imbf_m.imba005_desc = ''
   SELECT imeal003  INTO g_imbf_m.imba005_desc
     FROM imeal_t
    WHERE imeal001 = g_imbf_m.imba005
      AND imeal002 = g_dlang
      AND imealent = g_enterprise
   DISPLAY BY NAME g_imbf_m.imba005_desc

   SELECT oocal003  INTO g_imbf_m.imba006_desc
     FROM oocal_t
    WHERE oocal001 = g_imbf_m.imba006
      AND oocal002 = g_dlang
      AND oocalent = g_enterprise
   DISPLAY BY NAME g_imbf_m.imba006_desc

   SELECT oocql004  INTO g_imbf_m.imba010_desc
     FROM oocql_t
    WHERE oocql001 = '210'
      AND oocql002 = g_imbf_m.imba010
      AND oocql003 = g_dlang
      AND oocqlent = g_enterprise
   DISPLAY BY NAME g_imbf_m.imba010_desc
   
   INITIALIZE g_ref_fields TO NULL
   SELECT ooag002 INTO l_ooag002
     FROM ooag_t
    WHERE ooagent = g_enterprise
      AND ooag001 = g_imbf_m.imba900
   LET g_ref_fields[1] = l_ooag002
   CALL ap_ref_array2(g_ref_fields,"SELECT oofa011 FROM oofa_t WHERE oofaent='"||g_enterprise||"' AND oofa002='2' AND oofa001=? ","") RETURNING g_rtn_fields
   LET g_imbf_m.imba900_desc = g_rtn_fields[1]
   DISPLAY BY NAME g_imbf_m.imba900_desc

   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_imbf_m.imba901
   CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_imbf_m.imba901_desc = g_rtn_fields[1]
   DISPLAY BY NAME g_imbf_m.imba901_desc
END FUNCTION]]>
  </point>
  <point name="function.aimt302_desc" order="5" ver="4" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#參考欄位賦值
PRIVATE FUNCTION aimt302_desc()
DEFINE l_flag          LIKE type_t.num5          #标识符，TRUE/FALSE
DEFINE l_flag1         LIKE type_t.num5          #标识符，TRUE/FALSE
DEFINE l_ooba002       LIKE ooba_t.ooba002       #单据别


   LET l_flag = TRUE
   LET l_flag1 = TRUE
   LET l_ooba002 = NULL
   LET g_imbf_m.oobxl003 = ""
   IF NOT cl_null(g_imbf_m.imbfdocno) THEN
      CALL s_aooi200_get_slip(g_imbf_m.imbfdocno) RETURNING l_flag1,l_ooba002
      IF l_flag1 THEN
         IF NOT cl_null(l_ooba002) THEN
            SELECT oobxl003 INTO g_imbf_m.oobxl003
              FROM oobxl_t
             WHERE oobxl001 = l_ooba002
               AND oobxl002 = g_dlang
               AND oobxlent = g_enterprise
         ELSE
            LET g_imbf_m.oobxl003 = ""
         END IF
      END IF
   ELSE
      LET g_imbf_m.oobxl003 = ""
   END IF
   DISPLAY BY NAME g_imbf_m.oobxl003
   
   IF cl_null(g_imbf_m.imbf051) THEN
      LET g_imbf_m.imbf051_desc = ""
   ELSE
      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_imbf_m.imbf051
      CALL ap_ref_array2(g_ref_fields,"SELECT oocql004 FROM oocql_t WHERE oocqlent='"||g_enterprise||"' AND oocql001='201' AND oocql002 = ? AND oocql003='"||g_dlang||"'","") RETURNING g_rtn_fields
      LET g_imbf_m.imbf051_desc =  g_rtn_fields[1]  
   END IF
   DISPLAY BY NAME g_imbf_m.imbf051_desc
            
   IF cl_null(g_imbf_m.imbf052) THEN
      LET g_imbf_m.imbf052_desc = ""
   ELSE         
      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_imbf_m.imbf052
      CALL ap_ref_array2(g_ref_fields,"SELECT ooag011 FROM ooag_t WHERE ooagent='"||g_enterprise||"' AND ooag001=? ","") RETURNING g_rtn_fields
      LET g_imbf_m.imbf052_desc =  g_rtn_fields[1]
   END IF
   DISPLAY BY NAME g_imbf_m.imbf052_desc
   
   IF cl_null(g_imbf_m.imbf053) THEN
      LET g_imbf_m.imbf053_desc = ""
   ELSE   
      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_imbf_m.imbf053
      CALL ap_ref_array2(g_ref_fields,"SELECT oocal003 FROM oocal_t WHERE oocalent='"||g_enterprise||"' AND oocal001=? AND oocal002='"||g_dlang||"'","") RETURNING g_rtn_fields
      LET g_imbf_m.imbf053_desc =  g_rtn_fields[1] 
   END IF   
   DISPLAY BY NAME g_imbf_m.imbf053_desc
   
   IF cl_null(g_imbf_m.imbf091) THEN
      LET g_imbf_m.imbf091_desc = ""
   ELSE   
      CALL s_desc_get_stock_desc(g_site1,g_imbf_m.imbf091) RETURNING g_imbf_m.imbf091_desc
   END IF   
   DISPLAY BY NAME g_imbf_m.imbf091_desc
      
   IF cl_null(g_imbf_m.imbf092) THEN
      LET g_imbf_m.imbf092_desc = ""
   ELSE 
      IF cl_null(g_imbf_m.imbf091) THEN
         INITIALIZE g_ref_fields TO NULL 
