   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE aimt302_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE t0.imbfdocno,t0.imbf051,t0.imbf052,t0.imbf053,t0.imbf054,t0.imbf055,t0.imbf057, 
       t0.imbf058,t0.imbf059,t0.imbfownid,t0.imbfowndp,t0.imbfcrtid,t0.imbfcrtdp,t0.imbfcrtdt,t0.imbfmodid, 
       t0.imbfmoddt,t0.imbf061,t0.imbf062,t0.imbf063,t0.imbf064,t0.imbf071,t0.imbf072,t0.imbf073,t0.imbf074, 
       t0.imbf081,t0.imbf082,t0.imbf083,t0.imbf084,t0.imbf091,t0.imbf092,t0.imbf093,t0.imbf094,t0.imbf095, 
       t0.imbf101,t0.imbf102,t0.imbf096,t1.oocql004 ,t2.ooag011 ,t3.oocal003 ,t4.ooag011 ,t5.ooefl003 , 
       t6.ooag011 ,t7.ooefl003 ,t8.ooag011",
               " FROM imbf_t t0",
                              " LEFT JOIN oocql_t t1 ON t1.oocqlent='"||g_enterprise||"' AND t1.oocql001='201' AND t1.oocql002=t0.imbf051 AND t1.oocql003='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t2 ON t2.ooagent='"||g_enterprise||"' AND t2.ooag001=t0.imbf052  ",
               " LEFT JOIN oocal_t t3 ON t3.oocalent='"||g_enterprise||"' AND t3.oocal001=t0.imbf053 AND t3.oocal002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t4 ON t4.ooagent='"||g_enterprise||"' AND t4.ooag001=t0.imbfownid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=t0.imbfowndp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent='"||g_enterprise||"' AND t6.ooag001=t0.imbfcrtid  ",
