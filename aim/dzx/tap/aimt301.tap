   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON imbfdocno,imbf012,imbf013,imbf014,imbf156
                          FROM s_browse[1].b_imbfdocno,s_browse[1].b_imbf012,s_browse[1].b_imbf013,s_browse[1].b_imbf014, 
                              s_browse[1].b_imbf156
 
