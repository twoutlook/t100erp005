               #END add-point
               
            END IF
 
 
 
         #應用 a43 樣板自動產生(Version:2)
         ON ACTION about_file
            LET g_action_choice="about_file"
            IF cl_auth_chk_act("about_file") THEN
               
               #add-point:ON ACTION about_file
               {<point name="menu2.about_file" />}
