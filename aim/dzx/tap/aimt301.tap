            IF cl_auth_chk_act("open_pgjd") THEN
               
               #add-point:ON ACTION open_pgjd
               {<point name="menu2.open_pgjd" />}
               #END add-point
               
            END IF
 
 
 
         #應用 a43 樣板自動產生(Version:2)
         ON ACTION open_xsjd
