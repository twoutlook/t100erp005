   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT imbfdocno,'','','','','','','','','','','','','','','','','','','','', 
       '','','','',imbf012,imbf013,imbf014,imbf156,imbf015,'',imbf016,'',imbf017,'',imbf018,imbfownid, 
       '',imbfowndp,'',imbfcrtid,'',imbfcrtdp,'',imbfcrtdt,imbfmodid,'',imbfmoddt,imbf021,imbf022,imbf023, 
       imbf024,imbf025,imbf026,imbf027,imbf031,imbf032,imbf033,imbf034,imbf035,'',imbf177,imbf178,'', 
       imbf179", 
                      " FROM imbf_t",
                      " WHERE imbfent= ? AND imbfsite= ? AND imbfdocno=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE aimt301_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE t0.imbfdocno,t0.imbf012,t0.imbf013,t0.imbf014,t0.imbf156,t0.imbf015,t0.imbf016, 
       t0.imbf017,t0.imbf018,t0.imbfownid,t0.imbfowndp,t0.imbfcrtid,t0.imbfcrtdp,t0.imbfcrtdt,t0.imbfmodid, 
       t0.imbfmoddt,t0.imbf021,t0.imbf022,t0.imbf023,t0.imbf024,t0.imbf025,t0.imbf026,t0.imbf027,t0.imbf031, 
       t0.imbf032,t0.imbf033,t0.imbf034,t0.imbf035,t0.imbf177,t0.imbf178,t0.imbf179,t8.oocal003 ,t9.oocql004 , 
       t10.oodbl004 ,t11.ooag011 ,t12.ooefl003 ,t13.ooag011 ,t14.ooefl003 ,t15.ooag011 ,t16.imaal003 , 
       t17.oofgl004",
               " FROM imbf_t t0",
                              " LEFT JOIN oocal_t t8 ON t8.oocalent='"||g_enterprise||"' AND t8.oocal001=t0.imbf015 AND t8.oocal002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t9 ON t9.oocqlent='"||g_enterprise||"' AND t9.oocql001='210' AND t9.oocql002=t0.imbf016 AND t9.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oodbl_t t10 ON t10.oodblent='"||g_enterprise||"' AND t10.oodbl002=t0.imbf017 AND t10.oodbl003='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t11 ON t11.ooagent='"||g_enterprise||"' AND t11.ooag001=t0.imbfownid  ",
               " LEFT JOIN ooefl_t t12 ON t12.ooeflent='"||g_enterprise||"' AND t12.ooefl001=t0.imbfowndp AND t12.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t13 ON t13.ooagent='"||g_enterprise||"' AND t13.ooag001=t0.imbfcrtid  ",
               " LEFT JOIN ooefl_t t14 ON t14.ooeflent='"||g_enterprise||"' AND t14.ooefl001=t0.imbfcrtdp AND t14.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t15 ON t15.ooagent='"||g_enterprise||"' AND t15.ooag001=t0.imbfmodid  ",
               " LEFT JOIN imaal_t t16 ON t16.imaalent='"||g_enterprise||"' AND t16.imaal001=t0.imbf035 AND t16.imaal002='"||g_dlang||"' ",
               " LEFT JOIN oofgl_t t17 ON t17.oofglent='"||g_enterprise||"' AND t17.oofgl001=' ' AND t17.oofgl002=t0.imbf178 AND t17.oofgl003='"||g_dlang||"' ",
 
               " WHERE t0.imbfent = '" ||g_enterprise|| "' AND t0.imbfsite = ? AND t0.imbfdocno = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE aimt301_master_referesh FROM g_sql
 
    
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_aimt301 WITH FORM cl_ap_formpath("aim",g_code)
   
      #瀏覽頁簽資料初始化
