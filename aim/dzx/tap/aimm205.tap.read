  <point name="function.aimm205_imae041_chk" order="15" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#+
PRIVATE FUNCTION aimm205_imae041_chk(p_imae041)
DEFINE p_imae041      LIKE imae_t.imae041
DEFINE r_success      LIKE type_t.num5

       LET r_success = TRUE
       IF NOT cl_null(p_imae041) THEN
          #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
          INITIALIZE g_chkparam.* TO NULL
 
          #設定g_chkparam.*的參數
          IF g_site = 'ALL' THEN
             LET g_chkparam.where = " inaasite = '",g_site_t,"' "
          ELSE
             LET g_chkparam.where = " inaasite = '",g_site,"' "
          END IF
          LET g_chkparam.arg1 = p_imae041
          
          #呼叫檢查存在並帶值的library
          IF NOT cl_chk_exist("v_inaa001_3") THEN
             LET r_success = FALSE
             RETURN r_success
          END IF
          
       END IF
       RETURN r_success

       
END FUNCTION]]>
  </point>
  <point name="function.aimm205_imae041_ref" order="16" ver="4" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
