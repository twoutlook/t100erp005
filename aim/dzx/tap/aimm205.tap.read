      LET r_imaa003 = NULL
      LET r_imaa004 = NULL
      LET r_imaa005 = NULL
      LET r_imaa006 = NULL
      LET r_imaa010 = NULL
      LET r_imaastus = NULL
      SELECT imaa002,imaa009,imaa003,imaa004,imaa005,imaa006,imaa010,imaastus INTO r_imaa002,r_imaa009,r_imaa003,r_imaa004,r_imaa005,r_imaa006,r_imaa010,r_imaastus
