   CLEAR FORM #清畫面欄位內容
   INITIALIZE g_imcd_m.* LIKE imcd_t.*             #DEFAULT 設定
   LET g_imcd111_t = NULL
 
   
   #add-point:insert段before
   {<point name="insert.before"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   
   WHILE TRUE
      
      #公用欄位給值
      #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_imcd_m.imcdownid = g_user
      LET g_imcd_m.imcdowndp = g_dept
      LET g_imcd_m.imcdcrtid = g_user
      LET g_imcd_m.imcdcrtdp = g_dept 
      LET g_imcd_m.imcdcrtdt = cl_get_current()
      LET g_imcd_m.imcdmodid = g_user
      LET g_imcd_m.imcdmoddt = cl_get_current()
      LET g_imcd_m.imcdstus = 'Y'
 
 
 
      #append欄位給值
      
     
      #一般欄位給值
            LET g_imcd_m.imcd114 = "0"
      LET g_imcd_m.imcd115 = "0"
      LET g_imcd_m.imcd116 = "1"
      LET g_imcd_m.imcd117 = "0"
      LET g_imcd_m.imcd118 = "0"
      LET g_imcd_m.imcdstus = "Y"
