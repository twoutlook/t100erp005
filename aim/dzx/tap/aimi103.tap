      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:單頭複製後
   {<point name="reproduce.head.a_insert"/>}
   #end add-point
   
   CALL s_transaction_end('Y','0')
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL aimi103_set_act_visible()
   CALL aimi103_set_act_no_visible()
 
   #將新增的資料併入搜尋條件中
   LET g_state = "insert"
   
   LET g_imcd111_t = g_imcd_m.imcd111
 
   
   #組合新增資料的條件
   LET g_add_browse = " imcdent = '" ||g_enterprise|| "' AND imcdsite = '" ||g_site|| "' AND",
                      " imcd111 = '", g_imcd_m.imcd111, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL aimi103_browser_fill("","")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   #add-point:完成複製段落後
   {<point name="reproduce.after_reproduce" />}
   #end add-point
                 
   #功能已完成,通報訊息中心
   CALL aimi103_msgcentre_notify('reproduce')
                 
END FUNCTION
