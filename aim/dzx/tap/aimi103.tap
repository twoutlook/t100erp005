      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
   
   #備份key值
   LET g_imcd111_t = g_imcd_m.imcd111
 
   
   #清空key值
   LET g_imcd_m.imcd111 = ""
 
    
   CALL aimi103_set_entry("a")
   CALL aimi103_set_no_entry("a")
   
   #公用欄位給予預設值
   #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_imcd_m.imcdownid = g_user
      LET g_imcd_m.imcdowndp = g_dept
      LET g_imcd_m.imcdcrtid = g_user
      LET g_imcd_m.imcdcrtdp = g_dept 
      LET g_imcd_m.imcdcrtdt = cl_get_current()
      LET g_imcd_m.imcdmodid = g_user
      LET g_imcd_m.imcdmoddt = cl_get_current()
      LET g_imcd_m.imcdstus = 'Y'
 
 
   
   CALL s_transaction_begin()
   
   #add-point:複製輸入前
   {<point name="reproduce.head.b_input"/>}
   #end add-point
   
   #顯示狀態(stus)圖片
