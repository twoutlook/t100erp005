    <![CDATA[#+ 銷售單位檢查
PRIVATE FUNCTION aimi103_imcd112_imcd113(p_ooca001)
DEFINE p_ooca001    LIKE ooca_t.ooca001
   IF NOT cl_null(g_imcd_m.imcd112) OR NOT cl_null(g_imcd_m.imcd113) THEN
      IF NOT ap_chk_isExist(p_ooca001,"SELECT COUNT(*) FROM ooca_t WHERE "||"oocaent = '" ||g_enterprise|| "' AND "||"ooca001 = ?  ",'aim-00004',0) THEN
         RETURN FALSE
      END IF
      IF NOT ap_chk_isExist(p_ooca001,"SELECT COUNT(*) FROM ooca_t WHERE "||"oocaent = '" ||g_enterprise|| "' AND "||"ooca001 = ? AND oocastus = 'Y'",'aim-00005',0) THEN 
         RETURN FALSE
      END IF
   END IF
   RETURN TRUE
END FUNCTION]]>
