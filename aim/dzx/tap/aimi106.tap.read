  <point name="function.aimi106_imcg117_item" order="8" ver="2" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 根據傳入的檢驗標準來動態變更可使用的檢驗等級
# Memo...........:
# Usage..........: CALL aimi106_imcg117_item(p_imcg116)
#                  RETURNING 无
# Input parameter: p_imcg116   檢驗標準1,2,3,4   5用于查询时候
# Date & Author..: 2013/10/18 By xuxz
# Modify.........:
################################################################################
PRIVATE FUNCTION aimi106_imcg117_item(p_imcg116)
   DEFINE p_imcg116 LIKE imcg_t.imcg116
   DEFINE l_cnt     LIKE type_t.num5
   DEFINE l_sql     STRING
   DEFINE ls_gzcb002     LIKE gzcb_t.gzcb002      #系統分類值
   DEFINE ls_gzcbl004    LIKE gzcbl_t.gzcbl004    #說明

   DEFINE ls_array DYNAMIC ARRAY OF RECORD
             value       STRING,
             label_tag   STRING,
             label       STRING
                     END RECORD
                     
   IF NOT cl_null(p_imcg116) THEN
      IF p_imcg116 = '1' THEN
         CALL cl_set_combo_scc_part("imcg117",'5053','1,2,3') 
      END IF 
      IF p_imcg116 = '2' THEN
         CALL cl_set_combo_scc_part("imcg117",'5053','1,2,3,4')
      END IF 
      IF p_imcg116 = '3'  OR p_imcg116 = '4' THEN
         CALL cl_set_combo_scc_part("imcg117",'5053','1,2,3,4,5,6,7')
      END IF 
      IF p_imcg116 = '5' THEN
         LET l_sql = " SELECT gzcb002,gzcbl004 FROM gzcb_t LEFT OUTER JOIN gzcbl_t ",
                     " ON gzcb001 = gzcbl001 AND gzcb002 = gzcbl002 ",
                     " AND gzcbl003 = '",g_dlang,"'",
                     " WHERE gzcb001 IN ('5053') "
         PREPARE imcg117_item_pre FROM l_sql
         DECLARE imcg117_item_cs CURSOR FOR imcg117_item_pre
         LET l_cnt = 1
         
         FOREACH imcg117_item_cs INTO ls_gzcb002,ls_gzcbl004
            LET ls_array[l_cnt].value = ls_gzcb002 CLIPPED
            LET ls_array[l_cnt].label = ls_gzcbl004 CLIPPED
            LET ls_array[l_cnt].label_tag = ls_gzcb002 CLIPPED
            LET l_cnt = l_cnt +1
         END FOREACH
         CALL cl_set_combo_detail("imcg117",ls_array)
      END IF 
   END IF 
END FUNCTION]]>
  </point>
