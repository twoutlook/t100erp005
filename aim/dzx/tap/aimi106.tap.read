  <point name="function.aimi106_imcg111_chk" order="1" ver="2" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[# #檢查imcg111欄位key值是否重複
# #檢查imcg111是否存在于ACC205中且有效
PRIVATE FUNCTION aimi106_imcg111_chk(p_imcg111,p_imcg111_t,p_cmd)
DEFINE p_imcg111      LIKE imcg_t.imcg111
   DEFINE p_imcg111_t    LIKE imcg_t.imcg111
   DEFINE p_cmd          LIKE type_t.chr1
   DEFINE r_success      LIKE type_t.num5
   
#初始化返回值
   LET r_success = TRUE

#判斷入值是否為空
   IF NOT cl_null(p_imcg111) THEN
   
#邏輯處理
      DISPLAY ' ' TO imcg111_desc
   #判斷入的p_cmd值，如果為空給a
      IF cl_null(p_cmd) THEN LET p_cmd = 'a' END IF 
   
   #判斷key是否重複,重複就報錯：std-00004，資料重複性檢查未通過
      IF NOT cl_null(p_imcg111) THEN 
         IF p_cmd = 'a' OR ( p_cmd = 'u' AND ( p_cmd = 'u' AND (p_imcg111 != p_imcg111_t ))) THEN 
            IF NOT ap_chk_notDup(p_imcg111,"SELECT COUNT(*) FROM imcg_t WHERE "||"imcgent = '" 
                                ||g_enterprise|| "' AND imcgsite = '" ||g_site|| "' AND "
                                ||"imcg111 = '"||p_imcg111 ||"'",'std-00004',0) THEN 
               LET r_success = FALSE
            END IF
         END IF
      END IF
   
   #判斷是否存在oocq_t中，且否有效的資料v_oocq002_205
      IF r_success = TRUE THEN
#         #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
#         INITIALIZE g_chkparam.* TO NULL
#
#         #設定g_chkparam.*的參數
#         LET g_chkparam.arg1 = p_imcg111
#         #呼叫檢查存在並帶值的library
#         IF NOT cl_chk_exist("v_ooal002_3") THEN
#            LET r_success = FALSE
#         END IF
         
         CALL s_azzi650_chk_exist('205',p_imcg111)
              RETURNING r_success
      END IF 
   END IF 
   
#返回值
   RETURN r_success
END FUNCTION]]>
  </point>
  <point name="function.aimi106_chk_num" order="2" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[# #判斷輸入值是否不小於0
# #報錯：輸入值不可以小於零
PRIVATE FUNCTION aimi106_chk_num(p_num)
DEFINE p_num     LIKE imcg_t.imcg118
   DEFINE r_success LIKE type_t.num5

   LET r_success = TRUE
   IF NOT cl_null(p_num) THEN
      IF p_num < 0 THEN
         #報錯：輸入值不可以小於零
         CALL cl_err(p_num,'aim-00009',0)
         LET r_success = FALSE
      END IF 
   END IF 
   
   RETURN r_success
END FUNCTION]]>
  </point>
  <point name="function.aimi106_imcg112_ref" order="3" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[# #imcg112欄位帶出參考欄位imcg112_desc的值
#
PRIVATE FUNCTION aimi106_imcg112_ref(p_imcg112)
DEFINE p_imcg112      LIKE imcg_t.imcg112
   DEFINE r_imcg112_desc LIKE oofa_t.oofa011
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = p_imcg112
   CALL ap_ref_array2(g_ref_fields,"SELECT oofa011 FROM ooag_t LEFT OUTER JOIN "||
      "oofa_t ON oofaent = ooagent AND oofa001 = ooag002 WHERE ooagent='"
      ||g_enterprise||"' AND  ooag001 = ? ","") 
      RETURNING g_rtn_fields
   LET r_imcg112_desc = g_rtn_fields[1]
   RETURN r_imcg112_desc
END FUNCTION]]>
