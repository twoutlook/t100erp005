  <section id="aimi106.modify" ver="4" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aimi106_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point
   
   IF g_imcg_m.imcg111 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF 
   
   EXECUTE aimi106_master_referesh USING g_imcg_m.imcg111 INTO g_imcg_m.imcg111,g_imcg_m.imcg112,g_imcg_m.imcg113, 
       g_imcg_m.imcg114,g_imcg_m.imcg115,g_imcg_m.imcg116,g_imcg_m.imcg117,g_imcg_m.imcg120,g_imcg_m.imcgstus, 
       g_imcg_m.imcgownid,g_imcg_m.imcgowndp,g_imcg_m.imcgcrtid,g_imcg_m.imcgcrtdp,g_imcg_m.imcgcrtdt, 
       g_imcg_m.imcgmodid,g_imcg_m.imcgmoddt,g_imcg_m.imcg118,g_imcg_m.imcg119,g_imcg_m.imcg111_desc, 
       g_imcg_m.imcg112_desc,g_imcg_m.imcg113_desc,g_imcg_m.imcgownid_desc,g_imcg_m.imcgowndp_desc,g_imcg_m.imcgcrtid_desc, 
       g_imcg_m.imcgcrtdp_desc,g_imcg_m.imcgmodid_desc
 
   ERROR ""
  
   LET g_imcg111_t = g_imcg_m.imcg111
 
   
   CALL s_transaction_begin()
   
   OPEN aimi106_cl USING g_enterprise, g_site,g_imcg_m.imcg111
   IF STATUS THEN
      CALL cl_err("OPEN aimi106_cl:", STATUS, 1)
      CLOSE aimi106_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aimi106_master_referesh USING g_imcg_m.imcg111 INTO g_imcg_m.imcg111,g_imcg_m.imcg112,g_imcg_m.imcg113, 
       g_imcg_m.imcg114,g_imcg_m.imcg115,g_imcg_m.imcg116,g_imcg_m.imcg117,g_imcg_m.imcg120,g_imcg_m.imcgstus, 
       g_imcg_m.imcgownid,g_imcg_m.imcgowndp,g_imcg_m.imcgcrtid,g_imcg_m.imcgcrtdp,g_imcg_m.imcgcrtdt, 
       g_imcg_m.imcgmodid,g_imcg_m.imcgmoddt,g_imcg_m.imcg118,g_imcg_m.imcg119,g_imcg_m.imcg111_desc, 
       g_imcg_m.imcg112_desc,g_imcg_m.imcg113_desc,g_imcg_m.imcgownid_desc,g_imcg_m.imcgowndp_desc,g_imcg_m.imcgcrtid_desc, 
       g_imcg_m.imcgcrtdp_desc,g_imcg_m.imcgmodid_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err("imcg_t",SQLCA.sqlcode,0)
      CLOSE aimi106_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
 
   CALL aimi106_show()
   
   WHILE TRUE
      LET g_imcg_m.imcg111 = g_imcg111_t
 
      
      #寫入修改者/修改日期資訊
      LET g_imcg_m.imcgmodid = g_user 
LET g_imcg_m.imcgmoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
 
      CALL aimi106_input("u")     #欄位更改
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_imcg_m.* = g_imcg_m_t.*
         CALL aimi106_show()
         CALL cl_err("",9001,0)
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE imcg_t SET (imcgmodid,imcgmoddt) = (g_imcg_m.imcgmodid,g_imcg_m.imcgmoddt)
       WHERE imcgent = g_enterprise AND imcgsite = g_site AND imcg111 = g_imcg111_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_imcg_m.imcg111,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE aimi106_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_imcg_m.imcg111,"U")
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
]]>
