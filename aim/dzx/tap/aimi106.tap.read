    <![CDATA[#+ 資料新增
PRIVATE FUNCTION aimi106_insert()
   #add-point:insert段define
   {<point name="insert.define"/>}
   #end add-point    
   
   CLEAR FORM #清畫面欄位內容
   INITIALIZE g_imcg_m.* LIKE imcg_t.*             #DEFAULT 設定
   LET g_imcg111_t = NULL
 
   CALL s_transaction_begin()
   
   WHILE TRUE
      
      #公用欄位給值
      #此段落由子樣板a14產生    
      LET g_imcg_m.imcgownid = g_user
      LET g_imcg_m.imcgowndp = g_dept
      LET g_imcg_m.imcgcrtid = g_user
      LET g_imcg_m.imcgcrtdp = g_dept 
