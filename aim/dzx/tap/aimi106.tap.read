  <section id="aimi106.delete" ver="7" status="" src="s">
    <![CDATA[#+ 資料刪除 
PRIVATE FUNCTION aimi106_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point  
   
   IF g_imcg_m.imcg111 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   CALL aimi106_show()
   
   CALL s_transaction_begin()
    
   LET g_imcg111_t = g_imcg_m.imcg111
 
   
   
 
   OPEN aimi106_cl USING g_enterprise, g_site,
                           g_imcg_m.imcg111
 
   IF STATUS THEN
      CALL cl_err("OPEN aimi106_cl:", STATUS, 0)
      CLOSE aimi106_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aimi106_master_referesh USING g_imcg_m.imcg111 INTO g_imcg_m.imcg111,g_imcg_m.imcg112,g_imcg_m.imcg113, 
       g_imcg_m.imcg114,g_imcg_m.imcg115,g_imcg_m.imcg116,g_imcg_m.imcg117,g_imcg_m.imcg120,g_imcg_m.imcgstus, 
       g_imcg_m.imcgownid,g_imcg_m.imcgowndp,g_imcg_m.imcgcrtid,g_imcg_m.imcgcrtdp,g_imcg_m.imcgcrtdt, 
       g_imcg_m.imcgmodid,g_imcg_m.imcgmoddt,g_imcg_m.imcg118,g_imcg_m.imcg119,g_imcg_m.imcg111_desc, 
       g_imcg_m.imcg112_desc,g_imcg_m.imcg113_desc,g_imcg_m.imcgownid_desc,g_imcg_m.imcgowndp_desc,g_imcg_m.imcgcrtid_desc, 
       g_imcg_m.imcgcrtdp_desc,g_imcg_m.imcgmodid_desc
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_imcg_m.imcg111,SQLCA.sqlcode,0)
      RETURN
   END IF
   
   IF cl_ask_delete() THEN
 
      #add-point:單頭刪除前
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point
 
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL aimi106_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
      DELETE FROM imcg_t 
       WHERE imcgent = g_enterprise AND imcgsite = g_site AND imcg111 = g_imcg_m.imcg111 
 
 
      #add-point:單頭刪除中
      {<point name="delete.head.m_delete"/>}
      #end add-point
         
      IF SQLCA.sqlcode THEN
         CALL cl_err("imcg_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
  
      
      
      #add-point:單頭刪除後
      {<point name="delete.head.a_delete"/>}
      #end add-point
      
          
      CLEAR FORM
      CALL aimi106_ui_browser_refresh()
      IF g_browser_cnt > 0 THEN
         CALL aimi106_browser_fill(g_wc,"")
         CALL aimi106_fetch("P")
      ELSE
         CALL aimi106_browser_fill(" 1=1 ","F")
      END IF
      
   END IF
 
   CLOSE aimi106_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_imcg_m.imcg111,"D")
 
END FUNCTION
