  <section id="aimi091.b_fill" ver="7" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aimi091_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_imed_d.clear()
   CALL g_imed2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE imedstus,imed003,imed004,imed005,imed006,imed007,imed008,imed003,imedmodid, 
       imedmoddt,imedownid,imedowndp,imedcrtid,imedcrtdp,imedcrtdt,t1.oocql004 ,t2.oofa011 ,t3.oofa011 , 
       t4.oofa011 ,t5.ooefl003 ,t6.oofa011 ,t7.ooefl003 FROM imed_t",   
               "",
               
                              " LEFT JOIN oocql_t t1 ON t1.oocqlent='"||g_enterprise||"' AND t1.oocql001='5' AND t1.oocql002=imed005 AND t1.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t2 ON t2.oofaent='"||g_enterprise||"' AND t2.oofa002='2' AND t2.oofa003=imed006  ",
               " LEFT JOIN oofa_t t3 ON t3.oofaent='"||g_enterprise||"' AND t3.oofa002='2' AND t3.oofa003=imedmodid  ",
               " LEFT JOIN oofa_t t4 ON t4.oofaent='"||g_enterprise||"' AND t4.oofa002='2' AND t4.oofa003=imedownid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=imedowndp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t6 ON t6.oofaent='"||g_enterprise||"' AND t6.oofa002='2' AND t6.oofa003=imedcrtid  ",
               " LEFT JOIN ooefl_t t7 ON t7.ooeflent='"||g_enterprise||"' AND t7.ooefl001=imedcrtdp AND t7.ooefl002='"||g_dlang||"' ",
 
               " WHERE imedent= ? AND imed001=? AND imed002=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("imed_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF aimi091_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY imed_t.imed003"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE aimi091_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR aimi091_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_imed_m.imed001,g_imed_m.imed002
                                               
      FOREACH b_fill_cs INTO g_imed_d[l_ac].imedstus,g_imed_d[l_ac].imed003,g_imed_d[l_ac].imed004,g_imed_d[l_ac].imed005, 
          g_imed_d[l_ac].imed006,g_imed_d[l_ac].imed007,g_imed_d[l_ac].imed008,g_imed2_d[l_ac].imed003, 
          g_imed2_d[l_ac].imedmodid,g_imed2_d[l_ac].imedmoddt,g_imed2_d[l_ac].imedownid,g_imed2_d[l_ac].imedowndp, 
          g_imed2_d[l_ac].imedcrtid,g_imed2_d[l_ac].imedcrtdp,g_imed2_d[l_ac].imedcrtdt,g_imed_d[l_ac].imed005_desc, 
          g_imed_d[l_ac].imed006_desc,g_imed2_d[l_ac].imedmodid_desc,g_imed2_d[l_ac].imedownid_desc, 
          g_imed2_d[l_ac].imedowndp_desc,g_imed2_d[l_ac].imedcrtid_desc,g_imed2_d[l_ac].imedcrtdp_desc 

                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_imed2_d[l_ac].imedownid_desc = cl_get_username(g_imed2_d[l_ac].imedownid)
      #LET g_imed2_d[l_ac].imedowndp_desc = cl_get_deptname(g_imed2_d[l_ac].imedowndp)
      #LET g_imed2_d[l_ac].imedcrtid_desc = cl_get_username(g_imed2_d[l_ac].imedcrtid)
      #LET g_imed2_d[l_ac].imedcrtdp_desc = cl_get_deptname(g_imed2_d[l_ac].imedcrtdp)
      #LET g_imed2_d[l_ac].imedmodid_desc = cl_get_username(g_imed2_d[l_ac].imedmodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_imed_d.deleteElement(g_imed_d.getLength())
      CALL g_imed2_d.deleteElement(g_imed2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE aimi091_pb   
   
END FUNCTION
]]>
</section>
