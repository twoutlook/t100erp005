  <section id="aimi091.input" ver="3" status="" src="s">
<![CDATA[#+ 資料輸入
PRIVATE FUNCTION aimi091_input(p_cmd)
   DEFINE  p_cmd                 LIKE type_t.chr1
   DEFINE  l_cmd_t               LIKE type_t.chr1
   DEFINE  l_cmd                 LIKE type_t.chr1
   DEFINE  l_ac_t                LIKE type_t.num5                #未取消的ARRAY CNT 
   DEFINE  l_n                   LIKE type_t.num5                #檢查重複用  
   DEFINE  l_cnt                 LIKE type_t.num5                #檢查重複用  
   DEFINE  l_lock_sw             LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert        LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete        LIKE type_t.num5                #可刪除否  
   DEFINE  l_count               LIKE type_t.num5
   DEFINE  l_i                   LIKE type_t.num5
   DEFINE  l_insert              BOOLEAN
   DEFINE  ls_return             STRING
   DEFINE  l_var_keys            DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys          DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                DYNAMIC ARRAY OF STRING
   DEFINE  l_fields              DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak        DYNAMIC ARRAY OF STRING
   DEFINE  lb_reproduce          BOOLEAN
   DEFINE  li_reproduce          LIKE type_t.num5
   DEFINE  li_reproduce_target   LIKE type_t.num5
   #add-point:input段define
   {<point name="input.define"/>}
   #end add-point    
   
   #先做狀態判定
   IF p_cmd = 'r' THEN
      LET l_cmd_t = 'r'
      LET p_cmd   = 'a'
   ELSE
      LET l_cmd_t = p_cmd
   END IF   
   
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF  
 
   CALL cl_set_head_visible("","YES")  
 
   LET l_insert = FALSE
   LET g_action_choice = ""
 
   #add-point:input段define_sql
   {<point name="input.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT imedstus,imed003,imed004,imed005,imed006,imed007,imed008,imed003,imedmodid, 
       imedmoddt,imedownid,imedowndp,imedcrtid,imedcrtdp,imedcrtdt FROM imed_t WHERE imedent=? AND imed001=?  
       AND imed002=? AND imed003=? FOR UPDATE"
   #add-point:input段define_sql
   {<point name="input.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE aimi091_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR
 
 
   
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   LET g_qryparam.state = 'i'
   
   #控制key欄位可否輸入
   CALL aimi091_set_entry(p_cmd)
   #add-point:set_entry後
   {<point name="input.after_set_entry"/>}
   #end add-point
   CALL aimi091_set_no_entry(p_cmd)
   #add-point:set_no_entry後
   {<point name="input.after_set_no_entry"/>}
   #end add-point
 
   DISPLAY BY NAME g_imed_m.imed001,g_imed_m.imed002
   
   LET lb_reproduce = FALSE
   
   #add-point:進入修改段前
   {<point name="input.before_input"/>}
   #end add-point
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
]]>
</section>
  <section id="aimi091.input.body" ver="7" status="" src="s">
<![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_imed_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂單身ACTION
         
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_imed_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL aimi091_b_fill(g_wc2) 
            IF g_rec_b != 0 THEN
               CALL fgl_set_arr_curr(l_ac)
            END IF
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN aimi091_cl USING g_enterprise,
                                               g_imed_m.imed001
                                               ,g_imed_m.imed002
 
                                               
               IF STATUS THEN
                  CALL cl_err("OPEN aimi091_cl:", STATUS, 1)
                  CLOSE aimi091_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_imed_d[l_ac].imed003 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_imed_d_t.* = g_imed_d[l_ac].*  #BACKUP
               CALL aimi091_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL aimi091_set_no_entry_b(l_cmd)
               OPEN aimi091_bcl USING g_enterprise,g_imed_m.imed001,
                                                g_imed_m.imed002,
 
                                                g_imed_d_t.imed003
 
               IF STATUS THEN
                  CALL cl_err("OPEN aimi091_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aimi091_bcl INTO g_imed_d[l_ac].imedstus,g_imed_d[l_ac].imed003,g_imed_d[l_ac].imed004, 
                      g_imed_d[l_ac].imed005,g_imed_d[l_ac].imed006,g_imed_d[l_ac].imed007,g_imed_d[l_ac].imed008 

                  IF SQLCA.sqlcode THEN
                      CALL cl_err(g_imed_d_t.imed003,SQLCA.sqlcode,1)
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL aimi091_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
        
         BEFORE INSERT
            
            INITIALIZE g_imed_d_t.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_imed_d[l_ac].* TO NULL
            
            #公用欄位預設值
            #此段落由子樣板a14產生    
      LET g_imed2_d[l_ac].imedownid = g_user
      LET g_imed2_d[l_ac].imedowndp = g_dept
      LET g_imed2_d[l_ac].imedcrtid = g_user
      LET g_imed2_d[l_ac].imedcrtdp = g_dept 
      LET g_imed2_d[l_ac].imedcrtdt = cl_get_current()
      LET g_imed2_d[l_ac].imedmodid = ""
      LET g_imed2_d[l_ac].imedmoddt = ""
      LET g_imed_d[l_ac].imedstus = ""
 
  
            
            #一般欄位預設值
                  LET g_imed_d[l_ac].imedstus = "Y"
      LET g_imed_d[l_ac].imed004 = "2"
 
            
            
            LET g_imed_d_t.* = g_imed_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL aimi091_set_entry_b(l_cmd)
            #add-point:set_entry_b後
            {<point name="input.body.before_insert.set_entry_b"/>}
            #end add-point
            CALL aimi091_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_imed_d[li_reproduce_target].* = g_imed_d[li_reproduce].*
               LET g_imed2_d[li_reproduce_target].* = g_imed2_d[li_reproduce].*
 
               LET g_imed_d[g_imed_d.getLength()].imed003 = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
 
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM imed_t 
             WHERE imedent = g_enterprise AND imed001 = g_imed_m.imed001
               AND imed002 = g_imed_m.imed002
 
               AND imed003 = g_imed_d[l_ac].imed003
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               CALL s_transaction_begin()
               #add-point:單身新增前
               {<point name="input.body.b_insert" mark="Y"/>}
               #end add-point
               INSERT INTO imed_t
                           (imedent,
                            imed001,imed002,
                            imed003
                            ,imedstus,imed004,imed005,imed006,imed007,imed008,imedmodid,imedmoddt,imedownid,imedowndp,imedcrtid,imedcrtdp,imedcrtdt) 
                     VALUES(g_enterprise,
                            g_imed_m.imed001,g_imed_m.imed002,
                            g_imed_d[l_ac].imed003
                            ,g_imed_d[l_ac].imedstus,g_imed_d[l_ac].imed004,g_imed_d[l_ac].imed005,g_imed_d[l_ac].imed006, 
                                g_imed_d[l_ac].imed007,g_imed_d[l_ac].imed008,g_imed2_d[l_ac].imedmodid, 
                                g_imed2_d[l_ac].imedmoddt,g_imed2_d[l_ac].imedownid,g_imed2_d[l_ac].imedowndp, 
                                g_imed2_d[l_ac].imedcrtid,g_imed2_d[l_ac].imedcrtdp,g_imed2_d[l_ac].imedcrtdt) 

               #add-point:單身新增中
               {<point name="input.body.m_insert"/>}
               #end add-point
               LET p_cmd = 'u'
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_imed_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("imed_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR "INSERT O.K"
               LET g_rec_b=g_rec_b+1
               DISPLAY g_rec_b TO FORMONLY.cnt
            END IF
            
            #add-point:單身新增後
            {<point name="input.body.after_insert"/>}
            #end add-point
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_imed_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_imed_d.deleteElement(l_ac)
               NEXT FIELD imed003
            END IF
            IF g_imed_d_t.imed003 IS NOT NULL
 
               THEN
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete"/>}
               #end add-point
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               IF aimi091_before_delete() THEN 
                  CALL s_transaction_end('Y','0')
               ELSE 
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               END IF 
               CLOSE aimi091_bcl
               LET l_count = g_imed_d.getLength()
            END IF 
            
            #add-point:單身刪除後
            {<point name="input.body.a_delete"/>}
            #end add-point
              
         AFTER DELETE 
            #add-point:單身AFTER DELETE 
            {<point name="input.body.after_delete"/>}
            #end add-point
 
                  #此段落由子樣板a01產生
         BEFORE FIELD imedstus
            #add-point:BEFORE FIELD imedstus
            {<point name="input.b.page1.imedstus" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD imedstus
            
            #add-point:AFTER FIELD imedstus
            {<point name="input.a.page1.imedstus" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE imedstus
            #add-point:ON CHANGE imedstus
            {<point name="input.g.page1.imedstus" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD imed003
            #add-point:BEFORE FIELD imed003
            {<point name="input.b.page1.imed003" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD imed003
            
            #add-point:AFTER FIELD imed003
            {<point name="input.a.page1.imed003" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE imed003
            #add-point:ON CHANGE imed003
            {<point name="input.g.page1.imed003" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD imed004
            #add-point:BEFORE FIELD imed004
            {<point name="input.b.page1.imed004" />}
            #END add-point
 
         #此段落由子樣板a02產生
