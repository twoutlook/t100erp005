  <section id="aimi091.delete" ver="6" status="" src="s">
<![CDATA[#+ 資料刪除
PRIVATE FUNCTION aimi091_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_imed_m.imed001 IS NULL
   OR g_imed_m.imed002 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE aimi091_master_referesh USING g_imed_m.imed001,g_imed_m.imed002 INTO g_imed_m.imed001,g_imed_m.imed002, 
       g_imed_m.imed001_desc,g_imed_m.imed002_desc
   
   CALL aimi091_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN aimi091_cl USING g_enterprise,g_imed_m.imed001
                                                       ,g_imed_m.imed002
 
   IF STATUS THEN
      CALL cl_err("OPEN aimi091_cl:", STATUS, 1)
      CLOSE aimi091_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aimi091_cl INTO g_imed_m.imed001,g_imed_m.imed001_desc,g_imed_m.imed002,g_imed_m.imed002_desc 

   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_imed_m.imed001,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL aimi091_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM imed_t WHERE imedent = g_enterprise AND imed001 = g_imed_m.imed001
                                                               AND imed002 = g_imed_m.imed002
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("imed_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_imed_d.clear() 
      CALL g_imed2_d.clear()       
 
     
      CALL aimi091_ui_browser_refresh()  
      CALL aimi091_ui_headershow()  
      CALL aimi091_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL aimi091_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL aimi091_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE aimi091_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_imed_m.imed001,'D')
    
END FUNCTION
]]>
