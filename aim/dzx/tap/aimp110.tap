             pmdl016 LIKE pmdl_t.pmdl016,
             pmdl012 LIKE pmdl_t.pmdl012,
             pmdl013 LIKE pmdl_t.pmdl013,
             pmdl015 LIKE pmdl_t.pmdl015,
             pmdn006 LIKE pmdn_t.pmdn006,
             pmdn007 LIKE pmdn_t.pmdn007
          END RECORD  
   DEFINE l_ant    LIKE type_t.num20_6
   DEFINE l_qty    LIKE type_t.num20_6 
   DEFINE l_imaa001 LIKE imaa_t.imaa001,
          l_imaa006 LIKE imaa_t.imaa006,
          l_imai022 LIKE imai_t.imai022,
          l_rate    LIKE inaj_t.inaj014  
   DEFINE l_count   LIKE type_t.num10  
   DEFINE l_cnt     LIKE type_t.num10 
   DEFINE l_stagenow STRING   
   DEFINE l_ooef016 LIKE ooef_t.ooef016]]>
