  <point name="process.pre_process" order="" ver="2" cite_std="N" new="N" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[   #-------------------------------------計價單位，計價數量，單價 ，單頭匯率，稅率，單價含稅否，幣別,  採購單位，採購數量
   LET ls_sql = "SELECT pmdndocno,pmdnseq,pmdn010,pmdn011,pmdn015,pmdl016,pmdl012,pmdl013,pmdl015,pmdn006,pmdn007 ",
                "  FROM pmdl_t,pmdn_t",
                " WHERE pmdlent = pmdnent AND pmdldocno = pmdndocno ",
                "   AND pmdnent = '",g_enterprise,"'",
                "   AND pmdnsite = '",g_site,"'",
                "   AND pmdn001 = ? ",
                "   AND pmdldocdt BETWEEN '",g_master.bdate,"' AND '",g_master.edate,"'",
                "   AND pmdlstus = 'Y' "
   PREPARE aimp110_pmd_pr FROM ls_sql
   DECLARE aimp110_pmd_cs CURSOR FOR aimp110_pmd_pr]]>
  </point>
  <point name="process.process" order="" ver="2" cite_std="N" new="N" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[   CALL cl_err_collect_init()
   LET ls_sql = " SELECT COUNT(*) FROM imaa_t ",
                "  WHERE imaaent ='",g_enterprise,"' AND ",g_master.wc
   PREPARE aimp110_count_pr FROM ls_sql
   EXECUTE aimp110_count_pr INTO l_count
   LET l_cnt = 1

   LET ls_sql = " SELECT DISTINCT imaa001,imaa006 FROM imaa_t ",
                "  WHERE imaaent ='",g_enterprise,"' AND ",g_master.wc
   DECLARE aimp110_process_cs CURSOR FROM ls_sql
   FOREACH aimp110_process_cs INTO l_imaa001,l_imaa006
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF      
      LET li_count = (l_cnt/l_count)*100
      DISPLAY li_count TO FORMONLY.stagecomplete
      LET l_stagenow = l_imaa001
      DISPLAY l_stagenow TO FORMONLY.stagenow
      
      LET l_ant = 0 
      LET l_qty = 0
      FOREACH aimp110_pmd_cs USING l_imaa001 INTO l_pmdn.*
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF      
         IF cl_null(l_pmdn.pmdl016) THEN LET l_pmdn.pmdl016 = 1 END IF
         #計價數量
         CALL s_aooi250_convert_qty(l_imaa001,l_pmdn.pmdn010,l_imaa006,l_pmdn.pmdn011) RETURNING g_success,l_pmdn.pmdn011
         IF l_pmdn.pmdl013 = 'Y' THEN 
            LET l_pmdn.pmdn015 = l_pmdn.pmdn015/(1+l_pmdn.pmdl012/100)
         END IF           
         LET l_ant = l_ant +l_pmdn.pmdn011 * l_pmdn.pmdn015 * l_pmdn.pmdl016

         #採購數量
         CALL s_aooi250_convert_qty(l_imaa001,l_pmdn.pmdn006,l_imaa006,l_pmdn.pmdn007) RETURNING g_success,l_pmdn.pmdn007      
         LET l_qty = l_qty +l_pmdn.pmdn007 
      END FOREACH 
      LET l_imai022 = l_ant/l_qty
      SELECT ooef016 INTO l_ooef016
        FROM ooef_t
       WHERE ooefent = g_enterprise
         AND ooef001 = g_site       
      CALL s_curr_round(g_site,l_ooef016,l_imai022,'1') RETURNING l_imai022      
      IF g_master.a = 'Y' THEN 
         UPDATE imai_t
            SET imai022 = l_imai022
          WHERE imaient = g_enterprise
            AND imaisite = g_site
            AND imai001 = l_imaa001            
      END IF
      LET l_cnt = l_cnt + 1      
   END FOREACH
   CALL cl_err_collect_show()]]>
  </point>
