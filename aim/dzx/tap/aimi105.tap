   #add-point:reproduce段define
   {<point name="reproduce.define"/>}
   #end add-point   
   
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF
   
   IF g_imcf_m.imcfsite IS NULL
      OR g_imcf_m.imcf011 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   LET g_imcfsite_t = g_imcf_m.imcfsite
   LET g_imcf011_t = g_imcf_m.imcf011
 
   
   LET g_imcf_m.imcfsite = ""
   LET g_imcf_m.imcf011 = ""
 
    
   CALL aimi105_set_entry("a")
   CALL aimi105_set_no_entry("a")
   
   #公用欄位給予預設值
   #此段落由子樣板a14產生    
      LET g_imcf_m.imcfownid = g_user
      LET g_imcf_m.imcfowndp = g_dept
      LET g_imcf_m.imcfcrtid = g_user
      LET g_imcf_m.imcfcrtdp = g_dept 
      LET g_imcf_m.imcfcrtdt = cl_get_current()
      LET g_imcf_m.imcfmodid = ""
      LET g_imcf_m.imcfmoddt = ""
      LET g_imcf_m.imcfstus = "N"
 
 
   
   CALL s_transaction_begin()
   
   #add-point:複製輸入前
   {<point name="reproduce.head.b_input"/>}
   #end add-point
   
   CALL aimi105_input("r")
 
      LET g_imcf_m.imcf011_desc = ''
   DISPLAY BY NAME g_imcf_m.imcf011_desc
 
   
   IF INT_FLAG  THEN
      LET INT_FLAG = 0
      RETURN
   END IF
   
   CALL s_transaction_begin()
   
   #add-point:單頭複製前
   {<point name="reproduce.head.b_insert" mark="Y"/>}
   #end add-point
   
   #add-point:單頭複製中
   {<point name="reproduce.head.m_insert"/>}
   #end add-point
   
   IF SQLCA.sqlcode THEN
      CALL cl_err("imcf_t",SQLCA.sqlcode,1)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:單頭複製後
   {<point name="reproduce.head.a_insert"/>}
   #end add-point
   
   CALL s_transaction_end('Y','0')
   
   LET g_state = "Y"
   
   LET g_wc = g_wc,  
              " OR (",
              " imcfsite = '", g_imcf_m.imcfsite CLIPPED, "' "
              ," AND imcf011 = '", g_imcf_m.imcf011 CLIPPED, "' "
 
              , ") "
   
   LET g_imcfsite_t = g_imcf_m.imcfsite
   LET g_imcf011_t = g_imcf_m.imcf011
 
   
   #add-point:完成複製段落後
   {<point name="reproduce.after_reproduce" />}
   #end add-point
                   
END FUNCTION
