  <point name="reproduce.a.imcf011" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[           DISPLAY ' ' TO imcf011_desc 
           IF NOT cl_null(l_newno) AND NOT cl_null(l_newno02) THEN 
               #检查资料在应用分类码中是否存在且有效
               CALL aimi105_imcf011_chk(l_newno02)
               IF NOT cl_null(g_errno) THEN 
                  CALL cl_err(l_newno02,g_errno,1)
                  LET l_newno02= ''
                  LET g_imcf_m.imcf011_desc = ''
                  DISPLAY BY NAME g_imcf_m.imcf011_desc 
                  NEXT FIELD imcf011
               END IF
               #检查重复 
               LET l_cnt = 0
               SELECT COUNT(*) INTO l_cnt FROM imcf_t 
                WHERE imcfent = g_enterprise AND imcfsite = l_newno
                  AND imcf011 = l_newno02
               IF l_cnt > 0 THEN
                  CALL cl_err("Reproduce","std-00006",1)
                  LET l_newno02 = ''
                  LET g_imcf_m.imcf011_desc = ''
                  DISPLAY g_imcf_m.imcf011_desc TO imcf011_desc                
                  NEXT FIELD imcf011
               END IF
            END IF

            CALL aimi105_imcf011_desc(g_imcf_m.imcf011) RETURNING g_imcf_m.imcf011_desc
            DISPLAY g_imcf_m.imcf011_desc TO imcf011_desc
