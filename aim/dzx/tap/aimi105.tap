   LET g_sql = " SELECT UNIQUE imcfsite,imcf011,imcf012,imcfstus,imcfownid,imcfowndp,imcfcrtid,imcfcrtdp, 
       imcfcrtdt,imcfmodid,imcfmoddt,imcf013,imcf014,imcf015,imcf023,imcf016,imcf017,imcf018,imcf019, 
