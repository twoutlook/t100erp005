               " LEFT JOIN oofa_t t6 ON t6.oofaent='"||g_enterprise||"' AND t6.oofa002='2' AND t6.oofa003=imcfmodid  ",
               " LEFT JOIN oocal_t t7 ON t7.oocalent='"||g_enterprise||"' AND t7.oocal001=imcf016 AND t7.oocal002='"||g_dlang||"' ",
               " LEFT JOIN oobxl_t t8 ON t8.oobxlent='"||g_enterprise||"' AND t8.oobxl001=imcf031 AND t8.oobxl002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t9 ON t9.ooeflent='"||g_enterprise||"' AND t9.ooefl001=imcf034 AND t9.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t10 ON t10.imaalent='"||g_enterprise||"' AND t10.imaal001=imcf032 AND t10.imaal002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t11 ON t11.ooeflent='"||g_enterprise||"' AND t11.ooefl001=imcf035 AND t11.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN inaa_t t12 ON t12.inaaent='"||g_enterprise||"' AND t12.inaasite=imcfsite AND t12.inaa001=imcf041  ",
               " LEFT JOIN inab_t t13 ON t13.inabent='"||g_enterprise||"' AND t13.inabsite=imcfsite AND t13.inab001=imcf041 AND t13.inab002=imcf042  ",
               " LEFT JOIN oocal_t t14 ON t14.oocalent='"||g_enterprise||"' AND t14.oocal001=imcf081 AND t14.oocal002='"||g_dlang||"' ",
               " LEFT JOIN inaa_t t15 ON t15.inaaent='"||g_enterprise||"' AND t15.inaasite=imcfsite AND t15.inaa001=imcf101  ",
               " LEFT JOIN inab_t t16 ON t16.inabent='"||g_enterprise||"' AND t16.inabsite=imcfsite AND t16.inab001=imcf101 AND t16.inab002=imcf102  ",
               " LEFT JOIN inaa_t t17 ON t17.inaaent='"||g_enterprise||"' AND t17.inaasite=imcfsite AND t17.inaa001=imcf103  ",
               " LEFT JOIN inab_t t18 ON t18.inabent='"||g_enterprise||"' AND t18.inabsite=imcfsite AND t18.inab001=imcf103 AND t18.inab002=imcf104  ",
 
               " WHERE imcfent = '" ||g_enterprise|| "' AND imcfsite = ? AND imcf011 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE aimi105_master_referesh FROM g_sql
 
 
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_aimi105 WITH FORM cl_ap_formpath("aim",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL aimi105_init()   
 
      #進入選單 Menu (="N")
      CALL aimi105_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_aimi105
      
   END IF 
   
   CLOSE aimi105_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
   
END MAIN
 
 
