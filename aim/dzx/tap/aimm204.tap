   {<point name="delete.define" edit="s"/>}
   #end add-point  
   #add-point:delete段define(客製用)
   {<point name="delete.define_customerization" edit="c"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_imaf_m.imaf001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
    
   LET g_imaf001_t = g_imaf_m.imaf001
 
   
   LET g_master_multi_table_t.imaal001 = g_imaf_m.imaf001
LET g_master_multi_table_t.imaal003 = g_imaf_m.imaal003
LET g_master_multi_table_t.imaal004 = g_imaf_m.imaal004
LET g_master_multi_table_t.imaal005 = g_imaf_m.imaal005
 
 
   OPEN aimm204_cl USING g_enterprise, g_site,g_imaf_m.imaf001
 
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aimm204_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE aimm204_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aimm204_master_referesh USING g_site,g_imaf_m.imaf001 INTO g_imaf_m.imaf001,g_imaf_m.imaf141, 
       g_imaf_m.imaf142,g_imaf_m.imaf143,g_imaf_m.imaf144,g_imaf_m.imaf145,g_imaf_m.imaf146,g_imaf_m.imaf147, 
       g_imaf_m.imaf148,g_imaf_m.imaf149,g_imaf_m.imafownid,g_imaf_m.imafowndp,g_imaf_m.imafcrtid,g_imaf_m.imafcrtdp, 
       g_imaf_m.imafcrtdt,g_imaf_m.imafmodid,g_imaf_m.imafmoddt,g_imaf_m.imaf151,g_imaf_m.imaf158,g_imaf_m.imaf157, 
       g_imaf_m.imaf152,g_imaf_m.imaf153,g_imaf_m.imaf154,g_imaf_m.imaf155,g_imaf_m.imaf156,g_imaf_m.imaf161, 
       g_imaf_m.imaf162,g_imaf_m.imaf163,g_imaf_m.imaf164,g_imaf_m.imaf165,g_imaf_m.imaf166,g_imaf_m.imaf171, 
       g_imaf_m.imaf172,g_imaf_m.imaf173,g_imaf_m.imaf174,g_imaf_m.imaf175,g_imaf_m.imaf176,g_imaf_m.imaf141_desc, 
       g_imaf_m.imaf142_desc,g_imaf_m.imaf143_desc,g_imaf_m.imaf144_desc,g_imaf_m.imafownid_desc,g_imaf_m.imafowndp_desc, 
       g_imaf_m.imafcrtid_desc,g_imaf_m.imafcrtdp_desc,g_imaf_m.imafmodid_desc,g_imaf_m.imaf157_desc, 
       g_imaf_m.imaf153_desc,g_imaf_m.imaf176_desc
   
   #檢查是否允許此動作
   IF NOT aimm204_action_chk() THEN
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_imaf_m_mask_o.* =  g_imaf_m.*
   CALL aimm204_imaf_t_mask()
   LET g_imaf_m_mask_n.* =  g_imaf_m.*
   
   #將最新資料顯示到畫面上
   CALL aimm204_show()
   
   IF cl_ask_delete() THEN
 
      #add-point:單頭刪除前
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point
 
      #應用 a47 樣板自動產生(Version:2)
      #刪除相關文件
      CALL aimm204_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
 
      DELETE FROM imaf_t 
       WHERE imafent = g_enterprise AND imafsite = g_site AND imaf001 = g_imaf_m.imaf001 
 
 
      #add-point:單頭刪除中
      {<point name="delete.head.m_delete"/>}
      #end add-point
         
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "imaf_t" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
      END IF
  
      INITIALIZE l_var_keys_bak TO NULL 
   INITIALIZE l_field_keys   TO NULL 
   LET l_var_keys_bak[01] = g_enterprise
   LET l_field_keys[01] = 'imaalent'
   LET l_var_keys_bak[02] = g_master_multi_table_t.imaal001
   LET l_field_keys[02] = 'imaal001'
   CALL cl_multitable_delete(l_field_keys,l_var_keys_bak,'imaal_t')
 
      
      #add-point:單頭刪除後
      {<point name="delete.head.a_delete"/>}
      #end add-point
      
       
 
      #修改歷程記錄(刪除)
      IF NOT cl_log_modified_record('','') THEN 
         CLOSE aimm204_cl
         CALL s_transaction_end('N','0')
         RETURN
      END IF
      
      CLEAR FORM
      CALL aimm204_ui_browser_refresh()
      
      #確保畫面上保有資料
      IF g_browser_cnt > 0 THEN
         #CALL aimm204_browser_fill(g_wc,"")
         CALL aimm204_fetch("P")
      ELSE
         CLEAR FORM
      END IF
      
   END IF
 
   CLOSE aimm204_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aimm204_msgcentre_notify('delete')
 
END FUNCTION
