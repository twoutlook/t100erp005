               " LEFT JOIN oocal_t t9 ON t9.oocalent='"||g_enterprise||"' AND t9.oocal001=t0.imbf143 AND t9.oocal002='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t10 ON t10.oocalent='"||g_enterprise||"' AND t10.oocal001=t0.imbf144 AND t10.oocal002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t11 ON t11.ooagent='"||g_enterprise||"' AND t11.ooag001=t0.imbfownid  ",
