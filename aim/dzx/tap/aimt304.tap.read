  <point name="function.aimt304_imbf141_chk" order="1" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#+ imbf141欄位check
PRIVATE FUNCTION aimt304_imbf141_chk()
   DEFINE l_imce141       LIKE imce_t.imce141
   DEFINE l_imcestus      LIKE imce_t.imcestus  

   SELECT imce141 ,imcestus INTO l_imce141,l_imcestus FROM imce_t
    WHERE imceent = g_enterprise
      AND imcesite = g_site
      AND imce141 = g_imbf_m.imbf141
    IF cl_null(l_imce141) THEN 
        INITIALIZE g_errparam TO NULL
        LET g_errparam.code = 'aim-00048'
        LET g_errparam.extend = l_imce141
        LET g_errparam.popup = TRUE
        CALL cl_err()
         
      RETURN FALSE  
    END IF
    IF l_imcestus != 'Y' THEN
       INITIALIZE g_errparam TO NULL
       LET g_errparam.code = 'aim-00049'
       LET g_errparam.extend = l_imce141
       LET g_errparam.popup = TRUE
       CALL cl_err()
       
       RETURN FALSE  
    END IF     
    RETURN TRUE  
END FUNCTION]]>
  </point>
  <point name="function.aimt304_ooca_chk" order="2" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#+ 單位欄位檢查
PRIVATE FUNCTION aimt304_ooca_chk(p_imbf143)
   DEFINE p_imbf143       LIKE imbf_t.imbf143
   DEFINE l_ooca001       LIKE ooca_t.ooca001
   DEFINE l_oocastus      LIKE ooca_t.oocastus
   DEFINE l_success       LIKE type_t.num5
   DEFINE l_rate          LIKE inaj_t.inaj014    
   
   SELECT ooca001,oocastus INTO l_ooca001,l_oocastus FROM ooca_t
    WHERE oocaent = g_enterprise
      AND ooca001 = p_imbf143
    IF cl_null(l_ooca001) THEN 
       INITIALIZE g_errparam TO NULL
       LET g_errparam.code = 'aim-00004'
       LET g_errparam.extend = p_imbf143
       LET g_errparam.popup = TRUE
       CALL cl_err()

       RETURN FALSE
    END IF
    IF l_oocastus != 'Y' THEN
       INITIALIZE g_errparam TO NULL
       LET g_errparam.code = 'aim-00005'
       LET g_errparam.extend = p_imbf143
       LET g_errparam.popup = TRUE
       CALL cl_err()

       RETURN FALSE
    END IF
    CALL s_aimi190_get_convert(g_imbf_m.imba001,p_imbf143,g_imbf_m.imba006) RETURNING l_success,l_rate  
    IF l_success = FALSE THEN
       RETURN FALSE
    END IF    
    RETURN TRUE   
            
END FUNCTION]]>
  </point>
