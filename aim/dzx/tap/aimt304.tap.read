       t0.imbf156,t0.imbf161,t0.imbf162,t0.imbf163,t0.imbf164,t0.imbf165,t0.imbf166,t0.imbf171,t0.imbf172, 
       t0.imbf173,t0.imbf174,t0.imbf175,t0.imbf176,t7.oocql004 ,t8.ooag011 ,t9.oocal003 ,t10.oocal003 , 
       t11.ooag011 ,t12.ooefl003 ,t13.ooag011 ,t14.ooefl003 ,t15.ooag011 ,t16.imaal003 ,t17.pmaal003 , 
       t18.oocql004",
