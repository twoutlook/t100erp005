  <point name="function.aimt304_chk_ooeh" order="4" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 檢查aooi090是否設置對應欄位
# Date & Author..: 2014/02/17 By chenjing
# Modify.........:
################################################################################
PRIVATE FUNCTION aimt304_chk_ooeh(p_ooeh002)
DEFINE p_ooeh002        LIKE ooeh_t.ooeh002
DEFINE l_n              LIKE type_t.num5

   LET l_n = 0
   SELECT COUNT(*) INTO l_n
     FROM ooeh_t
    WHERE ooehent = g_enterprise
      AND ooeh001 = '1'
      AND ooeh002 = p_ooeh002
   IF l_n = 0 THEN
      RETURN FALSE
   END IF
   RETURN TRUE
END FUNCTION]]>
