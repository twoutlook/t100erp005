  <section id="aimm200.reproduce" ver="25" status="" src="s" readonly="">
    <![CDATA[#+ 資料複製
PRIVATE FUNCTION aimm200_reproduce()
   DEFINE l_newno     LIKE imaa_t.imaa001 
   DEFINE l_oldno     LIKE imaa_t.imaa001 
 
   DEFINE l_master    RECORD LIKE imaa_t.*
   DEFINE l_detail    RECORD LIKE imaj_t.*
   DEFINE l_detail2    RECORD LIKE imal_t.*
 
   DEFINE l_detail3    RECORD LIKE imam_t.*
 
   DEFINE l_detail4    RECORD LIKE imao_t.*
 
 
 
   DEFINE l_cnt       LIKE type_t.num10
   #add-point:reproduce段define
