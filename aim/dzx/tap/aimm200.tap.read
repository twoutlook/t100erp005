      #end add-point  
      
      #將遮罩欄位還原
      CALL aimm200_imao_t_mask_restore('restore_mask_o')
               
      UPDATE imao_t 
         SET (imao001,
              imao002
              ) 
              = 
             (ps_keys[1],ps_keys[2]
              ) 
         WHERE imaoent = g_enterprise AND imao001 = ps_keys_bak[1] AND imao002 = ps_keys_bak[2]
      #add-point:update_b段修改中
      {<point name="update_b.m_update4"/>}
      #end add-point  
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
