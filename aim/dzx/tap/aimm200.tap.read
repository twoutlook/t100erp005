  <point name="reproduce.head.a_insert" order="" ver="3" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   LET l_flag = ""
   IF NOT cl_null(g_imaa_m.ooff013) THEN
      CALL s_aooi360_gen('3',l_master.imaa001,'','','','','','','','','','4',g_imaa_m.ooff013) RETURNING l_success
      IF l_success = "FALSE" THEN
         LET l_flag = 'N'
      END IF
   ELSE
      CALL s_aooi360_del('3',l_master.imaa001,'','','','','','','','','','4') RETURNING l_success
      IF l_success = "FALSE" THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "ooff_t"
         LET g_errparam.popup = FALSE
         CALL cl_err()

         LET l_flag = 'N'  
      END IF                                      
   END IF 
   CALL s_aimm200_ins_item_site(l_master.imaa001,'1') RETURNING l_success,g_errno
   IF NOT l_success THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = g_errno
      LET g_errparam.extend = l_master.imaa001
      LET g_errparam.popup = TRUE
      CALL cl_err()

      LET l_flag = 'N'
   END IF
   SELECT imca003 INTO g_imaa_m.imac003 FROM imca_t WHERE imcaent = g_enterprise AND imca001 = l_master.imaa003 
   INSERT INTO imac_t (imacent,imac001,imac002,imac003) VALUES (g_enterprise,l_master.imaa001,'N',g_imaa_m.imac003)
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend = "imac_t"
      LET g_errparam.popup = FALSE
      CALL cl_err()

      LET l_flag = 'N'
   END IF 
   
   IF l_flag = 'N' THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF                   ]]>
