                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
         
         #----<<imchcnfdt>>----
         
         #----<<imchpstdt>>----
 
 
      
                  #此段落由子樣板a01產生
         BEFORE FIELD imchstus
            #add-point:BEFORE FIELD imchstus
            {<point name="query.b.page1.imchstus" />}
