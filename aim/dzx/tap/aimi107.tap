            #add-point:AFTER FIELD imchsite
            {<point name="query.a.page1.imchsite" />}
            #END add-point
            
 
         #Ctrlp:query.c.page1.imchsite
         ON ACTION controlp INFIELD imchsite
            #add-point:ON ACTION controlp INFIELD imchsite
            {<point name="query.c.page1.imchsite" />}
