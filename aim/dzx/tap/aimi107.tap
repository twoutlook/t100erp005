   LET g_qryparam.state = "c"
   
   #wc備份
   LET ls_wc = g_wc2
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      CONSTRUCT g_wc2 ON imchstus,imchsite,imch011,imch013,imch014,imch015,imchownid,imchowndp,imchcrtid, 
          imchcrtdp,imchcrtdt,imchmodid,imchmoddt 
 
