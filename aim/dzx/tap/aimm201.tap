  <point name="function.aimm201_b_imaa003_ref" order="1" ver="2" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#+
PRIVATE FUNCTION aimm201_b_imaa003_ref(p_imaa003)
DEFINE p_imaa003      LIKE imaa_t.imaa003
DEFINE r_imaa003_desc LIKE imckl_t.imckl003


      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = p_imaa003
      CALL ap_ref_array2(g_ref_fields,"SELECT imckl003 FROM imckl_t WHERE imcklent='"||g_enterprise||"' AND imckl001=? AND imckl002='"||g_lang||"'","") RETURNING g_rtn_fields
      LET r_imaa003_desc = '',g_rtn_fields[1],''
      RETURN r_imaa003_desc
      
END FUNCTION]]>
