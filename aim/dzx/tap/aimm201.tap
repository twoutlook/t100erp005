  <point name="function.aimm201_imed_show" order="15" ver="11" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[
#顯示產品檢核類型
PRIVATE FUNCTION aimm201_imed_show()

        #產品
        SELECT imed004 INTO g_imaf_m.imed004_1 FROM imed_t 
           WHERE imedent = g_enterprise AND imed001 = g_site AND imed002 = g_imaf_m.imaa003 AND imed003 = '1'
        
        #庫存
        SELECT imed004 INTO g_imaf_m.imed004_2 FROM imed_t 
           WHERE imedent = g_enterprise AND imed001 = g_site AND imed002 = g_imaf_m.imaa003 AND imed003 = '2'
        
        #銷售
        SELECT imed004 INTO g_imaf_m.imed004_3 FROM imed_t 
           WHERE imedent = g_enterprise AND imed001 = g_site AND imed002 = g_imaf_m.imaa003 AND imed003 = '3'
        
        #採購
        SELECT imed004 INTO g_imaf_m.imed004_4 FROM imed_t 
           WHERE imedent = g_enterprise AND imed001 = g_site AND imed002 = g_imaf_m.imaa003 AND imed003 = '4'
           
        #生管
        SELECT imed004 INTO g_imaf_m.imed004_5 FROM imed_t 
           WHERE imedent = g_enterprise AND imed001 = g_site AND imed002 = g_imaf_m.imaa003 AND imed003 = '5'
           
        #品管
        SELECT imed004 INTO g_imaf_m.imed004_6 FROM imed_t 
           WHERE imedent = g_enterprise AND imed001 = g_site AND imed002 = g_imaf_m.imaa003 AND imed003 = '6'
           
        #成本
        SELECT imed004 INTO g_imaf_m.imed004_7 FROM imed_t 
           WHERE imedent = g_enterprise AND imed001 = g_site AND imed002 = g_imaf_m.imaa003 AND imed003 = '7'
           
        #關務
        SELECT imed004 INTO g_imaf_m.imed004_8 FROM imed_t 
           WHERE imedent = g_enterprise AND imed001 = g_site AND imed002 = g_imaf_m.imaa003 AND imed003 = '8'
           
        DISPLAY BY NAME g_imaf_m.imed004_1,g_imaf_m.imed004_2,g_imaf_m.imed004_3,g_imaf_m.imed004_4,
                        g_imaf_m.imed004_5,g_imaf_m.imed004_6,g_imaf_m.imed004_7,g_imaf_m.imed004_8
END FUNCTION]]>
