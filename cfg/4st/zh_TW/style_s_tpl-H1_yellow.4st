  </Style>
  <Style name="Button.btnlinktype">
     <StyleAttribute name="buttonType" value="link" />
     <StyleAttribute name="fontWeight" value="bold" />
     <StyleAttribute name="textDecoration" value="underline" />
  </Style>
  <Style name=":message">
     <StyleAttribute name="backgroundColor" value="#FCFCFC" />
     <StyleAttribute name="fontFamily" value="微軟正黑體,Microsoft JhengHei" />
  </Style>
  <Style name=":error">
     <StyleAttribute name="backgroundColor" value="#FCFCFC" />
     <StyleAttribute name="textColor" value="#D82B50" />
     <StyleAttribute name="fontFamily" value="微軟正黑體,Microsoft JhengHei" />
     <StyleAttribute name="fontWeight" value="bold" />
     <StyleAttribute name="position" value="statustip" />
  </Style>
