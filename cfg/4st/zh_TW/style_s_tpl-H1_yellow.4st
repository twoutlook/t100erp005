  </Style>
  <Style name="DateEdit">
     <StyleAttribute name="buttonIcon" value="16/dateedit.png" />
  </Style>
  <Style name="DateEdit:input">
     <StyleAttribute name="backgroundColor" value="#ffffff" />
  </Style>
  <Style name="SpinEdit:input">
     <StyleAttribute name="backgroundColor" value="#ffffff" />
  </Style>
  <Style name="TimeEdit:input">
     <StyleAttribute name="backgroundColor" value="#ffffff" />
  </Style>
  <Style name=".reference">
     <StyleAttribute name="textColor" value="#008299" />
  </Style>
  <Style name=".extend_reference">
     <StyleAttribute name="textColor" value="#008299" />
  </Style>
  <Style name="Label.scrollgridtype">
     <StyleAttribute name="backgroundColor" value="transparent" />
  </Style>
  <Style name="Label.subparamgroup">
     <StyleAttribute name="textColor" value="#000000" />
     <StyleAttribute name="fontWeight" value="bold" />
  </Style>
  <Style name="ProgressBar">
     <StyleAttribute name="percentageVisible" value="center" />
  </Style>
  <Style name=".required:input:active">
     <StyleAttribute name="backgroundColor" value="#ffdddd" />
  </Style> 
  <Style name=".button_qrystr">
     <StyleAttribute name="buttonType" value="link" />
  </Style>
  <Style name=".textFormat_html">
     <StyleAttribute name="textFormat" value="html" />
  </Style>
  <Style name=".noBorder">
     <StyleAttribute name="border" value="none" />
  </Style>
  <Style name=".menubox">
     <StyleAttribute name="backgroundColor" value="#F0E4C3" />
  </Style>
  <Style name=".overview_titlebg">
     <StyleAttribute name="backgroundImage" value="bg/overview_titlebg_tpl-H1_yellow.png" />
  </Style>
  <Style name=".favorite_titlebg">
     <StyleAttribute name="backgroundImage" value="bg/favorite_titlebg_tpl-H1_yellow.png" />
  </Style>
  <Style name=".s_titlebg">
     <StyleAttribute name="backgroundImage" value="bg/s_titlebg_tpl-H1_yellow.png" />
  </Style>
  <Style name=".title_letter">
