   END IF
   #lori522612  150126 add -----------------------(E)
   
   #150122-00013#2 150304 pomelo add (s)
   
#   LET l_join_sql = " "        #150614-00021#1 20150618 mark by beckxie
#   IF g_argv[01] = '2' THEN    #150614-00021#1 20150618 mark by beckxie
      LET l_join_sql = " LEFT OUTER JOIN imaf_t ON imafent = imaaent",
                       "                       AND imafsite = 'ALL'",
                       "                       AND imaf001 = imaa001 "
#   END IF                      #150614-00021#1 20150618 mark by beckxie
   #150122-00013#2 150304 pomelo add (e)
   #end add-point
   
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   LET l_wc  = g_wc.trim() 
   LET l_wc2 = g_wc2.trim()
 
   #add-point:browser_fill,foreach前 name="browser_fill.before_foreach"
 
