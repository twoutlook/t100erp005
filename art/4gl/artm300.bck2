DEFINE l_chk     LIKE type_t.chr1
DEFINE i         LIKE type_t.num5
DEFINE j         LIKE type_t.num5
DEFINE l_num     DYNAMIC ARRAY OF RECORD
       number    LIKE type_t.num5
                 END RECORD
   LET g_errno = NULL

   IF g_imay_d[l_ac].imay002 = '1' THEN
      IF LENGTH(g_imay_d[l_ac].imay003) <> 8 AND LENGTH(g_imay_d[l_ac].imay003) <> 12
         AND LENGTH(g_imay_d[l_ac].imay003) <> 13 THEN
         LET g_errno = "art-00381"
         RETURN
      END IF
      
      #校驗位檢查
      LET l_start = 1
      LET l_end = LENGTH(g_imay_d[l_ac].imay003) - 1
      LET j = 1
      FOR i = l_end TO l_start Step -1
         LET l_num[j].number = g_imay_d[l_ac].imay003[i,i]
         LET j=j+1
      END FOR
      #開頭不能是2
      IF l_num[j-1].number = '2' THEN
         LET g_errno = "art-00383"
         RETURN
      END IF
      #奇數和
      FOR i=1 TO (l_end/2)
         IF i*2 > j-1 THEN
            EXIT FOR
         END IF
         LET l_n1=l_n1+l_num[i*2].number
      END FOR
      #偶數和
      FOR i=0 TO (l_end/2)
         IF i*2+1 > j-1 THEN
            EXIT FOR
         END IF
         LET l_n2=l_n2+l_num[i*2+1].number
      END FOR
      LET l_n3= l_n2*3
      LET l_sum= l_n1+l_n3
      LET l_mod = l_sum MOD 10
      LET l_sum = 10-l_mod
      LET l_str1 = l_sum
      LET l_len = LENGTH(g_imay_d[l_ac].imay003)
      LET l_sum = g_imay_d[l_ac].imay003[l_len,l_len]
      LET l_chk = l_sum
      IF l_chk <> l_str1 THEN
         LET g_errno = "art-00382"
         RETURN
      END IF
   END IF

END FUNCTION

