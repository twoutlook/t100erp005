PUBLIC FUNCTION artm300_upd_imaz()
   DEFINE l_cnt    LIKE type_t.num5
   
   #如果多條碼同時存在2筆以上的條碼資料時,將imaz_t裡 有用這樣子的條碼資料都update成null
   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt FROM imay_t
    WHERE imay001 = g_imaa_m.imaa001
      AND imay003 = g_imay_d[l_ac].imay003
      AND imayent = g_enterprise
      AND imaystus = 'Y'
   IF l_cnt > 1 THEN
      UPDATE imaz_t SET imaz003 = ''
       WHERE imaz001 = g_imaa_m.imaa001
         AND imazent = g_enterprise
         AND imaz003 = g_imay_d[l_ac].imay003
   ELSE
      IF l_cnt = 0 THEN      
         SELECT COUNT(*) INTO l_cnt FROM imay_t
          WHERE imay001 = g_imaa_m.imaa001
            AND imayent = g_enterprise
            AND imay003 = g_imay_d_t.imay003
            AND imaystus = 'Y'
         IF l_cnt = 0 THEN
            UPDATE imaz_t SET imaz003 = ''
             WHERE imaz001 = g_imaa_m.imaa001
               AND imazent = g_enterprise
               AND imaz003 = g_imay_d_t.imay003
         END IF
      END IF
   END IF
   
   #單imay_t的條碼資料改變,也一併將imaz_t中有使用imay_t的舊值資料的條碼改變
   IF cl_null(g_imay_d[l_ac].imay003) OR g_imay_d[l_ac].imay003 != g_imay_d_t.imay003 THEN
      IF NOT cl_null(g_imay_d_t.imay003) THEN
         UPDATE imaz_t SET imaz003 = g_imay_d[l_ac].imay003
          WHERE imaz001 = g_imaa_m.imaa001
            AND imazent = g_enterprise
            AND imaz003 = g_imay_d_t.imay003
      END IF  
   END IF
   IF g_imay_d[l_ac].imaystus = 'N' THEN
      UPDATE imaz_t SET imaz003 = ''
       WHERE imaz001 = g_imaa_m.imaa001
         AND imazent = g_enterprise
         AND imaz003 = g_imay_d[l_ac].imay003
   ELSE
      #將imaz_t 包裝單位與整包裝數資料 與imay_t的資料同步
      UPDATE imaz_t SET imaz004 = g_imay_d[l_ac].imay004,
                        imaz005 = g_imay_d[l_ac].imay005
       WHERE imaz001 = g_imaa_m.imaa001
         AND imazent = g_enterprise
         AND imaz003 = g_imay_d[l_ac].imay003
         
      UPDATE imaz_t SET imaz005 = g_imay_d[l_ac].imay005
       WHERE imaz001 = g_imaa_m.imaa001
         AND imazent = g_enterprise
         AND imaz004 = g_imay_d[l_ac].imay004  
   END IF   
END FUNCTION
