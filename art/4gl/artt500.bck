   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO rtik_detail SELECT * FROM rtik_t 
                         WHERE rtikent = g_enterprise AND rtikdocno = g_rtil_m.rtil041

   UPDATE rtik_detail 
      SET rtikdocno = g_rtil_m.rtildocno,
          rtiksite  = g_rtil_m.rtilsite,
