PRIVATE FUNCTION artt500_ins_rtim()
DEFINE l_sql   STRING 

   LET l_sql = "CREATE GLOBAL TEMPORARY TABLE rtim_detail AS ",
                "SELECT * FROM rtim_t "
   PREPARE rtim_tbl FROM l_sql
   EXECUTE rtim_tbl
   FREE rtim_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO rtim_detail SELECT * FROM rtim_t 
                         WHERE rtiment = g_enterprise AND rtimdocno = g_rtil_m.rtil041

   UPDATE rtim_detail 
      SET rtimdocno = g_rtil_m.rtildocno,
          rtimsite  = g_rtil_m.rtilsite,
          rtimunit  = g_rtil_m.rtilsite,
          rtimorga  = g_rtil_m.rtilsite
      
   INSERT INTO rtim_t SELECT * FROM rtim_detail
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "upd rtim_detail" 
      LET g_errparam.code   = SQLCA.sqlcode 
