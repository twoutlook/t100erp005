################################################################################
# Descriptions...: 確認產品組的條件編號(品類/品牌)是否符合條件
# Memo...........:
# Usage..........: CALL arti305_chk_imck143(p_type)
# Input parameter: p_type    1.imck143 2.其他欄位檢查
# Return code....: 無
# Date & Author..: 2014/04/30 By pomelo
# Modify.........:
################################################################################
PUBLIC FUNCTION arti305_chk_imck143(p_type)
DEFINE p_type          LIKE type_t.chr1
DEFINE r_success       LIKE type_t.num5
DEFINE r_errno         LIKE type_t.chr10
DEFINE r_rtax001       LIKE rtax_t.rtax001
DEFINE l_dbbastus      LIKE dbba_t.dbbastus
DEFINE l_dbbbstus      LIKE dbbb_t.dbbbstus
   
   LET g_errno = ''
   IF p_type = '2' THEN
      LET l_dbbastus = ''
      SELECT dbbastus INTO l_dbbastus
        FROM dbba_t
       WHERE dbbaent = g_enterprise
         AND dbba001 = g_imck_m.imck143
      CASE
         WHEN SQLCA.sqlcode = 100
            LET g_errno = 'adb-00030'
            RETURN
         WHEN l_dbbastus = 'N'
            LET g_errno = 'adb-00031'
            RETURN
      END CASE
   END IF
   
   IF NOT cl_null(g_imck_m.imck009) THEN
      LET r_success = ''
      LET r_errno = ''
      LET r_rtax001 = ''
      CALL s_arti202_search_manage_level(g_imck_m.imck009) 
         RETURNING r_success,r_errno,r_rtax001
      IF r_success = FALSE THEN LET r_rtax001 = '' END IF
   END IF
   
   LET l_dbbbstus = ''
   SELECT dbbbstus INTO l_dbbbstus
     FROM dbbb_t
    WHERE dbbbent = g_enterprise
      AND dbbb001 = g_imck_m.imck143
      AND ((dbbb002 = '1' AND dbbb003 = r_rtax001)
       OR ( dbbb002 = '2' AND dbbb003 = g_imck_m.imck126))
   
   CASE
      WHEN SQLCA.sqlcode = 100
         #此產品組下沒有符合條件的資料！
         LET g_errno = 'adb-00037'
      WHEN l_dbbbstus = 'N'
         LET g_errno = 'adb-00038'
   END CASE
END FUNCTION

