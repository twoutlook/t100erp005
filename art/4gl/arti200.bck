{<section id="arti200.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION arti200_b_fill(p_wc2)              #BODY FILL UP
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   
   #end add-point
   DEFINE p_wc2    STRING
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   
   #end add-point
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前 name="b_fill.sql_before"
   
   #end add-point
 
   LET g_sql = "SELECT  DISTINCT t0.rtaistus,t0.rtai001,t0.rtai002,t0.rtai003,t0.rtai004,t0.rtai005, 
       t0.rtai006,t0.rtai001,t0.rtai003,t0.rtaiownid,t0.rtaiowndp,t0.rtaicrtid,t0.rtaicrtdp,t0.rtaicrtdt, 
       t0.rtaimodid,t0.rtaimoddt ,t1.ooall004 ,t2.gzzol003 ,t3.gzzal003 ,t4.oobxl003 ,t5.oobxl003 ,t6.oobxl003 , 
       t7.ooag011 ,t8.ooefl003 ,t9.ooag011 ,t10.ooefl003 ,t11.ooag011 FROM rtai_t t0",
               "",
                              " LEFT JOIN ooall_t t1 ON t1.ooallent="||g_enterprise||" AND t1.ooall001='3' AND t1.ooall002=t0.rtai001 AND t1.ooall003='"||g_dlang||"' ",
               " LEFT JOIN gzzol_t t2 ON t2.gzzol001=t0.rtai002 AND t2.gzzol002='"||g_lang||"' ",
               " LEFT JOIN gzzal_t t3 ON t3.gzzal001=t0.rtai003 AND t3.gzzal002='"||g_lang||"' ",
               " LEFT JOIN oobxl_t t4 ON t4.oobxlent="||g_enterprise||" AND t4.oobxl001=t0.rtai004 AND t4.oobxl002='"||g_dlang||"' ",
               " LEFT JOIN oobxl_t t5 ON t5.oobxlent="||g_enterprise||" AND t5.oobxl001=t0.rtai005 AND t5.oobxl002='"||g_dlang||"' ",
               " LEFT JOIN oobxl_t t6 ON t6.oobxlent="||g_enterprise||" AND t6.oobxl001=t0.rtai006 AND t6.oobxl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent="||g_enterprise||" AND t7.ooag001=t0.rtaiownid  ",
               " LEFT JOIN ooefl_t t8 ON t8.ooeflent="||g_enterprise||" AND t8.ooefl001=t0.rtaiowndp AND t8.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t9 ON t9.ooagent="||g_enterprise||" AND t9.ooag001=t0.rtaicrtid  ",
               " LEFT JOIN ooefl_t t10 ON t10.ooeflent="||g_enterprise||" AND t10.ooefl001=t0.rtaicrtdp AND t10.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t11 ON t11.ooagent="||g_enterprise||" AND t11.ooag001=t0.rtaimodid  ",
 
               " WHERE t0.rtaient= ?  AND  1=1 AND (", p_wc2, ") " 
   #add-point:b_fill段sql wc name="b_fill.sql_wc"
   
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("rtai_t"),
                      " ORDER BY t0.rtai001,t0.rtai003"
   
   #add-point:b_fill段sql之後 name="b_fill.sql_after"
   
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"rtai_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE arti200_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR arti200_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_rtai_d.clear()
   CALL g_rtai2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_rtai_d[l_ac].rtaistus,g_rtai_d[l_ac].rtai001,g_rtai_d[l_ac].rtai002,g_rtai_d[l_ac].rtai003, 
       g_rtai_d[l_ac].rtai004,g_rtai_d[l_ac].rtai005,g_rtai_d[l_ac].rtai006,g_rtai2_d[l_ac].rtai001, 
       g_rtai2_d[l_ac].rtai003,g_rtai2_d[l_ac].rtaiownid,g_rtai2_d[l_ac].rtaiowndp,g_rtai2_d[l_ac].rtaicrtid, 
       g_rtai2_d[l_ac].rtaicrtdp,g_rtai2_d[l_ac].rtaicrtdt,g_rtai2_d[l_ac].rtaimodid,g_rtai2_d[l_ac].rtaimoddt, 
       g_rtai_d[l_ac].rtai001_desc,g_rtai_d[l_ac].rtai002_desc,g_rtai_d[l_ac].rtai003_desc,g_rtai_d[l_ac].rtai004_desc, 
       g_rtai_d[l_ac].rtai005_desc,g_rtai_d[l_ac].rtai006_desc,g_rtai2_d[l_ac].rtaiownid_desc,g_rtai2_d[l_ac].rtaiowndp_desc, 
       g_rtai2_d[l_ac].rtaicrtid_desc,g_rtai2_d[l_ac].rtaicrtdp_desc,g_rtai2_d[l_ac].rtaimodid_desc
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充 name="b_fill.fill"
      
      #end add-point
      
      CALL arti200_detail_show()      
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
 
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   LET g_error_show = 0
   
 
   
   CALL g_rtai_d.deleteElement(g_rtai_d.getLength())   
   CALL g_rtai2_d.deleteElement(g_rtai2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_rtai_d.getLength()
