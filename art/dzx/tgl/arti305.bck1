{<section id="arti305.fetch" >}
#+ 指定PK後抓取單頭其他資料
PRIVATE FUNCTION arti305_fetch(p_fl)
   #add-point:fetch段define(客製用) name="fetch.define_customerization"
   {<point name="fetch.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE p_fl       LIKE type_t.chr1
   DEFINE ls_msg     STRING
   #add-point:fetch段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="fetch.define"
   {<point name="fetch.define"/>}
   #end add-point  
   
   #add-point:Function前置處理  name="fetch.pre_function"
   {<point name="fetch.pre_function"/>}
   #end add-point
   
   #根據傳入的條件決定抓取的資料
   CASE p_fl
      WHEN "F" 
         LET g_current_idx = 1
      WHEN "P"
         IF g_current_idx > 1 THEN               
            LET g_current_idx = g_current_idx - 1
         END IF 
      WHEN "N"
         IF g_current_idx < g_header_cnt THEN
            LET g_current_idx =  g_current_idx + 1
         END IF        
      WHEN "L" 
         #LET g_current_idx = g_header_cnt        
         LET g_current_idx = g_browser.getLength()    
      WHEN "/"
         #詢問要指定的筆數
         IF (NOT g_no_ask) THEN      
            CALL cl_getmsg("fetch", g_lang) RETURNING ls_msg
            LET INT_FLAG = 0
 
            PROMPT ls_msg CLIPPED,": " FOR g_jump
               #交談指令共用ACTION
               &include "common_action.4gl"
            END PROMPT
            
            IF INT_FLAG THEN
               LET INT_FLAG = 0
               EXIT CASE  
            END IF           
         END IF
         IF g_jump > 0 THEN
            LET g_current_idx = g_jump
         END IF
         LET g_no_ask = FALSE     
   END CASE
 
   #筆數顯示
   LET g_browser_idx = g_current_idx 
   IF g_browser_cnt > 0 THEN
      DISPLAY g_browser_idx TO FORMONLY.b_index #當下筆數
      DISPLAY g_browser_cnt TO FORMONLY.b_count #總筆數
      DISPLAY g_browser_idx TO FORMONLY.h_index #當下筆數
      DISPLAY g_browser_cnt TO FORMONLY.h_count #總筆數
   ELSE
      DISPLAY '' TO FORMONLY.b_index #當下筆數
      DISPLAY '' TO FORMONLY.b_count #總筆數
      DISPLAY '' TO FORMONLY.h_index #當下筆數
      DISPLAY '' TO FORMONLY.h_count #總筆數
   END IF
   
   
   
   #避免超出browser資料筆數上限
   IF g_current_idx > g_browser.getLength() THEN
      LET g_browser_idx = g_browser.getLength()
      LET g_current_idx = g_browser.getLength() 
   END IF
   
   # 設定browse索引
   CALL cl_navigator_setting(g_browser_idx, g_browser_cnt) 
 
   #代表沒有資料, 無需做後續資料撈取之動作
   IF g_current_idx = 0 THEN
      RETURN
   END IF
 
   #根據選定的筆數給予key欄位值
   LET g_imck_m.imck001 = g_browser[g_current_idx].b_imck001
 
                       
   #讀取單頭所有欄位資料
   EXECUTE arti305_master_referesh USING g_imck_m.imck001 INTO g_imck_m.imckunit,g_imck_m.imck001,g_imck_m.imck108, 
       g_imck_m.imck004,g_imck_m.imck100,g_imck_m.imck109,g_imck_m.imck006,g_imck_m.imck105,g_imck_m.imck104, 
       g_imck_m.imck107,g_imck_m.imck106,g_imck_m.imck145,g_imck_m.imck146,g_imck_m.imck113,g_imck_m.imckstus, 
       g_imck_m.imckownid,g_imck_m.imckowndp,g_imck_m.imckcrtid,g_imck_m.imckcrtdp,g_imck_m.imckcrtdt, 
       g_imck_m.imckmodid,g_imck_m.imckmoddt,g_imck_m.imck009,g_imck_m.imck101,g_imck_m.imck102,g_imck_m.imck103, 
       g_imck_m.imck118,g_imck_m.imck119,g_imck_m.imck120,g_imck_m.imck114,g_imck_m.imck124,g_imck_m.imck125, 
       g_imck_m.imck121,g_imck_m.imck144,g_imck_m.imck110,g_imck_m.imck111,g_imck_m.imck112,g_imck_m.imck122, 
       g_imck_m.imck123,g_imck_m.imck131,g_imck_m.imck126,g_imck_m.imck127,g_imck_m.imck128,g_imck_m.imck129, 
       g_imck_m.imck130,g_imck_m.imck143,g_imck_m.imck132,g_imck_m.imck133,g_imck_m.imck134,g_imck_m.imck135, 
       g_imck_m.imck136,g_imck_m.imck137,g_imck_m.imck138,g_imck_m.imck139,g_imck_m.imck140,g_imck_m.imck141, 
       g_imck_m.imckunit_desc,g_imck_m.imck006_desc,g_imck_m.imck105_desc,g_imck_m.imck104_desc,g_imck_m.imck107_desc, 
       g_imck_m.imck106_desc,g_imck_m.imck145_desc,g_imck_m.imck146_desc,g_imck_m.imckownid_desc,g_imck_m.imckowndp_desc, 
       g_imck_m.imckcrtid_desc,g_imck_m.imckcrtdp_desc,g_imck_m.imckmodid_desc,g_imck_m.imck009_desc, 
       g_imck_m.imck101_desc,g_imck_m.imck114_desc,g_imck_m.imck124_desc,g_imck_m.imck122_desc,g_imck_m.imck131_desc, 
       g_imck_m.imck126_desc,g_imck_m.imck127_desc,g_imck_m.imck128_desc,g_imck_m.imck129_desc,g_imck_m.imck143_desc, 
       g_imck_m.imck132_desc,g_imck_m.imck133_desc,g_imck_m.imck134_desc,g_imck_m.imck135_desc,g_imck_m.imck136_desc, 
       g_imck_m.imck137_desc,g_imck_m.imck138_desc,g_imck_m.imck139_desc,g_imck_m.imck140_desc,g_imck_m.imck141_desc 

   
   #遮罩相關處理
   LET g_imck_m_mask_o.* =  g_imck_m.*
   CALL arti305_imck_t_mask()
   LET g_imck_m_mask_n.* =  g_imck_m.*
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL arti305_set_act_visible()
   CALL arti305_set_act_no_visible()
 
   #add-point:fetch段action控制 name="fetch.action_control"
   {<point name="fetch.action_control"/>}
   #end add-point  
   
   
   
   #保存單頭舊值
   LET g_imck_m_t.* = g_imck_m.*
   LET g_imck_m_o.* = g_imck_m.*
   
   LET g_data_owner = g_imck_m.imckownid      
   LET g_data_dept  = g_imck_m.imckowndp
   
   #重新顯示
   CALL arti305_show()
   
 
END FUNCTION

