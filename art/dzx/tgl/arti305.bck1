{<section id="arti305.insert" >}
#+ 資料新增
PRIVATE FUNCTION arti305_insert()
   #add-point:insert段define(客製用) name="insert.define_customerization"
   {<point name="insert.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   #add-point:insert段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="insert.define"
   {<point name="insert.define"/>}
   #end add-point    
   
   #add-point:Function前置處理  name="insert.pre_function"
   {<point name="insert.pre_function"/>}
   #end add-point
   
   CLEAR FORM #清畫面欄位內容
   INITIALIZE g_imck_m.* LIKE imck_t.*             #DEFAULT 設定
   LET g_imck001_t = NULL
 
   
   #add-point:insert段before name="insert.before"
   {<point name="insert.before"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   
   WHILE TRUE
      
      #公用欄位給值
      #應用 a14 樣板自動產生(Version:5)    
      #公用欄位新增給值  
      LET g_imck_m.imckownid = g_user
      LET g_imck_m.imckowndp = g_dept
      LET g_imck_m.imckcrtid = g_user
      LET g_imck_m.imckcrtdp = g_dept 
      LET g_imck_m.imckcrtdt = cl_get_current()
      LET g_imck_m.imckmodid = g_user
      LET g_imck_m.imckmoddt = cl_get_current()
      LET g_imck_m.imckstus = 'Y'
 
 
 
 
      #append欄位給值
      
     
      #一般欄位給值
            LET g_imck_m.imck108 = "1"
      LET g_imck_m.imck004 = "M"
      LET g_imck_m.imck100 = "1"
      LET g_imck_m.imck109 = "1"
      LET g_imck_m.imck113 = "1"
      LET g_imck_m.imckstus = "Y"
      LET g_imck_m.imck102 = "0"
      LET g_imck_m.imck103 = "0"
      LET g_imck_m.imck118 = "0"
      LET g_imck_m.imck119 = "0"
      LET g_imck_m.imck120 = "0"
      LET g_imck_m.imck125 = "N"
      LET g_imck_m.imck121 = "N"
      LET g_imck_m.imck144 = "N"
      LET g_imck_m.imck110 = "N"
 
 
      #add-point:單頭預設值 name="insert.default"
      {<point name="insert.default"/>}
      #end add-point   
     
      #顯示狀態(stus)圖片
            #應用 a21 樣板自動產生(Version:3)
	  #根據當下狀態碼顯示圖片
      CASE g_imck_m.imckstus 
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
         
      END CASE
 
 
 
     
      #資料輸入
      CALL arti305_input("a")
      
      #add-point:單頭輸入後 name="insert.after_insert"
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG THEN
         #取消
         LET INT_FLAG = 0
         DISPLAY g_current_cnt TO FORMONLY.h_count     #總筆數
         DISPLAY g_current_idx TO FORMONLY.h_index     #當下筆數
         INITIALIZE g_imck_m.* TO NULL
         CALL arti305_show()
         CALL s_transaction_end('N','0')
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         RETURN
      END IF
 
      LET g_rec_b = 0
      EXIT WHILE
