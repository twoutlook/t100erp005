   LET g_imck001_t = g_imck_m.imck001
 
   
   LET g_master_multi_table_t.imckl001 = g_imck_m.imck001
LET g_master_multi_table_t.imckl003 = g_imck_m.imckl003
LET g_master_multi_table_t.imckl004 = g_imck_m.imckl004
 
 
