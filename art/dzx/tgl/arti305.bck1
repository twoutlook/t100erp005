       g_imck_m.imck118,g_imck_m.imck119,g_imck_m.imck120,g_imck_m.imck114,g_imck_m.imck124,g_imck_m.imck125, 
       g_imck_m.imck121,g_imck_m.imck144,g_imck_m.imck110,g_imck_m.imck111,g_imck_m.imck112,g_imck_m.imck122, 
       g_imck_m.imck123,g_imck_m.imck131,g_imck_m.imck126,g_imck_m.imck127,g_imck_m.imck128,g_imck_m.imck129, 
       g_imck_m.imck130,g_imck_m.imck143,g_imck_m.imck132,g_imck_m.imck133,g_imck_m.imck134,g_imck_m.imck135, 
       g_imck_m.imck136,g_imck_m.imck137,g_imck_m.imck138,g_imck_m.imck139,g_imck_m.imck140,g_imck_m.imck141, 
       g_imck_m.imckunit_desc,g_imck_m.imck006_desc,g_imck_m.imck105_desc,g_imck_m.imck104_desc,g_imck_m.imck107_desc, 
       g_imck_m.imck106_desc,g_imck_m.imck145_desc,g_imck_m.imck146_desc,g_imck_m.imckownid_desc,g_imck_m.imckowndp_desc, 
       g_imck_m.imckcrtid_desc,g_imck_m.imckcrtdp_desc,g_imck_m.imckmodid_desc,g_imck_m.imck009_desc, 
       g_imck_m.imck101_desc,g_imck_m.imck114_desc,g_imck_m.imck124_desc,g_imck_m.imck122_desc,g_imck_m.imck131_desc, 
       g_imck_m.imck126_desc,g_imck_m.imck127_desc,g_imck_m.imck128_desc,g_imck_m.imck129_desc,g_imck_m.imck143_desc, 
       g_imck_m.imck132_desc,g_imck_m.imck133_desc,g_imck_m.imck134_desc,g_imck_m.imck135_desc,g_imck_m.imck136_desc, 
       g_imck_m.imck137_desc,g_imck_m.imck138_desc,g_imck_m.imck139_desc,g_imck_m.imck140_desc,g_imck_m.imck141_desc 

   
   
   #檢查是否允許此動作
   IF NOT arti305_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_imck_m_mask_o.* =  g_imck_m.*
   CALL arti305_imck_t_mask()
   LET g_imck_m_mask_n.* =  g_imck_m.*
   
   #將最新資料顯示到畫面上
   CALL arti305_show()
   
   IF cl_ask_delete() THEN
 
      #add-point:單頭刪除前 name="delete.head.b_delete"
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point
 
      #應用 a47 樣板自動產生(Version:4)
      #刪除相關文件
      CALL arti305_set_pk_array()
      #add-point:相關文件刪除前 name="delete.befroe.related_document_remove"
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
 
 
      DELETE FROM imck_t 
       WHERE imckent = g_enterprise AND imck001 = g_imck_m.imck001 
 
 
      #add-point:單頭刪除中 name="delete.head.m_delete"
      {<point name="delete.head.m_delete"/>}
      #end add-point
         
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "imck_t:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = FALSE 
