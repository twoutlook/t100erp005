   EXECUTE artm300_master_referesh USING g_imaa_m.imaa001 INTO g_imaa_m.imaa001,g_imaa_m.imaa003,g_imaa_m.imaa108, 
       g_imaa_m.imaa004,g_imaa_m.imaa100,g_imaa_m.imaa109,g_imaa_m.imaa014,g_imaa_m.imaa005,g_imaa_m.imaa002, 
       g_imaa_m.imaa006,g_imaa_m.imaa105,g_imaa_m.imaa104,g_imaa_m.imaa107,g_imaa_m.imaa106,g_imaa_m.imaa145, 
       g_imaa_m.imaa146,g_imaa_m.imaa113,g_imaa_m.imaa142,g_imaa_m.imaa019,g_imaa_m.imaa020,g_imaa_m.imaa021, 
