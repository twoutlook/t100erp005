                  "  LEFT JOIN imaz_t ON imazent = imaaent AND imaa001 = imaz001", "  ", 
                  #add-point:browser_fill段sql(imaz_t1) name="browser_fill.join.imaz_t1"
                  {<point name="browser_fill.join.imaz_t1"/>}
                  #end add-point
 
                  "  LEFT JOIN imao_t ON imaoent = imaaent AND imaa001 = imao001", "  ", 
