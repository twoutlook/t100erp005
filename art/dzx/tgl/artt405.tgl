   #end add-point
   
   
   
   IF g_bfill = "Y" THEN
      CALL artt405_b_fill() #單身填充
      CALL artt405_b_fill2('0') #單身填充
   END IF
     
   #帶出公用欄位reference值
   #應用 a12 樣板自動產生(Version:4)
 
 
 
   
