       g_rtdu_m.rtduowndp_desc,g_rtdu_m.rtducrtid_desc,g_rtdu_m.rtducrtdp_desc,g_rtdu_m.rtdumodid_desc, 
       g_rtdu_m.rtducnfid_desc
   
   
