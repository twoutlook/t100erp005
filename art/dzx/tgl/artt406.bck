   LET g_forupd_sql = " SELECT rtdrsite,'',rtdrdocdt,rtdrdocno,rtdr000,rtdrunit,rtdr001,'',rtdr002,'', 
       rtdr005,'',rtdr003,'','',rtdr004,rtdrstus,rtdrownid,'',rtdrowndp,'',rtdrcrtid,'',rtdrcrtdp,'', 
