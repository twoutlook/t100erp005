{<section id="artq901.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION artq901_b_fill()
   #add-point:b_fill段define-客製 name="b_fill.define_customerization"
   {<point name="b_fill.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準  (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   {<point name="b_fill.define"/>}
   #end add-point
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter, cl_sql_auth_filter()   #(ver:40) add cl_sql_auth_filter()
 
   LET ls_sql_rank = "SELECT  UNIQUE '',rtcdsite,'',rtcd001,rtcd002,'',rtcd003,'',rtcd004,rtcd005,'',rtcd006,rtcd007,rtcd008,rtcd009,rtcd010,rtcd011,rtcd012,rtcd013,rtcd014,rtcd015,rtcd016,rtcd017,rtcd018,rtcd019,rtcd020,rtcd021,rtcd022,rtcd023,rtcd024,rtcd025,rtcd026,rtcd027,rtcd028,rtcd029,rtcd030,rtcd031,rtcd032,rtcd033,rtcd034,rtcd035,rtcd036,rtcd037,'','','','','','','','',rtcdownid,'',rtcdowndp,'',rtcdcrtid,'',rtcdcrtdp,'',rtcdcrtdt,rtcdmodid,'',rtcdmoddt  ,DENSE_RANK() OVER( ORDER BY rtcd_t.rtcdsite,rtcd_t.rtcd001,rtcd_t.rtcd002,rtcd_t.rtcd003,rtcd_t.rtcd004,rtcd_t.rtcd005) AS RANK FROM rtcd_t",


                     "",
                     " WHERE rtcdent= ? AND 1=1 AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("rtcd_t"),
                     " ORDER BY rtcd_t.rtcdsite,rtcd_t.rtcd001,rtcd_t.rtcd002,rtcd_t.rtcd003,rtcd_t.rtcd004,rtcd_t.rtcd005"
 
   #add-point:b_fill段rank_sql_after name="b_fill.rank_sql_after"
   {<point name="b_fill.rank_sql_after"/>}
   #end add-point
 
   LET g_sql = "SELECT COUNT(1) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   #add-point:b_fill段rank_sql_after_count name="b_fill.rank_sql_after_count"
   {<point name="b_fill.rank_sql_after_count"/>}
   #end add-point
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT '',rtcdsite,'',rtcd001,rtcd002,'',rtcd003,'',rtcd004,rtcd005,'',rtcd006,rtcd007,rtcd008,rtcd009,rtcd010,rtcd011,rtcd012,rtcd013,rtcd014,rtcd015,rtcd016,rtcd017,rtcd018,rtcd019,rtcd020,rtcd021,rtcd022,rtcd023,rtcd024,rtcd025,rtcd026,rtcd027,rtcd028,rtcd029,rtcd030,rtcd031,rtcd032,rtcd033,rtcd034,rtcd035,rtcd036,rtcd037,'','','','','','','','',rtcdownid,'',rtcdowndp,'',rtcdcrtid,'',rtcdcrtdp,'',rtcdcrtdt,rtcdmodid,'',rtcdmoddt",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE artq901_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR artq901_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_rtcd_d.clear()
   CALL g_rtcd2_d.clear()   


   #add-point:陣列清空 name="b_fill.array_clear"
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1   
 
   FOREACH b_fill_curs INTO g_rtcd_d[l_ac].sel,g_rtcd_d[l_ac].rtcdsite,g_rtcd_d[l_ac].rtcdsite_desc,g_rtcd_d[l_ac].rtcd001,g_rtcd_d[l_ac].rtcd002,g_rtcd_d[l_ac].rtcd002_desc,g_rtcd_d[l_ac].rtcd003,g_rtcd_d[l_ac].rtcd003_desc,g_rtcd_d[l_ac].rtcd004,g_rtcd_d[l_ac].rtcd005,g_rtcd_d[l_ac].rtcd005_desc,g_rtcd_d[l_ac].rtcd006,g_rtcd_d[l_ac].rtcd007,g_rtcd_d[l_ac].rtcd008,g_rtcd_d[l_ac].rtcd009,g_rtcd_d[l_ac].rtcd010,g_rtcd_d[l_ac].rtcd011,g_rtcd_d[l_ac].rtcd012,g_rtcd_d[l_ac].rtcd013,g_rtcd_d[l_ac].rtcd014,g_rtcd_d[l_ac].rtcd015,g_rtcd_d[l_ac].rtcd016,g_rtcd_d[l_ac].rtcd017,g_rtcd_d[l_ac].rtcd018,g_rtcd_d[l_ac].rtcd019,g_rtcd_d[l_ac].rtcd020,g_rtcd_d[l_ac].rtcd021,g_rtcd_d[l_ac].rtcd022,g_rtcd_d[l_ac].rtcd023,g_rtcd_d[l_ac].rtcd024,g_rtcd_d[l_ac].rtcd025,g_rtcd_d[l_ac].rtcd026,g_rtcd_d[l_ac].rtcd027,g_rtcd_d[l_ac].rtcd028,g_rtcd_d[l_ac].rtcd029,g_rtcd_d[l_ac].rtcd030,g_rtcd_d[l_ac].rtcd031,g_rtcd_d[l_ac].rtcd032,g_rtcd_d[l_ac].rtcd033,g_rtcd_d[l_ac].rtcd034,g_rtcd_d[l_ac].rtcd035,g_rtcd_d[l_ac].rtcd036,g_rtcd_d[l_ac].rtcd037,g_rtcd2_d[l_ac].rtcd001,g_rtcd2_d[l_ac].rtcd002,g_rtcd2_d[l_ac].rtcd002_2_desc,g_rtcd2_d[l_ac].rtcd003,g_rtcd2_d[l_ac].rtcd003_2_desc,g_rtcd2_d[l_ac].rtcd004,g_rtcd2_d[l_ac].rtcd005,g_rtcd2_d[l_ac].rtcd005_2_desc,g_rtcd2_d[l_ac].rtcdownid,g_rtcd2_d[l_ac].rtcdownid_desc,g_rtcd2_d[l_ac].rtcdowndp,g_rtcd2_d[l_ac].rtcdowndp_desc,g_rtcd2_d[l_ac].rtcdcrtid,g_rtcd2_d[l_ac].rtcdcrtid_desc,g_rtcd2_d[l_ac].rtcdcrtdp,g_rtcd2_d[l_ac].rtcdcrtdp_desc,g_rtcd2_d[l_ac].rtcdcrtdt,g_rtcd2_d[l_ac].rtcdmodid,g_rtcd2_d[l_ac].rtcdmodid_desc,g_rtcd2_d[l_ac].rtcdmoddt
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code = SQLCA.SQLCODE 
         LET g_errparam.popup = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #LET g_rtcd_d[l_ac].statepic = cl_get_actipic(g_rtcd_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充 name="b_fill.fill"
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL artq901_detail_show("'1'")      
 
      CALL artq901_rtcd_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "" 
            LET g_errparam.code = 9035 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
   LET g_error_show = 0
   

   
   CALL g_rtcd_d.deleteElement(g_rtcd_d.getLength())   
   CALL g_rtcd2_d.deleteElement(g_rtcd2_d.getLength())


   #add-point:陣列長度調整 name="b_fill.array_deleteElement"
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
   
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = g_rtcd_d.getLength()
#  DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE artq901_pb
 
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL artq901_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL artq901_detail_action_trans()
 
   IF g_rtcd_d.getLength() > 0 THEN
      LET l_ac = 1
      CALL artq901_fetch()
   END IF
   
      CALL artq901_filter_show('rtcdsite','b_rtcdsite')
   CALL artq901_filter_show('rtcd001','b_rtcd001')
