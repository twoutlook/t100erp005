               " LEFT JOIN rtaxl_t t3 ON t3.rtaxlent="||g_enterprise||" AND t3.rtaxl001=inaa106 AND t3.rtaxl002='"||g_dlang||"' ",
               " LEFT JOIN mhadl_t t4 ON t4.mhadlent="||g_enterprise||" AND t4.mhadl001=inaa124 AND t4.mhadl002=inaa123 AND t4.mhadl003=inaa122 AND t4.mhadl004=inaa121 AND t4.mhadl005='"||g_dlang||"' ",
