   #  OPEN b_fill_cs USING g_enterprise,g_inaa_m.inaasite   #(ver:49)
                                               
      FOREACH b_fill_cs USING g_enterprise,g_inaa_m.inaasite INTO g_inaa_d[l_ac].inaastus,g_inaa_d[l_ac].inaa001, 
