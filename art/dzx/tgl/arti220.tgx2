   {<point name="reproduce.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE l_newno     LIKE inaa_t.inaasite 
   DEFINE l_oldno     LIKE inaa_t.inaasite 
 
   DEFINE l_master    RECORD LIKE inaa_t.* #此變數樣板目前無使用
   DEFINE l_detail    RECORD LIKE inaa_t.* #此變數樣板目前無使用
 
