          g_inaa_d[l_ac].inaa007,g_inaa_d[l_ac].inaa131,g_inaa_d[l_ac].inaa132,g_inaa_d[l_ac].inaa133, 
          g_inaa_d[l_ac].inaastamp,g_inaa_d[l_ac].inaa134,g_inaa_d[l_ac].inaa135,g_inaa_d[l_ac].inaa136, 
          g_inaa_d[l_ac].inaa137,g_inaa_d[l_ac].inaa138,g_inaa_d[l_ac].inaa139,g_inaa_d[l_ac].inaa140, 
          g_inaa_d[l_ac].inaa141,g_inaa_d[l_ac].inaa015,g_inaa_d[l_ac].inaa016,g_inaa_d[l_ac].inaa017, 
          g_inaa_d[l_ac].inaa012,g_inaa_d[l_ac].inaa142,g_inaa2_d[l_ac].inaa001,g_inaa2_d[l_ac].inaaownid, 
