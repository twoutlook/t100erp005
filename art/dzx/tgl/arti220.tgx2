               " LEFT JOIN inayl_t t10 ON t10.inaylent="||g_enterprise||" AND t10.inayl001=inaa141 AND t10.inayl002='"||g_dlang||"' ",
 
               " WHERE inaaent= ? AND inaasite=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
