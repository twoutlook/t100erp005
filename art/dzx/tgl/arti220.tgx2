   CALL arti220_set_entry('a')
   CALL arti220_set_no_entry('a')
   
   CALL cl_set_head_visible("","YES")
   CALL s_transaction_begin()
   
   #add-point:複製輸入前 name="reproduce.head.b_input"
   {<point name="reproduce.head.b_input"/>}
   #end add-point
   
   #清空key欄位的desc
      LET g_inaa_m.inaasite_desc = ''
   DISPLAY BY NAME g_inaa_m.inaasite_desc
 
   
   CALL arti220_input("r")
    
   IF INT_FLAG AND NOT g_master_insert THEN
      DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
      DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
      INITIALIZE g_inaa_m.* TO NULL
      INITIALIZE g_inaa_d TO NULL
      INITIALIZE g_inaa2_d TO NULL
 
      CALL arti220_show()
      LET INT_FLAG = 0
      CALL s_transaction_end('N','0')
