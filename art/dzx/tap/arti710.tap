  <section id="arti710.input" ver="11" status="" src="s">
<![CDATA[#+ 資料輸入
PRIVATE FUNCTION arti710_input(p_cmd)
   DEFINE  p_cmd                 LIKE type_t.chr1
   DEFINE  l_cmd_t               LIKE type_t.chr1
   DEFINE  l_cmd                 LIKE type_t.chr1
   DEFINE  l_ac_t                LIKE type_t.num5                #未取消的ARRAY CNT 
   DEFINE  l_n                   LIKE type_t.num5                #檢查重複用  
   DEFINE  l_cnt                 LIKE type_t.num5                #檢查重複用  
   DEFINE  l_lock_sw             LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert        LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete        LIKE type_t.num5                #可刪除否  
   DEFINE  l_count               LIKE type_t.num5
   DEFINE  l_i                   LIKE type_t.num5
   DEFINE  l_insert              BOOLEAN
   DEFINE  ls_return             STRING
   DEFINE  l_var_keys            DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys          DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                DYNAMIC ARRAY OF STRING
   DEFINE  l_fields              DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak        DYNAMIC ARRAY OF STRING
   DEFINE  lb_reproduce          BOOLEAN
   DEFINE  li_reproduce          LIKE type_t.num5
   DEFINE  li_reproduce_target   LIKE type_t.num5
   #add-point:input段define
   {<point name="input.define"/>}
   #end add-point    
   
   #先做狀態判定
   IF p_cmd = 'r' THEN
      LET l_cmd_t = 'r'
      LET p_cmd   = 'a'
   ELSE
      LET l_cmd_t = p_cmd
   END IF   
   
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF  
 
   CALL cl_set_head_visible("","YES")  
 
   LET l_insert = FALSE
   LET g_action_choice = ""
 
   #add-point:input段define_sql
   {<point name="input.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT rtkdstus,rtkd003,rtkd004,rtkd005,rtkd006,rtkd007,rtkdunit,'',rtkdownid, 
       '',rtkdowndp,'',rtkdcrtid,'',rtkdcrtdp,'',rtkdcrtdt,rtkdmodid,'',rtkdmoddt FROM rtkd_t WHERE  
       rtkdent=? AND rtkd001=? AND rtkd002=? AND rtkd003=? FOR UPDATE"
   #add-point:input段define_sql
   {<point name="input.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   DECLARE arti710_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR
 
 
   
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   LET g_qryparam.state = 'i'
   
   #控制key欄位可否輸入
   CALL arti710_set_entry(p_cmd)
   #add-point:set_entry後
   {<point name="input.after_set_entry"/>}
   #end add-point
   CALL arti710_set_no_entry(p_cmd)
   #add-point:set_no_entry後
   {<point name="input.after_set_no_entry"/>}
   #end add-point
 
   DISPLAY BY NAME g_rtkd_m.rtkd002
   
   LET lb_reproduce = FALSE
   
   #add-point:進入修改段前
   {<point name="input.before_input"/>}
   #end add-point
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
]]>
</section>
  <section id="arti710.input.body" ver="19" status="" src="s">
<![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_rtkd_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂單身ACTION
         
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_rtkd_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL arti710_b_fill(g_wc2) 
            IF g_rec_b != 0 THEN
               CALL fgl_set_arr_curr(l_ac)
            END IF
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN arti710_cl USING g_enterprise,
                                               g_rtkd_m.rtkd001
                                               ,g_rtkd_m.rtkd002
 
                                               
               IF STATUS THEN
                  CALL cl_err("OPEN arti710_cl:", STATUS, 1)
                  CLOSE arti710_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_rtkd_d[l_ac].rtkd003 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_rtkd_d_t.* = g_rtkd_d[l_ac].*  #BACKUP
               CALL arti710_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL arti710_set_no_entry_b(l_cmd)
               OPEN arti710_bcl USING g_enterprise,g_rtkd_m.rtkd001,
                                                g_rtkd_m.rtkd002,
 
                                                g_rtkd_d_t.rtkd003
 
               IF STATUS THEN
                  CALL cl_err("OPEN arti710_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH arti710_bcl INTO g_rtkd_d[l_ac].rtkdstus,g_rtkd_d[l_ac].rtkd003,g_rtkd_d[l_ac].rtkd004, 
                      g_rtkd_d[l_ac].rtkd005,g_rtkd_d[l_ac].rtkd006,g_rtkd_d[l_ac].rtkd007,g_rtkd_d[l_ac].rtkdunit, 
                      g_rtkd2_d[l_ac].rtkd003,g_rtkd2_d[l_ac].rtkdownid,g_rtkd2_d[l_ac].rtkdownid_desc, 
                      g_rtkd2_d[l_ac].rtkdowndp,g_rtkd2_d[l_ac].rtkdowndp_desc,g_rtkd2_d[l_ac].rtkdcrtid, 
                      g_rtkd2_d[l_ac].rtkdcrtid_desc,g_rtkd2_d[l_ac].rtkdcrtdp,g_rtkd2_d[l_ac].rtkdcrtdp_desc, 
                      g_rtkd2_d[l_ac].rtkdcrtdt,g_rtkd2_d[l_ac].rtkdmodid,g_rtkd2_d[l_ac].rtkdmodid_desc, 
                      g_rtkd2_d[l_ac].rtkdmoddt
                  IF SQLCA.sqlcode THEN
                      CALL cl_err(g_rtkd_d_t.rtkd003,SQLCA.sqlcode,1)
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL arti710_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
        
         BEFORE INSERT
            
            INITIALIZE g_rtkd_d_t.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_rtkd_d[l_ac].* TO NULL
            
            #公用欄位預設值
            #此段落由子樣板a14產生    
      LET g_rtkd2_d[l_ac].rtkdownid = g_user
      LET g_rtkd2_d[l_ac].rtkdowndp = g_dept
