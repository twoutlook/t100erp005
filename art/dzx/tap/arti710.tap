  <section id="arti710.b_fill" ver="19" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION arti710_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_rtkd_d.clear()
   CALL g_rtkd2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
      LET g_sql = "SELECT  UNIQUE rtkdstus,rtkd003,rtkd004,rtkd005,rtkd006,rtkd007,rtkdunit,rtkd003, 
          rtkdownid,'',rtkdowndp,'',rtkdcrtid,'',rtkdcrtdp,'',rtkdcrtdt,rtkdmodid,'',rtkdmoddt FROM rtkd_t", 
             
                  "",
                  
                  " WHERE rtkdent= ? AND rtkd001=? AND rtkd002=?"  
                  
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF arti710_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY rtkd_t.rtkd003"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      PREPARE arti710_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR arti710_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_rtkd_m.rtkd001
                                               ,g_rtkd_m.rtkd002
 
                                               
      FOREACH b_fill_cs INTO g_rtkd_d[l_ac].rtkdstus,g_rtkd_d[l_ac].rtkd003,g_rtkd_d[l_ac].rtkd004,g_rtkd_d[l_ac].rtkd005, 
          g_rtkd_d[l_ac].rtkd006,g_rtkd_d[l_ac].rtkd007,g_rtkd_d[l_ac].rtkdunit,g_rtkd2_d[l_ac].rtkd003, 
          g_rtkd2_d[l_ac].rtkdownid,g_rtkd2_d[l_ac].rtkdownid_desc,g_rtkd2_d[l_ac].rtkdowndp,g_rtkd2_d[l_ac].rtkdowndp_desc, 
          g_rtkd2_d[l_ac].rtkdcrtid,g_rtkd2_d[l_ac].rtkdcrtid_desc,g_rtkd2_d[l_ac].rtkdcrtdp,g_rtkd2_d[l_ac].rtkdcrtdp_desc, 
          g_rtkd2_d[l_ac].rtkdcrtdt,g_rtkd2_d[l_ac].rtkdmodid,g_rtkd2_d[l_ac].rtkdmodid_desc,g_rtkd2_d[l_ac].rtkdmoddt  

                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_rtkd2_d[l_ac].rtkdownid_desc = cl_get_username(g_rtkd2_d[l_ac].rtkdownid)
      #LET g_rtkd2_d[l_ac].rtkdowndp_desc = cl_get_deptname(g_rtkd2_d[l_ac].rtkdowndp)
      #LET g_rtkd2_d[l_ac].rtkdcrtid_desc = cl_get_username(g_rtkd2_d[l_ac].rtkdcrtid)
      #LET g_rtkd2_d[l_ac].rtkdcrtdp_desc = cl_get_deptname(g_rtkd2_d[l_ac].rtkdcrtdp)
      #LET g_rtkd2_d[l_ac].rtkdmodid_desc = cl_get_username(g_rtkd2_d[l_ac].rtkdmodid)
      ##LET .rtkdcnfid_desc = cl_get_deptname(.rtkdcnfid)
      ##LET .rtkdpstid_desc = cl_get_deptname(.rtkdpstid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_rtkd_d.deleteElement(g_rtkd_d.getLength())
      CALL g_rtkd2_d.deleteElement(g_rtkd2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE arti710_pb   
   
END FUNCTION
]]>
</section>
