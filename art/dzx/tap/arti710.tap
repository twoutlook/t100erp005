  <section id="arti710.delete" ver="7" status="" src="s">
<![CDATA[#+ 資料刪除
PRIVATE FUNCTION arti710_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_rtkd_m.rtkd001 IS NULL
   OR g_rtkd_m.rtkd002 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE arti710_master_referesh USING g_rtkd_m.rtkd001,g_rtkd_m.rtkd002 INTO g_rtkd_m.rtkd001,g_rtkd_m.rtkd002 

   
   CALL arti710_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN arti710_cl USING g_enterprise,g_rtkd_m.rtkd001
                                                       ,g_rtkd_m.rtkd002
 
   IF STATUS THEN
      CALL cl_err("OPEN arti710_cl:", STATUS, 1)
      CLOSE arti710_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH arti710_cl INTO g_rtkd_m.rtkd001,g_rtkd_m.rtkd002,g_rtkd_m.rtkd002_desc
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_rtkd_m.rtkd001,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL arti710_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM rtkd_t WHERE rtkdent = g_enterprise AND rtkd001 = g_rtkd_m.rtkd001
                                                               AND rtkd002 = g_rtkd_m.rtkd002
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("rtkd_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_rtkd_d.clear() 
      CALL g_rtkd2_d.clear()       
 
     
      CALL arti710_ui_browser_refresh()  
      CALL arti710_ui_headershow()  
      CALL arti710_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL arti710_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL arti710_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE arti710_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_rtkd_m.rtkd001,'D')
    
END FUNCTION
]]>
</section>
