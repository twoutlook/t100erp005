  <point name="function.arti710_rtkd002_ref" cite_std="N" status="" ver="1" src="s" new="Y" order="4" mark_hard="N">
<![CDATA[
################################################################################
# Descriptions...: 取供應商編號/配送中心說明
# Memo...........:
# Usage..........: CALL arti710_rtkd002_ref(p_rtkd002)
#                  RETURNING r_rtkd002_desc
# Input parameter: p_rtkd002        供應商編號/配送中心編號
# Return code....: r_rtkd002_desc   供應商編號/配送中心說明
# Date & Author..: 2014/04/07 By Lori
# Modify.........:
################################################################################
PRIVATE FUNCTION arti710_rtkd002_ref(p_rtkd002)
   DEFINE p_rtkd002       LIKE rtkd_t.rtkd002
   DEFINE r_rtkd002_desc  LIKE pmaal_t.pmaal003 
   
   LET r_rtkd002_desc = NULL
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = p_rtkd002
         
   CASE g_rtkd001 
      WHEN'1'     #資料類型 = 供應商
         CALL ap_ref_array2(g_ref_fields,"SELECT pmaal004 FROM pmaal_t WHERE pmaalent='"||g_enterprise||"' AND pmaal001=? AND pmaal002='"||g_dlang||"'","") RETURNING g_rtn_fields
      WHEN '2'    #資料類型 = 配送中心
         CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_lang||"'","") RETURNING g_rtn_fields
   END CASE
   
   LET r_rtkd002_desc = '', g_rtn_fields[1] , ''
   
   RETURN r_rtkd002_desc
END FUNCTION]]>
</point>
  <point name="function.arti710_rtkd002_chk" cite_std="N" status="" ver="1" src="s" new="Y" order="5" mark_hard="N">
<![CDATA[
################################################################################
# Descriptions...: 供應商/配送中心編號校驗
# Memo...........:
# Usage..........: CALL arti710_rtkd002_chk(p_rtkd002)
#                  RETURNING r_success
# Input parameter: p_rtkd002   供應商/配送中心編號
# Return code....: r_success   確認狀態
# Date & Author..: 2014/04/07 By Lori
# Modify.........:
################################################################################
PRIVATE FUNCTION arti710_rtkd002_chk(p_rtkd002)
   DEFINE p_rtkd002   LIKE rtkd_t.rtkd002
   DEFINE r_success   LIKE type_t.num5
   
   LET r_success = TRUE
   
   INITIALIZE g_chkparam.* TO NULL   
   LET g_chkparam.arg1 = p_rtkd002
   
   CASE g_rtkd001 
      WHEN '1'   #供應商
         IF cl_chk_exist("v_pmaa001") THEN
            LET r_success = TRUE
         ELSE
            LET r_success = FALSE
         END IF
      WHEN '2'    #配送中心
         LET g_chkparam.arg2 = 'C'
         IF cl_chk_exist("v_ooef001_2") THEN
            LET r_success = TRUE
         ELSE
            LET r_success = FALSE
         END IF
   END CASE
   
   RETURN r_success
END FUNCTION]]>
</point>
