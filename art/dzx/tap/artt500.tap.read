    <![CDATA[
################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL artt500_discount_input()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2015/06/28 By yangxf
# Modify.........:
################################################################################
PRIVATE FUNCTION artt500_discount_input()
DEFINE l_success              LIKE type_t.num5
DEFINE l_xrcd103              LIKE xrcd_t.xrcd103,
       l_xrcd104              LIKE xrcd_t.xrcd104,
       l_xrcd105              LIKE xrcd_t.xrcd105,
       l_xrcd113              LIKE xrcd_t.xrcd113,
       l_xrcd114              LIKE xrcd_t.xrcd114,
       l_xrcd115              LIKE xrcd_t.xrcd115,
       l_imaa005              LIKE imaa_t.imaa005,
       l_xrcd123              LIKE xrcd_t.xrcd113,
       l_xrcd124              LIKE xrcd_t.xrcd114,
       l_xrcd125              LIKE xrcd_t.xrcd115,
       l_xrcd133              LIKE xrcd_t.xrcd113,
       l_xrcd134              LIKE xrcd_t.xrcd114,
       l_xrcd135              LIKE xrcd_t.xrcd115
DEFINE l_price                LIKE rtib_t.rtib008
DEFINE l_price1               LIKE rtib_t.rtib008
DEFINE l_price2               LIKE rtib_t.rtib008
DEFINE l_price3               LIKE rtib_t.rtib008
DEFINE l_n                    LIKE type_t.num10
DEFINE l_lock_sw              LIKE type_t.chr1 
DEFINE l_oodb011              LIKE oodb_t.oodb011
DEFINE l_ooef016              LIKE ooef_t.ooef016


   LET g_forupd_sql = "SELECT rtimsite,rtimunit,rtimorga,rtimseq,rtim003,rtim004,rtim005,rtim006,rtim008, 
       rtim009,rtim010,rtim012,rtim013,rtim014,rtim015,rtim016,rtim018,rtim017,rtim019,rtim020,rtim021, 
       rtim029,rtim022,rtim024,rtim025,rtim026,rtim027,rtim028 FROM rtim_t WHERE rtiment=? AND rtimdocno=?  
       AND rtimseq=? FOR UPDATE"
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)
   DECLARE artt500_discount_bcl CURSOR FROM g_forupd_sql
   
   INPUT ARRAY g_rtim_d FROM s_detail1.*
       ATTRIBUTE(COUNT = g_rec_b,WITHOUT DEFAULTS, 
               INSERT ROW = FALSE, 
               DELETE ROW = FALSE,
               APPEND ROW = TRUE)
      
      BEFORE INPUT
         IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
           CALL FGL_SET_ARR_CURR(g_rtim_d.getLength()+1) 
           LET g_insert = 'N' 
        END IF 
    
         CALL artt500_b_fill()
         IF g_loc = 'm' AND g_rec_b != 0 THEN
            CALL FGL_SET_ARR_CURR(g_detail_idx)
         END IF
         LET g_loc = 'm'
         LET g_rec_b = g_rtim_d.getLength()
         
      BEFORE ROW
         LET l_ac = ARR_CURR()
         LET g_detail_idx = l_ac
         LET g_current_page = 1
         
         LET l_lock_sw = 'N'
         LET l_n = ARR_COUNT()
         DISPLAY l_ac TO FORMONLY.idx
         CALL cl_set_comp_entry('rtim010',TRUE)
         CALL cl_set_comp_required('rtim010',TRUE)
         CALL cl_set_comp_entry("rtimseq,rtim003,rtim004,rtim012,rtim024,rtim025,rtim026,rtim027,rtim028,",FALSE)
         CALL s_transaction_begin()
         OPEN artt500_cl USING g_enterprise,g_rtil_m.rtildocno
         IF STATUS THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "OPEN artt500_cl:" 
            LET g_errparam.code   = STATUS 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            CLOSE artt500_discount_bcl
            CALL s_transaction_end('N','0')
            RETURN
         END IF
         
         LET g_rec_b = g_rtim_d.getLength()
         
         IF g_rec_b >= l_ac 
            AND g_rtim_d[l_ac].rtimseq IS NOT NULL
    
         THEN
            LET g_rtim_d_t.* = g_rtim_d[l_ac].*  #BACKUP
            LET g_rtim_d_o.* = g_rtim_d[l_ac].*  #BACKUP
            OPEN artt500_discount_bcl USING g_enterprise,g_rtil_m.rtildocno,g_rtim_d[l_ac].rtimseq     
            IF SQLCA.sqlcode THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "artt500_discount_bcl" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               LET l_lock_sw='Y'
               EXIT INPUT
            ELSE
               FETCH artt500_discount_bcl INTO g_rtim_d[l_ac].rtimsite,g_rtim_d[l_ac].rtimunit,g_rtim_d[l_ac].rtimorga, 
                   g_rtim_d[l_ac].rtimseq,g_rtim_d[l_ac].rtim003,g_rtim_d[l_ac].rtim004,g_rtim_d[l_ac].rtim005, 
                   g_rtim_d[l_ac].rtim006,g_rtim_d[l_ac].rtim008,g_rtim_d[l_ac].rtim009,g_rtim_d[l_ac].rtim010, 
                   g_rtim_d[l_ac].rtim012,g_rtim_d[l_ac].rtim013,g_rtim_d[l_ac].rtim014,g_rtim_d[l_ac].rtim015, 
                   g_rtim_d[l_ac].rtim016,g_rtim_d[l_ac].rtim018,g_rtim_d[l_ac].rtim017,g_rtim_d[l_ac].rtim019, 
                   g_rtim_d[l_ac].rtim020,g_rtim_d[l_ac].rtim021,g_rtim_d[l_ac].rtim029,g_rtim_d[l_ac].rtim022, 
                   g_rtim_d[l_ac].rtim024,g_rtim_d[l_ac].rtim025,g_rtim_d[l_ac].rtim026,g_rtim_d[l_ac].rtim027, 
                   g_rtim_d[l_ac].rtim028
               IF SQLCA.sqlcode THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = g_rtim_d_t.rtimseq 
                  LET g_errparam.code   = SQLCA.sqlcode 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  LET l_lock_sw = "Y"
               END IF
               
               #遮罩相關處理
               LET g_rtim_d_mask_o[l_ac].* =  g_rtim_d[l_ac].*
               CALL artt500_rtim_t_mask()
               LET g_rtim_d_mask_n[l_ac].* =  g_rtim_d[l_ac].*
               
               LET g_bfill = "N"
               CALL artt500_show()
               LET g_bfill = "Y"
               
               CALL cl_show_fld_cont()
            END IF
         END IF
         INITIALIZE g_ref_fields TO NULL
         LET g_ref_fields[1] = g_rtim_d[l_ac].rtim013
         CALL ap_ref_array2(g_ref_fields,"SELECT oocal003 FROM oocal_t WHERE oocalent='"||g_enterprise||"' AND oocal001=? AND oocal002='"||g_dlang||"'","") RETURNING g_rtn_fields
         LET g_rtim_d[l_ac].rtim013_desc = '',g_rtn_fields[1],''
         INITIALIZE g_ref_fields TO NULL
         LET g_ref_fields[1] = g_rtim_d[l_ac].rtim018
         CALL ap_ref_array2(g_ref_fields,"SELECT oocal003 FROM oocal_t WHERE oocalent='"||g_enterprise||"' AND oocal001=? AND oocal002='"||g_dlang||"'","") RETURNING g_rtn_fields
         LET g_rtim_d[l_ac].rtim018_desc = '',g_rtn_fields[1],''    
         IF NOT cl_null(g_rtil_m.rtil025) THEN
            SELECT oodb011 INTO l_oodb011
              FROM oodb_t,ooef_t
             WHERE oodbent = g_enterprise
               AND oodb001 = ooef019
               AND oodb002 = g_rtil_m.rtil025
               AND oodbent = ooefent
               AND ooef001 = g_rtil_m.rtilsite
         END IF 
         SELECT oodbl004 INTO g_rtim_d[l_ac].rtim006_desc 
           FROM oodbl_t,ooef_t
          WHERE oodblent = ooefent
           AND ooefent = g_enterprise
           AND ooef001 = g_rtil_m.rtilsite
           AND oodbl001 = ooef019
           AND oodbl002 = g_rtim_d[l_ac].rtim006             
         
     
      BEFORE INSERT
         CANCEL INSERT 
          
      BEFORE DELETE
         CANCEL DELETE 
         
      AFTER FIELD rtim010
         IF NOT cl_null(g_rtim_d[l_ac].rtim010) THEN 
            IF g_rtim_d[l_ac].rtim010 < 0 THEN 
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 'art-00394'
               LET g_errparam.extend = ''
               LET g_errparam.popup = TRUE
               CALL cl_err()
               LET g_rtim_d[l_ac].rtim010 = g_rtim_d_t.rtim010
               NEXT FIELD rtim010
            END IF
            LET g_rtim_d[l_ac].rtim010 = s_curr_round(g_rtil_m.rtilsite,g_rtil_m.rtil023,g_rtim_d[l_ac].rtim010,'1')
            #应收金额 = 交易价*计价数量
            LET g_rtim_d[l_ac].rtim021 = g_rtim_d[l_ac].rtim010 * g_rtim_d[l_ac].rtim017
            LET g_rtim_d[l_ac].rtim021 = s_curr_round(g_rtil_m.rtilsite,g_rtil_m.rtil023,g_rtim_d[l_ac].rtim021,'2')
            #计算本币应收金额
            LET g_rtim_d[l_ac].rtim029 = g_rtim_d[l_ac].rtim021 * g_rtil_m.rtil024
            SELECT ooef016 INTO l_ooef016
              FROM ooef_t
             WHERE ooefent = g_enterprise
               AND ooef001 = g_rtil_m.rtilsite
            LET g_rtim_d[l_ac].rtim029 = s_curr_round(g_rtil_m.rtilsite,l_ooef016,g_rtim_d[l_ac].rtim029,'2')
         END IF 
         
      ON ROW CHANGE
         IF INT_FLAG THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = '' 
            LET g_errparam.code   = 9001 
            LET g_errparam.popup  = FALSE 
            CALL cl_err()
            LET INT_FLAG = 0
            LET g_rtim_d[l_ac].* = g_rtim_d_t.*
            CLOSE artt500_discount_bcl
            CALL s_transaction_end('N','0')
            EXIT INPUT 
         END IF
           
         IF l_lock_sw = 'Y' THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = g_rtim_d[l_ac].rtimseq 
            LET g_errparam.code   = -263 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            LET g_rtim_d[l_ac].* = g_rtim_d_t.*
         ELSE
         
            #add-point:單身修改前
                           
            #end add-point
            
            #寫入修改者/修改日期資訊(單身)
            
   
            #將遮罩欄位還原
            CALL artt500_rtim_t_mask_restore('restore_mask_o')
   
            UPDATE rtim_t SET (rtim010,rtim021,rtim029) = (g_rtim_d[l_ac].rtim010,g_rtim_d[l_ac].rtim021,g_rtim_d[l_ac].rtim029) 
             WHERE rtiment = g_enterprise AND rtimdocno = g_rtil_m.rtildocno
               AND rtimseq = g_rtim_d_t.rtimseq
            CASE
               WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "rtim_t" 
                  LET g_errparam.code   = "std-00009" 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  CALL s_transaction_end('N','0')
                  LET g_rtim_d[l_ac].* = g_rtim_d_t.*
               WHEN SQLCA.sqlcode #其他錯誤
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "rtim_t" 
                  LET g_errparam.code   = SQLCA.sqlcode 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()                   
                  CALL s_transaction_end('N','0')
                  LET g_rtim_d[l_ac].* = g_rtim_d_t.*  
               OTHERWISE
            END CASE
            #add-point:單身修改後稅別明細
                           #單號      項次       項次2                    營運據點           應收金額
            CALL s_tax_ins(g_rtil_m.rtildocno,g_rtim_d_t.rtimseq,'0',   g_rtil_m.rtilsite,g_rtim_d[l_ac].rtim021,
                           #稅別                   數量                   幣別             匯率             帳套 匯率2 匯率3
                           g_rtim_d[l_ac].rtim006,g_rtim_d[l_ac].rtim017,g_rtil_m.rtil023,g_rtil_m.rtil024,' ',' ',  ' ')
                      #原幣未稅金額 原幣稅額   原幣含稅金額 本幣未稅金額(AR/AP使用) 本幣稅額(AR/AP使用) 本幣含稅金額(AR/AP使用)
            RETURNING l_xrcd103,    l_xrcd104, l_xrcd105,   l_xrcd113,              l_xrcd114,          l_xrcd115,
                      l_xrcd123,l_xrcd124,l_xrcd125,l_xrcd133,l_xrcd134,l_xrcd135
            IF g_rtim_d[l_ac].rtim010 != g_rtim_d_t.rtim010 THEN 
               LET l_xrcd103 = s_curr_round(g_rtil_m.rtilsite,g_rtil_m.rtil023,l_xrcd103,'2')
               UPDATE rtim_t SET rtim022 = l_xrcd103
                WHERE rtiment = g_enterprise
                  AND rtimdocno = g_rtil_m.rtildocno
                  AND rtimseq = g_rtim_d_t.rtimseq
               CASE
                  WHEN SQLCA.sqlcode #其他錯誤
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code = SQLCA.sqlcode
                     LET g_errparam.extend = "rtim_t"
                     LET g_errparam.popup = TRUE
                     CALL cl_err()
    
                     CALL s_transaction_end('N','0')
                  OTHERWISE
               END CASE
               #寫折價
               IF NOT artt500_ins_rtic(g_rtim_d_t.rtimseq,g_rtim_d[l_ac].rtim021,g_rtim_d_t.rtim021,g_rtim_d[l_ac].rtim012) THEN
                  CALL s_transaction_end('N','0')
               END IF 
               UPDATE rtim_t SET rtim020 = g_rtim_d_t.rtim021 - g_rtim_d[l_ac].rtim021
                WHERE rtiment = g_enterprise
                  AND rtimdocno = g_rtil_m.rtildocno
                  AND rtimseq = g_rtim_d_t.rtimseq
            END IF
         END IF
         
      AFTER ROW
         #更新单头含税金额
         UPDATE rtil_t SET rtil028 = (SELECT SUM(rtim021)
                                        FROM rtim_t
                                       WHERE rtiment = g_enterprise
                                         AND rtimdocno = g_rtil_m.rtildocno),
                           rtil039 = (SELECT SUM(rtim029)*g_rtil_m.rtil040/100
                                        FROM rtim_t
                                       WHERE rtiment = g_enterprise
                                         AND rtimdocno = g_rtil_m.rtildocno)
          WHERE rtilent = g_enterprise
            AND rtildocno = g_rtil_m.rtildocno
         CALL artt500_unlock_b("rtim_t","'1'")
         CALL s_transaction_end('Y','0')
           
      AFTER INPUT
            
      ON ACTION accept 
         ACCEPT INPUT
        
      ON ACTION cancel 
         LET INT_FLAG = TRUE 
         EXIT INPUT
         
    END INPUT 
    CALL cl_set_comp_entry('rtim010',FALSE)
    CALL cl_set_comp_entry("rtimseq,rtim003,rtim004,rtim012,rtim024,rtim025,rtim026,rtim027,rtim028,",TRUE)
    SELECT SUM(rtim012),SUM(rtim008*rtim012),SUM(rtim020),SUM(rtim021)
      INTO g_rtil_m.snum,g_rtil_m.amount,g_rtil_m.amount2,g_rtil_m.amount3
      FROM rtim_t
     WHERE rtiment = g_enterprise
       AND rtimdocno = g_rtil_m.rtildocno
    DISPLAY g_rtil_m.snum TO FORMONLY.snum  
    DISPLAY g_rtil_m.amount TO FORMONLY.amount
    DISPLAY g_rtil_m.amount2 TO FORMONLY.amount2
    DISPLAY g_rtil_m.amount3 TO FORMONLY.amount3
END FUNCTION]]>
