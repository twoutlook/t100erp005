    <![CDATA[################################################################################
# Descriptions...: 地址设置
# Memo...........:
# Usage..........: 
# Input parameter: 无
# Return code....: 无
# Date & Author..: 2014/02/20 By yangxf
# Modify.........:
################################################################################
PRIVATE FUNCTION artt500_adress(p_sign)
DEFINE  l_oocn002   LIKE oocn_t.oocn002
DEFINE  l_rtil017   LIKE rtil_t.rtil017
DEFINE  l_rtil016   LIKE rtil_t.rtil016
DEFINE  p_sign      LIKE type_t.chr1

   LET g_errno = ''
   IF g_rtil_m.rtilstus<>'N' THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = "amm-00038"
      LET g_errparam.extend = g_rtil_m.rtilstus
      LET g_errparam.popup = TRUE
      CALL cl_err()

      RETURN
   END IF
   IF cl_null(g_rtil_m.rtildocno) THEN 
      RETURN 
   END IF 
   CALL s_transaction_begin()
   CALL artt600_01(g_rtil_m.rtildocno,g_enterprise,g_sign) RETURNING g_errno,l_rtil016,l_rtil017
   IF NOT cl_null(g_errno) THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = g_errno
      LET g_errparam.extend = g_rtil_m.rtildocno
      LET g_errparam.popup = TRUE
      CALL cl_err()

      CALL s_transaction_end('N','0')
      RETURN
   END IF
   IF NOT cl_null(l_rtil016) THEN
      LET g_rtil_m.rtil016 = l_rtil016
   END IF
   IF NOT cl_null(l_rtil017) THEN
      LET g_rtil_m.rtil017 = l_rtil017
   END IF
   IF cl_null(l_rtil016) AND cl_null(l_rtil017) THEN 
      RETURN 
   END IF 
   OPEN artt500_cl USING g_enterprise,g_rtil_m.rtildocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code =  STATUS
      LET g_errparam.extend = "OPEN artt500_cl:"
      LET g_errparam.popup = TRUE
      CALL cl_err()

      CLOSE artt500_cl
      CALL s_transaction_end('N','0')
   ELSE
      UPDATE rtil_t SET rtil016 = l_rtil016,
                        rtil017 = l_rtil017
       WHERE rtilent = g_enterprise
         AND rtildocno = g_rtil_m.rtildocno
      IF SQLCA.sqlcode THEN
        INITIALIZE g_errparam TO NULL
        LET g_errparam.code = SQLCA.sqlcode
        LET g_errparam.extend = ''
        LET g_errparam.popup = TRUE
        CALL cl_err()

         CLOSE artt500_cl
         CALL s_transaction_end('N','0')
      ELSE
         CLOSE artt500_cl
         CALL s_transaction_end('Y','0')
      END IF
   END IF 
   DISPLAY BY NAME g_rtil_m.rtil016,g_rtil_m.rtil017
END FUNCTION]]>
