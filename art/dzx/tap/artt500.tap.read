  <point name="function.artt500_get_price" order="35" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 取价
# Memo...........:
# Usage..........: CALL artt500_get_price()
# Input parameter: 无
# Return code....: 无
# Date & Author..: 2015/05/14 By yangxf
# Modify.........:
################################################################################
PRIVATE FUNCTION artt500_get_price()
DEFINE l_success   LIKE type_t.num5
DEFINE l_price1    LIKE rtim_t.rtim008
DEFINE l_ooef016   LIKE ooef_t.ooef016   #add by yangxf

   IF cl_null(g_rtim_d[l_ac].rtimseq) OR cl_null(g_rtim_d[l_ac].rtim003) OR cl_null(g_rtim_d[l_ac].rtim004) OR cl_null(g_rtim_d[l_ac].rtim017) OR cl_null(g_rtim_d[l_ac].rtim018) THEN
      RETURN
   END IF
   
   CALL s_get_price('1',g_rtil_m.rtildocno,g_rtil_m.rtildocdt,g_rtim_d[l_ac].rtimseq,g_rtil_m.rtil002,g_rtil_m.rtil001,g_rtim_d[l_ac].rtim004,g_rtim_d[l_ac].rtim003,g_rtim_d[l_ac].rtim018,g_rtim_d[l_ac].rtim017,'1')
      RETURNING l_success,g_rtim_d[l_ac].rtim008,l_price1,g_rtim_d[l_ac].rtim009,g_rtim_d[l_ac].rtim010
   #add by yangxf  ---start----    #交易价不为空AND 计价数量不为空
   IF NOT cl_null(g_rtim_d[l_ac].rtim010) AND NOT cl_null(g_rtim_d[l_ac].rtim017) THEN    
      #应收金额 = 交易价*计价数量
      LET g_rtim_d[l_ac].rtim021 = g_rtim_d[l_ac].rtim010 * g_rtim_d[l_ac].rtim017
      LET g_rtim_d[l_ac].rtim021 = s_curr_round(g_rtil_m.rtilsite,g_rtil_m.rtil023,g_rtim_d[l_ac].rtim021,'2')
      #计算本币应收金额
      LET g_rtim_d[l_ac].rtim029 = g_rtim_d[l_ac].rtim021 * g_rtil_m.rtil024
      SELECT ooef016 INTO l_ooef016
        FROM ooef_t
       WHERE ooefent = g_enterprise
         AND ooef001 = g_rtil_m.rtilsite
      LET g_rtim_d[l_ac].rtim029 = s_curr_round(g_rtil_m.rtilsite,l_ooef016,g_rtim_d[l_ac].rtim029,'2')
   END IF                         
   #add by yangxf  ----end----
END FUNCTION]]>
