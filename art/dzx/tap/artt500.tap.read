  <point name="statechange.before_menu" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[         CALL cl_set_act_visible("unconfirmed,invalid,confirmed,closed,approved,withdraw,rejection,signing",TRUE)
         CALL cl_set_act_visible("reconciliate,unreconciliate",FALSE)
         CASE g_rtil_m.rtilstus
            WHEN "N"
               #HIDE OPTION "open"
               CALL cl_set_act_visible("unconfirmed,closed,approved,withdraw,rejection,signing",FALSE)
               #需提交至BPM時，則顯示「提交」功能並隱藏「確認」功能
               IF cl_bpm_chk() THEN
                   CALL cl_set_act_visible("signing",TRUE)
               END IF

            WHEN "C"
               #HIDE OPTION "posted"
               CALL cl_set_act_visible("unconfirmed,invalid,closed,approved,withdraw,rejection,signing",FALSE)

            WHEN "X"
               #HIDE OPTION "invalid"
               CALL cl_set_act_visible("invalid,unconfirmed,confirmed,closed,approved,withdraw,rejection,signing",FALSE)

            WHEN "Y"
               #HIDE OPTION "confirmed"
               CALL cl_set_act_visible("invalid,confirmed,approved,withdraw,rejection,signing",FALSE)

            WHEN "A"
               CALL cl_set_act_visible("invalid,unconfirmed,closed,approved,withdraw,rejection,signing",FALSE)
               
            WHEN "D"
               CALL cl_set_act_visible("linvaid,unconfirmed,confirmed,closed,approved,withdraw,rejection,signing",FALSE)
               
            WHEN "R"
               CALL cl_set_act_visible("unconfirmed,confirmed,closed,approved,withdraw,rejection,signing",FALSE)
               
            WHEN "W"
               CALL cl_set_act_visible("invalid,unconfirmed,confirmed,closed,approved,rejection,signing",FALSE)
         END CASE]]>
  </point>
  <point name="statechange.closed" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[            IF NOT cl_ask_confirm('axm-00201') THEN
               RETURN
            END IF 
            CALL s_artt500_close_chk(g_rtil_m.rtildocno,g_rtil_m.rtilstus) RETURNING l_success,g_errno
            IF l_success THEN 
               CALL s_transaction_begin()
               CALL s_artt500_close_upd(g_rtil_m.rtildocno) RETURNING l_success
               IF NOT l_success THEN
                  CALL s_transaction_end('N','0')
