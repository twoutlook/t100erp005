# Modify.........:
################################################################################
PRIVATE FUNCTION artt500_rtim021()
#库存单位
DEFINE l_imaa104   LIKE imaa_t.imaa104
#销售单位
DEFINE l_imaa105   LIKE imaa_t.imaa105
#计价单位
DEFINE l_imaa106   LIKE imaa_t.imaa106
DEFINE l_rate      LIKE inaj_t.inaj014
DEFINE l_rate1     LIKE inaj_t.inaj014
DEFINE l_success   LIKE type_t.num5
DEFINE l_ooef016   LIKE ooef_t.ooef016

   IF NOT cl_null(g_rtim_d[l_ac].rtim004) THEN 
      IF NOT cl_null(g_rtim_d[l_ac].rtim003) THEN 
         SELECT imay004,imay012,imay014
           INTO l_imaa105,l_imaa106,l_imaa104
           FROM imay_t
          WHERE imayent = g_enterprise
            AND imay001 = g_rtim_d[l_ac].rtim004
            AND imay003 = g_rtim_d[l_ac].rtim003
      ELSE
         SELECT imaa104,imaa105,imaa106 INTO l_imaa104,l_imaa105,l_imaa106
           FROM imaa_t
          WHERE imaaent = g_enterprise
            AND imaa001 = g_rtim_d[l_ac].rtim004
      END IF 
   END IF 
   IF cl_null(g_rtim_d[l_ac].rtim004) OR cl_null(g_rtim_d[l_ac].rtim012) THEN 
      RETURN 
   END IF 
   #計價單位
   LET g_rtim_d[l_ac].rtim018 = l_imaa106
   #计算销售单位与库存单位转换率
  #141224-00013#15---sakura---mod---str 
   CALL s_aimi190_get_convert(g_rtim_d[l_ac].rtim004,l_imaa105,l_imaa104) RETURNING l_success,l_rate
   IF NOT l_success THEN 
      RETURN
   END IF
   CALL s_aooi250_convert_qty(g_rtim_d[l_ac].rtim004,l_imaa105,l_imaa104,g_rtim_d[l_ac].rtim012) RETURNING l_success,g_rtim_d[l_ac].rtim014
   IF NOT l_success THEN 
      RETURN
   END IF   
   #计算销售单位与计价单位转换率
   CALL s_aimi190_get_convert(g_rtim_d[l_ac].rtim004,l_imaa105,l_imaa106) RETURNING l_success,l_rate1
   IF NOT l_success THEN 
      RETURN
   END IF
   CALL s_aooi250_convert_qty(g_rtim_d[l_ac].rtim004,l_imaa105,l_imaa106,g_rtim_d[l_ac].rtim012) RETURNING l_success,g_rtim_d[l_ac].rtim017
   IF NOT l_success THEN 
      RETURN
   END IF   
   #库存数量
  #LET g_rtim_d[l_ac].rtim014 = g_rtim_d[l_ac].rtim012 * l_rate
   #库存单位
   LET g_rtim_d[l_ac].rtim015 = l_imaa104
   #銷售庫存單位換算率
   LET g_rtim_d[l_ac].rtim016 = l_rate
   #計價數量     
  #LET g_rtim_d[l_ac].rtim017 = g_rtim_d[l_ac].rtim012 * l_rate1
  #141224-00013#15---sakura---mod---end 
   #銷售計價單位換算率
   LET g_rtim_d[l_ac].rtim019 = l_rate1
   IF NOT cl_null(g_rtim_d[l_ac].rtim010) AND NOT cl_null(g_rtim_d[l_ac].rtim017) THEN     #add by yangxf
      #应收金额 = 交易价*计价数量
      LET g_rtim_d[l_ac].rtim021 = g_rtim_d[l_ac].rtim010 * g_rtim_d[l_ac].rtim017
      LET g_rtim_d[l_ac].rtim021 = s_curr_round(g_rtil_m.rtilsite,g_rtil_m.rtil023,g_rtim_d[l_ac].rtim021,'2')
#      #计算本币应收金额
      LET g_rtim_d[l_ac].rtim029 = g_rtim_d[l_ac].rtim021 * g_rtil_m.rtil024
      SELECT ooef016 INTO l_ooef016
        FROM ooef_t
       WHERE ooefent = g_enterprise
         AND ooef001 = g_rtil_m.rtilsite
      LET g_rtim_d[l_ac].rtim029 = s_curr_round(g_rtil_m.rtilsite,l_ooef016,g_rtim_d[l_ac].rtim029,'2')
   END IF                             #add by yangxf
END FUNCTION]]>
