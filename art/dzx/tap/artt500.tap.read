  <point name="modify.define" order="" ver="3" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   DEFINE l_n          LIKE type_t.num5
   #检查是否有收款资料
   LET l_n = 0
   SELECT COUNT(*) INTO l_n
     FROM rtif_t
    WHERE rtifent = g_enterprise
      AND rtifdocno = g_rtil_m.rtildocno   
   IF l_n > 0 THEN 
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = "art-00397"
      LET g_errparam.extend = ""
      LET g_errparam.popup = TRUE
      CALL cl_err()

      RETURN
   END IF  
   IF g_prog = 'artt500' THEN    
      LET l_n = 0
      #检查折扣明细是否存在人工折价
      SELECT COUNT(*) INTO l_n
