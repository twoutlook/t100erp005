               RETURN 
            END IF ]]>
  </point>
  <point name="statechange.confirmed" order="" ver="3" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
         IF g_rtil_m.rtilstus = 'C' THEN 
            IF NOT cl_ask_confirm('aim-00108') THEN
               RETURN
            END IF 
            CALL s_artt500_unclose_chk(g_rtil_m.rtildocno,g_rtil_m.rtilstus) RETURNING l_success,g_errno
            IF l_success THEN 
               CALL s_transaction_begin()
               CALL s_artt500_unclose_upd(g_rtil_m.rtildocno) RETURNING l_success
               IF NOT l_success THEN
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','0')
               END IF
            ELSE
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = g_errno
               LET g_errparam.extend = g_rtil_m.rtildocno
               LET g_errparam.popup = TRUE
               CALL cl_err()

               RETURN
            END IF
         ELSE
            IF NOT cl_ask_confirm('aim-00108') THEN
               RETURN
            END IF 
            CALL s_artt500_conf_chk(g_rtil_m.rtildocno,g_rtil_m.rtilstus) RETURNING l_success,g_errno
            IF l_success THEN 
               CALL s_transaction_begin()
               
               
               IF NOT cl_null(g_rtil_m.rtil001) THEN 
                  #计算积点
                  CALL s_card_point_create_tmp() RETURNING r_success
                  IF NOT r_success THEN 
                     RETURN
                  END IF 
                  SELECT rtil028 INTO l_rtil028
                    FROM rtil_t
                   WHERE rtilent = g_enterprise
                     AND rtildocno = g_rtil_m.rtildocno
                  IF cl_null(l_rtil028) THEN 
                     LET l_rtil028 = 0
                  END IF 
                  CALL s_transaction_begin()
                  LET l_date = cl_get_current()
                  #                    程式編號  單據編號           會員卡號          營運據點          交易日期          交易時間
                  CALL s_card_point_cal(g_prog, g_rtil_m.rtildocno,g_rtil_m.rtil001,g_rtil_m.rtilsite,g_rtil_m.rtil031,g_rtil_m.rtil032) 
                       RETURNING r_success,l_card_point
                  IF r_success THEN
                     IF cl_null(l_card_point) THEN LET l_card_point = 0 END IF      #add by yangxf
                     #                     會員卡號          異動來源          異動類別          異動單據編號   
                     CALL s_card_point_add(g_rtil_m.rtil001,'1'              ,'A'             ,g_rtil_m.rtildocno,
                     #                     異動日期   異動組織           消費金額          本次異動積點   需求組織
                                           l_date,   g_rtil_m.rtilsite,l_rtil028,l_card_point,g_rtil_m.rtilsite,'','') RETURNING r_success  #150505-00002#1 add '',''
                     IF r_success THEN 
                        CALL s_transaction_end('Y','0')
                     ELSE
                        CALL s_transaction_end('N','0')
                     END IF 
                  ELSE
                     CALL s_transaction_end('N','0')
                  END IF 
                  CALL s_card_point_drop_tmp() RETURNING r_success
                  IF NOT r_success THEN 
                     RETURN
                  END IF 
                  CALL s_transaction_begin()
               END IF 
