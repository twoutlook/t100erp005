  <point name="function.artt500_rtil041_chk" order="37" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 来源单号检查
# Memo...........:
# Usage..........: CALL artt500_rtil041_chk()
# Input parameter: 无
# Return code....: 无
# Date & Author..: 2015/06/11 By yangxf
# Modify.........:
################################################################################
PRIVATE FUNCTION artt500_rtil041_chk()
DEFINE l_rtilstus  LIKE rtil_t.rtilstus
DEFINE l_n         LIKE type_t.num5
DEFINE l_rtil039   LIKE rtil_t.rtil039
DEFINE l_rtif031   LIKE rtif_t.rtif031

   LET g_errno = ''
   SELECT rtilstus INTO l_rtilstus
     FROM rtil_t
    WHERE rtilent = g_enterprise
      AND rtildocno = g_rtil_m.rtil041
   CASE 
      WHEN SQLCA.SQLCODE = 100 LET g_errno = 'art-00226'
      WHEN l_rtilstus <> 'Y'   LET g_errno = 'art-00226'
      OTHERWISE                LET g_errno = SQLCA.SQLCODE USING '-------'
   END CASE
   IF cl_null(g_errno) THEN 
      LET l_n = 0
      #检查此订单存在销售单中
      SELECT COUNT(*) INTO l_n
        FROM rtia_t
       WHERE rtiaent = g_enterprise
         AND rtia007 = g_rtil_m.rtil041
         AND rtiastus <> 'X'
         AND rtia000 IN('artt600','artt610')
      IF l_n > 0 THEN 
         LET g_errno = 'art-00586'
         RETURN 
      END IF 
      #此订单是否存在销售单单身中
      SELECT COUNT(*) INTO l_n
        FROM rtia_t,rtib_t
       WHERE rtiaent = g_enterprise
         AND rtiadocno = rtibdocno
         AND rtiaent = rtibent 
         AND rtiastus <> 'X'
         AND rtib001 = g_rtil_m.rtil041
      IF l_n > 0 THEN 
         LET g_errno = 'art-00586'
         RETURN 
      END IF 
      #检查此订单存在退订单中
      SELECT COUNT(*) INTO l_n
        FROM rtil_t
       WHERE rtilent = g_enterprise
         AND rtil041 = g_rtil_m.rtil041
         AND rtilstus <> 'X'
      IF l_n > 0 THEN 
         LET g_errno = 'art-00627'
         RETURN 
      END IF
      #此订单是否已经存在发卡单中
      SELECT COUNT(*) INTO l_n
        FROM rtia_t
       WHERE rtiaent = g_enterprise
         AND rtia007 = g_rtil_m.rtil041
         AND rtiastus <> 'X'
         AND rtia000 = 'ammt405'
      IF l_n > 0 THEN 
         LET g_errno = 'art-00629'
         RETURN 
      END IF
      #此订单是否存在售券单中
      SELECT COUNT(*) INTO l_n
        FROM rtia_t
       WHERE rtiaent = g_enterprise
         AND rtia007 = g_rtil_m.rtil041
         AND rtiastus <> 'X'
         AND rtia000 = 'agct405'
      IF l_n > 0 THEN 
         LET g_errno = 'art-00630'
         RETURN 
      END IF
      #获取此订单冲预收金额
      SELECT SUM(rtif031) INTO l_rtif031
        FROM rtif_t
       WHERE rtifent = g_enterprise
         AND rtif004 = g_rtil_m.rtil041
         AND rtif021 = '1'    
      IF cl_null(l_rtif031) THEN 
         LET l_rtif031 = 0
      END IF 
      #获取此订单应收款金额
      SELECT rtil039 INTO l_rtil039
        FROM rtil_t
       WHERE rtilent = g_enterprise
         AND rtildocno = g_rtil_m.rtil041
      IF cl_null(l_rtil039) THEN 
         LET l_rtil039 = 0
      END IF 
      IF l_rtil039 - l_rtif031 <= 0 THEN 
         LET g_errno = 'art-00628'
         RETURN 
      END IF 
   END IF 
END FUNCTION]]>
  </point>
