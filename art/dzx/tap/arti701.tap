  <section id="arti701.input.body" ver="17" status="" src="s">
    <![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_rtka_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂單身ACTION
         
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_rtka_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL arti701_b_fill(g_wc2) 
            IF g_rec_b != 0 THEN
               CALL fgl_set_arr_curr(l_ac)
            END IF
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN arti701_cl USING g_enterprise,
                                               g_rtka_m.rtka001
                                               ,g_rtka_m.rtka002
 
                                               
               IF STATUS THEN
                  CALL cl_err("OPEN arti701_cl:", STATUS, 1)
                  CLOSE arti701_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_rtka_d[l_ac].rtka003 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_rtka_d_t.* = g_rtka_d[l_ac].*  #BACKUP
               CALL arti701_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL arti701_set_no_entry_b(l_cmd)
               OPEN arti701_bcl USING g_enterprise,g_rtka_m.rtka001,
                                                g_rtka_m.rtka002,
 
                                                g_rtka_d_t.rtka003
 
               IF STATUS THEN
                  CALL cl_err("OPEN arti701_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH arti701_bcl INTO g_rtka_d[l_ac].rtkaunit,g_rtka_d[l_ac].rtkastus,g_rtka_d[l_ac].rtka003, 
                      g_rtka_d[l_ac].rtka004,g_rtka_d[l_ac].rtka005,g_rtka_d[l_ac].rtka006,g_rtka_d[l_ac].rtka007, 
                      g_rtka_d[l_ac].rtka008,g_rtka_d[l_ac].rtka009,g_rtka_d[l_ac].rtka010,g_rtka_d[l_ac].rtka011, 
                      g_rtka_d[l_ac].rtka012
                  IF SQLCA.sqlcode THEN
                      CALL cl_err(g_rtka_d_t.rtka003,SQLCA.sqlcode,1)
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL arti701_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
        
         BEFORE INSERT
            
            INITIALIZE g_rtka_d_t.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_rtka_d[l_ac].* TO NULL
            
            #公用欄位預設值
            #此段落由子樣板a14產生    
      LET g_rtka2_d[l_ac].rtkaownid = g_user
      LET g_rtka2_d[l_ac].rtkaowndp = g_dept
      LET g_rtka2_d[l_ac].rtkacrtid = g_user
      LET g_rtka2_d[l_ac].rtkacrtdp = g_dept 
      LET g_rtka2_d[l_ac].rtkacrtdt = cl_get_current()
      LET g_rtka2_d[l_ac].rtkamodid = ""
      LET g_rtka2_d[l_ac].rtkamoddt = ""
      LET g_rtka_d[l_ac].rtkastus = ""
 
  
            
            #一般欄位預設值
                  LET g_rtka_d[l_ac].rtkastus = "Y"
      LET g_rtka_d[l_ac].rtka010 = "0"
      LET g_rtka_d[l_ac].rtka011 = "0"
      LET g_rtka_d[l_ac].rtka012 = "0"
 
            
            
            LET g_rtka_d_t.* = g_rtka_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL arti701_set_entry_b(l_cmd)
            #add-point:set_entry_b後
            {<point name="input.body.before_insert.set_entry_b"/>}
            #end add-point
            CALL arti701_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
