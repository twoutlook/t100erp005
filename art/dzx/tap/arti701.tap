  <section id="arti701.b_fill" ver="11" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION arti701_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_rtka_d.clear()
   CALL g_rtka2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE rtkaunit,rtkastus,rtka003,rtka004,rtka005,rtka006,rtka007,rtka008,rtka009, 
       rtka010,rtka011,rtka012,rtka003,rtkaownid,rtkaowndp,rtkacrtid,rtkacrtdp,rtkacrtdt,rtkamodid,rtkamoddt, 
       t1.rtaal003 ,t2.oocql004 ,t3.oofa011 ,t4.ooefl003 ,t5.oofa011 ,t6.ooefl003 ,t7.oofa011 FROM rtka_t", 
          
               "",
               
                              " LEFT JOIN rtaal_t t1 ON t1.rtaalent='"||g_enterprise||"' AND t1.rtaal001=rtka003 AND t1.rtaal002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t2 ON t2.oocqlent='"||g_enterprise||"' AND t2.oocql001=rtka001 AND t2.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t3 ON t3.oofaent='"||g_enterprise||"' AND t3.oofa002='2' AND t3.oofa003=rtkaownid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=rtkaowndp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t5 ON t5.oofaent='"||g_enterprise||"' AND t5.oofa002='2' AND t5.oofa003=rtkacrtid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent='"||g_enterprise||"' AND t6.ooefl001=rtkacrtdp AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t7 ON t7.oofaent='"||g_enterprise||"' AND t7.oofa002='2' AND t7.oofa003=rtkamodid  ",
 
               " WHERE rtkaent= ? AND rtka001=? AND rtka002=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("rtka_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF arti701_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY rtka_t.rtka003"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE arti701_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR arti701_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_rtka_m.rtka001,g_rtka_m.rtka002
                                               
      FOREACH b_fill_cs INTO g_rtka_d[l_ac].rtkaunit,g_rtka_d[l_ac].rtkastus,g_rtka_d[l_ac].rtka003, 
          g_rtka_d[l_ac].rtka004,g_rtka_d[l_ac].rtka005,g_rtka_d[l_ac].rtka006,g_rtka_d[l_ac].rtka007, 
          g_rtka_d[l_ac].rtka008,g_rtka_d[l_ac].rtka009,g_rtka_d[l_ac].rtka010,g_rtka_d[l_ac].rtka011, 
          g_rtka_d[l_ac].rtka012,g_rtka2_d[l_ac].rtka003,g_rtka2_d[l_ac].rtkaownid,g_rtka2_d[l_ac].rtkaowndp, 
          g_rtka2_d[l_ac].rtkacrtid,g_rtka2_d[l_ac].rtkacrtdp,g_rtka2_d[l_ac].rtkacrtdt,g_rtka2_d[l_ac].rtkamodid, 
