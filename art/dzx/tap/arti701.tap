  <section id="arti701.main" ver="8" status="" src="s">
    <![CDATA[#+ 此段落由子樣板a26產生
#OPTIONS SHORT CIRCUIT
#+ 作業開始 
MAIN
   #add-point:main段define
   {<point name="main.define"/>}
   #end add-point   
 
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("art","")
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT rtka001,'',rtka002", 
                      " FROM rtka_t",
                      " WHERE rtkaent= ? AND rtka001=? AND rtka002=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE arti701_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE rtka001,rtka002",
               " FROM rtka_t",
               
               " WHERE rtkaent = '" ||g_enterprise|| "' AND rtka001 = ? AND rtka002 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE arti701_master_referesh FROM g_sql
 
 
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_arti701 WITH FORM cl_ap_formpath("art",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL arti701_init()   
 
      #進入選單 Menu (="N")
      CALL arti701_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_arti701
      
   END IF 
   
   CLOSE arti701_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
   
END MAIN
 
 
]]>
  </section>
  <section id="arti701.modify" ver="6" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION arti701_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_rtka_m.rtka001 IS NULL
   OR g_rtka_m.rtka002 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE arti701_master_referesh USING g_rtka_m.rtka001,g_rtka_m.rtka002 INTO g_rtka_m.rtka001,g_rtka_m.rtka002 

 
   ERROR ""
  
   LET g_rtka001_t = g_rtka_m.rtka001
   LET g_rtka002_t = g_rtka_m.rtka002
 
   CALL s_transaction_begin()
   
   OPEN arti701_cl USING g_enterprise,g_rtka_m.rtka001
                                                       ,g_rtka_m.rtka002
 
   IF STATUS THEN
      CALL cl_err("OPEN arti701_cl:", STATUS, 1)
      CLOSE arti701_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH arti701_cl INTO g_rtka_m.rtka001,g_rtka_m.rtka001_desc_1,g_rtka_m.rtka002
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_rtka_m.rtka001,SQLCA.sqlcode,0)
      CLOSE arti701_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL arti701_show()
   WHILE TRUE
      LET g_rtka001_t = g_rtka_m.rtka001
      LET g_rtka002_t = g_rtka_m.rtka002
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL arti701_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_rtka_m.* = g_rtka_m_t.*
         CALL arti701_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_rtka_m.rtka001 != g_rtka001_t 
      OR g_rtka_m.rtka002 != g_rtka002_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE rtka_t SET rtka001 = g_rtka_m.rtka001
                                       , rtka002 = g_rtka_m.rtka002
 
          WHERE rtkaent = g_enterprise AND rtka001 = g_rtka001_t
            AND rtka002 = g_rtka002_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("rtka_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("rtka_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_rtka_m.rtka001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE arti701_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_rtka_m.rtka001,'U')
 
   CALL arti701_b_fill("1=1")
   
END FUNCTION   
]]>
  </section>
