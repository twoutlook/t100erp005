  <section id="arti701.delete" ver="5" status="" src="s">
    <![CDATA[#+ 資料刪除
PRIVATE FUNCTION arti701_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_rtka_m.rtka001 IS NULL
   OR g_rtka_m.rtka002 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE arti701_master_referesh USING g_rtka_m.rtka001,g_rtka_m.rtka002 INTO g_rtka_m.rtka001,g_rtka_m.rtka002 

   
   CALL arti701_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN arti701_cl USING g_enterprise,g_rtka_m.rtka001
                                                       ,g_rtka_m.rtka002
 
   IF STATUS THEN
      CALL cl_err("OPEN arti701_cl:", STATUS, 1)
      CLOSE arti701_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH arti701_cl INTO g_rtka_m.rtka001,g_rtka_m.rtka001_desc_1,g_rtka_m.rtka002
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_rtka_m.rtka001,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL arti701_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM rtka_t WHERE rtkaent = g_enterprise AND rtka001 = g_rtka_m.rtka001
                                                               AND rtka002 = g_rtka_m.rtka002
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("rtka_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_rtka_d.clear() 
      CALL g_rtka2_d.clear()       
 
     
      CALL arti701_ui_browser_refresh()  
      CALL arti701_ui_headershow()  
      CALL arti701_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL arti701_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL arti701_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE arti701_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_rtka_m.rtka001,'D')
    
END FUNCTION
]]>
