  <point name="function.arti701_rtka003_chk" order="8" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 店群/門店編號校驗
# Memo...........:
# Usage..........: CALL arti701_rtka003_chk(p_rtka003)
#                  RETURNING r_success
# Input parameter: p_rtka003   店群/門店編號
# Return code....: r_success   確認狀態
# Date & Author..: 2014/03/28 By Lori
# Modify.........:
################################################################################
PRIVATE FUNCTION arti701_rtka003_chk(p_rtka003)
   DEFINE p_rtka003   LIKE rtka_t.rtka003
   DEFINE r_success   LIKE type_t.num5
   
   LET r_success = TRUE
   
   INITIALIZE g_chkparam.* TO NULL   

   CASE g_rtka_m.rtka002 
      WHEN '1'   #店群
         LET g_chkparam.arg1 = p_rtka003
         LET g_chkparam.arg2 = '2'
         IF cl_chk_exist("v_rtaa001_2") THEN
            LET r_success = TRUE
         ELSE
            LET r_success = FALSE
         END IF
      WHEN '2'    #門店
         LET g_chkparam.arg1 = p_rtka003
         LET g_chkparam.arg2 = 'F'
         IF p_rtka003 <> 'ALL' THEN
            IF cl_chk_exist("v_ooef001_2") THEN
               LET r_success = TRUE
            ELSE
               LET r_success = FALSE
            END IF
         END IF   
   END CASE
   
   RETURN r_success
END FUNCTION]]>
