  <section id="arti705.b_fill" ver="6" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION arti705_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_rtkb_d.clear()
   CALL g_rtkb2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
      LET g_sql = "SELECT  UNIQUE rtkbstus,rtkb003,'',rtkb004,rtkb005,rtkb006,rtkb007,'',rtkb008,rtkb009, 
          rtkbunit,rtkb003,rtkbownid,'',rtkbowndp,'',rtkbcrtid,'',rtkbcrtdp,'',rtkbcrtdt,rtkbmodid,'', 
          rtkbmoddt FROM rtkb_t",   
                  "",
                  
                  " WHERE rtkbent= ? AND rtkb001=? AND rtkb002=?"  
                  
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF arti705_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY rtkb_t.rtkb003"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      PREPARE arti705_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR arti705_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_rtkb_m.rtkb001
                                               ,g_rtkb_m.rtkb002
 
                                               
      FOREACH b_fill_cs INTO g_rtkb_d[l_ac].rtkbstus,g_rtkb_d[l_ac].rtkb003,g_rtkb_d[l_ac].rtkb003_desc, 
          g_rtkb_d[l_ac].rtkb004,g_rtkb_d[l_ac].rtkb005,g_rtkb_d[l_ac].rtkb006,g_rtkb_d[l_ac].rtkb007, 
          g_rtkb_d[l_ac].rtkb007_desc,g_rtkb_d[l_ac].rtkb008,g_rtkb_d[l_ac].rtkb009,g_rtkb_d[l_ac].rtkbunit, 
          g_rtkb2_d[l_ac].rtkb003,g_rtkb2_d[l_ac].rtkbownid,g_rtkb2_d[l_ac].rtkbownid_desc,g_rtkb2_d[l_ac].rtkbowndp, 
          g_rtkb2_d[l_ac].rtkbowndp_desc,g_rtkb2_d[l_ac].rtkbcrtid,g_rtkb2_d[l_ac].rtkbcrtid_desc,g_rtkb2_d[l_ac].rtkbcrtdp, 
          g_rtkb2_d[l_ac].rtkbcrtdp_desc,g_rtkb2_d[l_ac].rtkbcrtdt,g_rtkb2_d[l_ac].rtkbmodid,g_rtkb2_d[l_ac].rtkbmodid_desc, 
          g_rtkb2_d[l_ac].rtkbmoddt 
                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_rtkb2_d[l_ac].rtkbownid_desc = cl_get_username(g_rtkb2_d[l_ac].rtkbownid)
      #LET g_rtkb2_d[l_ac].rtkbowndp_desc = cl_get_deptname(g_rtkb2_d[l_ac].rtkbowndp)
      #LET g_rtkb2_d[l_ac].rtkbcrtid_desc = cl_get_username(g_rtkb2_d[l_ac].rtkbcrtid)
      #LET g_rtkb2_d[l_ac].rtkbcrtdp_desc = cl_get_deptname(g_rtkb2_d[l_ac].rtkbcrtdp)
      #LET g_rtkb2_d[l_ac].rtkbmodid_desc = cl_get_username(g_rtkb2_d[l_ac].rtkbmodid)
      ##LET .rtkbcnfid_desc = cl_get_deptname(.rtkbcnfid)
      ##LET .rtkbpstid_desc = cl_get_deptname(.rtkbpstid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_rtkb_d.deleteElement(g_rtkb_d.getLength())
      CALL g_rtkb2_d.deleteElement(g_rtkb2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE arti705_pb   
   
END FUNCTION
]]>
</section>
