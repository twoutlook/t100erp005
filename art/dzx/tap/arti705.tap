  <section id="arti705.modify" ver="3" status="" src="s">
<![CDATA[#+ 資料修改
PRIVATE FUNCTION arti705_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_rtkb_m.rtkb001 IS NULL
   OR g_rtkb_m.rtkb002 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE arti705_master_referesh USING g_rtkb_m.rtkb001,g_rtkb_m.rtkb002 INTO g_rtkb_m.rtkb001,g_rtkb_m.rtkb002 

 
   ERROR ""
  
   LET g_rtkb001_t = g_rtkb_m.rtkb001
   LET g_rtkb002_t = g_rtkb_m.rtkb002
 
   CALL s_transaction_begin()
   
   OPEN arti705_cl USING g_enterprise,g_rtkb_m.rtkb001
                                                       ,g_rtkb_m.rtkb002
 
   IF STATUS THEN
      CALL cl_err("OPEN arti705_cl:", STATUS, 1)
      CLOSE arti705_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH arti705_cl INTO g_rtkb_m.rtkb001,g_rtkb_m.rtkb002,g_rtkb_m.rtkb002_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_rtkb_m.rtkb001,SQLCA.sqlcode,0)
      CLOSE arti705_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL arti705_show()
   WHILE TRUE
      LET g_rtkb001_t = g_rtkb_m.rtkb001
      LET g_rtkb002_t = g_rtkb_m.rtkb002
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL arti705_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_rtkb_m.* = g_rtkb_m_t.*
         CALL arti705_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_rtkb_m.rtkb001 != g_rtkb001_t 
      OR g_rtkb_m.rtkb002 != g_rtkb002_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE rtkb_t SET rtkb001 = g_rtkb_m.rtkb001
                                       , rtkb002 = g_rtkb_m.rtkb002
 
          WHERE rtkbent = g_enterprise AND rtkb001 = g_rtkb001_t
            AND rtkb002 = g_rtkb002_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("rtkb_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("rtkb_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_rtkb_m.rtkb001,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE arti705_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_rtkb_m.rtkb001,'U')
 
   CALL arti705_b_fill("1=1")
   
END FUNCTION   
]]>
</section>
