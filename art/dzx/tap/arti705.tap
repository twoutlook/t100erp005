  <point name="function.arti705_rtkb002_chk" cite_std="N" status="" ver="1" src="s" new="Y" order="6" mark_hard="N">
<![CDATA[
################################################################################
# Descriptions...: 店群/門店編號校驗
# Memo...........:
# Usage..........: CALL arti705_rtkb002_chk(p_rtkb002)
#                  RETURNING r_success
# Input parameter: p_rtkb002   店群/門店編號
# Return code....: r_success   確認狀態
# Date & Author..: 2014/03/31 By Lori
# Modify.........:
################################################################################
PRIVATE FUNCTION arti705_rtkb002_chk(p_rtkb002)
   DEFINE p_rtkb002   LIKE rtkb_t.rtkb002
   DEFINE r_success   LIKE type_t.num5
   
   LET r_success = TRUE
   
   INITIALIZE g_chkparam.* TO NULL   
   
   CASE g_rtkb_m.rtkb001 
      WHEN '1'   #店群
         LET g_chkparam.arg1 = p_rtkb002
         LET g_chkparam.arg2 = '2'
         IF cl_chk_exist("v_rtaa001_2") THEN
            LET r_success = TRUE
         ELSE
            LET r_success = FALSE
         END IF
      WHEN '2'    #門店
         LET g_chkparam.arg1 = p_rtkb002
         LET g_chkparam.arg2 = 'F'
         IF p_rtkb002 <> 'ALL' THEN
            IF cl_chk_exist("v_ooef001_2") THEN
               LET r_success = TRUE
            ELSE
               LET r_success = FALSE
            END IF
         END IF   
   END CASE   
   
   RETURN r_success
END FUNCTION]]>
</point>
