                     "         LEFT JOIN ooefl_t t4  ON t4.ooeflent=rtjaent  AND t4.ooefl001=rtja005   AND t4.ooefl002='"||g_dlang||"'",
                     "         LEFT JOIN pmaal_t t5  ON t5.pmaalent=rtjaent  AND t5.pmaal001=rtja010   AND t5.pmaal002='"||g_dlang||"'",
                     "         LEFT JOIN pmaal_t t6  ON t6.pmaalent=rtjaent  AND t6.pmaal001=rtja011   AND t6.pmaal002='"||g_dlang||"'",
                     "         LEFT JOIN pmaal_t t7  ON t7.pmaalent=rtjaent  AND t7.pmaal001=rtja012   AND t7.pmaal002='"||g_dlang||"'",
                     "         LEFT JOIN ooefl_t t8  ON t8.ooeflent=rtjaent  AND t8.ooefl001=rtja018   AND t8.ooefl002='"||g_dlang||"'",
                     "         LEFT JOIN ooefl_t t9  ON t9.ooeflent=rtjaent  AND t9.ooefl001=rtja024   AND t9.ooefl002='"||g_dlang||"'",
                     "         LEFT JOIN ooibl_t t10 ON t10.ooiblent=rtjaent AND t10.ooibl002=rtja025  AND t10.ooibl003='"||g_dlang||"'",
                     "         LEFT JOIN ooag_t  t11 ON t11.ooagent =rtjaent AND t11.ooag001 =rtja037",
                     "         LEFT JOIN ooefl_t t12 ON t12.ooeflent=rtjaent AND t12.ooefl001=rtjaunit AND t12.ooefl002='"||g_dlang||"'",
                     "         LEFT JOIN ooag_t  t13 ON t13.ooagent =rtjaent AND t13.ooag001 =rtjaownid",
