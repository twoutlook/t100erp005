                     "          rtja036,rtja037,t11.ooag011 rtja037_desc,rtja038,rtja039,",
                     "          rtja040,rtja041,rtja042,rtja043,rtja044,",
                     "          rtja045,rtja046,rtja047,rtja048,rtja049,",
                     "          rtja050,rtja051,rtja052,rtjaunit,t12.ooefl003 rtjaunit_desc,",
                     "          rtjaownid,t13.ooag011 rtjaownid_desc,rtjaowndp,t14.ooefl003 rtjaowndp_desc,rtjacrtid,",
                     "          t15.ooag011 rtjacrtid_desc,rtjacrtdp,t16.ooefl003 rtjacrtdp_desc,rtjacrtdt,rtjamodid, ",
                     "          t17.ooag011 rtjamodid_desc,rtjamoddt,rtjacnfid,t18.ooag011 rtjacnfid_desc,rtjacnfdt,",
                     "          rtjapstid,t19.ooag011 rtjapstid_desc,rtjapstdt,rtjastus  , ",
                     "          DENSE_RANK() OVER( ORDER BY rtja_t.rtjadocno) AS RANK ",
                     "  FROM rtja_t",
                     "         LEFT JOIN ooefl_t t1  ON t1.ooeflent=rtjaent  AND t1.ooefl001=rtjasite  AND t1.ooefl002='"||g_dlang||"'",
                     "         LEFT JOIN pmaal_t t2  ON t2.pmaalent=rtjaent  AND t2.pmaal001=rtja002   AND t2.pmaal002='"||g_dlang||"'",
