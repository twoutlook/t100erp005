  <section id="artq620.b_fill" ver="6" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION artq620_b_fill()
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準
   {<point name="b_fill.define" edit="s"/>}
   #end add-point
   #add-point:b_fill段define-客製
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   LET ls_sql_rank = "SELECT  UNIQUE '',rtjadocno,rtjadocdt,rtjasite,'',rtja000,rtja001,rtja002,'',rtja003, 
       rtja004,'',rtja005,'',rtja006,rtja007,rtja008,rtja009,rtja010,'',rtja011,'',rtja012,'',rtja013, 
       rtja014,rtja015,rtja016,rtja017,rtja018,'',rtja019,rtja020,rtja021,rtja022,rtja023,rtja024,'', 
       rtja025,'',rtja026,rtja027,rtja028,rtja029,rtja030,rtja031,rtja032,rtja033,rtja034,rtja035,rtja036, 
       rtja037,'',rtja038,rtja039,rtja040,rtja041,rtja042,rtja043,rtja044,rtja045,rtja046,rtja047,rtja048, 
       rtja049,rtja050,rtja051,rtja052,rtjaunit,'',rtjaownid,'',rtjaowndp,'',rtjacrtid,'',rtjacrtdp, 
       '',rtjacrtdt,rtjamodid,'',rtjamoddt,rtjacnfid,'',rtjacnfdt,rtjapstid,'',rtjapstdt,rtjastus  , 
       DENSE_RANK() OVER( ORDER BY rtja_t.rtjadocno) AS RANK FROM rtja_t",
 
                     " LEFT JOIN rtjb_t ON rtjbent = rtjaent AND rtjadocno = rtjbdocno",
 
                     " LEFT JOIN rtjc_t ON rtjcent = rtjaent AND rtjadocno = rtjcdocno",
 
                     " LEFT JOIN rtjd_t ON rtjdent = rtjaent AND rtjadocno = rtjddocno",
 
                     " LEFT JOIN rtje_t ON rtjeent = rtjaent AND rtjadocno = rtjedocno",
 
                     " LEFT JOIN rtjk_t ON rtjkent = rtjaent AND rtjadocno = rtjkdocno",
 
                     " LEFT JOIN rtjf_t ON rtjfent = rtjaent AND rtjadocno = rtjfdocno",
 
 
                     "",
                     " WHERE rtjaent= ? AND 1=1 AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("rtja_t"),
                     " ORDER BY rtja_t.rtjadocno"
 
   #add-point:b_fill段rank_sql_after
   {<point name="b_fill.rank_sql_after"/>}
   #end add-point
 
   LET g_sql = "SELECT COUNT(*) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT '',rtjadocno,rtjadocdt,rtjasite,'',rtja000,rtja001,rtja002,'',rtja003,rtja004, 
       '',rtja005,'',rtja006,rtja007,rtja008,rtja009,rtja010,'',rtja011,'',rtja012,'',rtja013,rtja014, 
       rtja015,rtja016,rtja017,rtja018,'',rtja019,rtja020,rtja021,rtja022,rtja023,rtja024,'',rtja025, 
       '',rtja026,rtja027,rtja028,rtja029,rtja030,rtja031,rtja032,rtja033,rtja034,rtja035,rtja036,rtja037, 
       '',rtja038,rtja039,rtja040,rtja041,rtja042,rtja043,rtja044,rtja045,rtja046,rtja047,rtja048,rtja049, 
       rtja050,rtja051,rtja052,rtjaunit,'',rtjaownid,'',rtjaowndp,'',rtjacrtid,'',rtjacrtdp,'',rtjacrtdt, 
       rtjamodid,'',rtjamoddt,rtjacnfid,'',rtjacnfdt,rtjapstid,'',rtjapstdt,rtjastus",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE artq620_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR artq620_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_rtja_d.clear()
 
   #add-point:陣列清空
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1   
 
   FOREACH b_fill_curs INTO g_rtja_d[l_ac].sel,g_rtja_d[l_ac].rtjadocno,g_rtja_d[l_ac].rtjadocdt,g_rtja_d[l_ac].rtjasite, 
       g_rtja_d[l_ac].rtjasite_desc,g_rtja_d[l_ac].rtja000,g_rtja_d[l_ac].rtja001,g_rtja_d[l_ac].rtja002, 
       g_rtja_d[l_ac].rtja002_desc,g_rtja_d[l_ac].rtja003,g_rtja_d[l_ac].rtja004,g_rtja_d[l_ac].rtja004_desc, 
       g_rtja_d[l_ac].rtja005,g_rtja_d[l_ac].rtja005_desc,g_rtja_d[l_ac].rtja006,g_rtja_d[l_ac].rtja007, 
       g_rtja_d[l_ac].rtja008,g_rtja_d[l_ac].rtja009,g_rtja_d[l_ac].rtja010,g_rtja_d[l_ac].rtja010_desc, 
       g_rtja_d[l_ac].rtja011,g_rtja_d[l_ac].rtja011_desc,g_rtja_d[l_ac].rtja012,g_rtja_d[l_ac].rtja012_desc, 
       g_rtja_d[l_ac].rtja013,g_rtja_d[l_ac].rtja014,g_rtja_d[l_ac].rtja015,g_rtja_d[l_ac].rtja016,g_rtja_d[l_ac].rtja017, 
       g_rtja_d[l_ac].rtja018,g_rtja_d[l_ac].rtja018_desc,g_rtja_d[l_ac].rtja019,g_rtja_d[l_ac].rtja020, 
       g_rtja_d[l_ac].rtja021,g_rtja_d[l_ac].rtja022,g_rtja_d[l_ac].rtja023,g_rtja_d[l_ac].rtja024,g_rtja_d[l_ac].rtja024_desc, 
       g_rtja_d[l_ac].rtja025,g_rtja_d[l_ac].rtja025_desc,g_rtja_d[l_ac].rtja026,g_rtja_d[l_ac].rtja027, 
       g_rtja_d[l_ac].rtja028,g_rtja_d[l_ac].rtja029,g_rtja_d[l_ac].rtja030,g_rtja_d[l_ac].rtja031,g_rtja_d[l_ac].rtja032, 
       g_rtja_d[l_ac].rtja033,g_rtja_d[l_ac].rtja034,g_rtja_d[l_ac].rtja035,g_rtja_d[l_ac].rtja036,g_rtja_d[l_ac].rtja037, 
       g_rtja_d[l_ac].rtja037_desc,g_rtja_d[l_ac].rtja038,g_rtja_d[l_ac].rtja039,g_rtja_d[l_ac].rtja040, 
       g_rtja_d[l_ac].rtja041,g_rtja_d[l_ac].rtja042,g_rtja_d[l_ac].rtja043,g_rtja_d[l_ac].rtja044,g_rtja_d[l_ac].rtja045, 
       g_rtja_d[l_ac].rtja046,g_rtja_d[l_ac].rtja047,g_rtja_d[l_ac].rtja048,g_rtja_d[l_ac].rtja049,g_rtja_d[l_ac].rtja050, 
       g_rtja_d[l_ac].rtja051,g_rtja_d[l_ac].rtja052,g_rtja_d[l_ac].rtjaunit,g_rtja_d[l_ac].rtjaunit_desc, 
       g_rtja_d[l_ac].rtjaownid,g_rtja_d[l_ac].rtjaownid_desc,g_rtja_d[l_ac].rtjaowndp,g_rtja_d[l_ac].rtjaowndp_desc, 
       g_rtja_d[l_ac].rtjacrtid,g_rtja_d[l_ac].rtjacrtid_desc,g_rtja_d[l_ac].rtjacrtdp,g_rtja_d[l_ac].rtjacrtdp_desc, 
       g_rtja_d[l_ac].rtjacrtdt,g_rtja_d[l_ac].rtjamodid,g_rtja_d[l_ac].rtjamodid_desc,g_rtja_d[l_ac].rtjamoddt, 
       g_rtja_d[l_ac].rtjacnfid,g_rtja_d[l_ac].rtjacnfid_desc,g_rtja_d[l_ac].rtjacnfdt,g_rtja_d[l_ac].rtjapstid, 
       g_rtja_d[l_ac].rtjapstid_desc,g_rtja_d[l_ac].rtjapstdt,g_rtja_d[l_ac].rtjastus
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #LET g_rtja_d[l_ac].statepic = cl_get_actipic(g_rtja_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL artq620_detail_show("'1'")      
 
      CALL artq620_rtja_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
   LET g_error_show = 0
   
 
   
   CALL g_rtja_d.deleteElement(g_rtja_d.getLength())   
 
   #add-point:陣列長度調整
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = g_rtja_d.getLength()
#  DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE artq620_pb
 
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL artq620_detail_index_setting()
 
   #重新計算單身筆數並呈現
