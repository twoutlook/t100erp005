                     " LEFT JOIN rtjd_t ON rtjdent = rtjaent AND rtjadocno = rtjddocno",
 
                     " LEFT JOIN rtje_t ON rtjeent = rtjaent AND rtjadocno = rtjedocno",
 
                     " LEFT JOIN rtjk_t ON rtjkent = rtjaent AND rtjadocno = rtjkdocno",
 
