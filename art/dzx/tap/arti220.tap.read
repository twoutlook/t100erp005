  <section id="arti220.detail_reproduce" ver="8" status="" src="s" readonly="">
    <![CDATA[#+ 單身自動複製
PRIVATE FUNCTION arti220_detail_reproduce()
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE inaa_t.*
 
 
   #add-point:delete段define
   {<point name="detail_reproduce.define" edit="s"/>}
   #end add-point    
   #add-point:delete段define
   {<point name="detail_reproduce.define_customerization" edit="c"/>}
   #end add-point 
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE arti220_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE arti220_detail AS ",
                "SELECT * FROM inaa_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO arti220_detail SELECT * FROM inaa_t 
                                         WHERE inaaent = g_enterprise AND inaasite = g_inaasite_t
 
   
   #將key修正為調整後   
   UPDATE arti220_detail 
      #更新key欄位
      SET inaasite = g_inaa_m.inaasite
 
      #更新共用欄位
      #應用 a13 樣板自動產生(Version:3)
       , inaaownid = g_user 
       , inaaowndp = g_dept
       , inaacrtid = g_user
       , inaacrtdp = g_dept 
       , inaacrtdt = ld_date
       , inaamodid = g_user
       , inaamoddt = ld_date
      #, inaastus = "Y" 
 
 
                                       
   #將資料塞回原table   
   INSERT INTO inaa_t SELECT * FROM arti220_detail
   
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "reproduce" 
      LET g_errparam.code   = SQLCA.sqlcode 
