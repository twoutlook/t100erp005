  <point name="function.arti220_inaa001" order="3" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#chk inaa001
PRIVATE FUNCTION arti220_inaa001(p_inaa001,p_cmd)
DEFINE l_cnt   LIKE type_t.num5
DEFINE l_cnt1  LIKE type_t.num5
DEFINE p_inaa001 LIKE inaa_t.inaa001
DEFINE p_cmd     LIKE type_t.chr1
DEFINE l_inaa101 LIKE inaa_t.inaa101

   LET l_cnt = 0
   LET l_cnt1 = 0
   LET g_errno = NULL
   SELECT inaa101 INTO l_inaa101 FROM inaa_t
    WHERE inaaent = g_enterprise AND inaa001 = p_inaa001
   IF p_cmd = 'a' THEN
      SELECT count(*) INTO l_cnt FROM inaa_t 
       WHERE inaa001 = p_inaa001 AND inaaent=g_enterprise
      IF l_cnt >0 THEN
         LET g_errno = "art-00143"
         IF l_inaa101 = '1' THEN
