         AFTER FIELD rtal001
            
            #add-point:AFTER FIELD rtal001
            {<point name="input.a.page1.rtal001" />}
            #END add-point
            
 
         #應用 a04 樣板自動產生(Version:2)
         ON CHANGE rtal001
            #add-point:ON CHANGE rtal001
