  <section id="arti050.construct" ver="4" status="" src="s" readonly="">
    <![CDATA[#+ QBE資料查詢
PRIVATE FUNCTION arti050_construct()
   DEFINE ls_return   STRING
   DEFINE ls_result   STRING 
   DEFINE ls_wc       STRING 
   #add-point:cs段define
   {<point name="cs.define" edit="s"/>}
   #end add-point 
    #add-point:cs段define
   {<point name="cs.define_customerization" edit="c"/>}
   #end add-point    
 
   #清除畫面上相關資料
   CLEAR FORM                 
   INITIALIZE g_rtal_m.* TO NULL
   CALL g_rtal_d.clear()
   CALL g_rtal2_d.clear()
 
   INITIALIZE g_wc TO NULL
   INITIALIZE g_wc2 TO NULL
   LET g_action_choice = ""
    
   LET g_qryparam.state = 'c'
   
   #add-point:cs段construct外
   {<point name="cs.head.before"/>}
   #end add-point 
   
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
      
      #單頭
      CONSTRUCT BY NAME g_wc ON rtalunit,rtalsite,rtal002
 
         BEFORE CONSTRUCT
            #add-point:cs段more_construct
            {<point name="cs.head.before_construct"/>}
            #end add-point 
            
                 #Ctrlp:construct.c.rtalunit
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD rtalunit
            #add-point:ON ACTION controlp INFIELD rtalunit
            {<point name="construct.c.rtalunit" />}
            #END add-point
 
         #應用 a01 樣板自動產生(Version:1)
         BEFORE FIELD rtalunit
            #add-point:BEFORE FIELD rtalunit
            {<point name="construct.b.rtalunit" />}
            #END add-point
 
         #應用 a02 樣板自動產生(Version:1)
         AFTER FIELD rtalunit
            
            #add-point:AFTER FIELD rtalunit
            {<point name="construct.a.rtalunit" />}
            #END add-point
            
 
         #應用 a01 樣板自動產生(Version:1)
         BEFORE FIELD rtalsite
            #add-point:BEFORE FIELD rtalsite
            {<point name="construct.b.rtalsite" />}
            #END add-point
 
         #應用 a02 樣板自動產生(Version:1)
         AFTER FIELD rtalsite
            
            #add-point:AFTER FIELD rtalsite
            {<point name="construct.a.rtalsite" />}
            #END add-point
            
 
         #Ctrlp:construct.c.rtalsite
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD rtalsite
            #add-point:ON ACTION controlp INFIELD rtalsite
            {<point name="construct.c.rtalsite" />}
            #END add-point
 
         #應用 a01 樣板自動產生(Version:1)
         BEFORE FIELD rtal002
            #add-point:BEFORE FIELD rtal002
            {<point name="construct.b.rtal002" />}
            #END add-point
 
         #應用 a02 樣板自動產生(Version:1)
         AFTER FIELD rtal002
            
            #add-point:AFTER FIELD rtal002
            {<point name="construct.a.rtal002" />}
            #END add-point
            
 
         #Ctrlp:construct.c.rtal002
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD rtal002
            #add-point:ON ACTION controlp INFIELD rtal002
            {<point name="construct.c.rtal002" />}
            #END add-point
 
 
         
      END CONSTRUCT
 
      CONSTRUCT g_wc2_table1 ON rtalstus,rtal001,rtall003,rtal015,rtal003,rtal004,rtal005,rtal006,rtal007, 
          rtal008,rtal009,rtal010,rtal011,rtal012,rtal013,rtal014,rtalownid,rtalowndp,rtalcrtid,rtalcrtdp, 
          rtalcrtdt,rtalmodid,rtalmoddt
