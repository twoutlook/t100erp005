  <section id="arti050.reproduce" ver="2" status="" src="s" readonly="">
    <![CDATA[#+ 資料複製
PRIVATE FUNCTION arti050_reproduce()
   DEFINE l_newno     LIKE rtal_t.rtalsite 
   DEFINE l_oldno     LIKE rtal_t.rtalsite 
   DEFINE l_newno02     LIKE rtal_t.rtal002 
   DEFINE l_oldno02     LIKE rtal_t.rtal002 
 
   DEFINE l_master    RECORD LIKE rtal_t.*
   DEFINE l_detail    RECORD LIKE rtal_t.*
 
   DEFINE l_cnt       LIKE type_t.num10
   #add-point:reproduce段define
   {<point name="reproduce.define" edit="s"/>}
   #end add-point
