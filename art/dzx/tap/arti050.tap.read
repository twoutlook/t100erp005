            {<point name="input.head.b_input"/>}
            #end add-point
          
                  #應用 a02 樣板自動產生(Version:1)
         AFTER FIELD rtalsite
            
            #add-point:AFTER FIELD rtalsite
            {<point name="input.a.rtalsite" />}
            #END add-point
            
 
         #應用 a01 樣板自動產生(Version:1)
         BEFORE FIELD rtalsite
            #add-point:BEFORE FIELD rtalsite
            {<point name="input.b.rtalsite" />}
            #END add-point
 
         #應用 a04 樣板自動產生(Version:2)
         ON CHANGE rtalsite
            #add-point:ON CHANGE rtalsite
            {<point name="input.g.rtalsite" />}
            #END add-point 
 
         #應用 a02 樣板自動產生(Version:1)
         AFTER FIELD rtal002
            
            #add-point:AFTER FIELD rtal002
            {<point name="input.a.rtal002" />}
            #END add-point
            
 
         #應用 a01 樣板自動產生(Version:1)
         BEFORE FIELD rtal002
            #add-point:BEFORE FIELD rtal002
            {<point name="input.b.rtal002" />}
            #END add-point
 
         #應用 a04 樣板自動產生(Version:2)
         ON CHANGE rtal002
            #add-point:ON CHANGE rtal002
            {<point name="input.g.rtal002" />}
            #END add-point 
 
 #欄位檢查
                  #Ctrlp:input.c.rtalsite
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD rtalsite
            #add-point:ON ACTION controlp INFIELD rtalsite
            {<point name="input.c.rtalsite" />}
            #END add-point
 
         #Ctrlp:input.c.rtal002
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD rtal002
            #add-point:ON ACTION controlp INFIELD rtal002
            {<point name="input.c.rtal002" />}
            #END add-point
 
 #欄位開窗
 
         AFTER INPUT
            IF INT_FLAG THEN
               EXIT DIALOG
            END IF
            
            IF s_transaction_chk("N",0) THEN
                CALL s_transaction_begin()
            END IF
            
            #錯誤訊息統整顯示
            #CALL cl_err_collect_show()
            #CALL cl_showmsg()
            DISPLAY BY NAME g_rtal_m.rtalsite             
                            ,g_rtal_m.rtal002   
 
 
            IF p_cmd = 'u' THEN
               #add-point:單頭修改前
               {<point name="input.head.b_update" mark="Y"/>}
               #end add-point
            
               #將遮罩欄位還原
               CALL arti050_rtal_t_mask_restore('restore_mask_o')
            
               UPDATE rtal_t SET (rtalunit,rtalsite,rtal002) = (g_rtal_m.rtalunit,g_rtal_m.rtalsite, 
                   g_rtal_m.rtal002)
                WHERE rtalent = g_enterprise AND rtalsite = g_rtalsite_t
                  AND rtal002 = g_rtal002_t
 
               #add-point:單頭修改中
               {<point name="input.head.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "rtal_t" 
                     LET g_errparam.code   = "std-00009" 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     CALL s_transaction_end('N','0')
                  WHEN SQLCA.sqlcode #其他錯誤
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "rtal_t" 
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                     #資料多語言用-增/改
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_rtal_m.rtalsite
               LET gs_keys_bak[1] = g_rtalsite_t
               LET gs_keys[2] = g_rtal_m.rtal002
               LET gs_keys_bak[2] = g_rtal002_t
               LET gs_keys[3] = g_rtal_d[g_detail_idx].rtal001
               LET gs_keys_bak[3] = g_rtal_d_t.rtal001
               CALL arti050_update_b('rtal_t',gs_keys,gs_keys_bak,"'1'")
                     
 
                     #add-point:單頭修改後
                     {<point name="input.head.a_update"/>}
                     #end add-point
    
                     #頭先不紀錄
                     #LET g_log1 = util.JSON.stringify(g_rtal_m_t)
                     #LET g_log2 = util.JSON.stringify(g_rtal_m)
                     #IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                     #   CALL s_transaction_end('N','0')
                     #ELSE
                        CALL s_transaction_end('Y','0')
                     #END IF
               END CASE
            
               #將遮罩欄位進行遮蔽
               CALL arti050_rtal_t_mask_restore('restore_mask_n')
            
            ELSE    
               #add-point:單頭新增
               {<point name="input.head.a_insert"/>}
               #end add-point
               
               #多語言處理
               
                         
               IF l_cmd_t = 'r' AND p_cmd = 'a' THEN
                  CALL arti050_detail_reproduce()
               END IF
            END IF
 
           LET g_rtalsite_t = g_rtal_m.rtalsite
           LET g_rtal002_t = g_rtal_m.rtal002
 
           
           IF g_rtal_d.getLength() = 0 THEN
              NEXT FIELD rtal001
           END IF
 
      END INPUT
]]>
