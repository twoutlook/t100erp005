              CALL FGL_SET_ARR_CURR(g_rtal_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL arti050_b_fill(g_wc2) #test 
            #如果一直都在單頭則控制筆數位置
            IF g_loc = 'm' THEN
               CALL FGL_SET_ARR_CURR(g_detail_idx)
            END IF
