  <section id="arti050.b_fill" ver="3" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION arti050_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define" edit="s"/>}
   #end add-point     
   #add-point:b_fill段define
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
   
   #先清空單身變數內容
   CALL g_rtal_d.clear()
   CALL g_rtal2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE rtalstus,rtal001,rtal015,rtal003,rtal004,rtal005,rtal006,rtal007,rtal008, 
       rtal009,rtal010,rtal011,rtal012,rtal013,rtal014,rtal001,rtalownid,rtalowndp,rtalcrtid,rtalcrtdp, 
       rtalcrtdt,rtalmodid,rtalmoddt FROM rtal_t",   
               " LEFT JOIN rtall_t ON rtallent = '"||g_enterprise||"' AND rtal001 = rtall001 AND rtall002 = '",g_dlang,"'",
               
               
               " WHERE rtalent= ? AND rtalsite=? AND rtal002=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("rtal_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF arti050_fill_chk(1) THEN
      IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
         LET g_sql = g_sql, " ORDER BY rtal_t.rtal001"
         #add-point:b_fill段fill_before
         {<point name="b_fill.fill_before"/>}
         #end add-point
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE arti050_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR arti050_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_rtal_m.rtalsite,g_rtal_m.rtal002
                                               
      FOREACH b_fill_cs INTO g_rtal_d[l_ac].rtalstus,g_rtal_d[l_ac].rtal001,g_rtal_d[l_ac].rtal015,g_rtal_d[l_ac].rtal003, 
          g_rtal_d[l_ac].rtal004,g_rtal_d[l_ac].rtal005,g_rtal_d[l_ac].rtal006,g_rtal_d[l_ac].rtal007, 
          g_rtal_d[l_ac].rtal008,g_rtal_d[l_ac].rtal009,g_rtal_d[l_ac].rtal010,g_rtal_d[l_ac].rtal011, 
          g_rtal_d[l_ac].rtal012,g_rtal_d[l_ac].rtal013,g_rtal_d[l_ac].rtal014,g_rtal2_d[l_ac].rtal001, 
          g_rtal2_d[l_ac].rtalownid,g_rtal2_d[l_ac].rtalowndp,g_rtal2_d[l_ac].rtalcrtid,g_rtal2_d[l_ac].rtalcrtdp, 
          g_rtal2_d[l_ac].rtalcrtdt,g_rtal2_d[l_ac].rtalmodid,g_rtal2_d[l_ac].rtalmoddt
                             
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #應用 a12 樣板自動產生(Version:3)
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF 
            EXIT FOREACH
         END IF
      
         LET l_ac = l_ac + 1
         
      END FOREACH
 
            CALL g_rtal_d.deleteElement(g_rtal_d.getLength())
      CALL g_rtal2_d.deleteElement(g_rtal2_d.getLength())
 
      
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_rtal_d.getLength()
      LET g_rtal_d_mask_o[l_ac].* =  g_rtal_d[l_ac].*
      CALL arti050_rtal_t_mask()
      LET g_rtal_d_mask_n[l_ac].* =  g_rtal_d[l_ac].*
   END FOR
   
   LET g_rtal2_d_mask_o.* =  g_rtal2_d.*
   FOR l_ac = 1 TO g_rtal2_d.getLength()
      LET g_rtal2_d_mask_o[l_ac].* =  g_rtal2_d[l_ac].*
      CALL arti050_rtal_t_mask()
      LET g_rtal2_d_mask_n[l_ac].* =  g_rtal2_d[l_ac].*
   END FOR
 
 
   FREE arti050_pb   
   
END FUNCTION
]]>
  </section>
