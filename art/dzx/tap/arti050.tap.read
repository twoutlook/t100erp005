            LET g_detail_multi_table_t.rtall001 = g_rtal_d[l_ac].rtal001
LET g_detail_multi_table_t.rtall002 = g_dlang
LET g_detail_multi_table_t.rtall003 = g_rtal_d[l_ac].rtall003
 
        
         BEFORE INSERT
            
            INITIALIZE g_rtal_d_t.* TO NULL
            INITIALIZE g_rtal_d_o.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_rtal_d[l_ac].* TO NULL
            #公用欄位預設值
            #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_rtal2_d[l_ac].rtalownid = g_user
      LET g_rtal2_d[l_ac].rtalowndp = g_dept
      LET g_rtal2_d[l_ac].rtalcrtid = g_user
      LET g_rtal2_d[l_ac].rtalcrtdp = g_dept 
      LET g_rtal2_d[l_ac].rtalcrtdt = cl_get_current()
      LET g_rtal2_d[l_ac].rtalmodid = g_user
      LET g_rtal2_d[l_ac].rtalmoddt = cl_get_current()
      LET g_rtal_d[l_ac].rtalstus = ''
 
  
            #一般欄位預設值
                  LET g_rtal_d[l_ac].rtalstus = "Y"
      LET g_rtal_d[l_ac].rtal003 = "1"
      LET g_rtal_d[l_ac].rtal004 = "2"
      LET g_rtal_d[l_ac].rtal006 = "N"
      LET g_rtal_d[l_ac].rtal007 = "1"
      LET g_rtal_d[l_ac].rtal008 = "0"
      LET g_rtal_d[l_ac].rtal009 = "1"
      LET g_rtal_d[l_ac].rtal010 = "0"
      LET g_rtal_d[l_ac].rtal012 = "1"
 
            
            #add-point:modify段before備份
            {<point name="input.body.before_insert.before_bak"/>}
            #end add-point
            LET g_rtal_d_t.* = g_rtal_d[l_ac].*     #新輸入資料
            LET g_rtal_d_o.* = g_rtal_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL arti050_set_entry_b(l_cmd)
            #add-point:set_entry_b後
            {<point name="input.body.before_insert.set_entry_b"/>}
            #end add-point
            CALL arti050_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_rtal_d[li_reproduce_target].* = g_rtal_d[li_reproduce].*
               LET g_rtal2_d[li_reproduce_target].* = g_rtal2_d[li_reproduce].*
 
               LET g_rtal_d[g_rtal_d.getLength()].rtal001 = NULL
 
            END IF
            LET g_detail_multi_table_t.rtall001 = g_rtal_d[l_ac].rtal001
LET g_detail_multi_table_t.rtall002 = g_dlang
LET g_detail_multi_table_t.rtall003 = g_rtal_d[l_ac].rtall003
 
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
 
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM rtal_t 
             WHERE rtalent = g_enterprise AND rtalsite = g_rtal_m.rtalsite
               AND rtal002 = g_rtal_m.rtal002
 
               AND rtal001 = g_rtal_d[l_ac].rtal001
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               CALL s_transaction_begin()
               #add-point:單身新增前
               {<point name="input.body.b_insert" mark="Y"/>}
               #end add-point
               INSERT INTO rtal_t
                           (rtalent,
                            rtalunit,rtalsite,rtal002,
                            rtal001
                            ,rtalstus,rtal015,rtal003,rtal004,rtal005,rtal006,rtal007,rtal008,rtal009,rtal010,rtal011,rtal012,rtal013,rtal014,rtalownid,rtalowndp,rtalcrtid,rtalcrtdp,rtalcrtdt,rtalmodid,rtalmoddt) 
                     VALUES(g_enterprise,
                            g_rtal_m.rtalunit,g_rtal_m.rtalsite,g_rtal_m.rtal002,
                            g_rtal_d[l_ac].rtal001
                            ,g_rtal_d[l_ac].rtalstus,g_rtal_d[l_ac].rtal015,g_rtal_d[l_ac].rtal003,g_rtal_d[l_ac].rtal004, 
                                g_rtal_d[l_ac].rtal005,g_rtal_d[l_ac].rtal006,g_rtal_d[l_ac].rtal007, 
                                g_rtal_d[l_ac].rtal008,g_rtal_d[l_ac].rtal009,g_rtal_d[l_ac].rtal010, 
                                g_rtal_d[l_ac].rtal011,g_rtal_d[l_ac].rtal012,g_rtal_d[l_ac].rtal013, 
                                g_rtal_d[l_ac].rtal014,g_rtal2_d[l_ac].rtalownid,g_rtal2_d[l_ac].rtalowndp, 
                                g_rtal2_d[l_ac].rtalcrtid,g_rtal2_d[l_ac].rtalcrtdp,g_rtal2_d[l_ac].rtalcrtdt, 
                                g_rtal2_d[l_ac].rtalmodid,g_rtal2_d[l_ac].rtalmoddt)
               #add-point:單身新增中
               {<point name="input.body.m_insert"/>}
               #end add-point
               LET p_cmd = 'u'
               LET g_master_insert = TRUE
            ELSE    
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = 'INSERT' 
               LET g_errparam.code   = "std-00006" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               INITIALIZE g_rtal_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "rtal_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #資料多語言用-增/改
                        INITIALIZE l_var_keys TO NULL 
         INITIALIZE l_field_keys TO NULL 
         INITIALIZE l_vars TO NULL 
         INITIALIZE l_fields TO NULL 
         INITIALIZE l_var_keys_bak TO NULL 
         IF g_rtal_d[l_ac].rtal001 = g_detail_multi_table_t.rtall001 AND
         g_rtal_d[l_ac].rtall003 = g_detail_multi_table_t.rtall003 THEN
         ELSE 
            LET l_var_keys[01] = g_enterprise
            LET l_field_keys[01] = 'rtallent'
            LET l_var_keys_bak[01] = g_enterprise
            LET l_var_keys[02] = g_rtal_d[l_ac].rtal001
            LET l_field_keys[02] = 'rtall001'
