   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE arti050_detail_lang AS ",
                "SELECT * FROM rtall_t "
   PREPARE repro_rtall_t FROM ls_sql
   EXECUTE repro_rtall_t
   FREE repro_rtall_t
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO arti050_detail_lang SELECT * FROM rtall_t 
                                             WHERE rtallent = g_enterprise AND rtall001 = g_rtalsite_t
                                             AND rtall002 = g_rtal002_t
   #將key修正為調整後   
   UPDATE arti050_detail_lang 
      #更新key欄位
