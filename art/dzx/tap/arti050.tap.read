            CALL arti050_idx_chk()
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN arti050_cl USING g_enterprise,g_rtal_m.rtalsite,g_rtal_m.rtal002                          
               IF STATUS THEN
                  INITIALIZE g_errparam TO NULL 
