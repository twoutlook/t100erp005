  <section id="arti050.modify" ver="3" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION arti050_modify()
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point  
   #add-point:modify段define
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point    
   
   IF g_rtal_m.rtalsite IS NULL
   OR g_rtal_m.rtal002 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
   LET g_rtalsite_t = g_rtal_m.rtalsite
   LET g_rtal002_t = g_rtal_m.rtal002
 
   CALL s_transaction_begin()
   
   OPEN arti050_cl USING g_enterprise,g_rtal_m.rtalsite,g_rtal_m.rtal002
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN arti050_cl:" 
      LET g_errparam.code   =  STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE arti050_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE arti050_master_referesh USING g_rtal_m.rtalsite,g_rtal_m.rtal002 INTO g_rtal_m.rtalunit,g_rtal_m.rtalsite, 
       g_rtal_m.rtal002,g_rtal_m.rtalunit_desc,g_rtal_m.rtalsite_desc,g_rtal_m.rtal002_desc
   
   #遮罩相關處理
   LET g_rtal_m_mask_o.* =  g_rtal_m.*
   CALL arti050_rtal_t_mask()
   LET g_rtal_m_mask_n.* =  g_rtal_m.*
   
   CALL s_transaction_end('Y','0')
 
   CALL arti050_show()
   WHILE TRUE
      LET g_rtalsite_t = g_rtal_m.rtalsite
      LET g_rtal002_t = g_rtal_m.rtal002
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL arti050_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_rtal_m.* = g_rtal_m_t.*
         CALL arti050_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更(更新單身table的key欄位值)
      IF g_rtal_m.rtalsite != g_rtalsite_t 
      OR g_rtal_m.rtal002 != g_rtal002_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("modify,modify_detail,delete,reproduce", TRUE)
   CALL arti050_set_act_visible()
   CALL arti050_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " rtalent = '" ||g_enterprise|| "' AND",
                      " rtalsite = '", g_rtal_m.rtalsite, "' "
                      ," AND rtal002 = '", g_rtal_m.rtal002, "' "
 
   #填到對應位置
   CALL arti050_browser_fill("")
 
   CALL arti050_idx_chk()
 
   CLOSE arti050_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL arti050_msgcentre_notify('modify')
   
END FUNCTION   
]]>
  </section>
