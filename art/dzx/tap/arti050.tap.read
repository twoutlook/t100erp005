               AND g_rtal_d[l_ac].rtal001 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_rtal_d_t.* = g_rtal_d[l_ac].*  #BACKUP
               LET g_rtal_d_o.* = g_rtal_d[l_ac].*  #BACKUP
               CALL arti050_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL arti050_set_no_entry_b(l_cmd)
               OPEN arti050_bcl USING g_enterprise,g_rtal_m.rtalsite,
                                                g_rtal_m.rtal002,
 
                                                g_rtal_d_t.rtal001
 
               IF STATUS THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "OPEN arti050_bcl:" 
                  LET g_errparam.code   =  STATUS 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  LET l_lock_sw='Y'
               ELSE
                  FETCH arti050_bcl INTO g_rtal_d[l_ac].rtalstus,g_rtal_d[l_ac].rtal001,g_rtal_d[l_ac].rtal015, 
                      g_rtal_d[l_ac].rtal003,g_rtal_d[l_ac].rtal004,g_rtal_d[l_ac].rtal005,g_rtal_d[l_ac].rtal006, 
                      g_rtal_d[l_ac].rtal007,g_rtal_d[l_ac].rtal008,g_rtal_d[l_ac].rtal009,g_rtal_d[l_ac].rtal010, 
                      g_rtal_d[l_ac].rtal011,g_rtal_d[l_ac].rtal012,g_rtal_d[l_ac].rtal013,g_rtal_d[l_ac].rtal014, 
                      g_rtal2_d[l_ac].rtal001,g_rtal2_d[l_ac].rtalownid,g_rtal2_d[l_ac].rtalowndp,g_rtal2_d[l_ac].rtalcrtid, 
                      g_rtal2_d[l_ac].rtalcrtdp,g_rtal2_d[l_ac].rtalcrtdt,g_rtal2_d[l_ac].rtalmodid, 
                      g_rtal2_d[l_ac].rtalmoddt
                  IF SQLCA.sqlcode THEN
                      INITIALIZE g_errparam TO NULL 
                      LET g_errparam.extend = g_rtal_d_t.rtal001 
                      LET g_errparam.code   = SQLCA.sqlcode 
                      LET g_errparam.popup  = TRUE 
                      CALL cl_err()
                      LET l_lock_sw = "Y"
                  END IF
                  
                  #遮罩相關處理
                  LET g_rtal_d_mask_o[l_ac].* =  g_rtal_d[l_ac].*
