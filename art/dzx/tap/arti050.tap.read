   {<point name="reproduce.define_customerization" edit="c"/>}
   #end add-point
 
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF     
 
   IF g_rtal_m.rtalsite IS NULL
      OR g_rtal_m.rtal002 IS NULL
 
      THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
   
   LET g_rtalsite_t = g_rtal_m.rtalsite
   LET g_rtal002_t = g_rtal_m.rtal002
 
   
   LET g_rtal_m.rtalsite = ""
   LET g_rtal_m.rtal002 = ""
 
   LET g_master_insert = FALSE
   CALL arti050_set_entry('a')
   CALL arti050_set_no_entry('a')
   
   CALL cl_set_head_visible("","YES")
   CALL s_transaction_begin()
   
   #add-point:複製輸入前
   {<point name="reproduce.head.b_input"/>}
   #end add-point
   
   #清空key欄位的desc
      LET g_rtal_m.rtalsite_desc = ''
   DISPLAY BY NAME g_rtal_m.rtalsite_desc
   LET g_rtal_m.rtal002_desc = ''
   DISPLAY BY NAME g_rtal_m.rtal002_desc
 
   
   CALL arti050_input("r")
    
   IF INT_FLAG AND NOT g_master_insert THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = '' 
      LET g_errparam.code   = 9001 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
      DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
      INITIALIZE g_rtal_m.* TO NULL
      INITIALIZE g_rtal_d TO NULL
      INITIALIZE g_rtal2_d TO NULL
 
      CALL arti050_show()
      LET INT_FLAG = 0
      RETURN 
   END IF
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("modify,modify_detail,delete,reproduce", TRUE)
   CALL arti050_set_act_visible()
   CALL arti050_set_act_no_visible()
 
   #將新增的資料併入搜尋條件中
   LET g_state = "insert"
   
   LET g_rtalsite_t = g_rtal_m.rtalsite
   LET g_rtal002_t = g_rtal_m.rtal002
 
   
   #組合新增資料的條件
   LET g_add_browse = " rtalent = '" ||g_enterprise|| "' AND",
                      " rtalsite = '", g_rtal_m.rtalsite, "' "
                      ," AND rtal002 = '", g_rtal_m.rtal002, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL arti050_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   LET g_curr_diag = ui.DIALOG.getCurrent()
   CALL arti050_idx_chk()
   
   #add-point:完成複製段落後
   {<point name="reproduce.after_reproduce" />}
   #end add-point
   
   #功能已完成,通報訊息中心
   CALL arti050_msgcentre_notify('reproduce')
   
END FUNCTION
