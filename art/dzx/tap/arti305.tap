            LET g_sql = ""
            IF NOT cl_null(g_imck_m.imck009) THEN
               LET r_success = ''
               LET r_errno = ''
               LET r_rtax001 = ''
               CALL s_arti202_search_manage_level(g_imck_m.imck009) 
                  RETURNING r_success,r_errno,r_rtax001
               IF r_success = FALSE THEN LET r_rtax001 = '' END IF
            END IF
            CASE
               WHEN cl_null(g_imck_m.imck009) AND NOT cl_null(g_imck_m.imck126)
                  LET g_sql = " (dbbb002 = '2' AND dbbb003 = '",g_imck_m.imck126,"')"
               WHEN NOT cl_null(g_imck_m.imck009) AND cl_null(g_imck_m.imck126)
                  LET g_sql = " (dbbb002 = '1' AND dbbb003 = '",r_rtax001,"')"
               WHEN NOT cl_null(g_imck_m.imck009) AND NOT cl_null(g_imck_m.imck126)
                  LET g_sql = " ((dbbb002 = '1' AND dbbb003 = '",r_rtax001,"') OR (dbbb002 = '2' AND dbbb003 = '",g_imck_m.imck126,"'))"
            END CASE
