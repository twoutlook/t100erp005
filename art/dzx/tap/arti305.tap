################################################################################
PUBLIC FUNCTION arti305_imck143_ref()
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_imck_m.imck143
   CALL ap_ref_array2(g_ref_fields," SELECT dbbal003 FROM dbbal_t WHERE dbbalent = '"||g_enterprise||"' AND dbbal001 = ? AND dbbal002 = '"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_imck_m.imck143_desc = g_rtn_fields[1]
   DISPLAY BY NAME g_imck_m.imck143_desc
END FUNCTION]]>
