  <section id="arti305.fetch" ver="4" status="" src="s">
<![CDATA[#+ 指定PK後抓取單頭其他資料
PRIVATE FUNCTION arti305_fetch(p_fl)
   DEFINE p_fl       LIKE type_t.chr1
   DEFINE ls_msg     STRING
   #add-point:fetch段define
   {<point name="fetch.define"/>}
   #end add-point  
   
   CASE p_fl
      WHEN "F" LET g_current_idx = 1
      WHEN "P"
         IF g_current_idx > 1 THEN               
            LET g_current_idx = g_current_idx - 1
         END IF 
      WHEN "N"
         IF g_current_idx < g_header_cnt THEN
            LET g_current_idx =  g_current_idx + 1
         END IF        
      WHEN "L" 
         LET g_current_idx = g_header_cnt        
      WHEN "/"
         IF (NOT g_no_ask) THEN      
            CALL cl_getmsg("fetch", g_lang) RETURNING ls_msg
            LET INT_FLAG = 0
 
            PROMPT ls_msg CLIPPED,": " FOR g_jump
               #交談指令共用ACTION
               &include "common_action.4gl"
            END PROMPT
            
            IF INT_FLAG THEN
               LET INT_FLAG = 0
               EXIT CASE  
            END IF           
         END IF
         IF g_jump > 0 THEN
            LET g_current_idx = g_jump
         END IF
         LET g_no_ask = FALSE     
   END CASE
   
   LET g_browser_cnt = g_browser.getLength()
 
   #瀏覽頁筆數顯示
   LET g_browser_idx = g_pagestart+g_current_idx-1
   DISPLAY g_browser_idx TO FORMONLY.b_index        #當下筆數
   DISPLAY g_browser_cnt TO FORMONLY.b_count        #總筆數
   DISPLAY g_browser_idx TO FORMONLY.h_index        #當下筆數
   DISPLAY g_browser_cnt TO FORMONLY.h_count        #總筆數
   CALL ui.Interface.refresh()
   
   #單頭筆數顯示
   #DISPLAY g_browser_idx TO FORMONLY.idx            #當下筆數
   #DISPLAY g_browser_cnt TO FORMONLY.cnt            #總筆數
   
   
   
   IF g_current_idx > g_browser.getLength() THEN
      LET g_current_idx = g_browser.getLength()
   END IF
   
   # 設定browse索引
 
   CALL cl_navigator_setting(g_browser_idx, g_browser_cnt )
 
   #代表沒有資料, 無需做後續資料撈取之動作
   IF g_current_idx = 0 THEN
      RETURN
   END IF
 
   LET g_imck_m.imck001 = g_browser[g_current_idx].b_imck001
 
                       
   #重讀DB,因TEMP有不被更新特性
   EXECUTE arti305_master_referesh USING g_imck_m.imck001 INTO g_imck_m.imck001,g_imck_m.imck108,g_imck_m.imck100, 
       g_imck_m.imck109,g_imck_m.imck105,g_imck_m.imck104,g_imck_m.imck107,g_imck_m.imck106,g_imck_m.imck113, 
       g_imck_m.imck142,g_imck_m.imckstus,g_imck_m.imckownid,g_imck_m.imckowndp,g_imck_m.imckcrtid,g_imck_m.imckcrtdp, 
       g_imck_m.imckcrtdt,g_imck_m.imckmodid,g_imck_m.imckmoddt,g_imck_m.imck009,g_imck_m.imck101,g_imck_m.imck102, 
       g_imck_m.imck103,g_imck_m.imck118,g_imck_m.imck119,g_imck_m.imck120,g_imck_m.imck114,g_imck_m.imck124, 
       g_imck_m.imck125,g_imck_m.imck121,g_imck_m.imck110,g_imck_m.imck111,g_imck_m.imck112,g_imck_m.imck122, 
       g_imck_m.imck123,g_imck_m.imck131,g_imck_m.imck126,g_imck_m.imck127,g_imck_m.imck128,g_imck_m.imck129, 
       g_imck_m.imck130,g_imck_m.imck143,g_imck_m.imck132,g_imck_m.imck133,g_imck_m.imck134,g_imck_m.imck135, 
       g_imck_m.imck136,g_imck_m.imck137,g_imck_m.imck138,g_imck_m.imck139,g_imck_m.imck140,g_imck_m.imck141 

   IF SQLCA.sqlcode THEN
      CALL cl_err("imck_t",SQLCA.sqlcode,0)
      INITIALIZE g_imck_m.* TO NULL
      RETURN
   END IF
   
   #add-point:fetch段action控制
   {<point name="fetch.action_control"/>}
   #end add-point  
   
   
   
   #重新顯示
