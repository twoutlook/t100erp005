  <point name="function.arti203_def_rtaj007" order="1" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 自定義條碼格式預設值
# Memo...........:
# Usage..........: CALL arti203_def_rtaj007()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/02/18 By xumm
# Modify.........:
################################################################################
PRIVATE FUNCTION arti203_def_rtaj007()
DEFINE l_str            STRING
DEFINE l_rtaj002        STRING
DEFINE l_rtaj003        STRING
DEFINE l_num            LIKE type_t.num5   
   
   IF cl_null(g_rtaj_d[l_ac].rtaj003) OR cl_null(g_rtaj_d[l_ac].rtaj002) THEN
      RETURN
   END IF
   IF g_rtaj_d[l_ac].rtaj003 = '0' THEN
      LET l_rtaj003 = 'XXXXXXXXXX'
      LET l_num = 10
   END IF
   IF g_rtaj_d[l_ac].rtaj003 = '1' THEN
      LET l_rtaj003 = 'XXXXXXXXXXXXXXX'
      LET l_num = 15
   END IF
   CASE g_rtaj_d[l_ac].rtaj002
      WHEN '0'  LET l_rtaj002 = '20'
      WHEN '1'  LET l_rtaj002 = '21'
      WHEN '2'  LET l_rtaj002 = '22'
      WHEN '3'  LET l_rtaj002 = '23'
      WHEN '4'  LET l_rtaj002 = '24'
   END CASE
   LET l_str = l_rtaj002,l_rtaj003,'C'
   IF g_rtaj_d[l_ac].rtaj001 = '1' OR g_rtaj_d[l_ac].rtaj001 = '2' OR g_rtaj_d[l_ac].rtaj001 = '3' THEN
      LET g_rtaj_d[l_ac].rtaj007 = l_str
      LET g_rtaj_d[l_ac].rtaj009 = 1
      LET g_rtaj_d[l_ac].rtaj010 = 2
      LET g_rtaj_d[l_ac].rtaj011 = 3
      LET g_rtaj_d[l_ac].rtaj012 = l_num+2
      LET g_rtaj_d[l_ac].rtaj013 = 0
      LET g_rtaj_d[l_ac].rtaj014 = 0
      LET g_rtaj_d[l_ac].rtaj015 = 0
      LET g_rtaj_d[l_ac].rtaj016 = 0
      LET g_rtaj_d[l_ac].rtaj017 = 0
      LET g_rtaj_d[l_ac].rtaj018 = 0     
      LET g_rtaj_d[l_ac].rtaj019 = l_num+3
      LET g_rtaj_d[l_ac].rtaj020 = l_num+3    
   END IF
   CALL arti203_rtaj007_apart()
END FUNCTION]]>
  </point>
  <point name="function.arti203_rtaj007_apart" order="2" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 自定義條碼格式拆分存儲
# Memo...........:
# Usage..........: CALL arti203_rtaj007_apart
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2013/02/18 By xumm
# Modify.........:
################################################################################
PRIVATE FUNCTION arti203_rtaj007_apart()
DEFINE l_rtaj007            STRING
DEFINE l_i                  LIKE type_t.num5
DEFINE l_length             LIKE type_t.num5
DEFINE l_x                  LIKE type_t.chr1
DEFINE l_m                  LIKE type_t.chr1
DEFINE l_w                  LIKE type_t.chr1
DEFINE l_p                  LIKE type_t.chr1

   LET l_rtaj007 = g_rtaj_d[l_ac].rtaj007 
   LET l_length = l_rtaj007.getLength()
   LET g_rtaj_d[l_ac].rtaj009 = 1
   LET g_rtaj_d[l_ac].rtaj010 = 2
   LET g_rtaj_d[l_ac].rtaj011 = l_rtaj007.getIndexOf('X',1)
   LET g_rtaj_d[l_ac].rtaj012 = g_rtaj_d[l_ac].rtaj011 
   FOR l_i = g_rtaj_d[l_ac].rtaj011+1 TO l_length
       LET l_x = l_rtaj007.subString(l_i,l_i)
       IF l_x = 'X' THEN
          LET g_rtaj_d[l_ac].rtaj012 = g_rtaj_d[l_ac].rtaj012 +1
       END IF
   END FOR
   LET g_rtaj_d[l_ac].rtaj013 = l_rtaj007.getIndexOf('M',1)
   LET g_rtaj_d[l_ac].rtaj014 = g_rtaj_d[l_ac].rtaj013 
   IF g_rtaj_d[l_ac].rtaj013 > 0 THEN
      FOR l_i = g_rtaj_d[l_ac].rtaj013+1 TO l_length
          LET l_m = l_rtaj007.subString(l_i,l_i)
          IF l_m = 'M' THEN
             LET g_rtaj_d[l_ac].rtaj014 = g_rtaj_d[l_ac].rtaj014 +1
          END IF
      END FOR
   ELSE
      LET g_rtaj_d[l_ac].rtaj014 = 0
   END IF
   LET g_rtaj_d[l_ac].rtaj015 = l_rtaj007.getIndexOf('W',1)
   LET g_rtaj_d[l_ac].rtaj016 = g_rtaj_d[l_ac].rtaj015 
   IF g_rtaj_d[l_ac].rtaj015 > 0 THEN
      FOR l_i = g_rtaj_d[l_ac].rtaj015+1 TO l_length
          LET l_w = l_rtaj007.subString(l_i,l_i)
          IF l_w = 'W' THEN
             LET g_rtaj_d[l_ac].rtaj016 = g_rtaj_d[l_ac].rtaj016 +1
          END IF
      END FOR
   ELSE
      LET g_rtaj_d[l_ac].rtaj016 = 0
   END IF
   LET g_rtaj_d[l_ac].rtaj017 = l_rtaj007.getIndexOf('P',1)
   LET g_rtaj_d[l_ac].rtaj018 = g_rtaj_d[l_ac].rtaj017
   IF g_rtaj_d[l_ac].rtaj017 > 0 THEN
      FOR l_i = g_rtaj_d[l_ac].rtaj017+1 TO l_length
          LET l_p = l_rtaj007.subString(l_i,l_i)
          IF l_p = 'P' THEN
             LET g_rtaj_d[l_ac].rtaj018 = g_rtaj_d[l_ac].rtaj018 +1
          END IF
      END FOR
   ELSE
      LET g_rtaj_d[l_ac].rtaj018 = 0
   END IF   
   LET g_rtaj_d[l_ac].rtaj019 = l_length
   LET g_rtaj_d[l_ac].rtaj020 = l_length
END FUNCTION]]>
