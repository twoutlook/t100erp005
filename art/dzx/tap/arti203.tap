  <section id="arti203.b_fill" ver="16" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION arti203_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2    STRING
   #add-point:b_fill段define
   {<point name="b_fill.define" edit="s"/>}
   #end add-point
   #add-point:b_fill段define(客製用)
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE t0.rtajstus,t0.rtaj001,t0.rtaj002,t0.rtaj003,t0.rtaj004,t0.rtaj005,t0.rtaj006, 
       t0.rtaj007,t0.rtaj008,t0.rtaj009,t0.rtaj010,t0.rtaj011,t0.rtaj012,t0.rtaj013,t0.rtaj014,t0.rtaj015, 
       t0.rtaj016,t0.rtaj017,t0.rtaj018,t0.rtaj019,t0.rtaj020,t0.rtaj021,t0.rtajstamp,t0.rtaj001,t0.rtajmodid, 
       t0.rtajmoddt,t0.rtajownid,t0.rtajowndp,t0.rtajcrtid,t0.rtajcrtdp,t0.rtajcrtdt ,t1.ooag011 ,t2.ooag011 , 
       t3.ooefl003 ,t4.ooag011 ,t5.ooefl003 FROM rtaj_t t0",
               "",
                              " LEFT JOIN ooag_t t1 ON t1.ooagent='"||g_enterprise||"' AND t1.ooag001=t0.rtajmodid  ",
               " LEFT JOIN ooag_t t2 ON t2.ooagent='"||g_enterprise||"' AND t2.ooag001=t0.rtajownid  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent='"||g_enterprise||"' AND t3.ooefl001=t0.rtajowndp AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t4 ON t4.ooagent='"||g_enterprise||"' AND t4.ooag001=t0.rtajcrtid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=t0.rtajcrtdp AND t5.ooefl002='"||g_dlang||"' ",
 
               " WHERE t0.rtajent= ?  AND  1=1 AND (", p_wc2, ") " 
   #add-point:b_fill段sql wc
   {<point name="b_fill.sql_wc"/>}
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("rtaj_t"),
                      " ORDER BY t0.rtaj001"
   
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"rtaj_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE arti203_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR arti203_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_rtaj_d.clear()
   CALL g_rtaj2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_rtaj_d[l_ac].rtajstus,g_rtaj_d[l_ac].rtaj001,g_rtaj_d[l_ac].rtaj002,g_rtaj_d[l_ac].rtaj003, 
       g_rtaj_d[l_ac].rtaj004,g_rtaj_d[l_ac].rtaj005,g_rtaj_d[l_ac].rtaj006,g_rtaj_d[l_ac].rtaj007,g_rtaj_d[l_ac].rtaj008, 
       g_rtaj_d[l_ac].rtaj009,g_rtaj_d[l_ac].rtaj010,g_rtaj_d[l_ac].rtaj011,g_rtaj_d[l_ac].rtaj012,g_rtaj_d[l_ac].rtaj013, 
       g_rtaj_d[l_ac].rtaj014,g_rtaj_d[l_ac].rtaj015,g_rtaj_d[l_ac].rtaj016,g_rtaj_d[l_ac].rtaj017,g_rtaj_d[l_ac].rtaj018, 
       g_rtaj_d[l_ac].rtaj019,g_rtaj_d[l_ac].rtaj020,g_rtaj_d[l_ac].rtaj021,g_rtaj_d[l_ac].rtajstamp, 
       g_rtaj2_d[l_ac].rtaj001,g_rtaj2_d[l_ac].rtajmodid,g_rtaj2_d[l_ac].rtajmoddt,g_rtaj2_d[l_ac].rtajownid, 
       g_rtaj2_d[l_ac].rtajowndp,g_rtaj2_d[l_ac].rtajcrtid,g_rtaj2_d[l_ac].rtajcrtdp,g_rtaj2_d[l_ac].rtajcrtdt, 
       g_rtaj2_d[l_ac].rtajmodid_desc,g_rtaj2_d[l_ac].rtajownid_desc,g_rtaj2_d[l_ac].rtajowndp_desc, 
       g_rtaj2_d[l_ac].rtajcrtid_desc,g_rtaj2_d[l_ac].rtajcrtdp_desc
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL arti203_detail_show()      
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
 
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   LET g_error_show = 0
   
 
   
   CALL g_rtaj_d.deleteElement(g_rtaj_d.getLength())   
   CALL g_rtaj2_d.deleteElement(g_rtaj2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_rtaj_d.getLength()
      LET g_rtaj2_d[l_ac].rtaj001 = g_rtaj_d[l_ac].rtaj001 
 
      #add-point:b_fill段key值相關欄位
      {<point name="b_fill.keys.fill"/>}
      #end add-point
   END FOR
   
   IF g_cnt > g_rtaj_d.getLength() THEN
      LET l_ac = g_rtaj_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_rtaj_d.getLength()
      LET g_rtaj_d_mask_o[l_ac].* =  g_rtaj_d[l_ac].*
      CALL arti203_rtaj_t_mask()
      LET g_rtaj_d_mask_n[l_ac].* =  g_rtaj_d[l_ac].*
   END FOR
   
   LET g_rtaj2_d_mask_o.* =  g_rtaj2_d.*
   FOR l_ac = 1 TO g_rtaj2_d.getLength()
      LET g_rtaj2_d_mask_o[l_ac].* =  g_rtaj2_d[l_ac].*
      CALL arti203_rtaj_t_mask()
      LET g_rtaj2_d_mask_n[l_ac].* =  g_rtaj2_d[l_ac].*
   END FOR
 
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_rtaj_d.getLength()
   DISPLAY g_detail_idx TO FORMONLY.idx
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE arti203_pb
   
END FUNCTION
]]>
  </section>
