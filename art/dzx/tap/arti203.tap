  <point name="function.arti203_chk_rtaj007" order="3" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 自定義條碼格式檢查
# Memo...........:
# Usage..........: CALL arti203_chk_rtaj007()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/02/18 By xumm
# Modify.........:
################################################################################
PRIVATE FUNCTION arti203_chk_rtaj007()
DEFINE l_rtaj007            STRING
DEFINE l_i                  LIKE type_t.num5
DEFINE l_length             LIKE type_t.num5
DEFINE l_chr                LIKE type_t.chr80
DEFINE l_n                  LIKE type_t.num5
DEFINE l_n1                 LIKE type_t.num5
DEFINE l_n2                 LIKE type_t.num5
DEFINE l_n3                 LIKE type_t.num5
DEFINE l_s                  LIKE type_t.chr1
DEFINE l_x                  LIKE type_t.num5
DEFINE l_m                  LIKE type_t.num5
DEFINE l_w                  LIKE type_t.num5
DEFINE l_p                  LIKE type_t.num5
DEFINE l_rtaj002            LIKE type_t.chr2

   LET g_errno = ''
   IF cl_null(g_rtaj_d[l_ac].rtaj001) OR cl_null(g_rtaj_d[l_ac].rtaj003) OR cl_null(g_rtaj_d[l_ac].rtaj002) THEN
      RETURN
   END IF
   LET g_rtaj_d[l_ac].rtaj007 = UPSHIFT(g_rtaj_d[l_ac].rtaj007)
   LET l_rtaj007 = g_rtaj_d[l_ac].rtaj007 
   LET l_length = l_rtaj007.getLength()
   LET l_chr = l_rtaj007.subString(1,2)
   CASE g_rtaj_d[l_ac].rtaj002
      WHEN '0'  LET l_rtaj002 = '20'
      WHEN '1'  LET l_rtaj002 = '21'
      WHEN '2'  LET l_rtaj002 = '22'
      WHEN '3'  LET l_rtaj002 = '23'
      WHEN '4'  LET l_rtaj002 = '24'
   END CASE
   IF l_chr <> l_rtaj002 THEN
      LET g_errno = 'art-00240'
      RETURN
   END IF
   LET l_chr = l_rtaj007.subString(l_length,l_length)
   IF l_chr <> 'C' THEN
      LET g_errno = 'art-00240'
      RETURN
   END IF
   IF g_rtaj_d[l_ac].rtaj003 = '0' THEN
      IF l_length <> 13 THEN
         LET g_errno = 'art-00241'
         RETURN
      END IF
   END IF
   IF g_rtaj_d[l_ac].rtaj003 = '1' THEN
      IF l_length <> 18 THEN
         LET g_errno = 'art-00241'
         RETURN
      END IF
   END IF
   
   LET l_n = 0
   LET l_n1 = 0
   LET l_n2 = 0
   LET l_n3 = 0
   FOR l_i = 3 TO l_length-1
       LET l_s = l_rtaj007.subString(l_i,l_i)
       IF l_s <> 'X' AND l_s <> 'M' AND  l_s <> 'W' AND l_s <> 'P' THEN
          LET g_errno = 'art-00242'
          RETURN
       END IF
       IF l_s = 'X' THEN
          LET l_n = l_n + 1
       END IF
       IF l_s = 'M' THEN
          IF g_rtaj_d[l_ac].rtaj004 = '0' THEN
             LET g_errno = 'art-00247'
             RETURN
          END IF
          LET l_n1 = l_n1 + 1
       END IF
       IF l_s = 'W' THEN
          IF g_rtaj_d[l_ac].rtaj005 = '0' THEN
             LET g_errno = 'art-00248'
             RETURN
          END IF
          LET l_n2 = l_n2 + 1
       END IF
       IF l_s = 'P' THEN
          IF g_rtaj_d[l_ac].rtaj006 = '0' THEN
             LET g_errno = 'art-00249'
             RETURN
          END IF
          LET l_n3 = l_n3 + 1
       END IF
   END FOR
   IF l_n < 5 THEN
      LET g_errno = 'art-00243'
      RETURN
   END IF
   LET l_x = l_rtaj007.getIndexOf('X',1)
   LET l_chr = l_rtaj007.subString(l_x,l_x+l_n-1)
   FOR l_i = l_x TO l_x+l_n-1
      LET l_s = l_rtaj007.subString(l_i,l_i) 
      IF l_s <> 'X' THEN
         LET g_errno = 'art-00244'
         RETURN
      END IF
   END FOR 
   LET l_m = l_rtaj007.getIndexOf('M',1)
   LET l_chr = l_rtaj007.subString(l_m,l_m+l_n1-1)
   FOR l_i = l_m TO l_m+l_n1-1
      LET l_s = l_rtaj007.subString(l_i,l_i) 
      IF l_s <> 'M' THEN
         LET g_errno = 'art-00244'
         RETURN
      END IF
   END FOR
   LET l_w = l_rtaj007.getIndexOf('W',1)
   LET l_chr = l_rtaj007.subString(l_w,l_w+l_n2-1)
   FOR l_i = l_w TO l_w+l_n2-1
      LET l_s = l_rtaj007.subString(l_i,l_i) 
      IF l_s <> 'W' THEN
         LET g_errno = 'art-00244'
         RETURN
      END IF
   END FOR
   LET l_p = l_rtaj007.getIndexOf('P',1)
   LET l_chr = l_rtaj007.subString(l_p,l_p+l_n3-1)
   FOR l_i = l_p TO l_p+l_n3-1
      LET l_s = l_rtaj007.subString(l_i,l_i) 
      IF l_s <> 'P' THEN
         LET g_errno = 'art-00244'
         RETURN
      END IF
   END FOR
END FUNCTION]]>
