  <section id="arti203.update_b" ver="6" status="" src="s" readonly="">
    <![CDATA[#+ 修改單身後其他table連動
PRIVATE FUNCTION arti203_update_b(ps_table,ps_keys,ps_keys_bak,ps_page)
   DEFINE ps_page          STRING
   DEFINE ps_table         STRING
   DEFINE ps_keys          DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ps_keys_bak      DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ls_group         STRING
   DEFINE li_idx           LIKE type_t.num10
   DEFINE lb_chk           BOOLEAN
   DEFINE l_new_key        DYNAMIC ARRAY OF STRING
   DEFINE l_old_key        DYNAMIC ARRAY OF STRING
   DEFINE l_field_key      DYNAMIC ARRAY OF STRING
   #add-point:update_b段define
   {<point name="update_b.define" edit="s"/>}
   #end add-point     
   #add-point:update_b段define(客製用)
   {<point name="update_b.define_customerization" edit="c"/>}
   #end add-point
   
   #比對新舊值, 判斷key是否有改變
   LET lb_chk = TRUE
   FOR li_idx = 1 TO ps_keys.getLength()
      IF ps_keys[li_idx] <> ps_keys_bak[li_idx] THEN
         LET lb_chk = FALSE
         EXIT FOR
      END IF
   END FOR
   
   #若key無變動, 不需要做處理
   IF lb_chk THEN
      RETURN
   END IF
    
   #若key有變動, 則連動其他table的資料   
   #判斷是否是同一群組的table
   LET ls_group = "rtaj_t,"
   IF ls_group.getIndexOf(ps_table,1) > 0 AND ps_table <> "rtaj_t" THEN
      #add-point:update_b段修改前
      {<point name="update_b.b_update" mark="Y"/>}
      #end add-point     
      UPDATE rtaj_t 
         SET (rtaj001
              ,rtajstus,rtaj002,rtaj003,rtaj004,rtaj005,rtaj006,rtaj007,rtaj008,rtaj009,rtaj010,rtaj011,rtaj012,rtaj013,rtaj014,rtaj015,rtaj016,rtaj017,rtaj018,rtaj019,rtaj020,rtaj021,rtajstamp,rtajmodid,rtajmoddt,rtajownid,rtajowndp,rtajcrtid,rtajcrtdp,rtajcrtdt) 
              = 
             (ps_keys[1]
              ,g_rtaj_d[l_ac].rtajstus,g_rtaj_d[l_ac].rtaj002,g_rtaj_d[l_ac].rtaj003,g_rtaj_d[l_ac].rtaj004, 
                  g_rtaj_d[l_ac].rtaj005,g_rtaj_d[l_ac].rtaj006,g_rtaj_d[l_ac].rtaj007,g_rtaj_d[l_ac].rtaj008, 
                  g_rtaj_d[l_ac].rtaj009,g_rtaj_d[l_ac].rtaj010,g_rtaj_d[l_ac].rtaj011,g_rtaj_d[l_ac].rtaj012, 
                  g_rtaj_d[l_ac].rtaj013,g_rtaj_d[l_ac].rtaj014,g_rtaj_d[l_ac].rtaj015,g_rtaj_d[l_ac].rtaj016, 
                  g_rtaj_d[l_ac].rtaj017,g_rtaj_d[l_ac].rtaj018,g_rtaj_d[l_ac].rtaj019,g_rtaj_d[l_ac].rtaj020, 
                  g_rtaj_d[l_ac].rtaj021,g_rtaj_d[l_ac].rtajstamp,g_rtaj2_d[l_ac].rtajmodid,g_rtaj2_d[l_ac].rtajmoddt, 
                  g_rtaj2_d[l_ac].rtajownid,g_rtaj2_d[l_ac].rtajowndp,g_rtaj2_d[l_ac].rtajcrtid,g_rtaj2_d[l_ac].rtajcrtdp, 
                  g_rtaj2_d[l_ac].rtajcrtdt) 
         WHERE rtaj001 = ps_keys_bak[1]
      #add-point:update_b段修改中
      {<point name="update_b.m_update"/>}
      #end add-point 
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "rtaj_t" 
            LET g_errparam.code   = "std-00009" 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            CALL s_transaction_end('N','0')
         WHEN SQLCA.sqlcode #其他錯誤
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "rtaj_t" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            CALL s_transaction_end('N','0')
         OTHERWISE
            
