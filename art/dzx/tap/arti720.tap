  <section id="arti720.input" ver="4" status="" src="s">
<![CDATA[#+ 資料輸入
PRIVATE FUNCTION arti720_input(p_cmd)
   DEFINE  p_cmd                 LIKE type_t.chr1
   DEFINE  l_cmd_t               LIKE type_t.chr1
   DEFINE  l_cmd                 LIKE type_t.chr1
   DEFINE  l_ac_t                LIKE type_t.num5                #未取消的ARRAY CNT 
   DEFINE  l_n                   LIKE type_t.num5                #檢查重複用  
   DEFINE  l_cnt                 LIKE type_t.num5                #檢查重複用  
   DEFINE  l_lock_sw             LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert        LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete        LIKE type_t.num5                #可刪除否  
   DEFINE  l_count               LIKE type_t.num5
   DEFINE  l_i                   LIKE type_t.num5
   DEFINE  l_insert              BOOLEAN
   DEFINE  ls_return             STRING
   DEFINE  l_var_keys            DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys          DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                DYNAMIC ARRAY OF STRING
   DEFINE  l_fields              DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak        DYNAMIC ARRAY OF STRING
   DEFINE  lb_reproduce          BOOLEAN
   DEFINE  li_reproduce          LIKE type_t.num5
   DEFINE  li_reproduce_target   LIKE type_t.num5
   #add-point:input段define
   {<point name="input.define"/>}
   #end add-point    
   
   #先做狀態判定
   IF p_cmd = 'r' THEN
      LET l_cmd_t = 'r'
      LET p_cmd   = 'a'
   ELSE
      LET l_cmd_t = p_cmd
   END IF   
   
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF  
 
   CALL cl_set_head_visible("","YES")  
 
   LET l_insert = FALSE
   LET g_action_choice = ""
 
   #add-point:input段define_sql
   {<point name="input.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT rtkcstus,rtkc003,rtkc004,rtkc005,rtkc006,rtkc007,rtkcunit,'',rtkcownid, 
       '',rtkcowndp,'',rtkccrtid,'',rtkccrtdp,'',rtkccrtdt,rtkcmodid,'',rtkcmoddt FROM rtkc_t WHERE  
       rtkcent=? AND rtkc001=? AND rtkc002=? AND rtkc003=? FOR UPDATE"
   #add-point:input段define_sql
   {<point name="input.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   DECLARE arti720_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR
 
 
   
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   LET g_qryparam.state = 'i'
   
   #控制key欄位可否輸入
   CALL arti720_set_entry(p_cmd)
   #add-point:set_entry後
   {<point name="input.after_set_entry"/>}
   #end add-point
   CALL arti720_set_no_entry(p_cmd)
   #add-point:set_no_entry後
   {<point name="input.after_set_no_entry"/>}
   #end add-point
 
   DISPLAY BY NAME g_rtkc_m.rtkc001,g_rtkc_m.rtkc002
   
   LET lb_reproduce = FALSE
   
   #add-point:進入修改段前
   {<point name="input.before_input"/>}
   #end add-point
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
]]>
</section>
  <section id="arti720.input.body" ver="8" status="" src="s">
<![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_rtkc_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂單身ACTION
         
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_rtkc_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL arti720_b_fill(g_wc2) 
            IF g_rec_b != 0 THEN
               CALL fgl_set_arr_curr(l_ac)
            END IF
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN arti720_cl USING g_enterprise,
                                               g_rtkc_m.rtkc001
                                               ,g_rtkc_m.rtkc002
 
                                               
               IF STATUS THEN
                  CALL cl_err("OPEN arti720_cl:", STATUS, 1)
                  CLOSE arti720_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_rtkc_d[l_ac].rtkc003 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_rtkc_d_t.* = g_rtkc_d[l_ac].*  #BACKUP
               CALL arti720_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL arti720_set_no_entry_b(l_cmd)
               OPEN arti720_bcl USING g_enterprise,g_rtkc_m.rtkc001,
                                                g_rtkc_m.rtkc002,
 
                                                g_rtkc_d_t.rtkc003
 
               IF STATUS THEN
                  CALL cl_err("OPEN arti720_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH arti720_bcl INTO g_rtkc_d[l_ac].rtkcstus,g_rtkc_d[l_ac].rtkc003,g_rtkc_d[l_ac].rtkc004, 
                      g_rtkc_d[l_ac].rtkc005,g_rtkc_d[l_ac].rtkc006,g_rtkc_d[l_ac].rtkc007,g_rtkc_d[l_ac].rtkcunit, 
                      g_rtkc2_d[l_ac].rtkc003,g_rtkc2_d[l_ac].rtkcownid,g_rtkc2_d[l_ac].rtkcownid_desc, 
                      g_rtkc2_d[l_ac].rtkcowndp,g_rtkc2_d[l_ac].rtkcowndp_desc,g_rtkc2_d[l_ac].rtkccrtid, 
                      g_rtkc2_d[l_ac].rtkccrtid_desc,g_rtkc2_d[l_ac].rtkccrtdp,g_rtkc2_d[l_ac].rtkccrtdp_desc, 
                      g_rtkc2_d[l_ac].rtkccrtdt,g_rtkc2_d[l_ac].rtkcmodid,g_rtkc2_d[l_ac].rtkcmodid_desc, 
                      g_rtkc2_d[l_ac].rtkcmoddt
                  IF SQLCA.sqlcode THEN
                      CALL cl_err(g_rtkc_d_t.rtkc003,SQLCA.sqlcode,1)
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL arti720_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
        
         BEFORE INSERT
            
            INITIALIZE g_rtkc_d_t.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_rtkc_d[l_ac].* TO NULL
            
            #公用欄位預設值
            #此段落由子樣板a14產生    
      LET g_rtkc2_d[l_ac].rtkcownid = g_user
      LET g_rtkc2_d[l_ac].rtkcowndp = g_dept
      LET g_rtkc2_d[l_ac].rtkccrtid = g_user
      LET g_rtkc2_d[l_ac].rtkccrtdp = g_dept 
      LET g_rtkc2_d[l_ac].rtkccrtdt = cl_get_current()
      LET g_rtkc2_d[l_ac].rtkcmodid = ""
      LET g_rtkc2_d[l_ac].rtkcmoddt = ""
      LET g_rtkc_d[l_ac].rtkcstus = ""
 
  
            
            #一般欄位預設值
                  LET g_rtkc_d[l_ac].rtkcstus = "Y"
 
 
            
            
            LET g_rtkc_d_t.* = g_rtkc_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL arti720_set_entry_b(l_cmd)
            #add-point:set_entry_b後
            {<point name="input.body.before_insert.set_entry_b"/>}
            #end add-point
            CALL arti720_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
