      CALL cl_set_comp_visible('rtkc001_comm',FALSE)
   END IF
END FUNCTION]]>
</point>
  <point name="function.arti720_cal_rtkc006" cite_std="N" status="" ver="1" src="s" new="Y" order="3" mark_hard="N">
<![CDATA[
################################################################################
# Descriptions...: 計算補貨天數
# Memo...........:
# Usage..........: CALL arti720_cal_rtkc006()
# Input parameter: 
# Return code....: 
# Date & Author..: 2014/03/31 By Lori
# Modify.........:
################################################################################
PRIVATE FUNCTION arti720_cal_rtkc006()
