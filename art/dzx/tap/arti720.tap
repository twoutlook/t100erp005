  <section id="arti720.delete" ver="3" status="" src="s">
<![CDATA[#+ 資料刪除
PRIVATE FUNCTION arti720_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_rtkc_m.rtkc001 IS NULL
   OR g_rtkc_m.rtkc002 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE arti720_master_referesh USING g_rtkc_m.rtkc001,g_rtkc_m.rtkc002 INTO g_rtkc_m.rtkc001,g_rtkc_m.rtkc002 

   
   CALL arti720_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN arti720_cl USING g_enterprise,g_rtkc_m.rtkc001
                                                       ,g_rtkc_m.rtkc002
 
   IF STATUS THEN
      CALL cl_err("OPEN arti720_cl:", STATUS, 1)
      CLOSE arti720_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH arti720_cl INTO g_rtkc_m.rtkc001,g_rtkc_m.rtkc002,g_rtkc_m.rtkc002_desc
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_rtkc_m.rtkc001,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL arti720_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM rtkc_t WHERE rtkcent = g_enterprise AND rtkc001 = g_rtkc_m.rtkc001
                                                               AND rtkc002 = g_rtkc_m.rtkc002
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("rtkc_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_rtkc_d.clear() 
      CALL g_rtkc2_d.clear()       
 
     
      CALL arti720_ui_browser_refresh()  
      CALL arti720_ui_headershow()  
      CALL arti720_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL arti720_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL arti720_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE arti720_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_rtkc_m.rtkc001,'D')
    
END FUNCTION
]]>
</section>
