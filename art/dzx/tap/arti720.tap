  <section id="arti720.b_fill" ver="5" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION arti720_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_rtkc_d.clear()
   CALL g_rtkc2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
      LET g_sql = "SELECT  UNIQUE rtkcstus,rtkc003,rtkc004,rtkc005,rtkc006,rtkc007,rtkcunit,rtkc003, 
          rtkcownid,'',rtkcowndp,'',rtkccrtid,'',rtkccrtdp,'',rtkccrtdt,rtkcmodid,'',rtkcmoddt FROM rtkc_t", 
             
                  "",
                  
                  " WHERE rtkcent= ? AND rtkc001=? AND rtkc002=?"  
                  
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF arti720_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY rtkc_t.rtkc003"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      PREPARE arti720_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR arti720_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_rtkc_m.rtkc001
                                               ,g_rtkc_m.rtkc002
 
                                               
      FOREACH b_fill_cs INTO g_rtkc_d[l_ac].rtkcstus,g_rtkc_d[l_ac].rtkc003,g_rtkc_d[l_ac].rtkc004,g_rtkc_d[l_ac].rtkc005, 
          g_rtkc_d[l_ac].rtkc006,g_rtkc_d[l_ac].rtkc007,g_rtkc_d[l_ac].rtkcunit,g_rtkc2_d[l_ac].rtkc003, 
          g_rtkc2_d[l_ac].rtkcownid,g_rtkc2_d[l_ac].rtkcownid_desc,g_rtkc2_d[l_ac].rtkcowndp,g_rtkc2_d[l_ac].rtkcowndp_desc, 
          g_rtkc2_d[l_ac].rtkccrtid,g_rtkc2_d[l_ac].rtkccrtid_desc,g_rtkc2_d[l_ac].rtkccrtdp,g_rtkc2_d[l_ac].rtkccrtdp_desc, 
          g_rtkc2_d[l_ac].rtkccrtdt,g_rtkc2_d[l_ac].rtkcmodid,g_rtkc2_d[l_ac].rtkcmodid_desc,g_rtkc2_d[l_ac].rtkcmoddt  

                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_rtkc2_d[l_ac].rtkcownid_desc = cl_get_username(g_rtkc2_d[l_ac].rtkcownid)
      #LET g_rtkc2_d[l_ac].rtkcowndp_desc = cl_get_deptname(g_rtkc2_d[l_ac].rtkcowndp)
      #LET g_rtkc2_d[l_ac].rtkccrtid_desc = cl_get_username(g_rtkc2_d[l_ac].rtkccrtid)
      #LET g_rtkc2_d[l_ac].rtkccrtdp_desc = cl_get_deptname(g_rtkc2_d[l_ac].rtkccrtdp)
      #LET g_rtkc2_d[l_ac].rtkcmodid_desc = cl_get_username(g_rtkc2_d[l_ac].rtkcmodid)
      ##LET .rtkccnfid_desc = cl_get_deptname(.rtkccnfid)
      ##LET .rtkcpstid_desc = cl_get_deptname(.rtkcpstid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_rtkc_d.deleteElement(g_rtkc_d.getLength())
      CALL g_rtkc2_d.deleteElement(g_rtkc2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE arti720_pb   
   
END FUNCTION
]]>
</section>
