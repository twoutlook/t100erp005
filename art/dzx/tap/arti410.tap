  <point name="function.arti410_create_tree" order="2" ver="1" cite_std="N" cite_ver="" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#create tree
PRIVATE FUNCTION arti410_create_tree()
   DEFINE ps_wc              STRING
   DEFINE ps_page_action     STRING
   DEFINE ls_sql             STRING
   DEFINE li_idx             LIKE type_t.num5 
   define l_n2               like type_t.num5
   CLEAR FORM
   INITIALIZE g_rtea_m.* TO NULL
   CALL g_browser.clear()
   
   LET ls_sql = " SELECT UNIQUE rtea001 ",
                " FROM rtea_t ",
                "  ",
                "  ",
                " WHERE rteaent = '" ||g_enterprise|| "' AND ", g_wc
                
   PREPARE browse_pre1 FROM ls_sql
   DECLARE browse_cur1 CURSOR FOR browse_pre1
   
   LET li_idx = 1 
   FOREACH browse_cur1 INTO g_browser[li_idx].b_rtea001
      
      LET g_browser[li_idx].b_pid     = 0
      LET g_browser[li_idx].b_id      = 0, ".", li_idx USING "<<<"
      LET g_browser[li_idx].b_exp     = FALSE
      LET g_browser[li_idx].b_expcode = 1
      LET g_browser[li_idx].b_hasC    = TRUE
      LET g_browser[li_idx].b_show    = g_browser[li_idx].b_rtea001
      LET g_browser[li_idx].b_isexp   = FALSE
      CALL arti410_desc_show(li_idx)
      LET li_idx = li_idx + 1
      
      IF li_idx > g_max_rec AND g_error_show = 1 THEN
         CALL cl_err( '', 9035, 1)
         EXIT FOREACH
      END IF
      
   END FOREACH
   LET g_error_show = 0
   
   CALL g_browser.deleteElement(g_browser.getLength())
   
   LET g_browser_cnt = g_browser.getLength()

   #瀏覽頁筆數顯示
   DISPLAY g_browser_cnt TO FORMONLY.b_count        #總筆數
   DISPLAY g_browser_cnt TO FORMONLY.h_count        #總筆數
   
   #CALL arti410_fetch("") 
   
   FREE browse_pre
   FOR l_n2 = 1 TO g_browser.getLength()
       IF g_browser[l_n2].b_isExp = FALSE THEN
          CALL arti410_browser_expand(l_n2)
          let g_browser[l_n2].b_isExp = TRUE
      END IF
  END FOR
END FUNCTION]]>
  </point>
  <point name="function.arti410_chk_rtea001" order="3" ver="1" cite_std="N" cite_ver="" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#chk rtea001
PRIVATE FUNCTION arti410_chk_rtea001()
   DEFINE   l_cnt       LIKE type_t.num5
   DEFINE   l_cnt1      LIKE type_t.num5 

   LET g_errno = NULL
   LET l_cnt = 0
   LET l_cnt1 = 0
   SELECT COUNT(*) INTO l_cnt  FROM ooef_t,ooee_t WHERE ooef001=ooee001 AND ooefent=ooeeent
      and ooee002='2' AND ooee003='F' and ooef001 = g_rtea_m.rtea001 AND ooefent = g_enterprise
      
   IF l_cnt <= 0 THEN
      LET g_errno = "aim-00060"
   ELSE
      SELECT COUNT(*) INTO l_cnt1  FROM ooef_t,ooee_t WHERE ooef001=ooee001 AND ooefent=ooeeent
      and ooee002='2' AND ooee003='F' and ooef001 = g_rtea_m.rtea001 AND ooefent = g_enterprise AND ooefstus='Y'
      IF l_cnt1 <= 0 THEN
         LET g_errno = "amm-00007"
      END IF         
   END IF
END FUNCTION]]>
  </point>
