  <section id="arti410.modify" ver="1" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION arti410_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point
   
   IF g_rtea_m.rtea001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF 
   
    SELECT UNIQUE rtea001,rtea002,rtea003,rtea004,rtea005,rtea006,rtea007,rteastus,rteaownid,rteaowndp, 
        rteacrtid,rteacrtdp,rteacrtdt,rteamodid,rteamoddt
 INTO g_rtea_m.rtea001,g_rtea_m.rtea002,g_rtea_m.rtea003,g_rtea_m.rtea004,g_rtea_m.rtea005,g_rtea_m.rtea006, 
     g_rtea_m.rtea007,g_rtea_m.rteastus,g_rtea_m.rteaownid,g_rtea_m.rteaowndp,g_rtea_m.rteacrtid,g_rtea_m.rteacrtdp, 
     g_rtea_m.rteacrtdt,g_rtea_m.rteamodid,g_rtea_m.rteamoddt
 FROM rtea_t
 WHERE rteaent = g_enterprise AND rtea001 = g_rtea_m.rtea001 AND rtea002 = g_rtea_m.rtea002
 
   ERROR ""
  
   LET g_rtea001_t = g_rtea_m.rtea001
   LET g_rtea002_t = g_rtea_m.rtea002
 
 
   
   CALL s_transaction_begin()
   
   OPEN arti410_cl USING g_enterprise,g_rtea_m.rtea001,g_rtea_m.rtea002
   IF STATUS THEN
      CALL cl_err("OPEN arti410_cl:", STATUS, 1)
      CLOSE arti410_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH arti410_cl INTO g_rtea_m.rtea001,g_rtea_m.rtea001_desc,g_rtea_m.rtea002,g_rtea_m.rtea003,g_rtea_m.rtea004, 
       g_rtea_m.rtea005,g_rtea_m.rtea006,g_rtea_m.rtea007,g_rtea_m.rteastus,g_rtea_m.rteaownid,g_rtea_m.rteaownid_desc, 
       g_rtea_m.rteaowndp,g_rtea_m.rteaowndp_desc,g_rtea_m.rteacrtid,g_rtea_m.rteacrtid_desc,g_rtea_m.rteacrtdp, 
       g_rtea_m.rteacrtdp_desc,g_rtea_m.rteacrtdt,g_rtea_m.rteamodid,g_rtea_m.rteamodid_desc,g_rtea_m.rteamoddt 

 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err("rtea_t",SQLCA.sqlcode,0)
      CLOSE arti410_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
 
   CALL arti410_show()
   
   WHILE TRUE
      LET g_rtea_m.rtea001 = g_rtea001_t
      LET g_rtea_m.rtea002 = g_rtea002_t
 
 
      
      #寫入修改者/修改日期資訊
      LET g_rtea_m.rteamodid = g_user 
LET g_rtea_m.rteamoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
 
      CALL arti410_input("u")     #欄位更改
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_rtea_m.* = g_rtea_m_t.*
         CALL arti410_show()
         CALL cl_err("",9001,0)
         EXIT WHILE
      END IF
 
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_rtea_m.rtea001,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE arti410_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_rtea_m.rtea001,"U")
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
]]>
