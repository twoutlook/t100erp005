      CALL arti410_browser_fill(g_wc,"")
      
      #判斷前一個動作是否為新增, 若是的話切換到新增的筆數
      IF g_state = "Y" THEN
         FOR li_idx = 1 TO g_browser.getLength()
            IF g_browser[li_idx].b_rtea001 = g_rtea001_t
               AND g_browser[li_idx].b_rtea002 = g_rtea002_t
 
 
               THEN
               LET g_current_row = li_idx
               EXIT FOR
            END IF
         END FOR
