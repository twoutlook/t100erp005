{<section id="acrr808_x02.sel_prep" readonly="Y" >}
#+ 選單功能實際執行處
PRIVATE FUNCTION acrr808_x02_sel_prep()
DEFINE g_select      STRING
DEFINE g_from        STRING
DEFINE g_where       STRING
#add-point:sel_prep段define(客製用) name="sel_prep.define_customerization"

#end add-point
#add-point:sel_prep段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="sel_prep.define"

#end add-point
 
   #add-point:sel_prep before name="sel_prep.before"
   LET  date1 = tm.date1
   LET  date2 = tm.date2
   #end add-point
 
   #add-point:sel_prep g_select name="sel_prep.g_select"
   
   #end add-point
   LET g_select = " SELECT mmaf001,mmaf008,oocql004,mmaf015,mmaf014,mmau_t.mmau006,NULL,mmau_t.mmau009, 
       NULL"
 
   #add-point:sel_prep g_from name="sel_prep.g_from"
   LET g_select = " SELECT DISTINCT mmaf001,mmaf008,oocql004,mmaf015,mmaf014,MAX(mmau006),COUNT(*),SUM(mmau009),''"
   #end add-point
    LET g_from = " FROM mmaf_t,oocql_t,mmau_t"
 
   #add-point:sel_prep g_where name="sel_prep.g_where"
   LET g_from = " FROM mmau_t,mmaf_t LEFT OUTER JOIN oocql_t ON oocqlent = mmafent AND oocql001 = '2016' AND oocql002 = mmaf001 AND oocql003 = '",g_dlang,"'"
   #end add-point
    LET g_where = " WHERE " ,tm.wc CLIPPED
 
   #add-point:sel_prep g_order name="sel_prep.g_order"
   LET g_where = " WHERE  mmafent = mmauent AND mmau002 = mmaf001 AND mmau004 = '5'  ",
                 " AND   mmafent = '" ||g_enterprise|| "'  AND ",
                  tm.wc CLIPPED
  #查询条件加上日期范围
   LET g_where = g_where," AND mmau006 >= TO_DATE( '",date1,"','YYYY-MM-DD HH24:MI:SS')"
 
   LET g_where = g_where," AND mmau006 < TO_DATE( '",date2,"','YYYY-MM-DD HH24:MI:SS')"
  

   #判断会员属性代码类别和会员属性代码值是否为空
   IF NOT cl_null(tm.wc2) AND tm.wc2 != " 1=1"  THEN
      LET g_where = g_where," AND EXISTS (SELECT mmag001 FROM mmag_t WHERE mmag001 = mmaf001 AND mmagent = mmafent AND ",tm.wc2, ") "
   END IF 
   
   #end add-point
 
   #add-point:sel_prep.sql.before name="sel_prep.sql.before"
   
   #end add-point:sel_prep.sql.before
   LET g_where = g_where ,cl_sql_add_filter("mmaf_t")   #資料過濾功能
   LET g_sql = g_select CLIPPED ," ",g_from CLIPPED ," ",g_where CLIPPED
   LET g_sql = cl_sql_add_mask(g_sql)    #遮蔽特定資料, 若寫至add-point也需複製此段
 
   #add-point:sel_prep.sql.after name="sel_prep.sql.after"
   LET g_sql = g_sql," GROUP BY mmaf001,mmaf008,oocql004,mmaf015,mmaf014 ",
                  " ORDER BY mmaf001 "
   #end add-point
   PREPARE acrr808_x02_prepare FROM g_sql
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.extend = 'prepare:'
      LET g_errparam.code   = STATUS
      LET g_errparam.popup  = TRUE
      CALL cl_err()
      LET g_rep_success = 'N' 
   END IF
   DECLARE acrr808_x02_curs CURSOR FOR acrr808_x02_prepare
 
END FUNCTION
 
