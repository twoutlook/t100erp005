  <section id="acrt410_01.main" ver="5" status="" src="s">
    <![CDATA[#+ 此段落由子樣板a27產生
#+ 資料輸入
PUBLIC FUNCTION acrt410_01(--)
   #add-point:main段變數傳入
   {<point name="main.get_var"/>}
   #end add-point
   )
   #add-point:main段define
   {<point name="main.define"/>}
   #end add-point   
 
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" mark="Y"/>}
   #end add-point
   LET g_forupd_sql = "SELECT crbcsite,'',crbcdocno,crbc000,crbc001,'',crbc002,'',crbc003,crbcownid, 
       '',crbcowndp,'',crbccrtid,'',crbccrtdp,'',crbccrtdt,crbcmodid,'',crbcmoddt FROM crbc_t WHERE  
       crbcent= ? AND crbcdocno=? AND crbc000=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)   #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE acrt410_01_cl CURSOR FROM g_forupd_sql     # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE crbcsite,crbcdocno,crbc000,crbc001,crbc002,crbc003,crbcownid,crbcowndp, 
       crbccrtid,crbccrtdp,crbccrtdt,crbcmodid,crbcmoddt",
               " FROM crbc_t",
               " WHERE crbcent = '" ||g_enterprise|| "' AND crbcdocno = ? AND crbc000 = ?"
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE acrt410_01_master_referesh FROM g_sql
   
 
 
   
   #畫面開啟 (identifier)
