  <section id="acrt410_01.b_fill" ver="3" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION acrt410_01_b_fill()
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_crbd_d.clear()    #g_crbd_d 單頭及單身 
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF acrt410_01_fill_chk(1) THEN
   
      LET g_sql = "SELECT  UNIQUE crbdseq,crbd001,crbd002,crbdsite ,t1.oocql004 FROM crbd_t",   
                  " INNER JOIN crbc_t ON crbcdocno = crbddocno ",
                  " AND crbc000 = crbd000 ",
 
                  #"",
                  
                  "",
                                 " LEFT JOIN oocql_t t1 ON t1.oocqlent='"||g_enterprise||"' AND t1.oocql001='2106' AND t1.oocql002=crbd001 AND t1.oocql003='"||g_dlang||"' ",
 
                  " WHERE"
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      #add-point:b_fill段sql_before
      {<point name="b_fill.body.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table1) THEN
         LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
      END IF
      
      #子單身的WC
      
      
      LET g_sql = g_sql, " ORDER BY crbd_t.crbdseq"
      
      #add-point:單身填充控制
      {<point name="b_fill.sql"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE acrt410_01_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR acrt410_01_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_crbc_m.crbcdocno,g_crbc_m.crbc000
                                               
      FOREACH b_fill_cs INTO g_crbd_d[l_ac].crbdseq,g_crbd_d[l_ac].crbd001,g_crbd_d[l_ac].crbd002,g_crbd_d[l_ac].crbdsite, 
          g_crbd_d[l_ac].crbd001_desc
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               CALL cl_err( '', 9035, 1)
            END IF
            EXIT FOREACH
         END IF
         
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
 
   
   #add-point:browser_fill段其他table處理
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_crbd_d.deleteElement(g_crbd_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE acrt410_01_pb
 
   
END FUNCTION
]]>
  </section>
