      #資料備份
      LET g_crbcdocno_t = g_crbc_m.crbcdocno
      LET g_crbc000_t = g_crbc_m.crbc000
 
 
      DELETE FROM crbc_t
       WHERE crbcent = g_enterprise AND crbcdocno = g_crbc_m.crbcdocno
         AND crbc000 = g_crbc_m.crbc000
 
       
      #add-point:單頭刪除中
      {<point name="delete.head.m_delete"/>}
      #end add-point
       
      IF SQLCA.sqlcode THEN
         CALL cl_err(g_crbc_m.crbcdocno,SQLCA.sqlcode,0) 
         CALL s_transaction_end('N','0')
         RETURN
      END IF
      
      #add-point:單頭刪除後
      {<point name="delete.head.a_delete"/>}
      #end add-point
  
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM crbd_t
       WHERE crbdent = g_enterprise AND crbddocno = g_crbc_m.crbcdocno
         AND crbd000 = g_crbc_m.crbc000
 
 
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
         
      IF SQLCA.sqlcode THEN
         CALL cl_err("crbd_t",SQLCA.sqlcode,0) 
         CALL s_transaction_end('N','0')
         RETURN
      END IF    
 
      #add-point:單身刪除後
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
            
                                                               
 
 
 
                                                               
      CLEAR FORM
      CALL g_crbd_d.clear() 
 
     
      #CALL acrt410_01_ui_browser_refresh()  
      CALL acrt410_01_ui_headershow()  
      CALL acrt410_01_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL acrt410_01_browser_fill("")
         CALL acrt410_01_fetch('P')
