  <section id="acrt410_01.fetch" ver="4" status="" src="s">
    <![CDATA[#+ 指定PK後抓取單頭其他資料
PRIVATE FUNCTION acrt410_01_fetch(p_flag)
   DEFINE p_flag     LIKE type_t.chr1
   DEFINE ls_msg     STRING
   #add-point:fetch段define
   {<point name="fetch.define"/>}
   #end add-point    
   
   IF g_browser_cnt = 0 THEN
      RETURN
   END IF
   
   CASE p_flag
      WHEN 'F' LET g_current_idx = 1
      WHEN 'L' LET g_current_idx = g_header_cnt        
      WHEN 'P'
         IF g_current_idx > 1 THEN               
            LET g_current_idx = g_current_idx - 1
         END IF 
      WHEN 'N'
         IF g_current_idx < g_header_cnt THEN
            LET g_current_idx =  g_current_idx + 1
         END IF        
      WHEN '/'
         IF (NOT g_no_ask) THEN    
            CALL cl_set_act_visible("accept,cancel", TRUE)    
            CALL cl_getmsg('fetch',g_lang) RETURNING ls_msg
            LET INT_FLAG = 0
 
            PROMPT ls_msg CLIPPED,':' FOR g_jump
               #交談指令共用ACTION
               &include "common_action.4gl" 
            END PROMPT
 
            CALL cl_set_act_visible("accept,cancel", FALSE)    
            IF INT_FLAG THEN
                LET INT_FLAG = 0
                EXIT CASE  
            END IF           
         END IF
         
         IF g_jump > 0 AND g_jump <= g_browser.getLength() THEN
             LET g_current_idx = g_jump
         END IF
         
         LET g_no_ask = FALSE  
   END CASE 
   
   CALL acrt410_01_browser_fill(p_flag)
   
   
   LET g_detail_cnt = g_header_cnt                  
   
   #單身總筆數顯示
   #LET g_detail_idx = 1
   IF g_detail_cnt > 0 THEN
      #LET g_detail_idx = 1
      DISPLAY g_detail_idx TO FORMONLY.idx  
   ELSE
      LET g_detail_idx = 0
      DISPLAY ' ' TO FORMONLY.idx    
   END IF
   
   #瀏覽頁筆數顯示
   LET g_pagestart = g_current_idx
   DISPLAY g_pagestart TO FORMONLY.b_index   #當下筆數
   DISPLAY g_pagestart TO FORMONLY.h_index   #當下筆數
   
   CALL cl_navigator_setting( g_pagestart, g_browser_cnt )
   
   #代表沒有資料
   IF g_current_idx = 0 OR g_browser.getLength() = 0 THEN
      RETURN
   END IF
   
   #超出範圍
   IF g_current_idx > g_browser.getLength() THEN
      LET g_current_idx = g_browser.getLength()
   END IF
   
   LET g_crbc_m.crbcdocno = g_browser[g_current_idx].b_crbcdocno
   LET g_crbc_m.crbc000 = g_browser[g_current_idx].b_crbc000
 
   
   #重讀DB,因TEMP有不被更新特性
   EXECUTE acrt410_01_master_referesh USING g_crbc_m.crbcdocno,g_crbc_m.crbc000 INTO g_crbc_m.crbcsite, 
       g_crbc_m.crbcdocno,g_crbc_m.crbc000,g_crbc_m.crbc001,g_crbc_m.crbc002,g_crbc_m.crbc003,g_crbc_m.crbcownid, 
       g_crbc_m.crbcowndp,g_crbc_m.crbccrtid,g_crbc_m.crbccrtdp,g_crbc_m.crbccrtdt,g_crbc_m.crbcmodid, 
       g_crbc_m.crbcmoddt,g_crbc_m.crbcsite_desc,g_crbc_m.crbc001_desc,g_crbc_m.crbc002_desc,g_crbc_m.crbcownid_desc, 
       g_crbc_m.crbcowndp_desc,g_crbc_m.crbccrtid_desc,g_crbc_m.crbccrtdp_desc,g_crbc_m.crbcmodid_desc 

   IF SQLCA.sqlcode THEN
      CALL cl_err("crbc_t",SQLCA.sqlcode,1)
      INITIALIZE g_crbc_m.* TO NULL
      RETURN
   END IF
   
   #add-point:fetch段action控制
   {<point name="fetch.action_control"/>}
   #end add-point  
   
   
   
   #add-point:fetch結束前
   {<point name="fetch.after" />}
   #end add-point
   
   #LET g_data_owner =       
   #LET g_data_group =   
   
   #重新顯示   
   CALL acrt410_01_show()
 
   
 
END FUNCTION
]]>
