  <section id="acrt410_01.modify" ver="6" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION acrt410_01_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_crbc_m.crbcdocno IS NULL
   OR g_crbc_m.crbc000 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   ERROR ""
  
   LET g_crbcdocno_t = g_crbc_m.crbcdocno
   LET g_crbc000_t = g_crbc_m.crbc000
 
   CALL s_transaction_begin()
   
   OPEN acrt410_01_cl USING g_enterprise,g_crbc_m.crbcdocno,g_crbc_m.crbc000
   IF STATUS THEN
      CALL cl_err("OPEN acrt410_01_cl:", STATUS, 1)
      CLOSE acrt410_01_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE acrt410_01_master_referesh USING g_crbc_m.crbcdocno,g_crbc_m.crbc000 INTO g_crbc_m.crbcsite, 
       g_crbc_m.crbcdocno,g_crbc_m.crbc000,g_crbc_m.crbc001,g_crbc_m.crbc002,g_crbc_m.crbc003,g_crbc_m.crbcownid, 
       g_crbc_m.crbcowndp,g_crbc_m.crbccrtid,g_crbc_m.crbccrtdp,g_crbc_m.crbccrtdt,g_crbc_m.crbcmodid, 
       g_crbc_m.crbcmoddt,g_crbc_m.crbcsite_desc,g_crbc_m.crbc001_desc,g_crbc_m.crbc002_desc,g_crbc_m.crbcownid_desc, 
       g_crbc_m.crbcowndp_desc,g_crbc_m.crbccrtid_desc,g_crbc_m.crbccrtdp_desc,g_crbc_m.crbcmodid_desc 

 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_crbc_m.crbcdocno,SQLCA.sqlcode,0)
      CLOSE acrt410_01_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   CALL acrt410_01_show()
   WHILE TRUE
      LET g_crbcdocno_t = g_crbc_m.crbcdocno
      LET g_crbc000_t = g_crbc_m.crbc000
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_crbc_m.crbcmodid = g_user 
LET g_crbc_m.crbcmoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      CALL acrt410_01_input("u")
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_crbc_m.* = g_crbc_m_t.*
         CALL acrt410_01_show()
         CALL cl_err('',9001,0)
         CALL s_transaction_end('N','0')
         EXIT WHILE
      END IF 
      
      #若有modid跟moddt則進行update
      UPDATE crbc_t SET (crbcmodid,crbcmoddt) = (g_crbc_m.crbcmodid,g_crbc_m.crbcmoddt)
       WHERE crbcent = g_enterprise AND crbcdocno = g_crbcdocno_t
         AND crbc000 = g_crbc000_t
 
                  
      #若單頭key欄位有變更
      IF g_crbc_m.crbcdocno != g_crbcdocno_t 
      OR g_crbc_m.crbc000 != g_crbc000_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE crbd_t SET crbddocno = g_crbc_m.crbcdocno
                                       ,crbd000 = g_crbc_m.crbc000
 
          WHERE crbdent = g_enterprise AND crbddocno = g_crbcdocno_t
            AND crbd000 = g_crbc000_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("crbd_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("crbd_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_crbc_m.crbcdocno,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE acrt410_01_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_crbc_m.crbcdocno,'U')
 
END FUNCTION   
]]>
