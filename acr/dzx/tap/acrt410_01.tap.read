  <section id="acrt410_01.input" ver="5" status="" src="s">
    <![CDATA[#+ 資料輸入
PRIVATE FUNCTION acrt410_01_input(p_cmd)
   DEFINE  p_cmd                 LIKE type_t.chr1
   DEFINE  l_cmd_t               LIKE type_t.chr1
   DEFINE  l_cmd                 LIKE type_t.chr1
   DEFINE  l_n                   LIKE type_t.num5                #檢查重複用  
   DEFINE  l_cnt                 LIKE type_t.num5                #檢查重複用  
   DEFINE  l_lock_sw             LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert        LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete        LIKE type_t.num5                #可刪除否  
   DEFINE  l_count               LIKE type_t.num5
   DEFINE  l_i                   LIKE type_t.num5
   DEFINE  l_insert              BOOLEAN
   DEFINE  ls_return             STRING
   DEFINE  l_var_keys            DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys          DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                DYNAMIC ARRAY OF STRING
   DEFINE  l_fields              DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak        DYNAMIC ARRAY OF STRING
   DEFINE  lb_reproduce          BOOLEAN
   DEFINE  li_reproduce          LIKE type_t.num5
   DEFINE  li_reproduce_target   LIKE type_t.num5
   #add-point:input段define
   {<point name="input.define"/>}
   #end add-point  
 
   #先做狀態判定
   IF p_cmd = 'r' THEN
      LET l_cmd_t = 'r'
      LET p_cmd   = 'a'
   ELSE
      LET l_cmd_t = p_cmd
   END IF   
   
   #切換畫面
 
   CALL cl_set_head_visible("","YES")  
 
   LET l_insert = FALSE
   LET g_action_choice = ""
 
   #add-point:input段define_sql
   {<point name="input.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT crbdseq,crbd001,crbd002,crbdsite FROM crbd_t WHERE crbdent=? AND crbddocno=?  
       AND crbd000=? AND crbdseq=? FOR UPDATE"
   #add-point:input段define_sql
   {<point name="input.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE acrt410_01_bcl CURSOR FROM g_forupd_sql
   
 
   
 
 
   #add-point:input段define_sql
   {<point name="input.other_sql"/>}
   #end add-point 
 
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   LET g_qryparam.state = 'i'
   
   #控制key欄位可否輸入
   CALL acrt410_01_set_entry(p_cmd)
   #add-point:set_entry後
   {<point name="input.after_set_entry"/>}
   #end add-point
   CALL acrt410_01_set_no_entry(p_cmd)
 
   DISPLAY BY NAME g_crbc_m.crbcsite,g_crbc_m.crbcdocno,g_crbc_m.crbc000,g_crbc_m.crbc001,g_crbc_m.crbc002, 
       g_crbc_m.crbc003
   
   LET lb_reproduce = FALSE
   
   #add-point:資料輸入前
   {<point name="input.before_input"/>}
   #end add-point
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
   
]]>
  </section>
  <section id="acrt410_01.input.body" ver="5" status="" src="s">
    <![CDATA[   
      #Page1 預設值產生於此處
      INPUT ARRAY g_crbd_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_crbd_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL acrt410_01_b_fill()
            LET g_rec_b = g_crbd_d.getLength()
            #add-point:資料輸入前
            {<point name="input.d.before_input"/>}
            #end add-point
         
         BEFORE ROW
            #add-point:modify段before row2
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET l_insert = FALSE
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET g_detail_idx = l_ac
            
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
         
            CALL s_transaction_begin()
            OPEN acrt410_01_cl USING g_enterprise,g_crbc_m.crbcdocno,g_crbc_m.crbc000
            IF STATUS THEN
               CALL cl_err("OPEN acrt410_01_cl:", STATUS, 1)
               CLOSE acrt410_01_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            
            LET g_rec_b = g_crbd_d.getLength()
            
            IF g_rec_b >= l_ac 
               AND g_crbd_d[l_ac].crbdseq IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_crbd_d_t.* = g_crbd_d[l_ac].*  #BACKUP
               CALL acrt410_01_set_entry_b(l_cmd)
               #add-point:modify段after_set_entry_b
               {<point name="input.body.after_set_entry_b"/>}
               #end add-point  
               CALL acrt410_01_set_no_entry_b(l_cmd)
               IF NOT acrt410_01_lock_b("crbd_t","'1'") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH acrt410_01_bcl INTO g_crbd_d[l_ac].crbdseq,g_crbd_d[l_ac].crbd001,g_crbd_d[l_ac].crbd002, 
                      g_crbd_d[l_ac].crbdsite
                  IF SQLCA.sqlcode THEN
                     CALL cl_err(g_crbd_d_t.crbdseq,SQLCA.sqlcode,1)
                     LET l_lock_sw = "Y"
                  END IF
                  
                  LET g_bfill = "N"
                  CALL acrt410_01_show()
                  LET g_bfill = "Y"
                  
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
        
         BEFORE INSERT
            
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_crbd_d[l_ac].* TO NULL 
            
            LET g_crbd_d_t.* = g_crbd_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL acrt410_01_set_entry_b(l_cmd)
            #add-point:modify段after_set_entry_b
            {<point name="input.body.insert.after_set_entry_b"/>}
            #end add-point
            CALL acrt410_01_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_crbd_d[li_reproduce_target].* = g_crbd_d[li_reproduce].*
 
               LET g_crbd_d[li_reproduce_target].crbdseq = NULL
 
            END IF
            #公用欄位給值(單身)
            
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            #add-point:單身新增
            {<point name="input.body.b_a_insert"/>}
            #end add-point
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM crbd_t 
             WHERE crbdent = g_enterprise AND crbddocno = g_crbc_m.crbcdocno
               AND crbd000 = g_crbc_m.crbc000
 
               AND crbdseq = g_crbd_d[l_ac].crbdseq
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前
               {<point name="input.body.b_insert"/>}
               #end add-point
            
               #同步新增到同層的table
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_crbc_m.crbcdocno
               LET gs_keys[2] = g_crbc_m.crbc000
               LET gs_keys[3] = g_crbd_d[g_detail_idx].crbdseq
               CALL acrt410_01_insert_b('crbd_t',gs_keys,"'1'")
                           
               #add-point:單身新增後
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_crbd_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("crbd_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL acrt410_01_b_fill()
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert2"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR 'INSERT O.K'
               LET g_rec_b = g_rec_b + 1
            END IF
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_crbd_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_crbd_d.deleteElement(l_ac)
               NEXT FIELD crbdseq
            END IF
         
            IF g_crbd_d[l_ac].crbdseq IS NOT NULL
 
               THEN 
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete" mark="Y"/>}
               #end add-point 
               
               DELETE FROM crbd_t
                WHERE crbdent = g_enterprise AND crbddocno = g_crbc_m.crbcdocno AND
                                          crbd000 = g_crbc_m.crbc000 AND
 
                      crbdseq = g_crbd_d_t.crbdseq
 
                  
               #add-point:單身刪除中
               {<point name="input.body.m_delete"/>}
               #end add-point 
               
               IF SQLCA.sqlcode THEN
                  CALL cl_err("crbd_t",SQLCA.sqlcode,1)
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               ELSE
                  LET g_rec_b = g_rec_b-1
                  
                  #add-point:單身刪除後
                  {<point name="input.body.a_delete"/>}
                  #end add-point
                  CALL s_transaction_end('Y','0')
               END IF 
               CLOSE acrt410_01_bcl
               LET l_count = g_crbd_d.getLength()
            END IF 
            
                           INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_crbc_m.crbcdocno
               LET gs_keys[2] = g_crbc_m.crbc000
               LET gs_keys[3] = g_crbd_d[g_detail_idx].crbdseq
 
              
         AFTER DELETE 
            #add-point:單身刪除後2
            {<point name="input.body.after_delete"/>}
            #end add-point
                           CALL acrt410_01_delete_b('crbd_t',gs_keys,"'1'")
 
                  #此段落由子樣板a02產生
         AFTER FIELD crbdseq
            #此段落由子樣板a15產生
            IF NOT cl_ap_chk_Range(g_crbd_d[l_ac].crbdseq,"0.000","0","","","azz-00079",1) THEN
               NEXT FIELD crbdseq
            END IF
 
 
            #add-point:AFTER FIELD crbdseq
            {<point name="input.a.page1.crbdseq" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD crbdseq
            #add-point:BEFORE FIELD crbdseq
            {<point name="input.b.page1.crbdseq" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE crbdseq
            #add-point:ON CHANGE crbdseq
            {<point name="input.g.page1.crbdseq" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD crbd001
            
            #add-point:AFTER FIELD crbd001
            {<point name="input.a.page1.crbd001" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD crbd001
            #add-point:BEFORE FIELD crbd001
            {<point name="input.b.page1.crbd001" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE crbd001
            #add-point:ON CHANGE crbd001
            {<point name="input.g.page1.crbd001" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD crbd002
            #add-point:BEFORE FIELD crbd002
            {<point name="input.b.page1.crbd002" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD crbd002
            
            #add-point:AFTER FIELD crbd002
            {<point name="input.a.page1.crbd002" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE crbd002
            #add-point:ON CHANGE crbd002
            {<point name="input.g.page1.crbd002" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD crbdsite
            #add-point:BEFORE FIELD crbdsite
            {<point name="input.b.page1.crbdsite" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD crbdsite
            
            #add-point:AFTER FIELD crbdsite
            {<point name="input.a.page1.crbdsite" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE crbdsite
            #add-point:ON CHANGE crbdsite
            {<point name="input.g.page1.crbdsite" />}
            #END add-point
 
 
                  #Ctrlp:input.c.page1.crbdseq
         ON ACTION controlp INFIELD crbdseq
            #add-point:ON ACTION controlp INFIELD crbdseq
            {<point name="input.c.page1.crbdseq" />}
            #END add-point
 
         #Ctrlp:input.c.page1.crbd001
         ON ACTION controlp INFIELD crbd001
            #add-point:ON ACTION controlp INFIELD crbd001
            {<point name="input.c.page1.crbd001" />}
            #END add-point
 
         #Ctrlp:input.c.page1.crbd002
         ON ACTION controlp INFIELD crbd002
            #add-point:ON ACTION controlp INFIELD crbd002
            {<point name="input.c.page1.crbd002" />}
            #END add-point
 
         #Ctrlp:input.c.page1.crbdsite
         ON ACTION controlp INFIELD crbdsite
            #add-point:ON ACTION controlp INFIELD crbdsite
            {<point name="input.c.page1.crbdsite" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_crbd_d[l_ac].* = g_crbd_d_t.*
               CLOSE acrt410_01_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_crbd_d[l_ac].crbdseq,-263,1)
               LET g_crbd_d[l_ac].* = g_crbd_d_t.*
            ELSE
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               #寫入修改者/修改日期資訊(單身)
               
      
               UPDATE crbd_t SET (crbddocno,crbd000,crbdseq,crbd001,crbd002,crbdsite) = (g_crbc_m.crbcdocno, 
                   g_crbc_m.crbc000,g_crbd_d[l_ac].crbdseq,g_crbd_d[l_ac].crbd001,g_crbd_d[l_ac].crbd002, 
                   g_crbd_d[l_ac].crbdsite)
                WHERE crbdent = g_enterprise AND crbddocno = g_crbc_m.crbcdocno 
                  AND crbd000 = g_crbc_m.crbc000 
 
                  AND crbdseq = g_crbd_d_t.crbdseq #項次   
 
                  
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("crbd_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                     LET g_crbd_d[l_ac].* = g_crbd_d_t.*
                  WHEN SQLCA.sqlcode #其他錯誤
                     CALL cl_err("crbd_t",SQLCA.sqlcode,1)
                     LET g_crbd_d[l_ac].* = g_crbd_d_t.*                     
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_crbc_m.crbcdocno
               LET gs_keys_bak[1] = g_crbcdocno_t
               LET gs_keys[2] = g_crbc_m.crbc000
               LET gs_keys_bak[2] = g_crbc000_t
               LET gs_keys[3] = g_crbd_d[g_detail_idx].crbdseq
               LET gs_keys_bak[3] = g_crbd_d_t.crbdseq
               CALL acrt410_01_update_b('crbd_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
               END CASE
               
               LET g_log1 = util.JSON.stringify(g_crbc_m),util.JSON.stringify(g_crbd_d_t)
               LET g_log2 = util.JSON.stringify(g_crbc_m),util.JSON.stringify(g_crbd_d[l_ac])
               
               IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                  CALL s_transaction_end('N','0')
               END IF
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            #add-point:單身after_row
            {<point name="input.body.after_row"/>}
            #end add-point
            CALL acrt410_01_unlock_b("crbd_t","'1'")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock
            #add-point:單身after_row2
            {<point name="input.body.after_row2"/>}
            #end add-point
              
         AFTER INPUT
            #add-point:input段after input 
            {<point name="input.body.after_input"/>}
            #end add-point 
 
         ON ACTION controlo    
            CALL FGL_SET_ARR_CURR(g_crbd_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_crbd_d.getLength()+1
            
         #ON ACTION cancel
         #   LET INT_FLAG = 1
         #   LET g_detail_idx = 1
         #   EXIT DIALOG 
 
      END INPUT
      
 
      
 
      
 
      
 
      
]]>
