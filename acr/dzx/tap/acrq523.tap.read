  <point name="function.acrq523_ins_tmp" order="2" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 抓取数据新增到临时表
# Memo...........:
# Usage..........: CALL acrq523_ins_tmp()
# Input parameter: 无
# Return code....: 无
# Date & Author..: 2014/07/02 By yangxf
# Modify.........:
################################################################################
PRIVATE FUNCTION acrq523_ins_tmp()
   DEFINE l_sql         STRING 
   DEFINE l_mmaf        RECORD 
                          mmafent   LIKE mmaf_t.mmafent,
                          mmaf001   LIKE mmaf_t.mmaf001,
                          mmaf008   LIKE mmaf_t.mmaf008,
                          oocql004  LIKE oocql_t.oocql004,
                          mmaf015   LIKE mmaf_t.mmaf015,
                          age       LIKE type_t.num5,
                          mmaf014   LIKE mmaf_t.mmaf014
                        END RECORD 
   DEFINE l_rtjf003     LIKE rtjf_t.rtjf003
   DEFINE l_rtjf022     LIKE rtjf_t.rtjf022
   
   
   WHENEVER ERROR CONTINUE
   
   DROP TABLE acrq523_tmp
   
   CREATE TEMP TABLE acrq523_tmp(
       mmafent      LIKE mmaf_t.mmafent,
       mmaf001      LIKE mmaf_t.mmaf001, 
       mmaf008      LIKE mmaf_t.mmaf008, 
       oocql004     LIKE oocql_t.oocql004, 
       mmaf015      LIKE mmaf_t.mmaf015, 
       mmaf015_1    LIKE type_t.num5, 
       mmaf014      LIKE mmaf_t.mmaf014, 
       rtjf003      LIKE rtjf_t.rtjf002,
       rtjf022      LIKE rtjf_t.rtjf022
      )
   IF NOT cl_null(g_mmag002) OR NOT cl_null(g_mmag004) THEN 
      LET l_sql = " SELECT mmafent,mmaf001,mmaf008,oocql004,mmaf015,'',mmaf014 ",
                  "   FROM mmaf_t LEFT JOIN oocql_t ON mmaf001 = oocql002 AND oocql001='2016' AND oocql003 = '",g_lang,"',mmag_t ",
                  "  WHERE mmafent = mmagent ",
                  "    AND mmaf001 = mmag001 ",
                  "    AND mmag003 = '2049' ",
                  "    AND ",g_wc_master CLIPPED 
   ELSE
      LET l_sql = " SELECT mmafent,mmaf001,mmaf008,oocql004,mmaf015,'',mmaf014 ",
                  "   FROM mmaf_t LEFT JOIN oocql_t ON mmaf001 = oocql002 AND oocql001='2016' AND oocql003 = '",g_lang,"'",
                  "  WHERE ",g_wc_master CLIPPED 
   END IF 
   PREPARE sel_mmaf_pb FROM l_sql
   DECLARE sel_mmaf_cs CURSOR FOR sel_mmaf_pb
   FOREACH sel_mmaf_cs INTO l_mmaf.*
      #查询消费金额，消费次数，退换货次数
      SELECT Nvl(SUM(rtjf003),0),Nvl(SUM(rtjf022),0)
        INTO l_rtjf003,l_rtjf022
        FROM rtjf_t,mmaq_t
       WHERE rtjfent = g_enterprise
         AND rtjf001 = ''
         AND rtjf014 = mmaq001
         AND mmaq003 = l_mmaf.mmaf001
         AND rtjf025 BETWEEN g_bdate AND g_edate
         
      LET l_mmaf.age = YEAR(g_today) - YEAR(l_mmaf.mmaf015) + 1
         
      INSERT INTO acrq523_tmp VALUES(l_mmaf.mmafent,l_mmaf.mmaf001,l_mmaf.mmaf008,l_mmaf.oocql004,l_mmaf.mmaf015,
                                     l_mmaf.age,l_mmaf.mmaf014,l_rtjf003,l_rtjf022) 
   
   END FOREACH 
END FUNCTION]]>
  </point>
