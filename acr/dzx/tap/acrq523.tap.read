  <section id="acrq523.b_fill" ver="11" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION acrq523_b_fill()
   {<Local define>}
   DEFINE ls_wc           STRING
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   LET g_sql = "SELECT  UNIQUE '','','','','','','','' FROM decc_t",
 
 
               "",
               " WHERE deccent= ? AND 1=1 AND ", ls_wc,cl_sql_add_filter("decc_t")
    
   LET g_sql = g_sql, cl_sql_add_filter("decc_t"),
                      " ORDER BY decc_t.decc001,decc_t.decc002"
  
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
  
   PREPARE acrq523_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR acrq523_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_decc_d.clear()
 
   #add-point:陣列清空
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_decc_d[l_ac].mmaf001,g_decc_d[l_ac].mmaf008,g_decc_d[l_ac].oocql004,g_decc_d[l_ac].mmaf015, 
       g_decc_d[l_ac].l_mmaf015_01,g_decc_d[l_ac].mmaf014,g_decc_d[l_ac].rtjf003,g_decc_d[l_ac].rtjf022 

      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
      
      LET g_decc_d[l_ac].sel = "N"
      #LET g_decc_d[l_ac].statepic = cl_get_actipic(g_decc_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL acrq523_detail_show("'1'")      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            CALL cl_err( "", 9035, 1 )
         END IF
         EXIT FOREACH
      END IF
      
   END FOREACH
   LET g_error_show = 0
   
 
   
   CALL g_decc_d.deleteElement(g_decc_d.getLength())   
 
   #add-point:陣列長度調整
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
    
   LET g_detail_cnt = l_ac - 1 
   DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE acrq523_pb
   
   LET l_ac = 1
   CALL acrq523_fetch()
   
      CALL acrq523_filter_show('mmaf001','b_mmaf001')
   CALL acrq523_filter_show('mmaf008','b_mmaf008')
   CALL acrq523_filter_show('oocql004','b_oocql004')
   CALL acrq523_filter_show('mmaf015','b_mmaf015')
   CALL acrq523_filter_show('mmaf014','b_mmaf014')
   CALL acrq523_filter_show('rtjf003','b_rtjf003')
   CALL acrq523_filter_show('rtjf022','b_rtjf022')
 
   
END FUNCTION
]]>
