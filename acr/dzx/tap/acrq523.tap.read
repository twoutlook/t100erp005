  <section id="acrq523.filter" ver="9" status="" src="s">
    <![CDATA[#+ filter過濾功能
PRIVATE FUNCTION acrq523_filter()
   {<Local define>}
   {</Local define>}
   #add-point:filter段define
   {<point name="filter.define"/>}
   #end add-point    
   
   LET l_ac = 1
   LET g_detail_idx = 1
   LET g_detail_idx2 = 1
 
   LET INT_FLAG = 0
 
   LET g_qryparam.state = 'c'
 
   LET g_wc_filter_t = g_wc_filter
   LET g_wc_t = g_wc
   
   CALL gfrm_curr.setFieldHidden("formonly.sel", TRUE)
   CALL gfrm_curr.setFieldHidden("formonly.statepic", TRUE)
 
   LET g_wc = cl_replace_str(g_wc, g_wc_filter, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON mmaf001,mmaf008,oocql004,mmaf015,mmaf014,rtjf003,rtjf022
                          FROM s_detail1[1].b_mmaf001,s_detail1[1].b_mmaf008,s_detail1[1].b_oocql004, 
                              s_detail1[1].b_mmaf015,s_detail1[1].b_mmaf014,s_detail1[1].b_rtjf003,s_detail1[1].b_rtjf022 

 
         BEFORE CONSTRUCT
#saki       CALL cl_qbe_init()
                     DISPLAY acrq523_filter_parser('mmaf001') TO s_detail1[1].b_mmaf001
            DISPLAY acrq523_filter_parser('mmaf008') TO s_detail1[1].b_mmaf008
            DISPLAY acrq523_filter_parser('oocql004') TO s_detail1[1].b_oocql004
            DISPLAY acrq523_filter_parser('mmaf015') TO s_detail1[1].b_mmaf015
            DISPLAY acrq523_filter_parser('mmaf014') TO s_detail1[1].b_mmaf014
            DISPLAY acrq523_filter_parser('rtjf003') TO s_detail1[1].b_rtjf003
            DISPLAY acrq523_filter_parser('rtjf022') TO s_detail1[1].b_rtjf022
 
 
      END CONSTRUCT
 
      #add-point:filter段add_cs
      {<point name="filter.add_cs"/>}
      #end add-point
 
      BEFORE DIALOG
         #add-point:filter段b_dialog
         {<point name="filter.b_dialog"/>}
         #end add-point  
 
      ON ACTION accept
         ACCEPT DIALOG 
 
      ON ACTION cancel
         LET INT_FLAG = 1
         EXIT DIALOG 
 
      #交談指令共用ACTION
      &include "common_action.4gl" 
         CONTINUE DIALOG
 
   END DIALOG
 
   IF NOT INT_FLAG THEN
      LET g_wc_filter = g_wc_filter, " "
   ELSE
      LET g_wc_filter = g_wc_filter_t
   END IF
   
      CALL acrq523_filter_show('mmaf001','b_mmaf001')
   CALL acrq523_filter_show('mmaf008','b_mmaf008')
   CALL acrq523_filter_show('oocql004','b_oocql004')
   CALL acrq523_filter_show('mmaf015','b_mmaf015')
   CALL acrq523_filter_show('mmaf014','b_mmaf014')
   CALL acrq523_filter_show('rtjf003','b_rtjf003')
   CALL acrq523_filter_show('rtjf022','b_rtjf022')
 
    
