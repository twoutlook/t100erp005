  <point name="query.more_construct" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[      CONSTRUCT g_wc_master ON mmaf001,mmaf008,mmaf014,mmaf015,mmag002,mmag004
           FROM mmaf001,mmaf008,mmaf014,mmaf015,mmag002,mmag004

         BEFORE CONSTRUCT
         
         ON ACTION controlp INFIELD mmaf001
            ###  ### start ###
            INITIALIZE g_qryparam.* TO NULL 
            LET g_qryparam.state = "c"
            LET g_qryparam.reqry = FALSE
            LET g_qryparam.where = "1=1"
            CALL q_mmaf001()
            DISPLAY g_qryparam.return1 TO mmaf001
            ###  ### end ###

         ON ACTION controlp INFIELD mmag002
            ###  ### start ###
            INITIALIZE g_qryparam.* TO NULL 
            LET g_qryparam.state = "c"
            LET g_qryparam.reqry = FALSE
            LET g_qryparam.where = "1=1"
            LET g_qryparam.arg1 = "2049"
            CALL q_oocq002_02()
            DISPLAY g_qryparam.return1 TO mmag002
            ###  ### end ###
            
         ON ACTION controlp INFIELD mmag004
            ###  ### start ###
            INITIALIZE g_qryparam.* TO NULL 
            LET g_qryparam.state = "c"
            LET g_qryparam.reqry = FALSE
            LET g_qryparam.where = "1=1"
            LET g_qryparam.arg1 = "2049"
            CALL q_oocq002_21()
            DISPLAY g_qryparam.return1 TO mmag004
            ###  ### end ###
            
         AFTER CONSTRUCT 
            LET g_mmag002 = GET_FLDBUF(mmag002)
            LET g_mmag004 = GET_FLDBUF(mmag004)
            
      END CONSTRUCT 
      
      INPUT g_bdate,g_edate,g_sel1,g_num1
       FROM bdate,edate,sel1,num1
       ATTRIBUTE(WITHOUT DEFAULTS)
       
         BEFORE INPUT 
            DISPLAY g_bdate,g_edate,g_sel1,g_num1
                 TO bdate,edate,sel1,num1
            CALL acrq523_set_entry()
         
         AFTER FIELD bdate
            IF NOT cl_null(g_bdate) THEN 
               IF NOT cl_null(g_edate) THEN 
                  IF g_bdate > g_edate THEN 
                     CALL cl_err('','ast-00063',1)
                     LET g_bdate = ''
                     NEXT FIELD CURRENT 
                  END IF 
               END IF 
            END IF 
            
         AFTER FIELD edate 
            IF NOT cl_null(g_edate) THEN 
               IF NOT cl_null(g_bdate) THEN 
                  IF g_bdate > g_edate THEN 
                     CALL cl_err('','ast-00063',1)
                     LET g_edate = ''
                     NEXT FIELD CURRENT 
                  END IF 
               END IF 
            END IF 

         AFTER FIELD num1
            IF NOT cl_null(g_num1) THEN 
               IF g_num1 < 0 THEN 
                  CALL cl_err('','ast-00083',1)
                  LET g_num1 = ''
                  DISPLAY g_num1 TO num1
                  NEXT FIELD CURRENT 
               END IF
            END IF 
            
         ON CHANGE sel1
            CALL acrq523_set_entry()
            
      END INPUT 
      BEFORE DIALOG 
         NEXT FIELD bdate]]>
  </point>
