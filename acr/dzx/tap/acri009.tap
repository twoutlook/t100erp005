  <section id="acri009.b_fill" ver="3" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION acri009_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_crdb_d.clear()
   CALL g_crdb2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
      LET g_sql = "SELECT  UNIQUE crdbstus,crdb002,crdb003,crdb004,crdb005,crdb007,crdb006,crdb002,crdb003, 
          crdb004,crdbownid,'',crdbowndp,'',crdbcrtid,'',crdbcrtdp,'',crdbcrtdt,crdbmodid,'',crdbmoddt FROM crdb_t", 
             
                  "",
                  
                  " WHERE crdbent= ? AND crdb001=?"  
                  
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF acri009_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY crdb_t.crdb002,crdb_t.crdb003,crdb_t.crdb004"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      PREPARE acri009_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR acri009_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_crdb_m.crdb001
 
                                               
      FOREACH b_fill_cs INTO g_crdb_d[l_ac].crdbstus,g_crdb_d[l_ac].crdb002,g_crdb_d[l_ac].crdb003,g_crdb_d[l_ac].crdb004, 
