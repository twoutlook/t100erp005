               CLOSE acri009_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_crdb_d[l_ac].crdb002,-263,1)
               LET g_crdb_d[l_ac].* = g_crdb_d_t.*
            ELSE
               #寫入修改者/修改日期資訊
               LET g_crdb2_d[l_ac].crdbmodid = g_user 
LET g_crdb2_d[l_ac].crdbmoddt = cl_get_current()
 
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
         
               UPDATE crdb_t SET (crdb001,crdbstus,crdb002,crdb003,crdb004,crdb005,crdb007,crdb006,crdbownid, 
                   crdbowndp,crdbcrtid,crdbcrtdp,crdbcrtdt,crdbmodid,crdbmoddt) = (g_crdb_m.crdb001, 
                   g_crdb_d[l_ac].crdbstus,g_crdb_d[l_ac].crdb002,g_crdb_d[l_ac].crdb003,g_crdb_d[l_ac].crdb004, 
                   g_crdb_d[l_ac].crdb005,g_crdb_d[l_ac].crdb007,g_crdb_d[l_ac].crdb006,g_crdb2_d[l_ac].crdbownid, 
                   g_crdb2_d[l_ac].crdbowndp,g_crdb2_d[l_ac].crdbcrtid,g_crdb2_d[l_ac].crdbcrtdp,g_crdb2_d[l_ac].crdbcrtdt, 
                   g_crdb2_d[l_ac].crdbmodid,g_crdb2_d[l_ac].crdbmoddt)
                WHERE crdbent = g_enterprise AND crdb001 = g_crdb_m.crdb001 
 
                 AND crdb002 = g_crdb_d_t.crdb002 #項次   
                 AND crdb003 = g_crdb_d_t.crdb003  
                 AND crdb004 = g_crdb_d_t.crdb004  
 
                 
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
               
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("crdb_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                  #WHEN SQLCA.sqlcode #其他錯誤
                  #   CALL cl_err("crdb_t",SQLCA.sqlcode,1)  
                  #   CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_crdb_m.crdb001
               LET gs_keys_bak[1] = g_crdb001_t
               LET gs_keys[2] = g_crdb_d[g_detail_idx].crdb002
               LET gs_keys_bak[2] = g_crdb_d_t.crdb002
               LET gs_keys[3] = g_crdb_d[g_detail_idx].crdb003
               LET gs_keys_bak[3] = g_crdb_d_t.crdb003
               LET gs_keys[4] = g_crdb_d[g_detail_idx].crdb004
               LET gs_keys_bak[4] = g_crdb_d_t.crdb004
               CALL acri009_update_b('crdb_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
               END CASE
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
            END IF
 
         AFTER INPUT
            #若單身還沒有輸入資料, 強制切換至單身
            IF cl_null(g_crdb_d[1].crdb002) THEN
               CALL g_crdb_d.deleteElement(1)
               NEXT FIELD crdb002
            END IF
            #add-point:input段after input 
            {<point name="input.body.after_input"/>}
            #end add-point    
            
         ON ACTION controlo   
            CALL FGL_SET_ARR_CURR(g_crdb_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_crdb_d.getLength()+1
            
      END INPUT
 
 
      
      DISPLAY ARRAY g_crdb2_d TO s_detail2.* ATTRIBUTES(COUNT=g_rec_b)  
      
         BEFORE ROW
            CALL acri009_b_fill(g_wc2) 
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail2")
            LET l_ac = g_detail_idx
            DISPLAY g_detail_idx TO FORMONLY.idx
            CALL acri009_ui_detailshow()
        
         BEFORE DISPLAY 
            CALL FGL_SET_ARR_CURR(g_detail_idx)      
         
         #add-point:page2自定義行為
         {<point name="input.body2.action"/>}
         #end add-point
         
      END DISPLAY
 
      
 
      
      #add-point:input段more_input
      {<point name="input.more_inputarray"/>}
      #end add-point    
      
      BEFORE DIALOG
         #add-point:input段before_dialog
         {<point name="input.before_dialog"/>}
         #end add-point 
         #新增時強制從單頭開始填
         IF p_cmd = 'a' THEN
            NEXT FIELD crdb001
        ELSE
            CASE g_aw
               WHEN "s_detail1"
                  NEXT FIELD crdbstus
               WHEN "s_detail2"
                  NEXT FIELD crdb002_2
 
            END CASE
         END IF
   
      ON ACTION controlf
         CALL cl_set_focus_form(ui.Interface.getRootNode()) RETURNING g_fld_name,g_frm_name
         CALL cl_fldhelp(g_frm_name,g_fld_name,g_lang)
 
      ON ACTION controlr
         CALL cl_show_req_fields()
 
      ON ACTION controls
         IF g_header_hidden THEN
            CALL gfrm_curr.setElementHidden("vb_master",0)
            CALL gfrm_curr.setElementImage("controls","small/arr-u.png")
            LET g_header_hidden = 0     #visible
         ELSE
            CALL gfrm_curr.setElementHidden("vb_master",1)
            CALL gfrm_curr.setElementImage("controls","small/arr-d.png")
            LET g_header_hidden = 1     #hidden     
         END IF
 
      ON ACTION accept
         ACCEPT DIALOG
        
      ON ACTION cancel      #在dialog button (放棄)
         LET g_action_choice=""
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION close       #在dialog 右上角 (X)
