  <point name="function.acrm350_user_ref" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 人員说明
# Memo...........:
# Usage..........: CALL acrm350_user_ref(p_userid)
#                  RETURNING r_userid_desc
# Input parameter: p_userid        人員編號
# Return code....: r_userid_desc   人員名稱
# Date & Author..: 2014/04/24 By Lori
# Modify.........:
################################################################################
PRIVATE FUNCTION acrm350_user_ref(p_userid)
   DEFINE p_userid        LIKE rtkd_t.rtkdmodid
   DEFINE r_userid_desc   LIKE oofa_t.oofa011
   
   LET r_userid_desc = NULL
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = p_userid
   CALL ap_ref_array2(g_ref_fields,"SELECT oofa011 FROM oofa_t WHERE oofaent='"||g_enterprise||"' AND oofa002='2' AND oofa003=? ","") RETURNING g_rtn_fields
   LET r_userid_desc = g_rtn_fields[1]
   
   RETURN r_userid_desc
END FUNCTION]]>
  </point>
  <point name="function.acrm350_get_crad005" order="2" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 取項次
# Memo...........:
# Usage..........: CALL acrm350_get_crad005(p_crad001,p_crad002,p_crad003)
#                  RETURNING r_crad005
# Input parameter: p_crad001   業務人員
#                  p_crad002   日報日期
#                  p_crad003   潛客編號
# Return code....: r_crad005   項次
# Date & Author..: 2014/04/23 By Lori
# Modify.........:
################################################################################
PRIVATE FUNCTION acrm350_get_crad005(p_crad001,p_crad002,p_crad003)
   DEFINE p_crad001   LIKE crad_t.crad001
   DEFINE p_crad002   LIKE crad_t.crad002
   DEFINE p_crad003   LIKE crad_t.crad003
   DEFINE r_crad005   LIKE crad_t.crad005
   
   LET r_crad005 = NULL
   
   SELECT MAX(crad005)+1 INTO r_crad005
     FROM crad_t
    WHERE cradent =  g_enterprise AND crad001 = p_crad001
      AND crad002 = p_crad002 AND crad003 = p_crad003
      
   IF cl_null(r_crad005) THEN
      LET r_crad005 = 1
   END IF
   
   RETURN r_crad005
END FUNCTION]]>
  </point>
