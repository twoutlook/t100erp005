  <section id="acrm350.modify" ver="2" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION acrm350_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_crad_m.crad001 IS NULL
   OR g_crad_m.crad002 IS NULL
   OR g_crad_m.crad003 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE acrm350_master_referesh USING g_crad_m.crad001,g_crad_m.crad002,g_crad_m.crad003 INTO g_crad_m.crad001, 
       g_crad_m.crad002,g_crad_m.crad003,g_crad_m.crad004,g_crad_m.cradunit
 
   ERROR ""
  
   LET g_crad001_t = g_crad_m.crad001
   LET g_crad002_t = g_crad_m.crad002
   LET g_crad003_t = g_crad_m.crad003
 
   CALL s_transaction_begin()
   
   OPEN acrm350_cl USING g_enterprise,g_crad_m.crad001
                                                       ,g_crad_m.crad002
                                                       ,g_crad_m.crad003
 
   IF STATUS THEN
      CALL cl_err("OPEN acrm350_cl:", STATUS, 1)
      CLOSE acrm350_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH acrm350_cl INTO g_crad_m.crad001,g_crad_m.crad001_desc,g_crad_m.crad002,g_crad_m.crad003,g_crad_m.crad003_desc, 
       g_crad_m.crad004,g_crad_m.cradunit,g_crad_m.cradunit_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_crad_m.crad001,SQLCA.sqlcode,0)
      CLOSE acrm350_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL acrm350_show()
   WHILE TRUE
      LET g_crad001_t = g_crad_m.crad001
      LET g_crad002_t = g_crad_m.crad002
      LET g_crad003_t = g_crad_m.crad003
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL acrm350_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_crad_m.* = g_crad_m_t.*
         CALL acrm350_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_crad_m.crad001 != g_crad001_t 
      OR g_crad_m.crad002 != g_crad002_t 
      OR g_crad_m.crad003 != g_crad003_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE crad_t SET crad001 = g_crad_m.crad001
                                       , crad002 = g_crad_m.crad002
                                       , crad003 = g_crad_m.crad003
 
          WHERE cradent = g_enterprise AND crad001 = g_crad001_t
            AND crad002 = g_crad002_t
            AND crad003 = g_crad003_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("crad_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("crad_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_crad_m.crad001,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE acrm350_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_crad_m.crad001,'U')
 
   CALL acrm350_b_fill("1=1")
   
END FUNCTION   
]]>
  </section>
