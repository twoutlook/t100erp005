  <section id="acrm350.b_fill" ver="2" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION acrm350_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_crad_d.clear()
   CALL g_crad2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
      LET g_sql = "SELECT  UNIQUE cradstus,crad005,crad006,'',crad007,crad005,cradownid,'',cradowndp, 
          '',cradcrtid,'',cradcrtdp,'',cradcrtdt,cradmodid,'',cradmoddt,cradcnfid,'',cradcnfdt FROM crad_t", 
             
                  "",
                  
                  " WHERE cradent= ? AND crad001=? AND crad002=? AND crad003=?"  
                  
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF acrm350_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY crad_t.crad005"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      PREPARE acrm350_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR acrm350_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_crad_m.crad001
                                               ,g_crad_m.crad002
                                               ,g_crad_m.crad003
 
                                               
      FOREACH b_fill_cs INTO g_crad_d[l_ac].cradstus,g_crad_d[l_ac].crad005,g_crad_d[l_ac].crad006,g_crad_d[l_ac].crad006_desc, 
          g_crad_d[l_ac].crad007,g_crad2_d[l_ac].crad005,g_crad2_d[l_ac].cradownid,g_crad2_d[l_ac].cradownid_desc, 
          g_crad2_d[l_ac].cradowndp,g_crad2_d[l_ac].cradowndp_desc,g_crad2_d[l_ac].cradcrtid,g_crad2_d[l_ac].cradcrtid_desc, 
          g_crad2_d[l_ac].cradcrtdp,g_crad2_d[l_ac].cradcrtdp_desc,g_crad2_d[l_ac].cradcrtdt,g_crad2_d[l_ac].cradmodid, 
          g_crad2_d[l_ac].cradmodid_desc,g_crad2_d[l_ac].cradmoddt,g_crad2_d[l_ac].cradcnfid,g_crad2_d[l_ac].cradcnfid_desc, 
          g_crad2_d[l_ac].cradcnfdt
                             ,g_crad_d[l_ac].cradstus,g_crad_d[l_ac].crad005,g_crad_d[l_ac].crad006, 
                                 g_crad_d[l_ac].crad006_desc,g_crad_d[l_ac].crad007,g_crad2_d[l_ac].crad005, 
                                 g_crad2_d[l_ac].cradownid,g_crad2_d[l_ac].cradownid_desc,g_crad2_d[l_ac].cradowndp, 
                                 g_crad2_d[l_ac].cradowndp_desc,g_crad2_d[l_ac].cradcrtid,g_crad2_d[l_ac].cradcrtid_desc, 
                                 g_crad2_d[l_ac].cradcrtdp,g_crad2_d[l_ac].cradcrtdp_desc,g_crad2_d[l_ac].cradcrtdt, 
                                 g_crad2_d[l_ac].cradmodid,g_crad2_d[l_ac].cradmodid_desc,g_crad2_d[l_ac].cradmoddt, 
                                 g_crad2_d[l_ac].cradcnfid,g_crad2_d[l_ac].cradcnfid_desc,g_crad2_d[l_ac].cradcnfdt 

 
                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_crad2_d[l_ac].cradownid_desc = cl_get_username(g_crad2_d[l_ac].cradownid)
      #LET g_crad2_d[l_ac].cradowndp_desc = cl_get_deptname(g_crad2_d[l_ac].cradowndp)
      #LET g_crad2_d[l_ac].cradcrtid_desc = cl_get_username(g_crad2_d[l_ac].cradcrtid)
      #LET g_crad2_d[l_ac].cradcrtdp_desc = cl_get_deptname(g_crad2_d[l_ac].cradcrtdp)
      #LET g_crad2_d[l_ac].cradmodid_desc = cl_get_username(g_crad2_d[l_ac].cradmodid)
      #LET g_crad2_d[l_ac].cradcnfid_desc = cl_get_deptname(g_crad2_d[l_ac].cradcnfid)
      ##LET .cradpstid_desc = cl_get_deptname(.cradpstid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_crad_d.deleteElement(g_crad_d.getLength())
      CALL g_crad2_d.deleteElement(g_crad2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE acrm350_pb   
   
END FUNCTION
]]>
  </section>
