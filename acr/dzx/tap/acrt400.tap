  <point name="input.a.crac004" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[            IF NOT cl_null(g_crac_m.crac004) THEN 
               IF p_cmd = 'a' OR ( p_cmd = 'u' AND (g_crac_m.crac004 != g_crac_m_t.crac004) ) THEN
                  #此段落由子樣板a19產生
                  #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
                  INITIALIZE g_chkparam.* TO NULL
                  
                  #設定g_chkparam.*的參數
                  LET g_chkparam.arg1 = '2105'
