  <section id="acrt400.modify" ver="5" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION acrt400_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point
   
   IF g_crac_m.cracdocno IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF 
   
   EXECUTE acrt400_master_referesh USING g_crac_m.cracdocno INTO g_crac_m.cracunit,g_crac_m.cracdocdt, 
       g_crac_m.cracdocno,g_crac_m.cracsite,g_crac_m.crac001,g_crac_m.crac002,g_crac_m.crac003,g_crac_m.cracstus, 
       g_crac_m.crac004,g_crac_m.crac005,g_crac_m.cracownid,g_crac_m.cracowndp,g_crac_m.craccrtid,g_crac_m.craccrtdp, 
       g_crac_m.craccrtdt,g_crac_m.cracmodid,g_crac_m.cracmoddt,g_crac_m.craccnfid,g_crac_m.craccnfdt 

 
   ERROR ""
  
   LET g_cracdocno_t = g_crac_m.cracdocno
 
   
   CALL s_transaction_begin()
   
   OPEN acrt400_cl USING g_enterprise,g_crac_m.cracdocno
   IF STATUS THEN
      CALL cl_err("OPEN acrt400_cl:", STATUS, 1)
      CLOSE acrt400_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH acrt400_cl INTO g_crac_m.cracunit,g_crac_m.cracunit_desc,g_crac_m.cracdocdt,g_crac_m.cracdocno, 
       g_crac_m.cracsite,g_crac_m.crac001,g_crac_m.crac001_desc,g_crac_m.crac002,g_crac_m.craa032,g_crac_m.crac003, 
       g_crac_m.crac003_desc,g_crac_m.cracstus,g_crac_m.crac004,g_crac_m.crac004_desc,g_crac_m.crac005, 
       g_crac_m.crac005_desc,g_crac_m.cracownid,g_crac_m.cracownid_desc,g_crac_m.cracowndp,g_crac_m.cracowndp_desc, 
       g_crac_m.craccrtid,g_crac_m.craccrtid_desc,g_crac_m.craccrtdp,g_crac_m.craccrtdp_desc,g_crac_m.craccrtdt, 
       g_crac_m.cracmodid,g_crac_m.cracmodid_desc,g_crac_m.cracmoddt,g_crac_m.craccnfid,g_crac_m.craccnfid_desc, 
       g_crac_m.craccnfdt
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err("crac_t",SQLCA.sqlcode,0)
      CLOSE acrt400_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
 
   CALL acrt400_show()
   
   WHILE TRUE
      LET g_crac_m.cracdocno = g_cracdocno_t
 
      
      #寫入修改者/修改日期資訊
      LET g_crac_m.cracmodid = g_user 
LET g_crac_m.cracmoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
 
      CALL acrt400_input("u")     #欄位更改
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_crac_m.* = g_crac_m_t.*
         CALL acrt400_show()
         CALL cl_err("",9001,0)
         EXIT WHILE
      END IF
 
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_crac_m.cracdocno,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE acrt400_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_crac_m.cracdocno,"U")
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
