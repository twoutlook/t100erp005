  <point name="function.acrt400_crac001_chk" order="4" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 檢核有無未確認的變更單
# Memo...........:
# Usage..........: CALL acrt400_crac001_chk(p_crac001)
#                  RETURN r_success
# Input parameter: p_crac001   潛客編號
# Return code....: r_success   檢核結果
# Date & Author..: 2014/04/23 By Lori
# Modify.........:
################################################################################
PRIVATE FUNCTION acrt400_crac001_chk(p_crac001)
   DEFINE p_crac001 LIKE crac_t.crac001
   DEFINE r_success LIKE type_t.num5
   DEFINE l_cnt     LIKE type_t.num5
   
   LET r_success = TRUE
   LET l_cnt = NULL
   
   SELECT COUNT(*) INTO l_cnt
     FROM crac_t 
    WHERE cracent = g_enterprise AND crac001 = p_crac001
      AND cracstus = 'N'

   IF l_cnt > 0 THEN
      LET r_success = FALSE
      CALL cl_err(g_crac_m.crac001,'acr-00021',1)
   END IF
   
   RETURN r_success
END FUNCTION]]>
  </point>
