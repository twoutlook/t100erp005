            #add-point:ON ACTION controlp INFIELD crbbsite
            {<point name="input.c.page1.crbbsite" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_crbb_d[l_ac].* = g_crbb_d_t.*
               CLOSE acrt410_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_crbb_d[l_ac].crbbseq,-263,1)
               LET g_crbb_d[l_ac].* = g_crbb_d_t.*
            ELSE
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               #寫入修改者/修改日期資訊(單身)
               
      
               UPDATE crbb_t SET (crbbdocno,crbbseq,crbb001,crbb002,crbbsite) = (g_crba_m.crbadocno, 
                   g_crbb_d[l_ac].crbbseq,g_crbb_d[l_ac].crbb001,g_crbb_d[l_ac].crbb002,g_crbb_d[l_ac].crbbsite) 

                WHERE crbbent = g_enterprise AND crbbdocno = g_crba_m.crbadocno 
 
                  AND crbbseq = g_crbb_d_t.crbbseq #項次   
 
                  
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("crbb_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                     LET g_crbb_d[l_ac].* = g_crbb_d_t.*
                  WHEN SQLCA.sqlcode #其他錯誤
                     CALL cl_err("crbb_t",SQLCA.sqlcode,1)
                     LET g_crbb_d[l_ac].* = g_crbb_d_t.*                     
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_crba_m.crbadocno
               LET gs_keys_bak[1] = g_crbadocno_t
               LET gs_keys[2] = g_crbb_d[g_detail_idx].crbbseq
               LET gs_keys_bak[2] = g_crbb_d_t.crbbseq
               CALL acrt410_update_b('crbb_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
               END CASE
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            #add-point:單身after_row
            {<point name="input.body.after_row"/>}
            #end add-point
            CALL acrt410_unlock_b("crbb_t","'1'")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock
            
            #add-point:單身after_row2
            {<point name="input.body.after_row2"/>}
            #end add-point
 
         AFTER INPUT
            #add-point:input段after input 
            {<point name="input.body.after_input"/>}
            #end add-point 
 
         ON ACTION controlo    
            CALL FGL_SET_ARR_CURR(g_crbb_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_crbb_d.getLength()+1
            
         #ON ACTION cancel
         #   LET INT_FLAG = 1
         #   LET g_detail_idx = 1
         #   EXIT DIALOG 
 
      END INPUT
      
 
      
 
      
 
      
 
      
]]>
