################################################################################
# Descriptions...: 營運組織帶出說明
# Memo...........:
# Usage..........: CALL acrt410_crbasite_desc()
# Date & Author..: 2014/04/21 By dongsz
# Modify.........:
################################################################################
PRIVATE FUNCTION acrt410_crbasite_desc()
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_crba_m.crbasite
   CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_crba_m.crbasite_desc = '', g_rtn_fields[1] , ''
   DISPLAY BY NAME g_crba_m.crbasite_desc
END FUNCTION]]>
