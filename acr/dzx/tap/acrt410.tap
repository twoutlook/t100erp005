  <section id="acrt410.modify" ver="7" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION acrt410_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_crba_m.crbadocno IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE acrt410_master_referesh USING g_crba_m.crbadocno INTO g_crba_m.crbasite,g_crba_m.crbadocdt, 
       g_crba_m.crbadocno,g_crba_m.crba004,g_crba_m.crba005,g_crba_m.crba001,g_crba_m.crba002,g_crba_m.crbastus, 
       g_crba_m.crba026,g_crba_m.crba006,g_crba_m.crba007,g_crba_m.crba008,g_crba_m.crba014,g_crba_m.crba015, 
       g_crba_m.crba011,g_crba_m.crba012,g_crba_m.crba013,g_crba_m.crba009,g_crba_m.crba010,g_crba_m.crba003, 
       g_crba_m.crba016,g_crba_m.crba017,g_crba_m.crba018,g_crba_m.crba019,g_crba_m.crba020,g_crba_m.crba021, 
       g_crba_m.crba022,g_crba_m.crba023,g_crba_m.crba024,g_crba_m.crba025,g_crba_m.crbaownid,g_crba_m.crbaowndp, 
       g_crba_m.crbacrtid,g_crba_m.crbacrtdp,g_crba_m.crbacrtdt,g_crba_m.crbamodid,g_crba_m.crbamoddt, 
       g_crba_m.crbacnfid,g_crba_m.crbacnfdt
 
   ERROR ""
  
   LET g_crbadocno_t = g_crba_m.crbadocno
 
   CALL s_transaction_begin()
   
   OPEN acrt410_cl USING g_enterprise,g_crba_m.crbadocno
 
   IF STATUS THEN
      CALL cl_err("OPEN acrt410_cl:", STATUS, 1)
      CLOSE acrt410_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH acrt410_cl INTO g_crba_m.crbasite,g_crba_m.crbasite_desc,g_crba_m.crbadocdt,g_crba_m.crbadocno, 
       g_crba_m.crba004,g_crba_m.crba005,g_crba_m.crba001,g_crba_m.crba001_desc,g_crba_m.crba002,g_crba_m.crba002_desc, 
       g_crba_m.crbastus,g_crba_m.crba026,g_crba_m.crba006,g_crba_m.crba007,g_crba_m.crba008,g_crba_m.crba014, 
       g_crba_m.crba015,g_crba_m.crba011,g_crba_m.crba012,g_crba_m.crba013,g_crba_m.crba009,g_crba_m.crba010, 
       g_crba_m.crba003,g_crba_m.crba003_desc,g_crba_m.crba016,g_crba_m.crba017,g_crba_m.imaal004,g_crba_m.crba018, 
       g_crba_m.crba018_desc,g_crba_m.crba019,g_crba_m.crba020,g_crba_m.crba021,g_crba_m.crba022,g_crba_m.crba023, 
       g_crba_m.crba024,g_crba_m.crba025,g_crba_m.crbaownid,g_crba_m.crbaownid_desc,g_crba_m.crbaowndp, 
       g_crba_m.crbaowndp_desc,g_crba_m.crbacrtid,g_crba_m.crbacrtid_desc,g_crba_m.crbacrtdp,g_crba_m.crbacrtdp_desc, 
       g_crba_m.crbacrtdt,g_crba_m.crbamodid,g_crba_m.crbamodid_desc,g_crba_m.crbamoddt,g_crba_m.crbacnfid, 
       g_crba_m.crbacnfid_desc,g_crba_m.crbacnfdt
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_crba_m.crbadocno,SQLCA.sqlcode,0)
      CLOSE acrt410_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   CALL acrt410_show()
   WHILE TRUE
      LET g_crbadocno_t = g_crba_m.crbadocno
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_crba_m.crbamodid = g_user 
LET g_crba_m.crbamoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL acrt410_input("u")     #欄位更改
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_crba_m.* = g_crba_m_t.*
         CALL acrt410_show()
         CALL cl_err('',9001,0)
         CALL s_transaction_end('N','0')
         EXIT WHILE
      END IF
     
      #若有modid跟moddt則進行update
      UPDATE crba_t SET (crbamodid,crbamoddt) = (g_crba_m.crbamodid,g_crba_m.crbamoddt)
       WHERE crbaent = g_enterprise AND crbadocno = g_crbadocno_t
 
 
      #若單頭key欄位有變更
      IF g_crba_m.crbadocno != g_crbadocno_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE crbb_t SET crbbdocno = g_crba_m.crbadocno
 
          WHERE crbbent = g_enterprise AND crbbdocno = g_crbadocno_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("crbb_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("crbb_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_crba_m.crbadocno,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE acrt410_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_crba_m.crbadocno,'U')
 
END FUNCTION   
]]>
