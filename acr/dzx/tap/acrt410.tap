         BEFORE FIELD crbb001
            #add-point:BEFORE FIELD crbb001
            {<point name="input.b.page1.crbb001" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD crbb001
            
            #add-point:AFTER FIELD crbb001
            {<point name="input.a.page1.crbb001" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE crbb001
            #add-point:ON CHANGE crbb001
            {<point name="input.g.page1.crbb001" />}
            #END add-point
 
         #----<<crbb002>>----
         #此段落由子樣板a01產生
         BEFORE FIELD crbb002
            #add-point:BEFORE FIELD crbb002
            {<point name="input.b.page1.crbb002" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD crbb002
            
            #add-point:AFTER FIELD crbb002
            {<point name="input.a.page1.crbb002" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE crbb002
            #add-point:ON CHANGE crbb002
            {<point name="input.g.page1.crbb002" />}
            #END add-point
 
         #----<<crbbsite>>----
         #此段落由子樣板a01產生
         BEFORE FIELD crbbsite
            #add-point:BEFORE FIELD crbbsite
            {<point name="input.b.page1.crbbsite" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD crbbsite
            
            #add-point:AFTER FIELD crbbsite
            {<point name="input.a.page1.crbbsite" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE crbbsite
            #add-point:ON CHANGE crbbsite
