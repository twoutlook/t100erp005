  <point name="statechange.b_update" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[   IF NOT cl_null(lc_state_t) THEN
      LET lc_state_o = lc_state
      LET lc_state = lc_state_t
   END IF
   LET l_success = TRUE
   CASE
      WHEN lc_state = 'Y'
         CALL s_acrt410_conf_chk(g_crba_m.crbadocno,g_crba_m.crbastus) RETURNING l_success,g_errno
         IF l_success THEN
            IF cl_ask_confirm('aim-00108') THEN
               CALL s_transaction_begin()
               CALL s_acrt410_conf_upd(g_crba_m.crbadocno) RETURNING l_success,g_errno
               IF NOT l_success THEN
                  CALL cl_err(g_crba_m.crbadocno,g_errno,1)
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','1')
               END IF
            ELSE
               RETURN
            END IF
         ELSE
            CALL cl_err(g_crba_m.crbadocno,g_errno,1)
            CALL s_transaction_end('N','0')
            RETURN
         END IF
      WHEN lc_state = 'N'
         CALL s_acrt410_unconf_chk(g_crba_m.crbadocno,g_crba_m.crbastus) RETURNING l_success,g_errno
         IF l_success THEN
            IF cl_ask_confirm('aim-00110') THEN
               CALL s_transaction_begin()
               CALL s_acrt410_unconf_upd(g_crba_m.crbadocno) RETURNING l_success,g_errno
               IF NOT l_success THEN
                  CALL cl_err(g_crba_m.crbadocno,g_errno,1)
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','1')
               END IF
            ELSE
               RETURN
            END IF
         ELSE
            CALL cl_err(g_crba_m.crbadocno,g_errno,1)
            CALL s_transaction_end('N','0')
            RETURN
         END IF
      WHEN lc_state = 'X'
         CALL s_acrt410_void_chk(g_crba_m.crbadocno,g_crba_m.crbastus) RETURNING l_success,g_errno
         IF l_success THEN
            IF cl_ask_confirm('aim-00109') THEN
               CALL s_transaction_begin()
               CALL s_acrt410_void_upd(g_crba_m.crbadocno) RETURNING l_success,g_errno
               IF NOT l_success THEN
                  CALL cl_err(g_crba_m.crbadocno,g_errno,1)
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','1')
               END IF
            ELSE
               RETURN
            END IF
         ELSE
            CALL cl_err(g_crba_m.crbadocno,g_errno,1)
            CALL s_transaction_end('N','0')
            RETURN
         END IF
      WHEN lc_state_t = 'U'
         CALL s_acrt410_unvoid_chk(g_crba_m.crbadocno,g_crba_m.crbastus) RETURNING l_success,g_errno
         IF l_success THEN
            IF cl_ask_confirm('aim-00129') THEN
               CALL s_transaction_begin()
               CALL s_acrt410_unvoid_upd(g_crba_m.crbadocno) RETURNING l_success,g_errno
               IF NOT l_success THEN
                  CALL cl_err(g_crba_m.crbadocno,g_errno,1)
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','1')
               END IF
            ELSE
               RETURN
            END IF
         ELSE
            CALL cl_err(g_crba_m.crbadocno,g_errno,1)
            CALL s_transaction_end('N','0')
            RETURN
         END IF
      WHEN lc_state = 'C'
         CALL s_acrt410_close_chk(g_crba_m.crbadocno,g_crba_m.crbastus) RETURNING l_success,g_errno
         IF l_success THEN
            IF cl_ask_confirm('axm-00201') THEN
               CALL s_transaction_begin()
               CALL s_acrt410_close_upd(g_crba_m.crbadocno) RETURNING l_success,g_errno
               IF NOT l_success THEN
                  CALL cl_err(g_crba_m.crbadocno,g_errno,1)
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','1')
               END IF
            ELSE
               RETURN
            END IF
         ELSE
            CALL cl_err(g_crba_m.crbadocno,g_errno,1)
            CALL s_transaction_end('N','0')
            RETURN
         END IF
      WHEN lc_state_t = 'D'
         CALL s_acrt410_unclose_chk(g_crba_m.crbadocno,g_crba_m.crbastus) RETURNING l_success,g_errno
         IF l_success THEN
            IF cl_ask_confirm('acr-00007') THEN
               CALL s_transaction_begin()
               CALL s_acrt410_unclose_upd(g_crba_m.crbadocno) RETURNING l_success,g_errno
               IF NOT l_success THEN
                  CALL cl_err(g_crba_m.crbadocno,g_errno,1)
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','1')
               END IF
            ELSE
               RETURN
            END IF
         ELSE
            CALL cl_err(g_crba_m.crbadocno,g_errno,1)
            CALL s_transaction_end('N','0')
            RETURN
         END IF
      OTHERWISE
         RETURN
   END CASE                 {#ADP版次:1#}  
   IF NOT cl_null(lc_state_o) THEN
      LET lc_state = lc_state_o
