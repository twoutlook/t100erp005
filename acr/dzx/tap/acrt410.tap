  <point name="function.acrt410_crba026_ref" order="4" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 由來源單號帶出原銷售單各欄位值
# Memo...........:
# Usage..........: CALL acrt410_crba026_ref()
# Date & Author..: 2014/04/21 By dongsz
# Modify.........:
################################################################################
PRIVATE FUNCTION acrt410_crba026_ref()
   IF NOT cl_null(g_crba_m.crba006) AND NOT cl_null(g_crba_m.crba007) THEN
      SELECT rtja033,rtjadocdt,rtja007,rtja010,rtja001,rtjb004,rtja026,rtja027,rtjb010,rtja004
        INTO g_crba_m.crba026,g_crba_m.crba008,g_crba_m.crba015,g_crba_m.crba009,g_crba_m.crba011,
             g_crba_m.crba016,g_crba_m.crba018,g_crba_m.crba019,g_crba_m.crba020,g_crba_m.crba003
        FROM rtja_t,rtjb_t
       WHERE rtjaent = g_enterprise
         AND rtjaent = rtjbent
         AND rtjadocno = rtjbdocno
         AND rtjadocno = g_crba_m.crba006
         AND rtjbseq = g_crba_m.crba007
   ELSE
      IF cl_null(g_crba_m.crba006) AND cl_null(g_crba_m.crba007) THEN
         SELECT rtjadocno,rtjadocdt,rtja007,rtja002,rtja001,rtja004
           INTO g_crba_m.crba006,g_crba_m.crba008,g_crba_m.crba015,g_crba_m.crba009,g_crba_m.crba011,g_crba_m.crba003
           FROM rtja_t
          WHERE rtjaent = g_enterprise
            AND rtja033 = g_crba_m.crba026
      END IF
      IF NOT cl_null(g_crba_m.crba006) AND cl_null(g_crba_m.crba007) THEN
         SELECT rtja033,rtjadocno,rtjadocdt,rtja007,rtja002,rtja001,rtja004
           INTO g_crba_m.crba026,g_crba_m.crba006,g_crba_m.crba008,g_crba_m.crba015,g_crba_m.crba009,g_crba_m.crba011,g_crba_m.crba003
           FROM rtja_t
          WHERE rtjaent = g_enterprise
            AND rtjadocno = g_crba_m.crba006
      END IF
   END IF
   SELECT pmaal004 INTO g_crba_m.crba010 FROM pmaal_t
    WHERE pmaalent = g_enterprise
      AND pmaal001 = g_crba_m.crba009
      AND pmaal002 = g_dlang
   SELECT mmaq003 INTO g_crba_m.crba012 FROM mmaq_t
    WHERE mmaqent = g_enterprise
      AND mmaq001 = g_crba_m.crba011
   SELECT mmaa008 INTO g_crba_m.crba013 FROM mmaa_t a
    WHERE mmaaent = g_enterprise
      AND mmaa001 = g_crba_m.crba012
      AND mmaa002 = (SELECT MAX(mmaa002) FROM mmaa_t WHERE mmaaent = a.mmaaent AND mmaa001 = a.mmaa001 AND mmaastus = 'Y')
   SELECT imaal003,imaal004 INTO g_crba_m.crba017,g_crba_m.imaal004 FROM imaal_t
    WHERE imaalent = g_enterprise
      AND imaal001 = g_crba_m.crba016
      AND imaal002 = g_dlang
   SELECT ooail003 INTO g_crba_m.crba018_desc FROM ooail_t
    WHERE ooailent = g_enterprise
      AND ooail001 = g_crba_m.crba018
      AND ooail002 = g_dlang
   SELECT oofa011 INTO g_crba_m.crba003_desc FROM oofa_t
    WHERE oofaent = g_enterprise
      AND oofa002 = '2'
      AND oofa003 = g_crba_m.crba003
   DISPLAY BY NAME g_crba_m.crba026,g_crba_m.crba008,g_crba_m.crba015,g_crba_m.crba009,g_crba_m.crba010,
                   g_crba_m.crba011,g_crba_m.crba012,g_crba_m.crba013,g_crba_m.crba017,g_crba_m.imaal004,
                   g_crba_m.crba016,g_crba_m.crba018,g_crba_m.crba018_desc,g_crba_m.crba019,g_crba_m.crba020,
                   g_crba_m.crba003,g_crba_m.crba003_desc
   LET g_crba_m_o.* = g_crba_m.*

END FUNCTION]]>
  </point>
  <point name="function.acrt410_crba015_ref" order="5" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 訂單帶值
# Memo...........:
# Usage..........: CALL acrt410_crba015_ref()
# Date & Author..: 2014/04/21 By dongsz
# Modify.........:
################################################################################
PRIVATE FUNCTION acrt410_crba015_ref()
   SELECT xmda004,xmda002 INTO g_crba_m.crba009,g_crba_m.crba003
     FROM xmda_t
    WHERE xmdaent = g_enterprise
      AND xmdadocno = g_crba_m.crba015
   SELECT pmaal004 INTO g_crba_m.crba010 FROM pmaal_t
    WHERE pmaalent = g_enterprise
      AND pmaal001 = g_crba_m.crba009
      AND pmaal002 = g_dlang
   SELECT oofa011 INTO g_crba_m.crba003_desc FROM oofa_t
    WHERE oofaent = g_enterprise
      AND oofa002 = '2'
      AND oofa003 = g_crba_m.crba003
   SELECT rtja001 INTO g_crba_m.crba011 FROM rtja_t,rtjb_t
    WHERE rtjaent = rtjbent AND rtjadocno = rtjbdocno
      AND rtjb001 = g_crba_m.crba015
   SELECT mmaq003 INTO g_crba_m.crba012 FROM mmaq_t
    WHERE mmaqent = g_enterprise
      AND mmaq001 = g_crba_m.crba011
   SELECT mmaa008 INTO g_crba_m.crba013 FROM mmaa_t a
    WHERE mmaaent = g_enterprise
      AND mmaa001 = g_crba_m.crba012
      AND mmaa002 = (SELECT MAX(mmaa002) FROM mmaa_t WHERE mmaaent = a.mmaaent AND mmaa001 = a.mmaa001 AND mmaastus = 'Y')
   DISPLAY BY NAME g_crba_m.crba009,g_crba_m.crba010,g_crba_m.crba003,g_crba_m.crba003_desc,
                   g_crba_m.crba011,g_crba_m.crba012,g_crba_m.crba013

END FUNCTION]]>
