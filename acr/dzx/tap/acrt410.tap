  <point name="function.acrt410_crba018_ref" order="9" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 由幣別帶匯率
# Memo...........:
# Usage..........: CALL acrt410_crba018_ref()
# Date & Author..: 2014/04/21 By dongsz
# Modify.........:
################################################################################
PRIVATE FUNCTION acrt410_crba018_ref()
DEFINE l_ooef015      LIKE ooef_t.ooef015
DEFINE l_ooef016      LIKE ooef_t.ooef016
DEFINE l_ooam005      LIKE ooam_t.ooam005
DEFINE l_gzsz008      LIKE gzsz_t.gzsz008

   #取匯率參照表號、主幣別
   SELECT ooef015,ooef016 INTO l_ooef015,l_ooef016 FROM ooef_t
    WHERE ooefent = g_enterprise AND ooef001 = g_crba_m.crbasite

   #取交易貨幣批量
   SELECT ooam005 INTO l_ooam005 FROM ooam_t
    WHERE ooament = g_enterprise AND ooam001 = l_ooef015
      AND ooam003 = g_crba_m.crba018 AND ooam004 = g_crba_m.crbadocdt

   #取匯率類型   
   SELECT gzsz008 INTO l_gzsz008 FROM gzsz_t
    WHERE gzsz001 = 'ooab_t' AND gzsz002 = 'S-BAS-0010'

   CALL s_aooi160_get_exrate('1',g_crba_m.crbasite,g_crba_m.crbadocdt,g_crba_m.crba018,l_ooef016,l_ooam005,l_gzsz008)
      RETURNING g_crba_m.crba019
      
   DISPLAY BY NAME g_crba_m.crba019
      
END FUNCTION]]>
  </point>
  <point name="function.acrt410_crba009_ref" order="10" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 根據客戶編號帶出幣別與匯率
# Memo...........:
# Usage..........: CALL acrt410_crba009_ref()
# Date & Author..: 2014/05/06 By dongsz
# Modify.........:
################################################################################
PRIVATE FUNCTION acrt410_crba009_ref()

   IF cl_null(g_crba_m.crba006) AND cl_null(g_crba_m.crba007) THEN
#      SELECT pmaa011 INTO g_crba_m.crba018 FROM pmaa_t
#       WHERE pmaaent = g_enterprise
#         AND pmaa001 = g_crba_m.crba009
      SELECT pmab083 INTO g_crba_m.crba018
        FROM pmab_t
       WHERE pmabent = g_enterprise
         AND pmabsite = g_crba_m.crbasite
         AND pmab001 = g_crba_m.crba009
      DISPLAY BY NAME g_crba_m.crba018
      CALL acrt410_crba018_ref()
   END IF
 
END FUNCTION]]>
