  <section id="acrt410.main" ver="15" status="" src="s">
    <![CDATA[#+ 此段落由子樣板a26產生
#OPTIONS SHORT CIRCUIT
#+ 作業開始 
MAIN
   #add-point:main段define
   {<point name="main.define"/>}
   #end add-point   
 
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("acr","")
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = "SELECT crbasite,'',crbadocdt,crbadocno,crba004,crba005,crba001,'',crba002,'', 
       crbastus,crba026,crba006,crba007,crba008,crba014,crba015,crba011,crba012,crba013,crba009,crba010, 
       crba003,'',crba016,crba017,'',crba018,'',crba019,crba020,crba021,crba022,crba023,crba024,crba025, 
       crbaownid,'',crbaowndp,'',crbacrtid,'',crbacrtdp,'',crbacrtdt,crbamodid,'',crbamoddt,crbacnfid, 
       '',crbacnfdt FROM crba_t WHERE crbaent= ? AND crbadocno=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   DECLARE acrt410_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE crbasite,crbadocdt,crbadocno,crba004,crba005,crba001,crba002,crbastus, 
       crba026,crba006,crba007,crba008,crba014,crba015,crba011,crba012,crba013,crba009,crba010,crba003, 
       crba016,crba017,crba018,crba019,crba020,crba021,crba022,crba023,crba024,crba025,crbaownid,crbaowndp, 
       crbacrtid,crbacrtdp,crbacrtdt,crbamodid,crbamoddt,crbacnfid,crbacnfdt",
               " FROM crba_t",
               " WHERE crbaent = '" ||g_enterprise|| "' AND crbadocno = ?"
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE acrt410_master_referesh FROM g_sql
 
 
 
 
   IF g_bgjob = "Y" THEN
 
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
 
   ELSE
      
      #畫面開啟 (identifier)
      OPEN WINDOW w_acrt410 WITH FORM cl_ap_formpath("acr",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL acrt410_init()   
 
      #進入選單 Menu (="N")
      CALL acrt410_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_acrt410
      
   END IF 
   
   CLOSE acrt410_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
   
END MAIN
 
 
]]>
