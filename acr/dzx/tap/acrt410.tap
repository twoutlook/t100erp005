   END IF   
 
   LET INT_FLAG = 0
 
   LET g_qryparam.state = 'c'
 
   LET g_wc_filter_t = g_wc_filter.trim()
   LET g_wc_t = g_wc
 
   LET g_wc = cl_replace_str(g_wc, g_wc_filter_t, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON crbasite,crbadocdt,crbadocno,crba004,crba005,crba001,crba002
                          FROM s_browse[1].b_crbasite,s_browse[1].b_crbadocdt,s_browse[1].b_crbadocno, 
                              s_browse[1].b_crba004,s_browse[1].b_crba005,s_browse[1].b_crba001,s_browse[1].b_crba002 

 
         BEFORE CONSTRUCT
               DISPLAY acrt410_filter_parser('crbasite') TO s_browse[1].b_crbasite
            DISPLAY acrt410_filter_parser('crbadocdt') TO s_browse[1].b_crbadocdt
            DISPLAY acrt410_filter_parser('crbadocno') TO s_browse[1].b_crbadocno
            DISPLAY acrt410_filter_parser('crba004') TO s_browse[1].b_crba004
            DISPLAY acrt410_filter_parser('crba005') TO s_browse[1].b_crba005
            DISPLAY acrt410_filter_parser('crba001') TO s_browse[1].b_crba001
            DISPLAY acrt410_filter_parser('crba002') TO s_browse[1].b_crba002
      
         #add-point:filter段cs_ctrl
         {<point name="filter.cs_ctrl"/>}
         #end add-point
      
      END CONSTRUCT
 
      #add-point:filter段add_cs
      {<point name="filter.add_cs"/>}
      #end add-point
 
      BEFORE DIALOG
         #add-point:filter段b_dialog
         {<point name="filter.b_dialog"/>}
         #end add-point  
      
      ON ACTION accept
         ACCEPT DIALOG
 
      ON ACTION cancel
         LET INT_FLAG = 1
         EXIT DIALOG 
 
      #交談指令共用ACTION
      &include "common_action.4gl" 
         CONTINUE DIALOG
   
   END DIALOG
 
   IF NOT INT_FLAG THEN
      LET g_wc_filter = "   AND   ", g_wc_filter, "   "
