                     IF NOT ap_chk_notDup("","SELECT COUNT(*) FROM crca_t WHERE "||"crcaent = '" ||g_enterprise|| "' AND "||"crcaseq = '"||g_crca_d[g_detail_idx].crcaseq ||"' AND "|| "crca001 = '"||g_crca_d[g_detail_idx].crca001 ||"'",'std-00004',0) THEN 
