                        LET g_crca_d[l_ac].crcaseq = g_crca_d_t.crcaseq
                        CALL acri007_crca001_ref()
                        NEXT FIELD CURRENT
                     END IF
                  END IF
                  #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
