  <point name="function.acri007_create_crcaseq" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
#ceate seq
PRIVATE FUNCTION acri007_create_crcaseq(p_cmd)
   DEFINE p_cmd    LIKE type_t.chr1
   IF p_cmd='u' THEN
      IF g_crca_d[l_ac].crca001<>g_crca_d_t.crca001 THEN
         SELECT max(crcaseq)+1 INTO g_crca_d[l_ac].crcaseq
           FROM crca_t
          WHERE crca001 = g_crca_d[l_ac].crca001
            AND crcaent = g_enterprise
      END IF
   END IF   
   IF p_cmd='a' THEN
      SELECT max(crcaseq)+1 INTO g_crca_d[l_ac].crcaseq
        FROM crca_t
       WHERE crca001 = g_crca_d[l_ac].crca001
         AND crcaent = g_enterprise
   END IF    
   IF cl_null(g_crca_d[l_ac].crcaseq) THEN
      LET g_crca_d[l_ac].crcaseq=1
   END IF   
END FUNCTION]]>
  </point>
  <point name="function.acri007_null_crca002" order="2" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
#null crca002
PRIVATE FUNCTION acri007_null_crca002()
   
   LET g_crca_d[l_ac].name = NULL
   LET g_crca_d[l_ac].sex = NULL
   LET g_crca_d[l_ac].age = NULL
END FUNCTION]]>
  </point>
  <point name="function.acri007_crca002_ref" order="3" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
##display crca002
PRIVATE FUNCTION acri007_crca002_ref()
   DEFINE l_mmaf015   LIKE mmaf_t.mmaf015
   
   SELECT mmaq003 INTO g_crca_d[l_ac].crca003 FROM mmaq_t
    WHERE mmaqent = g_enterprise AND mmaq001 = g_crca_d[l_ac].crca002
   
   DISPLAY g_crca_d[l_ac].crca003 TO s_detail1[l_ac].crca003  
    
END FUNCTION]]>
  </point>
  <point name="function.acri007_crca003_ref" order="4" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
#disply crca003
PRIVATE FUNCTION acri007_crca003_ref(p_cmd)
   DEFINE l_mmag004  LIKE mmag_t.mmag004
   DEFINE l_mmaf015  LIKE mmaf_t.mmaf015
   DEFINE l_mmaf016  LIKE mmaf_t.mmaf016
   DEFINE l_mmaf014  LIKE mmaf_t.mmaf014
   DEFINE p_cmd      LIKE type_t.chr1
   
   SELECT mmaf008,mmaf015,mmaf014 
     INTO g_crca_d[l_ac].name,l_mmaf015,g_crca_d[l_ac].crca009
     FROM mmaf_t
    WHERE mmafent = g_enterprise AND mmaf001 = g_crca_d[l_ac].crca003
   
   SELECT mmaf016 
     INTO l_mmaf016
     FROM mmaf_t,pmaa_t
    WHERE mmaf016=pmaa001 AND (pmaa002 ='2' OR pmaa002='3') AND mmafent = g_enterprise AND mmaf001 = g_crca_d[l_ac].crca003   
   if not cl_null( g_crca_d[l_ac].crca003) AND not cl_null(l_mmaf015) then
      SELECT round((to_number(sysdate-to_date(l_mmaf015,'yyyy-mm-dd hh24:mi:ss'))/365)) INTO g_crca_d[l_ac].age
        FROM dual 
   END IF 
   SELECT mmag004 INTO l_mmag004 FROM mmag_t
    WHERE mmagent = g_enterprise
      AND mmag001 = g_crca_d[l_ac].crca003 AND mmag002= '01'
      AND mmag003 = '2016'      
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = '2016'
   LET g_ref_fields[2] = l_mmag004
   CALL ap_ref_array2(g_ref_fields,"SELECT oocql004 FROM oocql_t WHERE oocqlent='"||g_enterprise||"' AND oocql001=? AND oocql002=? AND oocql003='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_crca_d[l_ac].sex = '', g_rtn_fields[1] , ''
   DISPLAY  g_crca_d[l_ac].name,g_crca_d[l_ac].sex,g_crca_d[l_ac].age,g_crca_d[l_ac].crca009
        TO s_detail1[l_ac].name,s_detail1[l_ac].sex,s_detail1[l_ac].age,s_detail1[l_ac].crca009
   IF NOT cl_null(l_mmaf016) THEN
      LET g_crca_d[l_ac].crca001 = l_mmaf016
      CALL acri007_crca001_ref()
      CALL acri007_create_crcaseq(p_cmd)
      DISPLAY g_crca_d[l_ac].crca001,g_crca_d[l_ac].crcaseq
           TO s_detail1[l_ac].crca001,s_detail1[l_ac].crcaseq
   END IF   
END FUNCTION]]>
  </point>
  <point name="function.acri007_check_crca002" order="5" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
#check crca002
PRIVATE FUNCTION acri007_check_crca002()
   DEFINE l_cnt   LIKE type_t.num5
   
   LET l_cnt = 0
   LET g_errno = NULL
   IF NOT cl_null(g_crca_d[l_ac].crca003) AND NOT cl_null(g_crca_d[l_ac].crca002) THEN
      SELECT count(*) INTO l_cnt FROM mmaq_t
       WHERE mmaq001 = g_crca_d[l_ac].crca002 AND mmaq003=g_crca_d[l_ac].crca003
         AND mmaqent = g_enterprise
      IF l_cnt <= 0 THEN
         LET g_errno = "acr-00011"
      END IF      
   END IF
END FUNCTION]]>
  </point>
