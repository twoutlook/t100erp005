  <section id="acri007.update_b" ver="1" status="" src="s">
    <![CDATA[#+ 修改單身後其他table連動
PRIVATE FUNCTION acri007_update_b(ps_table,ps_keys,ps_keys_bak,ps_page)
   DEFINE ps_page          STRING
   DEFINE ps_table         STRING
   DEFINE ps_keys          DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ps_keys_bak      DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ls_group         STRING
   DEFINE li_idx           LIKE type_t.num5 
   DEFINE lb_chk           BOOLEAN
   DEFINE l_new_key        DYNAMIC ARRAY OF STRING
   DEFINE l_old_key        DYNAMIC ARRAY OF STRING
   DEFINE l_field_key      DYNAMIC ARRAY OF STRING
   #add-point:update_b段define
   {<point name="update_b.define"/>}
   #end add-point     
   
   #判斷key是否有改變
   LET lb_chk = TRUE
   FOR li_idx = 1 TO ps_keys.getLength()
      IF ps_keys[li_idx] <> ps_keys_bak[li_idx] THEN
         LET lb_chk = FALSE
         EXIT FOR
      END IF
   END FOR
   
   #不需要做處理
   IF lb_chk THEN
      RETURN
   END IF
    
   #判斷是否是同一群組的table
   LET ls_group = "crca_t,"
   IF ls_group.getIndexOf(ps_table,1) > 0 AND ps_table <> "crca_t" THEN
      #add-point:update_b段修改前
      {<point name="update_b.b_update" mark="Y"/>}
      #end add-point     
      UPDATE crca_t 
         SET (crcaseq,crca001
              ,crcastus,crca002,crca003,crca009,crca004,crca010,crca005,crca006,crca007,crca008,crca011,crca012,crca013,crca014,crca015,crcaownid,crcaowndp,crcacrtid,crcacrtdp,crcacrtdt,crcamodid,crcamoddt) 
              = 
             (ps_keys[1],ps_keys[2]
              ,g_crca_d[l_ac].crcastus,g_crca_d[l_ac].crca002,g_crca_d[l_ac].crca003,g_crca_d[l_ac].crca009, 
                  g_crca_d[l_ac].crca004,g_crca_d[l_ac].crca010,g_crca_d[l_ac].crca005,g_crca_d[l_ac].crca006, 
                  g_crca_d[l_ac].crca007,g_crca_d[l_ac].crca008,g_crca_d[l_ac].crca011,g_crca_d[l_ac].crca012, 
                  g_crca_d[l_ac].crca013,g_crca_d[l_ac].crca014,g_crca_d[l_ac].crca015,g_crca2_d[l_ac].crcaownid, 
                  g_crca2_d[l_ac].crcaowndp,g_crca2_d[l_ac].crcacrtid,g_crca2_d[l_ac].crcacrtdp,g_crca2_d[l_ac].crcacrtdt, 
                  g_crca2_d[l_ac].crcamodid,g_crca2_d[l_ac].crcamoddt) 
         WHERE crcaseq = ps_keys_bak[1] AND crca001 = ps_keys_bak[2]
      #add-point:update_b段修改中
      {<point name="update_b.m_update"/>}
      #end add-point 
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            CALL cl_err("crca_t","std-00009",1)
            CALL s_transaction_end('N','0')
         WHEN SQLCA.sqlcode #其他錯誤
            CALL cl_err("crca_t",SQLCA.sqlcode,1)  
            CALL s_transaction_end('N','0')
         OTHERWISE
            
      END CASE
      #add-point:update_b段修改後
      {<point name="update_b.a_update"/>}
      #end add-point 
      RETURN
   END IF
   
 
   
END FUNCTION
]]>
