  <section id="acri007.b_fill" ver="3" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION acri007_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2    STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE crcastus,crca002,crca003,crca009,crca001,crca004,crcaseq,crca010,crca005, 
       crca006,crca007,crca008,crca011,crca012,crca013,crca014,crca015,crca001,crcaseq,crcaownid,crcaowndp, 
       crcacrtid,crcacrtdp,crcacrtdt,crcamodid,crcamoddt ,t1.pmaal004 ,t2.oofa011 ,t3.ooefl003 ,t4.oofa011 , 
       t5.ooefl003 ,t6.oofa011 FROM crca_t",
               "",
                              " LEFT JOIN pmaal_t t1 ON t1.pmaalent='"||g_enterprise||"' AND t1.pmaal001=crca001 AND t1.pmaal002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t2 ON t2.oofaent='"||g_enterprise||"' AND t2.oofa002='2' AND t2.oofa003=crcaownid  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent='"||g_enterprise||"' AND t3.ooefl001=crcaowndp AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t4 ON t4.oofaent='"||g_enterprise||"' AND t4.oofa002='2' AND t4.oofa003=crcacrtid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=crcacrtdp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t6 ON t6.oofaent='"||g_enterprise||"' AND t6.oofa002='2' AND t6.oofa003=crcamodid  ",
 
               " WHERE crcaent= ? AND 1=1 AND ", p_wc2 
    
   LET g_sql = g_sql, cl_sql_add_filter("crca_t"),
                      " ORDER BY crca_t.crcaseq,crca_t.crca001"
   
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE acri007_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR acri007_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_crca_d.clear()
   CALL g_crca2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_crca_d[l_ac].crcastus,g_crca_d[l_ac].crca002,g_crca_d[l_ac].crca003,g_crca_d[l_ac].crca009, 
       g_crca_d[l_ac].crca001,g_crca_d[l_ac].crca004,g_crca_d[l_ac].crcaseq,g_crca_d[l_ac].crca010,g_crca_d[l_ac].crca005, 
       g_crca_d[l_ac].crca006,g_crca_d[l_ac].crca007,g_crca_d[l_ac].crca008,g_crca_d[l_ac].crca011,g_crca_d[l_ac].crca012, 
       g_crca_d[l_ac].crca013,g_crca_d[l_ac].crca014,g_crca_d[l_ac].crca015,g_crca2_d[l_ac].crca001, 
       g_crca2_d[l_ac].crcaseq,g_crca2_d[l_ac].crcaownid,g_crca2_d[l_ac].crcaowndp,g_crca2_d[l_ac].crcacrtid, 
       g_crca2_d[l_ac].crcacrtdp,g_crca2_d[l_ac].crcacrtdt,g_crca2_d[l_ac].crcamodid,g_crca2_d[l_ac].crcamoddt, 
       g_crca_d[l_ac].crca001_desc,g_crca2_d[l_ac].crcaownid_desc,g_crca2_d[l_ac].crcaowndp_desc,g_crca2_d[l_ac].crcacrtid_desc, 
       g_crca2_d[l_ac].crcacrtdp_desc,g_crca2_d[l_ac].crcamodid_desc
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL acri007_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec THEN
      IF g_error_show = 1 THEN
         CALL cl_err("crca_t",9035,1)
      END IF
   END IF 
   LET g_error_show = 0
   
 
   
   CALL g_crca_d.deleteElement(g_crca_d.getLength())   
   CALL g_crca2_d.deleteElement(g_crca2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_crca_d.getLength()
      LET g_crca2_d[l_ac].crcaseq = g_crca_d[l_ac].crcaseq 
      LET g_crca2_d[l_ac].crca001 = g_crca_d[l_ac].crca001 
 
   END FOR
   
   IF g_cnt > g_crca_d.getLength() THEN
      LET l_ac = g_crca_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_crca_d.getLength()
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE acri007_pb
   
END FUNCTION
]]>
  </section>
