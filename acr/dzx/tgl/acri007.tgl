               LET li_reproduce = l_ac_t
               LET li_reproduce_target = l_ac
               LET g_crca_d[li_reproduce_target].* = g_crca_d[li_reproduce].*
               LET g_crca2_d[li_reproduce_target].* = g_crca2_d[li_reproduce].*
 
               LET g_crca_d[li_reproduce_target].crcaseq = NULL
               LET g_crca_d[li_reproduce_target].crca001 = NULL
 
            ELSE
               CALL FGL_SET_ARR_CURR(g_crca_d.getLength()+1)
               LET lb_reproduce = TRUE
               LET li_reproduce = l_ac
               LET li_reproduce_target = g_crca_d.getLength()+1
            END IF
            
