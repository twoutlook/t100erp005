               LET INT_FLAG = 0
               LET g_crca_d[l_ac].* = g_crca_d_t.*
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               #add-point:單身取消時 name="input.body.cancel"
               {<point name="input.body.cancel"/>}
               #end add-point
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = g_crca_d[l_ac].crcaseq 
               LET g_errparam.code   = -263 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               LET g_crca_d[l_ac].* = g_crca_d_t.*
            ELSE
               #寫入修改者/修改日期資訊(單身)
               LET g_crca2_d[l_ac].crcamodid = g_user 
LET g_crca2_d[l_ac].crcamoddt = cl_get_current()
LET g_crca2_d[l_ac].crcamodid_desc = cl_get_username(g_crca2_d[l_ac].crcamodid)
            
               #add-point:單身修改前 name="input.body.b_update"
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               #將遮罩欄位還原
               CALL acri007_crca_t_mask_restore('restore_mask_o')
 
               UPDATE crca_t SET (crcastus,crca002,crca003,crca009,crca001,crca004,crcaseq,crca010,crca005, 
                   crca006,crca007,crca008,crca011,crca012,crca013,crca014,crca015,crcaownid,crcaowndp, 
                   crcacrtid,crcacrtdp,crcacrtdt,crcamodid,crcamoddt) = (g_crca_d[l_ac].crcastus,g_crca_d[l_ac].crca002, 
                   g_crca_d[l_ac].crca003,g_crca_d[l_ac].crca009,g_crca_d[l_ac].crca001,g_crca_d[l_ac].crca004, 
                   g_crca_d[l_ac].crcaseq,g_crca_d[l_ac].crca010,g_crca_d[l_ac].crca005,g_crca_d[l_ac].crca006, 
                   g_crca_d[l_ac].crca007,g_crca_d[l_ac].crca008,g_crca_d[l_ac].crca011,g_crca_d[l_ac].crca012, 
                   g_crca_d[l_ac].crca013,g_crca_d[l_ac].crca014,g_crca_d[l_ac].crca015,g_crca2_d[l_ac].crcaownid, 
                   g_crca2_d[l_ac].crcaowndp,g_crca2_d[l_ac].crcacrtid,g_crca2_d[l_ac].crcacrtdp,g_crca2_d[l_ac].crcacrtdt, 
                   g_crca2_d[l_ac].crcamodid,g_crca2_d[l_ac].crcamoddt)
                WHERE crcaent = g_enterprise AND
                  crcaseq = g_crca_d_t.crcaseq #項次   
                  AND crca001 = g_crca_d_t.crca001  
 
                  
               #add-point:單身修改中 name="input.body.m_update"
               {<point name="input.body.m_update"/>}
               #end add-point
                  
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL s_transaction_end('N','0')
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "crca_t" 
                     LET g_errparam.code = "std-00009" 
                     LET g_errparam.popup = TRUE 
                     CALL cl_err()
                  WHEN SQLCA.SQLCODE #其他錯誤
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "crca_t:",SQLERRMESSAGE 
                     LET g_errparam.code = SQLCA.SQLCODE
                     LET g_errparam.popup = TRUE 
                     CALL s_transaction_end('N','0')
                     CALL cl_err()
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_crca_d[g_detail_idx].crcaseq
               LET gs_keys_bak[1] = g_crca_d_t.crcaseq
               LET gs_keys[2] = g_crca_d[g_detail_idx].crca001
               LET gs_keys_bak[2] = g_crca_d_t.crca001
               CALL acri007_update_b('crca_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
                     #修改歷程記錄(修改)
                     LET g_log1 = util.JSON.stringify(g_crca_d_t)
                     LET g_log2 = util.JSON.stringify(g_crca_d[l_ac])
                     IF NOT cl_log_modified_record(g_log1,g_log2) THEN
                        CALL s_transaction_end('N','0')
                     END IF
               END CASE
               
               #將遮罩欄位進行遮蔽
               CALL acri007_crca_t_mask_restore('restore_mask_n')
               
               #add-point:單身修改後 name="input.body.a_update"
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            CALL acri007_unlock_b("crca_t")
            IF INT_FLAG THEN
               CALL s_transaction_end('N','0')
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
