#+ filter過濾功能
PRIVATE FUNCTION acrm302_filter()
   #add-point:filter段define name="filter.define_customerization"
   {<point name="filter.define_customerization" edit="c" mark="Y"/>}
   #end add-point   
   #add-point:filter段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="filter.define"
   {<point name="filter.define"/>}
   #end add-point   
   
   #add-point:Function前置處理  name="filter.pre_function"
   {<point name="filter.pre_function"/>}
   #end add-point
   
   #切換畫面
   IF NOT g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",1)
      CALL gfrm_curr.setElementHidden("worksheet",0)
      LET g_main_hidden = 1
   END IF   
 
   LET INT_FLAG = 0
 
   LET g_qryparam.state = 'c'
 
   LET g_wc_filter_t = g_wc_filter.trim()
   LET g_wc_t = g_wc
 
   LET g_wc = cl_replace_str(g_wc, g_wc_filter_t, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON crafunit,craf001,craf002,craf003,craf004,craf005,craf006,craf007,craf008 

                          FROM s_browse[1].b_crafunit,s_browse[1].b_craf001,s_browse[1].b_craf002,s_browse[1].b_craf003, 
                              s_browse[1].b_craf004,s_browse[1].b_craf005,s_browse[1].b_craf006,s_browse[1].b_craf007, 
                              s_browse[1].b_craf008
 
         BEFORE CONSTRUCT
               DISPLAY acrm302_filter_parser('crafunit') TO s_browse[1].b_crafunit
            DISPLAY acrm302_filter_parser('craf001') TO s_browse[1].b_craf001
            DISPLAY acrm302_filter_parser('craf002') TO s_browse[1].b_craf002
            DISPLAY acrm302_filter_parser('craf003') TO s_browse[1].b_craf003
            DISPLAY acrm302_filter_parser('craf004') TO s_browse[1].b_craf004
            DISPLAY acrm302_filter_parser('craf005') TO s_browse[1].b_craf005
            DISPLAY acrm302_filter_parser('craf006') TO s_browse[1].b_craf006
            DISPLAY acrm302_filter_parser('craf007') TO s_browse[1].b_craf007
            DISPLAY acrm302_filter_parser('craf008') TO s_browse[1].b_craf008
      
         #add-point:filter段cs_ctrl name="filter.cs_ctrl"
         {<point name="filter.cs_ctrl"/>}
         #end add-point
      
      END CONSTRUCT
 
