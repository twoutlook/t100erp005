{<section id="acrm302.state_change" >}
   #應用 a09 樣板自動產生(Version:17)
#+ 確認碼變更 
PRIVATE FUNCTION acrm302_statechange()
   #add-point:statechange段define(客製用) name="statechange.define_customerization"
   {<point name="statechange.define_customerization" edit="c" mark="Y"/>}
   #end add-point  
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="statechange.define"
   {<point name="statechange.define"/>}
   #end add-point  
   
   #add-point:Function前置處理 name="statechange.before"
   {<point name="statechange.before"/>}
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_craf_m.craf001 IS NULL
      OR g_craf_m.craf003 IS NULL
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN acrm302_cl USING g_enterprise,g_craf_m.craf001,g_craf_m.craf003
   IF STATUS THEN
      CLOSE acrm302_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN acrm302_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE acrm302_master_referesh USING g_craf_m.craf001,g_craf_m.craf003 INTO g_craf_m.crafunit,g_craf_m.craf002, 
       g_craf_m.craf001,g_craf_m.craf003,g_craf_m.craf004,g_craf_m.craf005,g_craf_m.crafstus,g_craf_m.craf006, 
       g_craf_m.craf007,g_craf_m.craf008,g_craf_m.crafownid,g_craf_m.crafowndp,g_craf_m.crafcrtid,g_craf_m.crafcrtdp, 
       g_craf_m.crafcrtdt,g_craf_m.crafmodid,g_craf_m.crafmoddt,g_craf_m.crafcnfid,g_craf_m.crafcnfdt, 
       g_craf_m.crafunit_desc,g_craf_m.craf001_desc,g_craf_m.craf003_desc,g_craf_m.craf005_desc,g_craf_m.crafownid_desc, 
       g_craf_m.crafowndp_desc,g_craf_m.crafcrtid_desc,g_craf_m.crafcrtdp_desc,g_craf_m.crafmodid_desc, 
       g_craf_m.crafcnfid_desc
   
 
   #檢查是否允許此動作
   IF NOT acrm302_action_chk() THEN
      CLOSE acrm302_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_craf_m.crafunit,g_craf_m.crafunit_desc,g_craf_m.craf002,g_craf_m.craf001,g_craf_m.craf001_desc, 
       g_craf_m.craf003,g_craf_m.craf003_desc,g_craf_m.craf004,g_craf_m.craf005,g_craf_m.craf005_desc, 
       g_craf_m.crafstus,g_craf_m.craf006,g_craf_m.craf007,g_craf_m.craf008,g_craf_m.crafownid,g_craf_m.crafownid_desc, 
       g_craf_m.crafowndp,g_craf_m.crafowndp_desc,g_craf_m.crafcrtid,g_craf_m.crafcrtid_desc,g_craf_m.crafcrtdp, 
       g_craf_m.crafcrtdp_desc,g_craf_m.crafcrtdt,g_craf_m.crafmodid,g_craf_m.crafmodid_desc,g_craf_m.crafmoddt, 
       g_craf_m.crafcnfid,g_craf_m.crafcnfid_desc,g_craf_m.crafcnfdt
 
   CASE g_craf_m.crafstus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/open.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/valid.png")
      WHEN "X"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/void.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   {<point name="statechange.after_refresh"/>}
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_craf_m.crafstus
            
            WHEN "N"
               HIDE OPTION "open"
            WHEN "Y"
               HIDE OPTION "valid"
            WHEN "X"
               HIDE OPTION "void"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      {<point name="statechange.before_menu"/>}
      #end add-point
      
      
	  
      ON ACTION open
         IF cl_auth_chk_act("open") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.open"
            {<point name="statechange.open"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION valid
         IF cl_auth_chk_act("valid") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.valid"
            {<point name="statechange.valid"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION void
         IF cl_auth_chk_act("void") THEN
            LET lc_state = "X"
            #add-point:action控制 name="statechange.void"
            {<point name="statechange.void"/>}
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      {<point name="statechange.more_control"/>}
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      AND lc_state <> "X"
      ) OR 
      g_craf_m.crafstus = lc_state OR cl_null(lc_state) THEN
      CLOSE acrm302_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   {<point name="statechange.b_update"/>}
   #end add-point
   
   LET g_craf_m.crafmodid = g_user
   LET g_craf_m.crafmoddt = cl_get_current()
   LET g_craf_m.crafstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE craf_t 
      SET (crafstus,crafmodid,crafmoddt) 
        = (g_craf_m.crafstus,g_craf_m.crafmodid,g_craf_m.crafmoddt)     
    WHERE crafent = g_enterprise AND craf001 = g_craf_m.craf001
      AND craf003 = g_craf_m.craf003
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/open.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/valid.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/void.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE acrm302_master_referesh USING g_craf_m.craf001,g_craf_m.craf003 INTO g_craf_m.crafunit, 
          g_craf_m.craf002,g_craf_m.craf001,g_craf_m.craf003,g_craf_m.craf004,g_craf_m.craf005,g_craf_m.crafstus, 
          g_craf_m.craf006,g_craf_m.craf007,g_craf_m.craf008,g_craf_m.crafownid,g_craf_m.crafowndp,g_craf_m.crafcrtid, 
          g_craf_m.crafcrtdp,g_craf_m.crafcrtdt,g_craf_m.crafmodid,g_craf_m.crafmoddt,g_craf_m.crafcnfid, 
          g_craf_m.crafcnfdt,g_craf_m.crafunit_desc,g_craf_m.craf001_desc,g_craf_m.craf003_desc,g_craf_m.craf005_desc, 
          g_craf_m.crafownid_desc,g_craf_m.crafowndp_desc,g_craf_m.crafcrtid_desc,g_craf_m.crafcrtdp_desc, 
          g_craf_m.crafmodid_desc,g_craf_m.crafcnfid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_craf_m.crafunit,g_craf_m.crafunit_desc,g_craf_m.craf002,g_craf_m.craf001,g_craf_m.craf001_desc, 
          g_craf_m.craf003,g_craf_m.craf003_desc,g_craf_m.craf004,g_craf_m.craf005,g_craf_m.craf005_desc, 
          g_craf_m.crafstus,g_craf_m.craf006,g_craf_m.craf007,g_craf_m.craf008,g_craf_m.crafownid,g_craf_m.crafownid_desc, 
          g_craf_m.crafowndp,g_craf_m.crafowndp_desc,g_craf_m.crafcrtid,g_craf_m.crafcrtid_desc,g_craf_m.crafcrtdp, 
          g_craf_m.crafcrtdp_desc,g_craf_m.crafcrtdt,g_craf_m.crafmodid,g_craf_m.crafmodid_desc,g_craf_m.crafmoddt, 
          g_craf_m.crafcnfid,g_craf_m.crafcnfid_desc,g_craf_m.crafcnfdt
   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
   {<point name="statechange.a_update"/>}
   #end add-point
 
   #add-point:statechange段結束前 name="statechange.after"
   {<point name="statechange.after"/>}
   #end add-point  
 
   CLOSE acrm302_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL acrm302_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION
 
 
 
