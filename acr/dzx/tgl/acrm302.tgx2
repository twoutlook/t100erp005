   #end add-point   
   #add-point:main段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="main.define"
   {<point name="main.define"/>}
   #end add-point   
   
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("acr","")
 
   #add-point:作業初始化 name="main.init"
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define name="main.define_sql"
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT crafunit,'',craf002,craf001,'',craf003,'',craf004,craf005,'',crafstus, 
       craf006,craf007,craf008,crafownid,'',crafowndp,'',crafcrtid,'',crafcrtdp,'',crafcrtdt,crafmodid, 
       '',crafmoddt,crafcnfid,'',crafcnfdt", 
                      " FROM craf_t",
                      " WHERE crafent= ? AND craf001=? AND craf003=? FOR UPDATE"
   #add-point:SQL_define name="main.after_define_sql"
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE acrm302_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.crafunit,t0.craf002,t0.craf001,t0.craf003,t0.craf004,t0.craf005, 
       t0.crafstus,t0.craf006,t0.craf007,t0.craf008,t0.crafownid,t0.crafowndp,t0.crafcrtid,t0.crafcrtdp, 
       t0.crafcrtdt,t0.crafmodid,t0.crafmoddt,t0.crafcnfid,t0.crafcnfdt,t1.ooefl003 ,t2.craal004 ,t3.crabl004 , 
       t4.ooag011 ,t5.ooag011 ,t6.ooefl003 ,t7.ooag011 ,t8.ooefl003 ,t9.ooag011 ,t10.ooag011",
               " FROM craf_t t0",
                              " LEFT JOIN ooefl_t t1 ON t1.ooeflent="||g_enterprise||" AND t1.ooefl001=t0.crafunit AND t1.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN craal_t t2 ON t2.craalent="||g_enterprise||" AND t2.craal001=t0.craf001 AND t2.craal002='"||g_dlang||"' ",
               " LEFT JOIN crabl_t t3 ON t3.crablent="||g_enterprise||" AND t3.crabl001=t0.craf003 AND t3.crabl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t4 ON t4.ooagent="||g_enterprise||" AND t4.ooag001=t0.craf005  ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=t0.crafownid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent="||g_enterprise||" AND t6.ooefl001=t0.crafowndp AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent="||g_enterprise||" AND t7.ooag001=t0.crafcrtid  ",
               " LEFT JOIN ooefl_t t8 ON t8.ooeflent="||g_enterprise||" AND t8.ooefl001=t0.crafcrtdp AND t8.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t9 ON t9.ooagent="||g_enterprise||" AND t9.ooag001=t0.crafmodid  ",
               " LEFT JOIN ooag_t t10 ON t10.ooagent="||g_enterprise||" AND t10.ooag001=t0.crafcnfid  ",
 
               " WHERE t0.crafent = " ||g_enterprise|| " AND t0.craf001 = ? AND t0.craf003 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define name="main.after_refresh_sql"
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE acrm302_master_referesh FROM g_sql
 
    
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call name="main.servicecall"
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_acrm302 WITH FORM cl_ap_formpath("acr",g_code)
   
