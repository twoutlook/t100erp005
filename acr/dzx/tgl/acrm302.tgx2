{<section id="acrm302.modify" >}
#+ 資料修改
PRIVATE FUNCTION acrm302_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   {<point name="modify.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   {<point name="modify.define"/>}
   #end add-point
   
   #add-point:Function前置處理 name="modify.pre_function"
   {<point name="modify.pre_function"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_craf_m.craf001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_craf001_t = g_craf_m.craf001
   LET g_craf003_t = g_craf_m.craf003
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN acrm302_cl USING g_enterprise,g_craf_m.craf001,g_craf_m.craf003
   IF SQLCA.SQLCODE THEN    #(ver:49)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN acrm302_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE
      LET g_errparam.popup = TRUE 
      CLOSE acrm302_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE acrm302_master_referesh USING g_craf_m.craf001,g_craf_m.craf003 INTO g_craf_m.crafunit,g_craf_m.craf002, 
       g_craf_m.craf001,g_craf_m.craf003,g_craf_m.craf004,g_craf_m.craf005,g_craf_m.crafstus,g_craf_m.craf006, 
       g_craf_m.craf007,g_craf_m.craf008,g_craf_m.crafownid,g_craf_m.crafowndp,g_craf_m.crafcrtid,g_craf_m.crafcrtdp, 
       g_craf_m.crafcrtdt,g_craf_m.crafmodid,g_craf_m.crafmoddt,g_craf_m.crafcnfid,g_craf_m.crafcnfdt, 
       g_craf_m.crafunit_desc,g_craf_m.craf001_desc,g_craf_m.craf003_desc,g_craf_m.craf005_desc,g_craf_m.crafownid_desc, 
       g_craf_m.crafowndp_desc,g_craf_m.crafcrtid_desc,g_craf_m.crafcrtdp_desc,g_craf_m.crafmodid_desc, 
       g_craf_m.crafcnfid_desc
 
   #檢查是否允許此動作
   IF NOT acrm302_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_craf_m_mask_o.* =  g_craf_m.*
   CALL acrm302_craf_t_mask()
   LET g_craf_m_mask_n.* =  g_craf_m.*
   
   
 
   #顯示資料
   CALL acrm302_show()
   
   WHILE TRUE
      LET g_craf_m.craf001 = g_craf001_t
      LET g_craf_m.craf003 = g_craf003_t
 
      
      #寫入修改者/修改日期資訊
      LET g_craf_m.crafmodid = g_user 
LET g_craf_m.crafmoddt = cl_get_current()
LET g_craf_m.crafmodid_desc = cl_get_username(g_craf_m.crafmodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
      #end add-point
 
      #資料輸入
      CALL acrm302_input("u")     
 
      #add-point:modify段修改後 name="modify.after_input"
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         LET g_craf_m.* = g_craf_m_t.*
         CALL acrm302_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE craf_t SET (crafmodid,crafmoddt) = (g_craf_m.crafmodid,g_craf_m.crafmoddt)
       WHERE crafent = g_enterprise AND craf001 = g_craf001_t
         AND craf003 = g_craf003_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL acrm302_set_act_visible()
   CALL acrm302_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " crafent = " ||g_enterprise|| " AND",
