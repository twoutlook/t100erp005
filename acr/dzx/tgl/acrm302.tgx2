                   g_craf_m.craf004,g_craf_m.craf005,g_craf_m.crafstus,g_craf_m.craf006,g_craf_m.craf007, 
                   g_craf_m.craf008,g_craf_m.crafownid,g_craf_m.crafowndp,g_craf_m.crafcrtid,g_craf_m.crafcrtdp, 
