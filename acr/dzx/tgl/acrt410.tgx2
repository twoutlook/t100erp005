   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   DEFINE l_wc2_table1          STRING
 
 
   #add-point:modify段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   {<point name="modify.define"/>} 
   #end add-point    
   
   #add-point:Function前置處理  name="modify.pre_function"
   {<point name="modify.pre_function"/>}
   #end add-point
   
   #保存單頭舊值
   LET g_crba_m_t.* = g_crba_m.*
   LET g_crba_m_o.* = g_crba_m.*
   
   IF g_crba_m.crbadocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code = "std-00003" 
      LET g_errparam.popup = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_crbadocno_t = g_crba_m.crbadocno
 
   CALL s_transaction_begin()
   
   OPEN acrt410_cl USING g_enterprise,g_crba_m.crbadocno
   IF SQLCA.SQLCODE THEN   #(ver:78)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN acrt410_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE   #(ver:78)
      LET g_errparam.popup = TRUE 
      CLOSE acrt410_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE acrt410_master_referesh USING g_crba_m.crbadocno INTO g_crba_m.crbasite,g_crba_m.crbadocdt, 
       g_crba_m.crbadocno,g_crba_m.crba004,g_crba_m.crba005,g_crba_m.crba001,g_crba_m.crba002,g_crba_m.crbastus, 
       g_crba_m.crba026,g_crba_m.crba006,g_crba_m.crba007,g_crba_m.crba008,g_crba_m.crba014,g_crba_m.crba015, 
       g_crba_m.crba011,g_crba_m.crba012,g_crba_m.crba013,g_crba_m.crba009,g_crba_m.crba010,g_crba_m.crba003, 
       g_crba_m.crba016,g_crba_m.crba017,g_crba_m.crba018,g_crba_m.crba019,g_crba_m.crba020,g_crba_m.crba021, 
       g_crba_m.crba022,g_crba_m.crba023,g_crba_m.crba024,g_crba_m.crba025,g_crba_m.crbaownid,g_crba_m.crbaowndp, 
       g_crba_m.crbacrtid,g_crba_m.crbacrtdp,g_crba_m.crbacrtdt,g_crba_m.crbamodid,g_crba_m.crbamoddt, 
       g_crba_m.crbacnfid,g_crba_m.crbacnfdt,g_crba_m.crbasite_desc,g_crba_m.crba001_desc,g_crba_m.crba002_desc, 
       g_crba_m.crba003_desc,g_crba_m.crba018_desc,g_crba_m.crbaownid_desc,g_crba_m.crbaowndp_desc,g_crba_m.crbacrtid_desc, 
       g_crba_m.crbacrtdp_desc,g_crba_m.crbamodid_desc,g_crba_m.crbacnfid_desc
   
   #檢查是否允許此動作
   IF NOT acrt410_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_crba_m_mask_o.* =  g_crba_m.*
   CALL acrt410_crba_t_mask()
   LET g_crba_m_mask_n.* =  g_crba_m.*
   
   
   
   #add-point:modify段show之前 name="modify.before_show"
   {<point name="modify.before_show"/>}
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
 
   
   CALL acrt410_show()
   #add-point:modify段show之後 name="modify.after_show"
   {<point name="modify.after_show"/>}
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
 
 
    
   WHILE TRUE
      LET g_crbadocno_t = g_crba_m.crbadocno
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_crba_m.crbamodid = g_user 
LET g_crba_m.crbamoddt = cl_get_current()
LET g_crba_m.crbamodid_desc = cl_get_username(g_crba_m.crbamodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      LET g_master_commit = "N"
      CALL acrt410_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後 name="modify.after_input"
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE crba_t SET (crbamodid,crbamoddt) = (g_crba_m.crbamodid,g_crba_m.crbamoddt)
          WHERE crbaent = g_enterprise AND crbadocno = g_crbadocno_t
 
      END IF
    
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         #若單頭無commit則還原
         IF g_master_commit = "N" THEN
            LET g_crba_m.* = g_crba_m_t.*
            CALL acrt410_show()
         END IF
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code = 9001 
         LET g_errparam.popup = FALSE 
         CALL cl_err()
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_crba_m.crbadocno != g_crba_m_t.crbadocno
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前 name="modify.body.b_fk_update"
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE crbb_t SET crbbdocno = g_crba_m.crbadocno
 
          WHERE crbbent = g_enterprise AND crbbdocno = g_crba_m_t.crbadocno
 
            
         #add-point:單身fk修改中 name="modify.body.m_fk_update"
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            #   INITIALIZE g_errparam TO NULL 
            #   LET g_errparam.extend = "crbb_t" 
            #   LET g_errparam.code = "std-00009" 
            #   LET g_errparam.popup = TRUE 
            #   CALL cl_err()
            #   CALL s_transaction_end('N','0')
            #   CONTINUE WHILE
            WHEN SQLCA.SQLCODE #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "crbb_t:",SQLERRMESSAGE 
               LET g_errparam.code = SQLCA.SQLCODE 
               LET g_errparam.popup = TRUE 
               CALL s_transaction_end('N','0')
               CALL cl_err()
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後 name="modify.body.a_fk_update"
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL acrt410_set_act_visible()   
   CALL acrt410_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " crbaent = " ||g_enterprise|| " AND",
                      " crbadocno = '", g_crba_m.crbadocno, "' "
 
   #填到對應位置
   CALL acrt410_browser_fill("")
 
   CLOSE acrt410_cl
   
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL acrt410_msgcentre_notify('modify')
 
END FUNCTION 
