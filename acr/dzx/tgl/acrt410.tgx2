         RETURN
      END IF
      
      LET INT_FLAG = 0
      #CALL g_crbb_d.clear()
 
 
      LET g_rec_b = 0
      CALL s_transaction_end('Y','0')
      EXIT WHILE
        
   END WHILE
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL acrt410_set_act_visible()   
   CALL acrt410_set_act_no_visible()
   
   #將新增的資料併入搜尋條件中
   LET g_crbadocno_t = g_crba_m.crbadocno
 
   
   #組合新增資料的條件
   LET g_add_browse = " crbaent = " ||g_enterprise|| " AND",
                      " crbadocno = '", g_crba_m.crbadocno, "' "
 
                      
   #add-point:組合新增資料的條件後 name="insert.after.add_browse"
   {<point name="insert.after.add_browse"/>}
