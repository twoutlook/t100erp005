         LET l_success1 = FALSE
         CONTINUE FOREACH 
      END IF
      LET l_cnt = l_cnt+1
   END FOREACH
#   IF NOT l_success1 THEN   #qiaozy---有错误报错，程序要可以继续运行
#      CALL cl_showmsg()     #qiaozy
#   END IF                   #qiaozy   
#   IF l_success1 THEN       #qiaozy
   #160225-00040# 20160302 s983961--add(s)
   LET l_msg = cl_getmsg('amm-00494',g_lang)
