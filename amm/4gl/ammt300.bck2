       g_mmaa_m.mmaa012,g_mmaa_m.mmaaud001,g_mmaa_m.mmaaud002,g_mmaa_m.mmaaud003,g_mmaa_m.mmaaud004, 
       g_mmaa_m.mmaaud005,g_mmaa_m.mmaaud006,g_mmaa_m.mmaaud011,g_mmaa_m.mmaaud012,g_mmaa_m.mmaaud013, 
       g_mmaa_m.mmaaud014,g_mmaa_m.mmaaud015,g_mmaa_m.mmaaud016,g_mmaa_m.mmaaud021,g_mmaa_m.mmaaud022, 
