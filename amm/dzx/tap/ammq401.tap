  <section id="ammq401.filter" ver="1" status="" src="s">
<![CDATA[#+ filter過濾功能
PRIVATE FUNCTION ammq401_filter()
   {<Local define>}
   {</Local define>}
   #add-point:filter段define
   {<point name="filter.define"/>}
   #end add-point    
   
   LET l_ac = 1
   LET g_detail_idx = 1
   LET g_detail_idx2 = 1
 
   LET INT_FLAG = 0
 
   LET g_qryparam.state = 'c'
 
   LET g_wc_filter_t = g_wc_filter
   LET g_wc_t = g_wc
   
   CALL gfrm_curr.setFieldHidden("formonly.sel", TRUE)
   CALL gfrm_curr.setFieldHidden("formonly.statepic", TRUE)
 
   LET g_wc = cl_replace_str(g_wc, g_wc_filter, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON mmbj005,mmbj000,mmbj001,mmbj002,mmbj007,mmbj006,mmbjsite,mmbj003,mmbj004 

                          FROM s_detail1[1].b_mmbj005,s_detail1[1].b_mmbj000,s_detail1[1].b_mmbj001, 
                              s_detail1[1].b_mmbj002,s_detail1[1].b_mmbj007,s_detail1[1].b_mmbj006,s_detail1[1].b_mmbjsite, 
                              s_detail1[1].b_mmbj003,s_detail1[1].b_mmbj004
 
         BEFORE CONSTRUCT
#saki       CALL cl_qbe_init()
                     DISPLAY ammq401_filter_parser('mmbj005') TO s_detail1[1].b_mmbj005
            DISPLAY ammq401_filter_parser('mmbj000') TO s_detail1[1].b_mmbj000
            DISPLAY ammq401_filter_parser('mmbj001') TO s_detail1[1].b_mmbj001
            DISPLAY ammq401_filter_parser('mmbj002') TO s_detail1[1].b_mmbj002
            DISPLAY ammq401_filter_parser('mmbj007') TO s_detail1[1].b_mmbj007
            DISPLAY ammq401_filter_parser('mmbj006') TO s_detail1[1].b_mmbj006
            DISPLAY ammq401_filter_parser('mmbjsite') TO s_detail1[1].b_mmbjsite
            DISPLAY ammq401_filter_parser('mmbj003') TO s_detail1[1].b_mmbj003
            DISPLAY ammq401_filter_parser('mmbj004') TO s_detail1[1].b_mmbj004
 
 
      END CONSTRUCT
 
      #add-point:filter段add_cs
      {<point name="filter.add_cs"/>}
      #end add-point
 
      BEFORE DIALOG
         #add-point:filter段b_dialog
         {<point name="filter.b_dialog"/>}
         #end add-point  
 
      ON ACTION accept
         ACCEPT DIALOG 
 
      ON ACTION cancel
         LET INT_FLAG = 1
         EXIT DIALOG 
 
      #交談指令共用ACTION
      &include "common_action.4gl" 
         CONTINUE DIALOG
 
   END DIALOG
 
   IF NOT INT_FLAG THEN
      LET g_wc_filter = g_wc_filter, " "
   ELSE
      LET g_wc_filter = g_wc_filter_t
   END IF
   
      CALL ammq401_filter_show('mmbj005','b_mmbj005')
   CALL ammq401_filter_show('mmbj000','b_mmbj000')
   CALL ammq401_filter_show('mmbj001','b_mmbj001')
   CALL ammq401_filter_show('mmbj002','b_mmbj002')
   CALL ammq401_filter_show('mmbj007','b_mmbj007')
   CALL ammq401_filter_show('mmbj006','b_mmbj006')
   CALL ammq401_filter_show('mmbjsite','b_mmbjsite')
   CALL ammq401_filter_show('mmbj003','b_mmbj003')
   CALL ammq401_filter_show('mmbj004','b_mmbj004')
 
    
   CALL ammq401_b_fill()
   CALL ammq401_fetch()
   
   CALL gfrm_curr.setFieldHidden("formonly.sel", FALSE)
   CALL gfrm_curr.setFieldHidden("formonly.statepic", FALSE)
 
END FUNCTION
]]>
</section>
  <section id="ammq401.filter_parser" ver="1" status="" src="s">
<![CDATA[#+ filter欄位解析
PRIVATE FUNCTION ammq401_filter_parser(ps_field)
   {<Local define>}
   DEFINE ps_field   STRING
   DEFINE ls_tmp     STRING
   DEFINE li_tmp     LIKE type_t.num5
   DEFINE li_tmp2    LIKE type_t.num5
   DEFINE ls_var     STRING
   {</Local define>}
   #add-point:filter段define
   {<point name="filter_parser.define"/>}
   #end add-point    
 
   #一般條件解析
   LET ls_tmp = ps_field, "='"
   LET li_tmp = g_wc_filter.getIndexOf(ls_tmp,1)
   IF li_tmp > 0 THEN
      LET li_tmp = ls_tmp.getLength() + li_tmp
      LET li_tmp2 = g_wc_filter.getIndexOf("'",li_tmp + 1) - 1
      LET ls_var = g_wc_filter.subString(li_tmp,li_tmp2)
   END IF
 
   #模糊條件解析
   LET ls_tmp = ps_field, " like '"
   LET li_tmp = g_wc_filter.getIndexOf(ls_tmp,1)
   IF li_tmp > 0 THEN
      LET li_tmp = ls_tmp.getLength() + li_tmp
      LET li_tmp2 = g_wc_filter.getIndexOf("'",li_tmp + 1) - 1
      LET ls_var = g_wc_filter.subString(li_tmp,li_tmp2)
      LET ls_var = cl_replace_str(ls_var,'%','*')
   END IF
 
   RETURN ls_var
 
END FUNCTION
]]>
</section>
  <section id="ammq401.filter_show" ver="1" status="" src="s">
<![CDATA[#+ Browser標題欄位顯示搜尋條件
PRIVATE FUNCTION ammq401_filter_show(ps_field,ps_object)
   DEFINE ps_field         STRING
   DEFINE ps_object        STRING
   DEFINE lnode_item       om.DomNode
   DEFINE ls_title         STRING
   DEFINE ls_name          STRING
   DEFINE ls_condition     STRING
 
   LET ls_name = "formonly.", ps_object
 
   LET lnode_item = gfrm_curr.findNode("TableColumn", ls_name)
   LET ls_title = lnode_item.getAttribute("text")
   IF ls_title.getIndexOf('※',1) > 0 THEN
      LEt ls_title = ls_title.subString(1,ls_title.getIndexOf('※',1)-1)
   END IF
 
   #顯示資料組合
   LET ls_condition = ammq401_filter_parser(ps_field)
   IF NOT cl_null(ls_condition) THEN
      LET ls_title = ls_title, '※', ls_condition, '※'
   END IF
 
   #將資料顯示回去
   CALL lnode_item.setAttribute("text",ls_title)
 
END FUNCTION
]]>
</section>
  <section id="ammq401.global" ver="1" status="" src="s">
<![CDATA[{<point name="global.memo" />}
 
IMPORT os
IMPORT util
#add-point:增加匯入項目
{<point name="global.import" />}
#end add-point
 
SCHEMA ds
 
GLOBALS "../../cfg/top_global.inc"
 
{<Module define>}
 
#單身 type 宣告
PRIVATE TYPE type_g_mmbj_d RECORD
       #statepic       LIKE type_t.chr1,
       sel            LIKE type_t.chr1,
       mmbj005 LIKE mmbj_t.mmbj005, 
   mmbj000 LIKE mmbj_t.mmbj000, 
   mmbj001 LIKE mmbj_t.mmbj001, 
   mmbj002 LIKE mmbj_t.mmbj002, 
   mmbj002_desc LIKE type_t.chr500, 
   mmbj007 LIKE mmbj_t.mmbj007, 
   mmbj007_desc LIKE type_t.chr500, 
   mmbj006 LIKE mmbj_t.mmbj006, 
   mmbjsite LIKE mmbj_t.mmbjsite, 
   mmbjsite_desc LIKE type_t.chr500, 
   mmbj003 LIKE mmbj_t.mmbj003, 
   mmbj003_desc LIKE type_t.chr500, 
   mmbj004 LIKE mmbj_t.mmbj004 
       END RECORD
 
 
#模組變數(Module Variables)
