  <section id="ammq401.query" ver="1" status="" src="s">
<![CDATA[#+ QBE資料查詢
PRIVATE FUNCTION ammq401_query()
   {<Local define>}
   DEFINE ls_wc      LIKE type_t.chr500
   DEFINE ls_return  STRING
   DEFINE ls_result  STRING 
   {</Local define>}
   #add-point:query段define
   {<point name="query.define"/>}
   #end add-point 
   
   LET INT_FLAG = 0
   CLEAR FORM
   CALL g_mmbj_d.clear()
   LET g_wc_filter = " 1=1"
   
   CALL gfrm_curr.setFieldHidden("formonly.sel", TRUE)
   CALL gfrm_curr.setFieldHidden("formonly.statepic", TRUE)
   
   LET g_qryparam.state = "c"
   LET g_detail_idx  = 1
   LET g_detail_idx2 = 1
   
   #wc備份
   LET ls_wc = g_wc
   LET g_master_idx = l_ac
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單身根據table分拆construct
      CONSTRUCT g_wc_table ON mmbj005,mmbj000,mmbj001,mmbj002,mmbj007,mmbj006,mmbjsite,mmbj003,mmbj004 

           FROM s_detail1[1].b_mmbj005,s_detail1[1].b_mmbj000,s_detail1[1].b_mmbj001,s_detail1[1].b_mmbj002, 
               s_detail1[1].b_mmbj007,s_detail1[1].b_mmbj006,s_detail1[1].b_mmbjsite,s_detail1[1].b_mmbj003, 
               s_detail1[1].b_mmbj004
                      
         BEFORE CONSTRUCT
            #add-point:cs段more_construct
            {<point name="cs.head.before_construct"/>}
            #end add-point 
            
       #單身公用欄位開窗相關處理
       
         
       #單身一般欄位開窗相關處理
       #---------------------<  Detail: page1  >---------------------
         #----<<mmbj005>>----
         #Ctrlp:construct.c.page1.mmbj005
         ON ACTION controlp INFIELD mmbj005
            #add-point:ON ACTION controlp INFIELD mmbj005
            {<point name="construct.c.page1.mmbj005" />}
            #END add-point
 
         #----<<mmbj000>>----
         #Ctrlp:construct.c.page1.mmbj000
         ON ACTION controlp INFIELD mmbj000
            #add-point:ON ACTION controlp INFIELD mmbj000
            {<point name="construct.c.page1.mmbj000" />}
            #END add-point
 
         #----<<mmbj001>>----
         #Ctrlp:construct.c.page1.mmbj001
         ON ACTION controlp INFIELD mmbj001
            #add-point:ON ACTION controlp INFIELD mmbj001
            {<point name="construct.c.page1.mmbj001" />}
            #END add-point
 
         #----<<mmbj002>>----
         #Ctrlp:construct.c.page1.mmbj002
         ON ACTION controlp INFIELD mmbj002
            #add-point:ON ACTION controlp INFIELD mmbj002
            {<point name="construct.c.page1.mmbj002" />}
            #END add-point
 
         #----<<mmbj007>>----
         #Ctrlp:construct.c.page1.mmbj007
         ON ACTION controlp INFIELD mmbj007
            #add-point:ON ACTION controlp INFIELD mmbj007
            {<point name="construct.c.page1.mmbj007" />}
            #END add-point
 
         #----<<mmbj006>>----
         #Ctrlp:construct.c.page1.mmbj006
         ON ACTION controlp INFIELD mmbj006
            #add-point:ON ACTION controlp INFIELD mmbj006
            {<point name="construct.c.page1.mmbj006" />}
            #END add-point
 
         #----<<mmbjsite>>----
         #Ctrlp:construct.c.page1.mmbjsite
         ON ACTION controlp INFIELD mmbjsite
            #add-point:ON ACTION controlp INFIELD mmbjsite
            {<point name="construct.c.page1.mmbjsite" />}
            #END add-point
 
         #----<<mmbj003>>----
         #Ctrlp:construct.c.page1.mmbj003
         ON ACTION controlp INFIELD mmbj003
            #add-point:ON ACTION controlp INFIELD mmbj003
            {<point name="construct.c.page1.mmbj003" />}
            #END add-point
 
         #----<<mmbj004>>----
         #Ctrlp:construct.c.page1.mmbj004
         ON ACTION controlp INFIELD mmbj004
            #add-point:ON ACTION controlp INFIELD mmbj004
            {<point name="construct.c.page1.mmbj004" />}
            #END add-point
 
   
       
      END CONSTRUCT
      
 
      
 
  
      #add-point:query段more_construct
      {<point name="query.more_construct"/>}
      #end add-point 
      
      ON ACTION accept
         ACCEPT DIALOG
         
      ON ACTION cancel
         LET INT_FLAG = 1
         EXIT DIALOG
      
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE DIALOG 
   END DIALOG
 
   IF INT_FLAG THEN
      LET INT_FLAG = 0
      #還原
      LET g_wc = ls_wc
   ELSE
      LET g_master_idx = 1
   END IF
   
   LET g_wc = g_wc_table 
 
 
        
   LET g_wc2 = " 1=1"
 
        
   #add-point:cs段after_construct
   {<point name="cs.after_construct"/>}
   #end add-point
        
   LET g_error_show = 1
   CALL ammq401_b_fill()
   LET l_ac = g_master_idx
   CALL ammq401_fetch()
   IF g_detail_cnt = 0 AND NOT INT_FLAG THEN
      CALL cl_err("",-100,1)
   END IF
   
   CALL gfrm_curr.setFieldHidden("formonly.sel", FALSE)
   CALL gfrm_curr.setFieldHidden("formonly.statepic", FALSE)
   
END FUNCTION
]]>
