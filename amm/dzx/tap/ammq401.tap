  <section id="ammq401.b_fill" ver="1" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION ammq401_b_fill()
   {<Local define>}
   DEFINE ls_wc           STRING
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   LET g_sql = "SELECT  UNIQUE mmbj005,mmbj000,mmbj001,mmbj002,'',mmbj007,'',mmbj006,mmbjsite,'',mmbj003, 
       '',mmbj004 FROM mmbj_t",
 
 
               "",
               " WHERE mmbjent= ? AND 1=1 AND ", ls_wc
    
   LET g_sql = g_sql, " ORDER BY mmbj_t.mmbj000,mmbj_t.mmbj001,mmbj_t.mmbj004,mmbj_t.mmbj006"
  
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
  
   PREPARE ammq401_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR ammq401_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_mmbj_d.clear()
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_mmbj_d[l_ac].mmbj005,g_mmbj_d[l_ac].mmbj000,g_mmbj_d[l_ac].mmbj001,g_mmbj_d[l_ac].mmbj002, 
       g_mmbj_d[l_ac].mmbj002_desc,g_mmbj_d[l_ac].mmbj007,g_mmbj_d[l_ac].mmbj007_desc,g_mmbj_d[l_ac].mmbj006, 
       g_mmbj_d[l_ac].mmbjsite,g_mmbj_d[l_ac].mmbjsite_desc,g_mmbj_d[l_ac].mmbj003,g_mmbj_d[l_ac].mmbj003_desc, 
       g_mmbj_d[l_ac].mmbj004
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
      
      LET g_mmbj_d[l_ac].sel = "N"
      #LET g_mmbj_d[l_ac].statepic = cl_get_actipic(g_mmbj_d[l_ac].statepic)
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL ammq401_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            CALL cl_err( "", 9035, 1 )
         END IF
         EXIT FOREACH
      END IF
      
   END FOREACH
   LET g_error_show = 0
   
 
   
   CALL g_mmbj_d.deleteElement(g_mmbj_d.getLength())   
 
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
    
   LET g_detail_cnt = l_ac - 1 
   DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE ammq401_pb
   
   LET l_ac = 1
   CALL ammq401_fetch()
   
      CALL ammq401_filter_show('mmbj005','b_mmbj005')
   CALL ammq401_filter_show('mmbj000','b_mmbj000')
   CALL ammq401_filter_show('mmbj001','b_mmbj001')
   CALL ammq401_filter_show('mmbj002','b_mmbj002')
   CALL ammq401_filter_show('mmbj007','b_mmbj007')
   CALL ammq401_filter_show('mmbj006','b_mmbj006')
   CALL ammq401_filter_show('mmbjsite','b_mmbjsite')
   CALL ammq401_filter_show('mmbj003','b_mmbj003')
   CALL ammq401_filter_show('mmbj004','b_mmbj004')
 
   
END FUNCTION
]]>
</section>
