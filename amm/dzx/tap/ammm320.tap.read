  <point name="function.ammm320_chk_mman004" order="2" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
PUBLIC FUNCTION ammm320_chk_mman004()
   DEFINE l_stus    LIKE type_t.chr1

   LET g_errno = ''
   LET l_stus = ''
   SELECT ooeastus INTO l_stus FROM ooea_t
    WHERE ooeaent = g_enterprise
      AND ooea001 = g_mman_m.mman004
      AND ooea004 = 'Y'
   CASE WHEN SQLCA.SQLCODE = 100   LET g_errno = 'aoo-00016'
        WHEN l_stus='N'            LET g_errno = 'aoo-00012'
        OTHERWISE                  LET g_errno = SQLCA.SQLCODE USING '-------'
   END CASE
END FUNCTION]]>
  </point>
