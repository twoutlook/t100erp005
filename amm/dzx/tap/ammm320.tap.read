  <section id="ammm320.reproduce" ver="19" status="" src="s" readonly="">
    <![CDATA[#+ 資料複製
PRIVATE FUNCTION ammm320_reproduce()
   DEFINE l_newno     LIKE mman_t.mman001 
   DEFINE l_oldno     LIKE mman_t.mman001 
 
   DEFINE l_master    RECORD LIKE mman_t.*
   DEFINE l_cnt       LIKE type_t.num10
   #add-point:reproduce段define
   {<point name="reproduce.define" edit="s"/>}
   #end add-point   
   #add-point:reproduce段define(客製用)
   {<point name="reproduce.define_customerization" edit="c"/>}
   #end add-point
   
   #切換畫面
   
   #先確定key值無遺漏
   IF g_mman_m.mman001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
   
   #備份key值
   LET g_mman001_t = g_mman_m.mman001
 
   
   #清空key值
   LET g_mman_m.mman001 = ""
 
    
   CALL ammm320_set_entry("a")
   CALL ammm320_set_no_entry("a")
   
   #公用欄位給予預設值
   #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_mman_m.mmanownid = g_user
      LET g_mman_m.mmanowndp = g_dept
      LET g_mman_m.mmancrtid = g_user
      LET g_mman_m.mmancrtdp = g_dept 
      LET g_mman_m.mmancrtdt = cl_get_current()
      LET g_mman_m.mmanmodid = g_user
      LET g_mman_m.mmanmoddt = cl_get_current()
      LET g_mman_m.mmanstus = 'N'
 
 
   
   CALL s_transaction_begin()
   
   #add-point:複製輸入前
   {<point name="reproduce.head.b_input"/>}
   #end add-point
   
   #顯示狀態(stus)圖片
         #應用 a21 樣板自動產生(Version:2)
	  #根據當下狀態碼顯示圖片
      CASE g_mman_m.mmanstus 
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/open.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/valid.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/void.png")
         
      END CASE
 
 
   
   #清空key欄位的desc
   
   
   #資料輸入
   CALL ammm320_input("r")
   
   IF INT_FLAG THEN
      #取消
      LET INT_FLAG = 0
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = 9001 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      INITIALIZE g_mman_m.* TO NULL
      CALL ammm320_show()
      RETURN
   END IF
   
   CALL s_transaction_begin()
   
   #add-point:單頭複製前
   {<point name="reproduce.head.b_insert" mark="Y"/>}
   #end add-point
   
   #add-point:單頭複製中
   {<point name="reproduce.head.m_insert"/>}
   #end add-point
   
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "mman_t" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:單頭複製後
   {<point name="reproduce.head.a_insert"/>}
   #end add-point
   
   CALL s_transaction_end('Y','0')
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL ammm320_set_act_visible()
   CALL ammm320_set_act_no_visible()
 
   #將新增的資料併入搜尋條件中
   LET g_state = "insert"
   
   LET g_mman001_t = g_mman_m.mman001
 
   
   #組合新增資料的條件
   LET g_add_browse = " mmanent = '" ||g_enterprise|| "' AND",
                      " mman001 = '", g_mman_m.mman001, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL ammm320_browser_fill("","")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   #add-point:完成複製段落後
   {<point name="reproduce.after_reproduce" />}
   #end add-point
                 
   #功能已完成,通報訊息中心
   CALL ammm320_msgcentre_notify('')
                 
END FUNCTION
]]>
