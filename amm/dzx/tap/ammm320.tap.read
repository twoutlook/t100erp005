  <section id="ammm320.modify" ver="21" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION ammm320_modify()
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point
   #add-point:modify段define(客製用)
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_mman_m.mman001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_mman001_t = g_mman_m.mman001
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN ammm320_cl USING g_enterprise,g_mman_m.mman001
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN ammm320_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE ammm320_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE ammm320_master_referesh USING g_mman_m.mman001 INTO g_mman_m.mman001,g_mman_m.mman002,g_mman_m.mman066, 
       g_mman_m.mmanunit,g_mman_m.mman003,g_mman_m.mman004,g_mman_m.mmanstus,g_mman_m.mmanownid,g_mman_m.mmanowndp, 
       g_mman_m.mmancrtid,g_mman_m.mmancrtdp,g_mman_m.mmancrtdt,g_mman_m.mmanmodid,g_mman_m.mmanmoddt, 
       g_mman_m.mmancnfid,g_mman_m.mmancnfdt,g_mman_m.mman005,g_mman_m.mman006,g_mman_m.mman007,g_mman_m.mman008, 
       g_mman_m.mman009,g_mman_m.mman010,g_mman_m.mman011,g_mman_m.mman012,g_mman_m.mman013,g_mman_m.mman014, 
       g_mman_m.mman015,g_mman_m.mman069,g_mman_m.mman016,g_mman_m.mman017,g_mman_m.mman018,g_mman_m.mman019, 
       g_mman_m.mman020,g_mman_m.mman021,g_mman_m.mman022,g_mman_m.mman023,g_mman_m.mman024,g_mman_m.mman025, 
       g_mman_m.mman062,g_mman_m.mman063,g_mman_m.mman026,g_mman_m.mman064,g_mman_m.mman053,g_mman_m.mman060, 
       g_mman_m.mman054,g_mman_m.mman061,g_mman_m.mman058,g_mman_m.mman057,g_mman_m.mman059,g_mman_m.mman027, 
       g_mman_m.mman028,g_mman_m.mman029,g_mman_m.mman030,g_mman_m.mman067,g_mman_m.mman068,g_mman_m.mman036, 
       g_mman_m.mman037,g_mman_m.mman031,g_mman_m.mman032,g_mman_m.mman033,g_mman_m.mman034,g_mman_m.mman035, 
       g_mman_m.mman038,g_mman_m.mman039,g_mman_m.mman040,g_mman_m.mman041,g_mman_m.mman055,g_mman_m.mman056, 
       g_mman_m.mman042,g_mman_m.mman043,g_mman_m.mman065,g_mman_m.mman044,g_mman_m.mman045,g_mman_m.mman046, 
       g_mman_m.mman047,g_mman_m.mman048,g_mman_m.mman049,g_mman_m.mman050,g_mman_m.mman051,g_mman_m.mman052, 
       g_mman_m.mmanunit_desc,g_mman_m.mman004_desc,g_mman_m.mmanownid_desc,g_mman_m.mmanowndp_desc, 
       g_mman_m.mmancrtid_desc,g_mman_m.mmancrtdp_desc,g_mman_m.mmanmodid_desc,g_mman_m.mmancnfid_desc, 
       g_mman_m.mman012_desc,g_mman_m.mman053_desc,g_mman_m.mman060_desc,g_mman_m.mman054_desc,g_mman_m.mman058_desc, 
       g_mman_m.mman057_desc
 
   #檢查是否允許此動作
   IF NOT ammm320_action_chk() THEN
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_mman_m_mask_o.* =  g_mman_m.*
   CALL ammm320_mman_t_mask()
   LET g_mman_m_mask_n.* =  g_mman_m.*
   
   
 
   #顯示資料
   CALL ammm320_show()
   
   WHILE TRUE
      LET g_mman_m.mman001 = g_mman001_t
 
      
      #寫入修改者/修改日期資訊
      LET g_mman_m.mmanmodid = g_user 
LET g_mman_m.mmanmoddt = cl_get_current()
LET g_mman_m.mmanmodid_desc = cl_get_username(g_mman_m.mmanmodid)
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
 
      #資料輸入
      CALL ammm320_input("u")     
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_mman_m.* = g_mman_m_t.*
         CALL ammm320_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE mman_t SET (mmanmodid,mmanmoddt) = (g_mman_m.mmanmodid,g_mman_m.mmanmoddt)
       WHERE mmanent = g_enterprise AND mman001 = g_mman001_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL ammm320_set_act_visible()
   CALL ammm320_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " mmanent = '" ||g_enterprise|| "' AND",
                      " mman001 = '", g_mman_m.mman001, "' "
 
   #填到對應位置
   CALL ammm320_browser_fill(g_wc,"")
 
   CLOSE ammm320_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL ammm320_msgcentre_notify('')
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
