            #add-point:page1自定義行為
            {<point name="ui_dialog.page1.action"/>}
            #end add-point
               
         END DISPLAY
        
         DISPLAY ARRAY g_mmco2_d TO s_detail2.* ATTRIBUTES(COUNT=g_rec_b)  
    
