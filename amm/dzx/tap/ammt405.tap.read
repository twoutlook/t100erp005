               "    START WITH ooed004= '",g_rtia_m.rtiasite,"' AND ooed001 = '8' AND ooedent = '",g_enterprise,"' ",
               "      AND ooed006 <='",g_today,"' ",
               "      AND (ooed007 >='",g_today,"' OR ooed007 IS NULL)",
