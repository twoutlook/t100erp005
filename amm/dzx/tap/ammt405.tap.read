# Date & Author..: 2014/05/26 By yangxf
# Modify.........:
################################################################################
PRIVATE FUNCTION ammt405_card(p_card)
   DEFINE l_sql   STRING 
   DEFINE p_card  LIKE mmaq_t.mmaq001
   DEFINE r_no    LIKE type_t.num5
   LET r_no = 0
