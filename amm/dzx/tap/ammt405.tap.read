               "   FROM mmaq_t ",
               "  WHERE mmaqent = '",g_enterprise,"'",
               "    AND mmaq001 = '",p_card,"'",
               "    AND mmaq002 IN (SELECT mmap002 FROM mmap_t WHERE mmapent = '",g_enterprise,"' AND mmap001 = 'ammm320' ",
