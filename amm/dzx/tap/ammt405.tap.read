                               AND b.mmeaent = g_enterprise
                               AND a.mmeaent = b.mmeaent
                               AND a.mmeaseq = b.mmeaseq)
      END IF 
   END IF
   IF l_n > 0 THEN 
      LET g_errno = 'amm-00331'
   END IF

END FUNCTION]]>
