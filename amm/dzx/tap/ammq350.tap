  <section id="ammq350.b_fill" ver="11" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION ammq350_b_fill()
   {<Local define>}
   DEFINE ls_wc           STRING
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   LET g_sql = "SELECT  UNIQUE mmau001,mmau002,'',mmau003,mmau004,mmau005,mmau018,'',mmau006,mmau007, 
       mmau008,mmau009,mmau010,mmau011,mmau012,mmau013,mmau014,mmau015,mmau016,mmau017,mmau019,mmauseq FROM mmau_t", 

 
 
               "",
               " WHERE mmauent= ? AND 1=1 AND ", ls_wc
    
   LET g_sql = g_sql, " ORDER BY mmau_t.mmau001,mmau_t.mmau004,mmau_t.mmau005,mmau_t.mmauseq"
  
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
  
   PREPARE ammq350_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR ammq350_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_mmau_d.clear()
 
   #add-point:陣列清空
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   LET l_ac = 1   
#  ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_mmau_d[l_ac].mmau001,g_mmau_d[l_ac].mmau002,g_mmau_d[l_ac].mmau002_desc, 
       g_mmau_d[l_ac].mmau003,g_mmau_d[l_ac].mmau004,g_mmau_d[l_ac].mmau005,g_mmau_d[l_ac].mmau018,g_mmau_d[l_ac].mmau018_desc, 
       g_mmau_d[l_ac].mmau006,g_mmau_d[l_ac].mmau007,g_mmau_d[l_ac].mmau008,g_mmau_d[l_ac].mmau009,g_mmau_d[l_ac].mmau010, 
       g_mmau_d[l_ac].mmau011,g_mmau_d[l_ac].mmau012,g_mmau_d[l_ac].mmau013,g_mmau_d[l_ac].mmau014,g_mmau_d[l_ac].mmau015, 
       g_mmau_d[l_ac].mmau016,g_mmau_d[l_ac].mmau017,g_mmau_d[l_ac].mmau019,g_mmau_d[l_ac].mmauseq
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
      
      LET g_mmau_d[l_ac].sel = "N"
      #LET g_mmau_d[l_ac].statepic = cl_get_actipic(g_mmau_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL ammq350_detail_show("'1'")      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            CALL cl_err( "", 9035, 1 )
         END IF
         EXIT FOREACH
      END IF
      
   END FOREACH
   LET g_error_show = 0
   
   CALL g_mmau_d.deleteElement(g_mmau_d.getLength())   
   #add-point:陣列長度調整
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
    
   LET g_detail_cnt = l_ac - 1 
   DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET g_detail_idx = 1
   DISPLAY g_detail_idx TO FORMONLY.h_index
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE ammq350_pb
   
   LET l_ac = 1
   CALL ammq350_fetch()
   
      CALL ammq350_filter_show('mmau001','b_mmau001')
   CALL ammq350_filter_show('mmau002','b_mmau002')
   CALL ammq350_filter_show('mmau003','b_mmau003')
   CALL ammq350_filter_show('mmau004','b_mmau004')
   CALL ammq350_filter_show('mmau005','b_mmau005')
   CALL ammq350_filter_show('mmau018','b_mmau018')
   CALL ammq350_filter_show('mmau006','b_mmau006')
   CALL ammq350_filter_show('mmau007','b_mmau007')
   CALL ammq350_filter_show('mmau008','b_mmau008')
   CALL ammq350_filter_show('mmau009','b_mmau009')
   CALL ammq350_filter_show('mmau010','b_mmau010')
   CALL ammq350_filter_show('mmau011','b_mmau011')
   CALL ammq350_filter_show('mmau012','b_mmau012')
   CALL ammq350_filter_show('mmau013','b_mmau013')
   CALL ammq350_filter_show('mmau014','b_mmau014')
   CALL ammq350_filter_show('mmau015','b_mmau015')
   CALL ammq350_filter_show('mmau016','b_mmau016')
   CALL ammq350_filter_show('mmau017','b_mmau017')
   CALL ammq350_filter_show('mmau019','b_mmau019')
   CALL ammq350_filter_show('mmauseq','b_mmauseq')
 
   
END FUNCTION
]]>
  </section>
