  <section id="ammq302.b_fill" ver="2" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION ammq302_b_fill()
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準
   {<point name="b_fill.define" edit="s"/>}
   #end add-point
   #add-point:b_fill段define-客製
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   LET ls_sql_rank = "SELECT  UNIQUE mmaq001,mmaq002,'',mmaq003,'',mmaq004,mmaq005,mmaq006,mmaq007,mmaq008, 
       mmaq009,mmaq010,mmaq011,mmaq012,mmaq013,mmaq014,mmaq015,mmaq016,mmaq017,mmaq018,mmaq019,mmaq032, 
       mmaq040,mmaq041,mmaq042,mmaq100,'',mmaq020,'',mmaq021,mmaq022,'',mmaq023,mmaq024,'',mmaq025,mmaq026, 
       '',mmaq027,mmaq028,'',mmaq029,mmaq030,'',mmaq031,mmaq033,'',mmaq034,'',mmaq035,'',mmaq036,'', 
       mmaq037,'',mmaq038,''  ,DENSE_RANK() OVER( ORDER BY mmaq_t.mmaq001) AS RANK FROM mmaq_t",
 
 
                     "",
                     " WHERE mmaqent= ? AND 1=1 AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("mmaq_t"),
                     " ORDER BY mmaq_t.mmaq001"
 
   #add-point:b_fill段rank_sql_after
   {<point name="b_fill.rank_sql_after"/>}
   #end add-point
 
   LET g_sql = "SELECT COUNT(*) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT mmaq001,mmaq002,'',mmaq003,'',mmaq004,mmaq005,mmaq006,mmaq007,mmaq008,mmaq009, 
       mmaq010,mmaq011,mmaq012,mmaq013,mmaq014,mmaq015,mmaq016,mmaq017,mmaq018,mmaq019,mmaq032,mmaq040, 
       mmaq041,mmaq042,mmaq100,'',mmaq020,'',mmaq021,mmaq022,'',mmaq023,mmaq024,'',mmaq025,mmaq026,'', 
       mmaq027,mmaq028,'',mmaq029,mmaq030,'',mmaq031,mmaq033,'',mmaq034,'',mmaq035,'',mmaq036,'',mmaq037, 
       '',mmaq038,''",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE ammq302_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR ammq302_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_mmaq_d.clear()
   CALL g_mmaq2_d.clear()   
 
 
   #add-point:陣列清空
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1   
 
   FOREACH b_fill_curs INTO g_mmaq_d[l_ac].mmaq001,g_mmaq_d[l_ac].mmaq002,g_mmaq_d[l_ac].mmaq002_desc, 
       g_mmaq_d[l_ac].mmaq003,g_mmaq_d[l_ac].mmaq003_desc,g_mmaq_d[l_ac].mmaq004,g_mmaq_d[l_ac].mmaq005, 
       g_mmaq_d[l_ac].mmaq006,g_mmaq_d[l_ac].mmaq007,g_mmaq_d[l_ac].mmaq008,g_mmaq_d[l_ac].mmaq009,g_mmaq_d[l_ac].mmaq010, 
       g_mmaq_d[l_ac].mmaq011,g_mmaq_d[l_ac].mmaq012,g_mmaq_d[l_ac].mmaq013,g_mmaq_d[l_ac].mmaq014,g_mmaq_d[l_ac].mmaq015, 
       g_mmaq_d[l_ac].mmaq016,g_mmaq_d[l_ac].mmaq017,g_mmaq_d[l_ac].mmaq018,g_mmaq_d[l_ac].mmaq019,g_mmaq_d[l_ac].mmaq032, 
       g_mmaq_d[l_ac].mmaq040,g_mmaq_d[l_ac].mmaq041,g_mmaq_d[l_ac].mmaq042,g_mmaq_d[l_ac].mmaq100,g_mmaq2_d[l_ac].mmaq001, 
       g_mmaq2_d[l_ac].mmaq020,g_mmaq2_d[l_ac].mmaq020_desc,g_mmaq2_d[l_ac].mmaq021,g_mmaq2_d[l_ac].mmaq022, 
       g_mmaq2_d[l_ac].mmaq022_desc,g_mmaq2_d[l_ac].mmaq023,g_mmaq2_d[l_ac].mmaq024,g_mmaq2_d[l_ac].mmaq024_desc, 
       g_mmaq2_d[l_ac].mmaq025,g_mmaq2_d[l_ac].mmaq026,g_mmaq2_d[l_ac].mmaq026_desc,g_mmaq2_d[l_ac].mmaq027, 
       g_mmaq2_d[l_ac].mmaq028,g_mmaq2_d[l_ac].mmaq028_desc,g_mmaq2_d[l_ac].mmaq029,g_mmaq2_d[l_ac].mmaq030, 
       g_mmaq2_d[l_ac].mmaq030_desc,g_mmaq2_d[l_ac].mmaq031,g_mmaq2_d[l_ac].mmaq033,g_mmaq2_d[l_ac].mmaq033_desc, 
       g_mmaq2_d[l_ac].mmaq034,g_mmaq2_d[l_ac].mmaq034_desc,g_mmaq2_d[l_ac].mmaq035,g_mmaq2_d[l_ac].mmaq035_desc, 
       g_mmaq2_d[l_ac].mmaq036,g_mmaq2_d[l_ac].mmaq036_desc,g_mmaq2_d[l_ac].mmaq037,g_mmaq2_d[l_ac].mmaq037_desc, 
       g_mmaq2_d[l_ac].mmaq038,g_mmaq2_d[l_ac].mmaq038_desc
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #LET g_mmaq_d[l_ac].statepic = cl_get_actipic(g_mmaq_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL ammq302_detail_show("'1'")      
 
      CALL ammq302_mmaq_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
   LET g_error_show = 0
   
 
   
   CALL g_mmaq_d.deleteElement(g_mmaq_d.getLength())   
   CALL g_mmaq2_d.deleteElement(g_mmaq2_d.getLength())
 
 
   #add-point:陣列長度調整
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = g_mmaq_d.getLength()
#  DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE ammq302_pb
 
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL ammq302_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL ammq302_detail_action_trans()
 
   IF g_mmaq_d.getLength() > 0 THEN
      LET l_ac = 1
      CALL ammq302_fetch()
   END IF
   
      CALL ammq302_filter_show('mmaq001','b_mmaq001')
