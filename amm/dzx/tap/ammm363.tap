  <point name="function.ammm363_idx_chk" cite_std="N" status="" ver="1" src="s" new="Y" order="1" mark_hard="N">
<![CDATA[
################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL ammm363_idx_chk()
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION ammm363_idx_chk()
   IF g_current_page = 1 THEN
      LET g_detail_idx = g_curr_diag.getCurrentRow("s_detail1")
      IF g_detail_idx > g_mmco_d.getLength() THEN
         LET g_detail_idx = g_mmco_d.getLength()
      END IF
      IF g_detail_idx = 0 AND g_mmco_d.getLength() <> 0 THEN
         LET g_detail_idx = 1
      END IF
      DISPLAY g_detail_idx TO FORMONLY.idx
      DISPLAY g_mmco_d.getLength() TO FORMONLY.cnt
   END IF
   
   IF g_current_page = 2 THEN
      LET g_detail_idx = g_curr_diag.getCurrentRow("s_detail2")
      IF g_detail_idx > g_mmco2_d.getLength() THEN
         LET g_detail_idx = g_mmco2_d.getLength()
      END IF
      IF g_detail_idx = 0 AND g_mmco2_d.getLength() <> 0 THEN
         LET g_detail_idx = 1
      END IF
      DISPLAY g_detail_idx TO FORMONLY.idx
      DISPLAY g_mmco2_d.getLength() TO FORMONLY.cnt
   END IF

   IF g_current_page = 3 THEN
      LET g_detail_idx = g_curr_diag.getCurrentRow("s_detail3")
      IF g_detail_idx > g_mmco3_d.getLength() THEN
         LET g_detail_idx = g_mmco3_d.getLength()
      END IF
      IF g_detail_idx = 0 AND g_mmco3_d.getLength() <> 0 THEN
         LET g_detail_idx = 1
      END IF
      DISPLAY g_detail_idx TO FORMONLY.idx
      DISPLAY g_mmco3_d.getLength() TO FORMONLY.cnt
   END IF
END FUNCTION]]>
</point>
  <point name="function.ammm363_b_fill" cite_std="N" status="" ver="1" src="s" new="Y" order="2" mark_hard="N">
<![CDATA[
################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL ammm363_b_fill()
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION ammm363_b_fill()
   DEFINE p_wc2      STRING
 
   CALL g_mmco_d.clear()
   CALL g_mmco2_d.clear()
   CALL g_mmco3_d.clear()
   
   LET g_sql = "SELECT UNIQUE mmco004,'',mmco005,mmcoacti ",
               "  FROM mmco_t,mmbo_t,mmby_t ",
               " WHERE mmbyent = mmboent AND mmby001 = mmbo001 AND mmby002 = mmbo002 ",
               "   AND mmboent = mmcoent AND mmbodocno = mmcodocno ",
               "   AND mmbyent = '",g_enterprise,"' AND mmby001 = '",g_mmby_m.mmby001,"' AND mmby002 = '",g_mmby_m.mmby002,"' "
      
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
   END IF 
   
   LET g_sql = g_sql, " ORDER BY mmco004"
   
   PREPARE ammm363_pb FROM g_sql
   DECLARE b_fill_cs CURSOR FOR ammm363_pb
   LET g_cnt = l_ac
   LET l_ac = 1
    
   FOREACH b_fill_cs INTO g_mmco_d[l_ac].mmco004,g_mmco_d[l_ac].mmco004_desc,g_mmco_d[l_ac].mmco005,g_mmco_d[l_ac].mmcoacti
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
      
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec AND g_error_show = 1 THEN
         CALL cl_err( '', 9035, 1)
         EXIT FOREACH
      END IF
   END FOREACH
   
   LET g_sql = "SELECT UNIQUE mmbs004,'',mmbs005,mmbsacti ",
               "  FROM mmbs_t,mmby_t,mmbo_t ",
               " WHERE mmbyent = mmboent AND mmby001 = mmbo001 AND mmby002 = mmbo002 ",
               "   AND mmboent = mmbsent AND mmbodocno = mmbsdocno " ,
               "   AND mmbyent = '",g_enterprise,"' AND mmby001 = '",g_mmby_m.mmby001,"' AND mmby002 = '",g_mmby_m.mmby002,"' "
   
   IF NOT cl_null(g_wc2_table2) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
   END IF
   
   LET g_sql = g_sql, " ORDER BY mmbs004"
   
   PREPARE ammm363_pb2 FROM g_sql
   DECLARE b_fill_cs2 CURSOR FOR ammm363_pb2
   LET l_ac = 1
   
   FOREACH b_fill_cs2 INTO g_mmco2_d[l_ac].mmbs004,g_mmco2_d[l_ac].mmbs004_desc,g_mmco2_d[l_ac].mmbs005,g_mmco2_d[l_ac].mmbsacti
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
   
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         CALL cl_err( '', 9035, 0 )
         EXIT FOREACH
      END IF
   END FOREACH

   LET g_sql = "SELECT UNIQUE mmcp003,mmcp004,'',mmcp005,mmcp006,mmcp007,mmcp008,mmcp009,mmcp010,mmcp011,mmcp012,mmcp013,mmcp014,mmcp015,mmcp016,mmcpacti ",
               "  FROM mmcp_t,mmby_t,mmbo_t ",
               " WHERE mmbyent = mmboent AND mmby001 = mmbo001 AND mmby002 = mmbo002 ",
               "   AND mmboent = mmcpent AND mmbodocno = mmcpdocno ",
               "   AND mmbyent = '",g_enterprise,"' AND mmby001 = '",g_mmby_m.mmby001,"' AND mmby002 = '",g_mmby_m.mmby002,"' "
   
   IF NOT cl_null(g_wc2_table3) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table3 CLIPPED
   END IF
   
   LET g_sql = g_sql, " ORDER BY mmcp003,mmcp004"
   
   PREPARE ammm363_pb3 FROM g_sql
   DECLARE b_fill_cs3 CURSOR FOR ammm363_pb3
   LET l_ac = 1
   
   FOREACH b_fill_cs3 INTO g_mmco3_d[l_ac].mmcp003,g_mmco3_d[l_ac].mmcp004,g_mmco3_d[l_ac].mmcp004_desc,g_mmco3_d[l_ac].mmcp005,g_mmco3_d[l_ac].mmcp006,g_mmco3_d[l_ac].mmcp007,g_mmco3_d[l_ac].mmcp008,g_mmco3_d[l_ac].mmcp009,g_mmco3_d[l_ac].mmcp010,g_mmco3_d[l_ac].mmcp011,g_mmco3_d[l_ac].mmcp012,g_mmco3_d[l_ac].mmcp013,g_mmco3_d[l_ac].mmcp014,g_mmco3_d[l_ac].mmcp015,g_mmco3_d[l_ac].mmcp016,g_mmco3_d[l_ac].mmcpacti
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
   
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         CALL cl_err( '', 9035, 0 )
         EXIT FOREACH
      END IF
   END FOREACH

   CALL g_mmco_d.deleteElement(g_mmco_d.getLength())
   CALL g_mmco2_d.deleteElement(g_mmco2_d.getLength())
   CALL g_mmco3_d.deleteElement(g_mmco3_d.getLength())
   
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE ammm363_pb
   FREE ammm363_pb2
   FREE ammm363_pb3

END FUNCTION]]>
</point>
  <point name="function.ammm363_set_combo" cite_std="N" status="" ver="1" src="s" new="Y" order="3" mark_hard="N">
<![CDATA[
################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL ammm363_set_combo()
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION ammm363_set_combo()
DEFINE l_values   STRING
DEFINE l_items    STRING
DEFINE l_gzcb002  LIKE gzcb_t.gzcb002
DEFINE l_gzcbl004 LIKE gzcbl_t.gzcbl004

   LET l_values = NULL
   LET l_items = NULL
   LET l_gzcb002 = NULL
   LET l_gzcbl004 = NULL
   DECLARE m363_gzcb_cs1 CURSOR FOR
    SELECT gzcb002,gzcbl004 FROM gzcb_t,gzcbl_t
     WHERE gzcb001 = gzcbl001 AND gzcb002 = gzcbl002
       AND gzcb001 = '6517'   AND gzcbl003 = g_dlang
       AND gzcb002 IN ('0','1','2')
     ORDER BY gzcb001
   FOREACH m363_gzcb_cs1 INTO l_gzcb002,l_gzcbl004
      LET l_values = l_values CLIPPED ,",",l_gzcb002
      LET l_items  = l_items CLIPPED ,",",l_gzcb002 CLIPPED,':',l_gzcbl004
   END FOREACH
   CALL cl_set_combo_items("b_mmby007",l_values,l_items)
   CALL cl_set_combo_items("b_mmby008",l_values,l_items)
   CALL cl_set_combo_items("mmby007",l_values,l_items)
   CALL cl_set_combo_items("mmby008",l_values,l_items)
   
   LET l_values = NULL
   LET l_items = NULL
   LET l_gzcb002 = NULL
   LET l_gzcbl004 = NULL
   DECLARE m363_gzcb_cs2 CURSOR FOR
    SELECT gzcb002,gzcbl004 FROM gzcb_t,gzcbl_t
     WHERE gzcb001 = gzcbl001 AND gzcb002 = gzcbl002
       AND gzcb001 = '6517'   AND gzcbl003 = g_dlang
       AND gzcb002 IN ('1','2','Q','R','S')
     ORDER BY gzcb001
   FOREACH m363_gzcb_cs2 INTO l_gzcb002,l_gzcbl004
      LET l_values = l_values CLIPPED ,",",l_gzcb002
      LET l_items  = l_items CLIPPED ,",",l_gzcb002 CLIPPED,':',l_gzcbl004
   END FOREACH
   CALL cl_set_combo_items("mmcp003",l_values,l_items)
   
   LET l_values = NULL
   LET l_items = NULL
   LET l_gzcb002 = NULL
   LET l_gzcbl004 = NULL
   DECLARE m363_gzcb_cs3 CURSOR FOR
    SELECT gzcb002,gzcbl004 FROM gzcb_t,gzcbl_t
     WHERE gzcb001 = gzcbl001 AND gzcb002 = gzcbl002
       AND gzcb001 = '6520'   AND gzcbl003 = g_dlang
     ORDER BY gzcb001
   FOREACH m363_gzcb_cs3 INTO l_gzcb002,l_gzcbl004
      LET l_values = l_values CLIPPED ,",",l_gzcb002
      LET l_items  = l_items CLIPPED ,",",l_gzcb002 CLIPPED,':',l_gzcbl004
   END FOREACH
   CALL cl_set_combo_items("mmcp015_1",l_values,l_items)
   
   LET l_values = NULL
   LET l_items = NULL
   LET l_gzcb002 = NULL
   LET l_gzcbl004 = NULL
   DECLARE m363_gzcb_cs4 CURSOR FOR
    SELECT gzcb002,gzcbl004 FROM gzcb_t,gzcbl_t
     WHERE gzcb001 = gzcbl001 AND gzcb002 = gzcbl002
       AND gzcb001 = '30'   AND gzcbl003 = g_dlang
     ORDER BY gzcb001
   FOREACH m363_gzcb_cs4 INTO l_gzcb002,l_gzcbl004
      LET l_values = l_values CLIPPED ,",",l_gzcb002
      LET l_items  = l_items CLIPPED ,",",l_gzcb002 CLIPPED,':',l_gzcbl004
   END FOREACH
   CALL cl_set_combo_items("mmcp016_1",l_values,l_items)
   
END FUNCTION]]>
</point>
  <point name="function.ammm363_show_desc" cite_std="N" status="" ver="1" src="s" new="Y" order="4" mark_hard="N">
<![CDATA[
################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL ammm363_show_desc(p_ac)
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION ammm363_show_desc(p_ac)
DEFINE p_ac   LIKE type_t.num10

   IF cl_null(p_ac) OR p_ac <= 0 THEN LET p_ac = 1 END IF
   
   LET g_mmby_m.mmcp004_1      = g_mmco3_d[p_ac].mmcp004
   LET g_mmby_m.mmcp004_1_desc = g_mmco3_d[p_ac].mmcp004_desc
   LET g_mmby_m.mmcp011_1      = g_mmco3_d[p_ac].mmcp011
   LET g_mmby_m.mmcp012_1      = g_mmco3_d[p_ac].mmcp012
   LET g_mmby_m.mmcp013_1      = g_mmco3_d[p_ac].mmcp013
   LET g_mmby_m.mmcp014_1      = g_mmco3_d[p_ac].mmcp014
   LET g_mmby_m.mmcp015_1      = g_mmco3_d[p_ac].mmcp015
   LET g_mmby_m.mmcp016_1      = g_mmco3_d[p_ac].mmcp016
   
   DISPLAY BY NAME g_mmby_m.mmcp004_1,g_mmby_m.mmcp004_1_desc,g_mmby_m.mmcp011_1,g_mmby_m.mmcp012_1,
                   g_mmby_m.mmcp013_1,g_mmby_m.mmcp014_1,g_mmby_m.mmcp015_1,g_mmby_m.mmcp016_1
END FUNCTION]]>
</point>
  <point name="function.ammm363_mmco004_ref" cite_std="N" status="" ver="2" src="s" new="Y" order="5" mark_hard="N">
<![CDATA[
################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL ammm363_mmco004_ref()
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION ammm363_mmco004_ref()
   CASE g_mmby_m.mmby007
      WHEN '1'
         SELECT oocql004 INTO g_mmco_d[l_ac].mmco004_desc
           FROM oocql_t
          WHERE oocqlent = g_enterprise AND oocql001 = '2024' AND oocql002 = g_mmco_d[l_ac].mmco004 AND oocql003 = g_dlang
      WHEN '2'
         SELECT oocql004 INTO g_mmco_d[l_ac].mmco004_desc
           FROM oocql_t
          WHERE oocqlent = g_enterprise AND oocql001 = '2025' AND oocql002 = g_mmco_d[l_ac].mmco004 AND oocql003 = g_dlang
      WHEN  '3' #款別類型
          SELECT ooial003 INTO g_mmco_d[l_ac].mmco004_desc FROM ooial_t
           WHERE ooialent = g_enterprise AND ooial001 = g_mmco_d[l_ac].mmco004 AND ooial002 = g_dlang
       
       WHEN '4' #商品編號
          SELECT imaal003 INTO g_mmco_d[l_ac].mmco004_desc FROM imaal_t
           WHERE imaalent = g_enterprise AND imaal001 = g_mmco_d[l_ac].mmco004 AND imaal002 = g_dlang
           
       WHEN '5' #商品分類
           SELECT rtaxl003 INTO g_mmco_d[l_ac].mmco004_desc FROM rtaxl_t
            WHERE rtaxlent = g_enterprise AND rtaxl001 = g_mmco_d[l_ac].mmco004 AND rtaxl002 = g_dlang 

       WHEN '6' #商品屬性-產地分類
           SELECT oocql004 INTO g_mmco_d[l_ac].mmco004_desc FROM oocql_t 
           WHERE oocqlent = g_enterprise AND oocql001 = '2000' AND oocql002 = g_mmco_d[l_ac].mmco004 AND oocql003 = g_dlang
           
       WHEN '7' #商品屬性-價格帶
          SELECT oocql004 INTO g_mmco_d[l_ac].mmco004_desc FROM oocql_t 
           WHERE oocqlent = g_enterprise AND oocql001 = '2001' AND oocql002 = g_mmco_d[l_ac].mmco004 AND oocql003 = g_dlang
           
       WHEN '8' #商品屬性-品牌
          SELECT oocql004 INTO g_mmco_d[l_ac].mmco004_desc FROM oocql_t 
           WHERE oocqlent = g_enterprise AND oocql001 = '2002' AND oocql002 = g_mmco_d[l_ac].mmco004 AND oocql003 = g_dlang  

       WHEN '9' #商品屬性-系列
           SELECT oocql004 INTO g_mmco_d[l_ac].mmco004_desc FROM oocql_t 
           WHERE oocqlent = g_enterprise AND oocql001 = '2003' AND oocql002 = g_mmco_d[l_ac].mmco004 AND oocql003 = g_dlang   

       WHEN 'A' #商品屬性-型別
          SELECT oocql004 INTO g_mmco_d[l_ac].mmco004_desc FROM oocql_t 
           WHERE oocqlent = g_enterprise AND oocql001 = '2004' AND oocql002 = g_mmco_d[l_ac].mmco004 AND oocql003 = g_dlang 

       WHEN 'B' #商品屬性-功能
          SELECT oocql004 INTO g_mmco_d[l_ac].mmco004_desc FROM oocql_t 
           WHERE oocqlent = g_enterprise AND oocql001 = '2005' AND oocql002 = g_mmco_d[l_ac].mmco004 AND oocql003 = g_dlang 
       
       WHEN 'C' #商品屬性-其它屬性一
         SELECT oocql004 INTO g_mmco_d[l_ac].mmco004_desc FROM oocql_t 
           WHERE oocqlent = g_enterprise AND oocql001 = '2006' AND oocql002 = g_mmco_d[l_ac].mmco004 AND oocql003 = g_dlang  

       WHEN 'D' #商品屬性-其它屬性二
          SELECT oocql004 INTO g_mmco_d[l_ac].mmco004 FROM oocql_t 
           WHERE oocqlent = g_enterprise AND oocql001 = '2007' AND oocql002 = g_mmco_d[l_ac].mmco004 AND oocql003 = g_dlang  
   
       WHEN 'E' #商品屬性-其它屬性三
          SELECT oocql004 INTO g_mmco_d[l_ac].mmco004_desc FROM oocql_t 
           WHERE oocqlent = g_enterprise AND oocql001 = '2008' AND oocql002 = g_mmco_d[l_ac].mmco004 AND oocql003 = g_dlang  
       WHEN 'F' #商品屬性-其它屬性四
          SELECT oocql004 INTO g_mmco_d[l_ac].mmco004_desc FROM oocql_t 
           WHERE oocqlent = g_enterprise AND oocql001 = '2009' AND oocql002 = g_mmco_d[l_ac].mmco004 AND oocql003 = g_dlang  
       WHEN 'G' #商品屬性-其它屬性五
          SELECT oocql004 INTO g_mmco_d[l_ac].mmco004_desc FROM oocql_t 
           WHERE oocqlent = g_enterprise AND oocql001 = '2010' AND oocql002 = g_mmco_d[l_ac].mmco004 AND oocql003 = g_dlang  
        
       WHEN 'H' #商品屬性-其它屬性六
          SELECT oocql004 INTO g_mmco_d[l_ac].mmco004_desc FROM oocql_t 
           WHERE oocqlent = g_enterprise AND oocql001 = '2011' AND oocql002 = g_mmco_d[l_ac].mmco004 AND oocql003 = g_dlang  
           
       WHEN 'I' #商品屬性-其它屬性七
          SELECT oocql004 INTO g_mmco_d[l_ac].mmco004_desc FROM oocql_t 
           WHERE oocqlent = g_enterprise AND oocql001 = '2012' AND oocql002 = g_mmco_d[l_ac].mmco004 AND oocql003 = g_dlang   
       WHEN 'J' #商品屬性-其它屬性八
           SELECT oocql004 INTO g_mmco_d[l_ac].mmco004_desc FROM oocql_t 
           WHERE oocqlent = g_enterprise AND oocql001 = '2013' AND oocql002 = g_mmco_d[l_ac].mmco004 AND oocql003 = g_dlang           
      
       WHEN 'K' #商品屬性-其它屬性九
           SELECT oocql004 INTO g_mmco_d[l_ac].mmco004_desc FROM oocql_t 
           WHERE oocqlent = g_enterprise AND oocql001 = '2014' AND oocql002 = g_mmco_d[l_ac].mmco004 AND oocql003 = g_dlang   

       WHEN 'L' #商品屬性-其它屬性十
          SELECT oocql004 INTO g_mmco_d[l_ac].mmco004_desc FROM oocql_t 
           WHERE oocqlent = g_enterprise AND oocql001 = '2014' AND oocql002 = g_mmco_d[l_ac].mmco004 AND oocql003 = g_dlang 
           
       WHEN 'U'
         #庫區類別
           SELECT gzcbl004 INTO g_mmco_d[l_ac].mmco004_desc FROM gzcbl_t 
            WHERE gzcbl001 = '6200' AND gzcbl002 = g_mmco_d[l_ac].mmco004 AND gzcbl003 =  g_dlang 
       WHEN 'V' 
         #庫區品類 
           SELECT rtaxl003 INTO g_mmco_d[l_ac].mmco004_desc FROM rtaxl_t
            WHERE rtaxlent = g_enterprise AND rtaxl001 = g_mmco_d[l_ac].mmco004 AND rtaxl002 = g_dlang 
   END CASE
   DISPLAY BY NAME g_mmco_d[l_ac].mmco004_desc

END FUNCTION]]>
</point>
  <point name="function.ammm363_mmbs004_ref" cite_std="N" status="" ver="1" src="s" new="Y" order="6" mark_hard="N">
<![CDATA[
################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL ammm363_mmbs004_ref()
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION ammm363_mmbs004_ref()
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_mmco2_d[l_ac].mmbs004
   CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_mmco2_d[l_ac].mmbs004_desc = '', g_rtn_fields[1] , ''
   DISPLAY BY NAME g_mmco2_d[l_ac].mmbs004_desc

END FUNCTION]]>
</point>
  <point name="function.ammm363_mmcp004_ref" cite_std="N" status="" ver="2" src="s" new="Y" order="7" mark_hard="N">
<![CDATA[
################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL ammm363_mmcp004_ref()
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION ammm363_mmcp004_ref()
   CASE g_mmco3_d[l_ac].mmcp003
      WHEN '1' #會員等級
         SELECT oocql004 INTO g_mmco3_d[l_ac].mmcp004_desc
           FROM oocql_t
          WHERE oocqlent = g_enterprise AND oocql001 = '2024' AND oocql002 = g_mmco3_d[l_ac].mmcp004 AND oocql003 = g_dlang
      WHEN '2' #會員類型
         SELECT oocql004 INTO g_mmco3_d[l_ac].mmcp004_desc
           FROM oocql_t
          WHERE oocqlent = g_enterprise AND oocql001 = '2025' AND oocql002 = g_mmco3_d[l_ac].mmcp004 AND oocql003 = g_dlang
      WHEN  '3' #款別類型
          SELECT ooial003 INTO g_mmco3_d[l_ac].mmcp004_desc FROM ooial_t
           WHERE ooialent = g_enterprise AND ooial001 =  g_mmco3_d[l_ac].mmcp004 AND ooial002 = g_dlang
      WHEN 'Q' #紀念日
         SELECT oocql004 INTO g_mmco3_d[l_ac].mmcp004_desc
           FROM oocql_t
          WHERE oocqlent = g_enterprise AND oocql001 = '2050' AND oocql002 = g_mmco3_d[l_ac].mmcp004 AND oocql003 = g_dlang
      WHEN 'R' #促銷活動檔期
      WHEN 'S' #營運據點
         SELECT ooefl003 INTO g_mmco3_d[l_ac].mmcp004_desc
           FROM ooefl_t
          WHERE ooeflent = g_enterprise AND ooefl001 = g_mmco3_d[l_ac].mmcp004 AND ooefl002 = g_dlang
      WHEN 'T' #庫位編號
           SELECT inaa003 INTO g_mmco3_d[l_ac].mmcp004_desc FROM inaa_t
            WHERE inaaent = g_enterprise AND inaa001 = g_mmco3_d[l_ac].mmcp004 AND inaasite = g_site
       WHEN 'U'
         #庫區類別
           SELECT gzcbl004 INTO g_mmco3_d[l_ac].mmcp004_desc FROM gzcbl_t 
            WHERE gzcbl001 = '6200' AND gzcbl002 = g_mmco3_d[l_ac].mmcp004 AND gzcbl003 =  g_dlang 
       WHEN 'V' 
         #庫區品類 
           SELECT rtaxl003 INTO g_mmco3_d[l_ac].mmcp004_desc FROM rtaxl_t
            WHERE rtaxlent = g_enterprise AND rtaxl001 = g_mmco3_d[l_ac].mmcp004 AND rtaxl002 = g_dlang
   END CASE
   DISPLAY BY NAME g_mmco3_d[l_ac].mmcp004_desc

END FUNCTION]]>
</point>
