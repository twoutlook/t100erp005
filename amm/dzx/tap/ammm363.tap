       g_mmby_m.mmbymodid_desc,g_mmby_m.mmbymoddt,g_mmby_m.mmbycnfid,g_mmby_m.mmbycnfid_desc,g_mmby_m.mmbycnfdt, 
       g_mmby_m.mmcp004_1,g_mmby_m.mmcp011_1,g_mmby_m.mmcp013_1,g_mmby_m.mmcp015_1,g_mmby_m.mmcp004_1_desc, 
       g_mmby_m.mmcp012_1,g_mmby_m.mmcp014_1,g_mmby_m.mmcp016_1
   IF SQLCA.sqlcode THEN
