  <point name="function.ammt421_mmbi002_desc2" order="10" ver="5" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#+ 單身下方卡種編號、會員編號顯示
PRIVATE FUNCTION ammt421_mmbi002_desc2(p_mmbi003,p_mmbi004)
DEFINE p_mmbi003       LIKE mmbi_t.mmbi003 #卡種編號
DEFINE p_mmbi004       LIKE mmbi_t.mmbi004 #會員編號
DEFINE l_mmanl001_desc    LIKE mmanl_t.mmanl003
DEFINE l_mmaf008_desc     LIKE mmaf_t.mmaf008
   IF cl_null(p_mmbi003) OR cl_null(p_mmbi004) THEN
      LET l_mmanl001_desc = ''
      LET l_mmaf008_desc = ''
      LET g_mmbh_m.mmanl001 = ''
      LET g_mmbh_m.mmaf001 = ''
   ELSE
      LET g_mmbh_m.mmanl001 = p_mmbi003
      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_mmbh_m.mmanl001
      CALL ap_ref_array2(g_ref_fields,"SELECT mmanl003 FROM mmanl_t WHERE mmanlent='"||g_enterprise||"' AND mmanl001=? AND mmanl002='"||g_dlang||"'","") RETURNING g_rtn_fields
      LET l_mmanl001_desc = '', g_rtn_fields[1] , ''
      LET g_mmbh_m.mmaf001 = p_mmbi004
      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_mmbh_m.mmaf001
      CALL ap_ref_array2(g_ref_fields,"SELECT mmaf008 FROM mmaf_t WHERE mmafent='"||g_enterprise||"' AND mmaf001=? ","") RETURNING g_rtn_fields
      LET l_mmaf008_desc = '', g_rtn_fields[1] , ''
   END IF
   DISPLAY g_mmbh_m.mmanl001 TO mmanl001
   DISPLAY l_mmanl001_desc TO mmanl001_desc
   DISPLAY g_mmbh_m.mmaf001 TO mmaf001
   DISPLAY l_mmaf008_desc TO mmaf001_desc
END FUNCTION]]>
