  <section id="ammt421.filter" ver="11" status="" src="s" readonly="">
    <![CDATA[#應用 a50 樣板自動產生(Version:5)
#+ filter過濾功能
PRIVATE FUNCTION ammt421_filter()
   #add-point:filter段define
   {<point name="filter.define" edit="s"/>}
   #end add-point   
   #add-point:filter段define
   {<point name="filter.define_customerization" edit="c"/>}
   #end add-point   
   
   #切換畫面
   IF NOT g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",1)
      CALL gfrm_curr.setElementHidden("worksheet",0)
      LET g_main_hidden = 1
   END IF   
 
   LET INT_FLAG = 0
 
   LET g_qryparam.state = 'c'
 
   LET g_wc_filter_t = g_wc_filter.trim()
   LET g_wc_t = g_wc
 
   LET g_wc = cl_replace_str(g_wc, g_wc_filter_t, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON mmbhdocno,mmbhdocdt,mmbhsite,mmbh001,mmbh002
                          FROM s_browse[1].b_mmbhdocno,s_browse[1].b_mmbhdocdt,s_browse[1].b_mmbhsite, 
                              s_browse[1].b_mmbh001,s_browse[1].b_mmbh002
 
         BEFORE CONSTRUCT
               DISPLAY ammt421_filter_parser('mmbhdocno') TO s_browse[1].b_mmbhdocno
            DISPLAY ammt421_filter_parser('mmbhdocdt') TO s_browse[1].b_mmbhdocdt
            DISPLAY ammt421_filter_parser('mmbhsite') TO s_browse[1].b_mmbhsite
            DISPLAY ammt421_filter_parser('mmbh001') TO s_browse[1].b_mmbh001
            DISPLAY ammt421_filter_parser('mmbh002') TO s_browse[1].b_mmbh002
      
         #add-point:filter段cs_ctrl
         {<point name="filter.cs_ctrl"/>}
         #end add-point
      
      END CONSTRUCT
 
      #add-point:filter段add_cs
      {<point name="filter.add_cs"/>}
      #end add-point
 
      BEFORE DIALOG
         #add-point:filter段b_dialog
         {<point name="filter.b_dialog"/>}
         #end add-point  
      
      ON ACTION accept
         ACCEPT DIALOG
 
      ON ACTION cancel
         LET INT_FLAG = 1
         EXIT DIALOG 
 
      #交談指令共用ACTION
      &include "common_action.4gl" 
         CONTINUE DIALOG
   
   END DIALOG
 
   IF NOT INT_FLAG THEN
      LET g_wc_filter = "   AND   ", g_wc_filter, "   "
      LET g_wc = g_wc , g_wc_filter
   ELSE
      LET g_wc_filter = g_wc_filter_t
      LET g_wc = g_wc_t
   END IF
 
      CALL ammt421_filter_show('mmbhdocno')
   CALL ammt421_filter_show('mmbhdocdt')
   CALL ammt421_filter_show('mmbhsite')
   CALL ammt421_filter_show('mmbh001')
   CALL ammt421_filter_show('mmbh002')
 
END FUNCTION
]]>
