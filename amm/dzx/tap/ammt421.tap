  <section id="ammt421.fetch" ver="24" status="" src="s" readonly="">
    <![CDATA[#+ 指定PK後抓取單頭其他資料
PRIVATE FUNCTION ammt421_fetch(p_flag)
   DEFINE p_flag     LIKE type_t.chr1
   DEFINE ls_msg     STRING
   #add-point:fetch段define
   {<point name="fetch.define" edit="s"/>}
   #end add-point    
   #add-point:fetch段define(客製用)
   {<point name="fetch.define_customerization" edit="c"/>}
   #end add-point    
   
   IF g_browser_cnt = 0 THEN
      RETURN
   END IF
 
   CALL cl_ap_performance_next_start()
   CASE p_flag
      WHEN 'F' 
         LET g_current_idx = 1
      WHEN 'L'  
         LET g_current_idx = g_browser.getLength()              
      WHEN 'P'
         IF g_current_idx > 1 THEN               
            LET g_current_idx = g_current_idx - 1
         END IF 
      WHEN 'N'
         IF g_current_idx < g_header_cnt THEN
            LET g_current_idx =  g_current_idx + 1
         END IF        
      WHEN '/'
         IF (NOT g_no_ask) THEN    
            CALL cl_set_act_visible("accept,cancel", TRUE)    
            CALL cl_getmsg('fetch',g_lang) RETURNING ls_msg
            LET INT_FLAG = 0
 
            PROMPT ls_msg CLIPPED,':' FOR g_jump
               #交談指令共用ACTION
               &include "common_action.4gl" 
            END PROMPT
 
            CALL cl_set_act_visible("accept,cancel", FALSE)    
            IF INT_FLAG THEN
                LET INT_FLAG = 0
                EXIT CASE  
            END IF           
         END IF
         
         IF g_jump > 0 AND g_jump <= g_browser.getLength() THEN
             LET g_current_idx = g_jump
         END IF
         LET g_no_ask = FALSE  
   END CASE 
 
   CALL g_curr_diag.setCurrentRow("s_browse", g_current_idx) #設定browse 索引
   
   LET g_current_row = g_current_idx
   LET g_detail_cnt = g_header_cnt                  
   
   #單身總筆數顯示
   IF g_detail_cnt > 0 THEN
      #若單身有資料時, idx至少為1
      IF g_detail_idx <= 0 THEN
         LET g_detail_idx = 1
      END IF
      DISPLAY g_detail_idx TO FORMONLY.idx  
   ELSE
      LET g_detail_idx = 0
      DISPLAY '' TO FORMONLY.idx    
   END IF
   
   #瀏覽頁筆數顯示
   LET g_browser_idx = g_pagestart+g_current_idx-1
   DISPLAY g_browser_idx TO FORMONLY.b_index   #當下筆數
   DISPLAY g_browser_idx TO FORMONLY.h_index   #當下筆數
   
   CALL cl_navigator_setting( g_current_idx, g_browser_cnt )
 
   #代表沒有資料
   IF g_current_idx = 0 OR g_browser.getLength() = 0 THEN
      RETURN
   END IF
   
   #超出範圍
   IF g_current_idx > g_browser.getLength() THEN
      LET g_current_idx = g_browser.getLength()
   END IF
   
   LET g_mmbh_m.mmbhdocno = g_browser[g_current_idx].b_mmbhdocno
 
   
   #重讀DB,因TEMP有不被更新特性
   EXECUTE ammt421_master_referesh USING g_mmbh_m.mmbhdocno INTO g_mmbh_m.mmbhsite,g_mmbh_m.mmbhdocdt, 
       g_mmbh_m.mmbhdocno,g_mmbh_m.mmbhunit,g_mmbh_m.mmbh000,g_mmbh_m.mmbh001,g_mmbh_m.mmbh002,g_mmbh_m.mmbh003, 
       g_mmbh_m.mmbhstus,g_mmbh_m.mmbhownid,g_mmbh_m.mmbhowndp,g_mmbh_m.mmbhcrtid,g_mmbh_m.mmbhcrtdp, 
       g_mmbh_m.mmbhcrtdt,g_mmbh_m.mmbhmodid,g_mmbh_m.mmbhmoddt,g_mmbh_m.mmbhcnfid,g_mmbh_m.mmbhcnfdt, 
       g_mmbh_m.mmbhsite_desc,g_mmbh_m.mmbh001_desc,g_mmbh_m.mmbh002_desc,g_mmbh_m.mmbhownid_desc,g_mmbh_m.mmbhowndp_desc, 
       g_mmbh_m.mmbhcrtid_desc,g_mmbh_m.mmbhcrtdp_desc,g_mmbh_m.mmbhmodid_desc,g_mmbh_m.mmbhcnfid_desc 

   
   #遮罩相關處理
   LET g_mmbh_m_mask_o.* =  g_mmbh_m.*
   CALL ammt421_mmbh_t_mask()
   LET g_mmbh_m_mask_n.* =  g_mmbh_m.*
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL ammt421_set_act_visible()   
   CALL ammt421_set_act_no_visible()
   
   #add-point:fetch段action控制
   {<point name="fetch.action_control"/>}
   #end add-point  
   
   
   
   #add-point:fetch結束前
   {<point name="fetch.after" />}
   #end add-point
   
   #保存單頭舊值
   LET g_mmbh_m_t.* = g_mmbh_m.*
   LET g_mmbh_m_o.* = g_mmbh_m.*
   
   LET g_data_owner = g_mmbh_m.mmbhownid      
   LET g_data_dept  = g_mmbh_m.mmbhowndp
   
   #重新顯示   
   CALL ammt421_show()
 
   
 
END FUNCTION
]]>
  </section>
