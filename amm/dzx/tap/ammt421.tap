  <section id="ammt421.state_change" ver="21" status="" src="s" readonly="">
    <![CDATA[   #應用 a09 樣板自動產生(Version:12)
#+ 確認碼變更 
PRIVATE FUNCTION ammt421_statechange()
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define
   {<point name="statechange.define" edit="s"/>}
   #end add-point  
   #add-point:statechange段define(客製用)
   {<point name="statechange.define_customerization" edit="c"/>}
   #end add-point  
   
   #add-point:statechange段開始前
   {<point name="statechange.before"/>}
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_mmbh_m.mmbhdocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN ammt421_cl USING g_enterprise,g_mmbh_m.mmbhdocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN ammt421_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE ammt421_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE ammt421_master_referesh USING g_mmbh_m.mmbhdocno INTO g_mmbh_m.mmbhsite,g_mmbh_m.mmbhdocdt, 
       g_mmbh_m.mmbhdocno,g_mmbh_m.mmbhunit,g_mmbh_m.mmbh000,g_mmbh_m.mmbh001,g_mmbh_m.mmbh002,g_mmbh_m.mmbh003, 
       g_mmbh_m.mmbhstus,g_mmbh_m.mmbhownid,g_mmbh_m.mmbhowndp,g_mmbh_m.mmbhcrtid,g_mmbh_m.mmbhcrtdp, 
       g_mmbh_m.mmbhcrtdt,g_mmbh_m.mmbhmodid,g_mmbh_m.mmbhmoddt,g_mmbh_m.mmbhcnfid,g_mmbh_m.mmbhcnfdt, 
       g_mmbh_m.mmbhsite_desc,g_mmbh_m.mmbh001_desc,g_mmbh_m.mmbh002_desc,g_mmbh_m.mmbhownid_desc,g_mmbh_m.mmbhowndp_desc, 
       g_mmbh_m.mmbhcrtid_desc,g_mmbh_m.mmbhcrtdp_desc,g_mmbh_m.mmbhmodid_desc,g_mmbh_m.mmbhcnfid_desc 

 
   #檢查是否允許此動作
   IF NOT ammt421_action_chk() THEN
      CLOSE ammt421_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_mmbh_m.mmbhsite,g_mmbh_m.mmbhsite_desc,g_mmbh_m.mmbhdocdt,g_mmbh_m.mmbhdocno,g_mmbh_m.mmbhunit, 
       g_mmbh_m.mmbh000,g_mmbh_m.mmbh001,g_mmbh_m.mmbh001_desc,g_mmbh_m.mmbh002,g_mmbh_m.mmbh002_desc, 
       g_mmbh_m.mmbh003,g_mmbh_m.mmbhstus,g_mmbh_m.mmbhownid,g_mmbh_m.mmbhownid_desc,g_mmbh_m.mmbhowndp, 
       g_mmbh_m.mmbhowndp_desc,g_mmbh_m.mmbhcrtid,g_mmbh_m.mmbhcrtid_desc,g_mmbh_m.mmbhcrtdp,g_mmbh_m.mmbhcrtdp_desc, 
       g_mmbh_m.mmbhcrtdt,g_mmbh_m.mmbhmodid,g_mmbh_m.mmbhmodid_desc,g_mmbh_m.mmbhmoddt,g_mmbh_m.mmbhcnfid, 
       g_mmbh_m.mmbhcnfid_desc,g_mmbh_m.mmbhcnfdt,g_mmbh_m.mmanl001,g_mmbh_m.mmanl001_desc,g_mmbh_m.mmaf001, 
       g_mmbh_m.mmaf001_desc
 
   CASE g_mmbh_m.mmbhstus
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
      WHEN "X"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
      
   END CASE
 
   #add-point:資料刷新後
   {<point name="statechange.after_refresh"/>}
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_mmbh_m.mmbhstus
            
            WHEN "Y"
               HIDE OPTION "confirmed"
            WHEN "N"
               HIDE OPTION "unconfirmed"
            WHEN "X"
               HIDE OPTION "invalid"
         END CASE
     
      #add-point:menu前
      {<point name="statechange.before_menu"/>}
      #end add-point
      
      
	  
      ON ACTION confirmed
         IF cl_auth_chk_act("confirmed") THEN
            LET lc_state = "Y"
            #add-point:action控制
            {<point name="statechange.confirmed"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION unconfirmed
         IF cl_auth_chk_act("unconfirmed") THEN
            LET lc_state = "N"
            #add-point:action控制
            {<point name="statechange.unconfirmed"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION invalid
         IF cl_auth_chk_act("invalid") THEN
            LET lc_state = "X"
            #add-point:action控制
            {<point name="statechange.invalid"/>}
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制
      {<point name="statechange.more_control"/>}
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "Y" 
      AND lc_state <> "N"
      AND lc_state <> "X"
      ) OR 
      g_mmbh_m.mmbhstus = lc_state OR cl_null(lc_state) THEN
      CLOSE ammt421_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前
   {<point name="statechange.b_update"/>}
   #end add-point
   
   LET g_mmbh_m.mmbhmodid = g_user
   LET g_mmbh_m.mmbhmoddt = cl_get_current()
   LET g_mmbh_m.mmbhstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE mmbh_t 
      SET (mmbhstus,mmbhmodid,mmbhmoddt) 
        = (g_mmbh_m.mmbhstus,g_mmbh_m.mmbhmodid,g_mmbh_m.mmbhmoddt)     
    WHERE mmbhent = g_enterprise AND mmbhdocno = g_mmbh_m.mmbhdocno
 
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE ammt421_master_referesh USING g_mmbh_m.mmbhdocno INTO g_mmbh_m.mmbhsite,g_mmbh_m.mmbhdocdt, 
          g_mmbh_m.mmbhdocno,g_mmbh_m.mmbhunit,g_mmbh_m.mmbh000,g_mmbh_m.mmbh001,g_mmbh_m.mmbh002,g_mmbh_m.mmbh003, 
          g_mmbh_m.mmbhstus,g_mmbh_m.mmbhownid,g_mmbh_m.mmbhowndp,g_mmbh_m.mmbhcrtid,g_mmbh_m.mmbhcrtdp, 
          g_mmbh_m.mmbhcrtdt,g_mmbh_m.mmbhmodid,g_mmbh_m.mmbhmoddt,g_mmbh_m.mmbhcnfid,g_mmbh_m.mmbhcnfdt, 
          g_mmbh_m.mmbhsite_desc,g_mmbh_m.mmbh001_desc,g_mmbh_m.mmbh002_desc,g_mmbh_m.mmbhownid_desc, 
          g_mmbh_m.mmbhowndp_desc,g_mmbh_m.mmbhcrtid_desc,g_mmbh_m.mmbhcrtdp_desc,g_mmbh_m.mmbhmodid_desc, 
          g_mmbh_m.mmbhcnfid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_mmbh_m.mmbhsite,g_mmbh_m.mmbhsite_desc,g_mmbh_m.mmbhdocdt,g_mmbh_m.mmbhdocno, 
          g_mmbh_m.mmbhunit,g_mmbh_m.mmbh000,g_mmbh_m.mmbh001,g_mmbh_m.mmbh001_desc,g_mmbh_m.mmbh002, 
          g_mmbh_m.mmbh002_desc,g_mmbh_m.mmbh003,g_mmbh_m.mmbhstus,g_mmbh_m.mmbhownid,g_mmbh_m.mmbhownid_desc, 
          g_mmbh_m.mmbhowndp,g_mmbh_m.mmbhowndp_desc,g_mmbh_m.mmbhcrtid,g_mmbh_m.mmbhcrtid_desc,g_mmbh_m.mmbhcrtdp, 
          g_mmbh_m.mmbhcrtdp_desc,g_mmbh_m.mmbhcrtdt,g_mmbh_m.mmbhmodid,g_mmbh_m.mmbhmodid_desc,g_mmbh_m.mmbhmoddt, 
          g_mmbh_m.mmbhcnfid,g_mmbh_m.mmbhcnfid_desc,g_mmbh_m.mmbhcnfdt,g_mmbh_m.mmanl001,g_mmbh_m.mmanl001_desc, 
          g_mmbh_m.mmaf001,g_mmbh_m.mmaf001_desc
   END IF
 
   #add-point:stus修改後
   {<point name="statechange.a_update"/>}
   #end add-point
 
   #add-point:statechange段結束前
   {<point name="statechange.after"/>}
   #end add-point  
 
   CLOSE ammt421_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL ammt421_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION
 
 
]]>
  </section>
