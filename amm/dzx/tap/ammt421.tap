  <point name="function.ammt421_mmbhdocno_chk" order="12" ver="5" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#+
PRIVATE FUNCTION ammt421_mmbhdocno_chk(p_docno)
DEFINE l_stus   LIKE mmbh_t.mmbhstus
DEFINE p_docno  LIKE mmbh_t.mmbhdocno
   LET g_errno = null
   SELECT oobastus INTO l_stus  FROM ooba_t,oobl_t 
    WHERE oobaent = ooblent  AND ooba001 = oobl001
      AND ooba002 = oobl002   AND oobl003 = 'ammt421'
      AND oobl001 = g_ooef004 AND oobl002= p_docno
      AND ooblent = g_enterprise
      
   CASE WHEN SQLCA.SQLCODE = 100   LET g_errno = 'aim-00056'
        WHEN l_stus='N'            LET g_errno = 'aoo-00102'       
   END CASE    
   
END FUNCTION]]>
