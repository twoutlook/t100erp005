  <point name="function.ammt421_mmbi002_chk" order="5" ver="11" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[#檢查會員卡號的存在性、有效性
PRIVATE FUNCTION ammt421_mmbi002_chk(p_mmbi002)
DEFINE p_mmbi002        LIKE mmbi_t.mmbi002
DEFINE l_mmaq005        LIKE mmaq_t.mmaq005
DEFINE l_mmaq006        LIKE mmaq_t.mmaq006
DEFINE l_cnt            LIKE type_t.num5
DEFINE l_mman042        LIKE mman_t.mman042
DEFINE l_mman043        LIKE mman_t.mman043
   
   LET g_errno = ''
   SELECT mmaq005,mmaq006 INTO l_mmaq005,l_mmaq006 FROM mmaq_t WHERE mmaq001 = p_mmbi002
   IF cL_null(l_mmaq006) THEN
      LET g_errno = 'amm-00032'
      RETURN
   END IF
   IF l_mmaq006 != '2' AND l_mmaq006 != '3' THEN
   　 LET g_errno = 'amm-00033'
      RETURN　
   END IF  
   IF l_mmaq005 < g_today THEN
      LET g_errno = 'amm-00036'
      RETURN　
   END IF 
   SELECT COUNT(mmbi002) INTO l_cnt FROM mmbi_t 
    WHERE mmbient = g_enterprise
      AND mmbidocno = g_mmbh_m.mmbhdocno 
      AND mmbi002 = p_mmbi002      
   IF l_cnt > 0 THEN
      LET g_errno = 'amm-00034'
      RETURN　
   END IF 
   SELECT COUNT(*) INTO l_cnt FROM mmbh_t,mmbi_t 
    WHERE mmbhent = mmbient 
      AND mmbhdocno = mmbidocno AND mmbhstus = 'N'
      AND mmbi002 = p_mmbi002
   IF l_cnt > 0 THEN
      LET g_errno = 'amm-00035'
      RETURN　
   END IF    
   #add by yangxf --start---
   IF g_prog = 'ammt428' THEN 
      #检查卡号属否存在且为3.开卡状态
      SELECT mman042,mman043
        INTO l_mman042,l_mman043
        FROM mmaq_t,mman_t
       WHERE mmaqent = g_enterprise
         AND mmanent = mmaqent
         AND mman001 = mmaq002
         AND mmaq001 = p_mmbi002
      CASE 
            WHEN SQLCA.SQLCODE = 100 LET g_errno = 'amm-00323'
            WHEN l_mman042 IS NOT NULL AND l_mman042 = 'N' 
                                     LET g_errno = 'amm-00427'
            WHEN l_mman043 IS NOT NULL AND l_mman043 = 'N'
                                     LET g_errno = 'amm-00450'
            OTHERWISE                LET g_errno = SQLCA.SQLCODE USING '-------'
      END CASE
   END IF 
   #add by yangxf ---end----
END FUNCTION]]>
  </point>
  <point name="function.ammt421_mmbi002_desc" order="6" ver="11" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[#+ 會員編號帶出有效期至，原剩餘積點，卡種編號，會員編號
PRIVATE FUNCTION ammt421_mmbi002_desc(p_mmbi002)
DEFINE p_mmbi002   LIKE mmbi_t.mmbi002
DEFINE l_mmaq005   LIKE mmaq_t.mmaq005
   IF cl_null(p_mmbi002) THEN
      LET g_mmbi_d[l_ac].mmbi007 = ''
      LET g_mmbi_d[l_ac].mmaq018 = ''
      LET g_mmbi_d[l_ac].mmbi003 = ''
      LET g_mmbi_d[l_ac].mmbi004 = ''
      CALL ammt421_mmbi003_desc('')
      CALL ammt421_mmbi004_desc('')
   ELSE
      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = p_mmbi002
      IF g_prog = 'ammt421' THEN 
         CALL ap_ref_array2(g_ref_fields,"SELECT mmaq018,mmaq002,mmaq003 FROM mmaq_t WHERE mmaqent='"||g_enterprise||"' AND mmaq001=? ","") RETURNING g_rtn_fields
      #add by yangxf ----start---
      ELSE
         CALL ap_ref_array2(g_ref_fields,"SELECT mmaq009,mmaq002,mmaq003 FROM mmaq_t WHERE mmaqent='"||g_enterprise||"' AND mmaq001=? ","") RETURNING g_rtn_fields
      END IF
      #add by yangxf -----end----      
      LET g_mmbi_d[l_ac].mmaq018 = '', g_rtn_fields[1] , ''
      LET g_mmbi_d[l_ac].mmbi003 = '', g_rtn_fields[2] , ''
      LET g_mmbi_d[l_ac].mmbi004 = '', g_rtn_fields[3] , ''
      SELECT mmaq005 INTO l_mmaq005 FROM mmaq_t
       WHERE mmaqent = g_enterprise AND mmaq001 = p_mmbi002
             
      LET g_mmbi_d[l_ac].mmbi007 = l_mmaq005
      CALL ammt421_mmbi003_desc(g_mmbi_d[l_ac].mmbi003)
      CALL ammt421_mmbi004_desc(g_mmbi_d[l_ac].mmbi004)
   END IF    
   DISPLAY BY NAME g_mmbi_d[l_ac].mmbi007
   DISPLAY BY NAME g_mmbi_d[l_ac].mmaq018
   DISPLAY BY NAME g_mmbi_d[l_ac].mmbi003
   DISPLAY BY NAME g_mmbi_d[l_ac].mmbi004
END FUNCTION]]>
  </point>
