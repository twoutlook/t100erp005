  <section id="ammt421.input.body" ver="26" status="" src="s" readonly="">
    <![CDATA[   
      #Page1 預設值產生於此處
      INPUT ARRAY g_mmbi_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,WITHOUT DEFAULTS, #MAXCOUNT = g_max_rec,
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            #add-point:資料輸入前
            {<point name="input.body.before_input2"/>}
            #end add-point
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_mmbi_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL ammt421_b_fill()
            #如果一直都在單身1則控制筆數位置
            IF g_loc = 'm' AND g_rec_b != 0 THEN
               CALL FGL_SET_ARR_CURR(g_detail_idx)
            END IF
            LET g_loc = 'm'
            LET g_rec_b = g_mmbi_d.getLength()
            #add-point:資料輸入前
            {<point name="input.d.before_input"/>}
            #end add-point
         
         BEFORE ROW
            #add-point:modify段before row2
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET l_insert = FALSE
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET g_detail_idx = l_ac
            LET g_current_page = 1
            
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
         
            CALL s_transaction_begin()
            OPEN ammt421_cl USING g_enterprise,g_mmbh_m.mmbhdocno
            IF STATUS THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "OPEN ammt421_cl:" 
               LET g_errparam.code   = STATUS 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CLOSE ammt421_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            
            LET g_rec_b = g_mmbi_d.getLength()
            
            IF g_rec_b >= l_ac 
               AND g_mmbi_d[l_ac].mmbiseq IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_mmbi_d_t.* = g_mmbi_d[l_ac].*  #BACKUP
               LET g_mmbi_d_o.* = g_mmbi_d[l_ac].*  #BACKUP
               CALL ammt421_set_entry_b(l_cmd)
               #add-point:modify段after_set_entry_b
               {<point name="input.body.after_set_entry_b"/>}
               #end add-point  
               CALL ammt421_set_no_entry_b(l_cmd)
               IF NOT ammt421_lock_b("mmbi_t","'1'") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH ammt421_bcl INTO g_mmbi_d[l_ac].mmbiseq,g_mmbi_d[l_ac].mmbisite,g_mmbi_d[l_ac].mmbiunit, 
                      g_mmbi_d[l_ac].mmbi002,g_mmbi_d[l_ac].mmbi005,g_mmbi_d[l_ac].mmbi006,g_mmbi_d[l_ac].mmbi007, 
                      g_mmbi_d[l_ac].mmbi008,g_mmbi_d[l_ac].mmbi009,g_mmbi_d[l_ac].mmbi003,g_mmbi_d[l_ac].mmbi004 

                  IF SQLCA.sqlcode THEN
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = g_mmbi_d_t.mmbiseq 
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     LET l_lock_sw = "Y"
                  END IF
                  
                  #遮罩相關處理
                  LET g_mmbi_d_mask_o[l_ac].* =  g_mmbi_d[l_ac].*
                  CALL ammt421_mmbi_t_mask()
                  LET g_mmbi_d_mask_n[l_ac].* =  g_mmbi_d[l_ac].*
                  
                  LET g_bfill = "N"
                  CALL ammt421_show()
                  LET g_bfill = "Y"
                  
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
        
         BEFORE INSERT
            
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_mmbi_d[l_ac].* TO NULL 
            INITIALIZE g_mmbi_d_t.* TO NULL 
            INITIALIZE g_mmbi_d_o.* TO NULL 
            #公用欄位給值(單身)
            
            #自定義預設值
                  LET g_mmbi_d[l_ac].mmbi006 = "0"
 
            #add-point:modify段before備份
            {<point name="input.body.insert.before_bak"/>}
            #end add-point
            LET g_mmbi_d_t.* = g_mmbi_d[l_ac].*     #新輸入資料
            LET g_mmbi_d_o.* = g_mmbi_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL ammt421_set_entry_b(l_cmd)
            #add-point:modify段after_set_entry_b
            {<point name="input.body.insert.after_set_entry_b"/>}
            #end add-point
            CALL ammt421_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_mmbi_d[li_reproduce_target].* = g_mmbi_d[li_reproduce].*
 
               LET g_mmbi_d[li_reproduce_target].mmbiseq = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            #add-point:單身新增
            {<point name="input.body.b_a_insert"/>}
            #end add-point
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM mmbi_t 
             WHERE mmbient = g_enterprise AND mmbidocno = g_mmbh_m.mmbhdocno
 
               AND mmbiseq = g_mmbi_d[l_ac].mmbiseq
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前
               {<point name="input.body.b_insert"/>}
               #end add-point
            
               #同步新增到同層的table
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_mmbh_m.mmbhdocno
               LET gs_keys[2] = g_mmbi_d[g_detail_idx].mmbiseq
               CALL ammt421_insert_b('mmbi_t',gs_keys,"'1'")
                           
               #add-point:單身新增後
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE    
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = 'INSERT' 
               LET g_errparam.code   = "std-00006" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               INITIALIZE g_mmbi_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "mmbi_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL ammt421_b_fill()
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert2"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               #ERROR 'INSERT O.K'
               LET g_rec_b = g_rec_b + 1
            END IF
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' THEN
               LET l_cmd='d'
               #add-point:單身刪除後(=d)
               {<point name="input.body.after_delete_d"/>}
               #end add-point
            ELSE
