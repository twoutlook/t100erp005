    <![CDATA[   LET l_success = TRUE
   CASE lc_state 
      WHEN 'Y'       
         CALL s_ammt421_conf_chk(g_mmbh_m.mmbhdocno,g_mmbh_m.mmbhstus) RETURNING l_success,g_errno
         IF l_success THEN
            IF cl_ask_confirm('lib-014') THEN
               CALL s_transaction_begin()
               CALL s_ammt421_conf_upd(g_mmbh_m.mmbhdocno) RETURNING l_success
               IF NOT l_success THEN
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            ELSE
               RETURN
            END IF            
         ELSE
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = g_errno
            LET g_errparam.extend = g_mmbh_m.mmbhdocno
            LET g_errparam.popup = TRUE
            CALL cl_err()

            RETURN            
         END IF
                
      WHEN 'X'
         CALL s_ammt421_void_chk(g_mmbh_m.mmbhdocno,g_mmbh_m.mmbhstus) RETURNING l_success,g_errno
         IF l_success THEN
            IF cl_ask_confirm('lib-016') THEN
               CALL s_transaction_begin()
               CALL s_ammt421_void_upd(g_mmbh_m.mmbhdocno) RETURNING l_success
               IF NOT l_success THEN
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            ELSE
               RETURN
            END IF
         ELSE
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = g_errno
            LET g_errparam.extend = g_mmbh_m.mmbhdocno
            LET g_errparam.popup = TRUE
            CALL cl_err()

            RETURN    
         END IF
      WHEN 'N'
         CALL s_ammt421_unconf_chk(g_mmbh_m.mmbhdocno,g_mmbh_m.mmbhstus) RETURNING l_success,g_errno
         IF l_success THEN
            IF cl_ask_confirm('lib-015') THEN
               CALL s_transaction_begin()
               CALL s_ammt421_unconf_upd(g_mmbh_m.mmbhdocno) RETURNING l_success
