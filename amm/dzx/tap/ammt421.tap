  <section id="ammt421.insert" ver="30" status="" src="s" readonly="">
    <![CDATA[#+ 資料新增
PRIVATE FUNCTION ammt421_insert()
   #add-point:insert段define
   {<point name="insert.define" edit="s"/>}
   #end add-point    
   #add-point:insert段define(客製用)
   {<point name="insert.define_customerization" edit="c"/>}
   #end add-point    
   
   #清畫面欄位內容
   CLEAR FORM                    
   CALL g_mmbi_d.clear()   
 
 
   INITIALIZE g_mmbh_m.* LIKE mmbh_t.*             #DEFAULT 設定
   
   LET g_mmbhdocno_t = NULL
 
   
   LET g_master_insert = FALSE
   
   #add-point:insert段before
   {<point name="insert.before"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   WHILE TRUE
      #公用欄位給值(單頭)
      #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_mmbh_m.mmbhownid = g_user
      LET g_mmbh_m.mmbhowndp = g_dept
      LET g_mmbh_m.mmbhcrtid = g_user
      LET g_mmbh_m.mmbhcrtdp = g_dept 
      LET g_mmbh_m.mmbhcrtdt = cl_get_current()
      LET g_mmbh_m.mmbhmodid = g_user
      LET g_mmbh_m.mmbhmoddt = cl_get_current()
      LET g_mmbh_m.mmbhstus = 'N'
 
 
 
      #append欄位給值
      
     
      #一般欄位給值
      
  
      #add-point:單頭預設值
      {<point name="insert.default"/>}
      #end add-point 
      
      #保存單頭舊值(用於資料輸入錯誤還原預設值時使用)
      LET g_mmbh_m_t.* = g_mmbh_m.*
      LET g_mmbh_m_o.* = g_mmbh_m.*
      
      #顯示狀態(stus)圖片
            #應用 a21 樣板自動產生(Version:2)
	  #根據當下狀態碼顯示圖片
      CASE g_mmbh_m.mmbhstus 
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
 
 
    
      CALL ammt421_input("a")
      
      #add-point:單頭輸入後
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
      END IF
      
      IF NOT g_master_insert THEN
         DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
         DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
         INITIALIZE g_mmbh_m.* TO NULL
         INITIALIZE g_mmbi_d TO NULL
 
         #add-point:取消新增後
         {<point name="insert.cancel"/>}
         #end add-point 
         CALL ammt421_show()
         RETURN
      END IF
      
      LET INT_FLAG = 0
      #CALL g_mmbi_d.clear()
 
 
      LET g_rec_b = 0
      CALL s_transaction_end('Y','0')
      EXIT WHILE
        
   END WHILE
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL ammt421_set_act_visible()   
   CALL ammt421_set_act_no_visible()
   
   #將新增的資料併入搜尋條件中
   LET g_mmbhdocno_t = g_mmbh_m.mmbhdocno
 
   
   #組合新增資料的條件
   LET g_add_browse = " mmbhent = '" ||g_enterprise|| "' AND",
                      " mmbhdocno = '", g_mmbh_m.mmbhdocno, "' "
 
                      
   #add-point:組合新增資料的條件後
   {<point name="insert.after.add_browse"/>}
   #end add-point
      
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL ammt421_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   CLOSE ammt421_cl
   
   CALL ammt421_idx_chk()
   
   #撈取異動後的資料(主要是帶出reference)
   EXECUTE ammt421_master_referesh USING g_mmbh_m.mmbhdocno INTO g_mmbh_m.mmbhsite,g_mmbh_m.mmbhdocdt, 
       g_mmbh_m.mmbhdocno,g_mmbh_m.mmbhunit,g_mmbh_m.mmbh000,g_mmbh_m.mmbh001,g_mmbh_m.mmbh002,g_mmbh_m.mmbh003, 
       g_mmbh_m.mmbhstus,g_mmbh_m.mmbhownid,g_mmbh_m.mmbhowndp,g_mmbh_m.mmbhcrtid,g_mmbh_m.mmbhcrtdp, 
       g_mmbh_m.mmbhcrtdt,g_mmbh_m.mmbhmodid,g_mmbh_m.mmbhmoddt,g_mmbh_m.mmbhcnfid,g_mmbh_m.mmbhcnfdt, 
       g_mmbh_m.mmbhsite_desc,g_mmbh_m.mmbh001_desc,g_mmbh_m.mmbh002_desc,g_mmbh_m.mmbhownid_desc,g_mmbh_m.mmbhowndp_desc, 
       g_mmbh_m.mmbhcrtid_desc,g_mmbh_m.mmbhcrtdp_desc,g_mmbh_m.mmbhmodid_desc,g_mmbh_m.mmbhcnfid_desc 

   
   #遮罩相關處理
   LET g_mmbh_m_mask_o.* =  g_mmbh_m.*
   CALL ammt421_mmbh_t_mask()
   LET g_mmbh_m_mask_n.* =  g_mmbh_m.*
   
   #將資料顯示到畫面上
   DISPLAY BY NAME g_mmbh_m.mmbhsite,g_mmbh_m.mmbhsite_desc,g_mmbh_m.mmbhdocdt,g_mmbh_m.mmbhdocno,g_mmbh_m.mmbhunit, 
       g_mmbh_m.mmbh000,g_mmbh_m.mmbh001,g_mmbh_m.mmbh001_desc,g_mmbh_m.mmbh002,g_mmbh_m.mmbh002_desc, 
       g_mmbh_m.mmbh003,g_mmbh_m.mmbhstus,g_mmbh_m.mmbhownid,g_mmbh_m.mmbhownid_desc,g_mmbh_m.mmbhowndp, 
       g_mmbh_m.mmbhowndp_desc,g_mmbh_m.mmbhcrtid,g_mmbh_m.mmbhcrtid_desc,g_mmbh_m.mmbhcrtdp,g_mmbh_m.mmbhcrtdp_desc, 
       g_mmbh_m.mmbhcrtdt,g_mmbh_m.mmbhmodid,g_mmbh_m.mmbhmodid_desc,g_mmbh_m.mmbhmoddt,g_mmbh_m.mmbhcnfid, 
       g_mmbh_m.mmbhcnfid_desc,g_mmbh_m.mmbhcnfdt,g_mmbh_m.mmanl001,g_mmbh_m.mmanl001_desc,g_mmbh_m.mmaf001, 
       g_mmbh_m.mmaf001_desc
   
   #add-point:新增結束後
   {<point name="insert.after"/>}
   #end add-point 
   
   #功能已完成,通報訊息中心
   CALL ammt421_msgcentre_notify('insert')
   
END FUNCTION
]]>
