               CALL cl_err()
               LET INT_FLAG = 0
               LET g_mmbi_d[l_ac].* = g_mmbi_d_t.*
               CLOSE ammt421_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = g_mmbi_d[l_ac].mmbiseq 
               LET g_errparam.code   = -263 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               LET g_mmbi_d[l_ac].* = g_mmbi_d_t.*
            ELSE
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               #寫入修改者/修改日期資訊(單身)
               
      
               #將遮罩欄位還原
               CALL ammt421_mmbi_t_mask_restore('restore_mask_o')
      
               UPDATE mmbi_t SET (mmbidocno,mmbiseq,mmbisite,mmbiunit,mmbi002,mmbi005,mmbi006,mmbi007, 
                   mmbi008,mmbi009,mmbi003,mmbi004) = (g_mmbh_m.mmbhdocno,g_mmbi_d[l_ac].mmbiseq,g_mmbi_d[l_ac].mmbisite, 
                   g_mmbi_d[l_ac].mmbiunit,g_mmbi_d[l_ac].mmbi002,g_mmbi_d[l_ac].mmbi005,g_mmbi_d[l_ac].mmbi006, 
                   g_mmbi_d[l_ac].mmbi007,g_mmbi_d[l_ac].mmbi008,g_mmbi_d[l_ac].mmbi009,g_mmbi_d[l_ac].mmbi003, 
                   g_mmbi_d[l_ac].mmbi004)
                WHERE mmbient = g_enterprise AND mmbidocno = g_mmbh_m.mmbhdocno 
 
                  AND mmbiseq = g_mmbi_d_t.mmbiseq #項次   
 
                  
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "mmbi_t" 
                     LET g_errparam.code   = "std-00009" 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     CALL s_transaction_end('N','0')
                     LET g_mmbi_d[l_ac].* = g_mmbi_d_t.*
                  WHEN SQLCA.sqlcode #其他錯誤
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "mmbi_t" 
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()                   
                     CALL s_transaction_end('N','0')
                     LET g_mmbi_d[l_ac].* = g_mmbi_d_t.*  
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_mmbh_m.mmbhdocno
               LET gs_keys_bak[1] = g_mmbhdocno_t
               LET gs_keys[2] = g_mmbi_d[g_detail_idx].mmbiseq
               LET gs_keys_bak[2] = g_mmbi_d_t.mmbiseq
               CALL ammt421_update_b('mmbi_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
               END CASE
 
               #將遮罩欄位進行遮蔽
               CALL ammt421_mmbi_t_mask_restore('restore_mask_n')
               
               #判斷key是否有改變
               INITIALIZE gs_keys TO NULL
               IF NOT(g_mmbi_d[g_detail_idx].mmbiseq = g_mmbi_d_t.mmbiseq 
 
                  ) THEN
                  LET gs_keys[01] = g_mmbh_m.mmbhdocno
 
                  LET gs_keys[gs_keys.getLength()+1] = g_mmbi_d_t.mmbiseq
 
                  CALL ammt421_key_update_b(gs_keys,'mmbi_t')
               END IF
               
               #修改歷程記錄(單身修改)
               LET g_log1 = util.JSON.stringify(g_mmbh_m),util.JSON.stringify(g_mmbi_d_t)
               LET g_log2 = util.JSON.stringify(g_mmbh_m),util.JSON.stringify(g_mmbi_d[l_ac])
               IF NOT cl_log_modified_record_d(g_log1,g_log2) THEN 
                  CALL s_transaction_end('N','0')
               END IF
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            #add-point:單身after_row
            {<point name="input.body.after_row"/>}
            #end add-point
            CALL ammt421_unlock_b("mmbi_t","'1'")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock
            #add-point:單身after_row2
            {<point name="input.body.after_row2"/>}
            #end add-point
              
         AFTER INPUT
            #add-point:input段after input 
            {<point name="input.body.after_input"/>}
            #end add-point 
 
         ON ACTION controlo    
            CALL FGL_SET_ARR_CURR(g_mmbi_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_mmbi_d.getLength()+1
            
         #ON ACTION cancel
         #   LET INT_FLAG = 1
         #   LET g_detail_idx = 1
         #   EXIT DIALOG 
 
      END INPUT
      
 
      
 
 
 
]]>
