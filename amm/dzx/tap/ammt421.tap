  <section id="ammt421.modify" ver="25" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION ammt421_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   DEFINE l_wc2_table1          STRING
 
 
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point    
   #add-point:modify段define(客製用)
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point    
   
   #保存單頭舊值
   LET g_mmbh_m_t.* = g_mmbh_m.*
   LET g_mmbh_m_o.* = g_mmbh_m.*
   
   IF g_mmbh_m.mmbhdocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_mmbhdocno_t = g_mmbh_m.mmbhdocno
 
   CALL s_transaction_begin()
   
   OPEN ammt421_cl USING g_enterprise,g_mmbh_m.mmbhdocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN ammt421_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE ammt421_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE ammt421_master_referesh USING g_mmbh_m.mmbhdocno INTO g_mmbh_m.mmbhsite,g_mmbh_m.mmbhdocdt, 
       g_mmbh_m.mmbhdocno,g_mmbh_m.mmbhunit,g_mmbh_m.mmbh000,g_mmbh_m.mmbh001,g_mmbh_m.mmbh002,g_mmbh_m.mmbh003, 
       g_mmbh_m.mmbhstus,g_mmbh_m.mmbhownid,g_mmbh_m.mmbhowndp,g_mmbh_m.mmbhcrtid,g_mmbh_m.mmbhcrtdp, 
       g_mmbh_m.mmbhcrtdt,g_mmbh_m.mmbhmodid,g_mmbh_m.mmbhmoddt,g_mmbh_m.mmbhcnfid,g_mmbh_m.mmbhcnfdt, 
       g_mmbh_m.mmbhsite_desc,g_mmbh_m.mmbh001_desc,g_mmbh_m.mmbh002_desc,g_mmbh_m.mmbhownid_desc,g_mmbh_m.mmbhowndp_desc, 
       g_mmbh_m.mmbhcrtid_desc,g_mmbh_m.mmbhcrtdp_desc,g_mmbh_m.mmbhmodid_desc,g_mmbh_m.mmbhcnfid_desc 

   
   #檢查是否允許此動作
   IF NOT ammt421_action_chk() THEN
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_mmbh_m_mask_o.* =  g_mmbh_m.*
   CALL ammt421_mmbh_t_mask()
   LET g_mmbh_m_mask_n.* =  g_mmbh_m.*
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
 
   
   CALL ammt421_show()
   #add-point:modify段show之後
   {<point name="modify.after_show"/>}
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
 
 
    
   WHILE TRUE
      LET g_mmbhdocno_t = g_mmbh_m.mmbhdocno
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_mmbh_m.mmbhmodid = g_user 
LET g_mmbh_m.mmbhmoddt = cl_get_current()
LET g_mmbh_m.mmbhmodid_desc = cl_get_username(g_mmbh_m.mmbhmodid)
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      CALL ammt421_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE mmbh_t SET (mmbhmodid,mmbhmoddt) = (g_mmbh_m.mmbhmodid,g_mmbh_m.mmbhmoddt)
          WHERE mmbhent = g_enterprise AND mmbhdocno = g_mmbhdocno_t
 
      END IF
    
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_mmbh_m.* = g_mmbh_m_t.*
         CALL ammt421_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_mmbh_m.mmbhdocno != g_mmbhdocno_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE mmbi_t SET mmbidocno = g_mmbh_m.mmbhdocno
 
          WHERE mmbient = g_enterprise AND mmbidocno = g_mmbhdocno_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "mmbi_t" 
               LET g_errparam.code   = "std-00009" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "mmbi_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL ammt421_set_act_visible()   
   CALL ammt421_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " mmbhent = '" ||g_enterprise|| "' AND",
                      " mmbhdocno = '", g_mmbh_m.mmbhdocno, "' "
 
   #填到對應位置
   CALL ammt421_browser_fill("")
 
   CLOSE ammt421_cl
   
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL ammt421_msgcentre_notify('modify')
 
END FUNCTION   
