      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_ammm355
      
   END IF 
   
   CLOSE ammm355_cl
   
   
 
   #add-point:作業離開前 name="main.exit"
