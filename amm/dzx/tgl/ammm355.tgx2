      LET g_errparam.extend = "" 
      LET g_errparam.code = "std-00003" 
      LET g_errparam.popup = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_mmbt001_t = g_mmbt_m.mmbt001
 
   CALL s_transaction_begin()
   
   OPEN ammm355_cl USING g_enterprise,g_mmbt_m.mmbt001
