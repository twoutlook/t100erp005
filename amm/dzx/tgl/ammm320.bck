{<section id="ammm320.fetch" >}
#+ 指定PK後抓取單頭其他資料
PRIVATE FUNCTION ammm320_fetch(p_fl)
   #add-point:fetch段define(客製用) name="fetch.define_customerization"
   {<point name="fetch.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE p_fl       LIKE type_t.chr1
   DEFINE ls_msg     STRING
   #add-point:fetch段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="fetch.define"
   {<point name="fetch.define"/>}
   #end add-point  
   
   #add-point:Function前置處理  name="fetch.pre_function"
   {<point name="fetch.pre_function"/>}
   #end add-point
   
   #根據傳入的條件決定抓取的資料
   CASE p_fl
      WHEN "F" 
         LET g_current_idx = 1
      WHEN "P"
         IF g_current_idx > 1 THEN               
            LET g_current_idx = g_current_idx - 1
         END IF 
      WHEN "N"
         IF g_current_idx < g_header_cnt THEN
            LET g_current_idx =  g_current_idx + 1
         END IF        
      WHEN "L" 
         #LET g_current_idx = g_header_cnt        
         LET g_current_idx = g_browser.getLength()    
      WHEN "/"
         #詢問要指定的筆數
         IF (NOT g_no_ask) THEN      
            CALL cl_getmsg("fetch", g_lang) RETURNING ls_msg
            LET INT_FLAG = 0
 
            PROMPT ls_msg CLIPPED,": " FOR g_jump
               #交談指令共用ACTION
               &include "common_action.4gl"
            END PROMPT
            
            IF INT_FLAG THEN
               LET INT_FLAG = 0
               EXIT CASE  
            END IF           
         END IF
         IF g_jump > 0 THEN
            LET g_current_idx = g_jump
         END IF
         LET g_no_ask = FALSE     
   END CASE
 
   #筆數顯示
   LET g_browser_idx = g_current_idx 
   IF g_browser_cnt > 0 THEN
      DISPLAY g_browser_idx TO FORMONLY.b_index #當下筆數
      DISPLAY g_browser_cnt TO FORMONLY.b_count #總筆數
      DISPLAY g_browser_idx TO FORMONLY.h_index #當下筆數
      DISPLAY g_browser_cnt TO FORMONLY.h_count #總筆數
   ELSE
      DISPLAY '' TO FORMONLY.b_index #當下筆數
      DISPLAY '' TO FORMONLY.b_count #總筆數
      DISPLAY '' TO FORMONLY.h_index #當下筆數
      DISPLAY '' TO FORMONLY.h_count #總筆數
   END IF
   
   
   
   #避免超出browser資料筆數上限
   IF g_current_idx > g_browser.getLength() THEN
      LET g_browser_idx = g_browser.getLength()
      LET g_current_idx = g_browser.getLength() 
   END IF
   
   # 設定browse索引
   CALL cl_navigator_setting(g_browser_idx, g_browser_cnt) 
 
   #代表沒有資料, 無需做後續資料撈取之動作
   IF g_current_idx = 0 THEN
      RETURN
   END IF
 
   #根據選定的筆數給予key欄位值
   LET g_mman_m.mman001 = g_browser[g_current_idx].b_mman001
 
                       
   #讀取單頭所有欄位資料
   EXECUTE ammm320_master_referesh USING g_mman_m.mman001 INTO g_mman_m.mman001,g_mman_m.mman002,g_mman_m.mman066, 
       g_mman_m.mmanunit,g_mman_m.mman003,g_mman_m.mman004,g_mman_m.mmanstus,g_mman_m.mmanownid,g_mman_m.mmanowndp, 
       g_mman_m.mmancrtid,g_mman_m.mmancrtdp,g_mman_m.mmancrtdt,g_mman_m.mmanmodid,g_mman_m.mmanmoddt, 
       g_mman_m.mmancnfid,g_mman_m.mmancnfdt,g_mman_m.mman005,g_mman_m.mman006,g_mman_m.mman007,g_mman_m.mman008, 
       g_mman_m.mman009,g_mman_m.mman010,g_mman_m.mman011,g_mman_m.mman012,g_mman_m.mman013,g_mman_m.mman014, 
       g_mman_m.mman015,g_mman_m.mman069,g_mman_m.mman016,g_mman_m.mman017,g_mman_m.mman018,g_mman_m.mman019, 
       g_mman_m.mman020,g_mman_m.mman021,g_mman_m.mman022,g_mman_m.mman023,g_mman_m.mman024,g_mman_m.mman025, 
       g_mman_m.mman062,g_mman_m.mman063,g_mman_m.mman026,g_mman_m.mman064,g_mman_m.mman053,g_mman_m.mman060, 
       g_mman_m.mman054,g_mman_m.mman061,g_mman_m.mman058,g_mman_m.mman072,g_mman_m.mman057,g_mman_m.mman073, 
       g_mman_m.mman059,g_mman_m.mman027,g_mman_m.mman028,g_mman_m.mman029,g_mman_m.mman030,g_mman_m.mman067, 
       g_mman_m.mman068,g_mman_m.mman036,g_mman_m.mman037,g_mman_m.mman031,g_mman_m.mman032,g_mman_m.mman033, 
       g_mman_m.mman034,g_mman_m.mman035,g_mman_m.mman070,g_mman_m.mman038,g_mman_m.mman039,g_mman_m.mman040, 
       g_mman_m.mman041,g_mman_m.mman055,g_mman_m.mman056,g_mman_m.mman042,g_mman_m.mman043,g_mman_m.mman065, 
       g_mman_m.mman044,g_mman_m.mman045,g_mman_m.mman046,g_mman_m.mman047,g_mman_m.mman048,g_mman_m.mman049, 
       g_mman_m.mman071,g_mman_m.mman050,g_mman_m.mman051,g_mman_m.mman052,g_mman_m.mmanunit_desc,g_mman_m.mman004_desc, 
       g_mman_m.mmanownid_desc,g_mman_m.mmanowndp_desc,g_mman_m.mmancrtid_desc,g_mman_m.mmancrtdp_desc, 
       g_mman_m.mmanmodid_desc,g_mman_m.mmancnfid_desc,g_mman_m.mman012_desc,g_mman_m.mman053_desc,g_mman_m.mman060_desc, 
       g_mman_m.mman054_desc,g_mman_m.mman058_desc,g_mman_m.mman057_desc
   
   #遮罩相關處理
   LET g_mman_m_mask_o.* =  g_mman_m.*
   CALL ammm320_mman_t_mask()
   LET g_mman_m_mask_n.* =  g_mman_m.*
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL ammm320_set_act_visible()
   CALL ammm320_set_act_no_visible()
 
   #add-point:fetch段action控制 name="fetch.action_control"
   {<point name="fetch.action_control"/>}
   #end add-point  
   
   
   
   #保存單頭舊值
   LET g_mman_m_t.* = g_mman_m.*
   LET g_mman_m_o.* = g_mman_m.*
   
