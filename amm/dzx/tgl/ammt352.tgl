      IF g_detail_idx > g_mmcc3_d.getLength() THEN
         LET g_detail_idx = g_mmcc3_d.getLength()
      END IF
      IF g_detail_idx = 0 AND g_mmcc3_d.getLength() <> 0 THEN
         LET g_detail_idx = 1
      END IF
