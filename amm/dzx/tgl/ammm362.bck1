      IF g_browser_cnt > 0 THEN
         CALL ammm362_fetch("P")
      ELSE
         CALL ammm362_browser_fill(" 1=1 ","F")
      END IF
      
   END IF
 
   CLOSE ammm362_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_mmby_m.mmbysite,"D")
 
