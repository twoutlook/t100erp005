   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_mmby_m.mmbysite IS NULL
      OR g_mmby_m.mmby001 IS NULL      OR g_mmby_m.mmby002 IS NULL
   THEN
