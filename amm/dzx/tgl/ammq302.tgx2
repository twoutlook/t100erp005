      LET g_detail_idx = 1
      LET g_detail_idx2 = 1
      CALL ammq302_b_fill()
   ELSE
      CALL ammq302_query()
   END IF
   
   WHILE TRUE
 
      IF g_action_choice = "logistics" THEN
