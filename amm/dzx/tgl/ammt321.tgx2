               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               #寫入修改者/修改日期資訊(單身)
               
      
               #將遮罩欄位還原
               CALL ammt321_mmbg_t_mask_restore('restore_mask_o')
      
               UPDATE mmbg_t SET (mmbgdocno,mmbg001,mmbg002,mmbg003,mmbg004,mmbg005,mmbg006,mmbg007, 
