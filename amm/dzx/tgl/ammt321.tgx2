       g_mmbf_m.mmbfcrtid,g_mmbf_m.mmbfcrtdp,g_mmbf_m.mmbfcrtdt,g_mmbf_m.mmbfmodid,g_mmbf_m.mmbfmoddt, 
       g_mmbf_m.mmbfcnfid,g_mmbf_m.mmbfcnfdt,g_mmbf_m.mmbfsite_desc,g_mmbf_m.mmbf001_desc
   
