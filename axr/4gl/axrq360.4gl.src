#+ 單身陣列填充
PRIVATE FUNCTION axrq360_b_fill()
   #add-point:b_fill段define-客製 name="b_fill.define_customerization"
   
   #end add-point
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準  (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   DEFINE l_xrca001       LIKE type_t.chr500
   DEFINE l_xrcastus      LIKE type_t.chr80
   DEFINE l_glaa024       LIKE glaa_t.glaa024  #160518-00075#3
   DEFINE l_n1            LIKE type_t.num5     #160518-00075#3
   DEFINE l_n2            LIKE type_t.num5     #160518-00075#3
   DEFINE l_ld_str        STRING  #160811-00009#2
   DEFINE l_comp_str      STRING  #160811-00009#2
   DEFINE l_sfin1002      LIKE type_t.chr1 #170213-00013#2 add
   #end add-point
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   #151231-00010#7--(S)
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF NOT cl_null(g_sql_ctrl) AND NOT g_sql_ctrl = ' 1=1'  THEN
      LET g_wc = g_wc," AND xrca004 IN (SELECT pmaa001 FROM pmaa_t WHERE pmaaent = ",g_enterprise, " AND ", g_sql_ctrl,")",
                      " AND xrca004 IN (SELECT pmab001 FROM pmab_t WHERE pmabent = ",g_enterprise," AND pmabsite IN (SELECT a.ooef001 FROM ooef_t a,ooef_t b WHERE a.ooefent = b.ooefent AND a.ooef017 = b.ooef017 AND a.ooefent = ",g_enterprise," AND a.ooef001 = '",g_site,"')) "
   END IF
   #151231-00010#7--(E)
   
   #160811-00009#2 add s---
   CALL s_axrt300_get_site(g_user,g_site_str,'2') RETURNING l_ld_str
   LET l_ld_str = cl_replace_str(l_ld_str,"glaald","xrcald")
   LET g_wc = g_wc," AND ",l_ld_str

   CALL s_axrt300_get_site(g_user,'','1') RETURNING l_comp_str
   LET l_comp_str = cl_replace_str(l_comp_str,"ooef001","xrcacomp")
   LET g_wc = g_wc," AND ",l_comp_str
   #160811-00009#2 add e---
   
   #160518-00075#3 add s---
   LET g_sql = " SELECT glaa024 FROM glaa_t WHERE glaaent = '",g_enterprise,"' AND glaald = ? "
   PREPARE axrq360_glaa_pre FROM g_sql

   LET g_sql = " SELECT COUNT(*) FROM oobc_t,ooba_t WHERE oobcent = oobaent AND oobc001 = ooba001 AND oobc002 = ooba002 ",
               "    AND oobc001 = ? AND oobc002 = substr(?,?,?) "
   PREPARE axrq360_oobc_pre1 FROM g_sql 

   LET g_sql = " SELECT COUNT(*) FROM oobc_t,ooba_t WHERE oobcent = oobaent AND oobc001 = ooba001 AND oobc002 = ooba002 ",  
               "            AND oobc001 = ? ",
               "            AND oobc002 = substr(?,?,?) ",
               "            AND (oobc003 IN (SELECT ooha001 FROM ooha_t,oohc_t WHERE oohaent = oohcent AND ooha001 = oohc001 ",
               "                                AND oohc002 = '",g_user,"' ",
               "                                AND oohastus = 'Y'",  
               "                                AND oohc003 <= '",g_today,"' AND (oohc004 IS NULL OR oohc004 > '",g_today,"') ",
               "                              UNION ",
               "                             SELECT ooha001 FROM ooha_t,oohb_t WHERE oohaent = oohbent AND ooha001 = oohb001 ",
               "                                AND oohb002 = '",g_dept,"' ",
               "                                AND oohastus = 'Y'",  
               "                                AND oohb003 <= '",g_today,"' AND (oohb004 IS NULL OR oohb004 > '",g_today,"')) ",
               "             OR ((oobc003 = '",g_user,"' AND oobc004 = '8') ",
               "             OR (oobc003 = '",g_dept,"' AND oobc004 = '7')))"
   PREPARE axrq360_oobc_pre2 FROM g_sql 
   #160518-00075#3 add e---
   #end add-point
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter, cl_sql_auth_filter()   #(ver:40) add cl_sql_auth_filter()
 
   LET ls_sql_rank = "SELECT  UNIQUE '',xrcastus,xrcasite,'',xrcacomp,'',xrcald,'',xrcadocno,xrcadocdt, 
       xrca001,xrca004,'',xrca057,'',xrca035,'',xrca038,xrca014,'',xrca015,'',xrca058,'',xrca007,'', 
       xrca040,'','','','','','','','','','','','','',xrca053  ,DENSE_RANK() OVER( ORDER BY xrca_t.xrcald, 
       xrca_t.xrcadocno) AS RANK FROM xrca_t",
 
 
                     "",
                     " WHERE xrcaent= ? AND 1=1 AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("xrca_t"),
                     " ORDER BY xrca_t.xrcald,xrca_t.xrcadocno"
 
   #add-point:b_fill段rank_sql_after name="b_fill.rank_sql_after"
   
   #end add-point
 
