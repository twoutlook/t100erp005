                      "    AND xrepld = xreqld AND xreq003 IN ('2','4','21')  AND ",l_wc, " AND ", l_wc2, cl_sql_add_filter("xrep_t")   #151008-00009#9 
   END IF
   #end add-point
   
   LET g_sql = " SELECT COUNT(1) FROM (",l_sub_sql,")"
   
