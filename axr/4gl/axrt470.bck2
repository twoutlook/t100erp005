################################################################################
# Descriptions...: 依調整後匯率/金額推算多本位幣金額
# Memo...........:
# Usage..........: CALL axrt470_rate_money(p_ld,p_rate1,p_rate2,p_rate3,p_money,p_xreq100,p_flag)
#                  RETURNING r_money1,r_money2,r_money3
# Input parameter: p_ld     帳套
#                : p_rate1  匯率一  
#                : p_rate2  匯率二
#                : p_rate3  匯率三
#                : p_money  原幣金額
# Date & Author..: Jessy By 160112
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt470_rate_money(p_ld,p_rate1,p_rate2,p_rate3,p_money,p_xreq100,p_flag)
DEFINE p_ld        LIKE glaa_t.glaald
DEFINE p_rate1     LIKE ooao_t.ooao005 #匯率1
DEFINE p_rate2     LIKE ooao_t.ooao005 #匯率2
DEFINE p_rate3     LIKE ooao_t.ooao005 #匯率3
DEFINE p_money     LIKE type_t.num20_6 #金額 
DEFINE p_xreq100   LIKE xreq_t.xreq100 #原幣幣別
DEFINE p_flag      LIKE type_t.chr1    #參數
DEFINE l_glaa001   LIKE glaa_t.glaa001 #本位幣一
DEFINE l_glaa016   LIKE glaa_t.glaa016 #本位幣二
DEFINE l_glaa017   LIKE glaa_t.glaa017 #本位幣二 換算基準
DEFINE l_glaa020   LIKE glaa_t.glaa020 #本未幣三
DEFINE l_glaa021   LIKE glaa_t.glaa021 #本位幣三 換算基準
DEFINE l_glaa015   LIKE glaa_t.glaa015 #本幣二啟用否
DEFINE l_glaa019   LIKE glaa_t.glaa019 #本幣三啟用否
DEFINE r_money1    LIKE type_t.num20_6 #本幣一金額
DEFINE r_money2    LIKE type_t.num20_6 #本幣二金額
DEFINE r_money3    LIKE type_t.num20_6 #本幣三金額
    
        
   CALL s_ld_sel_glaa(p_ld,'glaa001|glaa016|glaa017|glaa020|glaa021|glaa015|glaa019')
        RETURNING  g_sub_success,l_glaa001,l_glaa016,l_glaa017,l_glaa020,l_glaa021,l_glaa015,l_glaa019
   
   #匯率取位   
   CALL s_curr_round_ld('1',p_ld,p_xreq100,p_rate1,3) RETURNING g_sub_success,g_errno,p_rate1
   CALL s_curr_round_ld('1',p_ld,p_xreq100,p_rate2,3) RETURNING g_sub_success,g_errno,p_rate2
   CALL s_curr_round_ld('1',p_ld,p_xreq100,p_rate3,3) RETURNING g_sub_success,g_errno,p_rate3
   
   #p_flag = 1 >> 修改原幣金額/本一匯率
   #p_flag = 2 >> 修改本一金額
   #p_flag = 3 >> 修改本二匯率
   #p_flag = 4 >> 修改本三匯率
   
   CASE p_flag
      WHEN 1 
         #本位幣別一
         LET r_money1 = p_rate1 * p_money   
         
         #本位幣二
         IF g_glaa.glaa015 = 'Y' THEN
            IF l_glaa017 = '1' THEN #本位幣二換算基準 : 交易原幣              
               LET r_money2 = p_money * p_rate2   #由原幣乘算匯率           
            ELSE
               LET r_money2 = r_money1 * p_rate2  #由本幣乘算匯率
            END IF
         ELSE
            LET r_money2 = 0
         END IF            
         #本位幣三
         IF g_glaa.glaa019 = 'Y' THEN
            IF l_glaa021 = '1' THEN  #本位幣三換算基準 : 交易原幣
               LET r_money3 = p_money * p_rate3   #由原幣乘算匯率 
            ELSE
               LET r_money3 = r_money1 * p_rate3  #由本幣乘算匯率
            END IF
         ELSE
            LET r_money3 = 0
         END IF
         
      WHEN 2
         #本位幣別一
         LET r_money1 = g_xreq_d[l_ac].xreq113
         
         #本位幣二
         IF g_glaa.glaa015 = 'Y' THEN
            IF l_glaa017 = '1' THEN #本位幣二換算基準 : 交易原幣              
               LET r_money2 = p_money * p_rate2   #由原幣乘算匯率           
            ELSE
               LET r_money2 = r_money1 * p_rate2  #由本幣乘算匯率
            END IF   
         ELSE
            LET r_money2 = 0
         END IF 
         
         #本位幣三
         IF g_glaa.glaa019 = 'Y' THEN
            IF l_glaa021 = '1' THEN  #本位幣三換算基準 : 交易原幣
               LET r_money3 = p_money * p_rate3   #由原幣乘算匯率 
            ELSE
               LET r_money3 = r_money1 * p_rate3  #由本幣乘算匯率
            END IF  
         ELSE
            LET r_money3 = 0
         END IF
         
      WHEN 3
         #本位幣別一
         LET r_money1 = g_xreq_d[l_ac].xreq113
         #本位幣二
         IF g_glaa.glaa015 = 'Y' THEN
            IF l_glaa017 = '1' THEN #本位幣二換算基準 : 交易原幣              
               LET r_money2 = p_money * p_rate2   #由原幣乘算匯率           
            ELSE
               LET r_money2 = r_money1 * p_rate2  #由本幣乘算匯率
            END IF
         ELSE
            LET r_money2 = 0
         END IF         
         
         IF g_glaa.glaa019 = 'Y' THEN
            LET r_money3 = g_xreq3_d[l_ac].xreq133          
         ELSE
            LET r_money3 = 0
         END IF 



      WHEN 4
         #本位幣別一
         LET r_money1 = g_xreq_d[l_ac].xreq113
         #本位幣二
         IF g_glaa.glaa015 = 'Y' THEN
            LET r_money2 = g_xreq3_d[l_ac].xreq123 
         ELSE
            LET r_money2 = 0
         END IF
         
         #本位幣三
         IF g_glaa.glaa019 = 'Y' THEN
            IF l_glaa021 = '1' THEN  #本位幣三換算基準 : 交易原幣
               LET r_money3 = p_money * p_rate3   #由原幣乘算匯率 
            ELSE
               LET r_money3 = r_money1 * p_rate3  #由本幣乘算匯率
            END IF
         ELSE 
            LET r_money3 = 0
         END IF
   END CASE         

   #取位
   CALL s_curr_round_ld('1',p_ld,l_glaa001,r_money1,2) RETURNING g_sub_success,g_errno,r_money1
   CALL s_curr_round_ld('1',p_ld,l_glaa016,r_money2,2) RETURNING g_sub_success,g_errno,r_money2
   CALL s_curr_round_ld('1',p_ld,l_glaa020,r_money3,2) RETURNING g_sub_success,g_errno,r_money3
   IF cl_null(r_money1) THEN LET r_money1 = 0 END IF  
   IF cl_null(r_money2) THEN LET r_money2 = 0 END IF  
   IF cl_null(r_money3) THEN LET r_money3 = 0 END IF  
      
   RETURN r_money1,r_money2,r_money3
END FUNCTION

