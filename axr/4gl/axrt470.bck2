################################################################################
PRIVATE FUNCTION axrt470_set_ld_info(p_ld)
DEFINE p_ld       LIKE glaa_t.glaald

   CALL s_ld_sel_glaa(p_ld,'glaald|glaacomp|glaa001|glaa004|glaa005|glaa015|glaa016|glaa017|glaa019|glaa020|glaa021|glaa024|glaa025|glaa026|glaa121')
        RETURNING g_sub_success,g_glaa.*   

   #是否啟用分錄底稿
   IF g_glaa.glaa121 = 'Y' THEN
      CALL cl_set_toolbaritem_visible("open_pre", TRUE)
   ELSE
      CALL cl_set_toolbaritem_visible("open_pre", FALSE)
   END IF
   
   #其他本位幣啟用否
   IF g_glaa.glaa015 = 'Y' THEN
      CALL cl_set_comp_visible('xreq121,xreq123',TRUE)
   ELSE
      CALL cl_set_comp_visible('xreq121,xreq123',FALSE)
   END IF
   IF g_glaa.glaa019 = 'Y' THEN
      CALL cl_set_comp_visible('xreq131,xreq133',TRUE)
   ELSE
      CALL cl_set_comp_visible('xreq131,xreq133',FALSE)
   END IF 
   
   IF g_glaa.glaa015 = 'N' AND g_glaa.glaa019 = 'N' THEN
      CALL cl_set_comp_visible('page_4',FALSE)    #頁籤隱藏   
   ELSE
      CALL cl_set_comp_visible('page_4',TRUE)     #頁籤顯示
   END IF   
   
END FUNCTION

################################################################################
# Descriptions...: 取得本幣後，亦須檢核是否超沖
# Memo...........:
# Usage..........: CALL axrt470_money_chk(p_xreq113,p_xreq123,p_xreq133)
# Date & Author..: 160216 By Jessy
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt470_money_chk(p_xreq113,p_xreq123,p_xreq133)
DEFINE p_xreq113 LIKE xreq_t.xreq113
DEFINE p_xreq123 LIKE xreq_t.xreq123
DEFINE p_xreq133 LIKE xreq_t.xreq133
DEFINE l_xreq113 LIKE xreq_t.xreq113
DEFINE l_xreq123 LIKE xreq_t.xreq123
DEFINE l_xreq133 LIKE xreq_t.xreq133
DEFINE l_glaa015 LIKE glaa_t.glaa015
DEFINE l_glaa019 LIKE glaa_t.glaa019
DEFINE l_xreq042 LIKE xreq_t.xreq042
DEFINE l_xreq043 LIKE xreq_t.xreq043
DEFINE l_xreq044 LIKE xreq_t.xreq044
DEFINE r_success LIKE type_t.num5

   LET r_success = TRUE

   CALL s_ld_sel_glaa(g_xrep_m.xrepld ,'glaa015|glaa019')
        RETURNING g_sub_success,l_glaa015,l_glaa019
   
   #取xreq042(遞延認列交易本幣金額)---------------
   SELECT xreq042 INTO l_xreq042 FROM xrep_t,xreq_t 
    WHERE xreqent = g_enterprise 
      AND xreqld = g_xrep_m.xrepld 
      AND xrepent = xreqent 
      AND xrepdocno = xreqdocno 
      AND xrepld = xreqld
      AND xreq004 = g_xreq_d[l_ac].xreq004 
      AND xreq005 = g_xreq_d[l_ac].xreq005 
      AND xreq006 = g_xreq_d[l_ac].xreq006
      AND xreq003 IN ('1','3') 
      AND xrepstus = 'Y'
      
   #取xreq043(遞延認列交易本幣二金額)---------------
   SELECT xreq043 INTO l_xreq043 FROM xrep_t,xreq_t 
    WHERE xreqent = g_enterprise 
      AND xreqld = g_xrep_m.xrepld 
      AND xrepent = xreqent 
      AND xrepdocno = xreqdocno 
      AND xrepld = xreqld
      AND xreq004 = g_xreq_d[l_ac].xreq004 
      AND xreq005 = g_xreq_d[l_ac].xreq005 
      AND xreq006 = g_xreq_d[l_ac].xreq006
      AND xreq003 IN ('1','3') 
      AND xrepstus = 'Y'
      
   #取xreq044(遞延認列交易本幣三金額)---------------
   SELECT xreq044 INTO l_xreq044 FROM xrep_t,xreq_t 
    WHERE xreqent = g_enterprise 
      AND xreqld = g_xrep_m.xrepld 
      AND xrepent = xreqent 
      AND xrepdocno = xreqdocno 
      AND xrepld = xreqld
      AND xreq004 = g_xreq_d[l_ac].xreq004 
      AND xreq005 = g_xreq_d[l_ac].xreq005 
      AND xreq006 = g_xreq_d[l_ac].xreq006
      AND xreq003 IN ('1','3') 
      AND xrepstus = 'Y'
      
      
   #本位幣一金額----------------- 
   #前期已實現本幣金額
   SELECT SUM(xreq113) INTO l_xreq113 FROM xrep_t,xreq_t 
    WHERE xreqent = g_enterprise AND xreqld = g_xrep_m.xrepld 
      AND xrepent = xreqent AND xrepdocno = xreqdocno AND xrepld = xreqld
      AND ((xreq001 = g_xrep_m.xrep001 AND xreq002 < g_xrep_m.xrep002 ) OR (xreq001 < g_xrep_m.xrep001)) 
      AND xreq004 = g_xreq_d[l_ac].xreq004 AND xreq005 = g_xreq_d[l_ac].xreq005 AND xreq006 = g_xreq_d[l_ac].xreq006
     #AND xreq003 IN ('2','4') AND xrepstus = 'Y'   #151008-00009#9 mark
      AND xreq003 IN ('2','4','21') AND xrepstus = 'Y'   #151008-00009#9 
   IF cl_null(l_xreq113) THEN LET l_xreq113 = 0 END IF
   LET l_xreq113 = l_xreq113 + p_xreq113
      
   #合計不可大於xreq042,超出時警訊並不可輸入   
   IF l_xreq113 > l_xreq042 THEN
      LET r_success = FALSE
      RETURN r_success
   END IF

   #本位幣二金額----------------- 
   IF l_glaa015 = 'Y' THEN
      #前期已實現本幣金額
      SELECT SUM(xreq123) INTO l_xreq123 FROM xrep_t,xreq_t 
       WHERE xreqent = g_enterprise AND xreqld = g_xrep_m.xrepld 
         AND xrepent = xreqent AND xrepdocno = xreqdocno AND xrepld = xreqld
         AND ((xreq001 = g_xrep_m.xrep001 AND xreq002 < g_xrep_m.xrep002 ) OR (xreq001 < g_xrep_m.xrep001))
         AND xreq004 = g_xreq_d[l_ac].xreq004 AND xreq005 = g_xreq_d[l_ac].xreq005 AND xreq006 = g_xreq_d[l_ac].xreq006
        #AND xreq003 IN ('2','4') AND xrepstus = 'Y'        #151008-00009#9  mark
         AND xreq003 IN ('2','4','21') AND xrepstus = 'Y'   #151008-00009#9 
       
      IF cl_null(l_xreq123) THEN LET l_xreq123 = 0 END IF
      LET l_xreq123 = l_xreq123 + p_xreq123
       
      #合計不可大於xreq043,超出時警訊並不可輸入
      IF l_xreq123 > l_xreq043 THEN
         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF

   #本位幣三金額----------------- 
   IF l_glaa019 = 'Y' THEN   
      #前期已實現本幣金額
      SELECT SUM(xreq133) INTO l_xreq133 FROM xrep_t,xreq_t 
       WHERE xreqent = g_enterprise AND xreqld = g_xrep_m.xrepld 
         AND xrepent = xreqent AND xrepdocno = xreqdocno AND xrepld = xreqld
         AND ((xreq001 = g_xrep_m.xrep001 AND xreq002 < g_xrep_m.xrep002 ) OR (xreq001 < g_xrep_m.xrep001))
         AND xreq004 = g_xreq_d[l_ac].xreq004 AND xreq005 = g_xreq_d[l_ac].xreq005 AND xreq006 = g_xreq_d[l_ac].xreq006
        #AND xreq003 IN ('2','4') AND xrepstus = 'Y'        #151008-00009#9  mark
         AND xreq003 IN ('2','4','21') AND xrepstus = 'Y'   #151008-00009#9 
       
      IF cl_null(l_xreq133) THEN LET l_xreq133 = 0 END IF
      LET l_xreq133 = l_xreq133 + p_xreq133
       
      #合計不可大於xreq044(遞延認列交易本幣金額),超出時警訊並不可輸入
      IF l_xreq133 > l_xreq044 THEN
         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF
   
   RETURN r_success
END FUNCTION

 
