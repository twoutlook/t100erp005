                     #" LEFT JOIN xreq_t ON xreqent = xrepent AND xrepdocno = xreqdocno AND xrepld = xreqld AND xreq003 IN ('2','4')  ", "  ",    #151008-00009#9 mark
                      " LEFT JOIN xreq_t ON xreqent = xrepent AND xrepdocno = xreqdocno AND xrepld = xreqld AND xreq003 IN ('2','4','21')  ", "  ",    #151008-00009#9 
                      " WHERE xrepent = '" ||g_enterprise|| "' AND xreqent = '" ||g_enterprise|| "' AND ",l_wc, " AND ", l_wc2, cl_sql_add_filter("xrep_t")
   ELSE
      #單身未輸入搜尋條件
      LET l_sub_sql = " SELECT UNIQUE xrepdocno,xrepld ",
                      "   FROM xrep_t,xreq_t ",
                      "  WHERE xrepent = '" ||g_enterprise|| "' AND xreqent = '" ||g_enterprise|| "' AND xreqent = xrepent AND xrepdocno = xreqdocno ",
