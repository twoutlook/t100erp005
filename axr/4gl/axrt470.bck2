                      " FROM xrep_t ", 
                      "  ",
                      "  ",
                      " WHERE xrepent = " ||g_enterprise|| " AND ",l_wc CLIPPED, cl_sql_add_filter("xrep_t")
   END IF
   
   #add-point:browser_fill,cnt wc name="browser_fill.cnt_sqlwc"
