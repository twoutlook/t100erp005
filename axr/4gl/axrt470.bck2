      LET l_sub_sql = " SELECT DISTINCT xrepdocno,xrepld ",
                      " FROM xrep_t ",
                      " ",
                      " LEFT JOIN xreq_t ON xreqent = xrepent AND xrepdocno = xreqdocno AND xrepld = xreqld ", "  ",
                      #add-point:browser_fill段sql(xreq_t1) name="browser_fill.cnt.join.}"
                      
                      #end add-point
 
 
