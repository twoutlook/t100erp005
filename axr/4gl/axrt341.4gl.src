   IF cl_null(g_xrcb_d[l_ac].xrcb020) THEN RETURN END IF

  CALL s_tax_get_ooef019(g_glaa.glaacomp) RETURNING l_success,l_ooef019
  CALL s_tax_chk(g_glaa.glaacomp,g_xrcb_d[l_ac].xrcb020)
     RETURNING l_success,l_oodbl004,l_oodb005,l_oodb006,l_oodb011

   SELECT oodb004 INTO l_oodb004 FROM oodb_t
    WHERE oodb002  = g_xrcb_d[l_ac].xrcb020
      AND oodb001  = l_ooef019
      AND oodbent  = g_enterprise

  CALL cl_set_comp_entry('xrcb103,xrcb104,xrcb105,xrcb113,xrcb114,xrcb115',TRUE)

  IF l_oodb004 = '2' THEN
     #複合稅:未稅不可錄入稅額、含稅金額;含稅不可錄入稅額、未稅金額
     IF l_oodb005 = 'Y' THEN
        CALL cl_set_comp_entry('xrcb103,xrcb104,xrcb113,xrcb114',FALSE)
     ELSE
        CALL cl_set_comp_entry('xrcb104,xrcb105,xrcb114,xrcb115',FALSE)
     END IF
  END IF

