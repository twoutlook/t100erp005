################################################################################
# Descriptions...: 銷退單可用性檢查
# Memo...........:
# Usage..........: CALL axrt341_axmt600_chk()
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 2014/04/02 By zhangwei
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt341_axmt600_chk(p_xrcb002,p_xrcb003)
   DEFINE p_xrcb002       LIKE xrcb_t.xrcb002
   DEFINE p_xrcb003       LIKE xrcb_t.xrcb003
   DEFINE l_count         LIKE type_t.num5
   DEFINE l_sql           STRING
   DEFINE l_s             LIKE type_t.chr1
   DEFINE l_xmdl038       LIKE xmdl_t.xmdl038
   DEFINE l_success       LIKE type_t.chr1

   #如果銷退項次為0表示由單頭進入函數，需要整批插入單身資料
   #如果銷退項次不為0表示由單身進入函數，需要搜索某筆資料顯示

   #STEP1.檢查單據存在性
   #STEP2.檢查單據可用性---是否存在可沖金額
   
   IF p_xrcb003 = 0 THEN
      #STEP1
      LET l_count = 0
      SELECT COUNT(*) INTO l_count FROM xmdl_t,xmdk_t WHERE xmdlent = g_enterprise
         AND xmdkent = xmdlent AND xmdkdocno = xmdldocno
         AND xmdldocno = g_xrcb_d[l_ac].xrcb002
         AND xmdk000 = '6'    #銷退性質  (axmt600未新增，暫時給3)
      IF cl_null(l_count) THEN LET l_count = 0 END IF
      
      IF l_count != 1 THEN LET g_errno = 'axr-00097' RETURN END IF   #輸入的銷退單號不存在!


      #STEP2
      CALL s_axrt300_sel_ld(g_xrca_m.xrcald) RETURNING l_success,l_s   #獲取帳套主/次
      
      LET l_sql = "SELECT SUM(CASE WHEN '",l_s,"' = '1' THEN xmdl022 - xmdl038              ",
                  "                WHEN '",l_s,"' = '2' THEN xmdl022 - xmdl039              ",
                  "                ELSE xmdl022 - xmdl040 END )                             ",
                  "  FROM (SELECT xmdk000,xmdldocno,xmdlseq,xmdl008,xmdl022,xmdl021,xmdl024,",
                  "               CASE WHEN xmdl038 IS NULL THEN 0 ELSE xmdl038 END xmdl038,",
                  "               CASE WHEN xmdl039 IS NULL THEN 0 ELSE xmdl039 END xmdl039,",
                  "               CASE WHEN xmdl040 IS NULL THEN 0 ELSE xmdl040 END xmdl040 ",
                  "          FROM xmdl_t, xmdk_t                                            ",
                  "         WHERE     xmdkdocno = xmdldocno                                 ",
                  "               AND xmdkdocdt <= '",g_xrca_m.xrcadocdt,"'                 ",
                  "               AND xmdkstus = 'S'                                        ",
                  "               AND xmdk008 = '",g_xrca_m.xrca004,"')                     ",
                  "               AND xmdldocno = '",p_xrcb002,"'                           ",
                  "               AND xmdlseq = '",p_xrcb003,"'                             "
      PREPARE axrt341_prep5 FROM l_sql
      EXECUTE axrt341_prep5 INTO l_xmdl038
      
      IF l_xmdl038 <= 0 THEN LET g_errno = 'axm-00205' RETURN END IF   #銷退單無可沖金額!
   ELSE
      #STEP1
      LET l_count = 0
      SELECT COUNT(*) INTO l_count FROM xmdl_t,xmdk_t WHERE xmdlent = g_enterprise
         AND xmdkent = xmdlent AND xmdkdocno = xmdldocno
         AND xmdldocno = g_xrcb_d[l_ac].xrcb002 AND xmdlseq = g_xrcb_d[l_ac].xrcb003
         AND xmdk000 = '3'    #銷退性質  (axmt600未新增，暫時給3)
      IF cl_null(l_count) THEN LET l_count = 0 END IF
      
      IF l_count != 1 THEN LET g_errno = 'axm-00204' RETURN END IF   #單號+項次不存在於銷退單中!

      #STEP2
      CALL s_axrt300_sel_ld(g_xrca_m.xrcald) RETURNING l_success,l_s   #獲取帳套主/次
      
      LET l_sql = "SELECT SUM(CASE WHEN '",l_s,"' = '1' THEN xmdl022 - xmdl038              ",
                  "                WHEN '",l_s,"' = '2' THEN xmdl022 - xmdl039              ",
                  "                ELSE xmdl022 - xmdl040 END )                             ",
                  "  FROM (SELECT xmdk000,xmdldocno,xmdlseq,xmdl008,xmdl022,xmdl021,xmdl024,",
                  "               CASE WHEN xmdl038 IS NULL THEN 0 ELSE xmdl038 END xmdl038,",
                  "               CASE WHEN xmdl039 IS NULL THEN 0 ELSE xmdl039 END xmdl039,",
                  "               CASE WHEN xmdl040 IS NULL THEN 0 ELSE xmdl040 END xmdl040 ",
                  "          FROM xmdl_t, xmdk_t                                            ",
                  "         WHERE     xmdkdocno = xmdldocno                                 ",
                  "               AND xmdkdocdt <= '",g_xrca_m.xrcadocdt,"'                 ",
                  "               AND xmdkstus = 'S'                                        ",
                  "               AND xmdk008 = '",g_xrca_m.xrca004,"')                     ",
                  "               AND xmdldocno = '",p_xrcb002,"'                           ",
                  "               AND xmdlseq = '",p_xrcb003,"'                             "
      PREPARE axrt341_prep6 FROM l_sql
      EXECUTE axrt341_prep6 INTO l_xmdl038
      
      IF l_xmdl038 <= 0 THEN LET g_errno = 'axm-00205' RETURN END IF   #銷退單無可沖金額!
   END IF
   
   LET g_errno = ' ' RETURN

END FUNCTION

