      CALL s_fin_get_doc_para(g_xrca_m.xrcald,g_xrca_m.xrcacomp,l_slip,'D-FIN-0031') RETURNING l_dfin0031
      CALL s_fin_get_doc_para(g_xrca_m.xrcald,g_xrca_m.xrcacomp,l_slip,'D-FIN-0032') RETURNING l_dfin0032
      IF NOT cl_null(l_dfin0031) AND l_dfin0031 MATCHES '[Yy]' THEN 
         IF cl_ask_confirm('aap-00403') THEN
            CALL s_transaction_begin()
            CALL s_axrt300_immediately_conf(g_xrca_m.xrcald,g_xrca_m.xrcacomp,g_xrca_m.xrcadocno,g_xrca_m.xrcamoddt,g_xrca_m.xrcacnfdt) RETURNING l_success
            IF l_success = 'N' THEN
               CALL s_transaction_end('N',0)
            ELSE
               CALL s_transaction_end('Y',0)
               IF NOT cl_null(l_dfin0032) AND l_dfin0032 MATCHES '[Yy]' THEN 
                  IF cl_ask_confirm('axr-00888') THEN              
                     CALL s_axrt300_immediately_gen(g_xrca_m.xrcald,g_xrca_m.xrcacomp,g_xrca_m.xrcadocno,g_xrca_m.xrcasite,g_xrca_m.xrcadocdt,g_xrca_m.xrca038)
                  END IF 
                END IF 
            END IF 
         END IF 
      END IF
     ##151125-00006#1-add-e
   END IF
