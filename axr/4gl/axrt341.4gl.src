# Descriptions...: 修改單頭匯率、原幣/本幣金額後金額刷新
# Memo...........:
# Usage..........: CALL axrt341_head_amount_refresh(p_chr)
#                  RETURNING ---
# Input parameter: p_chr          修改內容  1.修改原幣幣別 2.修改原幣匯率、金額 3.修改本位幣金額
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 2014/03/13 By zhangwei
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt341_head_amount_refresh(p_chr)
   DEFINE p_chr      LIKE type_t.chr1

   #單頭金額修改後處理邏輯:
   #CASE1.修改原幣幣別:異動本幣金額;異動其他本位幣金額
   #CASE2.修改原幣匯率:異動本幣金額;依照其他本位幣匯率參考依據決定是否異動其他本位幣金額
   #CASE3.修改本幣金額:依照其他本位幣匯率參考依據決定是否異動其他本位幣金額

   CASE
      WHEN p_chr = '1'      #CASE1
         #計算本位幣金額
         CALL axrt341_exrate(g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa001,g_xrca_m.xrca103,g_xrca_m.xrca101)
            RETURNING g_xrca_m.xrca113
         CALL axrt341_exrate(g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa001,g_xrca_m.xrca104,g_xrca_m.xrca101)
            RETURNING g_xrca_m.xrca114
         IF g_glaa.glaa015 = 'Y' THEN
            #計算本位幣二金額
            IF g_glaa.glaa017 = '1' THEN
               #换算基准:交易原幣
               CALL axrt341_exrate(g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa016,g_xrca_m.xrca103,g_xrca_m.xrca121)
                  RETURNING g_xrca_m.xrca123
               CALL axrt341_exrate(g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa016,g_xrca_m.xrca104,g_xrca_m.xrca121)
                  RETURNING g_xrca_m.xrca124
            ELSE
               #换算基准:帳簿幣別
               CALL axrt341_exrate(g_xrca_m.xrcadocdt,g_glaa.glaa001,g_glaa.glaa016,g_xrca_m.xrca113,g_xrca_m.xrca121)
                  RETURNING g_xrca_m.xrca123
               CALL axrt341_exrate(g_xrca_m.xrcadocdt,g_glaa.glaa001,g_glaa.glaa016,g_xrca_m.xrca114,g_xrca_m.xrca121)
                  RETURNING g_xrca_m.xrca124
            END IF
         END IF
         IF g_glaa.glaa019 = 'Y' THEN
            #計算本位幣三金額
            IF g_glaa.glaa021 = '1' THEN
               #换算基准:交易原幣
               CALL axrt341_exrate(g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa020,g_xrca_m.xrca103,g_xrca_m.xrca131)
                  RETURNING g_xrca_m.xrca133
               CALL axrt341_exrate(g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa020,g_xrca_m.xrca104,g_xrca_m.xrca131)
                  RETURNING g_xrca_m.xrca134
            ELSE
               #换算基准:帳簿幣別
               CALL axrt341_exrate(g_xrca_m.xrcadocdt,g_glaa.glaa001,g_glaa.glaa020,g_xrca_m.xrca113,g_xrca_m.xrca131)
                  RETURNING g_xrca_m.xrca133
               CALL axrt341_exrate(g_xrca_m.xrcadocdt,g_glaa.glaa001,g_glaa.glaa020,g_xrca_m.xrca114,g_xrca_m.xrca131)
                  RETURNING g_xrca_m.xrca134
            END IF
         END IF
      WHEN p_chr = '2'      #CASE2
         #計算本位幣金額
         CALL axrt341_exrate(g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa001,g_xrca_m.xrca103,g_xrca_m.xrca101)
            RETURNING g_xrca_m.xrca113
         CALL axrt341_exrate(g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa001,g_xrca_m.xrca104,g_xrca_m.xrca101)
            RETURNING g_xrca_m.xrca114
         IF g_glaa.glaa015 = 'Y' THEN
            #計算本位幣二金額
            IF g_glaa.glaa017 = '2' THEN
               #换算基准:帳簿幣別
               CALL axrt341_exrate(g_xrca_m.xrcadocdt,g_glaa.glaa001,g_glaa.glaa016,g_xrca_m.xrca113,g_xrca_m.xrca121)
                  RETURNING g_xrca_m.xrca123
               CALL axrt341_exrate(g_xrca_m.xrcadocdt,g_glaa.glaa001,g_glaa.glaa016,g_xrca_m.xrca114,g_xrca_m.xrca121)
                  RETURNING g_xrca_m.xrca124
            END IF
         END IF
         IF g_glaa.glaa019 = 'Y' THEN
            #計算本位幣三金額
            IF g_glaa.glaa021 = '2' THEN
               #换算基准:帳簿幣別
               CALL axrt341_exrate(g_xrca_m.xrcadocdt,g_glaa.glaa001,g_glaa.glaa020,g_xrca_m.xrca113,g_xrca_m.xrca131)
                  RETURNING g_xrca_m.xrca133
               CALL axrt341_exrate(g_xrca_m.xrcadocdt,g_glaa.glaa001,g_glaa.glaa020,g_xrca_m.xrca114,g_xrca_m.xrca131)
                  RETURNING g_xrca_m.xrca134
            END IF
         END IF
      WHEN p_chr = '3'      #CASE3
         IF g_glaa.glaa015 = 'Y' THEN
            #計算本位幣二金額
            IF g_glaa.glaa017 = '1' THEN
               #换算基准:交易原幣
               CALL axrt341_exrate(g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa016,g_xrca_m.xrca103,g_xrca_m.xrca121)
                  RETURNING g_xrca_m.xrca123
               CALL axrt341_exrate(g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa016,g_xrca_m.xrca104,g_xrca_m.xrca121)
                  RETURNING g_xrca_m.xrca124
            END IF
         END IF
         IF g_glaa.glaa019 = 'Y' THEN
            #計算本位幣三金額
            IF g_glaa.glaa021 = '1' THEN
               #换算基准:交易原幣
               CALL axrt341_exrate(g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa020,g_xrca_m.xrca103,g_xrca_m.xrca131)
                  RETURNING g_xrca_m.xrca133
               CALL axrt341_exrate(g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa020,g_xrca_m.xrca104,g_xrca_m.xrca131)
                  RETURNING g_xrca_m.xrca134
            END IF
         END IF
   END CASE
   
   DISPLAY BY NAME g_xrca_m.xrca113,g_xrca_m.xrca114,g_xrca_m.xrca123,g_xrca_m.xrca134,g_xrca_m.xrca133,g_xrca_m.xrca134
   
