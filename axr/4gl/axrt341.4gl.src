      #是否抛傳票
      CALL s_fin_get_doc_para(g_xrca_m.xrcald,g_xrca_m.xrcacomp,l_slip,'D-FIN-0030') RETURNING l_dfin0030
      IF g_glaa.glaa121 = 'Y' AND l_dfin0030 = 'Y' THEN
         IF s_axrt300_xrcb105_chk(g_xrca_m.xrcald,g_xrca_m.xrcadocno) THEN #160111-00020#1 Add
            CALL s_transaction_begin()
            CALL s_pre_voucher_ins('AR','R10',g_glaa.glaald,g_xrca_m.xrcadocno,g_xrca_m.xrcadocdt,'1') RETURNING l_success
            IF NOT l_success THEN
               CALL s_transaction_end('N',0)
            END IF
         END IF #160111-00020#1 Add
      END IF
