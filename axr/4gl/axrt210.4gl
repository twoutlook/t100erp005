PRIVATE FUNCTION axrt210_xrcb_chk()
   DEFINE l_amt         LIKE xrcb_t.xrcb105
   DEFINE r_success     LIKE type_t.chr1

   LET r_success = 'Y'
   LET l_amt = 0
   SELECT SUM(xrcb105 * xrcb022) INTO l_amt FROM xrcb_t WHERE xrcbent = g_enterprise
      AND xrcbdocno = g_xrca_m.xrcadocno AND xrcbld = g_xrca_m.xrcald
