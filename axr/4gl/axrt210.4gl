   CALL s_fin_account_center_comp_str() RETURNING ls_wc
   #將取回的字串轉換為SQL條件
   CALL axrt210_get_ooef001_wc(ls_wc) RETURNING ls_wc

   LET r_success = FALSE

   LET l_sql = "SELECT 'Y' FROM stbe_t,stae_t WHERE stbeent = '",g_enterprise,"'",
               "   AND stbesite IN ",ls_wc CLIPPED,
               "   AND ",p_sql CLIPPED,
               "   AND stbe016 - stbe037 > 0 ",   #160509-00004#41 change stbe021 to stbe037 lujh
               "   AND stbeent = staeent",
               "   AND stae001 = stbe005",
               "   AND stbe024 = 'Y'",
               "   AND stbe025 = 'N'",
               "   AND stbe001 = '3'"
   PREPARE axrt210_stbe_prep FROM l_sql
