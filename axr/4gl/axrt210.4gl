# Descriptions...: 根據稅別設置欄位開啟否
# Memo...........:
# Usage..........: CALL axrt210_tax_entry()
#                  RETURNING ---
# Input parameter: 
# Return code....: 
# Date & Author..: 2015/02/02 By zhangwei
# Modify.........: 20120202#1
################################################################################
PRIVATE FUNCTION axrt210_tax_entry()
   DEFINE l_success         LIKE type_t.num5
   DEFINE l_ooef019         LIKE ooef_t.ooef019
   DEFINE l_oodbl004        LIKE oodbl_t.oodbl004
   DEFINE l_oodb005         LIKE oodb_t.oodb005
   DEFINE l_oodb006         LIKE oodb_t.oodb006
   DEFINE l_oodb011         LIKE oodb_t.oodb011
   DEFINE l_oodb004         LIKE oodb_t.oodb004

   IF cl_null(g_xrcb_d[l_ac].xrcb020) THEN RETURN END IF

  CALL s_tax_get_ooef019(g_glaa.glaacomp) RETURNING l_success,l_ooef019
  CALL s_tax_chk(g_glaa.glaacomp,g_xrcb_d[l_ac].xrcb020)
     RETURNING l_success,l_oodbl004,l_oodb005,l_oodb006,l_oodb011

   SELECT oodb004 INTO l_oodb004 FROM oodb_t
    WHERE oodb002  = g_xrcb_d[l_ac].xrcb020
      AND oodb001  = l_ooef019
      AND oodbent  = g_enterprise

  CALL cl_set_comp_entry('xrcb103,xrcb104,xrcb105,xrcb113,xrcb114,xrcb115',TRUE)

  IF l_oodb004 = '2' THEN
     #複合稅:未稅不可錄入稅額、含稅金額;含稅不可錄入稅額、未稅金額
     IF l_oodb005 = 'Y' THEN
        CALL cl_set_comp_entry('xrcb103,xrcb104,xrcb113,xrcb114',FALSE)
     ELSE
        CALL cl_set_comp_entry('xrcb104,xrcb105,xrcb114,xrcb115',FALSE)
     END IF
  END IF

END FUNCTION

################################################################################
# Descriptions...: 更新astq330
# Memo...........:
# Usage..........: CALL axrt210_upd_stbc(p_chr)
#                  RETURNING r_success
# Input parameter: p_chr          状态  +/-
# Return code....: r_success      成功否
# Date & Author..: 2015/06/03 By 01727
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt210_upd_stbc(p_chr)
   DEFINE p_chr      LIKE type_t.chr1  #160509-00004 by liyan
   DEFINE l_sql      STRING
   DEFINE l_xrcborga LIKE xrcb_t.xrcborga
   DEFINE l_xrcb008  LIKE xrcb_t.xrcb008
   DEFINE l_xrcb009  LIKE xrcb_t.xrcb009
   DEFINE l_xrcb105  LIKE xrcb_t.xrcb105
   DEFINE r_success  LIKE type_t.num5

   LET l_sql = "SELECT xrcborga,xrcb008,xrcb009,xrcb105 FROM xrcb_t WHERE xrcbent = '",g_enterprise,"'",
              #"   AND xrcbdocno = '",g_xrca_m.xrcadocno,"' AND xrcbld = '",g_xrca_m.xrcald,"'"   #151201-00002#7 Mark
               "   AND xrcbdocno = '",g_xrca_m.xrcadocno,"' AND xrcbld = '",g_xrca_m.xrcald,"' AND xrcb001 = '3'"   #151201-00002#7 Add
   PREPARE axrt210_stbc_prep FROM l_sql
   DECLARE axrt210_stbc_curs CURSOR FOR axrt210_stbc_prep

   LET r_success = TRUE

   FOREACH axrt210_stbc_curs INTO l_xrcborga,l_xrcb008,l_xrcb009,l_xrcb105
      IF p_chr = '+' THEN
         UPDATE stbc_t SET stbc061 = COALESCE(stbc061,0) + l_xrcb105     #160509-00004#41 change stbc024 to stbc061 lujh
          WHERE stbcent = g_enterprise
            AND stbc004 = l_xrcb008
            AND stbc005 = l_xrcb009
      ELSE
         UPDATE stbc_t SET stbc061 = COALESCE(stbc061,0) - l_xrcb105     #160509-00004#41 change stbc024 to stbc061 lujh
          WHERE stbcent = g_enterprise
            AND stbc004 = l_xrcb008
            AND stbc005 = l_xrcb009
      END IF
      IF SQLCA.SQLERRD[3] <> 1 THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = ''
         LET g_errparam.code   = 'axr-00343'
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         LET r_success = FALSE
      END IF
   END FOREACH

   RETURN r_success

END FUNCTION

################################################################################
# Descriptions...: 新增/修改/复制单据后立即审核
# Memo...........:
# Usage..........: CALL axrt210_immediately_conf()
#                  RETURNING r_success
# Date & Author..: 2015/12/03 By 06862
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt210_immediately_conf()
   DEFINE l_success         LIKE type_t.num5
   DEFINE l_doc_success     LIKE type_t.chr1
   DEFINE l_slip            LIKE ooba_t.ooba002
   DEFINE l_dfin0031        LIKE type_t.chr1
   DEFINE l_count           LIKE type_t.num5
   DEFINE r_success         LIKE type_t.chr1

   LET r_success = 'Y' 
    #無資料直接返回不做處理
   IF cl_null(g_xrca_m.xrcald)  THEN 
      LET r_success='N'
      RETURN r_success 
   END IF
   #無資料直接返回不做處理   
   IF cl_null(g_xrca_m.xrcacomp)  THEN 
      LET r_success='N'
      RETURN r_success 
   END IF
   #無資料直接返回不做處理   
   IF cl_null(g_xrca_m.xrcadocno) THEN 
      LET r_success='N'
      RETURN r_success 
   END IF     

   LET l_count = 0
   SELECT COUNT(*) INTO l_count FROM xrcb_t WHERE xrcbent = g_enterprise
      AND xrcbdocno = g_xrca_m.xrcadocno
      AND xrcbld = g_xrca_m.xrcald
      
   IF cl_null(l_count) THEN LET l_count = 0 END IF
   IF l_count = 0 THEN 
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'aec-00020'
      LET g_errparam.extend = g_xrca_m.xrcadocno
      LET g_errparam.popup = TRUE
      CALL cl_err()
      LET r_success='N'
      RETURN r_success         
   END IF                   #無資料直接返回不做處理
   
   CALL cl_err_collect_init()
   CALL s_transaction_begin()
   
   LET l_doc_success = 'Y'
   
   CALL axrt210_xrcb_chk() RETURNING l_doc_success                              
   
   CALL s_axrt300_confirm(g_xrca_m.xrcald,g_xrca_m.xrcadocno) RETURNING l_doc_success
   
   CALL axrt210_upd_stbc('+') RETURNING l_doc_success

   UPDATE xrca_t SET xrcastus = 'Y',
                     xrcamodid= g_user,
                     xrcamoddt= g_xrca_m.xrcamoddt,
                     xrcacnfid= g_user,
                     xrcacnfdt= g_xrca_m.xrcacnfdt
    WHERE xrcaent = g_enterprise AND xrcald = g_xrca_m.xrcald
      AND xrcadocno = g_xrca_m.xrcadocno
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      LET l_doc_success = 'N'
   END IF
  
   IF l_doc_success THEN
      CALL s_transaction_end('Y',1)
      LET r_success = 'Y'
   ELSE
      LET r_success = 'N'
      CALL s_transaction_end('N',1)
      CALL cl_err_collect_show()
   END IF
   RETURN r_success 
END FUNCTION

