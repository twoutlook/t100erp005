            AND glab001= '41'
            AND glab002= '8714'
            AND glab003= 'F'
      END IF
      #161013-00014#1 Add  ---(E)---
      
      IF g_glaa_t.glaa015 = 'Y' THEN
         LET g_xrde_t.xrde120  = l_nmbb_t.nmbb011
         LET g_xrde_t.xrde121  = l_nmbb_t.nmbb012
      ELSE
         LET g_xrde_t.xrde120  = ''
         LET g_xrde_t.xrde121  = 0
