               AND glab001 = '40'
               AND glab002 = '40'     
               AND glab003 = l_nmbb_t.nmbb003  #交易帳戶編號
         END IF
         #依agli190设定
         IF l_nmbb_t.nmbb029 MATCHES '[3456]0' THEN
            SELECT DISTINCT glab005 INTO g_xrde_t.xrde016
              FROM glab_t
             WHERE glabent = g_enterprise 
