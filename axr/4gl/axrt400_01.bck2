      END IF
      LET g_xrde_t.xrde011  = l_nmbb_t.nmbb002
      LET g_xrde_t.xrde012  = l_nmbb_t.nmbb010
      LET g_xrde_t.xrde100  = l_nmbb_t.nmbb004
