                  LET g_master.xrcald_desc = ''
               END IF
               SELECT glaa024,glaa003,glaacomp INTO l_glaa024,l_glaa003,l_glaacomp FROM glaa_t
                WHERE glaaent = g_enterprise AND glaald = g_master.xrcald
