         END IF
      END IF

   #END FOR
   END FOREACH

   IF l_tot_success THEN
      IF l_flag = 'Y' THEN 
         CALL s_transaction_end('Y','0')
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'asf-00251'
