   DEFINE l_flag1           LIKE type_t.chr1
   DEFINE l_flag2           LIKE type_t.chr1
   #20150422--add--end--lujh
   #end add-point
 
  #INITIALIZE lc_param TO NULL           #p類不可以清空
   CALL util.JSON.parse(ls_js,lc_param)  #r類作業被t類呼叫時使用, p類主要解開參數處
   LET li_p01_status = 1
 
