################################################################################
PRIVATE FUNCTION axrt300_05_xrde011_chk()
   DEFINE l_nmajstus               LIKE nmaj_t.nmajstus
   DEFINE l_nmaj002                LIKE nmaj_t.nmaj002
   
   LET g_errno = ''
   
   SELECT nmajstus,nmaj002 INTO l_nmajstus,l_nmaj002
     FROM nmaj_t
    WHERE nmajent = g_enterprise
      AND nmaj001 = g_xrde_d[l_ac].xrde011

   CASE
      WHEN SQLCA.SQLCODE = 100    LET g_errno = 'anm-00017'
      WHEN l_nmajstus = 'N'       LET g_errno = 'anm-00016'
      WHEN l_nmaj002 <> '1'       LET g_errno = 'anm-00072'
   END CASE
   
   #帶值
   IF cl_null(g_xrde_d[l_ac].xrde012) AND cl_null(g_errno) THEN
      SELECT nmad003 INTO g_xrde_d[l_ac].xrde012
        FROM nmad_t
       WHERE nmadent = g_enterprise
         AND nmad001 = g_glaa_t.glaa005
         AND nmad002 = g_xrde_d[l_ac].xrde011
      DISPLAY g_xrde_d[l_ac].xrde012 TO s_detail1[l_ac].xrde012
      LET g_xrde_d[l_ac].xrde012_desc = s_desc_get_nmail004_desc(g_glaa_t.glaa005,g_xrde_d[l_ac].xrde012)
      DISPLAY BY NAME g_xrde_d[l_ac].xrde012_desc
   END IF
END FUNCTION

