################################################################################
# Descriptions...: 本位幣二、三金额计算
# Memo...........:
# Usage..........: CALL axrt300_05_other_amt()
#                  RETURNING 
# Input parameter: 
# Return code....: 
# Date & Author..: 2015/12/10 By 01727
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt300_05_other_amt()
   DEFINE l_success         LIKE type_t.num5

   IF g_glaa_t.glaa015 = 'N' THEN
      LET g_xrde2_d[l_ac].xrde131 = 0
      LET g_xrde2_d[l_ac].xrde139 = 0
   END IF

   IF g_glaa_t.glaa019 = 'N' THEN
      LET g_xrde2_d[l_ac].xrde121 = 0
      LET g_xrde2_d[l_ac].xrde129 = 0
   END IF

   IF g_glaa_t.glaa015 = 'N' AND g_glaa_t.glaa019 = 'N' THEN RETURN END IF

   #本位幣二金額
   IF g_glaa_t.glaa015 = 'Y' THEN
      IF g_glaa_t.glaa017 = '1' THEN
         LET g_xrde2_d[l_ac].xrde129 = g_xrde_d[l_ac].xrde109 * g_xrde2_d[l_ac].xrde121
      ELSE
         LET g_xrde2_d[l_ac].xrde129 = g_xrde_d[l_ac].xrde119 * g_xrde2_d[l_ac].xrde121
      END IF
      CALL s_curr_round_ld('1',g_xrca_t.xrcald,g_glaa_t.glaa016,g_xrde2_d[l_ac].xrde129,2)
         RETURNING l_success,g_errno,g_xrde2_d[l_ac].xrde129
   END IF

   #本位幣三金額
   IF g_glaa_t.glaa019 = 'Y' THEN
      IF g_glaa_t.glaa021 = '1' THEN
         LET g_xrde2_d[l_ac].xrde139 = g_xrde_d[l_ac].xrde109 * g_xrde2_d[l_ac].xrde131
      ELSE
         LET g_xrde2_d[l_ac].xrde139 = g_xrde_d[l_ac].xrde119 * g_xrde2_d[l_ac].xrde131
      END IF
      CALL s_curr_round_ld('1',g_xrca_t.xrcald,g_glaa_t.glaa020,g_xrde2_d[l_ac].xrde139,2)
         RETURNING l_success,g_errno,g_xrde2_d[l_ac].xrde139
   END IF

   DISPLAY g_xrde2_d[l_ac].xrde129,g_xrde2_d[l_ac].xrde139 TO s_detail3[l_ac].xrde129,s_detail3[l_ac].xrde139

END FUNCTION

################################################################################
# Descriptions...: 合計金額顯示
# Memo...........:
# Usage..........: CALL axrt300_05_dis_amt()
#                  RETURNING 
# Input parameter: 
# Return code....: 
# Date & Author..: 2015/12/10 By 01727
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt300_05_dis_amt()
   DEFINE l_amt1,l_amt2         LIKE xrde_t.xrde109
   DEFINE l_amt3,l_amt4         LIKE xrde_t.xrde109
   DEFINE l_amt5,l_amt6         LIKE xrde_t.xrde109
   DEFINE l_amt7,l_amt8         LIKE xrde_t.xrde109
   DEFINE l_amt9,l_amt10        LIKE xrde_t.xrde109
   DEFINE l_amt11,l_amt12       LIKE xrde_t.xrde109
   DEFINE l_amt13,l_amt14       LIKE xrde_t.xrde109
   DEFINE l_amt15,l_amt16       LIKE xrde_t.xrde109
   DEFINE l_amt17,l_amt18       LIKE xrde_t.xrde109
   DEFINE l_amt19,l_amt20       LIKE xrde_t.xrde109
   DEFINE l_xrde109             LIKE xrde_t.xrde109
   DEFINE l_xrde119             LIKE xrde_t.xrde109
   DEFINE l_xrde129             LIKE xrde_t.xrde109
   DEFINE l_xrde139             LIKE xrde_t.xrde109

   LET g_errno = " "

   LET l_amt1 = 0   LET l_amt2 = 0 LET l_amt3 = 0   LET l_amt4 = 0
   LET l_amt5 = 0   LET l_amt6 = 0 LET l_amt7 = 0   LET l_amt8 = 0
   LET l_amt9 = 0   LET l_amt10= 0 LET l_amt11= 0   LET l_amt12= 0
   LET l_amt13= 0   LET l_amt14= 0 LET l_amt15= 0   LET l_amt16= 0
   LET l_amt17= 0   LET l_amt18= 0 LET l_amt19= 0   LET l_amt20= 0
   LET l_xrde109=0  LET l_xrde119=0
   LET l_xrde129=0  LET l_xrde139=0

   #交易合計金額
   LET l_amt1 = g_xrca_t.xrca103 + g_xrca_t.xrca104
  #160824-00037#1 Mark ---(S)---
  #LET l_amt2 = g_xrca_t.xrca103 + g_xrca_t.xrca104
  #LET l_amt3 = g_xrca_t.xrca103 + g_xrca_t.xrca104
  #LET l_amt4 = g_xrca_t.xrca103 + g_xrca_t.xrca104
  #160824-00037#1 Mark ---(E)---
  #160824-00037#1 Add  ---(S)---
   LET l_amt2 = g_xrca_t.xrca113 + g_xrca_t.xrca114
   LET l_amt3 = g_xrca_t.xrca123 + g_xrca_t.xrca124
   LET l_amt4 = g_xrca_t.xrca133 + g_xrca_t.xrca134
  #160824-00037#1 Add  ---(E)---

   #稅前折抵金額
   #直接繳款金額
   SELECT SUM(xrde109 * CASE WHEN xrde015 = 'D' THEN 1 ELSE -1 END),SUM(xrde119 * CASE WHEN xrde015 = 'D' THEN 1 ELSE -1 END),
          SUM(xrde129 * CASE WHEN xrde015 = 'D' THEN 1 ELSE -1 END),SUM(xrde139 * CASE WHEN xrde015 = 'D' THEN 1 ELSE -1 END)
     INTO l_amt9,l_amt10,l_amt11,l_amt12 FROM xrde_t,gzcb_t
    WHERE xrdeent = g_enterprise
      AND xrdeld  = g_xrca_t.xrcald
      AND xrdedocno = g_xrca_t.xrcadocno
      AND xrde002 = gzcb002
      AND gzcb001 = '8306'
      AND xrde002 = '10'

   #直接沖銷金額
   SELECT SUM(xrde109 * CASE WHEN xrde015 = 'D' THEN 1 ELSE -1 END),SUM(xrde119 * CASE WHEN xrde015 = 'D' THEN 1 ELSE -1 END),
          SUM(xrde129 * CASE WHEN xrde015 = 'D' THEN 1 ELSE -1 END),SUM(xrde139 * CASE WHEN xrde015 = 'D' THEN 1 ELSE -1 END)
     INTO l_xrde109,l_xrde119,l_xrde129,l_xrde139 FROM xrde_t,gzcb_t
    WHERE xrdeent = g_enterprise
      AND xrdeld  = g_xrca_t.xrcald
      AND xrdedocno = g_xrca_t.xrcadocno
      AND xrde002 = gzcb002
      AND gzcb001 = '8306'
      AND xrde002 <> '10'

   SELECT SUM(xrce109 * CASE WHEN gzcb003 = 'D' THEN 1 ELSE -1 END),SUM(xrce119 * CASE WHEN gzcb003 = 'D' THEN 1 ELSE -1 END),
          SUM(xrce129 * CASE WHEN gzcb003 = 'D' THEN 1 ELSE -1 END),SUM(xrce139 * CASE WHEN gzcb003 = 'D' THEN 1 ELSE -1 END)
     INTO l_amt13,l_amt14,l_amt15,l_amt16 FROM xrce_t,gzcb_t
    WHERE xrceent = g_enterprise
      AND xrceld  = g_xrca_t.xrcald
      AND xrcedocno = g_xrca_t.xrcadocno
      AND gzcb001 = '8306'
      AND gzcb002 = xrce002



   IF cl_null(l_amt1) THEN LET l_amt1 = 0 END IF
   IF cl_null(l_amt2) THEN LET l_amt2 = 0 END IF
   IF cl_null(l_amt3) THEN LET l_amt3 = 0 END IF
   IF cl_null(l_amt4) THEN LET l_amt4 = 0 END IF
   IF cl_null(l_amt9) THEN LET l_amt9 = 0 END IF
   IF cl_null(l_amt10) THEN LET l_amt10= 0 END IF
   IF cl_null(l_amt11) THEN LET l_amt11= 0 END IF
   IF cl_null(l_amt12) THEN LET l_amt12= 0 END IF
   IF cl_null(l_xrde109) THEN LET l_xrde109=0 END IF
   IF cl_null(l_xrde119) THEN LET l_xrde119=0 END IF
   IF cl_null(l_xrde129) THEN LET l_xrde129=0 END IF
   IF cl_null(l_xrde139) THEN LET l_xrde139=0 END IF

   IF cl_null(l_amt13) THEN LET l_amt13 = 0 END IF
   IF cl_null(l_amt14) THEN LET l_amt14 = 0 END IF
   IF cl_null(l_amt15) THEN LET l_amt15 = 0 END IF
   IF cl_null(l_amt16) THEN LET l_amt16 = 0 END IF

   #應收收款金額
   LET l_amt13 = l_amt13 + l_xrde109
   LET l_amt14 = l_amt14 + l_xrde119
   LET l_amt15 = l_amt15 + l_xrde129
   LET l_amt16 = l_amt16 + l_xrde139

   LET l_amt17 = l_amt1 - l_amt9 - l_amt13
   LET l_amt18 = l_amt2 - l_amt10- l_amt14
   LET l_amt19 = l_amt3 - l_amt11- l_amt15
   LET l_amt20 = l_amt4 - l_amt12- l_amt16

   DISPLAY l_amt1 ,l_amt2 ,l_amt3 ,l_amt4 ,
           l_amt5 ,l_amt6 ,l_amt7 ,l_amt8 ,
           l_amt9 ,l_amt10,l_amt11,l_amt12,
           l_amt13,l_amt14,l_amt15,l_amt16,
           l_amt17,l_amt18,l_amt19,l_amt20
        TO amt1 ,amt2 ,amt3 ,amt4 ,
           amt5 ,amt6 ,amt7 ,amt8 ,
           amt9 ,amt10,amt11,amt12,
           amt13,amt14,amt15,amt16,
           amt17,amt18,amt19,amt20

   DISPLAY g_xrca_t.xrca100,g_glaa_t.glaa001,g_glaa_t.glaa016,g_glaa_t.glaa017
        TO xrca100,glaa001,glaa016,glaa017

   LET g_errno = " "
   IF l_amt17 < 0 OR l_amt18 < 0 THEN
      LET g_errno = "axr-00354"
   END IF

   IF g_glaa_t.glaa015 = 'Y' AND l_amt19 < 0 THEN
      LET g_errno = "axr-00354"
   END IF

   IF g_glaa_t.glaa019 = 'Y' AND l_amt20 < 0 THEN
      LET g_errno = "axr-00354"
   END IF

END FUNCTION
################################################################################
# Descriptions...: 获取显示给用户看到的交易账户型态
# Memo...........: #160122-00001#5
# Usage..........: CALL axrt300_05_get_lc_xrde008(p_xrde008)
#                  RETURNING lc_apde008
# Input parameter: p_xrde008      交易账户编号
# Return code....: lc_xrde008     返回现在在画面中的交易账户编号
# Date & Author..: 2016/03/02 By 07673
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt300_05_get_lc_xrde008(p_xrde008,p_xrde006)
   DEFINE p_xrde008        LIKE xrde_t.xrde008
   DEFINE p_xrde006        LIKE xrde_t.xrde006
   DEFINE l_cnt            LIKE type_t.num5
   DEFINE l_cnt2           LIKE type_t.num5      
   DEFINE lc_xrde008       LIKE xrde_t.xrde008  
   
   #判断当前用户是否有权限查看该交易账户，如果没有权限不可看到交易账户编号，用“*”显示
   LET lc_xrde008 = p_xrde008
   IF p_xrde006 != 30 THEN 
      IF NOT cl_null(p_xrde008) THEN
         LET l_cnt = 0
         SELECT COUNT(*) INTO l_cnt FROM nmll_t 
          WHERE nmllent = g_enterprise AND nmll002 = g_user AND nmllstus= 'Y' 
            AND nmll001 = p_xrde008 
         SELECT COUNT(*) INTO l_cnt2 FROM nmlm_t 
          WHERE nmlment = g_enterprise AND nmlm002 = g_dept AND nmlmstus= 'Y'
            AND nmlm001 = p_xrde008  
         IF l_cnt = 0 AND l_cnt2 = 0 THEN 
            LET lc_xrde008 = "********"
         END IF
      END IF
   END IF
   RETURN lc_xrde008

END FUNCTION

################################################################################
# Descriptions...: 檢查兩個日期是否在同一會計期間內
# Memo...........:
# Usage..........: CALL axrt300_05_chk_xrde032(p_date1,p_date2)
#                  RETURNING r_success
# Input parameter: p_glaald       帳套
#                : p_date1        日期一
#                : p_date2        日期二
# Return code....: r_success      是否同一年期
# Date & Author..: 2016/03/23 By 01727
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt300_05_chk_xrde032(p_glaald,p_date1,p_date2)
   DEFINE p_glaald        LIKE glaa_t.glaald
   DEFINE p_date1         LIKE xrde_t.xrde032
   DEFINE p_date2         LIKE xrde_t.xrde032
   DEFINE r_success       LIKE type_t.num5
   DEFINE l_sql           STRING
   DEFINE l_count         LIKE type_t.num5
   DEFINE l_glaa003       LIKE glaa_t.glaa003

   SELECT glaa003 INTO l_glaa003 FROM glaa_t WHERE glaaent = g_enterprise
      AND glaald = p_glaald

   LET r_success = TRUE

   LET l_sql = " SELECT SUM(date1) - SUM(date2) FROM(                                                      " CLIPPED,
               "    SELECT glav002 * 12 + glav006 date1,0 date2 FROM glav_t WHERE glav001 = '",l_glaa003,"'" CLIPPED,
               "       AND glav004 = '",p_date1,"' AND glavent = '",g_enterprise,"' AND glavstus = 'Y'     " CLIPPED,
               "    UNION                                                                                  " CLIPPED,
               "    SELECT 0 date1,glav002 * 12 + glav006 date2 FROM glav_t WHERE glav001 = '",l_glaa003,"'" CLIPPED,
               "       AND glav004 = '",p_date2,"' AND glavent = '",g_enterprise,"' AND glavstus = 'Y')    " CLIPPED
   PREPARE axrt300_05_prep FROM l_sql

   LET l_count = 0
   EXECUTE axrt300_05_prep INTO l_count
   IF cl_null(l_count) THEN LET l_count = 1 END IF

   IF l_count <> 0 THEN
      LET r_success = FALSE
   END IF

   RETURN r_success

END FUNCTION

 
