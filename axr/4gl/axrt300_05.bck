PRIVATE FUNCTION axrt300_05_xrde008_chk()
   DEFINE l_nmaastus            LIKE nmaa_t.nmaastus
   DEFINE l_ooab002             LIKE ooab_t.ooab002
   
   LET g_errno = ''
   
   SELECT nmaastus,nmas003 INTO l_nmaastus,g_xrde_d[l_ac].xrde100
     FROM nmaa_t,nmas_t
    WHERE nmaaent = g_enterprise
      AND nmaaent = nmasent
      AND nmaa001 = nmas001
