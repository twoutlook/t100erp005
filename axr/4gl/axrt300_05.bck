      AND nmas002 = g_xrde_d[l_ac].lc_xrde008
      AND nmaa002 IN (select ooef001 FROM ooef_t WHERE ooefent = g_enterprise
                                               AND ooef017 = g_xrde_d[l_ac].xrdecomp)

   CASE
      WHEN SQLCA.SQLCODE = 100    LET g_errno = 'anm-00026'
#      WHEN l_nmaastus = 'N'       LET g_errno = 'anm-00027' #160318-00005#53  mark
      WHEN l_nmaastus = 'N'       LET g_errno = 'sub-01302'  #160318-00005#53 add
   END CASE
