################################################################################
PRIVATE REPORT axrr400_g01_subrep07(sr5,p_docno,p_ld)
DEFINE sr5 sr5_r
DEFINE l_glaa001      LIKE glaa_t.glaa001
DEFINE l_xrce119_sum  LIKE xrce_t.xrce119
DEFINE p_docno        LIKE xrce_t.xrcedocno
DEFINE p_ld           LIKE xrce_t.xrceld
    FORMAT

        ON EVERY ROW
            PRINTX g_grNumFmt.*
            PRINTX sr5.*
        ON LAST ROW
        
        #本幣幣別
        CALL s_ld_sel_glaa(p_ld,'glaa001') RETURNING  g_sub_success,l_glaa001
        #本幣金額
        SELECT SUM(CASE xrce015 WHEN 'C' THEN xrce119 * -1 ELSE xrce119 END)
          INTO l_xrce119_sum
          FROM xrce_t
         WHERE xrceent = g_enterprise
           AND xrceld  = p_ld
           AND xrcedocno = p_docno

         PRINTX l_xrce119_sum,l_glaa001
END REPORT

