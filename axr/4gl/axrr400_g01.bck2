        ON EVERY ROW
            PRINTX g_grNumFmt.*
            PRINTX sr6.*
END REPORT

################################################################################
# Descriptions...: 差異處理幣別小記
# Memo...........:
# Usage..........: CALLaxrr400_g01_subrep09(sr7,p_docno,p_ld)
# Date & Author..: 2015/08/31 By Hans
# Modify.........:
################################################################################
PRIVATE REPORT axrr400_g01_subrep09(sr7,p_docno,p_ld)
DEFINE sr7 sr7_r
DEFINE l_glaa001      LIKE glaa_t.glaa001
DEFINE l_xrde119_sum  LIKE xrde_t.xrde119
DEFINE p_docno        LIKE xrda_t.xrdadocno
DEFINE p_ld           LIKE xrda_t.xrdald

    FORMAT

        ON EVERY ROW
            PRINTX g_grNumFmt.*
            PRINTX sr7.*
        ON LAST ROW
        
        #本幣幣別
        CALL s_ld_sel_glaa(p_ld,'glaa001') RETURNING  g_sub_success,l_glaa001
        #本幣金額
        SELECT SUM(CASE xrde015 WHEN 'C' THEN xrde119* - 1 ELSE xrde119 END)
          INTO l_xrde119_sum
          FROM xrde_t
         WHERE xrdeent = g_enterprise
           AND xrdeld = p_ld
           AND xrdedocno = p_docno
           AND xrde002  <> '10'
         

         PRINTX l_xrde119_sum,l_glaa001
END REPORT

 
