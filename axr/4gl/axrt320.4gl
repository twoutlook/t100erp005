PRIVATE FUNCTION axrt320_xmdl_bak()
   DEFINE l_xrcb     RECORD
      xrcb002        LIKE xrcb_t.xrcb002,
      xrcb003        LIKE xrcb_t.xrcb003,
      xrcb007        LIKE xrcb_t.xrcb007
                     END RECORD
   DEFINE l_sql      STRING
   DEFINE l_s        LIKE type_t.num5
   DEFINE l_success  LIKE type_t.chr1
   #151008-00009#8--s
   DEFINE ls_js  STRING
   DEFINE lc_param          RECORD
          xrcald            LIKE xrca_t.xrcald,        
          xrcadocno         LIKE xrca_t.xrcadocno,        
          type2             LIKE type_t.chr1       #刪除類型:1.刪除所有項次/2.刪除一筆項次
                        END RECORD
   #151008-00009#8--e      

   CALL s_axrt300_sel_ld(g_xrca_m.xrcald) RETURNING l_success,l_s

   LET l_sql = "SELECT xrcb002,xrcb003,xrcb007 FROM xrcb_t WHERE xrcbent = '",g_enterprise,"'",
               "   AND xrcbdocno = '",g_xrca_m.xrcadocno,"'",
               "   AND xrcbld    = '",g_xrca_m.xrcald,"'"
   PREPARE axrt320_bak_prep FROM l_sql
   DECLARE axrt320_bak_curs CURSOR FOR axrt320_bak_prep

   FOREACH axrt320_bak_curs INTO l_xrcb.*
      CASE
         WHEN l_s = '1'
            UPDATE xmdl_t SET xmdl053 = xmdl053 - l_xrcb.xrcb007
             WHERE xmdldocno = l_xrcb.xrcb002
               AND xmdlseq = l_xrcb.xrcb003
               AND xmdlent = g_enterprise
         WHEN l_s = '2'
            UPDATE xmdl_t SET xmdl054 = xmdl054 - l_xrcb.xrcb007
             WHERE xmdldocno = l_xrcb.xrcb002
               AND xmdlseq = l_xrcb.xrcb003
               AND xmdlent = g_enterprise
         WHEN l_s = '3'
            UPDATE xmdl_t SET xmdl055 = xmdl055 - l_xrcb.xrcb007
             WHERE xmdldocno = l_xrcb.xrcb002
               AND xmdlseq = l_xrcb.xrcb003
               AND xmdlent = g_enterprise
      END CASE
   END FOREACH
   #151008-00009#8--s
   IF g_glaa.glaa139 = 'Y' THEN
         #刪除遞延檔
         LET lc_param.xrcald    = g_xrca_m.xrcald
         LET lc_param.xrcadocno = g_xrca_m.xrcadocno  
         LET lc_param.type2     = '1'   #刪除類型:1.刪除所有項次/2.刪除一筆項次
         LET ls_js = util.JSON.stringify(lc_param)       
         CALL s_axrt470_del_xreq(ls_js) RETURNING g_sub_success
         IF NOT g_sub_success THEN
            RETURN 'N'
         END IF      
   END IF    
   #151008-00009#8--e      

   RETURN 'Y'

END FUNCTION

