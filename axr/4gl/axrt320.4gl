PRIVATE FUNCTION axrt320_get_gl()
   DEFINE l_glap001         LIKE glap_t.glap001
   DEFINE la_param          RECORD
          prog              STRING,
          param             DYNAMIC ARRAY OF STRING
                            END RECORD
   DEFINE ls_js             STRING
   DEFINE l_slip            LIKE xrca_t.xrcadocno
   DEFINE l_success         LIKE type_t.num5
   DEFINE l_glaa024         LIKE glaa_t.glaa024
   DEFINE l_ooac004         LIKE ooac_t.ooac004

   IF cl_null(g_xrca_m.xrcadocno) OR cl_null(g_xrca_m.xrcald) THEN RETURN END IF

   IF NOT cl_null(g_xrca_m.xrca038) THEN
      SELECT glap001 INTO l_glap001 FROM glap_t WHERE glapent = g_enterprise
         AND glapdocno = g_xrca_m.xrca038  AND glapld = g_xrca_m.xrcald
      CASE
         WHEN l_glap001 = '1'
            LET la_param.prog = "aglt310"
         WHEN l_glap001 = '2'
            LET la_param.prog = "aglt320"
         WHEN l_glap001 = '3'
            LET la_param.prog = "aglt330"
         WHEN l_glap001 = '5'
            LET la_param.prog = "aglt350"
      END CASE
      LET la_param.param[1] = g_xrca_m.xrcald
      LET la_param.param[2] = g_xrca_m.xrca038
      LET ls_js = util.JSON.stringify(la_param)
      CALL cl_cmdrun(ls_js)

   ELSE
      IF g_xrca_m.xrcastus <> 'Y' THEN RETURN END IF
      CALL s_aooi200_fin_get_slip(g_xrca_m.xrcadocno)
         RETURNING l_success,l_slip

      SELECT glaa024 INTO l_glaa024
        FROM glaa_t
       WHERE glaaent = g_enterprise
         AND glaald = g_xrca_m.xrcald
      
      SELECT ooac004 INTO l_ooac004
        FROM ooac_t
       WHERE ooacent = g_enterprise
         AND ooac001 = l_glaa024
         AND ooac002 = l_slip
         AND ooac003 = 'D-FIN-2002'

      LET la_param.prog = "axrp330"
      LET la_param.param[1] = g_xrca_m.xrcald
      LET la_param.param[2] = g_xrca_m.xrcadocno
      LET la_param.param[3] = g_xrca_m.xrcasite
      LET la_param.param[4] = l_ooac004
      LET la_param.param[5] = g_xrca_m.xrcadocdt
      LET ls_js = util.JSON.stringify(la_param)
      CALL cl_cmdrun_wait(ls_js)

   END IF
   CALL axrt320_ui_headershow()

END FUNCTION

