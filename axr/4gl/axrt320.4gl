   LET l_success = 'Y'
   FOREACH axrt320_bak_curs1 INTO l_xrcb.*
      #SELECT * INTO l_xmdl_t.* FROM xmdl_t WHERE xmdldocno = l_xrcb.xrcb002 AND xmdlseq = l_xrcb.xrcb003                            #160905-00002#6  mark
      #161128-00061#4---mdofiy--begin---------
