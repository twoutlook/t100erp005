################################################################################
# Descriptions...: 單身說明欄位資料刷新
# Memo...........:
# Usage..........: CALL axrt320_xrcb_ref(p_lab)
#                  RETURNING ---
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt320_xrcb_ref(p_lab)
   DEFINE p_lab          LIKE type_t.chr20
   DEFINE l_ooef019      LIKE ooef_t.ooef019
   
   CASE
      WHEN p_lab = 'xrcb020'
         SELECT ooef019 INTO l_ooef019 FROM ooef_t WHERE ooefent = g_enterprise AND ooef001 = g_xrca_m.xrcacomp
         INITIALIZE g_ref_fields TO NULL
         LET g_ref_fields[1] = g_xrcb_d[l_ac].xrcb020
         CALL ap_ref_array2(g_ref_fields,"SELECT oodbl004 FROM oodbl_t WHERE oodblent='"||g_enterprise||"' AND oodbl001='"||l_ooef019||"' AND oodbl002=? AND oodbl003='"||g_dlang||"'","") RETURNING g_rtn_fields
         LET g_xrcb_d[l_ac].xrcb020_desc = '', g_rtn_fields[1] , ''
         DISPLAY BY NAME g_xrcb_d[l_ac].xrcb020_desc
      WHEN p_lab = 'xrcd009'
         INITIALIZE g_ref_fields TO NULL
         LET g_ref_fields[1] = g_xrcb2_d[l_ac].xrcd009
         CALL ap_ref_array2(g_ref_fields,"SELECT glacl004 FROM glacl_t WHERE glaclent='"||g_enterprise||"' AND glacl001='"||g_glaa.glaa004||"' AND glacl002=? AND glacl003='"||g_dlang||"'","") RETURNING g_rtn_fields
         LET g_xrcb2_d[l_ac].xrcd009_desc = '', g_rtn_fields[1] , ''
         DISPLAY BY NAME g_xrcb2_d[l_ac].xrcd009_desc
      WHEN p_lab = 'xrcd002'
         SELECT ooef019 INTO l_ooef019 FROM ooef_t WHERE ooefent = g_enterprise AND ooef001 = g_xrca_m.xrcacomp
         INITIALIZE g_ref_fields TO NULL
         LET g_ref_fields[1] = g_xrcb2_d[l_ac].xrcd002
         CALL ap_ref_array2(g_ref_fields,"SELECT oodbl004 FROM oodbl_t WHERE oodblent='"||g_enterprise||"' AND oodbl001='"||l_ooef019||"' AND oodbl002=? AND oodbl003='"||g_dlang||"'","") RETURNING g_rtn_fields
         LET g_xrcb2_d[l_ac].xrcd002_desc = '', g_rtn_fields[1] , ''
         DISPLAY BY NAME g_xrcb2_d[l_ac].xrcd002_desc
   END CASE
END FUNCTION
