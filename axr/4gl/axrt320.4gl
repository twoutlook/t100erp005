        #150515-00013#3 Add  ---(S)---
         IF g_xrca_m.xrca100 = g_glaa.glaa001 THEN
            LET l_xrcb.xrcb113 = l_xrcb.xrcb103
            LET l_xrcb.xrcb114 = l_xrcb.xrcb104
            LET l_xrcb.xrcb115 = l_xrcb.xrcb105
            UPDATE xrcd_t SET xrcd113 = xrcd103,
                              xrcd114 = xrcd104,
                              xrcd115 = xrcd105
             WHERE xrcdent = g_enterprise
               AND xrcddocno = g_xrca_m.xrcadocno
               AND xrcdld = g_xrca_m.xrcald
         END IF
        #150515-00013#3 Add  ---(E)---

        UPDATE xrcb_t 
           SET xrcb020 = l_xrcb.xrcb020,
               xrcb103 = l_xrcb.xrcb103,
               xrcb104 = l_xrcb.xrcb104,
               xrcb105 = l_xrcb.xrcb105,
               xrcb113 = l_xrcb.xrcb113,
               xrcb114 = l_xrcb.xrcb114,
               xrcb115 = l_xrcb.xrcb115,
               xrcb123 = l_xrcb.xrcb123,
               xrcb124 = l_xrcb.xrcb124,
               xrcb125 = l_xrcb.xrcb125,
               xrcb133 = l_xrcb.xrcb133,
               xrcb134 = l_xrcb.xrcb134,
               xrcb135 = l_xrcb.xrcb135
         WHERE xrcbent = g_enterprise 
           AND xrcbld = g_xrca_m.xrcald
           AND xrcbdocno = g_xrca_m.xrcadocno
           AND xrcbseq = l_xrcb.xrcbseq    
      IF SQLCA.sqlcode THEN 
          INITIALIZE g_errparam TO NULL
          LET g_errparam.code = SQLCA.sqlcode
          LET g_errparam.extend = "xrcb_t"
          LET g_errparam.popup = TRUE
          CALL cl_err()
 
         LET r_success = FALSE
         EXIT FOREACH 
      END IF
   END FOREACH
   IF r_success = TRUE THEN 
      CALL s_axrt300_installments(g_xrca_m.xrcald,g_xrca_m.xrcadocno) RETURNING r_success
      CALL s_axrt300_xrca_upd(g_xrca_m.xrcald,g_xrca_m.xrcadocno)
         RETURNING l_success
      DISPLAY BY NAME g_xrca_m.xrca103,g_xrca_m.xrca104,g_xrca_m.xrca113,g_xrca_m.xrca114,
                   g_xrca_m.xrca123,g_xrca_m.xrca124,g_xrca_m.xrca133,g_xrca_m.xrca134,
                   g_xrca_m.xrca108,g_xrca_m.xrca118,g_xrca_m.xrca128,g_xrca_m.xrca138
   END IF
   IF r_success = TRUE THEN
      CALL s_transaction_end('Y','0')
   ELSE
      CALL s_transaction_end('N','0')
   END IF
   RETURN r_success
END FUNCTION
