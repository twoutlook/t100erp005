# Descriptions...: 修改單頭匯率、原幣/本幣金額後金額刷新
# Memo...........:
# Usage..........: CALL axrt320_head_amount_refresh(p_chr)
#                  RETURNING 
# Input parameter: p_chr          修改內容  1.修改原幣幣別 2.修改原幣匯率、金額 3.修改本位幣金額
# Return code....:
# Date & Author..: 2014/04/01 By zhangwei
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt320_head_amount_refresh(p_chr)
   #傳入值
   DEFINE p_chr            LIKE type_t.chr1
   #其他
   DEFINE l_success      LIKE type_t.chr1
   
   #DEFINE l_glaa_t       RECORD LIKE glaa_t.*  #161128-00061#4--mark
   #DEFINE l_xrca         RECORD LIKE xrca_t.*  #161128-00061#4--mark

   #單頭金額修改後處理邏輯:
   #CASE1.修改原幣幣別:異動本幣金額;異動其他本位幣金額
   #CASE2.修改原幣匯率:異動本幣金額;依照其他本位幣匯率參考依據決定是否異動其他本位幣金額
   #CASE3.修改本幣金額:依照其他本位幣匯率參考依據決定是否異動其他本位幣金額

   CASE
      WHEN p_chr = '1'      #CASE1
         #計算本位幣金額
         CALL s_axrt300_exrate(g_glaa.glaa002,g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa001,g_xrca_m.xrca103,g_xrca_m.xrca101,g_glaa.glaacomp)
            RETURNING l_success,g_xrca_m.xrca113
         CALL s_axrt300_exrate(g_glaa.glaa002,g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa001,g_xrca_m.xrca104,g_xrca_m.xrca101,g_glaa.glaacomp)
            RETURNING l_success,g_xrca_m.xrca114
         IF g_glaa.glaa015 = 'Y' THEN
            #計算本位幣二金額
            IF g_glaa.glaa017 = '1' THEN
               #换算基准:交易原幣
               CALL s_axrt300_exrate(g_glaa.glaa002,g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa016,g_xrca_m.xrca103,g_xrca_m.xrca121,g_glaa.glaacomp)
                  RETURNING l_success,g_xrca_m.xrca123
               CALL s_axrt300_exrate(g_glaa.glaa002,g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa016,g_xrca_m.xrca104,g_xrca_m.xrca121,g_glaa.glaacomp)
                  RETURNING l_success,g_xrca_m.xrca124
            ELSE
               #换算基准:帳簿幣別
               CALL s_axrt300_exrate(g_glaa.glaa002,g_xrca_m.xrcadocdt,g_glaa.glaa001,g_glaa.glaa016,g_xrca_m.xrca113,g_xrca_m.xrca121,g_glaa.glaacomp)
                  RETURNING l_success,g_xrca_m.xrca123
               CALL s_axrt300_exrate(g_glaa.glaa002,g_xrca_m.xrcadocdt,g_glaa.glaa001,g_glaa.glaa016,g_xrca_m.xrca114,g_xrca_m.xrca121,g_glaa.glaacomp)
                  RETURNING l_success,g_xrca_m.xrca124
            END IF
         END IF
         IF g_glaa.glaa019 = 'Y' THEN
            #計算本位幣三金額
            IF g_glaa.glaa021 = '1' THEN
               #换算基准:交易原幣
               CALL s_axrt300_exrate(g_glaa.glaa002,g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa020,g_xrca_m.xrca103,g_xrca_m.xrca131,g_glaa.glaacomp)
                  RETURNING l_success,g_xrca_m.xrca133
               CALL s_axrt300_exrate(g_glaa.glaa002,g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa020,g_xrca_m.xrca104,g_xrca_m.xrca131,g_glaa.glaacomp)
                  RETURNING l_success,g_xrca_m.xrca134
            ELSE
               #换算基准:帳簿幣別
               CALL s_axrt300_exrate(g_glaa.glaa002,g_xrca_m.xrcadocdt,g_glaa.glaa001,g_glaa.glaa020,g_xrca_m.xrca113,g_xrca_m.xrca131,g_glaa.glaacomp)
                  RETURNING l_success,g_xrca_m.xrca133
               CALL s_axrt300_exrate(g_glaa.glaa002,g_xrca_m.xrcadocdt,g_glaa.glaa001,g_glaa.glaa020,g_xrca_m.xrca114,g_xrca_m.xrca131,g_glaa.glaacomp)
                  RETURNING l_success,g_xrca_m.xrca134
            END IF
         END IF
      WHEN p_chr = '2'      #CASE2
         #計算本位幣金額
         CALL s_axrt300_exrate(g_glaa.glaa002,g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa001,g_xrca_m.xrca103,g_xrca_m.xrca101,g_glaa.glaacomp)
            RETURNING l_success,g_xrca_m.xrca113
         CALL s_axrt300_exrate(g_glaa.glaa002,g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa001,g_xrca_m.xrca104,g_xrca_m.xrca101,g_glaa.glaacomp)
            RETURNING l_success,g_xrca_m.xrca114
         IF g_glaa.glaa015 = 'Y' THEN
            #計算本位幣二金額
            IF g_glaa.glaa017 = '2' THEN
               #换算基准:帳簿幣別
               CALL s_axrt300_exrate(g_glaa.glaa002,g_xrca_m.xrcadocdt,g_glaa.glaa001,g_glaa.glaa016,g_xrca_m.xrca113,g_xrca_m.xrca121,g_glaa.glaacomp)
                  RETURNING l_success,g_xrca_m.xrca123
               CALL s_axrt300_exrate(g_glaa.glaa002,g_xrca_m.xrcadocdt,g_glaa.glaa001,g_glaa.glaa016,g_xrca_m.xrca114,g_xrca_m.xrca121,g_glaa.glaacomp)
                  RETURNING l_success,g_xrca_m.xrca124
            END IF
         END IF
         IF g_glaa.glaa019 = 'Y' THEN
            #計算本位幣三金額
            IF g_glaa.glaa021 = '2' THEN
