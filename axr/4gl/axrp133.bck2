   LET l_orders= l_orders,",CASE WHEN xmdk000 = 6 THEN 2 ELSE 1 END"

   LET l_sql = "SELECT ",l_str,",xmdldocno,xmdlseq,xrcb007 FROM axrp133_tmp01",      #160727-00019#33  mod axrp133_xmdk_tmp --> axrp133_tmp01
