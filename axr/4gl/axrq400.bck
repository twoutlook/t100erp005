PRIVATE FUNCTION axrq400_upd_tmp()
   DEFINE l_i          LIKE type_t.num5
   DEFINE l_sql        STRING
   DEFINE l_apcb002    LIKE apcb_t.apcb002
   DEFINE l_xrcc001    LIKE xrcc_t.xrcc001
 
   FOR l_i = 1 TO g_xrcc_d.getLength()   
