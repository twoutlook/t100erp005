#+ filter過濾功能
PRIVATE FUNCTION axrq400_filter()
   #add-point:filter段define-客製 name="filter.define_customerization"
   
   #end add-point
   DEFINE  ls_result   STRING
   #add-point:filter段define-標準  (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="filter.define"
   DEFINE l_ooef017     LIKE ooef_t.ooef017   #161111-00049#2 Add
   DEFINE l_glaa004     LIKE glaa_t.glaa004   #161111-00049#2 Add
   DEFINE l_ld_str      STRING                #161111-00049#2 Add
   #end add-point
   
   #add-point:FUNCTION前置處理 name="filter.before_function"
   #161111-00049#2 Add  ---(S)---
   SELECT ooef017 INTO l_ooef017 FROM ooef_t WHERE ooefent = g_enterprise AND ooef001 = g_site
   #161111-00049#2 Add  ---(E)---
   #end add-point
 
   LET l_ac = 1
   LET g_detail_idx = 1
   LET g_detail_idx2 = 1
 
   LET INT_FLAG = 0
 
   LET g_qryparam.state = 'c'
 
   LET g_wc_filter_t = g_wc_filter
   LET g_wc_t = g_wc
   
   CALL gfrm_curr.setFieldHidden("formonly.sel", TRUE)
   CALL gfrm_curr.setFieldHidden("formonly.statepic", TRUE)
 
   LET g_wc = cl_replace_str(g_wc, g_wc_filter, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON xrccdocno,xrcadocdt,xrcc009,xrca035,xrcc003,xrcc004,xrcc002,xrcc100
                          FROM s_detail1[1].b_xrccdocno,s_detail1[1].b_xrcadocdt,s_detail1[1].b_xrcc009, 
                              s_detail1[1].b_xrca035,s_detail1[1].b_xrcc003,s_detail1[1].b_xrcc004,s_detail1[1].b_xrcc002, 
                              s_detail1[1].b_xrcc100
 
         BEFORE CONSTRUCT
                     DISPLAY axrq400_filter_parser('xrccdocno') TO s_detail1[1].b_xrccdocno
            DISPLAY axrq400_filter_parser('xrcadocdt') TO s_detail1[1].b_xrcadocdt
            DISPLAY axrq400_filter_parser('xrcc009') TO s_detail1[1].b_xrcc009
            DISPLAY axrq400_filter_parser('xrca035') TO s_detail1[1].b_xrca035
            DISPLAY axrq400_filter_parser('xrcc003') TO s_detail1[1].b_xrcc003
            DISPLAY axrq400_filter_parser('xrcc004') TO s_detail1[1].b_xrcc004
            DISPLAY axrq400_filter_parser('xrcc002') TO s_detail1[1].b_xrcc002
            DISPLAY axrq400_filter_parser('xrcc100') TO s_detail1[1].b_xrcc100
 
 
         #單身公用欄位開窗相關處理
         
           
         #單身一般欄位開窗相關處理
                  #----<<sel>>----
         #----<<b_xrccld>>----
         #----<<b_xrca005>>----
         #----<<b_xrccdocno>>----
         #Ctrlp:construct.c.filter.page1.b_xrccdocno
         #應用 a03 樣板自動產生(Version:3)
         ON ACTION controlp INFIELD b_xrccdocno
            #add-point:ON ACTION controlp INFIELD b_xrccdocno name="construct.c.filter.page1.b_xrccdocno"
            CALL s_axrt300_get_site(g_user,'','2') RETURNING l_ld_str                    #161111-00049#2 Add
            CALL cl_replace_str(l_ld_str,'glaald','xrcald') RETURNING l_ld_str           #161111-00049#2 Add
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c'
            LET g_qryparam.reqry = FALSE
            LET g_qryparam.where = " xrcastus = 'Y' AND xrcc108-xrcc109 <> 0 ",
                                   " AND xrcald IN ",g_wc_ld,
                                   " AND xrcasite IN ",g_wc_site,
                                   " AND (EXISTS(SELECT 1 FROM glba_t WHERE glbaent = xrcaent AND glbald = xrcald AND glba001 = '",g_user,"') ",
                                   "         OR EXISTS(SELECT 1 FROM glbb_t WHERE glbbent = xrcaent AND glbbld = xrcald AND glbb001 = '",g_dept,"') ",
                                   "         OR (NOT EXISTS(SELECT 1 FROM glba_t WHERE glbaent = xrcaent AND glbald = xrcald) AND NOT EXISTS(SELECT 1 FROM glbb_t WHERE glbbent = xrcaent AND glbbld = xrcald))) ",
                                   " AND xrca001 NOT LIKE ('3%') AND xrca001 NOT LIKE ('0%') "
            IF NOT cl_null(g_sql_ctrl) AND NOT g_sql_ctrl = ' 1=1'  THEN
               LET g_qryparam.where = g_qryparam.where," AND xrca004 IN (SELECT pmaa001 FROM pmaa_t WHERE pmaaent = ",g_enterprise, " AND ", g_sql_ctrl,")"
            END IF
            IF NOT cl_null(g_user_dept_wc_q) THEN
               LET g_qryparam.where = g_qryparam.where," AND ",g_user_dept_wc_q  
            END IF                        
            LET g_qryparam.where = g_qryparam.where," AND ",l_ld_str CLIPPED  
            CALL q_xrcadocno_14()
            DISPLAY g_qryparam.return1 TO b_xrccdocno
            NEXT FIELD b_xrccdocno         
            #END add-point
 
 
         #----<<b_xrcadocdt>>----
         #Ctrlp:construct.c.filter.page1.b_xrcadocdt
#         #應用 a03 樣板自動產生(Version:3)
         ON ACTION controlp INFIELD b_xrcadocdt
            #add-point:ON ACTION controlp INFIELD b_xrcadocdt name="construct.c.filter.page1.b_xrcadocdt"
            
            #END add-point
 
 
         #----<<b_xrcc009>>----
         #Ctrlp:construct.c.filter.page1.b_xrcc009
#         #應用 a03 樣板自動產生(Version:3)
         ON ACTION controlp INFIELD b_xrcc009
            #add-point:ON ACTION controlp INFIELD b_xrcc009 name="construct.c.filter.page1.b_xrcc009"
            
            #END add-point
 
 
         #----<<b_xrccseq>>----
         #----<<b_xrcc001>>----
         #----<<b_xrca035>>----
         #Ctrlp:construct.c.page1.b_xrca035
         #應用 a03 樣板自動產生(Version:3)
         ON ACTION controlp INFIELD b_xrca035
            #add-point:ON ACTION controlp INFIELD b_xrca035 name="construct.c.filter.page1.b_xrca035"
            #應用 a08 樣板自動產生(Version:3)
            #開窗c段
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c' 
            LET g_qryparam.reqry = FALSE
            #161111-00049#2 Add  ---(S)---
            SELECT glaa004 INTO l_glaa004 FROM glaa_t WHERE glaaent = g_enterprise AND glaacomp = l_ooef017 AND glaa014 = 'Y'
			   LET g_qryparam.where = " glac001 = '",l_glaa004,"'"
            #161111-00049#2 Add  ---(E)---
            CALL aglt310_04()                           #呼叫開窗
            DISPLAY g_qryparam.return1 TO b_xrca035  #顯示到畫面上
            NEXT FIELD b_xrca035                     #返回原欄位
    



            #END add-point
 
 
         #----<<b_xrca035_desc>>----
         #----<<b_xrcc003>>----
         #Ctrlp:construct.c.filter.page1.b_xrcc003
#         #應用 a03 樣板自動產生(Version:3)
         ON ACTION controlp INFIELD b_xrcc003
            #add-point:ON ACTION controlp INFIELD b_xrcc003 name="construct.c.filter.page1.b_xrcc003"
            
            #END add-point
 
 
