               LET g_qryparam.reqry = FALSE
               CALL q_ooeg001_4()
               DISPLAY g_qryparam.return1 TO b_xrca015  #顯示到畫面上
               NEXT FIELD b_xrca015  
            WHEN INFIELD(b_xrca007)
               INITIALIZE g_qryparam.* TO NULL
