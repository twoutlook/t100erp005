END FUNCTION

################################################################################
# Descriptions...: 匯總單身
# Memo...........:
# Date & Author..: 2016/08/29 By lujh
# Modify.........:
################################################################################
PRIVATE FUNCTION axrq400_b_fill2()
   DEFINE l_sql           STRING
   DEFINE l_field         STRING
   DEFINE l_group         STRING
   DEFINE ls_sql_count    STRING
   DEFINE l_i             LIKE type_t.num10
   DEFINE l_comp          LIKE ooef_t.ooef001
   DEFINE l_rate1         LIKE type_t.num20_6
   DEFINE l_rate2         LIKE type_t.num20_6
   DEFINE l_rate3         LIKE type_t.num20_6  

   CALL axrq400_upd_tmp()
   CALL cl_set_comp_visible('b_xrccld2,b_xrca0052,b_xrcc0022,b_xrcc1002,b_xrcc0032,b_xrcc0042',TRUE)
   CALL cl_set_comp_visible('b_xrca0352,b_xrca035_desc2',TRUE)   #161011-00003#1 Add 
   CASE
      WHEN g_input.grouptype = '1'
         LET l_field = " xrccld,xrca005,xrcc002,'','',xrcc100,xrcc003,xrcc004,SUM(l_xrcc109),SUM(l_xrcc119),'' "   #161011-00003#1 Add  '',''
         LET l_group = " xrccld,xrca005,xrcc002,xrcc100,xrcc003,xrcc004 "
         CALL cl_set_comp_visible('b_xrca0352,b_xrca035_desc2',FALSE)   #161011-00003#1 Add 
         
      WHEN g_input.grouptype = '2'
         LET l_field = " xrccld,'',xrcc002,'','',xrcc100,xrcc003,xrcc004,SUM(l_xrcc109),SUM(l_xrcc119),'' "    #161011-00003#1 Add  '',''
         LET l_group = " xrccld,xrcc002,xrcc100,xrcc003,xrcc004 "
         CALL cl_set_comp_visible('b_xrca0052',FALSE)
         CALL cl_set_comp_visible('b_xrca0352,b_xrca035_desc2',FALSE)   #161011-00003#1 Add 
      WHEN g_input.grouptype = '3'
         LET l_field = " xrccld,'',xrcc002,'','',xrcc100,'',xrcc004,SUM(l_xrcc109),SUM(l_xrcc119),'' "     #161011-00003#1 Add  '',''
         LET l_group = " xrccld,xrcc002,xrcc100,xrcc004 "
         CALL cl_set_comp_visible('b_xrca0052,b_xrcc0032',FALSE)
      #161011-00003#1 Add  ---(S)---
         CALL cl_set_comp_visible('b_xrca0352,b_xrca035_desc2',FALSE)
      WHEN g_input.grouptype = '4'
         LET l_field = " xrccld,'','',xrca035,glacl004,xrcc100,'',xrcc004,SUM(l_xrcc109),SUM(l_xrcc119),'' "
         LET l_group = " xrccld,xrca035,glacl004,xrcc100,xrcc004 "
         CALL cl_set_comp_visible('b_xrcc0022,b_xrca0052,b_xrcc0032',FALSE)
      #161011-00003#1 Add  ---(E)---
   END CASE
   
   LET l_sql = "SELECT ",l_field CLIPPED,
               "  FROM axrq400_tmp ",
               " WHERE sel = 'Y' ",
               " GROUP BY ",l_group CLIPPED,
               " ORDER BY ",l_group CLIPPED
   PREPARE axrq400_sel_tmpp1 FROM l_sql
   DECLARE axrq400_sel_tmpc1 CURSOR FOR axrq400_sel_tmpp1
   
   LET ls_sql_count = "SELECT COUNT(1) FROM (",l_sql,")"
 
   PREPARE b_fill_cnt_pre2 FROM ls_sql_count
   EXECUTE b_fill_cnt_pre2 USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre2
   
   CALL g_xrcc_d2.clear()
   
   LET l_i = 1
   FOREACH axrq400_sel_tmpc1 INTO g_xrcc_d2[l_i].*
      IF SQLCA.SQLCODE THEN EXIT FOREACH END IF
      
      #依帳別+幣別取今日匯率
      LET l_comp = NULL
      SELECT glaacomp INTO l_comp FROM glaa_t
       WHERE glaaent = g_enterprise
         AND glaald  = g_xrcc_d2[l_i].xrccld2
      CALL s_fin_get_curr_rate(l_comp,g_xrcc_d2[l_i].xrccld2,g_today,g_xrcc_d2[l_i].xrcc1002,'')
         RETURNING l_rate1,l_rate2,l_rate3
      #本幣=原幣*今日匯率 
      #LET g_xrcc_d2[l_i].xrcc1192 = g_xrcc_d2[l_i].xrcc1092 * l_rate1
      
      #取說明
      LET g_xrcc_d2[l_i].xrccld2  = s_desc_show1(g_xrcc_d2[l_i].xrccld2,s_desc_get_ld_desc(g_xrcc_d2[l_i].xrccld2))
      LET g_xrcc_d2[l_i].xrca0052 = s_desc_show1(g_xrcc_d2[l_i].xrca0052,s_desc_get_trading_partner_abbr_desc(g_xrcc_d2[l_i].xrca0052))
      
      #現行匯率=原幣金額/本幣金額
      LET g_xrcc_d2[l_i].xrcc101 = l_rate1
      
      LET l_i = l_i + 1
   END FOREACH
   CALL g_xrcc_d2.deleteElement(g_xrcc_d2.getLength())   
   LET g_tot_cnt = g_xrcc_d2.getLength()
   DISPLAY g_tot_cnt TO FORMONLY.h_count
END FUNCTION

################################################################################
# Descriptions...: 計算勾選的數字並顯出
# Date & Author..: 2016/08/29 By lujh
# Modify.........:
################################################################################
PRIVATE FUNCTION axrq400_sumchoice()
   DEFINE l_i    LIKE type_t.num10
   LET g_sum.sumxrcc1092 = 0 
   LET g_sum.sumxrcc1192 = 0
   FOR l_i = 1 TO g_xrcc_d.getLength()
      IF g_xrcc_d[l_i].sel = 'Y' THEN
         IF cl_null(g_xrcc_d[l_i].l_xrcc109)THEN
            LET g_xrcc_d[l_i].l_xrcc109 = 0 
         END IF
         
         IF cl_null(g_xrcc_d[l_i].l_xrcc119)THEN
            LET g_xrcc_d[l_i].l_xrcc119 = 0 
         END IF
                          
         LET g_sum.sumxrcc1092 = g_sum.sumxrcc1092 + g_xrcc_d[l_i].l_xrcc109
         LET g_sum.sumxrcc1192 = g_sum.sumxrcc1192 + g_xrcc_d[l_i].l_xrcc119
      END IF
   END FOR
   
   DISPLAY BY NAME g_sum.sumxrcc1092,g_sum.sumxrcc1192
END FUNCTION

