{<section id="axrq400.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION axrq400_b_fill()
   #add-point:b_fill段define-客製 name="b_fill.define_customerization"
   
   #end add-point
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準  (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   DEFINE l_tmp           type_g_xrcc_d
   #end add-point
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   
   #end add-point
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter, cl_sql_auth_filter()   #(ver:40) add cl_sql_auth_filter()
 
   LET ls_sql_rank = "SELECT  UNIQUE '','','',xrccdocno,'',xrcc009,xrccseq,xrcc001,'','',xrcc003,xrcc004, 
       xrcc002,xrcc100,xrcc108,xrcc109,'','',xrcc118,xrcc119,'','','','','','','','',''  ,DENSE_RANK() OVER( ORDER BY xrcc_t.xrccld, 
       xrcc_t.xrccdocno,xrcc_t.xrccseq,xrcc_t.xrcc001) AS RANK FROM xrcc_t",
 
 
                     "",
                     " WHERE AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("xrcc_t"),
                     " ORDER BY xrcc_t.xrccld,xrcc_t.xrccdocno,xrcc_t.xrccseq,xrcc_t.xrcc001"
 
   #add-point:b_fill段rank_sql_after name="b_fill.rank_sql_after"
   
   #end add-point
 
   LET g_sql = "SELECT COUNT(1) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   #add-point:b_fill段rank_sql_after_count name="b_fill.rank_sql_after_count"
   
   #end add-point
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT '','','',xrccdocno,'',xrcc009,xrccseq,xrcc001,'','',xrcc003,xrcc004,xrcc002,xrcc100, 
       xrcc108,xrcc109,'','',xrcc118,xrcc119,'','','','','','','','',''",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   #處理帳套串連
   #帳務中心限制加入
   LET ls_wc = ls_wc CLIPPED," AND xrcald IN ",g_wc_ld," AND xrcasite IN ",g_wc_site," "
   LET ls_wc = ls_wc CLIPPED," AND xrca001 NOT LIKE ('3%') AND xrca001 NOT LIKE ('0%') "
   LET ls_wc = ls_wc CLIPPED," AND xrcastus = 'Y' "
   IF NOT cl_null(g_sql_ctrl) AND NOT g_sql_ctrl = ' 1=1'  THEN
      LET ls_wc =  ls_wc ," AND xrca005 IN (SELECT pmaa001 FROM pmaa_t WHERE pmaaent = ",g_enterprise, " AND ", g_sql_ctrl,")"
   END IF
   LET ls_wc = ls_wc," AND ",g_user_dept_wc    
   DELETE FROM axrq400_tmp
   
   IF g_input.type = '1' THEN 
      LET g_sql  = "INSERT INTO axrq400_tmp ",
                   "SELECT 'N',xrca_t.xrcald,xrca_t.xrca005,xrca_t.xrcadocno,xrca_t.xrcadocdt,xrcc009,xrccseq,xrcc001,",
                   #161011-00003#1 Add  ---(S)---
                   "xrca035,(SELECT glacl004 FROM glacl_t WHERE glaclent = xrcaent AND glacl001 = (SELECT glaa004 FROM glaa_t WHERE glaaent = xrcaent AND glaald = xrcald) AND glacl002 = xrca035 AND glacl003 = '",g_lang,"'),",
                   #161011-00003#1 Add  ---(E)---
                   "       xrcc003,xrcc004,xrcc002,xrcc100, ",
                   "       COALESCE(xrcc108,0),COALESCE(xrcc109,0),COALESCE(s1.xrce109,0)+COALESCE(s2.xrce109,0)+COALESCE(s3.apce109,0)+COALESCE(s4.apce109,0),0,",
                   "       COALESCE(xrcc118,0),COALESCE(xrcc119,0),COALESCE(s1.xrce119,0)+COALESCE(s2.xrce119,0)+COALESCE(s3.apce119,0)+COALESCE(s4.apce119,0),0,",
                   "       xrca_t.xrca004,",
                   "       xrca_t.xrca057,xrca_t.xrca007,xrca_t.xrca033,xrca_t.xrca015,xrca_t.xrca014,xrca_t.xrca001, ",
                   "       xrccent, ",
                   "       COALESCE(xrca_t.xrca040,'N'),COALESCE(xrca_t.xrca044,0), ",                                    
                   "       COALESCE(s5.xrcc108s5,0), xrca_t.xrca053,0,0",                    
                   " FROM xrca_t ",
                   #抓取待抵來源單的資料 做判斷
                   " LEFT OUTER JOIN(",
                   " SELECT xrccent ent,xrccld ld,xrccdocno docno,(SUM(xrcc108)- SUM(xrcc109)) xrcc108s5 ",
                   "   FROM xrca_t,xrcc_t ",
                   "  WHERE xrcaent = xrccent ",
                   "    AND xrcald  = xrccld ",
                   "    AND xrcadocno = xrccdocno ",
                   " GROUP BY xrccent,xrccld,xrccdocno ",
                   " ) s5 ON s5.ent = xrca_t.xrcaent AND s5.ld = xrca_t.xrcald AND s5.docno = xrca_t.xrca018 ",
                   ",xrcc_t ",
                   #應收沖帳未確認
                   " LEFT OUTER JOIN( ",
                   "    SELECT xrceent,xrceld,xrce003,xrce004,xrce005,",
                   "           SUM(xrce109) xrce109,SUM(xrce119) xrce119 ",
                   "      FROM xrce_t,xrda_t ",
                   "     WHERE xrceent = xrdaent ",
                   "       AND xrceld  = xrdald ",
                   "       AND xrcedocno = xrdadocno ",
                   "       AND xrdastus NOT IN('X','Y') ",
                   "      GROUP BY xrceent,xrceld,xrce003,xrce004,xrce005 ",
                   "                ) s1 ON s1.xrceent = xrccent AND s1.xrceld = xrccld ",
                   "                        AND s1.xrce003 = xrccdocno AND s1.xrce004 = xrccseq AND s1.xrce005 = xrcc001 ",
                   #應收直接沖銷未確認
                   " LEFT OUTER JOIN( ",
                   "    SELECT xrceent,xrceld,xrce003,xrce004,xrce005,",
                   "           SUM(xrce109) xrce109,SUM(xrce119) xrce119 ",
                   "      FROM xrce_t,xrca_t ",
                   "     WHERE xrceent = xrcaent ",
                   "       AND xrceld  = xrcald ",
                   "       AND xrcedocno = xrcadocno ",
                   "       AND xrcastus NOT IN('X','Y') ",
                   "      GROUP BY xrceent,xrceld,xrce003,xrce004,xrce005 ",
                   "                ) s2 ON s2.xrceent = xrccent AND s2.xrceld = xrccld ",
                   "                        AND s2.xrce003 = xrccdocno AND s2.xrce004 = xrccseq AND s2.xrce005 = xrcc001 ",  
                   #應付
                   " LEFT OUTER JOIN( ",
                   "    SELECT apceent,apceld,apce003,apce004,apce005,",
                   "           SUM(apce109) apce109,SUM(apce119) apce119 ",
                   "      FROM apce_t,apda_t ",
                   "     WHERE apceent = apdaent ",
                   "       AND apceld  = apdald ",
                   "       AND apcedocno = apdadocno ",
                   "       AND apdastus NOT IN('X','Y') ",
                   "      GROUP BY apceent,apceld,apce003,apce004,apce005 ",
                   "                ) s3 ON s3.apceent = xrccent AND s3.apceld = xrccld ",
                   "                        AND s3.apce003 = xrccdocno AND s3.apce004 = xrccseq AND s3.apce005 = xrcc001 ",
                   #應付直接沖銷未確認
                   " LEFT OUTER JOIN( ",
                   "    SELECT apceent,apceld,apce003,apce004,apce005,",
                   "           SUM(apce109) apce109,SUM(apce119) apce119 ",
                   "      FROM apce_t,apca_t ",
                   "     WHERE apceent = apcaent ",
                   "       AND apceld  = apcald ",
                   "       AND apcedocno = apcadocno ",
                   "       AND apcastus NOT IN('X','Y') ",
                   "      GROUP BY apceent,apceld,apce003,apce004,apce005 ",
                   "                ) s4 ON s4.apceent = xrccent AND s4.apceld = xrccld ",
                   "                        AND s4.apce003 = xrccdocno AND s4.apce004 = xrccseq AND s4.apce005 = xrcc001 ",                  
                   "WHERE xrcaent = xrccent ",
                   "  AND xrcald  = xrccld ",
                   "  AND xrcadocno = xrccdocno ",
                   "  AND xrccent = ? AND 1=1 AND ",ls_wc,cl_sql_add_filter("xrcc_t"),cl_sql_add_filter("xrca_t"),
                   "  AND (EXISTS(SELECT 1 FROM glba_t WHERE glbaent = xrca_t.xrcaent AND glbald = xrca_t.xrcald AND glba001 = '",g_user,"') ",
                   "         OR EXISTS(SELECT 1 FROM glbb_t WHERE glbbent = xrca_t.xrcaent AND glbbld = xrca_t.xrcald AND glbb001 = '",g_dept,"') ",
                   "         OR (NOT EXISTS(SELECT 1 FROM glba_t WHERE glbaent = xrca_t.xrcaent AND glbald = xrca_t.xrcald) AND NOT EXISTS(SELECT 1 FROM glbb_t WHERE glbbent = xrca_t.xrcaent AND glbbld = xrca_t.xrcald))) ",
                   " ORDER BY xrca_t.xrcald,xrca_t.xrca005,xrca_t.xrcadocno,xrcc009,xrccseq,xrcc001 "
   ELSE
      LET g_sql  = "INSERT INTO axrq400_tmp ",
