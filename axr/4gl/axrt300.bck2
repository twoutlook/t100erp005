          xrcb021,xrcb024,xrcb025,xrcb026,xrcb027,xrcb028,xrcb029,xrcb100,xrcb106,xrcb116,xrcb117,xrcb118, 
          xrcb119,xrcb126,xrcb136,xrcblegl,xrcbsite,xrcbseq,xrcb002,xrcb003,xrcb004,xrcb005,xrcb103, 
          xrcb105,xrcb113,xrcb115,xrcb123,xrcb124,xrcb125,xrcb133,xrcb134,xrcb135
          INTO l_xrcb_t.xrcbseq,l_xrcb_t.xrcborga,l_xrcb_t.xrcb001,l_xrcb_t.xrcb023, 
          l_xrcb_t.xrcb002,l_xrcb_t.xrcb003,l_xrcb_t.xrcb004,l_xrcb_t.xrcb005, 
          l_xrcb_t.xrcb020,l_xrcb_t.xrcb020_desc,l_xrcb_t.xrcb006,l_xrcb_t.xrcb007, 
          l_xrcb_t.xrcb101,l_xrcb_t.xrcb103,l_xrcb_t.xrcb104,l_xrcb_t.xrcb105, 
