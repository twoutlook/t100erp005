         LET l_count = 0
         SELECT COUNT(*) INTO l_count FROM xmdl_t
          #WHERE     xmdldocno = g_xrcb_d[l_ac].xrcb002                             #160905-00002#6  mark
          WHERE     xmdlent = g_enterprise AND xmdldocno = g_xrcb_d[l_ac].xrcb002   #160905-00002#6  add
                AND xmdlseq   = g_xrcb_d[l_ac].xrcb003
         IF l_count < 1 THEN RETURN 'axm-00292' END IF

         LET g_xrcb_d[l_ac].xrcborga = l_xmdl.xmdlsite
         LET g_xrcb_d[l_ac].xrcb023  = 'N'
         LET g_xrcb_d[l_ac].xrcb004  = l_xmdl.xmdl008
        #150828-00001#2 Mark ---(S)---
        #SELECT imaal003 INTO g_xrcb_d[l_ac].xrcb005
        #  FROM imaal_t
        # WHERE imaalent = g_enterprise AND imaal002 = g_lang
        #   AND imaal001 = g_xrcb_d[l_ac].xrcb004
        #150828-00001#2 Mark ---(E)---

         CALL s_axrt300_get_xrcb005(g_xrcb_d[l_ac].xrcb004) RETURNING g_xrcb_d[l_ac].xrcb005   #150828-00001#2 Add

         LET g_xrcb_d[l_ac].xrcb020  = l_xmdk.xmdk012
         #150807-00010#1
         IF l_xmdk.xmdk082 = '4' THEN
            CALL s_fin_dept_reasons_with_ld_get_account(l_xmdk.xmdk004,'310',l_xmdl.xmdl050,g_xrca_m.xrcald,'@2@11',g_xrca_m.xrcadocdt)
                 RETURNING g_sub_success,g_xrcb_d[l_ac].xrcb021,g_errno
