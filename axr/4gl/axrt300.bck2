            WHEN l_s = '2'
               IF l_xmdl.xmdl022 - l_xmdl.xmdl039 <= 0 THEN RETURN 'axm-00283' END IF
            WHEN l_s = '3'
               IF l_xmdl.xmdl022 - l_xmdl.xmdl040 <= 0 THEN RETURN 'axm-00283' END IF
