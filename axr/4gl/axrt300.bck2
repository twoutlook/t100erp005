# Usage..........: CALL axrt300_xrca_dis()
#                  RETURNING --
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt300_xrca_dis(p_cmd)
   DEFINE p_cmd      LIKE type_t.chr1
   
   IF p_cmd = 'a' THEN
      #原幣                    #本位幣
      LET g_xrca_m.xrca103 = 0 LET g_xrca_m.xrca113 = 0
      LET g_xrca_m.xrca106 = 0 LET g_xrca_m.xrca116 = 0
      LET g_xrca_m.xrca104 = 0 LET g_xrca_m.xrca114 = 0
      LET g_xrca_m.xrca107 = 0 LET g_xrca_m.xrca117 = 0
      LET g_xrca_m.xrca108 = 0 LET g_xrca_m.xrca118 = 0
      LET g_xrca_m.xrcc109 = 0 LET g_xrca_m.xrcc119 = 0
      LET g_xrca_m.lbl_calc= 0 LET g_xrca_m.lbl_calc2= 0
      
      #原幣                    #本位幣
      LET g_xrca_m.xrca103_2 = 0 LET g_xrca_m.xrca113_2 = 0
      LET g_xrca_m.xrca104_2 = 0 LET g_xrca_m.xrca114_2 = 0
      LET g_xrca_m.xrca106_2 = 0 LET g_xrca_m.xrca116_2 = 0
      LET g_xrca_m.xrca108_2 = 0 LET g_xrca_m.xrca118_2 = 0
      LET g_xrca_m.xrcc109_2 = 0 LET g_xrca_m.xrcc119_2 = 0
      LET g_xrca_m.lbl_calc_2= 0 LET g_xrca_m.lbl_calc2_2 = 0
      
      #本位幣二                 本位幣三
      LET g_xrca_m.xrca123 = 0 LET g_xrca_m.xrca133 = 0
      LET g_xrca_m.xrcc126 = 0 LET g_xrca_m.xrcc136 = 0
      LET g_xrca_m.xrca124 = 0 LET g_xrca_m.xrca134 = 0
      LET g_xrca_m.xrcc127 = 0 LET g_xrca_m.xrcc137 = 0
      LET g_xrca_m.xrcc128 = 0 LET g_xrca_m.xrcc138 = 0
      LET g_xrca_m.xrcc129 = 0 LET g_xrca_m.xrcc139 = 0
      LET g_xrca_m.lbl_calc3= 0 LET g_xrca_m.lbl_calc4= 0
   END IF
   
   IF p_cmd = 's' THEN
      #原幣、本位幣
      SELECT SUM(xrcc109),SUM(xrcc119) INTO g_xrca_m.xrcc109,g_xrca_m.xrcc119 FROM xrcc_t
       WHERE xrccent = g_enterprise
         AND xrccdocno = g_xrca_m.xrcadocno
         AND xrccld  = g_xrca_m.xrcald
      IF cl_null(g_xrca_m.xrcc109) THEN LET g_xrca_m.xrcc109 = 0 END IF
      IF cl_null(g_xrca_m.xrcc119) THEN LET g_xrca_m.xrcc119 = 0 END IF
         
      LET g_xrca_m.lbl_calc = g_xrca_m.xrca108 - g_xrca_m.xrcc109
      LET g_xrca_m.lbl_calc2= g_xrca_m.xrca118 - g_xrca_m.xrcc119
      
      LET g_xrca_m.lbl_calc_2 = g_xrca_m.lbl_calc
      LET g_xrca_m.lbl_calc2_2= g_xrca_m.lbl_calc2
      
      LET g_xrca_m.xrcc109_2 = g_xrca_m.xrcc109
      LET g_xrca_m.xrcc119_2 = g_xrca_m.xrcc119
   
      #本位幣二
      SELECT SUM(xrcc126),SUM(xrcc127),SUM(xrcc128),SUM(xrcc129)
        INTO g_xrca_m.xrcc126,g_xrca_m.xrcc127,g_xrca_m.xrcc128,g_xrca_m.xrcc129
        FROM xrcc_t
       WHERE xrccent = g_enterprise
         AND xrccdocno = g_xrca_m.xrcadocno
         AND xrccld  = g_xrca_m.xrcald
      IF cl_null(g_xrca_m.xrcc126) THEN LET g_xrca_m.xrcc126 = 0 END IF
      IF cl_null(g_xrca_m.xrcc127) THEN LET g_xrca_m.xrcc127 = 0 END IF
      IF cl_null(g_xrca_m.xrcc128) THEN LET g_xrca_m.xrcc128 = 0 END IF
      IF cl_null(g_xrca_m.xrcc129) THEN LET g_xrca_m.xrcc129 = 0 END IF
   
      LET g_xrca_m.lbl_calc3 = g_xrca_m.xrca128 - g_xrca_m.xrcc129
      
      #本位幣三
      SELECT SUM(xrcc136),SUM(xrcc137),SUM(xrcc138),SUM(xrcc139)
        INTO g_xrca_m.xrcc136,g_xrca_m.xrcc137,g_xrca_m.xrcc138,g_xrca_m.xrcc139
        FROM xrcc_t
       WHERE xrccent = g_enterprise
         AND xrccdocno = g_xrca_m.xrcadocno
         AND xrccld  = g_xrca_m.xrcald
      IF cl_null(g_xrca_m.xrcc136) THEN LET g_xrca_m.xrcc136 = 0 END IF
      IF cl_null(g_xrca_m.xrcc137) THEN LET g_xrca_m.xrcc137 = 0 END IF
      IF cl_null(g_xrca_m.xrcc138) THEN LET g_xrca_m.xrcc138 = 0 END IF
      IF cl_null(g_xrca_m.xrcc139) THEN LET g_xrca_m.xrcc139 = 0 END IF
   
      LET g_xrca_m.lbl_calc4 = g_xrca_m.xrca138 - g_xrca_m.xrcc139
   
   END IF
   
   DISPLAY BY NAME g_xrca_m.xrca103,g_xrca_m.xrca113,g_xrca_m.xrca106,g_xrca_m.xrca116,g_xrca_m.xrca104,g_xrca_m.xrca114,
                   g_xrca_m.xrca107,g_xrca_m.xrca117,g_xrca_m.xrca108,g_xrca_m.xrca118,g_xrca_m.xrcc109,g_xrca_m.xrcc119,
                   g_xrca_m.lbl_calc,g_xrca_m.lbl_calc2,g_xrca_m.xrca103_2,g_xrca_m.xrca113_2,g_xrca_m.xrca104_2,
                   g_xrca_m.xrca114_2,g_xrca_m.xrca106_2,g_xrca_m.xrca116_2,g_xrca_m.xrca108_2,g_xrca_m.xrca118_2,
                   g_xrca_m.xrcc109_2,g_xrca_m.xrcc119_2,g_xrca_m.lbl_calc_2,g_xrca_m.lbl_calc2_2,
                   g_xrca_m.xrca123,g_xrca_m.xrca133,g_xrca_m.xrcc126,g_xrca_m.xrcc136,g_xrca_m.xrca124,g_xrca_m.xrca134,
                   g_xrca_m.xrcc127,g_xrca_m.xrcc137,g_xrca_m.xrcc128,g_xrca_m.xrcc138,g_xrca_m.xrcc129,g_xrca_m.xrcc139,
                   g_xrca_m.lbl_calc3,g_xrca_m.lbl_calc4

END FUNCTION
################################################################################
# Descriptions...: 單頭匯率、收款條件、稅別修改後重新計算單身本幣金額,
#                  然後依次更新單頭本幣金額、多帳期金額、沖暫估金額
# Memo...........:
# Usage..........: CALL axrt300_amount_refresh(p_flag)
#                  RETURNING --
# Input parameter: p_flag         是否需要修改單身稅別  Y/N
# Return code....: 传入参数变量1   传入参数变量说明1
# Date & Author..: 2014/03/14 By zhangwei
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt300_amount_refresh(p_flag)
   DEFINE p_flag                LIKE type_t.chr1
   DEFINE l_count               LIKE type_t.num5
   DEFINE l_chr                 LIKE type_t.chr1
   DEFINE l_xrcb                RECORD LIKE xrcb_t.*
   DEFINE l_sql                 STRING
   DEFINE l_xrcd009             LIKE xrcd_t.xrcd009
   DEFINE l_success             LIKE type_t.num5
   #151008-00009#8--s
   DEFINE ls_js             STRING
   DEFINE lc_param          RECORD
          xrcasite          LIKE xrca_t.xrcasite,     
          xrcald            LIKE xrca_t.xrcald,       
          xrcacomp          LIKE xrca_t.xrcacomp,     
          xrcadocno         LIKE xrca_t.xrcadocno,    
          xrcadocdt         LIKE xrca_t.xrcadocdt,    
          xrca001           LIKE xrca_t.xrca001,
          xrca003           LIKE xrca_t.xrca003,      
          xrca004           LIKE xrca_t.xrca004,      
          xrca005           LIKE xrca_t.xrca005,      
          xrca058           LIKE xrca_t.xrca058,      
          xrca100           LIKE xrca_t.xrca100,      
          xrca101           LIKE xrca_t.xrca101,      
          xrca121           LIKE xrca_t.xrca121,      
          xrca131           LIKE xrca_t.xrca131,      
          xrcbseq           LIKE xrcb_t.xrcbseq,      
          xrcb002           LIKE xrcb_t.xrcb002,    
          xrcb003           LIKE xrcb_t.xrcb003,    
          xrcb004           LIKE xrcb_t.xrcb004,   
          xrcb008           LIKE xrcb_t.xrcb008,    
          xrcb009           LIKE xrcb_t.xrcb009,    
          xrcb010           LIKE xrcb_t.xrcb010,      
          xrcb011           LIKE xrcb_t.xrcb011,      
          xrcb012           LIKE xrcb_t.xrcb012,      
          xrcb015           LIKE xrcb_t.xrcb015,      
          xrcb016           LIKE xrcb_t.xrcb016,      
          xrcb021           LIKE xrcb_t.xrcb021,
          xrcb024           LIKE xrcb_t.xrcb024,      
          xrcb033           LIKE xrcb_t.xrcb033,      
          xrcb034           LIKE xrcb_t.xrcb034,      
          xrcb035           LIKE xrcb_t.xrcb035,      
          xrcb036           LIKE xrcb_t.xrcb036,      
          xrcb037           LIKE xrcb_t.xrcb037,      
          xrcb038           LIKE xrcb_t.xrcb038,      
          xrcb039           LIKE xrcb_t.xrcb039,      
          xrcb040           LIKE xrcb_t.xrcb040,      
          xrcb041           LIKE xrcb_t.xrcb041,      
          xrcb042           LIKE xrcb_t.xrcb042,      
          xrcb043           LIKE xrcb_t.xrcb043,      
          xrcb044           LIKE xrcb_t.xrcb044,      
          xrcb045           LIKE xrcb_t.xrcb045,      
          xrcb046           LIKE xrcb_t.xrcb046,      
          xrcb047           LIKE xrcb_t.xrcb047,      
          xrcb051           LIKE xrcb_t.xrcb051,      
          xrcb103           LIKE xrcb_t.xrcb103,      
          xrcb113           LIKE xrcb_t.xrcb113,      
          xrcb123           LIKE xrcb_t.xrcb123,      
          xrcb133           LIKE xrcb_t.xrcb133,
          type1             LIKE type_t.chr1       #異動類型(xreq003)          
                        END RECORD   
   #151008-00009#8--e   
   DEFINE l_xmdb002             LIKE xmdb_t.xmdb002 #160517-00001#7 Add
   DEFINE l_xmdasite            LIKE xmdb_t.xmdb002 #160517-00001#7 Add

   LET l_success = TRUE

   IF cl_null(p_flag) THEN LET p_flag = 'N' END IF

   #STEP1.判斷單身是否有資料,若無資料則不做差異判斷
   #STEP2.判斷單頭收款條件、稅別、匯率是否有做修改
   #STEP2.重新產生單身資料、重新產生沖暫估資料
   #STEP3.重新產生單頭金額
   #STEP4.重新展算多帳期

   #STEP1
   SELECT COUNT(*) INTO l_count FROM xrcb_t WHERE xrcbent = g_enterprise AND xrcbdocno = g_xrca_m.xrcadocno AND xrcbld = g_xrca_m.xrcald
   
   IF l_count < 1 THEN RETURN END IF

   #STEP2
   #稅別修改　　：重產單身、重產沖暫估、回寫單頭、重產多帳期
   #匯率修改　　：重產單身、重產沖暫估、回寫單頭、重產多帳期
   #收款條件修改：重產多帳期

   LET l_chr = 'N'
   IF g_xrca_m.xrca101 <> g_xrca_m_t.xrca101 OR g_xrca_m_t.xrca101  IS NULL THEN LET l_chr = 'Y' END IF
   IF (g_xrca_m.xrca121 <> g_xrca_m_t.xrca121 OR g_xrca_m_t.xrca121 IS NULL) AND g_glaa.glaa015 = 'Y' THEN LET l_chr = 'Y' END IF
   IF (g_xrca_m.xrca131 <> g_xrca_m_t.xrca131 OR g_xrca_m_t.xrca131 IS NULL) AND g_glaa.glaa019 = 'Y' THEN LET l_chr = 'Y' END IF

   #160517-00001#7 Add  ---(S)---
   IF g_xrca_m.xrca001 = '12' AND g_xrca_m.xrca016 = '10' THEN
      SELECT xmdb002 INTO l_xmdb002 FROM xmdb_t
       WHERE xmdbent = g_enterprise AND xmdbdocno = g_xrca_m.xrca018 AND xmdb001 = g_xrca_m.xrca064
      SELECT xmdasite FROM xmda_t WHERE xmdaent = g_enterprise
         AND xmdadocno = g_xrca_m.xrca018
      CALL s_dep_pay(g_xrca_m.xrca018,g_xrca_m.xrca064,g_xrca_m.xrca011,l_xmdasite,g_xrca_m.xrca101,l_xmdb002) RETURNING l_success

      LET l_sql = "SELECT xmdcseq,xmdc011,xmdc046,xmdc048,xmdc047,flag FROM s_dep_pay_tmp WHERE xmdadocno = ? AND xmdb001 = ? ORDER BY xmdcseq"   #160517-00001#8 Add
      PREPARE axrt300_sub_prep FROM l_sql
      DECLARE axrt300_sub_curs CURSOR FOR axrt300_sub_prep
      SELECT * INTO g_glaa.* FROM glaa_t WHERE glaaent = g_enterprise AND glaald = g_xrca_m.xrcald

   END IF
   #160517-00001#7 Add  ---(E)---

   IF p_flag = 'Y' THEN
      UPDATE xrcb_t SET xrcb020 = g_xrca_m.xrca011
       WHERE xrcbent   = g_enterprise
         AND xrcbdocno = g_xrca_m.xrcadocno
         AND xrcbld    = g_xrca_m.xrcald
      IF SQLCA.SQLCODE OR SQLCA.SQLERRD[3] < 1 THEN
          CALL s_transaction_end('N','0')
          INITIALIZE g_errparam TO NULL 
          LET g_errparam.extend = "" 
          LET g_errparam.code   = "axr-00242" 
          LET g_errparam.popup  = TRUE 
          CALL cl_err()
          CALL s_transaction_end('N','0')
          RETURN
      END IF
   END IF

   LET l_sql = "SELECT * FROM xrcb_t WHERE xrcbent = '",g_enterprise,"' AND xrcbdocno = '",g_xrca_m.xrcadocno,"' AND xrcbld = '",g_xrca_m.xrcald,"'",
               "   AND xrcb001 <> '10' "   #160517-00001#7 Add
   PREPARE axrt300_xrcb_prep2 FROM l_sql
   DECLARE axrt300_xrcb_curs2 CURSOR FOR axrt300_xrcb_prep2

   IF p_flag = 'Y' OR l_chr = 'Y' THEN
      #刪除沖暫估信息
      DELETE FROM xrcf_t WHERE xrcfent = g_enterprise
         AND xrcfdocno = g_xrca_m.xrcadocno
         AND xrcfld    = g_xrca_m.xrcald

      FOREACH axrt300_xrcb_curs2 INTO l_xrcb.*
         IF l_xrcb.xrcb001 <> '10' THEN   #160517-00001#7 Add
            #更新單身本幣金額
            CALL s_tax_ins(g_xrca_m.xrcadocno,l_xrcb.xrcbseq,0,g_glaa.glaacomp,
                           l_xrcb.xrcb007*l_xrcb.xrcb101,l_xrcb.xrcb020,
                           l_xrcb.xrcb007,g_xrca_m.xrca100,g_xrca_m.xrca101,g_xrca_m.xrcald,g_xrca_m.xrca121,g_xrca_m.xrca131)
               RETURNING l_xrcb.xrcb103,l_xrcb.xrcb104,l_xrcb.xrcb105,
                         l_xrcb.xrcb113,l_xrcb.xrcb114,l_xrcb.xrcb115,
                         l_xrcb.xrcb123,l_xrcb.xrcb124,l_xrcb.xrcb125,
                         l_xrcb.xrcb133,l_xrcb.xrcb134,l_xrcb.xrcb135
            SELECT glab005 INTO l_xrcd009 FROM glab_t 
             WHERE glabld = g_xrca_m.xrcald 
               AND glabent = g_enterprise
               AND glab002 = g_xrca_m.xrca007   # 帳款類別
               AND glab001 = '13'             # 應收帳務類型科目設定
               AND glab003 = '8304_29'
            UPDATE xrcd_t SET xrcd009 = l_xrcd009
             WHERE xrcd009 IS NULL
               AND xrcdent = g_enterprise
               AND xrcddocno = g_xrca_m.xrcadocno
               AND xrcdld = g_xrca_m.xrcald

           #150515-00013#3 Add  ---(S)---
            IF g_xrca_m.xrca100 = g_glaa.glaa001 THEN
               LET l_xrcb.xrcb113 = l_xrcb.xrcb103
               LET l_xrcb.xrcb114 = l_xrcb.xrcb104
               LET l_xrcb.xrcb115 = l_xrcb.xrcb105
               UPDATE xrcd_t SET xrcd113 = xrcd103,
                                 xrcd114 = xrcd104,
                                 xrcd115 = xrcd105
                WHERE xrcdent = g_enterprise
                  AND xrcddocno = g_xrca_m.xrcadocno
