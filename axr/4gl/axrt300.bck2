# Memo...........:
# Usage..........: CALL s_aooi150_ins (传入参数)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt300_set_scc()
   DEFINE l_sfin2022            LIKE type_t.chr10
   DEFINE l_sql                 STRING
   DEFINE l_gzcb002             LIKE gzcb_t.gzcb002
   DEFINE l_str                 STRING
   DEFINE l_glaacomp            LIKE glaa_t.glaacomp

   IF NOT cl_null(g_xrca_m.xrcald) THEN
      SELECT glaacomp INTO l_glaacomp FROM glaa_t WHERE glaaent = g_enterprise
         AND glaald = g_xrca_m.xrcald
      CALL cl_get_para(g_enterprise,l_glaacomp,'S-FIN-2022') RETURNING l_sfin2022
   ELSE
      LET l_sfin2022 = '02'
   END IF
   IF g_xrca_m.xrca001 = '12' AND l_sfin2022 = '02' THEN
      LET l_sql = "SELECT gzcb002 FROM gzcb_t ",
                  " WHERE gzcb001 = '8340' AND (gzcb004 LIKE '%",g_prog,"%' OR gzcb005 LIKE '%",g_prog,"%')",
                  "   AND gzcb002 <> '10'"
   ELSE
      LET l_sql = "SELECT gzcb002 FROM gzcb_t ",
                  " WHERE gzcb001 = '8340' AND (gzcb004 LIKE '%",g_prog,"%' OR gzcb005 LIKE '%",g_prog,"%')"
   END IF
   PREPARE axrt300_xrcb001_p1 FROM l_sql
   DECLARE axrt300_xrcb001_c1 CURSOR FOR axrt300_xrcb001_p1
   LET l_str = Null
   LET l_gzcb002 = Null
   FOREACH axrt300_xrcb001_c1 INTO l_gzcb002
      IF cl_null(l_str) THEN LET l_str = l_gzcb002 CONTINUE FOREACH END IF
      LET l_str = l_str,",",l_gzcb002
   END FOREACH
   CALL cl_set_combo_scc_part('xrca016','8340',l_str)
   CALL cl_set_combo_scc_part('xrcb001','8340',l_str)


END FUNCTION

 
