              g_xrcb3_d[l_ac].xrcb123,g_xrcb3_d[l_ac].xrcb124,g_xrcb3_d[l_ac].xrcb125,g_xrcb3_d[l_ac].xrcb133,g_xrcb3_d[l_ac].xrcb134,
              g_xrcb3_d[l_ac].xrcb135,g_xrcb_d[l_ac].xrcb004
           TO s_detail1[l_ac].xrcborga,s_detail1[l_ac].xrcb023,s_detail1[l_ac].xrcb005,s_detail1[l_ac].xrcb020,s_detail1[l_ac].xrcb022,
              s_detail1[l_ac].xrcb006, s_detail1[l_ac].xrcb007,s_detail1[l_ac].xrcb101,s_detail1[l_ac].xrcb111,s_detail1[l_ac].xrcb103,
              s_detail1[l_ac].xrcb104, s_detail1[l_ac].xrcb105,s_detail1[l_ac].xrcb113,s_detail1[l_ac].xrcb114,s_detail1[l_ac].xrcb115,
              s_detail3[l_ac].xrcb123, s_detail3[l_ac].xrcb124,s_detail3[l_ac].xrcb125,s_detail3[l_ac].xrcb133,s_detail3[l_ac].xrcb134,
              s_detail3[l_ac].xrcb135, s_detail1[l_ac].xrcb004
      
      #150518-00046#2 --s add      
      WHEN g_xrcb_d[l_ac].xrcb001 = '1D'
         #判斷資料存在性
         INITIALIZE l_rmda.* TO NULL
         INITIALIZE l_rmdb.* TO NULL
         SELECT * INTO l_rmda.*,l_rmdb.*
           FROM rmda_t,rmdb_t,rmba_t a
          WHERE rmdaent   = rmdbent
            AND rmdadocno = rmdbdocno
            AND rmdbent = rmbaent
            AND rmdb001 = rmbadocno
            AND rmba000 = (SELECT MAX (rmba000)     #以版次最大為準
                             FROM rmba_t
                            WHERE rmbadocno = a.rmbadocno AND rmbaent = a.rmbaent)      
            AND rmdaent   = g_enterprise
            AND rmdadocno = g_xrcb_d[l_ac].xrcb002
            AND rmdbseq   = g_xrcb_d[l_ac].xrcb003
            AND rmda007   = g_xrca_m.xrca004
            AND rmda008   = g_xrca_m.xrca004
         
         IF cl_null(l_rmda.rmdadocno) THEN RETURN 'arm-00009' END IF
         
         #判斷資料有效性
         IF l_rmda.rmdastus <> 'S' THEN RETURN 'sub-00231' END IF

         #只處理出貨日<=立帳日
         IF l_rmda.rmda001 > g_xrca_m.xrcadocdt THEN RETURN 'arm-00052' END IF
         #只處理同帳款對象
         IF l_rmda.rmda007 <> g_xrca_m.xrca004 THEN RETURN 'arm-00053' END IF

         LET l_count = 0
         SELECT COUNT(*) INTO l_count FROM rmdb_t
          WHERE rmdbent = g_enterprise
            AND rmdbdocno = g_xrcb_d[l_ac].xrcb002
            AND rmdbseq   = g_xrcb_d[l_ac].xrcb003
            
         IF l_count < 1 THEN RETURN 'ais-00307' END IF

         LET g_xrcb_d[l_ac].xrcborga = l_rmdb.rmdbsite
         LET g_xrcb_d[l_ac].xrcb023  = 'N'
         LET g_xrcb_d[l_ac].xrcb004  = l_rmdb.rmdb003
         CALL s_axrt300_get_xrcb005(g_xrcb_d[l_ac].xrcb004) RETURNING g_xrcb_d[l_ac].xrcb005 
         
         SELECT rmba006,rmba008 INTO g_xrcb_d[l_ac].xrcb020,l_rmba008
           FROM rmba_t
          WHERE rmbaent = g_enterprise
            AND rmbadocno = l_rmda.rmda004
            AND rmba000 IN (SELECT MAX(rmba000) FROM rmba_t 
                             WHERE rmbaent = g_enterprise 
                               AND rmbadocno = l_rmda.rmda004)          
         LET g_xrcb_d[l_ac].xrcb022  = 1
         LET g_xrcb_d[l_ac].xrcb006  = l_rmdb.rmdb005
         LET g_xrcb_d[l_ac].xrcb101  = l_rmdb.rmdb015
         LET g_xrcb_d[l_ac].xrcb031 = g_xrca_m.xrca008
         CALL s_axrt300_exrate(g_glaa.glaa002,g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa001,g_xrcb_d[l_ac].xrcb101,g_xrca_m.xrca101,g_glaa.glaacomp)
            RETURNING l_success,g_xrcb_d[l_ac].xrcb111

	      #已存在axrt300單身之數量與金額
　　　    SELECT SUM(xrcb007),SUM(xrcb103),SUM(xrcb105) 
           INTO g_xrcb_d[l_ac].xrcb030,l_rmdb016_u,l_rmdb017_u 
           FROM xrca_t,xrcb_t 
          WHERE xrcaent = g_enterprise
            AND xrcaent = xrcbent 
            AND xrcald = xrcbld 
            AND xrcadocno = xrcbdocno
            AND xrcb002 = g_xrcb_d[l_ac].xrcb002 
            AND xrcb003 = g_xrcb_d[l_ac].xrcb003
            AND xrcastus <> 'X'
            AND ((xrcbdocno = g_xrca_m.xrcadocno
            AND   xrcbseq <> g_xrcb_d[l_ac].xrcbseq)
             OR   xrcbdocno <> g_xrca_m.xrcadocno )   
         
         IF cl_null(g_xrcb_d[l_ac].xrcb030) THEN LET g_xrcb_d[l_ac].xrcb030 = 0 END IF
         IF cl_null(l_rmdb016_u) THEN LET l_rmdb016_u = 0 END IF
         IF cl_null(l_rmdb017_u) THEN LET l_rmdb017_u = 0 END IF
         LET g_xrcb_d[l_ac].xrcb007 = l_rmdb.rmdb013 - g_xrcb_d[l_ac].xrcb030
         
         #一次性立帳(本次數量=計價數量),則原幣金額全部從RMA覆出單身而來
         IF g_xrcb_d[l_ac].xrcb007 = l_rmdb.rmdb013 THEN
            LET g_xrcb_d[l_ac].xrcb103 = l_rmdb.rmdb016
            LET g_xrcb_d[l_ac].xrcb105 = l_rmdb.rmdb017
            LET g_xrcb_d[l_ac].xrcb104 = g_xrcb_d[l_ac].xrcb105 - g_xrcb_d[l_ac].xrcb103
            IF l_rmba008 = 'Y' THEN   #含稅
               LET l_xrcb105 = l_rmdb.rmdb017
            ELSE
               LET l_xrcb105 = l_rmdb.rmdb016
            END IF             

            CALL s_tax_ins(g_xrca_m.xrcadocno,g_xrcb_d[l_ac].xrcbseq,0,g_glaa.glaacomp,
                           l_xrcb105,g_xrcb_d[l_ac].xrcb020,g_xrcb_d[l_ac].xrcb007,g_xrca_m.xrca100,
                           g_xrca_m.xrca101,g_xrca_m.xrcald,g_xrca_m.xrca121,g_xrca_m.xrca131)
               RETURNING r_xrcd103,r_xrcd104,r_xrcd105,
                         g_xrcb_d[l_ac].xrcb113,g_xrcb_d[l_ac].xrcb114,g_xrcb_d[l_ac].xrcb115,
                         r_xrcd123,r_xrcd124,r_xrcd125,r_xrcd133,r_xrcd134,r_xrcd135
         ELSE
            IF l_rmba008 = 'Y' THEN   #含稅
               LET l_xrcb105 = l_rmdb.rmdb017 - l_rmdb017_u
            ELSE
               LET l_xrcb105 = l_rmdb.rmdb016 - l_rmdb016_u
            END IF

            CALL s_tax_ins(g_xrca_m.xrcadocno,g_xrcb_d[l_ac].xrcbseq,0,g_glaa.glaacomp,
                           l_xrcb105,g_xrcb_d[l_ac].xrcb020,g_xrcb_d[l_ac].xrcb007,g_xrca_m.xrca100,
                           g_xrca_m.xrca101,g_xrca_m.xrcald,g_xrca_m.xrca121,g_xrca_m.xrca131)
               RETURNING g_xrcb_d[l_ac].xrcb103,g_xrcb_d[l_ac].xrcb104,g_xrcb_d[l_ac].xrcb105,
                         g_xrcb_d[l_ac].xrcb113,g_xrcb_d[l_ac].xrcb114,g_xrcb_d[l_ac].xrcb115,
                         g_xrcb3_d[l_ac].xrcb123,g_xrcb3_d[l_ac].xrcb124,g_xrcb3_d[l_ac].xrcb125,
                         g_xrcb3_d[l_ac].xrcb133,g_xrcb3_d[l_ac].xrcb134,g_xrcb3_d[l_ac].xrcb135
         END IF                     
                      
         SELECT glab005 INTO l_xrcd009 FROM glab_t 
          WHERE glabld = g_xrca_m.xrcald 
            AND glabent = g_enterprise
            AND glab002 = g_xrca_m.xrca007   # 帳款類別
            AND glab001 = '13'             # 應收帳務類型科目設定
            AND glab003 = '8304_29'
         UPDATE xrcd_t SET xrcd009 = l_xrcd009
          WHERE xrcd009 IS NULL
            AND xrcdent = g_enterprise
            AND xrcddocno = g_xrca_m.xrcadocno
            AND xrcdld = g_xrca_m.xrcald
                      
         IF g_xrca_m.xrca100 = g_glaa.glaa001 THEN
            LET g_xrcb_d[l_ac].xrcb113 = g_xrcb_d[l_ac].xrcb103
            LET g_xrcb_d[l_ac].xrcb114 = g_xrcb_d[l_ac].xrcb104
            LET g_xrcb_d[l_ac].xrcb115 = g_xrcb_d[l_ac].xrcb105
            UPDATE xrcd_t SET xrcd113 = xrcd103,
                              xrcd114 = xrcd104,
                              xrcd115 = xrcd105
             WHERE xrcdent = g_enterprise
               AND xrcddocno = g_xrca_m.xrcadocno
               AND xrcdld = g_xrca_m.xrcald
         END IF
         
      LET g_xrcb_d[l_ac].xrcb102 = g_xrca_m.xrca101   #160816-00014#1 Add

      DISPLAY g_xrcb_d[l_ac].xrcborga,g_xrcb_d[l_ac].xrcb023, g_xrcb_d[l_ac].xrcb005, g_xrcb_d[l_ac].xrcb020, g_xrcb_d[l_ac].xrcb022,
              g_xrcb_d[l_ac].xrcb006, g_xrcb_d[l_ac].xrcb007, g_xrcb_d[l_ac].xrcb101, g_xrcb_d[l_ac].xrcb111, g_xrcb_d[l_ac].xrcb103,
              g_xrcb_d[l_ac].xrcb104, g_xrcb_d[l_ac].xrcb105, g_xrcb_d[l_ac].xrcb113, g_xrcb_d[l_ac].xrcb114, g_xrcb_d[l_ac].xrcb115,
              g_xrcb3_d[l_ac].xrcb123,g_xrcb3_d[l_ac].xrcb124,g_xrcb3_d[l_ac].xrcb125,g_xrcb3_d[l_ac].xrcb133,g_xrcb3_d[l_ac].xrcb134,
              g_xrcb3_d[l_ac].xrcb135,g_xrcb_d[l_ac].xrcb004
           TO s_detail1[l_ac].xrcborga,s_detail1[l_ac].xrcb023,s_detail1[l_ac].xrcb005,s_detail1[l_ac].xrcb020,s_detail1[l_ac].xrcb022,
              s_detail1[l_ac].xrcb006, s_detail1[l_ac].xrcb007,s_detail1[l_ac].xrcb101,s_detail1[l_ac].xrcb111,s_detail1[l_ac].xrcb103,
              s_detail1[l_ac].xrcb104, s_detail1[l_ac].xrcb105,s_detail1[l_ac].xrcb113,s_detail1[l_ac].xrcb114,s_detail1[l_ac].xrcb115,
              s_detail3[l_ac].xrcb123, s_detail3[l_ac].xrcb124,s_detail3[l_ac].xrcb125,s_detail3[l_ac].xrcb133,s_detail3[l_ac].xrcb134,
              s_detail3[l_ac].xrcb135, s_detail1[l_ac].xrcb004
      
      LET g_xrcb_d_t.* = g_xrcb_d[l_ac].*
      #150518-00046#2 --e add
      
      #160426-00013#4-s
      WHEN g_xrcb_d[l_ac].xrcb001 = '10' #訂金發票
         
         #判斷資料存在性
         SELECT * INTO l_isaf.* FROM isaf_t WHERE isafent = g_enterprise AND isafdocno = g_xrcb_d[l_ac].xrcb002

         SELECT * INTO l_isag.* FROM isag_t
          WHERE isagent   = g_enterprise
            AND isagdocno = g_xrcb_d[l_ac].xrcb002
            AND isagseq   = g_xrcb_d[l_ac].xrcb003
         
         IF cl_null(l_isag.isagdocno) THEN RETURN 'ais-00029' END IF
         
         #單據要確認
         IF l_isaf.isafstus <> 'Y' THEN RETURN 'aap-00324' END IF
         
         #日期不可大於立帳單日期
         IF l_isaf.isafdocdt > g_xrca_m.xrcadocdt THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = 'axm-00784'
            LET g_errparam.extend = g_xrca_m.xrca018
            LET g_errparam.popup = TRUE
            CALL cl_err()
            RETURN 'N'
         END IF
         
         #只處理同帳款對象
         IF l_isaf.isaf003 <> g_xrca_m.xrca004 THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = 'axm-00282'
            LET g_errparam.extend = g_xrca_m.xrca018
            LET g_errparam.popup = TRUE
            CALL cl_err()
            RETURN 'N'
         END IF

         #160802-00007#1 Add  ---(S)---
         SELECT pmaa004 INTO l_pmaa004 FROM pmaa_t WHERE pmaaent = g_enterprise AND pmaa001 = g_xrca_m.xrca004
         IF l_pmaa004 = '2' THEN
            LET l_count = 0 
