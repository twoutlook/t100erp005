         IF l_xmdk.xmdk001 > g_xrca_m.xrcadocdt THEN RETURN 'axm-00290' END IF
        ##150210-00011(1)--20150330--add--
        ##来源单据扣账日期必须要和单头的立账日期在同一个会计期间内
        #IF l_xmdk.xmdk001 < g_pdate_s OR l_xmdk.xmdk001 > g_pdate_e THEN RETURN 'axr-00318' END IF
        ##150210-00011(1)--20150330--add--
          #只處理同帳款對象
         IF l_xmdk.xmdk008 <> g_xrca_m.xrca004 THEN RETURN 'axm-00291' END IF

         #160802-00007#1 Add  ---(S)---
         SELECT pmaa004 INTO l_pmaa004 FROM pmaa_t WHERE pmaaent = g_enterprise AND pmaa001 = g_xrca_m.xrca004
         IF l_pmaa004 = '2' THEN
            LET l_count = 0 
            SELECT COUNT(1) INTO l_count FROM xmdk_t WHERE xmdkent = g_enterprise AND xmdkdocno = g_xrcb_d[l_ac].xrcb002
               AND xmdk047 = g_xrca_m.xrca057
            IF cl_null(l_count) THEN LET l_count = 0 END IF
            IF l_count = 0 THEN
               RETURN 'axr-00379'
            END IF
         END IF
         #160802-00007#1 Add  ---(E)---

         CALL s_axrt300_sel_ld(g_xrca_m.xrcald) RETURNING l_success,l_s

         #檢查單身是否存在符合條件的資料
         IF cl_null(l_xmdl.xmdl022) THEN LET l_xmdl.xmdl022 = 0 END IF
         IF cl_null(l_xmdl.xmdl038) THEN LET l_xmdl.xmdl038 = 0 END IF
         IF cl_null(l_xmdl.xmdl039) THEN LET l_xmdl.xmdl039 = 0 END IF
         IF cl_null(l_xmdl.xmdl040) THEN LET l_xmdl.xmdl040 = 0 END IF
         CASE
            WHEN l_s = '1'
               IF l_xmdl.xmdl022 - l_xmdl.xmdl038 <= 0 THEN RETURN 'axm-00293' END IF
            WHEN l_s = '2'
               IF l_xmdl.xmdl022 - l_xmdl.xmdl039 <= 0 THEN RETURN 'axm-00293' END IF
            WHEN l_s = '3'
               IF l_xmdl.xmdl022 - l_xmdl.xmdl040 <= 0 THEN RETURN 'axm-00293' END IF
         END CASE
