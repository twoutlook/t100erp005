         END IF
         #150807-00010#1
         LET g_xrcb_d[l_ac].xrcb022  = -1
         LET g_xrcb_d[l_ac].xrcb006  = l_xmdl.xmdl021   #160614-00016#1 Mod xmdl017 --> xmdl021 
         LET g_xrcb_d[l_ac].xrcb007  = l_xmdl.xmdl018
         LET g_xrcb_d[l_ac].xrcb101  = l_xmdl.xmdl024
         LET g_xrcb_d[l_ac].xrcb031 = ''
