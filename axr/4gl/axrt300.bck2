         IF l_count < 1 THEN RETURN 'axm-00285' END IF

         LET g_xrcb_d[l_ac].xrcborga = l_xmdl.xmdlsite
         LET g_xrcb_d[l_ac].xrcb023  = 'N'
         LET g_xrcb_d[l_ac].xrcb004  = l_xmdl.xmdl008
        #150828-00001#2 Mark ---(S)---
        #SELECT imaal003 INTO g_xrcb_d[l_ac].xrcb005
        #  FROM imaal_t
        # WHERE imaalent = g_enterprise AND imaal002 = g_lang
        #   AND imaal001 = g_xrcb_d[l_ac].xrcb004
        #150828-00001#2 Mark ---(E)---

         CALL s_axrt300_get_xrcb005(g_xrcb_d[l_ac].xrcb004) RETURNING g_xrcb_d[l_ac].xrcb005   #150828-00001#2 Add

         LET g_xrcb_d[l_ac].xrcb020  = l_xmdk.xmdk012
         LET g_xrcb_d[l_ac].xrcb022  = 1
         LET g_xrcb_d[l_ac].xrcb006  = l_xmdl.xmdl021   #160614-00016#1 Mod xmdl017 --> xmdl021 
         LET g_xrcb_d[l_ac].xrcb007  = l_xmdl.xmdl018
         LET g_xrcb_d[l_ac].xrcb101  = l_xmdl.xmdl024
         LET g_xrcb_d[l_ac].xrcb031 = l_xmdk.xmdk010
         LET g_xrcb4_d[l_ac].xrcb015 = l_xmdl.xmdl030   #160520-00008#1 Add
         LET g_xrcb_d[l_ac].xrcb102 = l_xmdk.xmdk013   #160907-00009#1 Add
         IF cl_null(g_xrcb_d[l_ac].xrcb031) THEN
            LET g_xrcb_d[l_ac].xrcb031 = g_xrca_m.xrca008
         END IF
         CALL s_axrt300_exrate(g_glaa.glaa002,g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa001,g_xrcb_d[l_ac].xrcb101,g_xrca_m.xrca101,g_glaa.glaacomp)
            RETURNING l_success,g_xrcb_d[l_ac].xrcb111
         CALL s_tax_ins(g_xrca_m.xrcadocno,g_xrcb_d[l_ac].xrcbseq,0,g_glaa.glaacomp,
                        g_xrcb_d[l_ac].xrcb007*g_xrcb_d[l_ac].xrcb101,g_xrcb_d[l_ac].xrcb020,
                        g_xrcb_d[l_ac].xrcb007,g_xrca_m.xrca100,g_xrca_m.xrca101,g_xrca_m.xrcald,g_xrca_m.xrca121,g_xrca_m.xrca131)
            RETURNING g_xrcb_d[l_ac].xrcb103,g_xrcb_d[l_ac].xrcb104,g_xrcb_d[l_ac].xrcb105,
                      g_xrcb_d[l_ac].xrcb113,g_xrcb_d[l_ac].xrcb114,g_xrcb_d[l_ac].xrcb115,
                      g_xrcb3_d[l_ac].xrcb123,g_xrcb3_d[l_ac].xrcb124,g_xrcb3_d[l_ac].xrcb125,
                      g_xrcb3_d[l_ac].xrcb133,g_xrcb3_d[l_ac].xrcb134,g_xrcb3_d[l_ac].xrcb135
      SELECT glab005 INTO l_xrcd009 FROM glab_t 
       WHERE glabld = g_xrca_m.xrcald 
         AND glabent = g_enterprise
         AND glab002 = g_xrca_m.xrca007   # 帳款類別
         AND glab001 = '13'             # 應收帳務類型科目設定
         AND glab003 = '8304_29'
      UPDATE xrcd_t SET xrcd009 = l_xrcd009
       WHERE xrcd009 IS NULL
         AND xrcdent = g_enterprise
         AND xrcddocno = g_xrca_m.xrcadocno
         AND xrcdld = g_xrca_m.xrcald

        #150515-00013#3 Add  ---(S)---
         IF g_xrca_m.xrca100 = g_glaa.glaa001 THEN
            LET g_xrcb_d[l_ac].xrcb113 = g_xrcb_d[l_ac].xrcb103
            LET g_xrcb_d[l_ac].xrcb114 = g_xrcb_d[l_ac].xrcb104
            LET g_xrcb_d[l_ac].xrcb115 = g_xrcb_d[l_ac].xrcb105
            UPDATE xrcd_t SET xrcd113 = xrcd103,
                              xrcd114 = xrcd104,
                              xrcd115 = xrcd105
             WHERE xrcdent = g_enterprise
               AND xrcddocno = g_xrca_m.xrcadocno
               AND xrcdld = g_xrca_m.xrcald
         END IF
        #150515-00013#3 Add  ---(E)---
      LET g_xrcb_d[l_ac].xrcb102 = g_xrca_m.xrca101   #160816-00014#1 Add

      DISPLAY g_xrcb_d[l_ac].xrcborga,g_xrcb_d[l_ac].xrcb023, g_xrcb_d[l_ac].xrcb005, g_xrcb_d[l_ac].xrcb020, g_xrcb_d[l_ac].xrcb022,
              g_xrcb_d[l_ac].xrcb006, g_xrcb_d[l_ac].xrcb007, g_xrcb_d[l_ac].xrcb101, g_xrcb_d[l_ac].xrcb111, g_xrcb_d[l_ac].xrcb103,
              g_xrcb_d[l_ac].xrcb104, g_xrcb_d[l_ac].xrcb105, g_xrcb_d[l_ac].xrcb113, g_xrcb_d[l_ac].xrcb114, g_xrcb_d[l_ac].xrcb115,
              g_xrcb3_d[l_ac].xrcb123,g_xrcb3_d[l_ac].xrcb124,g_xrcb3_d[l_ac].xrcb125,g_xrcb3_d[l_ac].xrcb133,g_xrcb3_d[l_ac].xrcb134,
              g_xrcb3_d[l_ac].xrcb135,g_xrcb_d[l_ac].xrcb004
           TO s_detail1[l_ac].xrcborga,s_detail1[l_ac].xrcb023,s_detail1[l_ac].xrcb005,s_detail1[l_ac].xrcb020,s_detail1[l_ac].xrcb022,
              s_detail1[l_ac].xrcb006, s_detail1[l_ac].xrcb007,s_detail1[l_ac].xrcb101,s_detail1[l_ac].xrcb111,s_detail1[l_ac].xrcb103,
              s_detail1[l_ac].xrcb104, s_detail1[l_ac].xrcb105,s_detail1[l_ac].xrcb113,s_detail1[l_ac].xrcb114,s_detail1[l_ac].xrcb115,
              s_detail3[l_ac].xrcb123, s_detail3[l_ac].xrcb124,s_detail3[l_ac].xrcb125,s_detail3[l_ac].xrcb133,s_detail3[l_ac].xrcb134,
              s_detail3[l_ac].xrcb135, s_detail1[l_ac].xrcb004

      
      WHEN g_xrcb_d[l_ac].xrcb001 = '21'
         #判斷資料存在性
         SELECT * INTO l_xmdk.* FROM xmdk_t WHERE xmdkent = g_enterprise AND xmdkdocno = g_xrcb_d[l_ac].xrcb002

         SELECT * INTO l_xmdl.* FROM xmdl_t
          WHERE xmdlent   = g_enterprise
            AND xmdldocno =  g_xrcb_d[l_ac].xrcb002
            AND xmdlseq   =  g_xrcb_d[l_ac].xrcb003
         
         IF cl_null(l_xmdl.xmdldocno) THEN RETURN 'axm-00286' END IF
         
         #判斷資料有效性
          #只處理退貨、驗退
        #IF NOT (l_xmdk.xmdk000 = '5' OR l_xmdk.xmdk000 = '6') THEN RETURN 'axm-00287' END IF   #151207-00018#1   Mark
         IF l_xmdk.xmdk000 <> '6' THEN RETURN 'axm-00287' END IF                                #151207-00018#1   Mark
          #只處理銷退性質為可折讓者
         LET l_cnt=0
         SELECT COUNT(*) INTO l_cnt FROM gzcb_t WHERE gzcb001='2088' AND gzcb002=l_xmdk.xmdk082 AND gzcb003='Y'
         IF l_cnt=0 THEN RETURN 'axm-00288' END IF
          #只處理已出貨過帳者
         IF l_xmdk.xmdkstus <> 'S' THEN RETURN 'axm-00289' END IF
