PRIVATE FUNCTION axrt300_xmdl_bak1()
   DEFINE l_xrcb     RECORD
      xrcb002        LIKE xrcb_t.xrcb002,
      xrcb003        LIKE xrcb_t.xrcb003,
      xrcb007        LIKE xrcb_t.xrcb007
                     END RECORD
   DEFINE l_sql      STRING
   DEFINE l_s        LIKE type_t.num5
   DEFINE l_success  LIKE type_t.chr1
   DEFINE l_xmdl   RECORD LIKE xmdl_t.*

   CALL s_axrt300_sel_ld(g_xrca_m.xrcald) RETURNING l_success,l_s

   LET l_sql = "SELECT xrcb002,xrcb003,xrcb007 FROM xrcb_t WHERE xrcbent = '",g_enterprise,"'",
               "   AND xrcbdocno = '",g_xrca_m.xrcadocno,"'",
               "   AND xrcbld    = '",g_xrca_m.xrcald,"'"
   PREPARE axrt320_bak_prep1 FROM l_sql
   DECLARE axrt320_bak_curs1 CURSOR FOR axrt320_bak_prep1

   LET l_success = 'Y'

   IF g_xrca_m.xrcastus = 'Y' THEN RETURN l_success END IF

   FOREACH axrt320_bak_curs1 INTO l_xrcb.*
      #SELECT * INTO l_xmdl.* FROM xmdl_t WHERE xmdldocno = l_xrcb.xrcb002 AND xmdlseq = l_xrcb.xrcb003                            #160905-00002#6  mark
      SELECT * INTO l_xmdl.* FROM xmdl_t WHERE xmdlent = g_enterprise AND xmdldocno = l_xrcb.xrcb002 AND xmdlseq = l_xrcb.xrcb003  #160905-00002#6  add
      CASE
         WHEN l_s = '1'
            IF NOT cl_null(l_xmdl.xmdl038) AND NOT cl_null(l_xmdl.xmdl053) 
               AND l_xmdl.xmdl022 - l_xmdl.xmdl038 - l_xmdl.xmdl053 <= 0 THEN
                  LET l_success = 'N'
                  EXIT FOREACH
            END IF
         WHEN l_s = '2'
            IF NOT cl_null(l_xmdl.xmdl039) AND NOT cl_null(l_xmdl.xmdl054)
               AND l_xmdl.xmdl022 - l_xmdl.xmdl039 - l_xmdl.xmdl054 <= 0 THEN
                  LET l_success = 'N'
                  EXIT FOREACH
            END IF
         WHEN l_s = '3'
            IF NOT cl_null(l_xmdl.xmdl040) AND NOT cl_null(l_xmdl.xmdl055)
               AND l_xmdl.xmdl022 - l_xmdl.xmdl040 - l_xmdl.xmdl055 <= 0 THEN
                  LET l_success = 'N'
                  EXIT FOREACH
            END IF
      END CASE

      IF l_success = 'N' THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = "axr-00173"
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         RETURN 'N'
      END IF

      CASE
         WHEN l_s = '1'
            UPDATE xmdl_t SET xmdl038 = xmdl038 + l_xrcb.xrcb007
             WHERE xmdldocno = l_xrcb.xrcb002
               AND xmdlseq = l_xrcb.xrcb003
               AND xmdlent = g_enterprise
         WHEN l_s = '2'
            UPDATE xmdl_t SET xmdl039 = xmdl039 + l_xrcb.xrcb007
             WHERE xmdldocno = l_xrcb.xrcb002
               AND xmdlseq = l_xrcb.xrcb003
               AND xmdlent = g_enterprise
         WHEN l_s = '3'
            UPDATE xmdl_t SET xmdl040 = xmdl040 + l_xrcb.xrcb007
             WHERE xmdldocno = l_xrcb.xrcb002
               AND xmdlseq = l_xrcb.xrcb003
               AND xmdlent = g_enterprise
      END CASE
   END FOREACH

   RETURN l_success

END FUNCTION

