  <section id="axrq330.show" ver="5" status="" src="s" readonly="">
    <![CDATA[PRIVATE FUNCTION axrq330_show()
   DEFINE ls_sql    STRING
   #add-point:show段define-標準
   {<point name="show.define" edit="s"/>}
   #end add-point
   #add-point:show段define-客製
   {<point name="show.define_customerization" edit="c"/>}
   #end add-point
 
   DISPLAY g_master.* TO g_xrca004,g_xrca100,g_type1,amt
 
   #讀入ref值
   #add-point:show段單身reference
   {<point name="show.head.reference"/>}
   #end add-point
 
   LET g_detail_page_action = "detail_first"
