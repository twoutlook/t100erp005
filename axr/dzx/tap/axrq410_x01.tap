  <section id="axrq410_x01.ins_data" ver="2" status="" src="s" readonly="">
    <![CDATA[PRIVATE FUNCTION axrq410_x01_ins_data()
DEFINE sr RECORD 
   l_xrdald_desc LIKE type_t.chr500, 
   l_nmbb027_desc LIKE type_t.chr500, 
   l_nmbadocdt LIKE nmba_t.nmbadocdt, 
   l_nmbb029 LIKE type_t.chr500, 
   nmbb004 LIKE nmbb_t.nmbb004, 
   nmbb006 LIKE nmbb_t.nmbb006, 
   l_nmbb053_desc LIKE type_t.chr500, 
   nmbb031 LIKE nmbb_t.nmbb031, 
   l_nmbb003_desc LIKE type_t.chr500, 
   nmbb030 LIKE nmbb_t.nmbb030, 
   l_nmbb042 LIKE type_t.chr500, 
   nmbb040 LIKE nmbb_t.nmbb040, 
   l_unrev LIKE type_t.num20_6, 
   nmbbdocno LIKE nmbb_t.nmbbdocno, 
   nmbbseq LIKE nmbb_t.nmbbseq, 
   nmbborga LIKE nmbb_t.nmbborga, 
   l_nmbborga LIKE type_t.chr500, 
   nmbb026 LIKE nmbb_t.nmbb026, 
   l_nmbb026_desc LIKE type_t.chr500
 END RECORD
#add-point:ins_data段define
{<point name="ins_data.define"/>}
#end add-point
 
    #add-point:ins_data段before
    {<point name="ins_data.before"/>}
    #end add-point
 
    LET g_rep_success = 'Y'
 
    FOREACH axrq410_x01_curs INTO sr.*                               
       IF STATUS THEN
          INITIALIZE g_errparam TO NULL
          LET g_errparam.extend = 'foreach:'
          LET g_errparam.code   = STATUS
          LET g_errparam.popup  = TRUE
          CALL cl_err()
          LET g_rep_success = 'N'
          EXIT FOREACH
       END IF
 
       #add-point:ins_data段foreach
       {<point name="ins_data.foreach"/>}
       #end add-point
 
       #add-point:ins_data段before.save
       {<point name="ins_data.before.save"/>}
       #end add-point
 
       #EXECUTE
       EXECUTE insert_prep USING sr.l_xrdald_desc,sr.l_nmbb027_desc,sr.l_nmbadocdt,sr.l_nmbb029,sr.nmbb004,sr.nmbb006,sr.l_nmbb053_desc,sr.nmbb031,sr.l_nmbb003_desc,sr.nmbb030,sr.l_nmbb042,sr.nmbb040,sr.l_unrev,sr.nmbbdocno,sr.nmbbseq,sr.nmbborga,sr.l_nmbborga,sr.nmbb026,sr.l_nmbb026_desc
 
       IF SQLCA.sqlcode THEN
          INITIALIZE g_errparam TO NULL
          LET g_errparam.extend = "axrq410_x01_execute"
          LET g_errparam.code   = SQLCA.sqlcode
          LET g_errparam.popup  = FALSE
          CALL cl_err()       
          LET g_rep_success = 'N'
          EXIT FOREACH
       END IF
 
       #add-point:ins_data段after_save
       {<point name="ins_data.after.save"/>}
       #end add-point
       
    END FOREACH
    
    #add-point:ins_data段after
    {<point name="ins_data.after"/>}
    #end add-point
 
END FUNCTION
]]>
  </section>
  <section id="axrq410_x01.ins_prep" ver="1" status="" src="s" readonly="">
    <![CDATA[PRIVATE FUNCTION axrq410_x01_ins_prep()
DEFINE i              INTEGER
DEFINE l_prep_str     STRING
#add-point:ins_prep.define
{<point name="ins_prep.define"/>}
#end add-point:ins_prep.define
 
   FOR i = 1 TO g_rep_tmpname.getLength()
      CALL cl_xg_del_data(g_rep_tmpname[i])
      #LET g_sql = g_rep_ins_prep[i]              #透過此lib取得prepare字串 lib精簡
      CASE i
         WHEN 1
         #INSERT INTO PREP
         LET g_sql = " INSERT INTO ",g_rep_db CLIPPED,g_rep_tmpname[1] CLIPPED," VALUES(?,?,?,?,?,?, 
             ?,?,?,?,?,?,?,?,?,?,?,?,?)"
         PREPARE insert_prep FROM g_sql
         IF STATUS THEN
            LET l_prep_str ="insert_prep",i
            INITIALIZE g_errparam TO NULL
            LET g_errparam.extend = l_prep_str
            LET g_errparam.code   = status
            LET g_errparam.popup  = TRUE
            CALL cl_err()
            CALL cl_xg_drop_tmptable()
            LET g_rep_success = 'N'           
         END IF 
         #add-point:insert_prep段
         {<point name="insert_prep"/>}
         #end add-point                  
 
 
      END CASE
   END FOR
END FUNCTION
]]>
  </section>
  <section id="axrq410_x01.main" ver="1" status="" src="s" readonly="">
    <![CDATA[PUBLIC FUNCTION axrq410_x01(p_arg1,p_arg2)
DEFINE  p_arg1 STRING                  #tm.wc  condition:where 
DEFINE  p_arg2 STRING                  #tm.a1  print_tmp
#add-point:init段define
{<point name="component.define"/>}
#end add-point
 
   LET tm.wc = p_arg1
   LET tm.a1 = p_arg2
 
   #add-point:報表元件參數準備
   {<point name="component.arg.prep" />}
   #end add-point
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
 
   ##報表元件執行期間是否有錯誤代碼
   LET g_rep_success = 'Y'
   
   #報表元件代號      
   LET g_rep_code = "axrq410_x01"
   IF cl_null(tm.wc) THEN LET tm.wc = " 1=1" END IF
 
   #create 暫存檔
   CALL axrq410_x01_create_tmptable()
 
   IF g_rep_success = 'N' THEN
      RETURN
   END IF
   #報表select欄位準備
   CALL axrq410_x01_sel_prep()
 
   IF g_rep_success = 'N' THEN
      RETURN
   END IF   
   #報表insert的prepare
   CALL axrq410_x01_ins_prep()  
 
   IF g_rep_success = 'N' THEN
      RETURN
   END IF
   #將資料存入tmptable
   CALL axrq410_x01_ins_data() 
 
   IF g_rep_success = 'N' THEN
      RETURN
   END IF   
   #將tmptable資料印出
   CALL axrq410_x01_rep_data()
 
END FUNCTION
]]>
