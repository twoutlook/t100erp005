  <point name="process.process" order="" ver="4" cite_std="N" new="N" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[   #150623-00039#1 -----s
   IF cl_null(g_master.comb)THEN
      LET g_master.comb = g_argv[1]
   END IF
   
   IF cl_null(g_master.wc) THEN  ## 若是t類進來 會是傳字串參數的方式
       CALL l_arg.clear()
       LET l_token = base.StringTokenizer.create(ls_js,",")
       DISPLAY 'ls_jstoken:',ls_js
       LET l_cnt = 1
       WHILE l_token.hasMoreTokens()
             LET ls_next = l_token.nextToken()
             LET l_arg[l_cnt] = ls_next
             LET l_cnt = l_cnt + 1
       END WHILE
       CALL l_arg.deleteElement(l_cnt)
       LET g_master.wc = l_arg[01]
   ELSE
   #150623-00039#1-----e
  
      LET g_master.wc=g_master.wc CLIPPED," AND xrcasite='",g_master.xrcasite,"' AND xrcald='",g_master.xrcald,"'",
                                          " AND xrcacomp='",g_master.xrcacomp,"' AND xrca001='",g_master.xrca001,"'"
                                          
      IF NOT cl_null(g_master.xrcastus) THEN
         IF g_master.xrcastus='1' THEN  #
            LET g_master.wc=g_master.wc CLIPPED," AND xrcastus='N'"
         END IF
         IF g_master.xrcastus='2' THEN  #
            LET g_master.wc=g_master.wc CLIPPED," AND xrcastus='Y'"
         END IF      
      END IF
      IF NOT cl_null(g_master.xrca052) THEN
         IF g_master.xrca052=1 THEN  #未列印
            LET g_master.wc=g_master.wc CLIPPED," AND xrca052=0"
         END IF
         IF g_master.xrca052=2 THEN  #已列印
            LET g_master.wc=g_master.wc CLIPPED," AND xrca052>0"
         END IF      
      END IF
   
   END IF #150623-00039#1 add
   
   IF g_master.comb = '1' THEN   #150428-00011#1 Add
      CALL axrr300_g01(g_master.wc)
   ##150428-00011#1 Add  ---(S)---
   ELSE
      CALL axrr300_g02(g_master.wc)
   END IF
   ##150428-00011#1 Add  ---(S)---
   
   #更新列印次數---xxx...
   LET l_sql = " UPDATE xrca_t SET xrca052=xrca052+1",
               "  WHERE xrcaent='",g_enterprise,"'",
               "    AND ",g_master.wc CLIPPED 
               
   PREPARE axrr300_upd_xrca052_prepare FROM l_sql    
   EXECUTE axrr300_upd_xrca052_prepare
]]>
