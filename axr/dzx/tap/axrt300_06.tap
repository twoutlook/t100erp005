                      g_xrcf_d[l_ac].xrcf117,g_xrcf2_d[l_ac].xrcfdocno,g_xrcf2_d[l_ac].xrcfld,g_xrcf2_d[l_ac].xrcfseq, 
                      g_xrcf2_d[l_ac].xrcfseq2,g_xrcf2_d[l_ac].xrcf022,g_xrcf2_d[l_ac].xrcf023,g_xrcf2_d[l_ac].xrcf024, 
