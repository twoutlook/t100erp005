                      g_xrcf_d[l_ac].xrcfseq,g_xrcf_d[l_ac].xrcf008,g_xrcf_d[l_ac].xrcf009,g_xrcf_d[l_ac].xrcf021, 
                      g_xrcf_d[l_ac].xrcf007,g_xrcf_d[l_ac].xrcf101,g_xrcf_d[l_ac].xrcf103,g_xrcf_d[l_ac].xrcf104, 
