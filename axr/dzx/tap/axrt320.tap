    <![CDATA[   IF t_success = 'Y' THEN   #2015/01/16
      CALL s_transaction_end('Y',1)
      CALL cl_err_collect_init() #20150710 by 02599 add
      CALL cl_err_collect_show() #20150710 by 02599 add
   ELSE
      CALL s_transaction_end('N',1)
      CALL cl_err_collect_show()
      RETURN
   END IF
   IF lc_state = 'Y' THEN
      LET g_xrca_m.xrcacnfid = g_user 
      LET g_xrca_m.xrcacnfdt = cl_get_current()
   ELSE
      LET g_xrca_m.xrcacnfid = ''
      LET g_xrca_m.xrcacnfdt = ''
   END IF
   # 151105-00007#3 --- add start --- by 06948
   IF lc_state = 'X' THEN
      IF NOT cl_ask_confirm('aim-00109') THEN
         CALL s_transaction_end('N','0') 
         CALL cl_err_collect_show()   
         RETURN
      ELSE
         CALL s_transaction_begin()
         CALL cl_set_comp_entry("xrca051",TRUE) 
         INPUT BY NAME g_xrca_m.xrca051 WITHOUT DEFAULTS
            BEFORE INPUT
               LET INT_FLAG = 0
            
            ON ACTION controlp INFIELD xrca051
               #作廢
      		   INITIALIZE g_qryparam.* TO NULL
               LET g_qryparam.state = 'i'
      		   LET g_qryparam.reqry = FALSE
               LET g_qryparam.default1 = g_xrca_m.xrca051       
               LET g_qryparam.arg1 = "3115"
               CALL q_oocq002()                           
               LET g_xrca_m.xrca051 = g_qryparam.return1     
               CALL s_desc_get_acc_desc('3115',g_xrca_m.xrca051) RETURNING g_xrca_m.xrca051_desc
               DISPLAY BY NAME g_xrca_m.xrca051,g_xrca_m.xrca051_desc            
               NEXT FIELD xrca051   
            
            AFTER FIELD xrca051
               LET g_xrca_m.xrca051_desc = ' '
               IF NOT cl_null(g_xrca_m.xrca051) THEN
                  IF NOT s_azzi650_chk_exist('3115',g_xrca_m.xrca051) THEN
                     LET g_xrca_m.xrca051 = g_xrca_m_t.xrca051
                     CALL s_desc_get_acc_desc('3115',g_xrca_m.xrca051) RETURNING g_xrca_m.xrca051_desc
                     DISPLAY BY NAME g_xrca_m.xrca051_desc
                     NEXT FIELD CURRENT
                  END IF
               ELSE
                  NEXT FIELD CURRENT 
               END IF
               CALL s_desc_get_acc_desc('3115',g_xrca_m.xrca051) RETURNING g_xrca_m.xrca051_desc
               DISPLAY BY NAME g_xrca_m.xrca051_desc 
            
            AFTER INPUT
               IF NOT cl_null(g_xrca_m.xrcald) THEN
                  UPDATE xrca_t SET xrca051 = g_xrca_m.xrca051
                   WHERE xrcaent   = g_enterprise
                     AND xrcald    = g_xrca_m.xrcald
                     AND xrcadocno = g_xrca_m.xrcadocno
               END IF
            ON ACTION accept
               ACCEPT INPUT
           
            ON ACTION cancel
               LET INT_FLAG = 1
               LET g_xrca_m.xrca051 = ''
               LET g_xrca_m.xrca051_desc = ''
               EXIT INPUT 
               
            IF INT_FLAG THEN
               CALL cl_err_collect_show()   
               CALL s_transaction_end('N','0') 
               RETURN
            END IF
         END INPUT
         IF cl_null(g_xrca_m.xrca051) THEN
            LET lc_state = 'N'
