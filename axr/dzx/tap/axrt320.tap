################################################################################
PRIVATE FUNCTION axrt320_xrcb_chk()
   DEFINE l_amt         LIKE xrcb_t.xrcb105
   DEFINE r_success     LIKE type_t.chr1

   LET r_success = 'Y'
   LET l_amt = 0
   SELECT SUM(xrcb105 * xrcb022) INTO l_amt FROM xrcb_t WHERE xrcbent = g_enterprise
      AND xrcbdocno = g_xrca_m.xrcadocno AND xrcbld = g_xrca_m.xrcald
   IF cl_null(l_amt) THEN LET l_amt = 0 END IF

   IF g_xrca_m.xrca001 = '01' OR g_xrca_m.xrca001 = '03' THEN
      IF l_amt < 0 THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = ''
         LET g_errparam.code   = 'axr-00275'
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         LET r_success = 'N'
      END IF

      RETURN r_success
   END IF

   IF g_xrca_m.xrca001 = '02' OR g_xrca_m.xrca001 = '04' THEN
      IF l_amt > 0 THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = ''
         LET g_errparam.code   = 'axr-00276'
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         LET r_success = 'N'
      END IF

      RETURN r_success
   END IF

   RETURN r_success

