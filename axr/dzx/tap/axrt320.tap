               " LEFT JOIN ooefl_t t22 ON t22.ooeflent='"||g_enterprise||"' AND t22.ooefl001=t0.xrcaowndp AND t22.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t23 ON t23.ooagent='"||g_enterprise||"' AND t23.ooag001=t0.xrcacrtid  ",
               " LEFT JOIN ooefl_t t24 ON t24.ooeflent='"||g_enterprise||"' AND t24.ooefl001=t0.xrcacrtdp AND t24.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t25 ON t25.ooagent='"||g_enterprise||"' AND t25.ooag001=t0.xrcamodid  ",
               " LEFT JOIN ooag_t t26 ON t26.ooagent='"||g_enterprise||"' AND t26.ooag001=t0.xrcacnfid  ",
 
               " WHERE t0.xrcaent = '" ||g_enterprise|| "' AND t0.xrcald = ? AND t0.xrcadocno = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE axrt320_master_referesh FROM g_sql
 
    
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_axrt320 WITH FORM cl_ap_formpath("axr",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL axrt320_init()   
 
      #進入選單 Menu (="N")
      CALL axrt320_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_axrt320
      
   END IF 
   
   CLOSE axrt320_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
END MAIN
 
 
]]>
