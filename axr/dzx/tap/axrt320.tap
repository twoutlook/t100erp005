                  g_xrcb_d[g_detail_idx].xrcb003,g_xrcb_d[g_detail_idx].xrcb004,g_xrcb_d[g_detail_idx].xrcb005, 
                  g_xrcb_d[g_detail_idx].xrcb031,g_xrcb_d[g_detail_idx].xrcb020,g_xrcb_d[g_detail_idx].xrcb006, 
                  g_xrcb_d[g_detail_idx].xrcb022,g_xrcb_d[g_detail_idx].xrcb007,g_xrcb_d[g_detail_idx].xrcb101, 
