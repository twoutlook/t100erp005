################################################################################
PRIVATE FUNCTION axrt320_xrca018_axmt600()
   DEFINE l_xmdk_t      RECORD LIKE xmdk_t.*
   DEFINE l_count       LIKE type_t.num5
   DEFINE l_xrcb_t      RECORD LIKE xrcb_t.*
   DEFINE l_xmdl_t      RECORD LIKE xmdl_t.*
   DEFINE l_sql         STRING
   DEFINE l_success     LIKE type_t.chr1
   DEFINE l_s           LIKE type_t.num5
   DEFINE l_cnt         LIKE type_t.num5
   DEFINE l_xrcb007     LIKE xrcb_t.xrcb007
   DEFINE l_xrcd009      LIKE xrcd_t.xrcd009  #1029 add 
   #150518-00044#5
   DEFINE ls_js         STRING
   DEFINE lc_param      RECORD
            apca004     LIKE apca_t.apca004
                    END RECORD
   #150518-00044#5
   DEFINE  l_flag1               LIKE type_t.chr1       
   DEFINE  l_errno               LIKE type_t.chr100
   #判斷資料存在性
   SELECT * INTO l_xmdk_t.* FROM xmdk_t WHERE xmdkent = g_enterprise AND xmdkdocno = g_xrca_m.xrca018

   IF cl_null(l_xmdk_t.xmdkdocno) THEN RETURN END IF

   #判斷資料有效性
    #只處理退貨、驗退
   IF NOT (l_xmdk_t.xmdk000 = '6' OR l_xmdk_t.xmdk000 = '5') THEN RETURN END IF
    #只處理銷退性質為可折讓者
   LET l_cnt=0
   SELECT COUNT(*) INTO l_cnt FROM gzcb_t WHERE gzcb001='2088' AND gzcb002=l_xmdk_t.xmdk082 AND gzcb003='Y'
   IF l_cnt=0 THEN RETURN END IF
    #只處理已出貨過帳者
   IF l_xmdk_t.xmdkstus <> 'S' THEN RETURN END IF
