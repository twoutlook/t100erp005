    <![CDATA[
################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL s_aooi150_ins (传入参数)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt320_stbg_bak()
   DEFINE l_xrcb     RECORD
      xrcb002        LIKE xrcb_t.xrcb002,
      xrcb003        LIKE xrcb_t.xrcb003
                     END RECORD
   DEFINE l_sql      STRING
   DEFINE l_s        LIKE type_t.num5
   DEFINE l_success  LIKE type_t.chr1

   CALL s_axrt300_sel_ld(g_xrca_m.xrcald) RETURNING l_success,l_s

   LET l_sql = "SELECT xrcb002,xrcb003 FROM xrcb_t WHERE xrcbent = '",g_enterprise,"'",
               "   AND xrcbdocno = '",g_xrca_m.xrcadocno,"'",
               "   AND xrcbld    = '",g_xrca_m.xrcald,"'"
   PREPARE axrt320_bak_prep2 FROM l_sql
   DECLARE axrt320_bak_curs2 CURSOR FOR axrt320_bak_prep2

   FOREACH axrt320_bak_curs2 INTO l_xrcb.*
      UPDATE stbg_t SET stbgstus = '1'
       WHERE stbgdocno = l_xrcb.xrcb002
         AND stbgseq = l_xrcb.xrcb003
         AND stbgent = g_enterprise
   END FOREACH

   RETURN 'Y'

END FUNCTION]]>
