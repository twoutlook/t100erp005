    <![CDATA[################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL s_aooi150_ins (传入参数)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt320_xrca018_axmt540()
   DEFINE l_xmdk_t      RECORD LIKE xmdk_t.*
   DEFINE l_count       LIKE type_t.num5
   DEFINE l_xrcb_t      RECORD LIKE xrcb_t.*
   DEFINE l_xmdl_t      RECORD LIKE xmdl_t.*
   DEFINE l_sql         STRING
   DEFINE l_success     LIKE type_t.chr1
   DEFINE l_s           LIKE type_t.num5
   DEFINE l_xrcb007     LIKE xrcb_t.xrcb007
   DEFINE l_xrcd009      LIKE xrcd_t.xrcd009  #1029 add
   #150518-00044#5
   DEFINE ls_js         STRING
   DEFINE lc_param      RECORD
            apca004     LIKE apca_t.apca004
                    END RECORD
   #150518-00044#5
   DEFINE  l_flag1               LIKE type_t.chr1       
   DEFINE  l_errno               LIKE type_t.chr100
   
   #判斷資料存在性
   SELECT * INTO l_xmdk_t.* FROM xmdk_t WHERE xmdkent = g_enterprise AND xmdkdocno = g_xrca_m.xrca018

   IF cl_null(l_xmdk_t.xmdkdocno) THEN RETURN END IF

   #判斷資料有效性
    #只處理退貨、驗退
   IF NOT (l_xmdk_t.xmdk000 = '1' OR l_xmdk_t.xmdk000 = '2' OR l_xmdk_t.xmdk000 = '3') THEN RETURN END IF
    #只處理已出貨過帳者
   IF l_xmdk_t.xmdkstus <> 'S' THEN RETURN END IF
    #只處理出貨日<=立帳日
   IF l_xmdk_t.xmdk001 > g_xrca_m.xrcadocdt THEN RETURN END IF
    #只處理同帳款對象
   IF l_xmdk_t.xmdk008 <> g_xrca_m.xrca004 THEN RETURN END IF

  #2015/11/17 Add  ---(S)--- By 01727
   CALL s_get_accdate(g_glaa.glaa003,g_xrca_m.xrcadocdt,'','') 
   RETURNING l_flag1,l_errno,g_glav002,g_glav005,g_sdate_s,g_sdate_e,
             g_glav006,g_pdate_s,g_pdate_e,g_glav007,g_wdate_s,g_wdate_e

   #来源单据扣账日期必须要和单头的立账日期在同一个会计期间内
   IF l_xmdk_t.xmdk001 < g_pdate_s OR l_xmdk_t.xmdk001 > g_pdate_e THEN RETURN 'axr-00318' END IF
  #2015/11/17 Add  ---(E)---

   CALL s_axrt300_sel_ld(g_xrca_m.xrcald) RETURNING l_success,l_s

   #檢查單身是否存在符合條件的資料
   LET l_count = 0
   SELECT COUNT (*) INTO l_count
     FROM (SELECT CASE
                     WHEN l_s = '1' THEN
                        CASE WHEN xmdl022 IS NULL THEN 0 ELSE xmdl022 END
                      - CASE WHEN xmdl038 IS NULL THEN 0 ELSE xmdl038 END
                      - CASE WHEN xmdl053 IS NULL THEN 0 ELSE xmdl053 END
                     WHEN l_s = '2' THEN
                        CASE WHEN xmdl022 IS NULL THEN 0 ELSE xmdl022 END
                      - CASE WHEN xmdl039 IS NULL THEN 0 ELSE xmdl039 END
                      - CASE WHEN xmdl054 IS NULL THEN 0 ELSE xmdl054 END
                     WHEN l_s = '3' THEN
                        CASE WHEN xmdl022 IS NULL THEN 0 ELSE xmdl022 END
                      - CASE WHEN xmdl040 IS NULL THEN 0 ELSE xmdl040 END
                      - CASE WHEN xmdl055 IS NULL THEN 0 ELSE xmdl055 END
                  END xmdl022,xmdl087,xmdlsite
             FROM xmdl_t
            WHERE xmdlent = g_enterprise AND xmdldocno = g_xrca_m.xrca018
          )
    WHERE     xmdl022 > 0
          AND xmdl087 = 'Y'
   IF l_count < 1 THEN RETURN END IF

   LET g_xrca_m.xrca008 = l_xmdk_t.xmdk010
   
   #應收日期/票據到期日
   CALL s_fin_date_ar_receivable(g_xrca_m.xrcasite,g_xrca_m.xrca004,g_xrca_m.xrca008,g_xrca_m.xrcadocdt,
     g_xrca_m.xrcadocdt,g_xrca_m.xrcadocdt,'') RETURNING l_success,g_xrca_m.xrca009,g_xrca_m.xrca010
   SELECT ooib025,ooib005 INTO g_xrca_m.xrca054,g_xrca_m.xrca055 FROM ooib_t
    WHERE ooibent = g_enterprise AND ooib001 = g_xrca_m.xrca008
   CALL s_axrt300_xrca_ref('xrca008',g_xrca_m.xrca008,'','') RETURNING l_success,g_xrca_m.xrca008_desc
   CALL s_axrt300_xrca_ref('xrca054',g_xrca_m.xrca054,'','') RETURNING l_success,g_xrca_m.xrca054_desc
   CALL s_axrt300_xrca_ref('xrca055',g_xrca_m.xrca055,'','') RETURNING l_success,g_xrca_m.xrca055_desc
   
   LET g_xrca_m.xrca100 = l_xmdk_t.xmdk016
   #CALL s_axrt300_get_exrate(g_xrca_m.xrcadocdt,g_xrca_m.xrcald,g_xrca_m.xrcacomp,g_xrca_m.xrca100)         #150518-00044#5 mark
   #150518-00044#5--(S)
   LET lc_param.apca004 = g_xrca_m.xrca004
   LET ls_js = util.JSON.stringify(lc_param)
   #150518-00044#5--(E)
   CALL s_fin_get_curr_rate(g_xrca_m.xrcacomp,g_xrca_m.xrcald,g_xrca_m.xrcadocdt,g_xrca_m.xrca100,ls_js)    #150518-00044#5
        RETURNING g_xrca_m.xrca101,g_xrca_m.xrca121,g_xrca_m.xrca131
   
   LET g_xrca_m.xrca011 = l_xmdk_t.xmdk012 LET g_xrca_m.xrca013 = l_xmdk_t.xmdk014 LET g_xrca_m.xrca012 = l_xmdk_t.xmdk013
   
   LET g_xrca_m.xrca014 = l_xmdk_t.xmdk003
   CALL s_axrt300_xrca_ref('xrca014',g_xrca_m.xrca014,'','') RETURNING l_success,g_xrca_m.xrca014_desc
   
   LET g_xrca_m.xrca015 = l_xmdk_t.xmdk004
   CALL s_axrt300_xrca_ref('xrca015',g_xrca_m.xrca015,'','') RETURNING l_success,g_xrca_m.xrca015_desc
   
   SELECT ooeg004 INTO g_xrca_m.xrca034 FROM ooge_t
    WHERE ooegent = g_enterprise AND ooeg001 = g_xrca_m.xrca015
    
   LET l_count = 0
   SELECT COUNT(*) INTO l_count FROM xrcb_t
    WHERE xrcbent = g_enterprise AND xrcbdocno = g_xrca_m.xrcadocno AND xrcbld = g_xrca_m.xrcald
    
   IF l_count = 0 THEN
      IF NOT cl_ask_confirm('axr-00095') THEN
         RETURN
      END IF
   ELSE
      RETURN
   END IF
   
   LET l_sql = "SELECT * FROM xmdl_t WHERE xmdlent = '",g_enterprise,"' AND xmdldocno = '",l_xmdk_t.xmdkdocno,"'",
               "   AND xmdl087 = 'Y' "
   PREPARE axrt320_prep4 FROM l_sql
   DECLARE axrt320_curs4 CURSOR FOR axrt320_prep4
   
   LET l_xrcb_t.xrcbseq = 1
   FOREACH axrt320_curs4 INTO l_xmdl_t.*
      CASE
         WHEN l_s = '1'
            IF NOT cl_null(l_xmdl_t.xmdl038) AND NOT cl_null(l_xmdl_t.xmdl053) AND l_xmdl_t.xmdl022 - l_xmdl_t.xmdl038 - l_xmdl_t.xmdl053 <= 0 THEN CONTINUE FOREACH END IF
            LET l_xrcb007 = l_xmdl_t.xmdl022 - l_xmdl_t.xmdl038 - l_xmdl_t.xmdl053
         WHEN l_s = '2'
            IF NOT cl_null(l_xmdl_t.xmdl039) AND NOT cl_null(l_xmdl_t.xmdl054) AND l_xmdl_t.xmdl022 - l_xmdl_t.xmdl039 - l_xmdl_t.xmdl054 <= 0 THEN CONTINUE FOREACH END IF
            LET l_xrcb007 = l_xmdl_t.xmdl022 - l_xmdl_t.xmdl039
         WHEN l_s = '3'
            IF NOT cl_null(l_xmdl_t.xmdl040) AND NOT cl_null(l_xmdl_t.xmdl055) AND l_xmdl_t.xmdl022 - l_xmdl_t.xmdl040 - l_xmdl_t.xmdl055 <= 0 THEN CONTINUE FOREACH END IF
            LET l_xrcb007 = l_xmdl_t.xmdl022 - l_xmdl_t.xmdl040 - l_xmdl_t.xmdl055
      END CASE

      LET l_xrcb_t.xrcbdocno = g_xrca_m.xrcadocno
      LET l_xrcb_t.xrcbld    = g_xrca_m.xrcald
      LET l_xrcb_t.xrcbent   = g_enterprise
      LET l_xrcb_t.xrcborga = g_xrca_m.xrcasite
      LET l_xrcb_t.xrcb001 = g_xrca_m.xrca016
      LET l_xrcb_t.xrcb002 = l_xmdl_t.xmdldocno
      LET l_xrcb_t.xrcb003 = l_xmdl_t.xmdlseq
      LET l_xrcb_t.xrcb004 = l_xmdl_t.xmdl008
     #150828-00001#2 Mark ---(S)---
     #SELECT imaal003 INTO l_xrcb_t.xrcb005 FROM imaal_t
     # WHERE imaalent = g_enterprise AND imaal002 = g_lang AND imaal001 = l_xrcb_t.xrcb004
     #150828-00001#2 Mark ---(E)---

      CALL s_axrt300_get_xrcb005(l_xrcb_t.xrcb004) RETURNING l_xrcb_t.xrcb005   #150828-00001#2 Add

      LET l_xrcb_t.xrcb020 = l_xmdl_t.xmdl025
      LET l_xrcb_t.xrcb006 = l_xmdl_t.xmdl017
      LET l_xrcb_t.xrcb007 = l_xrcb007
      LET l_xrcb_t.xrcb101 = l_xmdl_t.xmdl024
      LET l_xrcb_t.xrcb021 = g_xrca_m.xrca036
      LET l_xrcb_t.xrcb029 = g_xrca_m.xrca035
      LET l_xrcb_t.xrcb031 = l_xmdk_t.xmdk010
      CALL s_axrt300_exrate(g_glaa.glaa002,g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa001,l_xrcb_t.xrcb101,g_xrca_m.xrca101,g_glaa.glaacomp)
         RETURNING l_success,l_xrcb_t.xrcb111
      CALL s_tax_ins(g_xrca_m.xrcadocno,l_xrcb_t.xrcbseq,0,l_xrcb_t.xrcborga,
                     l_xrcb_t.xrcb007*l_xrcb_t.xrcb101,l_xrcb_t.xrcb020,
                     l_xrcb_t.xrcb007,g_xrca_m.xrca100,g_xrca_m.xrca101,g_xrca_m.xrcald,g_xrca_m.xrca121,g_xrca_m.xrca131)
         RETURNING l_xrcb_t.xrcb103,l_xrcb_t.xrcb104,l_xrcb_t.xrcb105,
                   l_xrcb_t.xrcb113,l_xrcb_t.xrcb114,l_xrcb_t.xrcb115,
                   l_xrcb_t.xrcb123,l_xrcb_t.xrcb124,l_xrcb_t.xrcb125,
                   l_xrcb_t.xrcb133,l_xrcb_t.xrcb134,l_xrcb_t.xrcb135
      #1029 --add--str---
      SELECT glab005 INTO l_xrcd009 FROM glab_t 
       WHERE glabld = g_xrca_m.xrcald 
         AND glabent = g_enterprise
         AND glab002 = g_xrca_m.xrca007   # 帳款類別
         AND glab001 = '13'             # 應收帳務類型科目設定
         AND glab003 = '8304_29'
      UPDATE xrcd_t SET xrcd009 = l_xrcd009
       WHERE xrcd009 IS NULL
         AND xrcdent = g_enterprise
         AND xrcddocno = g_xrca_m.xrcadocno
         AND xrcdld = g_xrca_m.xrcald 
      #1029 add--end---         
        #150515-00013#3 Add  ---(S)---
         IF g_xrca_m.xrca100 = g_glaa.glaa001 THEN
            LET l_xrcb_t.xrcb113 = l_xrcb_t.xrcb103
            LET l_xrcb_t.xrcb114 = l_xrcb_t.xrcb104
            LET l_xrcb_t.xrcb115 = l_xrcb_t.xrcb105
            UPDATE xrcd_t SET xrcd113 = xrcd103,
                              xrcd114 = xrcd104,
                              xrcd115 = xrcd105
             WHERE xrcdent = g_enterprise
               AND xrcddocno = g_xrca_m.xrcadocno
               AND xrcdld = g_xrca_m.xrcald
         END IF
        #150515-00013#3 Add  ---(E)---

      LET l_xrcb_t.xrcb022 = 1
      INSERT INTO xrcb_t VALUES (l_xrcb_t.*)
      CASE
         WHEN l_s = '1'
            UPDATE xmdl_t SET xmdl053 = 0
             WHERE xmdldocno = l_xrcb_t.xrcb002
               AND xmdlseq = l_xrcb_t.xrcb003
               AND xmdlent = g_enterprise
               AND xmdl053 IS NULL
            UPDATE xmdl_t SET xmdl053 = xmdl053 + l_xrcb_t.xrcb007
             WHERE xmdldocno = l_xrcb_t.xrcb002
               AND xmdlseq = l_xrcb_t.xrcb003
               AND xmdlent = g_enterprise
         WHEN l_s = '2'
            UPDATE xmdl_t SET xmdl054 = 0
             WHERE xmdldocno = l_xrcb_t.xrcb002
               AND xmdlseq = l_xrcb_t.xrcb003
               AND xmdlent = g_enterprise
               AND xmdl054 IS NULL
            UPDATE xmdl_t SET xmdl054 = xmdl054 + l_xrcb_t.xrcb007
             WHERE xmdldocno = l_xrcb_t.xrcb002
               AND xmdlseq = l_xrcb_t.xrcb003
               AND xmdlent = g_enterprise
         WHEN l_s = '3'
            UPDATE xmdl_t SET xmdl055 = 0
             WHERE xmdldocno = l_xrcb_t.xrcb002
               AND xmdlseq = l_xrcb_t.xrcb003
               AND xmdlent = g_enterprise
               AND xmdl055 IS NULL
            UPDATE xmdl_t SET xmdl055 = xmdl055 + l_xrcb_t.xrcb007
             WHERE xmdldocno = l_xrcb_t.xrcb002
               AND xmdlseq = l_xrcb_t.xrcb003
               AND xmdlent = g_enterprise
      END CASE
      LET l_xrcb_t.xrcbseq = l_xrcb_t.xrcbseq + 1
   END FOREACH
   
   CALL s_axrt300_installments(g_xrca_m.xrcald,g_xrca_m.xrcadocno) RETURNING l_success
   CALL s_axrt300_xrca_upd(g_xrca_m.xrcald,g_xrca_m.xrcadocno)
      RETURNING l_success
   DISPLAY BY NAME g_xrca_m.xrca103,g_xrca_m.xrca104,g_xrca_m.xrca113,g_xrca_m.xrca114,
                   g_xrca_m.xrca123,g_xrca_m.xrca124,g_xrca_m.xrca133,g_xrca_m.xrca134,
                   g_xrca_m.xrca108,g_xrca_m.xrca118,g_xrca_m.xrca128,g_xrca_m.xrca138
   
   CALL axrt320_b_fill()
   
END FUNCTION]]>
