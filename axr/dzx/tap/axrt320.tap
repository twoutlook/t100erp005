  <point name="input.a.page1.xrcb007" order="" ver="10" cite_std="N" new="Y" ch="" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[            IF NOT cl_null(g_xrcb_d[l_ac].xrcb007) THEN
               IF p_cmd = 'a' OR (p_cmd = 'u' AND (g_xrcb_d[l_ac].xrcb007 != g_xrcb_d_t.xrcb007 OR g_xrcb_d_t.xrcb007 IS NULL )) THEN
                  #有来源单据
                  IF NOT cl_null(g_xrcb_d[l_ac].xrcb002) AND NOT cl_null(g_xrcb_d[l_ac].xrcb003) THEN
                     IF g_xrcb_d[l_ac].xrcb023 = 'Y' THEN
                         #取得暂估数量
                         SELECT SUM(xrcb007) INTO l_xrcb007 FROM xrcb_t,xrca_t
                          WHERE xrcbent = g_enterprise AND xrcbld = g_xrca_m.xrcald
                            AND xrcb002 = g_xrcb_d[l_ac].xrcb002
                            AND xrcb003 = g_xrcb_d[l_ac].xrcb003
                            AND xrcbent = xrcaent AND xrcbdocno = xrcadocno
                            AND xrcbld = xrcald   AND xrca001 = '01' AND xrcastus = 'Y'
                        
                         #取得已冲暂估数量&其他未確認單, 有沖暫估記錄者
                         SELECT SUM(xrcb007) INTO l_xrcb0071 FROM xrcb_t,xrca_t
                          WHERE xrcbent = g_enterprise AND xrcbld = g_xrca_m.xrcald
                            AND xrcb002 = g_xrcb_d[l_ac].xrcb002
                            AND xrcb003 = g_xrcb_d[l_ac].xrcb003
                            AND xrcb023 = 'Y'
                            AND xrcbent = xrcaent AND xrcbdocno = xrcadocno
                            AND xrcbld = xrcald   AND xrca001 <> '01'
                        LET l_xrcb007 = l_xrcb007 - l_xrcb0071
                     ELSE
                        #取得出货数量
                        SELECT SUM(xmdl018) INTO l_xrcb007 FROM xmdl_t
                         WHERE xmdldocno = g_xrcb_d[l_ac].xrcb002 AND xmdlseq = g_xrcb_d[l_ac].xrcb002
                           AND xmdlent = g_enterprise
                        #取得已立账数量&其他未确认单据，有立账记录者
                         SELECT SUM(xrcb007) INTO l_xrcb0071 FROM xrcb_t
                          WHERE xrcbent = g_enterprise AND xrcbld = g_xrca_m.xrcald
                            AND xrcb002 = g_xrcb_d[l_ac].xrcb002
                            AND xrcb003 = g_xrcb_d[l_ac].xrcb003
                        LET l_xrcb007 = l_xrcb007 - l_xrcb0071
                     END IF
                     IF NOT cl_null(g_xrcb_d_t.xrcb007) THEN
                        LET l_xrcb007 = l_xrcb007 + g_xrcb_d_t.xrcb007
                     END IF
                     IF g_xrcb_d[l_ac].xrcb007 > l_xrcb007 THEN
                        LET g_xrcb_d[l_ac].xrcb007 = g_xrcb_d_t.xrcb007
                        INITIALIZE g_errparam TO NULL
                        LET g_errparam.code = 'aap-00060'
                        LET g_errparam.extend = ''
                        LET g_errparam.popup = TRUE
                        CALL cl_err()

                        NEXT FIELD CURRENT
                     END IF
                  END IF
               END IF
               IF NOT cl_null(g_xrcb_d[l_ac].xrcb101) AND NOT cl_null(g_xrcb_d[l_ac].xrcb020) THEN
                  CALL s_tax_ins(g_xrca_m.xrcadocno,g_xrcb_d[l_ac].xrcbseq,0,g_xrcb_d[l_ac].xrcborga,
                                 g_xrcb_d[l_ac].xrcb007*g_xrcb_d[l_ac].xrcb101,g_xrcb_d[l_ac].xrcb020,
                                 g_xrcb_d[l_ac].xrcb007,g_xrca_m.xrca100,g_xrca_m.xrca101,g_xrca_m.xrcald,
                                 g_xrca_m.xrca121,g_xrca_m.xrca131)
                    RETURNING g_xrcb_d[l_ac].xrcb103,g_xrcb_d[l_ac].xrcb104,g_xrcb_d[l_ac].xrcb105,
                              g_xrcb_d[l_ac].xrcb113,g_xrcb_d[l_ac].xrcb114,g_xrcb_d[l_ac].xrcb115,
                              g_xrcb3_d[l_ac].xrcb123,g_xrcb3_d[l_ac].xrcb124,g_xrcb3_d[l_ac].xrcb125,
                              g_xrcb3_d[l_ac].xrcb133,g_xrcb3_d[l_ac].xrcb134,g_xrcb3_d[l_ac].xrcb135
                     #1029 --add--str---
                     SELECT glab005 INTO l_xrcd009 FROM glab_t 
                      WHERE glabld = g_xrca_m.xrcald 
                        AND glabent = g_enterprise
                        AND glab002 = g_xrca_m.xrca007   # 帳款類別
                        AND glab001 = '13'             # 應收帳務類型科目設定
                        AND glab003 = '8304_29'
                     UPDATE xrcd_t SET xrcd009 = l_xrcd009
                      WHERE xrcd009 IS NULL
                        AND xrcdent = g_enterprise
                        AND xrcddocno = g_xrca_m.xrcadocno
                        AND xrcdld = g_xrca_m.xrcald 
