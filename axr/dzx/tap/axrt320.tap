                        xrcb039 = g_xrcb4_d[l_ac].xrcb039, xrcb040 = g_xrcb4_d[l_ac].xrcb040,
                        xrcb041 = g_xrcb4_d[l_ac].xrcb041, xrcb042 = g_xrcb4_d[l_ac].xrcb042,
                        xrcb043 = g_xrcb4_d[l_ac].xrcb043, xrcb044 = g_xrcb4_d[l_ac].xrcb044,
                        xrcb045 = g_xrcb4_d[l_ac].xrcb045, xrcb046 = g_xrcb4_d[l_ac].xrcb046
                       #xrcb047 = g_xrcb4_d[l_ac].xrcb047   #151106-00005#1 Mark
       WHERE xrcbent   = g_enterprise AND xrcbld =  ps_keys[1]
         AND xrcbdocno = ps_keys[2]   AND xrcbseq = ps_keys[3]
      CASE
         WHEN l_s = '1'
            UPDATE xmdl_t SET xmdl053 = 0
             WHERE xmdldocno = g_xrcb_d[g_detail_idx].xrcb002
               AND xmdlseq = g_xrcb_d[g_detail_idx].xrcb003
               AND xmdlent = g_enterprise
