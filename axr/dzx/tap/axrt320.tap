                  g_xrcb_d[g_detail_idx].xrcb009,g_xrcb_d[g_detail_idx].xrcb010,g_xrcb_d[g_detail_idx].xrcb047, 
                  g_xrcb_d[g_detail_idx].xrcb029,g_xrcb_d[g_detail_idx].xrcb021,g_xrcb_d[g_detail_idx].xrcb023, 
                  g_xrcb_d[g_detail_idx].xrcb013,g_xrcb_d[g_detail_idx].xrcb014,g_xrcb_d[g_detail_idx].xrcb017, 
                  g_xrcb_d[g_detail_idx].xrcb018,g_xrcb_d[g_detail_idx].xrcb019,g_xrcb_d[g_detail_idx].xrcb025, 
                  g_xrcb_d[g_detail_idx].xrcb026,g_xrcb_d[g_detail_idx].xrcb027,g_xrcb_d[g_detail_idx].xrcb028, 
                  g_xrcb_d[g_detail_idx].xrcb100,g_xrcb_d[g_detail_idx].xrcb106,g_xrcb_d[g_detail_idx].xrcb116, 
                  g_xrcb_d[g_detail_idx].xrcb117,g_xrcb_d[g_detail_idx].xrcb118,g_xrcb_d[g_detail_idx].xrcb119, 
                  g_xrcb_d[g_detail_idx].xrcb126,g_xrcb_d[g_detail_idx].xrcb136,g_xrcb_d[g_detail_idx].xrcblegl, 
