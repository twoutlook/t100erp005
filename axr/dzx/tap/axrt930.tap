  <section id="axrt930.input.body" ver="39" status="" src="s">
<![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_xreb_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = FALSE,
                  DELETE ROW = FALSE,
                  APPEND ROW = FALSE)
 
         #自訂單身ACTION
         
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_xreb_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL axrt930_b_fill(g_wc2) 
            IF g_rec_b != 0 THEN
               CALL fgl_set_arr_curr(l_ac)
            END IF
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN axrt930_cl USING g_enterprise,
                                               g_xreb_m.xrebld
                                               ,g_xreb_m.xreb001
                                               ,g_xreb_m.xreb002
 
                                               
               IF STATUS THEN
                  CALL cl_err("OPEN axrt930_cl:", STATUS, 1)
                  CLOSE axrt930_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_xreb_d[l_ac].xreb005 IS NOT NULL
               AND g_xreb_d[l_ac].xreb006 IS NOT NULL
               AND g_xreb_d[l_ac].xreb007 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_xreb_d_t.* = g_xreb_d[l_ac].*  #BACKUP
               LET g_xreb_d_o.* = g_xreb_d[l_ac].*  #BACKUP
               CALL axrt930_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL axrt930_set_no_entry_b(l_cmd)
               OPEN axrt930_bcl USING g_enterprise,g_xreb_m.xrebld,
                                                g_xreb_m.xreb001,
                                                g_xreb_m.xreb002,
 
                                                g_xreb_d_t.xreb005
                                                ,g_xreb_d_t.xreb006
                                                ,g_xreb_d_t.xreb007
 
               IF STATUS THEN
                  CALL cl_err("OPEN axrt930_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH axrt930_bcl INTO g_xreb_d[l_ac].xreb004,g_xreb_d[l_ac].xreb005,g_xreb_d[l_ac].xreb006, 
                      g_xreb_d[l_ac].xreb007,g_xreb_d[l_ac].xreb009,g_xreb_d[l_ac].xreb008,g_xreb_d[l_ac].xreb103, 
                      g_xreb_d[l_ac].xreb113,g_xreb_d[l_ac].xreb115,g_xreb_d[l_ac].xreb125,g_xreb_d[l_ac].xreb135 

                  IF SQLCA.sqlcode THEN
                      CALL cl_err(g_xreb_d_t.xreb005,SQLCA.sqlcode,1)
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL axrt930_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
        
         BEFORE INSERT
            
            INITIALIZE g_xreb_d_t.* TO NULL
            INITIALIZE g_xreb_d_o.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_xreb_d[l_ac].* TO NULL
            
            #公用欄位預設值
              
            
            #一般欄位預設值
            
            
            
            LET g_xreb_d_t.* = g_xreb_d[l_ac].*     #新輸入資料
            LET g_xreb_d_o.* = g_xreb_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL axrt930_set_entry_b(l_cmd)
            #add-point:set_entry_b後
            {<point name="input.body.before_insert.set_entry_b"/>}
            #end add-point
            CALL axrt930_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_xreb_d[li_reproduce_target].* = g_xreb_d[li_reproduce].*
 
               LET g_xreb_d[g_xreb_d.getLength()].xreb005 = NULL
               LET g_xreb_d[g_xreb_d.getLength()].xreb006 = NULL
               LET g_xreb_d[g_xreb_d.getLength()].xreb007 = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
 
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM xreb_t 
             WHERE xrebent = g_enterprise AND xrebld = g_xreb_m.xrebld
               AND xreb001 = g_xreb_m.xreb001
               AND xreb002 = g_xreb_m.xreb002
 
               AND xreb005 = g_xreb_d[l_ac].xreb005
               AND xreb006 = g_xreb_d[l_ac].xreb006
               AND xreb007 = g_xreb_d[l_ac].xreb007
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               CALL s_transaction_begin()
               #add-point:單身新增前
               {<point name="input.body.b_insert" mark="Y"/>}
               #end add-point
               INSERT INTO xreb_t
                           (xrebent,
                            xrebld,xreb001,xrebcomp,xreb003,xreb002,
                            xreb005,xreb006,xreb007
                            ,xreb004,xreb009,xreb008,xreb103,xreb113,xreb115,xreb125,xreb135) 
                     VALUES(g_enterprise,
                            g_xreb_m.xrebld,g_xreb_m.xreb001,g_xreb_m.xrebcomp,g_xreb_m.xreb003,g_xreb_m.xreb002,
                            g_xreb_d[l_ac].xreb005,g_xreb_d[l_ac].xreb006,g_xreb_d[l_ac].xreb007
                            ,g_xreb_d[l_ac].xreb004,g_xreb_d[l_ac].xreb009,g_xreb_d[l_ac].xreb008,g_xreb_d[l_ac].xreb103, 
                                g_xreb_d[l_ac].xreb113,g_xreb_d[l_ac].xreb115,g_xreb_d[l_ac].xreb125, 
                                g_xreb_d[l_ac].xreb135)
               #add-point:單身新增中
               {<point name="input.body.m_insert"/>}
               #end add-point
               LET p_cmd = 'u'
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_xreb_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("xreb_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR "INSERT O.K"
               LET g_rec_b=g_rec_b+1
               DISPLAY g_rec_b TO FORMONLY.cnt
            END IF
            
            #add-point:單身新增後
            {<point name="input.body.after_insert"/>}
            #end add-point
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_xreb_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_xreb_d.deleteElement(l_ac)
               NEXT FIELD xreb005
            END IF
            IF g_xreb_d_t.xreb005 IS NOT NULL
               AND g_xreb_d_t.xreb006 IS NOT NULL
               AND g_xreb_d_t.xreb007 IS NOT NULL
 
               THEN
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete"/>}
               #end add-point
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               IF axrt930_before_delete() THEN 
                  CALL s_transaction_end('Y','0')
               ELSE 
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               END IF 
               CLOSE axrt930_bcl
               LET l_count = g_xreb_d.getLength()
            END IF 
            
            #add-point:單身刪除後
            {<point name="input.body.a_delete"/>}
