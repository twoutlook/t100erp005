          , xreb001 = g_xreb_m.xreb001
          , xreb002 = g_xreb_m.xreb002
 
      #更新共用欄位
      
                                       
  
   #將資料塞回原table   
   INSERT INTO xreb_t SELECT * FROM axrt930_detail
   
   IF SQLCA.sqlcode THEN
      CALL cl_err("reproduce",SQLCA.sqlcode,1)
