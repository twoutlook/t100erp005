  <section id="axrt930.filter" ver="1" status="" src="s">
<![CDATA[#+ 此段落由子樣板a50產生
#+ filter過濾功能
PRIVATE FUNCTION axrt930_filter()
   #add-point:filter段define
   {<point name="filter.define"/>}
   #end add-point   
 
   #切換畫面
   IF NOT g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",1)
      CALL gfrm_curr.setElementHidden("worksheet",0)
      LET g_main_hidden = 1
   END IF   
 
   LET INT_FLAG = 0
 
   LET g_qryparam.state = 'c'
 
   LET g_wc_filter_t = g_wc_filter.trim()
   LET g_wc_t = g_wc
 
   LET g_wc = cl_replace_str(g_wc, g_wc_filter_t, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON xrebld,xreb001,xreb002
                          FROM s_browse[1].b_xrebld,s_browse[1].b_xreb001,s_browse[1].b_xreb002
 
         BEFORE CONSTRUCT
               DISPLAY axrt930_filter_parser('xrebld') TO s_browse[1].b_xrebld
            DISPLAY axrt930_filter_parser('xreb001') TO s_browse[1].b_xreb001
            DISPLAY axrt930_filter_parser('xreb002') TO s_browse[1].b_xreb002
      
         #add-point:filter段cs_ctrl
         {<point name="filter.cs_ctrl"/>}
         #end add-point
      
      END CONSTRUCT
 
      #add-point:filter段add_cs
