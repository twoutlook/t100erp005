  <section id="axrt930.delete" ver="34" status="" src="s">
<![CDATA[#+ 資料刪除
PRIVATE FUNCTION axrt930_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_xreb_m.xrebld IS NULL
   OR g_xreb_m.xreb001 IS NULL
   OR g_xreb_m.xreb002 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE axrt930_master_referesh USING g_xreb_m.xrebld,g_xreb_m.xreb001,g_xreb_m.xreb002 INTO g_xreb_m.xrebld, 
       g_xreb_m.xreb001,g_xreb_m.xrebcomp,g_xreb_m.xreb003,g_xreb_m.xreb002,g_xreb_m.xrebld_desc,g_xreb_m.xrebcomp_desc 

   
   CALL axrt930_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN axrt930_cl USING g_enterprise,g_xreb_m.xrebld
                                                       ,g_xreb_m.xreb001
                                                       ,g_xreb_m.xreb002
 
   IF STATUS THEN
      CALL cl_err("OPEN axrt930_cl:", STATUS, 1)
      CLOSE axrt930_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH axrt930_cl INTO g_xreb_m.xrebld_desc,g_xreb_m.xrebcomp_desc,g_xreb_m.xrebld,g_xreb_m.xreb001, 
       g_xreb_m.xrebcomp,g_xreb_m.xreb003,g_xreb_m.xreb002
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_xreb_m.xrebld,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL axrt930_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM xreb_t WHERE xrebent = g_enterprise AND xrebld = g_xreb_m.xrebld
                                                               AND xreb001 = g_xreb_m.xreb001
                                                               AND xreb002 = g_xreb_m.xreb002
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("xreb_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_xreb_d.clear() 
 
     
      CALL axrt930_ui_browser_refresh()  
      CALL axrt930_ui_headershow()  
      CALL axrt930_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL axrt930_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL axrt930_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE axrt930_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_xreb_m.xrebld,'D')
    
END FUNCTION
]]>
