  <section id="axrt930.b_fill" ver="30" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION axrt930_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_xreb_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE xreb004,xreb005,xreb006,xreb007,xreb009,xreb008,xreb103,xreb113,xreb115, 
       xreb125,xreb135,t1.ooag011 FROM xreb_t",   
               "",
               
                              " LEFT JOIN ooag_t t1 ON t1.ooagent='"||g_enterprise||"' AND t1.ooag001=xreb009  ",
 
               " WHERE xrebent= ? AND xrebld=? AND xreb001=? AND xreb002=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("xreb_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF axrt930_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY xreb_t.xreb005,xreb_t.xreb006,xreb_t.xreb007"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE axrt930_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR axrt930_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_xreb_m.xrebld,g_xreb_m.xreb001,g_xreb_m.xreb002
                                               
      FOREACH b_fill_cs INTO g_xreb_d[l_ac].xreb004,g_xreb_d[l_ac].xreb005,g_xreb_d[l_ac].xreb006,g_xreb_d[l_ac].xreb007, 
          g_xreb_d[l_ac].xreb009,g_xreb_d[l_ac].xreb008,g_xreb_d[l_ac].xreb103,g_xreb_d[l_ac].xreb113, 
          g_xreb_d[l_ac].xreb115,g_xreb_d[l_ac].xreb125,g_xreb_d[l_ac].xreb135,g_xreb_d[l_ac].xreb009_desc 

                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_xreb_d.deleteElement(g_xreb_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE axrt930_pb   
   
END FUNCTION
]]>
</section>
