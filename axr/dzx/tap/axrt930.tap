  <section id="axrt930.main" ver="33" status="" src="s">
<![CDATA[#+ 此段落由子樣板a26產生
#OPTIONS SHORT CIRCUIT
#+ 作業開始 
MAIN
   #add-point:main段define
   {<point name="main.define"/>}
   #end add-point   
 
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("axr","")
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT '','',xrebld,xreb001,xrebcomp,xreb003,xreb002", 
                      " FROM xreb_t",
                      " WHERE xrebent= ? AND xrebld=? AND xreb001=? AND xreb002=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE axrt930_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE xrebld,xreb001,xrebcomp,xreb003,xreb002,t1.glaal002 ,t2.ooefl003",
               " FROM xreb_t",
                              " LEFT JOIN glaal_t t1 ON t1.glaalent='"||g_enterprise||"' AND t1.glaalld=xrebld AND t1.glaal001='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent='"||g_enterprise||"' AND t2.ooefl001=xrebcomp AND t2.ooefl002='"||g_dlang||"' ",
 
               " WHERE xrebent = '" ||g_enterprise|| "' AND xrebld = ? AND xreb001 = ? AND xreb002 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE axrt930_master_referesh FROM g_sql
 
 
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_axrt930 WITH FORM cl_ap_formpath("axr",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL axrt930_init()   
 
      #進入選單 Menu (="N")
      CALL axrt930_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_axrt930
      
   END IF 
   
   CLOSE axrt930_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
   
END MAIN
 
 
]]>
</section>
  <section id="axrt930.modify" ver="32" status="" src="s">
<![CDATA[#+ 資料修改
PRIVATE FUNCTION axrt930_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_xreb_m.xrebld IS NULL
   OR g_xreb_m.xreb001 IS NULL
   OR g_xreb_m.xreb002 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE axrt930_master_referesh USING g_xreb_m.xrebld,g_xreb_m.xreb001,g_xreb_m.xreb002 INTO g_xreb_m.xrebld, 
       g_xreb_m.xreb001,g_xreb_m.xrebcomp,g_xreb_m.xreb003,g_xreb_m.xreb002,g_xreb_m.xrebld_desc,g_xreb_m.xrebcomp_desc 

 
   ERROR ""
  
   LET g_xrebld_t = g_xreb_m.xrebld
   LET g_xreb001_t = g_xreb_m.xreb001
   LET g_xreb002_t = g_xreb_m.xreb002
 
   CALL s_transaction_begin()
   
   OPEN axrt930_cl USING g_enterprise,g_xreb_m.xrebld
                                                       ,g_xreb_m.xreb001
                                                       ,g_xreb_m.xreb002
 
   IF STATUS THEN
      CALL cl_err("OPEN axrt930_cl:", STATUS, 1)
      CLOSE axrt930_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH axrt930_cl INTO g_xreb_m.xrebld_desc,g_xreb_m.xrebcomp_desc,g_xreb_m.xrebld,g_xreb_m.xreb001, 
       g_xreb_m.xrebcomp,g_xreb_m.xreb003,g_xreb_m.xreb002
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_xreb_m.xrebld,SQLCA.sqlcode,0)
      CLOSE axrt930_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL axrt930_show()
   WHILE TRUE
      LET g_xrebld_t = g_xreb_m.xrebld
      LET g_xreb001_t = g_xreb_m.xreb001
      LET g_xreb002_t = g_xreb_m.xreb002
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL axrt930_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_xreb_m.* = g_xreb_m_t.*
         CALL axrt930_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_xreb_m.xrebld != g_xrebld_t 
      OR g_xreb_m.xreb001 != g_xreb001_t 
      OR g_xreb_m.xreb002 != g_xreb002_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE xreb_t SET xrebld = g_xreb_m.xrebld
                                       , xreb001 = g_xreb_m.xreb001
                                       , xreb002 = g_xreb_m.xreb002
 
          WHERE xrebent = g_enterprise AND xrebld = g_xrebld_t
            AND xreb001 = g_xreb001_t
            AND xreb002 = g_xreb002_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("xreb_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("xreb_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_xreb_m.xrebld,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE axrt930_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_xreb_m.xrebld,'U')
 
   CALL axrt930_b_fill("1=1")
   
END FUNCTION   
]]>
</section>
