   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE axrt930_detail AS ",
                "SELECT * FROM xreb_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
