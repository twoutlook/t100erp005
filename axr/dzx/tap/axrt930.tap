  <point name="function.axrt930_xreb_ref" cite_std="N" status="" ver="2" src="s" new="Y" order="1" mark_hard="N">
<![CDATA[
################################################################################
# Descriptions...: 多語言欄位帶值
# Memo...........:
# Usage..........: CALL axrt930_xreb_ref(p_lab)
#                  RETURNING ---
# Input parameter: p_lab          欄位名稱
# Return code....: 
# Date & Author..: 2014/04/22 By zhangwei
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt930_xreb_ref(p_lab)
   DEFINE p_lab         LIKE type_t.chr20

   CASE
      WHEN p_lab = 'xrebld'
         INITIALIZE g_ref_fields TO NULL
         LET g_ref_fields[1] = g_xreb_m.xrebld
         CALL ap_ref_array2(g_ref_fields,"SELECT glaal002 FROM glaal_t WHERE glaalent='"||g_enterprise||"' AND glaalld=? AND glaal001='"||g_dlang||"'","") RETURNING g_rtn_fields
         LET g_xreb_i.xrebld_desc = '', g_rtn_fields[1] , ''
         DISPLAY BY NAME g_xreb_m.xrebld_desc
         SELECT glaacomp INTO g_xreb_i.xrebcomp_i FROM glaa_t WHERE glaaent = g_enterprise AND glaald =  g_xreb_i.xrebld_i
         
         INITIALIZE g_ref_fields TO NULL
         LET g_ref_fields[1] = g_xreb_i.xrebcomp_i
         CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
         LET g_xreb_i.xrebcomp_desc = '', g_rtn_fields[1] , ''
         DISPLAY BY NAME g_xreb_i.xrebcomp_i,g_xreb_i.xrebcomp_desc
   END CASE
   
END FUNCTION]]>
