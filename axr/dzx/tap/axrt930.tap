  <point name="cs.more_construct" cite_std="N" status="" ver="2" src="s" new="N" mark_hard="N">
<![CDATA[      INPUT BY NAME g_xreb_i.xrebld_i,g_xreb_i.xreb001_i,g_xreb_i.xreb002_i,g_xreb_i.combx
         ATTRIBUTE(WITHOUT DEFAULTS)
         
         BEFORE INPUT
            LET g_xreb_i.xreb001_i =  YEAR(g_today)
            LET g_xreb_i.xreb002_i = MONTH(g_today)
            DISPLAY BY NAME g_xreb_i.xreb001_i,g_xreb_i.xreb002_i

         AFTER FIELD xrebld_i
            LET g_xreb_i.xrebld_desc = ' '
            DISPLAY BY NAME g_xreb_i.xrebld_desc
            IF NOT cl_null(g_xreb_i.xrebld_i) THEN
               INITIALIZE g_chkparam.* TO NULL
               LET g_chkparam.arg1 = g_xreb_i.xrebld_i
               LET g_chkparam.arg2 = ' '
               IF NOT cl_chk_exist("v_glaald_1") THEN
                  LET g_xreb_i.xrebld_i = ''
                  CALL axrt930_xreb_ref('xrebld')
                  NEXT FIELD CURRENT
               END IF
               #資料邏輯正確性檢查
               CALL s_ld_chk_authorization(g_user,g_xreb_i.xrebld_i) RETURNING l_success
               IF l_success = 'N' THEN
                  CALL cl_err(g_xreb_i.xrebld_i,'axr-00022',1)
                  LET g_xreb_i.xrebld_i = ''
                  CALL axrt930_xreb_ref('xrebld')
                  NEXT FIELD CURRENT
               END IF
            END IF
            CALL axrt930_xreb_ref('xrebld')

         AFTER FIELD xreb001_i


         AFTER FIELD xreb002_i

         ON ACTION controlp INFIELD xrebld_i
			   INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'i'
			   LET g_qryparam.reqry = FALSE
            
            LET g_qryparam.default1 = g_xreb_i.xrebld_i           #給予default值
            
            #給予arg
            SELECT ooag003 INTO l_ooag003 FROM ooag_t WHERE ooagent = g_enterprise
               AND ooag001 = g_user
            LET g_qryparam.arg1 = g_user
            LET g_qryparam.arg2 = l_ooag003

            CALL q_authorised_ld()                                #呼叫開窗
            
            LET g_xreb_i.xrebld_i = g_qryparam.return1            #將開窗取得的值回傳到變數
            
            DISPLAY g_xreb_i.xrebld_i TO xrebld_i                 #顯示到畫面上
            CALL axrt930_xreb_ref('xrebld')
            
            NEXT FIELD xrebld_i                                   #返回原欄位
            
            
         AFTER INPUT
            IF INT_FLAG THEN
               EXIT DIALOG
            END IF

      END INPUT

      INPUT BY NAME g_xreb_i.check1,g_xreb_i.check2,g_xreb_i.check3
         ATTRIBUTE(WITHOUT DEFAULTS)
         
         BEFORE INPUT
            IF cl_null(g_xreb_i.check1) THEN LET g_xreb_i.check1 = 'N' END IF
            IF cl_null(g_xreb_i.check2) THEN LET g_xreb_i.check2 = 'N' END IF
            IF cl_null(g_xreb_i.check3) THEN LET g_xreb_i.check3 = 'N' END IF
            DISPLAY BY NAME g_xreb_i.check1,g_xreb_i.check2,g_xreb_i.check3

         AFTER INPUT
            IF INT_FLAG THEN
               EXIT DIALOG
            END IF

      END INPUT
]]>
</point>
