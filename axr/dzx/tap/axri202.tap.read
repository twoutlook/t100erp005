   {<point name="delete.define_customerization" edit="c"/>}
   #end add-point
   
   IF g_glab_m.glabld IS NULL
   OR g_glab_m.glab001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   
 
   OPEN axri202_cl USING g_enterprise,g_glab_m.glabld,g_glab_m.glab001
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN axri202_cl:" 
      LET g_errparam.code   =  STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE axri202_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE axri202_master_referesh USING g_glab_m.glabld,g_glab_m.glab001 INTO g_glab_m.glabld,g_glab_m.glab001, 
       g_glab_m.glab004,g_glab_m.glabld_desc
   
   #遮罩相關處理
   LET g_glab_m_mask_o.* =  g_glab_m.*
   CALL axri202_glab_t_mask()
   LET g_glab_m_mask_n.* =  g_glab_m.*
   
   CALL axri202_show()
 
   #單頭多語言資料備份
   
   
   IF cl_ask_del_master() THEN              #確認一下
      #應用 a47 樣板自動產生(Version:2)
      #刪除相關文件
      CALL axri202_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM glab_t WHERE glabent = g_enterprise AND glabld = g_glab_m.glabld
                                                               AND glab001 = g_glab_m.glab001
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "glab_t" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      #單頭多語言刪除
      
      
      #單身多語言刪除
      
 
 
   
      #add-point:多語言刪除
      {<point name="delete.lang.delete"/>}
      #end add-point
      
      #頭先不紀錄
      #IF NOT cl_log_modified_record('','') THEN 
      #   CLOSE axri202_cl
      #   CALL s_transaction_end('N','0')
      #   RETURN 
      #END IF
      
      CLEAR FORM
      CALL g_glab_d.clear() 
 
     
      CALL axri202_ui_browser_refresh()  
      #CALL axri202_ui_headershow()  
      #CALL axri202_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL axri202_fetch('P')
      ELSE
         #LET g_wc = " 1=1"
         #LET g_wc2 = " 1=1"
         #CALL axri202_browser_fill("F")
