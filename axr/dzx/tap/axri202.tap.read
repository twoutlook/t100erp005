  <point name="function.axri202_glaa_dis" order="1" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 單頭排除欄位資料顯示
# Memo...........:
# Usage..........: CALL axri202_glaa_dis()
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION axri202_glaa_dis()
   DEFINE l_glaacomp      LIKE glaa_t.glaacomp
   DEFINE l_glaa008       LIKE glaa_t.glaa008
   DEFINE l_glaa014       LIKE glaa_t.glaa014
   DEFINE l_glaacomp_desc LIKE ooefl_t.ooefl003
   DEFINE l_success       LIKE type_t.num5

   SELECT glaacomp,glaa008,glaa014 INTO l_glaacomp,l_glaa008,l_glaa014 FROM glaa_t
    WHERE glaaent = g_enterprise
      AND glaald = g_glab_m.glabld

   CALL s_axrt300_xrca_ref('xrcasite',l_glaacomp,'','') RETURNING l_success,l_glaacomp_desc
   CALL s_axrt300_xrca_ref('xrcald',g_glab_m.glabld,'','') RETURNING l_success,g_glab_m.glabld_desc

   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = 0
   LET g_ref_fields[2] = g_glab_m.glab004
   CALL ap_ref_array2(g_ref_fields,"SELECT ooall004 FROM ooall_t WHERE ooallent='"||g_enterprise||"' AND ooall001=? AND ooall002=? AND ooall003='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_glab_m.glab004_desc = g_rtn_fields[1]

   DISPLAY l_glaacomp,l_glaa008,l_glaa014,l_glaacomp_desc,g_glab_m.glab004_desc,g_glab_m.glabld_desc
        TO glaacomp,glaa008,glaa014,glaacomp_desc,glab004_desc,glabld_desc

END FUNCTION]]>
  </point>
  <point name="function.axri202_rebuild_glab" order="2" ver="2" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 刷新單身默認值
# Memo...........:
# Usage..........: CALL axri202_rebuild_glab()
#                  RETURNING r_success
# Input parameter: 
# Return code....: r_success      執行成功否
# Date & Author..: 2015/05/05 By 01727
# Modify.........:
################################################################################
PRIVATE FUNCTION axri202_rebuild_glab()
   DEFINE r_success         LIKE type_t.num5
   DEFINE l_gzcb002         LIKE gzcb_t.gzcb002
   DEFINE l_rtax001         LIKE rtax_t.rtax001   #150417-00007#60 Add
   DEFINE l_count           LIKE type_t.num5      #150417-00007#60 Add
   DEFINE l_sql             STRING
   #2015/06/11--add--str--
   DEFINE l_ooaa002         LIKE ooaa_t.ooaa002
  
   #品类管理层级aoos010
   CALL cl_get_para(g_enterprise,'','E-CIR-0001') RETURNING l_ooaa002
   IF cl_null(l_ooaa002) THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "cxr-00001"
      LET g_errparam.popup  = TRUE 
      CALL cl_err() 
      RETURN
   END IF
   #2015/06/11--add--end

   #STEP1.將單身品類不為最底层的删除
   #STEP2.将在arti202中新增的品類匯入單身

   #150417-00007#60 Add  ---(S)---
   #SA確定說上線後經營方式基本不會調整,所以不刷新
   #修改狀態下進入單身.如果出現了新的品類,只根據單身現有的經營方式的個數去產生新的品類
   #150417-00007#60 Add  ---(E)---

   CALL s_transaction_end('N','0')

   CALL s_transaction_begin()

   #STEP1
   DELETE FROM glab_t WHERE glabent = g_enterprise
      AND glabld = g_glab_m.glabld
      AND glab001 = 'RTAX'
     #AND glab002 = 'RTAX'   #150417-00007#60  Mark
      AND glab003 IN (SELECT rtax001 FROM rtax_t WHERE rtaxent = g_enterprise AND rtax005 <> 0)

   #150417-00007#60 Add  ---(S)---
   LET l_count = 0
   SELECT COUNT (*) INTO l_count FROM glab_t WHERE glabent = g_enterprise
      AND glabld = g_glab_m.glabld
      AND glab001 = 'RTAX'
   IF cl_null(l_count) THEN LET l_count = 0 END IF
   #150417-00007#60 Add  ---(E)---

   #STEP2
   #150417-00007#60 Mark ---(S)---
  #LET l_sql = "SELECT rtax001 FROM rtax_t WHERE rtaxent = '",g_enterprise,"'              ",
  #            "   AND rtax005 = 0                                                         ",
  #            "   AND NOT EXISTS (SELECT 1 FROM glab_t                                    ",
  #            "      WHERE glabent = '",g_enterprise,"' AND glabld = '",g_glab_m.glabld,"'",
  #            "        AND glab001 = 'RTAX' AND glab002 = 'RTAX' AND glab003 = rtax001)   "
  #PREPARE axri202_rebuild_prep FROM l_sql
  #DECLARE axri202_rebuild_curs CURSOR FOR axri202_rebuild_prep
   #150417-00007#60 Mark ---(E)---
   #150417-00007#60 Add  ---(S)---
   IF l_count = 0 THEN
      #單身無資料表示為新增,所以用所有最下級品類+經營方式產生單身資料
      LET l_sql = "SELECT rtax001,gzcb002 FROM rtax_t,gzcb_t WHERE rtaxent = '",g_enterprise,"'",
                  "   AND rtax004 = ",l_ooaa002,"      AND gzcb001 = '6013'                    ",
                  "   AND rtaxent = '",g_enterprise,"' AND rtaxstus = 'Y'                      ",
                  "   AND NOT EXISTS (SELECT 1 FROM glab_t                                     ",
                  "      WHERE glabent = '",g_enterprise,"' AND glabld = '",g_glab_m.glabld,"' ",
                  "        AND glab001 = 'RTAX' AND glab003 = rtax001)                         "
   ELSE
      #單身有資料表示為修改,所以用新增最下級品類+單身經營方式產生單身資料
      LET l_sql = "SELECT rtax001,glab002 FROM rtax_t,(SELECT DISTINCT glab002 FROM glab_t     ",
                  "       WHERE glabent = '",g_enterprise,"' AND glabld = '",g_glab_m.glabld,"'",
                  "         AND glab001 = 'RTAX') WHERE rtaxent = '",g_enterprise,"'           ",
                  "   AND rtax004 = ",l_ooaa002,"       AND rtaxstus = 'Y'                     ",
                  "   AND NOT EXISTS (SELECT 1 FROM glab_t                                     ",
                  "      WHERE glabent = '",g_enterprise,"' AND glabld = '",g_glab_m.glabld,"' ",
                  "        AND glab001 = 'RTAX' AND glab003 = rtax001)                         "
   END IF
   PREPARE axri202_rebuild_prep FROM l_sql
   DECLARE axri202_rebuild_curs CURSOR FOR axri202_rebuild_prep
   #150417-00007#60 Add  ---(E)---

   FOREACH axri202_rebuild_curs INTO l_rtax001,l_gzcb002
      INSERT INTO glab_t(glabent,glabld,glab001,glab002,glab003,glab004,glab011)
                  VALUES(g_enterprise,g_glab_m.glabld,'RTAX',l_gzcb002,l_rtax001,g_glab_m.glab004,1)
   END FOREACH

   CALL s_transaction_end('Y','0')

   CALL s_transaction_begin()

END FUNCTION]]>
  </point>
