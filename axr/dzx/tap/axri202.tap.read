   #將新增的資料併入搜尋條件中
   LET g_state = "insert"
   
   LET g_glabld_t = g_glab_m.glabld
   LET g_glab001_t = g_glab_m.glab001
 
   
   #組合新增資料的條件
   LET g_add_browse = " glabent = '" ||g_enterprise|| "' AND",
                      " glabld = '", g_glab_m.glabld, "' "
                      ," AND glab001 = '", g_glab_m.glab001, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL axri202_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   CALL axri202_idx_chk()
   
   #撈取異動後的資料(主要是帶出reference)
   EXECUTE axri202_master_referesh USING g_glab_m.glabld,g_glab_m.glab001 INTO g_glab_m.glabld,g_glab_m.glab001, 
       g_glab_m.glab004,g_glab_m.glabld_desc
   
   #遮罩相關處理
   LET g_glab_m_mask_o.* =  g_glab_m.*
   CALL axri202_glab_t_mask()
   LET g_glab_m_mask_n.* =  g_glab_m.*
   
   #將資料顯示到畫面上
