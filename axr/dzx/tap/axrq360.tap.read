  <section id="axrq360.b_fill" ver="14" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION axrq360_b_fill()
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準
   {<point name="b_fill.define" edit="s"/>}
   #end add-point
   #add-point:b_fill段define-客製
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   LET ls_sql_rank = "SELECT  UNIQUE '',xrcasite,'',xrcacomp,'',xrcald,'',xrcadocno,xrcastus,xrcadocdt, 
       xrca001,xrca004,'',xrca057,'',xrca035,'',xrca038,xrca014,'',xrca015,'',xrca058,'',xrca007,'', 
       xrca040,'','','','','','','','','','','','',''  ,DENSE_RANK() OVER( ORDER BY xrca_t.xrcald,xrca_t.xrcadocno) AS RANK FROM xrca_t", 

 
 
                     "",
                     " WHERE xrcaent= ? AND 1=1 AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("xrca_t"),
                     " ORDER BY xrca_t.xrcald,xrca_t.xrcadocno"
 
   #add-point:b_fill段rank_sql_after
   {<point name="b_fill.rank_sql_after"/>}
   #end add-point
 
   LET g_sql = "SELECT COUNT(*) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT '',xrcasite,'',xrcacomp,'',xrcald,'',xrcadocno,xrcastus,xrcadocdt,xrca001,xrca004, 
       '',xrca057,'',xrca035,'',xrca038,xrca014,'',xrca015,'',xrca058,'',xrca007,'',xrca040,'','','', 
       '','','','','','','','','',''",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE axrq360_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR axrq360_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_xrca_d.clear()
 
   #add-point:陣列清空
   {<point name="b_fill.array_clear" />}
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1   
 
   FOREACH b_fill_curs INTO g_xrca_d[l_ac].sel,g_xrca_d[l_ac].xrcasite,g_xrca_d[l_ac].xrcasite_desc, 
       g_xrca_d[l_ac].xrcacomp,g_xrca_d[l_ac].xrcacomp_desc,g_xrca_d[l_ac].xrcald,g_xrca_d[l_ac].xrcald_desc, 
       g_xrca_d[l_ac].xrcadocno,g_xrca_d[l_ac].xrcastus,g_xrca_d[l_ac].xrcadocdt,g_xrca_d[l_ac].xrca001, 
       g_xrca_d[l_ac].xrca004,g_xrca_d[l_ac].xrca004_desc,g_xrca_d[l_ac].xrca057,g_xrca_d[l_ac].xrca057_desc, 
       g_xrca_d[l_ac].xrca035,g_xrca_d[l_ac].xrca035_desc,g_xrca_d[l_ac].xrca038,g_xrca_d[l_ac].xrca014, 
       g_xrca_d[l_ac].xrca014_desc,g_xrca_d[l_ac].xrca015,g_xrca_d[l_ac].xrca015_desc,g_xrca_d[l_ac].xrca058, 
       g_xrca_d[l_ac].xrca058_desc,g_xrca_d[l_ac].xrca007,g_xrca_d[l_ac].xrca007_desc,g_xrca_d[l_ac].xrca040, 
       g_xrca_d[l_ac].xrcc100,g_xrca_d[l_ac].xrcc102,g_xrca_d[l_ac].xrccseq,g_xrca_d[l_ac].xrcc001,g_xrca_d[l_ac].xrcc003, 
       g_xrca_d[l_ac].xrcc004,g_xrca_d[l_ac].xrcc009,g_xrca_d[l_ac].xrcc108,g_xrca_d[l_ac].xrcc109,g_xrca_d[l_ac].l_calc, 
       g_xrca_d[l_ac].xrcc118,g_xrca_d[l_ac].xrcc119,g_xrca_d[l_ac].l_calc2
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #LET g_xrca_d[l_ac].statepic = cl_get_actipic(g_xrca_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL axrq360_detail_show("'1'")      
 
      CALL axrq360_xrca_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
      
   END FOREACH
   LET g_error_show = 0
   
 
   
   CALL g_xrca_d.deleteElement(g_xrca_d.getLength())   
 
   #add-point:陣列長度調整
   {<point name="b_fill.array_deleteElement" />}
   #end add-point
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = g_xrca_d.getLength()
#  DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE axrq360_pb
 
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL axrq360_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL axrq360_detail_action_trans()
 
   IF g_xrca_d.getLength() > 0 THEN
      LET l_ac = 1
      CALL axrq360_fetch()
   END IF
   
      CALL axrq360_filter_show('xrcasite','b_xrcasite')
   CALL axrq360_filter_show('xrcacomp','b_xrcacomp')
   CALL axrq360_filter_show('xrcald','b_xrcald')
   CALL axrq360_filter_show('xrcadocno','b_xrcadocno')
   CALL axrq360_filter_show('xrcastus','b_xrcastus')
   CALL axrq360_filter_show('xrcadocdt','b_xrcadocdt')
   CALL axrq360_filter_show('xrca001','b_xrca001')
   CALL axrq360_filter_show('xrca004','b_xrca004')
   CALL axrq360_filter_show('xrca035','b_xrca035')
   CALL axrq360_filter_show('xrca038','b_xrca038')
   CALL axrq360_filter_show('xrca014','b_xrca014')
   CALL axrq360_filter_show('xrca015','b_xrca015')
   CALL axrq360_filter_show('xrca058','b_xrca058')
   CALL axrq360_filter_show('xrca007','b_xrca007')
   CALL axrq360_filter_show('xrca040','b_xrca040')
   CALL axrq360_filter_show('xrcc100','b_xrcc100')
   CALL axrq360_filter_show('xrcc003','b_xrcc003')
   CALL axrq360_filter_show('xrcc004','b_xrcc004')
   CALL axrq360_filter_show('xrcc009','b_xrcc009')
 
 
   #add-point:b_fill段結束前
   {<point name="b_fill.after"/>}
   #end add-point
 
END FUNCTION
]]>
  </section>
