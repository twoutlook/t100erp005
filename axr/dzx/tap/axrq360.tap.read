  <point name="b_fill.sql_after" order="" ver="2" cite_std="N" new="N" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[   CALL axrq360_create_tmp()
   DELETE FROM axrq360_print_tmp
   LET g_sql = "SELECT UNIQUE '',xrcasite,'',xrcacomp,'',xrcald,'',xrcadocno,xrcastus,xrcadocdt,xrca001, 
                       xrca004,'',xrca057,'',xrca035,'',xrca038,xrca014,'',xrca015,'',xrca058,'',xrca007,'',
                       xrca040,xrcc100,xrcc102,xrccseq,xrcc001,xrcc003,xrcc004,xrcc009,
                       CASE WHEN xrcc108 IS NULL THEN 0 ELSE xrcc108 END,
                       CASE WHEN xrcc109 IS NULL THEN 0 ELSE xrcc109 END,
                       CASE WHEN xrcc108 IS NULL THEN 0 ELSE xrcc108 END
                       - CASE WHEN xrcc109 IS NULL THEN 0 ELSE xrcc109 END,
                       CASE WHEN xrcc118 IS NULL THEN 0 ELSE xrcc118 END
                       + CASE WHEN xrcc113 IS NULL THEN 0 ELSE xrcc113 END,
                       CASE WHEN xrcc119 IS NULL THEN 0 ELSE xrcc119 END,
                       CASE WHEN xrcc118 IS NULL THEN 0 ELSE xrcc118 END
                       + CASE WHEN xrcc113 IS NULL THEN 0 ELSE xrcc113 END
                       - CASE WHEN xrcc119 IS NULL THEN 0 ELSE xrcc119 END
                       FROM xrca_t LEFT JOIN xrcc_t ON xrccent = xrcaent AND xrccdocno = xrcadocno AND xrccld = xrcald",
               " WHERE xrcaent= ? AND 1=1 AND ", ls_wc,cl_sql_add_filter("xrca_t")
    
   LET g_sql = g_sql, cl_sql_add_filter("xrca_t"),
                      " ORDER BY xrca_t.xrcald,xrca_t.xrcadocno"]]>
