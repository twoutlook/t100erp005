  <point name="detail_show.body.reference" order="" ver="2" cite_std="N" new="N" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[            SELECT glaa004 INTO l_glaa004 FROM glaa_t WHERE glaaent = g_enterprise
               AND glaald = g_xrca_d[l_ac].xrcald

            INITIALIZE g_ref_fields TO NULL
            LET g_ref_fields[1] = g_xrca_d[l_ac].xrcasite
            LET ls_sql = "SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'"
            LET ls_sql = cl_sql_add_mask(ls_sql)              #遮蔽特定資料
            CALL ap_ref_array2(g_ref_fields,ls_sql,"") RETURNING g_rtn_fields
            LET g_xrca_d[l_ac].xrcasite_desc = '', g_rtn_fields[1] , ''
            DISPLAY BY NAME g_xrca_d[l_ac].xrcasite_desc

            INITIALIZE g_ref_fields TO NULL
            LET g_ref_fields[1] = g_xrca_d[l_ac].xrcacomp
            LET ls_sql = "SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'"
            LET ls_sql = cl_sql_add_mask(ls_sql)              #遮蔽特定資料
            CALL ap_ref_array2(g_ref_fields,ls_sql,"") RETURNING g_rtn_fields
            LET g_xrca_d[l_ac].xrcacomp_desc = '', g_rtn_fields[1] , ''
