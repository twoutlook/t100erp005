  <point name="function.axrq360_qbe_clear" order="1" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 預設顯示
# Memo...........:
# Usage..........: CALL axrq360_qbe_clear()

# Date & Author..: 2015/02/09 By 01727
# Modify.........:
################################################################################
PRIVATE FUNCTION axrq360_qbe_clear()
   LET l_ac = 1
   LET g_xrca_d[l_ac].xrcasite = ''
   DISPLAY ARRAY g_xrca_d TO s_detail1.*
      BEFORE DISPLAY
         EXIT DISPLAY
   END DISPLAY
END FUNCTION]]>
