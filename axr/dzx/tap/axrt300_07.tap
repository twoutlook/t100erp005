################################################################################
DIALOG axrt300_07_input()
DEFINE xrcb         RECORD LIKE xrcb_t.*
DEFINE l_forupd_sql   STRING
DEFINE l_lock_sw      LIKE type_t.chr1 
DEFINE l_xrcadocdt    LIKE xrca_t.xrcadocdt
