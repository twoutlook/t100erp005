            #其他table資料備份(確定是否更改用)

            #其他table進行lock


         BEFORE INSERT

            CALL s_transaction_begin()
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_glaq_d_t.* TO NULL
            INITIALIZE g_glaq_d[l_ac].* TO NULL

            LET g_glaq_d_t.* = g_glaq_d[l_ac].*     #新輸入資料
