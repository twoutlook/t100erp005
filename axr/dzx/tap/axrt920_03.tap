       glaqent=? AND glaqld=? AND glaqdocno=? AND glaqseq=? FOR UPDATE"
   #add-point:modify段define_sql

   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   DECLARE axrt920_03_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR



   LET INT_FLAG = FALSE
