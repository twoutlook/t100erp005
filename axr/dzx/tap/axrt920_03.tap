         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_glaq_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_glaq_d.deleteElement(l_ac)
               NEXT FIELD glaqld
            END IF
            IF g_glaq_d[l_ac].glaqld IS NOT NULL
               AND g_glaq_d_t.glaqdocno IS NOT NULL
