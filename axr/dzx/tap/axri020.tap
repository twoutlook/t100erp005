   IF NOT cl_null(g_xrah_m.xrah006) THEN 
      IF NOT cl_null(g_xrah_m.xrah001) AND NOT cl_null(g_xrah_m.xrah002) AND NOT cl_null(g_xrah_m.xrah003) AND g_xrah_m.xrah003 > 1THEN 
