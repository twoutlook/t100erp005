  <point name="function.axri020_statechange" cite_std="N" status="" ver="1" src="s" new="Y" cite_ver="" order="35" mark_hard="N">
<![CDATA[#+ 確認碼變更
PRIVATE FUNCTION axri020_statechange()
   {<Local define>}
   DEFINE lc_state LIKE type_t.chr5
   {</Local define>}
   DEFINE l_xrah006_max  LIKE xrah_t.xrah006
   DEFINE l_xrah003_max  LIKE xrah_t.xrah003
   
   ERROR ""     #清空畫面右下側ERROR區塊

   IF g_xrah_m.xrah001 IS NULL
      #key2
      OR g_xrah_m.xrah002 IS NULL
      OR g_xrah_m.xrah003 IS NULL
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF

   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         CASE g_xrah_m.xrahstus
            WHEN "N"
               HIDE OPTION "open"

            WHEN "Y"
               HIDE OPTION "confirmed"
            
            WHEN "X"
               HIDE OPTION "invalid"

         END CASE
     
      #add-point:menu前

      #end add-point
	  
      ON ACTION open
         LET lc_state = "N"
         #add-point:action控制
         SELECT MAX(xrah003),MAX(xrah006) INTO l_xrah003_max,l_xrah006_max
          FROM xrah_t
         WHERE xrahent = g_enterprise 
           AND xrah001 = g_xrah_m.xrah001 
           AND xrah002 = g_xrah_m.xrah002
         IF NOT (l_xrah003_max = g_xrah_m.xrah003 OR l_xrah006_max = g_xrah_m.xrah006) THEN 
            LET lc_state = ''
            CALL cl_err("",'axr-00059',1)
         END IF
         #end add-point
         EXIT MENU

      ON ACTION confirmed
         LET lc_state = "Y"
         #add-point:action控制

         #end add-point
         EXIT MENU

      ON ACTION invalid
         LET lc_state = "X"
         #add-point:action控制
         IF g_xrah_m.xrah007 ='Y' THEN 
            LET lc_state = ''
            CALL cl_err("",'axr-00015',1)
         END IF
         #end add-point
         EXIT MENU

	  
      #add-point:stus控制

      #end add-point
	  
   END MENU
   
   IF (lc_state <> "N" 
      AND lc_state <> "Y" AND lc_state <> "X"
      ) OR 
      cl_null(lc_state) THEN
      RETURN
   END IF
   
   #add-point:stus修改前

   #end add-point
      
   UPDATE xrah_t SET xrahstus = lc_state 
    WHERE xrahent = g_enterprise AND xrah001 = g_xrah_m.xrah001
      AND xrah002 = g_xrah_m.xrah002
      AND xrah003 = g_xrah_m.xrah003

  
   IF SQLCA.sqlcode THEN
      CALL cl_err("",SQLCA.sqlcode,0)
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/open.png")
            
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
            
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
           
      END CASE
   END IF

END FUNCTION]]>
</point>
