  <point name="b_fill.fill" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[      LET g_xrcc2_d[l_ac].xrcc108 = g_xrcc_d[l_ac].xrcc108
      LET g_xrcc2_d[l_ac].xrcc118 = g_xrcc_d[l_ac].xrcc118
      SELECT xrac004||'/'||xrac005||'/'||xrac006 INTO g_xrcc_d[l_ac].date FROM xrac_t
       WHERE xracent = g_enterprise AND xrac001 = '3114' 
