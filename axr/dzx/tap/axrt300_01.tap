  <point name="function.axrt300_01_display" order="2" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 本位幣名稱顯示
# Memo...........:
# Usage..........: CALL axrt300_01_display()
# Date & Author..: 2014/02/12 By xianghui
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt300_01_display()
DEFINE　l_xrca100   LIKE xrca_t.xrca100
DEFINE　l_xrca120   LIKE xrca_t.xrca120
DEFINE　l_xrca130   LIKE xrca_t.xrca130
DEFINE  l_glaa001   LIKE glaa_t.glaa001
DEFINE  l_msg       STRING
DEFINE  l_msg1       STRING

   SELECT xrca100,xrca120,xrca130 INTO l_xrca100,l_xrca120,l_xrca130 FROM xrca_t
    WHERE xrcaent = g_enterprise AND xrcald = g_no AND xrcadocno = g_xrccdocno
   SELECT glaa001 INTO l_glaa001 FROM glaa_t WHERE glaald = g_no AND glaaent = g_enterprise 

   #交易金額
   CALL cl_getmsg('axr-00077',g_lang) RETURNING l_msg
   LET l_msg1 = l_msg,'(',l_xrca120,')'
   CALL cl_set_comp_att_text("xrcc125",l_msg1 CLIPPED)
   LET l_msg1 = l_msg,'(',l_xrca130,')'
   CALL cl_set_comp_att_text("xrcc135",l_msg1 CLIPPED) 

   #稅前折抵
   CALL cl_getmsg('axr-00078',g_lang) RETURNING l_msg
   LET l_msg1 = l_msg,'(',l_xrca120,')'
   CALL cl_set_comp_att_text("xrcc126",l_msg1 CLIPPED)
   LET l_msg1 = l_msg,'(',l_xrca130,')'
   CALL cl_set_comp_att_text("xrcc136",l_msg1 CLIPPED) 
   
   #沖帳金額
   CALL cl_getmsg('axr-00079',g_lang) RETURNING l_msg
   LET l_msg1 = l_msg,'(',l_xrca120,')'
   CALL cl_set_comp_att_text("xrcc127",l_msg1 CLIPPED)
   LET l_msg1 = l_msg,'(',l_xrca130,')'
   CALL cl_set_comp_att_text("xrcc137",l_msg1 CLIPPED) 

   #調整金額
   CALL cl_getmsg('axr-00080',g_lang) RETURNING l_msg
   LET l_msg1 = l_msg,'(',l_xrca120,')'
   CALL cl_set_comp_att_text("xrcc124",l_msg1 CLIPPED)
   LET l_msg1 = l_msg,'(',l_xrca130,')'
   CALL cl_set_comp_att_text("xrcc134",l_msg1 CLIPPED) 
   
   #應收收款
   CALL cl_getmsg('axr-00081',g_lang) RETURNING l_msg
   LET l_msg1 = l_msg,'(',l_xrca120,')'
   CALL cl_set_comp_att_text("xrcc128",l_msg1 CLIPPED)
   LET l_msg1 = l_msg,'(',l_xrca130,')'
   CALL cl_set_comp_att_text("xrcc138",l_msg1 CLIPPED)    

   #原幣應收
   CALL cl_getmsg('axr-00082',g_lang) RETURNING l_msg
   LET l_msg1 = l_msg,'(',l_xrca100,')'
   CALL cl_set_comp_att_text("xrcc108_2",l_msg1 CLIPPED)
   
   #原幣應收
   CALL cl_getmsg('axr-00083',g_lang) RETURNING l_msg
   LET l_msg1 = l_msg,'(',l_glaa001,')'
   CALL cl_set_comp_att_text("xrcc118_2",l_msg1 CLIPPED)

END FUNCTION]]>
