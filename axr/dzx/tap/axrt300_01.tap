               SELECT DISTINCT * INTO l_xrcd.* FROM xrcd_t WHERE xrcdent = g_enterprise
                  AND xrcddocno = g_xrcc_d[l_ac].xrccdocno AND xrcdseq = g_xrcc_d[l_ac].xrccseq

               LET l_xrcd.xrcd004 = 0
               LET l_xrcd.xrcd005 = 1
               LET l_xrcd.xrcd006 = 'N'
               LET l_xrcd.xrcd007 = 99
               LET l_xrcd.xrcd102 = g_xrcc_d[l_ac].xrcc104
               LET l_xrcd.xrcd103 = g_xrcc_d[l_ac].xrcc104
               LET l_xrcd.xrcd104 = g_xrcc_d[l_ac].xrcc104 * l_xrcd.xrcd003
               LET l_xrcd.xrcd105 = l_xrcd.xrcd103 + l_xrcd.xrcd104
               LET l_xrcd.xrcd112 = g_xrcc_d[l_ac].xrcc114
               LET l_xrcd.xrcd113 = g_xrcc_d[l_ac].xrcc114
