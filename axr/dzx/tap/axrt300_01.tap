                                 xrca108 = xrca108 + g_xrcc_d[l_ac].xrcc104 - g_xrcc_d_t.xrcc104,
                                 xrca117 = xrca117 + g_xrcc_d[l_ac].xrcc114 - g_xrcc_d_t.xrcc114,
                                 xrca118 = xrca118 + g_xrcc_d[l_ac].xrcc114 - g_xrcc_d_t.xrcc114,
                                 xrca127 = xrca127 + g_xrcc2_d[l_ac].xrcc124 - g_xrcc2_d_t.xrcc124,
                                 xrca128 = xrca128 + g_xrcc2_d[l_ac].xrcc124 - g_xrcc2_d_t.xrcc124,
                                 xrca137 = xrca137 + g_xrcc2_d[l_ac].xrcc134 - g_xrcc2_d_t.xrcc134,
