               LET l_xrcd.xrcd125 = l_xrcd.xrcd123 + l_xrcd.xrcd104
               LET l_xrcd.xrcd133 = g_xrcc2_d[l_ac].xrcc134
               LET l_xrcd.xrcd134 = g_xrcc2_d[l_ac].xrcc134 * l_xrcd.xrcd003
               LET l_xrcd.xrcd135 = l_xrcd.xrcd133 + l_xrcd.xrcd104

               DELETE FROM xrcd_t WHERE xrcdent = g_enterrpise AND xrcd007 = 99
                  AND xrcddocno = g_xrcc_d[l_ac].xrccdocno AND xrcdseq = g_xrcc_d[l_ac].xrccseq
                  
               INSERT INTO xrcd VALUES (l_xrcd.*)

               SELECT SUM(xrcd104) INTO l_xrcd104 FROM xrcd_t WHERE xrcdent = g_enterrpise
                  AND xrcddocno = g_xrcc_d[l_ac].xrccdocno AND xrcdseq = g_xrcc_d[l_ac].xrccseq

               UPDATE xrcb_t SET xrcb106 = xrcb106 + g_xrcc_d[l_ac].xrcc104,
                                 xrcb118 = xrcb103 - xrcb106 + l_xrcd104,
                                 xrcb116 = xrcb116 + g_xrcc_d[l_ac].xrcc114,
                                 xrcb126 = xrcb126 + g_xrcc2_d[l_ac].xrcc124,
                                 xrcb136 = xrcb136 + g_xrcc2_d[l_ac].xrcc134]]>
