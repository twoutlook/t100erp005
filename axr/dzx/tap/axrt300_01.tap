  </point>
  <point name="b_fill.others.fill" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[   SELECT xrca100,xrca101,xrca120,xrca121,xrca130,xrca131,
          xrca103+xrca104,xrca113+xrca114,xrca123+xrca124,xrca133+xrca134,
          xrca106+xrca107,xrca116+xrca117,xrca126+xrca127,xrca136+xrca137,
          xrca108,xrca118,xrca128,xrca138
     INTO g_xrcc_m.xrca100,g_xrcc_m.xrca101,g_xrcc_m.xrca120,g_xrcc_m.xrca121,g_xrcc_m.xrca130,g_xrcc_m.xrca131,
          g_xrcc_m.sum_a1,g_xrcc_m.sum_a2,g_xrcc_m.sum_a3,g_xrcc_m.sum_a4,
          g_xrcc_m.sum_b1,g_xrcc_m.sum_b2,g_xrcc_m.sum_b3,g_xrcc_m.sum_b4,
          g_xrcc_m.sum_d1,g_xrcc_m.sum_d2,g_xrcc_m.sum_d3,g_xrcc_m.sum_d4          
     FROM xrca_t 
    WHERE xrcaent = g_enterprise
      AND xrcald = g_no
      AND xrcadocno = g_xrccdocno 
      
   IF cl_null(g_xrcc_m.sum_a1) THEN LET g_xrcc_m.sum_a1 = 0 END IF IF cl_null(g_xrcc_m.sum_a2) THEN LET g_xrcc_m.sum_a2 = 0 END IF
   IF cl_null(g_xrcc_m.sum_a3) THEN LET g_xrcc_m.sum_a3 = 0 END IF IF cl_null(g_xrcc_m.sum_a4) THEN LET g_xrcc_m.sum_a4 = 0 END IF
   IF cl_null(g_xrcc_m.sum_b1) THEN LET g_xrcc_m.sum_b1 = 0 END IF IF cl_null(g_xrcc_m.sum_b2) THEN LET g_xrcc_m.sum_b2 = 0 END IF
   IF cl_null(g_xrcc_m.sum_b3) THEN LET g_xrcc_m.sum_b3 = 0 END IF IF cl_null(g_xrcc_m.sum_b4) THEN LET g_xrcc_m.sum_b4 = 0 END IF
   IF cl_null(g_xrcc_m.sum_d1) THEN LET g_xrcc_m.sum_d1 = 0 END IF IF cl_null(g_xrcc_m.sum_d2) THEN LET g_xrcc_m.sum_d2 = 0 END IF
   IF cl_null(g_xrcc_m.sum_d3) THEN LET g_xrcc_m.sum_d3 = 0 END IF IF cl_null(g_xrcc_m.sum_d4) THEN LET g_xrcc_m.sum_d4 = 0 END IF
   
   SELECT SUM(xrcc104),SUM(xrcc114),SUM(xrcc124),SUM(xrcc134),
          SUM(xrcc108),SUM(xrcc118),SUM(xrcc128),SUM(xrcc138)
     INTO g_xrcc_m.sum_c1,g_xrcc_m.sum_c2,g_xrcc_m.sum_c3,g_xrcc_m.sum_c4,
          g_xrcc_m.sum_e1,g_xrcc_m.sum_e2,g_xrcc_m.sum_e3,g_xrcc_m.sum_e4     
     FROM xrcc_t 
    WHERE xrccent = g_enterprise
      AND xrccld = g_no
      AND xrccdocno = g_xrccdocno       
   LET g_xrcc_m.sum_e1 = g_xrcc_m.sum_d1 - g_xrcc_m.sum_e1   
   LET g_xrcc_m.sum_e2 = g_xrcc_m.sum_d2 - g_xrcc_m.sum_e2
   LET g_xrcc_m.sum_e3 = g_xrcc_m.sum_d3 - g_xrcc_m.sum_e3
   LET g_xrcc_m.sum_e4 = g_xrcc_m.sum_d4 - g_xrcc_m.sum_e4

   LET g_xrcc_m.sum_b1 = g_xrcc_m.sum_b1 - g_xrcc_m.sum_c1
