  <section id="axrt300_01.b_fill" ver="6" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION axrt300_01_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2    STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE xrccdocno,xrccld,xrccseq,xrcc001,xrcc002,xrcc003,xrcc004,xrcc100,xrcc101, 
       xrcc105,xrcc106,xrcc107,xrcc104,xrcc108,xrcc115,xrcc116,xrcc117,xrcc114,xrcc118,xrcc001,xrccdocno, 
       xrccld,xrccseq,xrcc108,xrcc118,xrcc120,xrcc121,xrcc125,xrcc126,xrcc127,xrcc124,xrcc128,xrcc130, 
       xrcc131,xrcc135,xrcc136,xrcc137,xrcc134,xrcc138  FROM xrcc_t",
               "",
               
               " WHERE xrccent= ? AND 1=1 AND ", p_wc2 
    
   LET g_sql = g_sql, cl_sql_add_filter("xrcc_t"),
                      " ORDER BY xrcc_t.xrccld,xrcc_t.xrccdocno,xrcc_t.xrccseq,xrcc_t.xrcc001"
   
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE axrt300_01_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR axrt300_01_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_xrcc_d.clear()
   CALL g_xrcc2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_xrcc_d[l_ac].xrccdocno,g_xrcc_d[l_ac].xrccld,g_xrcc_d[l_ac].xrccseq,g_xrcc_d[l_ac].xrcc001, 
       g_xrcc_d[l_ac].xrcc002,g_xrcc_d[l_ac].xrcc003,g_xrcc_d[l_ac].xrcc004,g_xrcc_d[l_ac].xrcc100,g_xrcc_d[l_ac].xrcc101, 
       g_xrcc_d[l_ac].xrcc105,g_xrcc_d[l_ac].xrcc106,g_xrcc_d[l_ac].xrcc107,g_xrcc_d[l_ac].xrcc104,g_xrcc_d[l_ac].xrcc108, 
       g_xrcc_d[l_ac].xrcc115,g_xrcc_d[l_ac].xrcc116,g_xrcc_d[l_ac].xrcc117,g_xrcc_d[l_ac].xrcc114,g_xrcc_d[l_ac].xrcc118, 
       g_xrcc2_d[l_ac].xrcc001,g_xrcc2_d[l_ac].xrccdocno,g_xrcc2_d[l_ac].xrccld,g_xrcc2_d[l_ac].xrccseq, 
       g_xrcc2_d[l_ac].xrcc108,g_xrcc2_d[l_ac].xrcc118,g_xrcc2_d[l_ac].xrcc120,g_xrcc2_d[l_ac].xrcc121, 
       g_xrcc2_d[l_ac].xrcc125,g_xrcc2_d[l_ac].xrcc126,g_xrcc2_d[l_ac].xrcc127,g_xrcc2_d[l_ac].xrcc124, 
       g_xrcc2_d[l_ac].xrcc128,g_xrcc2_d[l_ac].xrcc130,g_xrcc2_d[l_ac].xrcc131,g_xrcc2_d[l_ac].xrcc135, 
       g_xrcc2_d[l_ac].xrcc136,g_xrcc2_d[l_ac].xrcc137,g_xrcc2_d[l_ac].xrcc134,g_xrcc2_d[l_ac].xrcc138 

      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL axrt300_01_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec THEN
      IF g_error_show = 1 THEN
         CALL cl_err("xrcc_t",9035,1)
      END IF
   END IF 
   LET g_error_show = 0
   
 
   
   CALL g_xrcc_d.deleteElement(g_xrcc_d.getLength())   
   CALL g_xrcc2_d.deleteElement(g_xrcc2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_xrcc_d.getLength()
      LET g_xrcc2_d[l_ac].xrccld = g_xrcc_d[l_ac].xrccld 
      LET g_xrcc2_d[l_ac].xrccdocno = g_xrcc_d[l_ac].xrccdocno 
      LET g_xrcc2_d[l_ac].xrccseq = g_xrcc_d[l_ac].xrccseq 
      LET g_xrcc2_d[l_ac].xrcc001 = g_xrcc_d[l_ac].xrcc001 
 
   END FOR
   
   IF g_cnt > g_xrcc_d.getLength() THEN
      LET l_ac = g_xrcc_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_xrcc_d.getLength()
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
