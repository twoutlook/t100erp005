  <point name="function.axrt300_01_set_visible" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#欄位隱藏
PRIVATE FUNCTION axrt300_01_set_visible()
DEFINE  l_glaa015      LIKE glaa_t.glaa015
DEFINE  l_glaa019      LIKE glaa_t.glaa019

   SELECT glaa015,glaa019 INTO l_glaa015,l_glaa019 FROM glaa_t
    WHERE glaaent = g_enterprise AND glaald = g_no
    
   
   IF l_glaa015 = 'Y' THEN
      CALL cl_set_comp_visible('lbl_xrca120,xrca120,xrca121,sum_a3,sum_b3,sum_c3,sum_d3,sum_e3',TRUE)
   ELSE 
      CALL cl_set_comp_visible('lbl_xrca120,xrca120,xrca121,sum_a3,sum_b3,sum_c3,sum_d3,sum_e3',FALSE)
   END IF   
   
   IF l_glaa019 = 'Y' THEN
      CALL cl_set_comp_visible('lbl_xrca130,xrca130,xrca131,sum_a4,sum_b4,sum_c4,sum_d4,sum_e4',TRUE)
   ELSE 
      CALL cl_set_comp_visible('lbl_xrca130,xrca130,xrca131,sum_a4,sum_b4,sum_c4,sum_d4,sum_e4',FALSE)
   END IF  


END FUNCTION]]>
