LET g_xrca_m.xrcamoddt = cl_get_current()
LET g_xrca_m.xrcamodid_desc = cl_get_username(g_xrca_m.xrcamodid)
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      CALL axrt380_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE xrca_t SET (xrcamodid,xrcamoddt) = (g_xrca_m.xrcamodid,g_xrca_m.xrcamoddt)
          WHERE xrcaent = g_enterprise AND xrcald = g_xrcald_t
            AND xrcadocno = g_xrcadocno_t
 
      END IF
    
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_xrca_m.* = g_xrca_m_t.*
         CALL axrt380_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_xrca_m.xrcald != g_xrcald_t 
      OR g_xrca_m.xrcadocno != g_xrcadocno_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE xrcb_t SET xrcbld = g_xrca_m.xrcald
                                       ,xrcbdocno = g_xrca_m.xrcadocno
 
          WHERE xrcbent = g_enterprise AND xrcbld = g_xrcald_t
            AND xrcbdocno = g_xrcadocno_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "xrcb_t" 
               LET g_errparam.code   = "std-00009" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "xrcb_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
         #更新單身key值
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update2" mark="Y"/>}
         #end add-point
         
         UPDATE xrcd_t
            SET xrcdld = g_xrca_m.xrcald
               ,xrcddocno = g_xrca_m.xrcadocno
 
          WHERE xrcdent = g_enterprise AND
                xrcdld = g_xrcald_t
            AND xrcddocno = g_xrcadocno_t
 
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update2"/>}
         #end add-point
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "xrcd_t" 
               LET g_errparam.code   = "std-00009" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "xrcd_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update2"/>}
         #end add-point
 
 
         
 
         
         #UPDATE 多語言table key值
         
         
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL axrt380_set_act_visible()   
   CALL axrt380_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " xrcaent = '" ||g_enterprise|| "' AND",
                      " xrcald = '", g_xrca_m.xrcald, "' "
                      ," AND xrcadocno = '", g_xrca_m.xrcadocno, "' "
 
   #填到對應位置
   CALL axrt380_browser_fill("")
 
   CLOSE axrt380_cl
   
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL axrt380_msgcentre_notify('modify')
 
END FUNCTION   
]]>
