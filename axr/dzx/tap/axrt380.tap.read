            IF NOT cl_null(g_xrca_m.xrca059) THEN
               INITIALIZE g_chkparam.* TO NULL
               LET g_chkparam.arg1 = g_xrca_m.xrca059
               LET g_chkparam.arg2 = ' '
               IF NOT cl_chk_exist("v_bgaa001") THEN
