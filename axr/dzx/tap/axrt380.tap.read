  <point name="function.axrt380_update_rtia" order="39" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[# 回寫卡券立賬單號
PRIVATE FUNCTION axrt380_update_rtia(p_type)
   DEFINE p_type        LIKE type_t.chr1    #"+" :立賬,回寫立賬單號, "-" :取消立賬,清空立賬單號 
   DEFINE l_sql         STRING
   DEFINE l_xrcb008     LIKE xrcb_t.xrcb008
   DEFINE r_success     LIKE type_t.num5
   DEFINE l_rtia052     LIKE rtia_t.rtia052
   DEFINE l_gcao035     LIKE gcao_t.gcao035
   
   LET r_success = TRUE
   
   IF p_type = '+' THEN 
      LET l_rtia052 = g_xrca_m.xrcadocno
      LET l_gcao035 = 'Y'
   ELSE
      LET l_rtia052 = ''
      LET l_gcao035 = 'N'
   END IF      
      
   UPDATE rtia_t SET rtia052 = l_rtia052
    WHERE rtiaent = g_enterprise
      AND rtiadocno IN (SELECT DISTINCT xrcb008 FROM xrcb_t
                         WHERE xrcbent = g_enterprise
                           AND xrcbld = g_xrca_m.xrcald
                           AND xrcbdocno = g_xrca_m.xrcadocno
                       )
      
   IF SQLCA.SQLcode THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend = "upd rtia_t"
      LET g_errparam.popup = TRUE
      CALL cl_err()
   
      LET r_success = FALSE               
      RETURN r_success     
   END IF
         
   UPDATE gcao_t SET gcao035 = l_gcao035
    WHERE gcaoent = g_enterprise
      AND gcao001 IN (SELECT DISTINCT xrcb008 FROM xrcb_t
                       WHERE xrcbent = g_enterprise
                         AND xrcbld = g_xrca_m.xrcald
                         AND xrcbdocno = g_xrca_m.xrcadocno
                     )
      
   IF SQLCA.SQLcode THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend = "upd gcao_t"
      LET g_errparam.popup = TRUE
      CALL cl_err()
   
      LET r_success = FALSE 
      RETURN r_success           
   END IF
   
   RETURN r_success
END FUNCTION]]>
