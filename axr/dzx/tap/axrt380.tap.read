  <point name="function.axrt380_exrate" order="14" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL axrt380_exrate(p_ooan004,p_ooan002,p_ooan003,p_amount,p_tmp)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt380_exrate(p_ooan004,p_ooan002,p_ooan003,p_amount,p_tmp)
   DEFINE p_ooan004      LIKE ooan_t.ooan004
   DEFINE p_ooan002      LIKE ooan_t.ooan002
   DEFINE p_ooan003      LIKE ooan_t.ooan003
   DEFINE p_amount       LIKE ooan_t.ooan005
   DEFINE p_tmp          LIKE ooan_t.ooan005
   DEFINE l_ooef014      LIKE ooef_t.ooef014
   DEFINE l_ooaj004      LIKE ooaj_t.ooaj004
   DEFINE l_ooaj005      LIKE ooaj_t.ooaj005
   DEFINE l_ooan         RECORD LIKE ooan_t.*
   DEFINE l_conv         LIKE type_t.chr1
   DEFINE l_rate         LIKE ooan_t.ooan005
   DEFINE l_ooan001      LIKE ooan_t.ooan001

   SELECT ooef014 INTO l_ooef014 FROM ooef_t
    WHERE ooefent = g_enterprise AND ooef001 = g_glaa.glaacomp

   #1.取基础币种的金额精度--若有传入p_amount时,返回的是金额,非汇率
   CALL s_curr_sel_ooaj004(l_ooef014,p_ooan003)
        RETURNING l_ooaj004

   #2.取基础币种的汇率精度
   CALL s_curr_sel_ooaj005(l_ooef014,p_ooan003)
        RETURNING l_ooaj005

   #3.取汇率 & 汇率方向
   LET l_conv = '1'  #交易币种对基础币种
   SELECT ooan_t.* INTO l_ooan.*
     FROM ooan_t,ooam_t
    WHERE ooanent = g_enterprise
      AND ooan001 = g_glaa.glaa002   #汇率参照表号
      AND ooan002 = p_ooan002   #交易币种
      AND ooan003 = p_ooan003   #基础币种
      AND ooan004 = p_ooan004   #日期
      AND ooament = ooanent
      AND ooam001 = ooan001
      AND ooam003 = ooan003
      AND ooam004 = ooan004
      AND ooamstus = 'Y'
   IF SQLCA.sqlcode THEN
      #交易币种对基础币种的关系不存在时,反向查找
      SELECT ooan_t.* INTO l_ooan.*
        FROM ooan_t,ooam_t
       WHERE ooanent = g_enterprise
         AND ooan001 = g_glaa.glaa002   #汇率参照表号
         AND ooan002 = p_ooan003   #基础币种
         AND ooan003 = p_ooan002   #交易币种
         AND ooan004 = p_ooan004
         AND ooament = ooanent
         AND ooam001 = ooan001
         AND ooam003 = ooan003
         AND ooam004 = ooan004
         AND ooamstus = 'Y'
      IF NOT SQLCA.sqlcode THEN
         LET l_conv = '2'   #基础币种对交易币种
      END IF
   END IF

   #交易币种批量
   IF cl_null(l_ooan.ooan012) THEN LET l_ooan.ooan012 = 1 END IF
   
   #4.计算汇率
   #减少处理步骤,以便精确度降低
   IF l_conv = '1' THEN  #存在交易对基础币种的置换关系
      IF l_ooan.ooan013 = '1' OR cl_null(l_ooan.ooan013) THEN   #存在正向的汇率关系
         LET l_rate = p_tmp / l_ooan.ooan012 * p_amount
      ELSE               #若为反向时,要1除取得的汇率
         LET l_rate = 1 / p_tmp * l_ooan.ooan012 * p_amount
      END IF
   ELSE                  #存在基础对交易币种的转换关系
      IF l_ooan.ooan013 = '1' THEN
         LET l_rate = 1 / p_tmp * l_ooan.ooan012 * p_amount
      ELSE
         LET l_rate = p_tmp / l_ooan.ooan012 * p_amount
      END IF
   END IF

   #5.按精度进位小数取位
   IF p_amount > 1 THEN
      #传入的为金额,直接按ooaj004取位
      CALL s_num_round('1',l_rate,l_ooaj004) RETURNING l_rate
   ELSE
      #没有传入金额,根据汇率的精度进行取位
      CALL s_num_round('1',l_rate,l_ooaj005) RETURNING l_rate
   END IF

   RETURN l_rate

END FUNCTION]]>
  </point>
  <point name="function.axrt380_amount_refresh" order="15" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 離開單頭後,判斷單頭金額是否有異動.
#                  若單頭金額與單身金額不符,詢問是否
#                  將差異分攤至單身
# Memo...........:
# Usage..........: CALL axrt380_amount_refresh()
#                  RETURNING --
# Input parameter: 
# Return code....: 
# Date & Author..: 2014/03/14 By zhangwei
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt380_amount_refresh(p_flag)
   DEFINE p_flag                LIKE type_t.chr1
   DEFINE l_count               LIKE type_t.num5
   DEFINE l_chr                 LIKE type_t.chr1
   DEFINE l_xrcb                RECORD LIKE xrcb_t.*
   DEFINE l_sql                 STRING
   DEFINE l_xrcd009             LIKE xrcd_t.xrcd009
   DEFINE l_success             LIKE type_t.num5

   LET l_success = TRUE

   IF cl_null(p_flag) THEN LET p_flag = 'N' END IF

   #STEP1.判斷單身是否有資料,若無資料則不做差異判斷
   #STEP2.判斷單頭收款條件、稅別、匯率是否有做修改
   #STEP2.重新產生單身資料、重新產生沖暫估資料
   #STEP3.重新產生單頭金額
   #STEP4.重新展算多帳期

   #STEP1
   SELECT COUNT(*) INTO l_count FROM xrcb_t WHERE xrcbent = g_enterprise AND xrcbdocno = g_xrca_m.xrcadocno AND xrcbld = g_xrca_m.xrcald
   
   IF l_count < 1 THEN RETURN END IF

   #STEP2
   #稅別修改　　：重產單身、重產沖暫估、回寫單頭、重產多帳期
   #匯率修改　　：重產單身、重產沖暫估、回寫單頭、重產多帳期
   #收款條件修改：重產多帳期

   LET l_chr = 'N'
   IF g_xrca_m.xrca101 <> g_xrca_m_t.xrca101 OR g_xrca_m_t.xrca101  IS NULL THEN LET l_chr = 'Y' END IF
   IF (g_xrca_m.xrca121 <> g_xrca_m_t.xrca121 OR g_xrca_m_t.xrca121 IS NULL) AND g_glaa.glaa015 = 'Y' THEN LET l_chr = 'Y' END IF
   IF (g_xrca_m.xrca131 <> g_xrca_m_t.xrca131 OR g_xrca_m_t.xrca131 IS NULL) AND g_glaa.glaa019 = 'Y' THEN LET l_chr = 'Y' END IF

   IF p_flag = 'Y' THEN
      UPDATE xrcb_t SET xrcb020 = g_xrca_m.xrca011
       WHERE xrcbent   = g_enterprise
         AND xrcbdocno = g_xrca_m.xrcadocno
         AND xrcbld    = g_xrca_m.xrcald
      IF SQLCA.SQLCODE OR SQLCA.SQLERRD[3] < 1 THEN
          CALL s_transaction_end('N','0')
          INITIALIZE g_errparam TO NULL 
          LET g_errparam.extend = "" 
          LET g_errparam.code   = "axr-00242" 
          LET g_errparam.popup  = TRUE 
          CALL cl_err()
          RETURN
      END IF
   END IF

   LET l_sql = "SELECT * FROM xrcb_t WHERE xrcbent = '",g_enterprise,"' AND xrcbdocno = '",g_xrca_m.xrcadocno,"' AND xrcbld = '",g_xrca_m.xrcald,"'"
   PREPARE axrt300_xrcb_prep2 FROM l_sql
   DECLARE axrt300_xrcb_curs2 CURSOR FOR axrt300_xrcb_prep2

   IF p_flag = 'Y' OR l_chr = 'Y' THEN
      #刪除沖暫估信息
      DELETE FROM xrcf_t WHERE xrcfent = g_enterprise
         AND xrcfdocno = g_xrca_m.xrcadocno
         AND xrcfld    = g_xrca_m.xrcald

      FOREACH axrt300_xrcb_curs2 INTO l_xrcb.*
         #更新單身本幣金額
         CALL s_tax_ins(g_xrca_m.xrcadocno,l_xrcb.xrcbseq,0,g_glaa.glaacomp,
                        l_xrcb.xrcb007*l_xrcb.xrcb101,l_xrcb.xrcb020,
                        l_xrcb.xrcb007,g_xrca_m.xrca100,g_xrca_m.xrca101,g_xrca_m.xrcald,g_xrca_m.xrca121,g_xrca_m.xrca131)
            RETURNING l_xrcb.xrcb103,l_xrcb.xrcb104,l_xrcb.xrcb105,
                      l_xrcb.xrcb113,l_xrcb.xrcb114,l_xrcb.xrcb115,
                      l_xrcb.xrcb123,l_xrcb.xrcb124,l_xrcb.xrcb125,
                      l_xrcb.xrcb133,l_xrcb.xrcb134,l_xrcb.xrcb135
         SELECT glab005 INTO l_xrcd009 FROM glab_t 
          WHERE glabld = g_xrca_m.xrcald 
            AND glabent = g_enterprise
            AND glab002 = g_xrca_m.xrca007   # 帳款類別
            AND glab001 = '13'             # 應收帳務類型科目設定
            AND glab003 = '8304_29'
         UPDATE xrcd_t SET xrcd009 = l_xrcd009
          WHERE xrcd009 IS NULL
            AND xrcdent = g_enterprise
            AND xrcddocno = g_xrca_m.xrcadocno
            AND xrcdld = g_xrca_m.xrcald
         UPDATE xrcb_t SET xrcb113 = l_xrcb.xrcb113,
                           xrcb114 = l_xrcb.xrcb114,
                           xrcb115 = l_xrcb.xrcb115,
                           xrcb123 = l_xrcb.xrcb123,
                           xrcb124 = l_xrcb.xrcb124,
                           xrcb125 = l_xrcb.xrcb125,
                           xrcb133 = l_xrcb.xrcb133,
                           xrcb134 = l_xrcb.xrcb134,
                           xrcb135 = l_xrcb.xrcb135,
                           xrcb023 = l_xrcb.xrcb023
          WHERE xrcbent   = g_enterprise
            AND xrcbdocno = g_xrca_m.xrcadocno
            AND xrcbld    = g_xrca_m.xrcald
            AND xrcbseq   = l_xrcb.xrcbseq
         IF SQLCA.SQLCODE OR SQLCA.SQLERRD[3] < 1 THEN
             LET l_success = FALSE
             INITIALIZE g_errparam TO NULL 
             LET g_errparam.extend = "" 
             LET g_errparam.code   = "axr-00243" 
             LET g_errparam.popup  = TRUE 
             CALL cl_err()
             EXIT FOREACH
         END IF
         CALL s_axrp130_ins_xrcf(l_xrcb.xrcbld,l_xrcb.xrcbdocno,l_xrcb.xrcbseq,'Y') RETURNING l_xrcb.xrcb023   #重寫沖暫估信息
         UPDATE xrcb_t SET xrcb023 = l_xrcb.xrcb023
          WHERE xrcbent   = g_enterprise
            AND xrcbdocno = g_xrca_m.xrcadocno
            AND xrcbld    = g_xrca_m.xrcald
            AND xrcbseq   = l_xrcb.xrcbseq
      END FOREACH
      IF NOT l_success THEN
         CALL s_transaction_end('N','0')
         RETURN
      END IF
      #重新產生多帳期
      CALL s_axrt300_installments(g_xrca_m.xrcald,g_xrca_m.xrcadocno) RETURNING l_success
      IF l_success THEN
         CALL s_transaction_end('Y','0')
      ELSE
         CALL s_transaction_end('N','0')
      END IF
      RETURN
   END IF

   IF g_xrca_m.xrca008 <> g_xrca_m_t.xrca008 OR g_xrca_m_t.xrca008 IS NULL THEN
      #重新產生多帳期
      CALL s_axrt300_installments(g_xrca_m.xrcald,g_xrca_m.xrcadocno) RETURNING l_success
      IF l_success THEN
         CALL s_transaction_end('Y','0')
      ELSE
         CALL s_transaction_end('N','0')
      END IF
      RETURN
   END IF

END FUNCTION]]>
  </point>
  <point name="function.axrt380_xrcd_refresh" order="16" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 修改交易稅額後,重新計算單身、多帳期、單頭金額
# Memo...........:
# Usage..........: CALL s_aooi150_ins (传入参数)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt380_xrcd_refresh()
   DEFINE l_sql            STRING
   DEFINE l_chr            LIKE type_t.chr1
   DEFINE l_xrcbseq        LIKE xrcb_t.xrcbseq
   DEFINE l_xrcbseq_t      LIKE xrcb_t.xrcbseq
   DEFINE l_xrcb020        LIKE xrcb_t.xrcb020
   DEFINE l_xrcb104        LIKE xrcb_t.xrcb104
   DEFINE l_xrcd103        LIKE xrcd_t.xrcd103
   DEFINE l_xrcd104        LIKE xrcd_t.xrcd104
   DEFINE l_xrcd105        LIKE xrcd_t.xrcd105
   DEFINE l_success        LIKE type_t.num5
   DEFINE l_oodbl004       LIKE oodbl_t.oodbl004
   DEFINE l_oodb005        LIKE oodb_t.oodb005
   DEFINE l_oodb006        LIKE oodb_t.oodb006
   DEFINE l_oodb011        LIKE oodb_t.oodb011
   DEFINE l_xrcb103        LIKE xrcb_t.xrcb103
   DEFINE l_xrcb105        LIKE xrcb_t.xrcb105
   DEFINE l_xrcb113        LIKE xrcb_t.xrcb113
   DEFINE l_xrcb114        LIKE xrcb_t.xrcb114
   DEFINE l_xrcb115        LIKE xrcb_t.xrcb115
   DEFINE l_xrcb123        LIKE xrcb_t.xrcb123
   DEFINE l_xrcb124        LIKE xrcb_t.xrcb124
   DEFINE l_xrcb125        LIKE xrcb_t.xrcb125
   DEFINE l_xrcb133        LIKE xrcb_t.xrcb133
   DEFINE l_xrcb134        LIKE xrcb_t.xrcb134
   DEFINE l_xrcb135        LIKE xrcb_t.xrcb135
   
   CALL s_transaction_begin()
  
   LET l_sql = "SELECT xrcbseq,xrcb020,xrcb103,xrcb105,xrcb104,xrcd103,xrcd104,xrcd105 FROM xrcb_t,xrcd_t WHERE xrcbent = xrcdent ",
               "   AND xrcbdocno = xrcddocno",
               "   AND xrcbseq = xrcdseq",
               "   AND xrcdseq2 = 0",
               "   AND xrcbent = '",g_enterprise,"'",
               "   AND xrcbdocno = '",g_xrca_m.xrcadocno,"'",
               "   AND xrcbld = '",g_xrca_m.xrcald,"'"
   PREPARE axrt380_prep3 FROM l_sql
   DECLARE axrt380_curs3 CURSOR FOR axrt380_prep3
   
   LET l_chr = 'N'

   LET l_xrcbseq_t = 0

   FOREACH axrt380_curs3 INTO l_xrcbseq,l_xrcb020,l_xrcb103,l_xrcb105,l_xrcb104,l_xrcd103,l_xrcd104,l_xrcd105
      IF l_xrcb104 = l_xrcd104 THEN CONTINUE FOREACH END IF
      CALL s_tax_chk(g_glaa.glaacomp,l_xrcb020) RETURNING l_success,l_oodbl004,l_oodb005,l_oodb006,l_oodb011

      IF l_xrcbseq_t <> 0 AND l_xrcbseq_t <> l_xrcbseq THEN
         LET l_xrcbseq_t = 0
      END IF

      IF l_xrcbseq_t = 0 THEN
         UPDATE xrcb_t SET xrcb103 = 0,
                           xrcb104 = 0,
                           xrcb105 = 0,
                           xrcb113 = 0,
                           xrcb114 = 0,
                           xrcb115 = 0,
                           xrcb123 = 0,
                           xrcb124 = 0,
                           xrcb125 = 0,
                           xrcb133 = 0,
                           xrcb134 = 0,
                           xrcb135 = 0
          WHERE xrcbent = g_enterprise
            AND xrcbdocno = g_xrca_m.xrcadocno
            AND xrcbld = g_xrca_m.xrcald
            AND xrcbseq = l_xrcbseq
      END IF

      LET l_xrcbseq_t = l_xrcbseq

      LET l_xrcb103 = l_xrcd103
      LET l_xrcb104 = l_xrcd104
      LET l_xrcb105 = l_xrcd105

      CALL axrt380_exrate(g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa001,l_xrcb103,g_xrca_m.xrca101)
         RETURNING l_xrcb113
      CALL axrt380_exrate(g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa001,l_xrcb104,g_xrca_m.xrca101)
         RETURNING l_xrcb114
      LET l_xrcb115 = l_xrcb113 + l_xrcb114
   
      IF g_glaa.glaa015 = 'Y' THEN
         IF g_glaa.glaa017 = '1' THEN
            #换算基准:交易原幣
            CALL axrt380_exrate(g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa016,l_xrcb103,g_xrca_m.xrca121)
               RETURNING l_xrcb123
            CALL axrt380_exrate(g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa016,l_xrcb104,g_xrca_m.xrca121)
               RETURNING l_xrcb124
         ELSE
            #换算基准:帳簿幣別
            CALL axrt380_exrate(g_xrca_m.xrcadocdt,g_glaa.glaa001,g_glaa.glaa016,l_xrcb113,g_xrca_m.xrca121)
               RETURNING l_xrcb123
            CALL axrt380_exrate(g_xrca_m.xrcadocdt,g_glaa.glaa001,g_glaa.glaa016,l_xrcb114,g_xrca_m.xrca121)
               RETURNING l_xrcb124
         END IF
         LET l_xrcb125 = l_xrcb123 + l_xrcb124
      ELSE
         LET l_xrcb123 = 0 LET l_xrcb124 = 0 LET l_xrcb125 = 0
      END IF   
   
      IF g_glaa.glaa019 = 'Y' THEN
         #計算本位幣三金額
         IF g_glaa.glaa021 = '1' THEN
            #换算基准:交易原幣
            CALL axrt380_exrate(g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa020,l_xrcb103,g_xrca_m.xrca131)
               RETURNING l_xrcb133
            CALL axrt380_exrate(g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa020,l_xrcb104,g_xrca_m.xrca131)
               RETURNING l_xrcb134
         ELSE
            #换算基准:帳簿幣別
            CALL axrt380_exrate(g_xrca_m.xrcadocdt,g_glaa.glaa001,g_glaa.glaa020,l_xrcb113,g_xrca_m.xrca131)
               RETURNING l_xrcb133
            CALL axrt380_exrate(g_xrca_m.xrcadocdt,g_glaa.glaa001,g_glaa.glaa020,l_xrcb114,g_xrca_m.xrca131)
               RETURNING l_xrcb134
         END IF
         LET l_xrcb135 = l_xrcb133 + l_xrcb134
      ELSE
         LET l_xrcb133 = 0 LET l_xrcb134 = 0 LET l_xrcb135 = 0
      END IF
   
      UPDATE xrcb_t SET xrcb103 = xrcb103 + l_xrcb103,
                        xrcb104 = xrcb104 + l_xrcb104,
                        xrcb105 = xrcb105 + l_xrcb105,
                        xrcb113 = xrcb113 + l_xrcb113,
                        xrcb114 = xrcb114 + l_xrcb114,
                        xrcb115 = xrcb115 + l_xrcb115,
                        xrcb123 = xrcb123 + l_xrcb123,
                        xrcb124 = xrcb124 + l_xrcb124,
                        xrcb125 = xrcb125 + l_xrcb125,
                        xrcb133 = xrcb133 + l_xrcb133,
                        xrcb134 = xrcb134 + l_xrcb134,
                        xrcb135 = xrcb135 + l_xrcb135
       WHERE xrcbent = g_enterprise
         AND xrcbdocno = g_xrca_m.xrcadocno
         AND xrcbld = g_xrca_m.xrcald
         AND xrcbseq = l_xrcbseq
   
      LET l_chr = 'Y'
   
   END FOREACH
   
   IF l_chr = 'Y' THEN
      CALL s_axrt300_installments(g_xrca_m.xrcald,g_xrca_m.xrcadocno) RETURNING l_success

      CALL axrt380_xrca_upd()
   END IF
   CALL s_transaction_end('Y','0')

END FUNCTION]]>
