            CALL q_oocq002()                                   #呼叫開窗
            LET g_xrca_m.xrca007 = g_qryparam.return1          #將開窗取得的值回傳到變數
            DISPLAY g_xrca_m.xrca007 TO xrca007                #顯示到畫面上
            CALL axrt380_xrca_ref('xrca007')
            NEXT FIELD xrca007                                 #返回原欄位

         ON ACTION controlp INFIELD xrca035        
            #開窗i段
			   INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'i'
			   LET g_qryparam.reqry = FALSE
            LET g_qryparam.default1 = g_xrca_m.xrca035         #給予default值
            LET g_qryparam.where = " glac001 = '",g_glaa.glaa004,"' AND glac003 <> 1 AND glac006 = '1'"
            #給予arg
            CALL aglt310_04()                                  #呼叫開窗
            LET g_xrca_m.xrca035 = g_qryparam.return1          #將開窗取得的值回傳到變數
            DISPLAY g_xrca_m.xrca035 TO xrca035                #顯示到畫面上
            CALL axrt380_xrca_ref('xrca035')
            NEXT FIELD xrca035                                 #返回原欄位

         ON ACTION controlp INFIELD xrca036        
            #開窗i段
		   	INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'i'
			   LET g_qryparam.reqry = FALSE
            LET g_qryparam.default1 = g_xrca_m.xrca036         #給予default值
            LET g_qryparam.where = " glac001 = '",g_glaa.glaa004,"' AND glac003 <> 1 AND glac006 = '1'"
            #給予arg
            CALL aglt310_04()                                  #呼叫開窗
            LET g_xrca_m.xrca036 = g_qryparam.return1          #將開窗取得的值回傳到變數
            DISPLAY g_xrca_m.xrca036 TO xrca036                #顯示到畫面上
