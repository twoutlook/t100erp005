PRIVATE FUNCTION axrt380_acc_chk()

   IF cl_null(g_xrca_m.xrcadocno) OR cl_null(g_xrca_m.xrcald) THEN RETURN END IF

   INPUT BY NAME g_xrca_m.xrca007,g_xrca_m.xrca035,g_xrca_m.xrca036,g_xrca_m.xrca059,g_xrca_m.xrca039,g_xrca_m.xrca056
      ATTRIBUTE(WITHOUT DEFAULTS)

         AFTER FIELD xrca007
            LET g_xrca_m.xrca007_desc = ' '
            DISPLAY BY NAME g_xrca_m.xrca007_desc
            IF NOT cl_null(g_xrca_m.xrca007) THEN
               INITIALIZE g_chkparam.* TO NULL
               LET g_chkparam.arg1 = g_xrca_m.xrca007
               IF NOT cl_chk_exist("v_oocq002_3111") THEN
                  LET g_xrca_m.xrca007 = g_xrca_m_t.xrca007
