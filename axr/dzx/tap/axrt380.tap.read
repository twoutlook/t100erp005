                  CALL axrt380_xrca_ref('xrca059')
                  NEXT FIELD CURRENT
               END IF
            END IF
            CALL axrt380_xrca_ref('xrca059')

         ON ACTION controlp INFIELD xrca007
            #開窗i段
			   INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'i'
			   LET g_qryparam.reqry = FALSE
            LET g_qryparam.default1 = g_xrca_m.xrca007         #給予default值
            #給予arg
