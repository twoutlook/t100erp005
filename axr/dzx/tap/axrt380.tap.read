  <point name="function.axrt380_ins_default" order="3" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 新增時,資料初始化
# Memo...........:
# Usage..........: CALL s_aooi150_ins (传入参数)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt380_ins_default(p_lab)
   DEFINE p_lab      LIKE type_t.chr20
   DEFINE l_sql      STRING
   DEFINE l_success  LIKE type_t.chr1
   DEFINE l_cnt      LIKE type_t.num5

   IF p_lab = 'xrca' THEN
      #帳務中心
      #取得預設的帳務中心,因新增階段的時候,並不會知道site,所以以登入人員做為依據
      CALL s_fin_get_account_center('',g_user,'3',g_xrca_m.xrcadocdt) RETURNING g_sub_success,g_xrca_m.xrcasite,g_errno
      CALL s_desc_get_department_desc(g_xrca_m.xrcasite) RETURNING g_xrca_m.xrcasite_desc
      #g_bookno,先做帳務中心及g_bookno的勾稽,登入人員帳務人員時,基本上會有該g_bookno的帳務中心權限
      IF NOT cl_null(g_bookno) THEN
         #帳務中心與帳別勾稽
         CALL s_fin_account_center_with_ld_chk(g_xrca_m.xrcasite,g_bookno,g_user,'3','N','',g_xrca_m.xrcadocdt) RETURNING g_sub_success,g_errno
      END IF
      #該帳務中心與帳別無法勾稽到,以登入人員g_user取得預設帳別/法人
      IF NOT g_sub_success OR cl_null(g_bookno) THEN
         CALL s_fin_ld_carry('',g_user) RETURNING g_sub_success,g_xrca_m.xrcald,g_xrca_m.xrcacomp,g_errno
         CALL s_fin_account_center_with_ld_chk(g_xrca_m.xrcasite,g_xrca_m.xrcald,g_user,'3','N','',g_xrca_m.xrcadocdt) RETURNING g_sub_success,g_errno
      ELSE
         LET g_xrca_m.xrcald = g_bookno
      END IF
       
      #若取不出資料,則不預設帳別
      IF NOT g_sub_success THEN
         LET g_xrca_m.xrcald   = ''
         LET g_xrca_m.xrcacomp = ''
      END IF      
      LET g_xrca_m.xrca003   = g_user        #帳務人員
      SELECT * INTO g_glaa.* FROM glaa_t WHERE glaaent = g_enterprise AND glaald = g_xrca_m.xrcald
      IF cl_null(g_xrca_m.xrcasite) THEN
         LET g_xrca_m.xrca003 = NULL
      END IF
      CALL axrt380_xrca_ref('xrcasite')
      CALL axrt380_xrca_ref('xrcald')
      CALL axrt380_xrca_ref('xrca003')
      
      LET g_xrca_m.xrca001 = '142'      #帳款單性質
      LET g_xrca_m.xrcadocdt = g_today #立帳日期
      
      DISPLAY BY NAME g_xrca_m.xrcasite,g_xrca_m.xrca003,g_xrca_m.xrcald,g_xrca_m.xrca001,g_xrca_m.xrcadocdt
      
      CALL axrt380_xrca_dis('a')
      CALL axrt380_othercurr_dis()

   ELSE
   END IF
END FUNCTION]]>
  </point>
  <point name="function.axrt380_chk_xrdasite" order="4" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 帳務中心、帳務人員、帳套 聯動檢查
# Memo...........:
# Usage..........: CALL axrt380_chk_xrdasite(p_site,p_user,p_ld)
#                  RETURNING g_errno
# Input parameter: site           帳務中心
#                : p_user         人員
#                : p_ld           帳套
# Return code....: g_errno        報錯訊息
# Date & Author..: 2014/03/05 By zhangweib
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt380_chk_xrdasite(p_site,p_user,p_ld)
   DEFINE p_site      LIKE xrca_t.xrcasite
   DEFINE p_user      LIKE xrca_t.xrca003
   DEFINE p_ld        LIKE xrca_t.xrcald
   DEFINE l_xrahstus  LIKE xrah_t.xrahstus
   DEFINE l_xrah007   LIKE xrah_t.xrah007
   DEFINE l_ooagstus  LIKE ooag_t.ooagstus
   DEFINE l_cnt       LIKE type_t.num5
   DEFINE l_success   LIKE type_t.num5
   
   LET g_errno = ' '
   
   IF cl_null(p_site) THEN RETURN END IF
   IF cl_null(p_ld)   THEN RETURN END IF
   
   #帳套下屬於法人否檢查
    SELECT COUNT(*) INTO l_cnt FROM glaa_t,ooef_t,xrah_t 
    WHERE glaald   = p_ld
      AND glaacomp = ooef017 
      AND glaaent  = ooefent 
      AND ooef001  = xrah004 
      AND ooefent  = xrahent 
      AND xrah002  = p_site
      AND xrah001  = 1
      
   IF l_cnt < 1 OR cl_null(l_cnt) THEN
      LET g_errno = 'axr-00089' RETURN
   END IF
   
   IF cl_null(p_user) THEN RETURN g_errno END IF
   
   #人員對應帳套使用權限檢查
   IF NOT cl_null(p_ld) AND NOT cl_null(p_user) THEN
      CALL s_ld_chk_authorization(p_user,p_ld) 
         RETURNING l_success
      IF l_success = FALSE THEN
         LET g_errno = 'axr-00022' RETURN
      END IF
   END IF

END FUNCTION]]>
  </point>
  <point name="function.axrt380_xrca004_ref" order="5" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 預設交易資料
# Memo...........:
# Usage..........: CALL s_aooi150_ins (传入参数)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
