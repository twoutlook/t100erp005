    <![CDATA[# 整批產生單身
PRIVATE FUNCTION axrt380_p_ins_xrcb()
   DEFINE l_sel         LIKE type_t.chr1
   DEFINE l_sql         STRING
   DEFINE l_success     LIKE type_t.num5
   DEFINE r_success     LIKE type_t.num5
   DEFINE l_glab005_1   LIKE glab_t.glab005
   DEFINE l_glab005_2   LIKE glab_t.glab005
   DEFINE l_ooef019     LIKE ooef_t.ooef019
   DEFINE ls_wc         STRING
   DEFINE l_xrcb        RECORD LIKE xrcb_t.*
   DEFINE l_tmp         RECORD 
                        rtiadocno       LIKE rtia_t.rtiadocno,
                        rtia000         LIKE rtia_t.rtia000,
                        rtib004         LIKE rtib_t.rtib004,
                        rtib012         LIKE rtib_t.rtib012,
                        rtib006         LIKE rtib_t.rtib006,
                        rtib010         LIKE rtib_t.rtib010,
                        rtib013         LIKE rtib_t.rtib013,
                        rtib020         LIKE rtib_t.rtib020,
                        rtib021         LIKE rtib_t.rtib021
                        END RECORD
   
   LET l_success = TRUE
   
   CALL s_transaction_begin()
   CALL cl_err_collect_init()
   
   CALL axrt380_01() RETURNING l_sel
   IF cl_null(l_sel) THEN 
      RETURN
   END IF
   
   #取得帳務組織管轄的營運據點   
   CALL s_fin_account_center_comp_str() RETURNING ls_wc
   #將取回的字串轉換為SQL條件
   CALL axrt380_get_ooef001_wc(ls_wc) RETURNING ls_wc  
   
   SELECT ooef019 INTO l_ooef019
     FROM ooef_t
    WHERE ooefent = g_enterprise
      AND ooef001 = g_xrca_m.xrcacomp
      AND ooefstus = 'Y' 
   
   LET l_glab005_1 = NULL
   SELECT glab005 INTO l_glab005_1 FROM glab_t 
    WHERE glabld = g_xrca_m.xrcald 
      AND glabent = g_enterprise
      AND glab002 = g_xrca_m.xrca007 # 帳款類別
      AND glab001 = '13'             # 應收帳務類型科目設定
　　  AND glab003 = '8304_01'

   LET l_glab005_2 = NULL
   SELECT glab005 INTO l_glab005_2 FROM glab_t 
    WHERE glabld = g_xrca_m.xrcald 
      AND glabent = g_enterprise
      AND glab002 = g_xrca_m.xrca007 # 帳款類別
      AND glab001 = '13'             # 應收帳務類型科目設定
　　  AND glab003 = '8304_21'
   
   #卡券赠送
   LET l_sql = "SELECT rtiadocno,rtia000,rtib004,rtib012,rtib006,rtib010,rtib013,rtib020,rtib021 ",
               "  FROM rtia_t,rtib_t ",
               " WHERE rtiaent = rtibent ",
               "   AND rtiadocno = rtibdocno ",
               "   AND rtia000 IN ('agct405','ammt405') ",
               "   AND rtiastus = 'S' ",
               "   AND rtia050 = 'Y' ",
               "   AND rtiasite IN ",ls_wc,
               "   AND rtia052 IS NULL "
   
   PREPARE axrt380_p_pre FROM l_sql
   DECLARE axrt380_p_cs CURSOR FOR axrt380_p_pre
   
   #卡券注销
   LET l_sql = "SELECT rtiadocno,rtia000,rtib004,rtib012,rtib006,rtib010,rtib013,rtib020,rtib021 ",
               "  FROM rtia_t,rtib_t ",
               " WHERE rtiaent = rtibent ",
               "   AND rtiadocno = rtibdocno ",
               "   AND rtia000 = 'ammt426'",
               "   AND rtiastus = 'S' ",
               "   AND rtiasite IN ",ls_wc,
               "   AND rtia052 IS NULL ",
               " UNION ",
               "SELECT gcao001,'gcao',gcao001,1,'',gcao032,'',gcao032,gcao032 ",
               "  FROM gcao_t ",
               " WHERE gcaoent = '",g_enterprise,"'",
               "   AND gcao005 = '8' ",
               "   AND gcao023 IN ",ls_wc,
               "   AND gcao024 <= '",g_xrca_m.xrcadocdt,"'",
