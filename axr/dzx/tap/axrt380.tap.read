  <section id="axrt380.filter" ver="12" status="" src="s" readonly="">
    <![CDATA[#應用 a50 樣板自動產生(Version:5)
#+ filter過濾功能
PRIVATE FUNCTION axrt380_filter()
   #add-point:filter段define
   {<point name="filter.define" edit="s"/>}
   #end add-point   
   #add-point:filter段define
   {<point name="filter.define_customerization" edit="c"/>}
   #end add-point   
   
   #切換畫面
   IF NOT g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",1)
      CALL gfrm_curr.setElementHidden("worksheet",0)
      LET g_main_hidden = 1
   END IF   
 
   LET INT_FLAG = 0
 
   LET g_qryparam.state = 'c'
 
   LET g_wc_filter_t = g_wc_filter.trim()
   LET g_wc_t = g_wc
 
   LET g_wc = cl_replace_str(g_wc, g_wc_filter_t, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON xrcadocdt,xrca004,xrcadocno,xrca001,xrca038,xrcasite,xrca003,xrcald
                          FROM s_browse[1].b_xrcadocdt,s_browse[1].b_xrca004,s_browse[1].b_xrcadocno, 
                              s_browse[1].b_xrca001,s_browse[1].b_xrca038,s_browse[1].b_xrcasite,s_browse[1].b_xrca003, 
                              s_browse[1].b_xrcald
 
         BEFORE CONSTRUCT
               DISPLAY axrt380_filter_parser('xrcadocdt') TO s_browse[1].b_xrcadocdt
            DISPLAY axrt380_filter_parser('xrca004') TO s_browse[1].b_xrca004
            DISPLAY axrt380_filter_parser('xrcadocno') TO s_browse[1].b_xrcadocno
            DISPLAY axrt380_filter_parser('xrca001') TO s_browse[1].b_xrca001
            DISPLAY axrt380_filter_parser('xrca038') TO s_browse[1].b_xrca038
            DISPLAY axrt380_filter_parser('xrcasite') TO s_browse[1].b_xrcasite
            DISPLAY axrt380_filter_parser('xrca003') TO s_browse[1].b_xrca003
            DISPLAY axrt380_filter_parser('xrcald') TO s_browse[1].b_xrcald
      
         #add-point:filter段cs_ctrl
         {<point name="filter.cs_ctrl"/>}
         #end add-point
      
      END CONSTRUCT
 
      #add-point:filter段add_cs
      {<point name="filter.add_cs"/>}
      #end add-point
 
      BEFORE DIALOG
         #add-point:filter段b_dialog
         {<point name="filter.b_dialog"/>}
         #end add-point  
      
      ON ACTION accept
         ACCEPT DIALOG
 
      ON ACTION cancel
         LET INT_FLAG = 1
         EXIT DIALOG 
 
      #交談指令共用ACTION
      &include "common_action.4gl" 
         CONTINUE DIALOG
   
   END DIALOG
 
   IF NOT INT_FLAG THEN
      LET g_wc_filter = "   AND   ", g_wc_filter, "   "
      LET g_wc = g_wc , g_wc_filter
   ELSE
      LET g_wc_filter = g_wc_filter_t
      LET g_wc = g_wc_t
   END IF
 
      CALL axrt380_filter_show('xrcadocdt')
   CALL axrt380_filter_show('xrca004')
   CALL axrt380_filter_show('xrcadocno')
   CALL axrt380_filter_show('xrca001')
   CALL axrt380_filter_show('xrca038')
   CALL axrt380_filter_show('xrcasite')
   CALL axrt380_filter_show('xrca003')
   CALL axrt380_filter_show('xrcald')
 
END FUNCTION
]]>
