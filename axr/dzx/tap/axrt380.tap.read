         CALL s_axrt300_account_desc('xrcb010',g_xrcb4_d[l_ac].xrcb010,'') RETURNING l_success,g_xrcb4_d[l_ac].xrcb010_desc 
         CALL s_axrt300_account_desc('xrcb011',g_xrcb4_d[l_ac].xrcb011,'') RETURNING l_success,g_xrcb4_d[l_ac].xrcb011_desc
         CALL s_axrt300_account_desc('xrcb024',g_xrcb4_d[l_ac].xrcb024,'') RETURNING l_success,g_xrcb4_d[l_ac].xrcb024_desc
         CALL s_axrt300_account_desc('xrcb036',g_xrcb4_d[l_ac].xrcb036,'') RETURNING l_success,g_xrcb4_d[l_ac].xrcb036_desc
         CALL s_axrt300_account_desc('xrcb012',g_xrcb4_d[l_ac].xrcb012,'') RETURNING l_success,g_xrcb4_d[l_ac].xrcb012_desc
         CALL s_axrt300_account_desc('xrcb034',g_xrcb4_d[l_ac].xrcb034,'') RETURNING l_success,g_xrcb4_d[l_ac].xrcb034_desc
         CALL s_axrt300_account_desc('xrcb035',g_xrcb4_d[l_ac].xrcb035,'') RETURNING l_success,g_xrcb4_d[l_ac].xrcb035_desc
         CALL s_desc_get_project_desc(g_xrcb4_d[l_ac].xrcb015) RETURNING g_xrcb4_d[l_ac].xrcb015_desc 
