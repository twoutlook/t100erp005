                  LET l_n1 = 0
                  LET l_xrca065 = ''
                  LET l_xrca065 = g_xrca_m.xrca065,g_xrca_m.xrca066                   
                  SELECT COUNT(*) INTO l_n1 FROM isat_t 
                   WHERE isatent = g_enterprise
                     AND isatcomp = g_xrca_m.xrcacomp
                     AND (isat003||isat004) = l_xrca065
                     
                  IF l_n1 != 0 THEN 
                     #若存在则帶出開票單號 isatdocno 放到 xrca018,來源類型 = 12:銷項發票
                     #來源參考單號、發票日期
                     SELECT isatdocno,isat007 #1022 add isat007 
                       INTO g_xrca_m.xrca018,g_xrca_m.xrca061 
                       FROM isat_t
                      WHERE isatent = g_enterprise
                        AND isatcomp = g_xrca_m.xrcacomp    
                        AND (isat003||isat004) = l_xrca065

                     #發票號碼不為空且存在時，帶出發票日期且不可異動
