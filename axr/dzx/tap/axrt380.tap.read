            CALL axrt380_xrca_ref('xrca059')
            NEXT FIELD xrca059                                 #返回原欄位


      AFTER INPUT
         UPDATE xrca_t SET xrca007 = g_xrca_m.xrca007,
                           xrca035 = g_xrca_m.xrca035,
                           xrca036 = g_xrca_m.xrca036,
                           xrca059 = g_xrca_m.xrca059,
                           xrca039 = g_xrca_m.xrca039,
                           xrca056 = g_xrca_m.xrca056
          WHERE xrcaent = g_enterprise
            AND xrcadocno = g_xrca_m.xrcadocno
            AND xrcald  = g_xrca_m.xrcald

   END INPUT

