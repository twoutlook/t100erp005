#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt300_invalid()
   DEFINE l_success      LIKE type_t.chr1

   LET l_success = 'Y'

   INPUT BY NAME g_xrca_m.xrca051
      BEFORE INPUT
         LET g_xrca_m_t.xrca051 = g_xrca_m.xrca051

      AFTER FIELD xrca051
         LET g_xrca_m.xrca051_desc = ' '
         DISPLAY BY NAME g_xrca_m.xrca051_desc
         IF NOT cl_null(g_xrca_m.xrca051) THEN
               INITIALIZE g_chkparam.* TO NULL
               LET g_chkparam.arg1 = '3115'
               LET g_chkparam.arg2 = g_xrca_m.xrca051
               IF NOT cl_chk_exist("v_oocq002_01") THEN
                  LET g_xrca_m.xrca051 = g_xrca_m_t.xrca051
                  CALL s_axrt300_xrca_ref('xrca051',g_xrca_m.xrca051,'','') RETURNING l_success,g_xrca_m.xrca051_desc
                  DISPLAY BY NAME g_xrca_m.xrca051_desc
                  NEXT FIELD CURRENT
               END IF
         END IF
         CALL s_axrt300_xrca_ref('xrca051',g_xrca_m.xrca051,'','') RETURNING l_success,g_xrca_m.xrca051_desc
         DISPLAY BY NAME g_xrca_m.xrca051_desc

      ON ACTION controlp INFIELD xrca051
         #開窗i段
			INITIALIZE g_qryparam.* TO NULL
         LET g_qryparam.state = 'i'
			LET g_qryparam.reqry = FALSE
         LET g_qryparam.default1 = g_xrca_m.xrca051   #給予default值
         #給予arg
         LET g_qryparam.arg1 = '3115'
         CALL q_oocq002 ()                           #呼叫開窗
         LET g_xrca_m.xrca051 = g_qryparam.return1    #將開窗取得的值回傳到變數
         DISPLAY g_xrca_m.xrca051 TO xrca051          #顯示到畫面上
         CALL s_axrt300_xrca_ref('xrca051',g_xrca_m.xrca051,'','') RETURNING l_success,g_xrca_m.xrca051_desc
         DISPLAY BY NAME g_xrca_m.xrca051_desc

         NEXT FIELD xrca051

      AFTER INPUT
         UPDATE xrca_t SET xrca051 = g_xrca_m.xrca051
          WHERE xrcaent = g_enterprise
            AND xrcadocno = g_xrca_m.xrcadocno
            AND xrcald    = g_xrca_m.xrcald

   END INPUT

   IF g_xrca_m.xrca051 IS NULL THEN LET l_success = 'N' END IF
   
#   CALL axrt380_axmt500_upd('-')

   RETURN l_success

END FUNCTION]]>
