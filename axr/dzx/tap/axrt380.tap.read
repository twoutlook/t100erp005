  <point name="function.axrt380_b_fill5" order="30" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 单头发票信息显示
# Memo...........:
# Usage..........: CALL axrt380_b_fill5()
# Input parameter:  
# Date & Author..: 2014/10/16 By 01531
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt380_b_fill5()
#1016 add--str--
DEFINE l_success   LIKE type_t.chr1
DEFINE l_s         LIKE type_t.chr1
DEFINE l_isafdocno LIKE isaf_t.isafdocno
DEFINE ls_wc1      STRING

   CALL g_isat_d.clear()
   LET g_cnt = l_ac
   CALL s_axrt300_sel_ld(g_xrca_m.xrcald) RETURNING l_success,l_s   #獲取帳套主/次
   
   CASE l_s
      WHEN '1'
         LET ls_wc1 = " isaf035 = '",g_xrca_m.xrcadocno,"'"
      WHEN '2'
         LET ls_wc1 = " isaf048 = '",g_xrca_m.xrcadocno,"'"
      WHEN '3'
         LET ls_wc1 = " isaf049 = '",g_xrca_m.xrcadocno,"'"
   END CASE
   
   #应收单据获取开票单号
   LET g_sql = " SELECT isafdocno FROM isaf_t ",
               "  WHERE isafent = '",g_enterprise,"' AND isafcomp = '",g_xrca_m.xrcacomp,"'",
               "    AND ",ls_wc1 CLIPPED,
               "    AND isafstus = 'Y' "
   PREPARE axrt380_isaf_prep FROM g_sql
   DECLARE axrt380_isaf_curs CURSOR FOR axrt380_isaf_prep  

   #开票单号串查isat资料
   LET g_sql = " SELECT 0,isaf002,isat001,isat003,isat004,isat007,isat023,isat103,isat104,",
               "        isat105,isat101,isat113,isat114,isat115,isatdocno,isat014 ",
               "   FROM isat_t,isaf_t ",
               "  WHERE isatent = isafent AND isatcomp = isafcomp AND isatdocno = isafdocno ",
               "    AND isatent = '",g_enterprise,"' AND isatcomp = '",g_xrca_m.xrcacomp,"'",
               "    AND isatdocno = ? "
   PREPARE axrt380_isat_prep FROM g_sql
   DECLARE axrt380_isat_curs CURSOR FOR axrt380_isat_prep   

   LET l_isafdocno = NULL
   LET l_ac = 1
   FOREACH axrt380_isaf_curs INTO l_isafdocno 
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH isaf:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF

      FOREACH axrt380_isat_curs USING l_isafdocno INTO g_isat_d[l_ac].*
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH isat:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         
            EXIT FOREACH
         END IF 

         #发票客户
         INITIALIZE g_ref_fields TO NULL
         LET g_ref_fields[1] = g_isat_d[l_ac].isaf002
         CALL ap_ref_array2(g_ref_fields,"SELECT pmaal004 FROM pmaal_t WHERE pmaalent='"||g_enterprise||"' AND pmaal001=? AND pmaal002='"||g_dlang||"'","") RETURNING g_rtn_fields
         LET g_isat_d[l_ac].isaf002 = g_isat_d[l_ac].isaf002,' ', g_rtn_fields[1] , ''
 
         #发票类型
         SELECT ooef019 INTO g_ooef019
           FROM ooef_t
          WHERE ooefent = g_enterprise
            AND ooef001 = g_xrca_m.xrcacomp
            AND ooefstus = 'Y'         
         
         INITIALIZE g_ref_fields TO NULL
         LET g_ref_fields[1] = g_ooef019
         LET g_ref_fields[2] = g_isat_d[l_ac].isat001
