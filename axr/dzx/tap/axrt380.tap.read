  <point name="function.axrt380_get_exrate" order="9" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 獲取本位幣匯率
# Memo...........:
# Usage..........: CALL axrt380_get_exrate()
#                  RETURNING r_rate1,r_rate2,r_rate3
# Input parameter: 
# Return code....: r_rate1        本位幣匯率
#                : r_rate2        本位幣二匯率
#                : r_rate3        本位幣三匯率
# Date & Author..: 2014/03/13 By zhangwei
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt380_get_exrate()
   DEFINE p_cmd          LIKE type_t.chr1
   DEFINE r_rate1        LIKE xrca_t.xrca103
   DEFINE r_rate2        LIKE xrca_t.xrca103
   DEFINE r_rate3        LIKE xrca_t.xrca103
   DEFINE l_scc40        LIKE type_t.chr2 

   #抓取系統默認匯率
   #STEP1.本位幣
   CALL cl_get_para(g_enterprise,g_xrca_m.xrcacomp,'S-BAS-0010') RETURNING l_scc40
   CALL s_aooi160_get_exrate('2',g_xrca_m.xrcald,g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa001,0,l_scc40)
      RETURNING r_rate1
   #STEP2.本位幣二
   IF g_glaa.glaa015 = 'Y' THEN
      IF g_glaa.glaa017 = '1' THEN
         #换算基准:交易原幣
         CALL s_aooi160_get_exrate('2',g_xrca_m.xrcald,g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa016,0,g_glaa.glaa018)
            RETURNING r_rate2
      ELSE
         #换算基准:帳簿幣別
         CALL s_aooi160_get_exrate('2',g_xrca_m.xrcald,g_xrca_m.xrcadocdt,g_glaa.glaa001,g_glaa.glaa016,0,g_glaa.glaa018)
            RETURNING r_rate2
      END IF
   ELSE
      LET r_rate2 = 0
   END IF
   #STEP3.本位幣三
   IF g_glaa.glaa019 = 'Y' THEN
      IF g_glaa.glaa021 = '1' THEN
         #换算基准:交易原幣
         CALL s_aooi160_get_exrate('2',g_xrca_m.xrcald,g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa020,0,g_glaa.glaa022)
            RETURNING r_rate3
      ELSE
         #换算基准:帳簿幣別
         CALL s_aooi160_get_exrate('2',g_xrca_m.xrcald,g_xrca_m.xrcadocdt,g_glaa.glaa001,g_glaa.glaa020,0,g_glaa.glaa022)
            RETURNING r_rate3
      END IF
   ELSE
      LET r_rate3 = 0
   END IF

   RETURN r_rate1,r_rate2,r_rate3

END FUNCTION]]>
  </point>
  <point name="function.axrt380_xrcb020_upd" order="10" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 稅別更改xrcb_t相關欄位更改
# Memo...........:
# Usage..........: CALL axrt380_xrcb020_upd()
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt380_xrcb020_upd()
DEFINE l_glaa015 LIKE glaa_t.glaa015
DEFINE l_glaa019 LIKE glaa_t.glaa019

   LET g_xrcb_d[l_ac].xrcb111 = g_xrcb_d[l_ac].xrcb101 * g_xrca_m.xrca101
   
   SELECT glaa015,glaa019 INTO l_glaa015,l_glaa019 FROM glaa_t
    WHERE glaaent = g_enterprise AND glaald = g_xrca_m.xrcald
   IF l_glaa015 = 'Y' THEN  
      LET g_xrcb3_d[l_ac].xrcb123 = g_xrcb_d[l_ac].xrcb103 * g_xrca_m.xrca121
      LET g_xrcb3_d[l_ac].xrcb125 = g_xrcb_d[l_ac].xrcb105 * g_xrca_m.xrca121
