  <point name="function.axrt380_xrca018_axmt500" order="18" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL s_aooi150_ins (传入参数)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt380_xrca018_axmt500()
   DEFINE l_xmda         RECORD LIKE xmda_t.*
   DEFINE l_xmdc         RECORD LIKE xmdc_t.*
   DEFINE l_xrcb         RECORD LIKE xrcb_t.*
   DEFINE l_xmdb002      LIKE xmdb_t.xmdb002
   DEFINE l_xmdcseq      LIKE xmdc_t.xmdcseq
   DEFINE l_xrcd009      LIKE xrcd_t.xrcd009
   DEFINE l_oodbl004     LIKE oodbl_t.oodbl004
   DEFINE l_oodb011      LIKE oodb_t.oodb011
   DEFINE l_oodb005      LIKE oodb_t.oodb005
   DEFINE l_ooba002      LIKE ooba_t.ooba002
   DEFINE l_xrcb005      LIKE xrcb_t.xrcb005
   DEFINE l_sql          STRING
   DEFINE l_success      LIKE type_t.num5
   DEFINE l_s            LIKE type_t.num5
   DEFINE l_xrcbseq      LIKE xrcb_t.xrcbseq
   DEFINE l_xrcb105      LIKE xrcb_t.xrcb105
   DEFINE l_xmdb006      LIKE xmdb_t.xmdb006
   DEFINE  l_dfin0030            LIKE type_t.chr1

   CALL s_axrt300_sel_ld(g_xrca_m.xrcald) RETURNING l_success,l_s

   SELECT * INTO l_xmda.* FROM xmda_t WHERE xmdadocno = g_xrca_m.xrca018 AND xmdaent = g_enterprise
   SELECT xmdb002 INTO l_xmdb002 FROM xmdb_t
    WHERE xmdbent = g_enterprise AND xmdbdocno = g_xrca_m.xrca018 AND xmdb001 = g_xrca_m.xrca064
   CALL s_dep_pay(l_xmda.xmdadocno,g_xrca_m.xrca064,g_xrca_m.xrca011,l_xmda.xmdasite,g_xrca_m.xrca101,l_xmdb002) RETURNING l_success

   LET l_sql = "SELECT xmdcseq,qty,amt FROM s_dep_pay_tmp WHERE xmdadocno = ? AND xmdb001 = ? ORDER BY xmdcseq"
   PREPARE axrt380_xmdc_prep FROM l_sql
   DECLARE axrt380_xmdc_curs CURSOR FOR axrt380_xmdc_prep

   FOREACH axrt380_xmdc_curs USING g_xrca_m.xrca018,g_xrca_m.xrca064 INTO l_xmdcseq,l_xrcb.xrcb007,l_xrcb.xrcb105
      SELECT * INTO l_xmdc.* FROM xmdc_t WHERE xmdcdocno = g_xrca_m.xrca018 AND xmdcseq = l_xmdcseq

      SELECT imaal003 INTO l_xrcb005 FROM imaal_t
       WHERE imaalent = g_enterprise AND imaal002 = g_lang
         AND imaal001 = l_xmdc.xmdc001

      LET l_xrcb.xrcbent = g_enterprise
      LET l_xrcb.xrcbld  = g_xrca_m.xrcald
      LET l_xrcb.xrcbdocno = g_xrca_m.xrcadocno
      LET l_xrcb.xrcbseq = l_xrcb.xrcbseq + 1
      LET l_xrcb.xrcbsite= g_xrca_m.xrcasite
      LET l_xrcb.xrcborga=l_xmda.xmdasite
      LET l_xrcb.xrcb001 = '10'
      LET l_xrcb.xrcb002 = l_xmdc.xmdcdocno
      LET l_xrcb.xrcb003 = l_xmdc.xmdcseq
      LET l_xrcb.xrcb004 = l_xmdc.xmdc001
      LET l_xrcb.xrcb005 = l_xrcb005
      LET l_xrcb.xrcb006 = l_xmdc.xmdc006
      LET l_xrcb.xrcb008 = ''
      LET l_xrcb.xrcb009 = ''
      LET l_xrcb.xrcblegl= g_xrca_m.xrcacomp
      LET l_xrcb.xrcb051 = l_xmda.xmda002
      LET l_xrcb.xrcb010 = l_xmda.xmda003
      LET l_xrcb.xrcb011 = g_xrca_m.xrca034
      LET l_xrcb.xrcb012 = ''
      LET l_xrcb.xrcb013 = ''
      LET l_xrcb.xrcb014 = ''
      LET l_xrcb.xrcb015 = ''
      LET l_xrcb.xrcb016 = ''
      LET l_xrcb.xrcb017 = ''
      LET l_xrcb.xrcb018 = ''
      LET l_xrcb.xrcb019 = ''
      LET l_xrcb.xrcb020 = g_xrca_m.xrca011
      CALL s_axrt300_item_acc(g_xrca_m.xrcald,g_xrca_m.xrca036,l_xrcb.xrcb004)
         RETURNING l_xrcb.xrcb021
      IF l_xrcb.xrcb001 = '29' THEN LET l_xrcb.xrcb022 = -1 ELSE LET l_xrcb.xrcb022 = 1 END IF
      LET l_xrcb.xrcb024 = ''
      LET l_xrcb.xrcb025 = ''
      LET l_xrcb.xrcb026 = ''
      LET l_xrcb.xrcb027 = ''
      LET l_xrcb.xrcb028 = ''
      LET l_xrcb.xrcb029 = g_xrca_m.xrca035
      LET l_xrcb.xrcb030 = 0
      LET l_xrcb.xrcb031 = l_xmdb002
      LET l_xrcb.xrcb032 = g_xrca_m.xrca064
      IF cl_null(l_xrcb.xrcb031) THEN
         LET l_xrcb.xrcb031 = g_xrca_m.xrca008
      END IF
      LET l_xrcb.xrcb100 = g_xrca_m.xrca100
      LET l_xrcb.xrcb101 = l_xmdc.xmdc015
      CALL s_axrt300_exrate(g_glaa.glaa002,g_xrca_m.xrcadocdt,g_xrca_m.xrca100,g_glaa.glaa001,l_xrcb.xrcb101,g_xrca_m.xrca101,g_glaa.glaacomp)
         RETURNING l_success,l_xrcb.xrcb111
      CALL s_tax_ins(g_xrca_m.xrcadocno,l_xrcb.xrcbseq,0,g_glaa.glaacomp,
                     l_xrcb.xrcb105,l_xrcb.xrcb020,
                     l_xrcb.xrcb007,l_xrcb.xrcb100,g_xrca_m.xrca101,g_xrca_m.xrcald,g_xrca_m.xrca121,g_xrca_m.xrca131)
         RETURNING l_xrcb.xrcb103,l_xrcb.xrcb104,l_xrcb.xrcb105,
                   l_xrcb.xrcb113,l_xrcb.xrcb114,l_xrcb.xrcb115,
                   l_xrcb.xrcb123,l_xrcb.xrcb124,l_xrcb.xrcb125,
                   l_xrcb.xrcb133,l_xrcb.xrcb134,l_xrcb.xrcb135
      SELECT glab005 INTO l_xrcd009 FROM glab_t 
       WHERE glabld = g_xrca_m.xrcald 
         AND glabent = g_enterprise
         AND glab002 = g_xrca_m.xrca007   # 帳款類別
         AND glab001 = '13'             # 應收帳務類型科目設定
         AND glab003 = '8304_29'
      UPDATE xrcd_t SET xrcd009 = l_xrcd009
       WHERE xrcd009 IS NULL
         AND xrcdent = g_enterprise
         AND xrcddocno = g_xrca_m.xrcadocno
         AND xrcdld = g_xrca_m.xrcald
      CALL s_tax_chk(g_glaa.glaacomp,l_xrcb.xrcb020)
         RETURNING l_success,l_oodbl004,l_oodb005,g_xrca_m.xrca012,l_oodb011
      IF l_oodb005 = 'Y' THEN
         LET l_xrcb.xrcb113 = l_xrcb.xrcb115 - l_xrcb.xrcb114
         LET l_xrcb.xrcb123 = l_xrcb.xrcb125 - l_xrcb.xrcb124
         LET l_xrcb.xrcb133 = l_xrcb.xrcb135 - l_xrcb.xrcb134
      ELSE
         LET l_xrcb.xrcb115 = l_xrcb.xrcb113 + l_xrcb.xrcb114
         LET l_xrcb.xrcb125 = l_xrcb.xrcb123 + l_xrcb.xrcb124
         LET l_xrcb.xrcb135 = l_xrcb.xrcb133 + l_xrcb.xrcb134
      END IF
      LET l_xrcb.xrcb106 = 0
      LET l_xrcb.xrcb116 = 0
      LET l_xrcb.xrcb117 = 0
      LET l_xrcb.xrcb118 = l_xrcb.xrcb113
      LET l_xrcb.xrcb119 = l_xrcb.xrcb115
      LET l_xrcb.xrcb126 = 0
      LET l_xrcb.xrcb136 = 0
      
      INSERT INTO xrcb_t VALUES (l_xrcb.*)
      IF SQLCA.SQLCODE OR SQLCA.SQLERRD[3] <> 1 THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.extend = g_xrca_m.xrcadocno
         LET g_errparam.code   = 'aap-00187'
         LET g_errparam.popup  = TRUE
         CALL cl_err()
         LET g_success = 'N'
      END IF

   END FOREACH


   LET l_xrcb105 = 0
   SELECT SUM(xrcb105) INTO l_xrcb105 FROM xrcb_t WHERE xrcbent = g_enterprise
      AND xrcbdocno = g_xrca_m.xrcadocno AND xrcbld = g_xrca_m.xrcald
   IF cl_null(l_xrcb105) THEN LET l_xrcb105 = 0 END IF

   LET l_xmdb006 = 0
   SELECT xmdb006 INTO l_xmdb006 FROM xmdb_t WHERE xmdbent = g_enterprise
      AND xmdbdocno = g_xrca_m.xrca018 AND xmdb001 = g_xrca_m.xrca064
   IF cl_null(l_xmdb006) THEN LET l_xmdb006 = 0 END IF

   #訂金金額和應收金額存在差異
   IF l_xrcb105 <> l_xmdb006 THEN
      SELECT xrcbseq INTO l_xrcbseq FROM xrcb_t WHERE xrcbent = g_enterprise
         AND xrcbdocno = g_xrca_m.xrcadocno
         AND xrcbld = g_xrca_m.xrcald
         AND xrcb105 = (SELECT MAX(xrcb105) FROM xrcb_t WHERE xrcbent = g_enterprise
                           AND xrcbdocno = g_xrca_m.xrcadocno
                           AND xrcbld = g_xrca_m.xrcald)
      IF cl_null(l_xrcbseq) THEN LET l_xrcbseq = 1 END IF
      #將差異金額放入應收單據單身金額最大的一筆的含稅金額中
      LET l_xrcb105 = l_xmdb006 - l_xrcb105
      UPDATE xrcb_t SET xrcb103 = xrcb103 + l_xrcb105,
                        xrcb105 = xrcb105 + l_xrcb105
                       #xrcb113 = xrcb113 + l_xrcb105 * g_xrca_m.xrca101,
                       #xrcb115 = xrcb115 + l_xrcb105 * g_xrca_m.xrca101,
                       #xrcb123 = xrcb123 + l_xrcb105 * g_xrca_m.xrca121,
                       #xrcb125 = xrcb125 + l_xrcb105 * g_xrca_m.xrca121,
                       #xrcb133 = xrcb133 + l_xrcb105 * g_xrca_m.xrca131,
                       #xrcb135 = xrcb135 + l_xrcb105 * g_xrca_m.xrca131
       WHERE xrcbent = g_enterprise
         AND xrcbdocno = g_xrca_m.xrcadocno
         AND xrcbld  = g_xrca_m.xrcald
         AND xrcbseq = 1
      UPDATE xrcb_t SET xrcb113 = xrcb103 * g_xrca_m.xrca101,
                        xrcb115 = xrcb105 * g_xrca_m.xrca101,
                        xrcb123 = xrcb103 * g_xrca_m.xrca121,
                        xrcb125 = xrcb105 * g_xrca_m.xrca121,
                        xrcb133 = xrcb103 * g_xrca_m.xrca131,
                        xrcb135 = xrcb105 * g_xrca_m.xrca131
       WHERE xrcbent = g_enterprise
         AND xrcbdocno = g_xrca_m.xrcadocno
         AND xrcbld  = g_xrca_m.xrcald
         AND xrcbseq = 1
   END IF


   CASE
      WHEN l_s = '1'
         UPDATE xmdb_t SET xmdb008 = xmdb005,xmdb009 = xmdb006,xmdb007 = g_xrca_m.xrcadocno
          WHERE xmdbdocno = g_xrca_m.xrca018
            AND xmdb001 = g_xrca_m.xrca064
      WHEN l_s = '2'
         UPDATE xmdb_t SET xmdb010 = xmdb005,xmdb011 = xmdb006
          WHERE xmdbdocno = g_xrca_m.xrca018
            AND xmdb001 = g_xrca_m.xrca064
      WHEN l_s = '3'
         UPDATE xmdb_t SET xmdb014 = xmdb005,xmdb015 = xmdb006
          WHERE xmdbdocno = g_xrca_m.xrca018
            AND xmdb001 = g_xrca_m.xrca064
   END CASE

   CALL s_axrt300_installments(g_xrca_m.xrcald,g_xrca_m.xrcadocno) RETURNING l_success
   CALL s_axrt300_xrca_upd(g_xrca_m.xrcald,g_xrca_m.xrcadocno) RETURNING l_success
   DISPLAY BY NAME g_xrca_m.xrca103,g_xrca_m.xrca104,g_xrca_m.xrca113,g_xrca_m.xrca114,
                   g_xrca_m.xrca123,g_xrca_m.xrca124,g_xrca_m.xrca133,g_xrca_m.xrca134,
                   g_xrca_m.xrca108,g_xrca_m.xrca118,g_xrca_m.xrca128,g_xrca_m.xrca138
   CALL s_fin_get_doc_para(g_xrca_m.xrcald,g_xrca_m.xrcacomp,l_ooba002,'D-FIN-0030') RETURNING l_dfin0030
   IF g_glaa.glaa121 = 'Y' AND l_dfin0030 = 'Y' THEN
      CALL s_pre_voucher_ins('AR','R10',g_glaa.glaald,g_xrca_m.xrcadocno,g_xrca_m.xrcadocdt,'1') RETURNING l_success
   END IF
   IF l_success = 'Y' OR l_success THEN
      CALL s_transaction_end('Y',1)
   ELSE
      CALL s_transaction_end('N',1)
   END IF

END FUNCTION]]>
  </point>
  <point name="function.axrt380_get_data" order="19" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL s_aooi150_ins (传入参数)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt380_get_data()
   DEFINE l_xmda_t      RECORD LIKE xmda_t.*
   DEFINE l_count       LIKE type_t.num5
   DEFINE l_xrcb_t      RECORD LIKE xrcb_t.*
   DEFINE l_xmdb_t      RECORD LIKE xmdb_t.*
   DEFINE l_sql         STRING
   DEFINE l_success     LIKE type_t.chr1
   DEFINE l_s           LIKE type_t.chr1
   DEFINE l_oodbl004    LIKE oodbl_t.oodbl004
   DEFINE l_oodb005     LIKE oodb_t.oodb005
   DEFINE l_oodb006     LIKE oodb_t.oodb006
   DEFINE l_oodb011     LIKE oodb_t.oodb011
   DEFINE l_xrcd009      LIKE xrcd_t.xrcd009  #1029 add 
   SELECT * INTO l_xmda_t.* FROM xmda_t WHERE xmdaent = g_enterprise AND xmdadocno = g_xrcb_d[l_ac].xrcb002

   IF cl_null(l_xmda_t.xmdadocno) THEN RETURN 'axm-00299' END IF

   IF NOT cl_null(g_xrcb_d[l_ac].xrcborga) THEN
      SELECT * INTO l_xmda_t.* FROM xmda_t
       WHERE xmdaent = g_enterprise AND xmdadocno = g_xrcb_d[l_ac].xrcb002 AND xmdasite = g_xrcb_d[l_ac].xrcborga
      
      IF cl_null(l_xmda_t.xmdadocno) THEN RETURN 'axm-00299' END IF
   END IF

   IF l_xmda_t.xmdastus <> 'Y' THEN RETURN 'axm-00305' END IF

   IF l_xmda_t.xmdadocdt > g_xrca_m.xrcadocdt THEN RETURN 'axm-00300' END IF
   
   #150210-00011(1)--20150330--add--
   #订单日期必须要和单头的立账日期在同一个会计期间内
   IF l_xmda_t.xmdadocdt < g_pdate_s OR l_xmda_t.xmdadocdt > g_pdate_e THEN RETURN 'axr-00319' END IF
   #150210-00011(1)--20150330--add--

