# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt380_xrca004_ref()
DEFINE l_site         LIKE xrca_t.xrcasite
DEFINE l_ooba002      LIKE ooba_t.ooba002
DEFINE l_success      LIKE type_t.num5
DEFINE l_pmab085      LIKE pmab_t.pmab085
DEFINE l_oodbl004     LIKE oodbl_t.oodbl004
DEFINE l_oodb011      LIKE oodb_t.oodb011
DEFINE l_ooib         RECORD LIKE ooib_t.*

   #新增/修改帳款對象後,依帳款對象更新客戶慣用資料
   
   ################################################
   #      STEP1-7 欄位取用原則:
   #      依帳務人員所屬 site  取出該客戶的相關欄位
   #      若取不到時再取 xrcacomp =pmabsite  為條件取
   #     【20141014調整】 STEP1-7 欄位取用原則:
   #      直接以 xrcacomp = pmabsite 為條件取     
   ################################################  
   
   SELECT DISTINCT ooag004 INTO l_site
     FROM ooag_t
    WHERE ooag001 = g_xrca_m.xrca003 AND ooagstus ='Y' 
   
   #STEP1.獲取交易對象簡稱
   CALL axrt380_xrca_ref('xrca004')
   
   #STEP2.帶出主要應收條件
#   SELECT pmab087 INTO g_xrca_m.xrca008 FROM pmab_t
#    WHERE pmabent = g_enterprise AND pmabsite = l_site AND pmab001 = g_xrca_m.xrca004
#   IF SQLCA.sqlcode = 100 THEN 
      SELECT pmab087 INTO g_xrca_m.xrca008 FROM pmab_t
       WHERE pmabent = g_enterprise AND pmabsite = g_xrca_m.xrcacomp AND pmab001 = g_xrca_m.xrca004
#   END IF
   CALL axrt380_xrca_ref('xrca008')
   
