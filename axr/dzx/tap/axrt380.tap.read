           FROM imaal_t
          WHERE imaalent = g_enterprise AND imaal002 = g_lang
            AND imaal001 = l_xrcb.xrcb004
         
         LET l_xrcb.xrcb006 = l_tmp.rtib013
         LET l_xrcb.xrcb007 = l_tmp.rtib012
         LET l_xrcb.xrcb008 = l_tmp.rtiadocno
         LET l_xrcb.xrcb020 = l_tmp.rtib006
         LET l_xrcb.xrcb023 = 'N'
         LET l_xrcb.xrcb029 = l_glab005_1
         LET l_xrcb.xrcb021 = l_glab005_2
         LET l_xrcb.xrcb100 = g_xrca_m.xrca100
         LET l_xrcb.xrcb101 = l_tmp.rtib010
         LET l_xrcb.xrcb102 = g_xrca_m.xrca101
         LET l_xrcb.xrcb111 = l_tmp.rtib010 * g_xrca_m.xrca101
         
         CALL s_tax_ins(g_xrca_m.xrcadocno,l_xrcb.xrcbseq,0,l_xrcb.xrcborga,
                        l_xrcb.xrcb007 * l_xrcb.xrcb101,l_xrcb.xrcb020,
                        l_xrcb.xrcb007,g_xrca_m.xrca100,g_xrca_m.xrca101,g_xrca_m.xrcald,g_xrca_m.xrca121,g_xrca_m.xrca131)
         RETURNING l_xrcb.xrcb103,l_xrcb.xrcb104,l_xrcb.xrcb105,
                   l_xrcb.xrcb113,l_xrcb.xrcb114,l_xrcb.xrcb115,
                   l_xrcb.xrcb123,l_xrcb.xrcb124,l_xrcb.xrcb125,
                   l_xrcb.xrcb133,l_xrcb.xrcb134,l_xrcb.xrcb135
                   
         LET l_xrcb.xrcb113 = l_xrcb.xrcb103
         LET l_xrcb.xrcb114 = l_xrcb.xrcb104
         LET l_xrcb.xrcb115 = l_xrcb.xrcb105
         UPDATE xrcd_t SET xrcd113 = xrcd103,
                           xrcd114 = xrcd104,
                           xrcd115 = xrcd105
          WHERE xrcdent = g_enterprise
            AND xrcddocno = g_xrca_m.xrcadocno
            AND xrcdld = g_xrca_m.xrcald
         
         INSERT INTO xrcb_t VALUES(l_xrcb.*)
         
         IF SQLCA.SQLcode THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "ins xrcb_t"
            LET g_errparam.popup = TRUE
            CALL cl_err()
      
            LET l_success = FALSE                           
         END IF
      END FOREACH
   END IF 
   
   #卡券註銷
   IF l_sel MATCHES '[23]' THEN 
      FOREACH axrt380_p_cs1 INTO l_tmp.*
      
         LET l_xrcb.xrcbent = g_enterprise
         LET l_xrcb.xrcbld = g_xrca_m.xrcald
         LET l_xrcb.xrcbdocno = g_xrca_m.xrcadocno
         SELECT MAX(xrcbseq) + 1 INTO l_xrcb.xrcbseq
           FROM xrcb_t
          WHERE xrcbent = g_enterprise
            AND xrcbld = l_xrcb.xrcbld
            AND xrcbdocno = l_xrcb.xrcbdocno
            
         IF cl_null(l_xrcb.xrcbseq) THEN 
            LET l_xrcb.xrcbseq = 1
         END IF
            
         LET l_xrcb.xrcbsite = g_xrca_m.xrcasite
         LET l_xrcb.xrcborga = g_xrca_m.xrcasite      
         
         IF l_tmp.rtia000 = 'agct405' OR l_tmp.rtia000 = 'ammt405' THEN      
            LET l_xrcb.xrcb001 = '131'   
            LET l_xrcb.xrcb022 = '1'           
         ELSE
            LET l_xrcb.xrcb001 = '132'   
            LET l_xrcb.xrcb022 = '-1'
         END IF  
         
         LET l_xrcb.xrcb004 = l_tmp.rtib004
         SELECT imaal003 INTO l_xrcb.xrcb005
           FROM imaal_t
          WHERE imaalent = g_enterprise AND imaal002 = g_lang
            AND imaal001 = l_xrcb.xrcb004       
            
         #來源gcao表時,抓第一筆0稅率的稅別   
         IF l_tmp.rtia000 = 'gcao' THEN 
            LET l_sql = "SELECT oodb002 FROM oodb_t ",
               " WHERE oodb001 = '",l_ooef019,"'",
               "   AND oodb008 = '2' ",
               "   AND oodb006 = 0 "
            PREPARE oodb_pre FROM l_sql
            DECLARE oodb_cur SCROLL CURSOR WITH HOLD FOR oodb_pre
            OPEN oodb_cur
            FETCH FIRST oodb_cur INTO l_xrcb.xrcb020
            CLOSE oodb_cur
         ELSE
            LET l_xrcb.xrcb020 = l_tmp.rtib006
         END IF
         
         LET l_xrcb.xrcb006 = l_tmp.rtib013
         LET l_xrcb.xrcb007 = l_tmp.rtib012
         LET l_xrcb.xrcb008 = l_tmp.rtiadocno
         LET l_xrcb.xrcb023 = 'N'
         LET l_xrcb.xrcb029 = l_glab005_1
         LET l_xrcb.xrcb021 = l_glab005_2
         LET l_xrcb.xrcb100 = g_xrca_m.xrca100
         LET l_xrcb.xrcb101 = l_tmp.rtib010
         LET l_xrcb.xrcb102 = g_xrca_m.xrca101
         LET l_xrcb.xrcb111 = l_tmp.rtib010 * g_xrca_m.xrca101
         
         CALL s_tax_ins(g_xrca_m.xrcadocno,l_xrcb.xrcbseq,0,l_xrcb.xrcborga,
                        l_xrcb.xrcb007 * l_xrcb.xrcb101,l_xrcb.xrcb020,
                        l_xrcb.xrcb007,g_xrca_m.xrca100,g_xrca_m.xrca101,g_xrca_m.xrcald,g_xrca_m.xrca121,g_xrca_m.xrca131)
         RETURNING l_xrcb.xrcb103,l_xrcb.xrcb104,l_xrcb.xrcb105,
                   l_xrcb.xrcb113,l_xrcb.xrcb114,l_xrcb.xrcb115,
                   l_xrcb.xrcb123,l_xrcb.xrcb124,l_xrcb.xrcb125,
                   l_xrcb.xrcb133,l_xrcb.xrcb134,l_xrcb.xrcb135
                   
         LET l_xrcb.xrcb113 = l_xrcb.xrcb103
         LET l_xrcb.xrcb114 = l_xrcb.xrcb104
         LET l_xrcb.xrcb115 = l_xrcb.xrcb105
         UPDATE xrcd_t SET xrcd113 = xrcd103,
