  <point name="function.axrt380_xrca_upd" order="12" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 更新單頭金額
# Memo...........:
# Usage..........: CALL axrt380_xrca_upd()
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION axrt380_xrca_upd()
DEFINE l_xrca103_sum   LIKE xrca_t.xrca103 
DEFINE l_xrca104_sum   LIKE xrca_t.xrca104
DEFINE l_xrca108_sum   LIKE xrca_t.xrca108
DEFINE l_xrca113_sum   LIKE xrca_t.xrca113 
DEFINE l_xrca114_sum   LIKE xrca_t.xrca114
DEFINE l_xrca118_sum   LIKE xrca_t.xrca118
DEFINE l_xrca123_sum   LIKE xrca_t.xrca123 
DEFINE l_xrca128_sum   LIKE xrca_t.xrca124
DEFINE l_xrca124_sum   LIKE xrca_t.xrca128
DEFINE l_xrca133_sum   LIKE xrca_t.xrca133 
DEFINE l_xrca134_sum   LIKE xrca_t.xrca134
DEFINE l_xrca138_sum   LIKE xrca_t.xrca138
DEFINE l_cnt           LIKE type_t.num5


   LET l_cnt = 0 
   SELECT COUNT(*) INTO l_cnt FROM xrcb_t
    WHERE xrcbent = g_enterprise AND xrcbld = g_xrca_m.xrcald AND xrcbdocno = g_xrca_m.xrcadocno 
   IF l_cnt > 0 THEN  
      SELECT SUM(xrcb103 * xrcb022),SUM(xrcb104 * xrcb022),SUM(xrcb113 * xrcb022),SUM(xrcb114 * xrcb022),
             SUM(xrcb123 * xrcb022),SUM(xrcb124 * xrcb022),SUM(xrcb133 * xrcb022),SUM(xrcb134 * xrcb022)  
        INTO g_xrca_m.xrca103,g_xrca_m.xrca104,g_xrca_m.xrca113,g_xrca_m.xrca114, 
             g_xrca_m.xrca123,g_xrca_m.xrca124,g_xrca_m.xrca133,g_xrca_m.xrca134     
        FROM xrcb_t
       WHERE xrcbent = g_enterprise AND xrcbld = g_xrca_m.xrcald AND xrcbdocno = g_xrca_m.xrcadocno
      IF cl_null(g_xrca_m.xrca103) THEN LET g_xrca_m.xrca103 = 0 END IF 
      IF cl_null(g_xrca_m.xrca104) THEN LET g_xrca_m.xrca104 = 0 END IF 
      IF cl_null(g_xrca_m.xrca113) THEN LET g_xrca_m.xrca113 = 0 END IF 
      IF cl_null(g_xrca_m.xrca114) THEN LET g_xrca_m.xrca114 = 0 END IF
      IF cl_null(g_xrca_m.xrca123) THEN LET g_xrca_m.xrca123 = 0 END IF 
      IF cl_null(g_xrca_m.xrca124) THEN LET g_xrca_m.xrca124 = 0 END IF
      IF cl_null(g_xrca_m.xrca133) THEN LET g_xrca_m.xrca133 = 0 END IF 
      IF cl_null(g_xrca_m.xrca134) THEN LET g_xrca_m.xrca134 = 0 END IF
      UPDATE xrca_t SET (xrca103,xrca104,xrca113,xrca114,xrca123,xrca124,xrca133,xrca134) = (g_xrca_m.xrca103,g_xrca_m.xrca104,g_xrca_m.xrca113,g_xrca_m.xrca114,g_xrca_m.xrca123,g_xrca_m.xrca124,g_xrca_m.xrca133,g_xrca_m.xrca134) 
       WHERE xrcaent = g_enterprise AND xrcald = g_xrca_m.xrcald AND xrcadocno = g_xrca_m.xrcadocno
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "xrca_t"
         LET g_errparam.popup = FALSE
         CALL cl_err()

      END IF        
   END IF 
   
   SELECT SUM(xrcc108),SUM(xrcc118),SUM(xrcc128),SUM(xrcc138) 
     INTO g_xrca_m.xrca108,g_xrca_m.xrca118,g_xrca_m.xrca128,g_xrca_m.xrca138 
     FROM xrcc_t
    WHERE xrccent = g_enterprise AND xrccld = g_xrca_m.xrcald AND xrccdocno = g_xrca_m.xrcadocno
  
   IF cl_null(g_xrca_m.xrca108) THEN LET g_xrca_m.xrca108 = 0 END IF   
   IF cl_null(g_xrca_m.xrca118) THEN LET g_xrca_m.xrca118 = 0 END IF   
   IF cl_null(g_xrca_m.xrca128) THEN LET g_xrca_m.xrca128 = 0 END IF   
   IF cl_null(g_xrca_m.xrca138) THEN LET g_xrca_m.xrca138 = 0 END IF    
   
   UPDATE xrca_t SET (xrca108,xrca118,xrca128,xrca138) = (g_xrca_m.xrca108,g_xrca_m.xrca118,g_xrca_m.xrca128,g_xrca_m.xrca138) 
    WHERE xrcaent = g_enterprise AND xrcald = g_xrca_m.xrcald AND xrcadocno = g_xrca_m.xrcadocno
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "xrca_t"
         LET g_errparam.popup = FALSE
         CALL cl_err()

   END IF
   
   DISPLAY BY NAME g_xrca_m.xrca103,g_xrca_m.xrca104,g_xrca_m.xrca113,g_xrca_m.xrca114,
                   g_xrca_m.xrca123,g_xrca_m.xrca124,g_xrca_m.xrca133,g_xrca_m.xrca134,
                   g_xrca_m.xrca108,g_xrca_m.xrca118,g_xrca_m.xrca128,g_xrca_m.xrca138
END FUNCTION]]>
