  <section id="axrt380.update_b" ver="34" status="" src="s" readonly="">
    <![CDATA[#+ 修改單身後其他table連動
PRIVATE FUNCTION axrt380_update_b(ps_table,ps_keys,ps_keys_bak,ps_page)
   DEFINE ps_table         STRING
   DEFINE ps_page          STRING
   DEFINE ps_keys          DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ps_keys_bak      DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ls_group         STRING
   DEFINE li_idx           LIKE type_t.num10 
   DEFINE lb_chk           BOOLEAN
   DEFINE l_new_key        DYNAMIC ARRAY OF STRING
   DEFINE l_old_key        DYNAMIC ARRAY OF STRING
   DEFINE l_field_key      DYNAMIC ARRAY OF STRING
   #add-point:update_b段define
   {<point name="update_b.define" edit="s"/>}
   #end add-point   
   #add-point:update_b段define(客製用)
   {<point name="update_b.define_customerization" edit="c"/>}
   #end add-point   
   
   LET g_update = TRUE   
   
   #判斷key是否有改變
   LET lb_chk = TRUE
   FOR li_idx = 1 TO ps_keys.getLength()
      IF ps_keys[li_idx] <> ps_keys_bak[li_idx] THEN
         LET lb_chk = FALSE
         EXIT FOR
      END IF
   END FOR
   
   #不需要做處理
   IF lb_chk THEN
      RETURN
   END IF
   
   #判斷是否是同一群組的table
   LET ls_group = "'1','3',"
   IF ls_group.getIndexOf(ps_page,1) > 0 AND ps_table <> "xrcb_t" THEN
      #add-point:update_b段修改前
      {<point name="update_b.before_update" mark="Y"/>}
      #end add-point 
      
      #將遮罩欄位還原
      CALL axrt380_xrcb_t_mask_restore('restore_mask_o')
               
      UPDATE xrcb_t 
         SET (xrcbld,xrcbdocno,
              xrcbseq
              ,xrcborga,xrcb001,xrcb002,xrcb003,xrcb018,xrcb004,xrcb005,xrcb007,xrcb020,xrcb031,xrcb006,xrcb022,xrcb101,xrcb103,xrcb104,xrcb105,xrcb111,xrcb113,xrcb114,xrcb115,xrcb027,xrcb028,xrcb047,xrcb029,xrcb021,xrcb048,xrcb008,xrcb013,xrcb014,xrcb017,xrcb019,xrcb023,xrcb025,xrcb026,xrcb100,xrcb106,xrcb116,xrcb117,xrcb118,xrcb119,xrcb126,xrcb136,xrcblegl,xrcbsite,xrcb030,xrcb009,xrcb102,xrcb018,xrcb123,xrcb124,xrcb125,xrcb133,xrcb134,xrcb135) 
              = 
             (ps_keys[1],ps_keys[2],ps_keys[3]
              ,g_xrcb_d[g_detail_idx].xrcborga,g_xrcb_d[g_detail_idx].xrcb001,g_xrcb_d[g_detail_idx].xrcb002, 
                  g_xrcb_d[g_detail_idx].xrcb003,g_xrcb_d[g_detail_idx].xrcb018,g_xrcb_d[g_detail_idx].xrcb004, 
                  g_xrcb_d[g_detail_idx].xrcb005,g_xrcb_d[g_detail_idx].xrcb007,g_xrcb_d[g_detail_idx].xrcb020, 
                  g_xrcb_d[g_detail_idx].xrcb031,g_xrcb_d[g_detail_idx].xrcb006,g_xrcb_d[g_detail_idx].xrcb022, 
                  g_xrcb_d[g_detail_idx].xrcb101,g_xrcb_d[g_detail_idx].xrcb103,g_xrcb_d[g_detail_idx].xrcb104, 
                  g_xrcb_d[g_detail_idx].xrcb105,g_xrcb_d[g_detail_idx].xrcb111,g_xrcb_d[g_detail_idx].xrcb113, 
                  g_xrcb_d[g_detail_idx].xrcb114,g_xrcb_d[g_detail_idx].xrcb115,g_xrcb_d[g_detail_idx].xrcb027, 
                  g_xrcb_d[g_detail_idx].xrcb028,g_xrcb_d[g_detail_idx].xrcb047,g_xrcb_d[g_detail_idx].xrcb029, 
                  g_xrcb_d[g_detail_idx].xrcb021,g_xrcb_d[g_detail_idx].xrcb048,g_xrcb_d[g_detail_idx].xrcb008, 
                  g_xrcb_d[g_detail_idx].xrcb013,g_xrcb_d[g_detail_idx].xrcb014,g_xrcb_d[g_detail_idx].xrcb017, 
                  g_xrcb_d[g_detail_idx].xrcb019,g_xrcb_d[g_detail_idx].xrcb023,g_xrcb_d[g_detail_idx].xrcb025, 
                  g_xrcb_d[g_detail_idx].xrcb026,g_xrcb_d[g_detail_idx].xrcb100,g_xrcb_d[g_detail_idx].xrcb106, 
                  g_xrcb_d[g_detail_idx].xrcb116,g_xrcb_d[g_detail_idx].xrcb117,g_xrcb_d[g_detail_idx].xrcb118, 
                  g_xrcb_d[g_detail_idx].xrcb119,g_xrcb_d[g_detail_idx].xrcb126,g_xrcb_d[g_detail_idx].xrcb136, 
                  g_xrcb_d[g_detail_idx].xrcblegl,g_xrcb_d[g_detail_idx].xrcbsite,g_xrcb_d[g_detail_idx].xrcb030, 
                  g_xrcb_d[g_detail_idx].xrcb009,g_xrcb_d[g_detail_idx].xrcb102,g_xrcb_d[g_detail_idx].xrcb018, 
                  g_xrcb3_d[g_detail_idx].xrcb123,g_xrcb3_d[g_detail_idx].xrcb124,g_xrcb3_d[g_detail_idx].xrcb125, 
                  g_xrcb3_d[g_detail_idx].xrcb133,g_xrcb3_d[g_detail_idx].xrcb134,g_xrcb3_d[g_detail_idx].xrcb135)  

         WHERE xrcbent = g_enterprise AND xrcbld = ps_keys_bak[1] AND xrcbdocno = ps_keys_bak[2] AND xrcbseq = ps_keys_bak[3]
      #add-point:update_b段修改中
      {<point name="update_b.m_update"/>}
