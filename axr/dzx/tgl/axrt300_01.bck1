      #add-point:insert_b段新增前 name="insert_b.b_insert"
      {<point name="insert_b.b_insert" mark="Y"/>}
      #end add-point    
      INSERT INTO xrcc_t
                  (xrccent,
                   xrccld,xrccdocno,xrccseq,xrcc001
