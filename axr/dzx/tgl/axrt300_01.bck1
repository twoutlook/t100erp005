{<section id="axrt300_01.update_b" >}
#+ 修改單身後其他table連動
PRIVATE FUNCTION axrt300_01_update_b(ps_table,ps_keys,ps_keys_bak,ps_page)
   #add-point:update_b段define(客製用) name="update_b.define_customerization"
   {<point name="update_b.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE ps_page          STRING
   DEFINE ps_table         STRING
   DEFINE ps_keys          DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ps_keys_bak      DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ls_group         STRING
   DEFINE li_idx           LIKE type_t.num10
   DEFINE lb_chk           BOOLEAN
   DEFINE l_new_key        DYNAMIC ARRAY OF STRING
   DEFINE l_old_key        DYNAMIC ARRAY OF STRING
   DEFINE l_field_key      DYNAMIC ARRAY OF STRING
   #add-point:update_b段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="update_b.define"
   {<point name="update_b.define"/>}
   #end add-point     
   
   #add-point:Function前置處理  name="update_b.pre_function"
   {<point name="update_b.pre_function"/>}
   #end add-point
   
   #比對新舊值, 判斷key是否有改變
   LET lb_chk = TRUE
   FOR li_idx = 1 TO ps_keys.getLength()
      IF ps_keys[li_idx] <> ps_keys_bak[li_idx] THEN
         LET lb_chk = FALSE
         EXIT FOR
      END IF
   END FOR
   
   #若key無變動, 不需要做處理
   IF lb_chk THEN
      RETURN
   END IF
    
   #若key有變動, 則連動其他table的資料   
   #判斷是否是同一群組的table
   LET ls_group = "xrcc_t,"
   IF ls_group.getIndexOf(ps_table,1) > 0 AND ps_table <> "xrcc_t" THEN
      #add-point:update_b段修改前 name="update_b.b_update"
      {<point name="update_b.b_update" mark="Y"/>}
      #end add-point     
      UPDATE xrcc_t 
         SET (xrccld,xrccdocno,xrccseq,xrcc001
              ,xrcc015,xrcc003,xrcc004,xrcc009,xrcc108,xrcc101,xrcc118,xrcc109,xrcc102,xrcc119,xrcc113,xrcc017,xrcc016,xrcc009,xrcc108,xrcc128,xrcc129,xrcc123,xrcc138,xrcc139,xrcc133,xrcc002,xrcc005,xrcc006,xrcc008,xrcc010,xrcc011,xrcc012,xrcc013,xrcc014,xrcc100,xrcc103,xrcc104,xrcc106,xrcc107,xrcc114,xrcc105,xrcc115,xrcc116,xrcc117,xrcc120,xrcc121,xrcc122,xrcc124,xrcc125,xrcc126,xrcc127,xrcc130,xrcc131,xrcc132,xrcc134,xrcc135,xrcc136,xrcc137,xrcccomp,xrcclegl,xrccsite) 
              = 
             (ps_keys[1],ps_keys[2],ps_keys[3],ps_keys[4]
              ,g_xrcc_d[l_ac].xrcc015,g_xrcc_d[l_ac].xrcc003,g_xrcc_d[l_ac].xrcc004,g_xrcc_d[l_ac].xrcc009, 
                  g_xrcc_d[l_ac].xrcc108,g_xrcc_d[l_ac].xrcc101,g_xrcc_d[l_ac].xrcc118,g_xrcc_d[l_ac].xrcc109, 
                  g_xrcc_d[l_ac].xrcc102,g_xrcc_d[l_ac].xrcc119,g_xrcc_d[l_ac].xrcc113,g_xrcc_d[l_ac].xrcc017, 
                  g_xrcc_d[l_ac].xrcc016,g_xrcc_d[l_ac].xrcc009,g_xrcc_d[l_ac].xrcc108,g_xrcc2_d[l_ac].xrcc128, 
                  g_xrcc2_d[l_ac].xrcc129,g_xrcc2_d[l_ac].xrcc123,g_xrcc2_d[l_ac].xrcc138,g_xrcc2_d[l_ac].xrcc139, 
                  g_xrcc2_d[l_ac].xrcc133,g_xrcc3_d[l_ac].xrcc002,g_xrcc3_d[l_ac].xrcc005,g_xrcc3_d[l_ac].xrcc006, 
                  g_xrcc3_d[l_ac].xrcc008,g_xrcc3_d[l_ac].xrcc010,g_xrcc3_d[l_ac].xrcc011,g_xrcc3_d[l_ac].xrcc012, 
                  g_xrcc3_d[l_ac].xrcc013,g_xrcc3_d[l_ac].xrcc014,g_xrcc3_d[l_ac].xrcc100,g_xrcc3_d[l_ac].xrcc103, 
                  g_xrcc3_d[l_ac].xrcc104,g_xrcc3_d[l_ac].xrcc106,g_xrcc3_d[l_ac].xrcc107,g_xrcc3_d[l_ac].xrcc114, 
                  g_xrcc3_d[l_ac].xrcc105,g_xrcc3_d[l_ac].xrcc115,g_xrcc3_d[l_ac].xrcc116,g_xrcc3_d[l_ac].xrcc117, 
                  g_xrcc3_d[l_ac].xrcc120,g_xrcc3_d[l_ac].xrcc121,g_xrcc3_d[l_ac].xrcc122,g_xrcc3_d[l_ac].xrcc124, 
                  g_xrcc3_d[l_ac].xrcc125,g_xrcc3_d[l_ac].xrcc126,g_xrcc3_d[l_ac].xrcc127,g_xrcc3_d[l_ac].xrcc130, 
                  g_xrcc3_d[l_ac].xrcc131,g_xrcc3_d[l_ac].xrcc132,g_xrcc3_d[l_ac].xrcc134,g_xrcc3_d[l_ac].xrcc135, 
                  g_xrcc3_d[l_ac].xrcc136,g_xrcc3_d[l_ac].xrcc137,g_xrcc3_d[l_ac].xrcccomp,g_xrcc3_d[l_ac].xrcclegl, 
                  g_xrcc3_d[l_ac].xrccsite) 
         WHERE xrccent = g_enterprise AND xrccld = ps_keys_bak[1] AND xrccdocno = ps_keys_bak[2] AND xrccseq = ps_keys_bak[3] AND xrcc001 = ps_keys_bak[4]
      #add-point:update_b段修改中 name="update_b.m_update"
      {<point name="update_b.m_update"/>}
      #end add-point 
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "xrcc_t" 
            LET g_errparam.code   = "std-00009" 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         WHEN SQLCA.sqlcode #其他錯誤
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "xrcc_t:",SQLERRMESSAGE 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         OTHERWISE
            
      END CASE
      #add-point:update_b段修改後 name="update_b.a_update"
      {<point name="update_b.a_update"/>}
      #end add-point 
      RETURN
   END IF
   
 
   
END FUNCTION

