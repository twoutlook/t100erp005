   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN axrt470_cl USING g_enterprise,g_xrep_m.xrepdocno,g_xrep_m.xrepld
   IF STATUS THEN
      CLOSE axrt470_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN axrt470_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE axrt470_master_referesh USING g_xrep_m.xrepdocno,g_xrep_m.xrepld INTO g_xrep_m.xrepsite,g_xrep_m.xrepld, 
       g_xrep_m.xrep003,g_xrep_m.xrepdocno,g_xrep_m.xrepdocdt,g_xrep_m.xrep001,g_xrep_m.xrep002,g_xrep_m.xrep004, 
       g_xrep_m.xrep005,g_xrep_m.xrepstus,g_xrep_m.xrepownid,g_xrep_m.xrepowndp,g_xrep_m.xrepcrtid,g_xrep_m.xrepcrtdp, 
       g_xrep_m.xrepcrtdt,g_xrep_m.xrepmodid,g_xrep_m.xrepmoddt,g_xrep_m.xrepcnfid,g_xrep_m.xrepcnfdt, 
       g_xrep_m.xrepownid_desc,g_xrep_m.xrepowndp_desc,g_xrep_m.xrepcrtid_desc,g_xrep_m.xrepcrtdp_desc, 
       g_xrep_m.xrepmodid_desc,g_xrep_m.xrepcnfid_desc
   
 
   #檢查是否允許此動作
   IF NOT axrt470_action_chk() THEN
      CLOSE axrt470_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_xrep_m.xrepsite,g_xrep_m.xrepsite_desc,g_xrep_m.xrepld,g_xrep_m.xrepld_desc,g_xrep_m.xrep003, 
       g_xrep_m.xrep003_desc,g_xrep_m.xrepdocno,g_xrep_m.xrepdocdt,g_xrep_m.xrep001,g_xrep_m.xrep002, 
       g_xrep_m.xrep004,g_xrep_m.xrep005,g_xrep_m.xrepstus,g_xrep_m.xrepownid,g_xrep_m.xrepownid_desc, 
       g_xrep_m.xrepowndp,g_xrep_m.xrepowndp_desc,g_xrep_m.xrepcrtid,g_xrep_m.xrepcrtid_desc,g_xrep_m.xrepcrtdp, 
       g_xrep_m.xrepcrtdp_desc,g_xrep_m.xrepcrtdt,g_xrep_m.xrepmodid,g_xrep_m.xrepmodid_desc,g_xrep_m.xrepmoddt, 
       g_xrep_m.xrepcnfid,g_xrep_m.xrepcnfid_desc,g_xrep_m.xrepcnfdt
 
   CASE g_xrep_m.xrepstus
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
      WHEN "X"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   {<point name="statechange.after_refresh"/>}
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_xrep_m.xrepstus
            
            WHEN "Y"
               HIDE OPTION "confirmed"
            WHEN "N"
               HIDE OPTION "unconfirmed"
            WHEN "X"
               HIDE OPTION "invalid"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      {<point name="statechange.before_menu"/>}
      #end add-point
      
      
	  
      ON ACTION confirmed
         IF cl_auth_chk_act("confirmed") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.confirmed"
            {<point name="statechange.confirmed"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION unconfirmed
         IF cl_auth_chk_act("unconfirmed") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.unconfirmed"
            {<point name="statechange.unconfirmed"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION invalid
         IF cl_auth_chk_act("invalid") THEN
            LET lc_state = "X"
            #add-point:action控制 name="statechange.invalid"
            {<point name="statechange.invalid"/>}
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      {<point name="statechange.more_control"/>}
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "Y" 
      AND lc_state <> "N"
      AND lc_state <> "X"
      ) OR 
      g_xrep_m.xrepstus = lc_state OR cl_null(lc_state) THEN
      CLOSE axrt470_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   {<point name="statechange.b_update"/>}
   #end add-point
   
   LET g_xrep_m.xrepmodid = g_user
   LET g_xrep_m.xrepmoddt = cl_get_current()
   LET g_xrep_m.xrepstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE xrep_t 
      SET (xrepstus,xrepmodid,xrepmoddt) 
        = (g_xrep_m.xrepstus,g_xrep_m.xrepmodid,g_xrep_m.xrepmoddt)     
    WHERE xrepent = g_enterprise AND xrepdocno = g_xrep_m.xrepdocno
      AND xrepld = g_xrep_m.xrepld
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE axrt470_master_referesh USING g_xrep_m.xrepdocno,g_xrep_m.xrepld INTO g_xrep_m.xrepsite, 
          g_xrep_m.xrepld,g_xrep_m.xrep003,g_xrep_m.xrepdocno,g_xrep_m.xrepdocdt,g_xrep_m.xrep001,g_xrep_m.xrep002, 
          g_xrep_m.xrep004,g_xrep_m.xrep005,g_xrep_m.xrepstus,g_xrep_m.xrepownid,g_xrep_m.xrepowndp, 
          g_xrep_m.xrepcrtid,g_xrep_m.xrepcrtdp,g_xrep_m.xrepcrtdt,g_xrep_m.xrepmodid,g_xrep_m.xrepmoddt, 
          g_xrep_m.xrepcnfid,g_xrep_m.xrepcnfdt,g_xrep_m.xrepownid_desc,g_xrep_m.xrepowndp_desc,g_xrep_m.xrepcrtid_desc, 
          g_xrep_m.xrepcrtdp_desc,g_xrep_m.xrepmodid_desc,g_xrep_m.xrepcnfid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_xrep_m.xrepsite,g_xrep_m.xrepsite_desc,g_xrep_m.xrepld,g_xrep_m.xrepld_desc, 
          g_xrep_m.xrep003,g_xrep_m.xrep003_desc,g_xrep_m.xrepdocno,g_xrep_m.xrepdocdt,g_xrep_m.xrep001, 
          g_xrep_m.xrep002,g_xrep_m.xrep004,g_xrep_m.xrep005,g_xrep_m.xrepstus,g_xrep_m.xrepownid,g_xrep_m.xrepownid_desc, 
          g_xrep_m.xrepowndp,g_xrep_m.xrepowndp_desc,g_xrep_m.xrepcrtid,g_xrep_m.xrepcrtid_desc,g_xrep_m.xrepcrtdp, 
          g_xrep_m.xrepcrtdp_desc,g_xrep_m.xrepcrtdt,g_xrep_m.xrepmodid,g_xrep_m.xrepmodid_desc,g_xrep_m.xrepmoddt, 
          g_xrep_m.xrepcnfid,g_xrep_m.xrepcnfid_desc,g_xrep_m.xrepcnfdt
   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
   {<point name="statechange.a_update"/>}
   #end add-point
 
   #add-point:statechange段結束前 name="statechange.after"
   {<point name="statechange.after"/>}
   #end add-point  
 
   CLOSE axrt470_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL axrt470_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION
 
 
 
