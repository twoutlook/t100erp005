      END IF
      IF g_detail_idx = 0 AND g_xreq2_d.getLength() <> 0 THEN
         LET g_detail_idx = 1
      END IF
      DISPLAY g_detail_idx TO FORMONLY.idx
      DISPLAY g_xreq2_d.getLength() TO FORMONLY.cnt
   END IF
   IF g_current_page = 3 THEN
      LET g_detail_idx = g_curr_diag.getCurrentRow("s_detail3")
      IF g_detail_idx > g_xreq3_d.getLength() THEN
         LET g_detail_idx = g_xreq3_d.getLength()
      END IF
      IF g_detail_idx = 0 AND g_xreq3_d.getLength() <> 0 THEN
         LET g_detail_idx = 1
      END IF
      DISPLAY g_detail_idx TO FORMONLY.idx
      DISPLAY g_xreq3_d.getLength() TO FORMONLY.cnt
   END IF
 
   
