{<section id="axrt470.idx_chk" >}
#+ 顯示正確的單身資料筆數
PRIVATE FUNCTION axrt470_idx_chk()
   #add-point:idx_chk段define(客製用) name="idx_chk.define_customerization"
   {<point name="idx_chk.define_customerization" edit="c" mark="Y"/>}
   #end add-point  
