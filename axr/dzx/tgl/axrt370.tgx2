   CALL g_xrcb2_d.clear()
   CALL g_xrcb3_d.clear()
 
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF axrt370_fill_chk(1) THEN
      #切換上下筆時不重組SQL
      IF (g_action_choice = "query" OR cl_null(g_action_choice))
      #add-point:b_fill段long_sql_if name="b_fill.long_sql_if"
      {<point name="b_fill.long_sql_if"/>}
      #end add-point
      THEN
         LET g_sql = "SELECT  DISTINCT xrcbseq,xrcborga,xrcb001,xrcb002,xrcb003,xrcb018,xrcb004,xrcb005, 
             xrcb007,xrcb020,xrcb031,xrcb006,xrcb022,xrcb101,xrcb103,xrcb104,xrcb105,xrcb111,xrcb113, 
             xrcb114,xrcb115,xrcb027,xrcb028,xrcb047,xrcb029,xrcb021,xrcb023,xrcb048,xrcb008,xrcb009, 
             xrcb013,xrcb014,xrcb017,xrcb019,xrcb025,xrcb026,xrcb100,xrcb106,xrcb116,xrcb117,xrcb118, 
             xrcb119,xrcb126,xrcb136,xrcblegl,xrcbsite,xrcb030,xrcb102,xrcbseq,xrcb018,xrcb123,xrcb124, 
             xrcb125,xrcb133,xrcb134,xrcb135 ,t1.oodbl004 FROM xrcb_t",   
                     " INNER JOIN xrca_t ON xrcaent = " ||g_enterprise|| " AND xrcald = xrcbld ",
                     " AND xrcadocno = xrcbdocno ",
 
                     #"",
                     
                     "",
                     #下層單身所需的join條件
 
                                    " LEFT JOIN oodbl_t t1 ON t1.oodblent="||g_enterprise||" AND t1.oodbl001='' AND t1.oodbl002=xrcb020 AND t1.oodbl003='"||g_dlang||"' ",
 
                     " WHERE xrcbent=? AND xrcbld=? AND xrcbdocno=?"
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before name="b_fill.body.fill_sql"
         {<point name="b_fill.body.fill_sql"/>}
         #end add-point
         IF NOT cl_null(g_wc2_table1) THEN
            LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY xrcb_t.xrcbseq"
         
         #add-point:單身填充控制 name="b_fill.sql"
         {<point name="b_fill.sql"/>}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE axrt370_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR axrt370_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
   #  OPEN b_fill_cs USING g_enterprise,g_xrca_m.xrcald,g_xrca_m.xrcadocno   #(ver:78)
                                               
      FOREACH b_fill_cs USING g_enterprise,g_xrca_m.xrcald,g_xrca_m.xrcadocno INTO g_xrcb_d[l_ac].xrcbseq, 
          g_xrcb_d[l_ac].xrcborga,g_xrcb_d[l_ac].xrcb001,g_xrcb_d[l_ac].xrcb002,g_xrcb_d[l_ac].xrcb003, 
          g_xrcb_d[l_ac].xrcb018,g_xrcb_d[l_ac].xrcb004,g_xrcb_d[l_ac].xrcb005,g_xrcb_d[l_ac].xrcb007, 
          g_xrcb_d[l_ac].xrcb020,g_xrcb_d[l_ac].xrcb031,g_xrcb_d[l_ac].xrcb006,g_xrcb_d[l_ac].xrcb022, 
          g_xrcb_d[l_ac].xrcb101,g_xrcb_d[l_ac].xrcb103,g_xrcb_d[l_ac].xrcb104,g_xrcb_d[l_ac].xrcb105, 
          g_xrcb_d[l_ac].xrcb111,g_xrcb_d[l_ac].xrcb113,g_xrcb_d[l_ac].xrcb114,g_xrcb_d[l_ac].xrcb115, 
          g_xrcb_d[l_ac].xrcb027,g_xrcb_d[l_ac].xrcb028,g_xrcb_d[l_ac].xrcb047,g_xrcb_d[l_ac].xrcb029, 
          g_xrcb_d[l_ac].xrcb021,g_xrcb_d[l_ac].xrcb023,g_xrcb_d[l_ac].xrcb048,g_xrcb_d[l_ac].xrcb008, 
          g_xrcb_d[l_ac].xrcb009,g_xrcb_d[l_ac].xrcb013,g_xrcb_d[l_ac].xrcb014,g_xrcb_d[l_ac].xrcb017, 
          g_xrcb_d[l_ac].xrcb019,g_xrcb_d[l_ac].xrcb025,g_xrcb_d[l_ac].xrcb026,g_xrcb_d[l_ac].xrcb100, 
          g_xrcb_d[l_ac].xrcb106,g_xrcb_d[l_ac].xrcb116,g_xrcb_d[l_ac].xrcb117,g_xrcb_d[l_ac].xrcb118, 
          g_xrcb_d[l_ac].xrcb119,g_xrcb_d[l_ac].xrcb126,g_xrcb_d[l_ac].xrcb136,g_xrcb_d[l_ac].xrcblegl, 
          g_xrcb_d[l_ac].xrcbsite,g_xrcb_d[l_ac].xrcb030,g_xrcb_d[l_ac].xrcb102,g_xrcb3_d[l_ac].xrcbseq, 
          g_xrcb3_d[l_ac].xrcb018,g_xrcb3_d[l_ac].xrcb123,g_xrcb3_d[l_ac].xrcb124,g_xrcb3_d[l_ac].xrcb125, 
          g_xrcb3_d[l_ac].xrcb133,g_xrcb3_d[l_ac].xrcb134,g_xrcb3_d[l_ac].xrcb135,g_xrcb_d[l_ac].xrcb020_desc  
            #(ver:78)
         IF SQLCA.SQLCODE THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code = SQLCA.SQLCODE 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充 name="b_fill.fill"
         {<point name="b_fill.fill"/>}
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code = 9035 
               LET g_errparam.popup = TRUE 
               CALL cl_err()
            END IF
            EXIT FOREACH
         END IF
         
         LET l_ac = l_ac + 1
      END FOREACH
      LET g_error_show = 0
   
   END IF
    
   #判斷是否填充
   IF axrt370_fill_chk(2) THEN
      IF (g_action_choice = "query" OR cl_null(g_action_choice))
         #add-point:b_fill段long_sql_if name="b_fill.body2.long_sql_if"
         {<point name="b_fill.body2.long_sql_if"/>}
         #end add-point
      THEN
         LET g_sql = "SELECT  DISTINCT xrcdseq2,xrcdseq,xrcd007,xrcd002,xrcd003,xrcd006,xrcd005,xrcd104, 
             xrcd103,xrcd105,xrcd114,xrcd113,xrcd115,xrcd009 ,t2.oodbl004 ,t3.glacl004 FROM xrcd_t", 
                
                     " INNER JOIN  xrca_t ON xrcaent = " ||g_enterprise|| " AND xrcald = xrcdld ",
                     " AND xrcadocno = xrcddocno ",
 
                     "",
                     
                                    " LEFT JOIN oodbl_t t2 ON t2.oodblent="||g_enterprise||" AND t2.oodbl001='2' AND t2.oodbl002=xrcd002 AND t2.oodbl003='"||g_dlang||"' ",
               " LEFT JOIN glacl_t t3 ON t3.glaclent="||g_enterprise||" AND t3.glacl001='' AND t3.glacl002=xrcd009 AND t3.glacl003='"||g_dlang||"' ",
 
                     " WHERE xrcdent=? AND xrcdld=? AND xrcddocno=?"   
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段fill_sql name="b_fill.body2.fill_sql"
         {<point name="b_fill.body2.fill_sql"/>}
         #end add-point
         IF NOT cl_null(g_wc2_table2) THEN
            LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY xrcd_t.xrcdseq,xrcd_t.xrcdseq2,xrcd_t.xrcd007"
         
         #add-point:單身填充控制 name="b_fill.sql2"
         {<point name="b_fill.sql2"/>}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE axrt370_pb2 FROM g_sql
         DECLARE b_fill_cs2 CURSOR FOR axrt370_pb2
      END IF
    
      LET l_ac = 1
      
   #  OPEN b_fill_cs2 USING g_enterprise,g_xrca_m.xrcald,g_xrca_m.xrcadocno   #(ver:78)
                                               
      FOREACH b_fill_cs2 USING g_enterprise,g_xrca_m.xrcald,g_xrca_m.xrcadocno INTO g_xrcb2_d[l_ac].xrcdseq2, 
