          g_xrcb2_d[l_ac].xrcd006,g_xrcb2_d[l_ac].xrcd005,g_xrcb2_d[l_ac].xrcd104,g_xrcb2_d[l_ac].xrcd103, 
          g_xrcb2_d[l_ac].xrcd105,g_xrcb2_d[l_ac].xrcd114,g_xrcb2_d[l_ac].xrcd113,g_xrcb2_d[l_ac].xrcd115, 
          g_xrcb2_d[l_ac].xrcd009,g_xrcb2_d[l_ac].xrcd002_desc,g_xrcb2_d[l_ac].xrcd009_desc   #(ver:78) 

         IF SQLCA.SQLCODE THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code = SQLCA.SQLCODE 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充 name="b_fill2.fill"
         {<point name="b_fill2.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code = 9035 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
         
      END FOREACH
   END IF
 
 
   
   #add-point:browser_fill段其他table處理 name="browser_fill.other_fill"
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_xrcb_d.deleteElement(g_xrcb_d.getLength())
   CALL g_xrcb2_d.deleteElement(g_xrcb2_d.getLength())
   CALL g_xrcb3_d.deleteElement(g_xrcb3_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE axrt370_pb
   FREE axrt370_pb2
 
 
   
   LET li_idx = l_ac
   
   #遮罩相關處理
   FOR l_ac = 1 TO g_xrcb_d.getLength()
      LET g_xrcb_d_mask_o[l_ac].* =  g_xrcb_d[l_ac].*
      CALL axrt370_xrcb_t_mask()
      LET g_xrcb_d_mask_n[l_ac].* =  g_xrcb_d[l_ac].*
   END FOR
   
   LET g_xrcb2_d_mask_o.* =  g_xrcb2_d.*
   FOR l_ac = 1 TO g_xrcb2_d.getLength()
      LET g_xrcb2_d_mask_o[l_ac].* =  g_xrcb2_d[l_ac].*
      CALL axrt370_xrcd_t_mask()
      LET g_xrcb2_d_mask_n[l_ac].* =  g_xrcb2_d[l_ac].*
   END FOR
   LET g_xrcb3_d_mask_o.* =  g_xrcb3_d.*
   FOR l_ac = 1 TO g_xrcb3_d.getLength()
      LET g_xrcb3_d_mask_o[l_ac].* =  g_xrcb3_d[l_ac].*
      CALL axrt370_xrcb_t_mask()
      LET g_xrcb3_d_mask_n[l_ac].* =  g_xrcb3_d[l_ac].*
   END FOR
 
   
   LET l_ac = li_idx
   
   CALL cl_ap_performance_next_end()
 
