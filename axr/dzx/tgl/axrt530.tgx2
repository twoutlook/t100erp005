                      " xrgfcomp = '", g_xrgf_m.xrgfcomp, "' "
                      ," AND xrgfdocno = '", g_xrgf_m.xrgfdocno, "' "
                      ," AND xrgf001 = '", g_xrgf_m.xrgf001, "' "
 
