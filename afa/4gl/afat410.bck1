################################################################################
# Descriptions...: 审核
# Memo...........:
# Usage..........: CALL s_aooi150_ins (传入参数)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION afat410_confirm()
   DEFINE  l_faah043_044         LIKE faah_t.faah043
   DEFINE  l_fabb005             LIKE fabb_t.fabb005
   DEFINE  l_fabb001             LIKE fabb_t.fabb001    
   DEFINE  r_success             LIKE type_t.chr1  
   DEFINE  l_n                   LIKE type_t.num5
   DEFINE  l_sql                 STRING    
   DEFINE  l_ooab002             LIKE ooab_t.ooab002
   DEFINE  l_fabbdocno           LIKE fabb_t.fabbdocno
   DEFINE  l_fabb000             LIKE fabb_t.fabb000
   DEFINE  l_fabb002             LIKE fabb_t.fabb002
   
   LET g_errno = ""
   LET r_success = 'Y'
#   财产编号不可为空
   LET l_n = 0
   SELECT COUNT(*) INTO l_n FROM fabb_t,faba_t
    WHERE fabaent = fabbent AND fabadocno = fabbdocno 
      AND fabbent = g_enterprise AND fabbdocno = g_faba_m.fabadocno
      AND fabb001 IS NULL
    IF l_n != 0 THEN
       LET r_success = 'N'
       LET g_errno = 'afa-00046'
       RETURN r_success
    END IF
   #工作量值不可為0
   LET l_n = 0
   SELECT COUNT(*) INTO l_n FROM fabb_t,faba_t
    WHERE fabaent = fabbent AND fabadocno = fabbdocno 
      AND fabbent = g_enterprise AND fabbdocno= g_faba_m.fabadocno
      AND (fabb005 IS NULL OR fabb005 = 0)
    IF l_n != 0 THEN
       LET r_success = 'N'
       LET g_errno = 'afa-00047'
       RETURN r_success
    END IF
    #狀態碼=N,才可進行審核動作
    IF g_faba_m.fabastus='Y' THEN
       LET r_success = 'N'
       LET g_errno = 'afa-00024'
       RETURN r_success
    END IF
    #收支日期是否小於資产模塊關帳日期,参数为设定，此逻辑后续添加
    IF NOT cl_null(g_faba_m.fabadocdt) THEN
       #單據日期不能小於關賬日期
       #S-FIN-9003
       SELECT ooab002 INTO l_ooab002 FROM ooab_t
        WHERE ooabent = g_enterprise
          AND ooab001 = 'S-FIN-9003'
          AND ooabsite = g_faba_m.fabacomp
      IF g_faba_m.fabadocdt <= l_ooab002 THEN
         LET r_success = 'N'
         LET g_errno = 'afa-00060'
         RETURN r_success
      END IF
    END IF 

#   审核时，检核已审核工作量单号的财产编号的工作量不能大于未使用量
   #取fabb_t中财编编号已录入工作量
   LET l_sql = " SELECT SUM(fabb005),fabbdocno,fabb000,fabb001,fabb002 FROM fabb_t,faba_t ",
               "  WHERE fabaent = fabbent AND fabadocno = fabbdocno ",
               "    AND fabbent = '",g_enterprise,"' AND fabadocno = '",g_faba_m.fabadocno,"'",
               "    AND faba003 = '5'",
               " GROUP BY fabbdocno,fabb000,fabb001,fabb002  ",
               " ORDER BY fabbdocno,fabb000,fabb001,fabb002  "
   PREPARE fabb001_prep FROM l_sql
   DECLARE fabb001_curs CURSOR FOR fabb001_prep
   
   FOREACH fabb001_curs INTO l_fabb005,l_fabbdocno,l_fabb000,l_fabb001,l_fabb002 
      IF cl_null(l_fabb005) THEN LET l_fabb005 = 0 END IF
      #抓取财产编号未使用工作量
      SELECT faah043-faah044 INTO l_faah043_044 FROM faah_t
       WHERE faahent = g_enterprise AND faah003 = l_fabb001
         AND faah004 = l_fabb002 AND faah001 = l_fabb000
       
      IF cl_null(l_faah043_044) THEN LET l_faah043_044 = 0 END IF
 
#      IF l_faah043_044 < l_fabb005 THEN
#         LET r_success = 'N'
#         LET g_errno = 'afa-00044' 
#         EXIT FOREACH         
#      END IF
      #审核成功，回写已使用工作量
      IF r_success = 'Y' THEN  
         UPDATE faah_t SET faah044 = faah044 + l_fabb005
          WHERE faahent = g_enterprise AND faah003 = l_fabb001
            AND faah004 = l_fabb002 AND faah001 = l_fabb000
      END IF
   END FOREACH
   
   RETURN r_success
END FUNCTION
################################################################################
# Descriptions...: 取消审核
# Memo...........:
# Usage..........: CALL s_aooi150_ins (传入参数)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION afat410_unconfirm()
   DEFINE  l_faah043_044         LIKE faah_t.faah043
   DEFINE  l_fabb005             LIKE fabb_t.fabb005
   DEFINE  l_fabb001             LIKE fabb_t.fabb001    
   DEFINE  r_success             LIKE type_t.chr1  
   DEFINE  l_n                   LIKE type_t.num5
   DEFINE  l_sql                 STRING  
   DEFINE  l_fabbdocno           LIKE fabb_t.fabbdocno
   DEFINE  l_fabb000             LIKE fabb_t.fabb000
   DEFINE  l_fabb002             LIKE fabb_t.fabb002
   DEFINE  l_ooab002             LIKE ooab_t.ooab002
   
   LET g_errno = ""
   LET r_success = 'Y'
   
   #狀態碼=Y,才可進行取消確認動作
   IF g_faba_m.fabastus = 'N' THEN
      LET g_errno = 'afa-00026'
      LET r_success = 'N'
      RETURN r_success
   END IF
   
   #收支日期是否小於資产模塊關帳日期,参数为设定，此逻辑后续添加
    IF NOT cl_null(g_faba_m.fabadocdt) THEN
       #單據日期不能小於關賬日期
       #S-FIN-9003
       SELECT ooab002 INTO l_ooab002 FROM ooab_t
        WHERE ooabent = g_enterprise
          AND ooab001 = 'S-FIN-9003'
          AND ooabsite = g_faba_m.fabacomp
      IF g_faba_m.fabadocdt <= l_ooab002 THEN
         LET r_success = 'N'
         LET g_errno = 'afa-00060'
         RETURN r_success
      END IF
    END IF
   
   #取消审核时，检核已审核工作量单号的财产编号的工作量不能大于未使用量
   #取fabb_t中财编编号已录入工作量
   LET l_sql = " SELECT SUM(fabb005),fabbdocno,fabb000,fabb001,fabb002 FROM fabb_t,faba_t ",
               "  WHERE fabaent = fabbent AND fabadocno = fabbdocno ",
               "    AND fabbent = '",g_enterprise,"' AND fabadocno = '",g_faba_m.fabadocno,"'",
               "    AND fabastus = 'Y' AND faba003 = '5'",
               " GROUP BY fabbdocno,fabb000,fabb001,fabb002 ",
               " ORDER BY fabbdocno,fabb000,fabb001,fabb002 "
               
   PREPARE fabb001_prep1 FROM l_sql
   DECLARE fabb001_curs1 CURSOR FOR fabb001_prep1
   
   FOREACH fabb001_curs1 INTO l_fabb005,l_fabbdocno,l_fabb000,l_fabb001,l_fabb002 
      IF cl_null(l_fabb005) THEN LET l_fabb005 = 0 END IF
      #抓取财产编号已使用工作量
      SELECT faah044 INTO l_faah043_044 FROM faah_t
       WHERE faahent = g_enterprise AND faah003 = l_fabb001
         AND faah004 = l_fabb002 AND faah001 = l_fabb000
       
      IF cl_null(l_faah043_044) THEN LET l_faah043_044 = 0 END IF
      #已使用工作量不能小于当前工作量
      IF l_faah043_044 < l_fabb005 THEN
         LET r_success = 'N'
         LET g_errno = 'afa-00048'    
         EXIT FOREACH         
      END IF
      
      #取消审核成功，回写已使用工作量
      IF r_success = 'Y' THEN  
         UPDATE faah_t SET faah044 = faah044 - l_fabb005
          WHERE faahent = g_enterprise AND faah003 = l_fabb001
            AND faah004 = l_fabb002 AND faah001 = l_fabb000
      END IF
   END FOREACH
   
   RETURN r_success
END FUNCTION
