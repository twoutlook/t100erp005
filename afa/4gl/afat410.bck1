   #add-point:stus修改前 name="statechange.b_update"
   CALL s_transaction_begin()
   #151125-00001#1 add start ------------------
   IF lc_state = 'X' THEN
      IF NOT cl_ask_confirm('aim-00109') THEN
         CALL s_transaction_end('N','0')   #160812-00017#6 20160822 add by beckxie
         RETURN
      END IF
