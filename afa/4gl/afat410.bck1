     #检查是否是资产中心
    SELECT COUNT(*) INTO l_n FROM ooef_t WHERE ooefent = g_enterprise AND ooef001 = p_site AND ooefstus = 'Y' AND ooef207 = 'Y'
    IF l_n = 0 THEN      
