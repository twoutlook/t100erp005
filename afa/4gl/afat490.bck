      #end add-point
      CALL afat490_show()
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = '' 
      LET g_errparam.code = 9001 
      LET g_errparam.popup = FALSE 
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL afat490_set_act_visible()   
   CALL afat490_set_act_no_visible()
   
   #將新增的資料併入搜尋條件中
   LET g_fabxdocno_t = g_fabx_m.fabxdocno
 
   
   #組合新增資料的條件
   LET g_add_browse = " fabxent = " ||g_enterprise|| " AND",
                      " fabxdocno = '", g_fabx_m.fabxdocno, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL afat490_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   #add-point:完成複製段落後 name="reproduce.after_reproduce"
   
   #end add-point
   
   CALL afat490_idx_chk()
   
   LET g_data_owner = g_fabx_m.fabxownid      
   LET g_data_dept  = g_fabx_m.fabxowndp
   
   #功能已完成,通報訊息中心
   CALL afat490_msgcentre_notify('reproduce')
 
