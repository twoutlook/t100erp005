# 财产编号+符号+卡片编号检查
PRIVATE FUNCTION afat490_fabv003_fabv004_fabv005_chk(p_cmd)
   DEFINE p_cmd           LIKE type_t.chr1
   DEFINE l_n             LIKE type_t.num5  
   DEFINE l_faah032       LIKE faah_t.faah032
   DEFINE l_faah015       LIKE faah_t.faah015


   IF cl_null(g_fabv_d[l_ac].fabv003) OR g_fabv_d[l_ac].fabv004 IS NULL THEN 
      RETURN TRUE
   END IF   
    
   IF cl_null(g_fabv_d[l_ac].fabv005) THEN
      RETURN TRUE
   END IF
   
   LET l_n = 0
   IF p_cmd = 'a' THEN
      SELECT COUNT(*) INTO l_n 
        FROM fabx_t,fabv_t
       WHERE fabxdocno = fabvdocno
         AND fabvent = g_enterprise
         AND fabvdocno = g_fabx_m.fabxdocno
         AND fabv003 = g_fabv_d[l_ac].fabv003 
         AND fabv004 = g_fabv_d[l_ac].fabv004
         AND fabv005 = g_fabv_d[l_ac].fabv005
         AND fabxstus <> 'X'
   ELSE
      SELECT COUNT(*) INTO l_n 
        FROM fabx_t,fabv_t
       WHERE fabxdocno = fabvdocno
         AND fabvent = g_enterprise
         AND fabvdocno = g_fabx_m.fabxdocno
         AND fabv003 = g_fabv_d[l_ac].fabv003 
         AND fabv004 = g_fabv_d[l_ac].fabv004
         AND fabv005 = g_fabv_d[l_ac].fabv005
         AND fabvseq <> g_fabv_d_t.fabvseq
         AND fabxstus <> 'X'
   END IF

   IF l_n > 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'afa-01095'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()
      RETURN FALSE              
   END IF  
    
   #财产编号+附号+卡片编号在单头法人的主账套存在折旧信息
   LET l_n = 0
   SELECT COUNT(1) INTO l_n   
     FROM faaj_t
    WHERE faajent = g_enterprise
      AND faaj001 = g_fabv_d[l_ac].fabv003 
      AND faaj002 = g_fabv_d[l_ac].fabv004
      AND faaj037 = g_fabv_d[l_ac].fabv005
      AND faajld = g_glaa.glaald
      
   IF l_n = 0 THEN 
      INITIALIZE g_errparam TO NULL
       LET g_errparam.code = 'afa-01093'
       LET g_errparam.extend = ''
       LET g_errparam.popup = TRUE
       CALL cl_err()
       RETURN FALSE  
   END IF
   
   LET l_faah015 = ''
   SELECT faah015 INTO l_faah015
     FROM faah_t
    WHERE faahent = g_enterprise
      AND faah003 = g_fabv_d[l_ac].fabv003     
      AND faah004 = g_fabv_d[l_ac].fabv004
      AND faah001 = g_fabv_d[l_ac].fabv005
      
   #资产状态为取得,出售,销帐,停用,被资本化,拨出不可进行调拨
   IF l_faah015 MATCHES '[0568]' OR l_faah015 MATCHES '1[01]' THEN 
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = ''
      LET g_errparam.code   = "afa-01036" 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN FALSE  
   END IF
      
   #当前作业存在未审核的财产资料不允许录
   LET l_n = 0
   SELECT COUNT(1) INTO l_n FROM fabx_t,fabv_t
    WHERE fabxent = fabvent AND fabxdocno = fabvdocno AND fabvent = g_enterprise
      AND fabvdocno <> g_fabx_m.fabxdocno AND fabx001 = '33'
      AND fabv003 = g_fabv_d[l_ac].fabv003 
      AND fabv004 = g_fabv_d[l_ac].fabv004
      AND fabv005 = g_fabv_d[l_ac].fabv005
      AND fabxstus = 'N'
    
   IF l_n > 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'afa-00363'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()
      RETURN FALSE   
   END IF
   
   #盘点中的资产不可进行调拨
   LET l_n = 0
   SELECT COUNT(1) INTO l_n
     FROM fabr_t
    WHERE fabrent = g_enterprise
      AND fabr005 = g_fabv_d[l_ac].fabv003
      AND fabr006 = g_fabv_d[l_ac].fabv004
      AND fabr007 = g_fabv_d[l_ac].fabv005
      AND fabrstus <> 'X'
      
   IF l_n > 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'afa-01041'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()
      RETURN FALSE   
   END IF 

   RETURN TRUE  
END FUNCTION
# 根据財產編號+附號自動帶出、部門、主次類型及資產狀態
PRIVATE FUNCTION afat490_get_faah()
   DEFINE l_ooab002              LIKE ooab_t.ooab002 
   DEFINE l_curr                 LIKE glaa_t.glaa001 #來源幣別  
   DEFINE l_faah005              LIKE faah_t.faah005   
   DEFINE l_amt1                 LIKE fabv_t.fabv015 #账面成本
   DEFINE l_amt2                 LIKE fabv_t.fabv015 #累折
   DEFINE l_amt3                 LIKE fabv_t.fabv015 #减值准备
   
   CALL cl_get_para(g_enterprise,g_fabx_m.fabxcomp,'S-FIN-9016') RETURNING l_ooab002
   
   SELECT faah012,faah013,faah006,faah027,faah020,faah005  
     INTO g_fabv_d[l_ac].fabv006,g_fabv_d[l_ac].fabv007,
          g_fabv_d[l_ac].fabv008,g_fabv_d[l_ac].fabv009,
          g_fabv_d[l_ac].fabv016,l_faah005          
     FROM faah_t
    WHERE faahent = g_enterprise
      AND faah003 = g_fabv_d[l_ac].fabv003   
      AND faah004 = g_fabv_d[l_ac].fabv004
      AND faah001 = g_fabv_d[l_ac].fabv005
      
   CALL afat490_get_faaj()
    
   #调拨数量=数量faah018-处置数量faaj033    
   LET g_fabv_d[l_ac].fabv010 = g_faah018 - g_faaj.faaj033
   
   IF l_faah005 <> '5' THEN   
      LET l_amt1 = g_faaj.faaj016 - g_faaj.faaj034
   ELSE   #費用類計算時不抓本幣成本
      LET l_amt1 = g_faaj.faaj020 - g_faaj.faaj034
   END IF         
   LET l_amt2 = g_faaj.faaj017 - g_faaj.faaj035
   LET l_amt3 = g_faaj.faaj021 - g_faaj.faaj027
   
   LET g_fabv_d[l_ac].fabv031 = l_amt1 - l_amt2 - l_amt3
   
   IF l_ooab002 = '1' THEN 
      #成本价
      #成本=固定资产的原价faaj016+调整成本faaj020-处置成本faaj034
      #累计折旧=（计提的累计折旧faaj017-处置的累折faaj035）/(数量faah018-处置数量faaj033）
      #减值准备=（计提的减值准备faaj021-处置的减值准备faaj027）/(数量faah018-处置数量faaj033）       
      LET g_fabv_d[l_ac].fabv011 = g_faaj.faaj016 + g_faaj.faaj020 - g_faaj.faaj034
      LET g_fabv_d[l_ac].fabv012 = g_faaj.faaj017 - g_faaj.faaj035
      LET g_fabv_d[l_ac].fabv013 = g_faaj.faaj021 - g_faaj.faaj027
      LET g_fabv_d[l_ac].fabv014 = g_faaj.faaj019
      
      #本位币二
      IF g_glaa.glaa015  = 'Y' THEN
         LET g_fabv2_d[l_ac].fabv017 = g_faaj.faaj103 + g_faaj.faaj117 - g_faaj.faaj113
         LET g_fabv2_d[l_ac].fabv018 = g_faaj.faaj104 - g_faaj.faaj114
         LET g_fabv2_d[l_ac].fabv019 = g_faaj.faaj112 - g_faaj.faaj110
         LET g_fabv2_d[l_ac].fabv020 = g_faaj.faaj105
      END IF
      
      #本位币三
      IF g_glaa.glaa019 = 'Y' THEN 
         LET g_fabv2_d[l_ac].fabv024 = g_faaj.faaj153 + g_faaj.faaj167 - g_faaj.faaj163
         LET g_fabv2_d[l_ac].fabv025 = g_faaj.faaj154 - g_faaj.faaj164
         LET g_fabv2_d[l_ac].fabv026 = g_faaj.faaj162 - g_faaj.faaj160
         LET g_fabv2_d[l_ac].fabv027 = g_faaj.faaj115
      END IF
   ELSE
      LET g_fabv_d[l_ac].fabv011 = g_faaj.faaj028
      LET g_fabv_d[l_ac].fabv012 = 0
      LET g_fabv_d[l_ac].fabv013 = 0
      LET g_fabv_d[l_ac].fabv014 = 0
      
      #本位币二
      IF g_glaa.glaa015  = 'Y' THEN
         LET g_fabv2_d[l_ac].fabv017 = g_faaj.faaj108
         LET g_fabv2_d[l_ac].fabv018 = 0
         LET g_fabv2_d[l_ac].fabv019 = 0
         LET g_fabv2_d[l_ac].fabv020 = 0
      END IF
      
      #本位币三
      IF g_glaa.glaa019 = 'Y' THEN 
         LET g_fabv2_d[l_ac].fabv024 = g_faaj.faaj158
         LET g_fabv2_d[l_ac].fabv025 = 0
         LET g_fabv2_d[l_ac].fabv026 = 0
         LET g_fabv2_d[l_ac].fabv027 = 0
      END IF
   END IF
   LET g_fabv_d[l_ac].fabv015 = 0
   LET g_fabv2_d[l_ac].fabv021 = 0
   LET g_fabv2_d[l_ac].fabv028 = 0
   
   #本位币二
   IF g_glaa.glaa015  = 'Y' THEN
      LET g_fabv2_d[l_ac].fabv022 = g_glaa.glaa016
      IF g_glaa.glaa017 = '1' THEN #依交易原幣轉換
         LET l_curr = g_fabv_d[l_ac].fabv016
      ELSE
         LET l_curr = g_glaa.glaa001
      END IF
      
                               #匯率參照表;帳套;           日期;      來源幣別
      CALL s_aooi160_get_exrate('2',g_glaa.glaald,g_fabx_m.fabxdocdt,l_curr,
                                #目的幣別;          交易金額; 匯類類型
                                g_glaa.glaa016 ,0,g_glaa.glaa018)
      RETURNING g_fabv2_d[l_ac].fabv023   
      
      CALL s_curr_round_ld('1',g_glaa.glaald,g_fabv2_d[l_ac].fabv022,g_fabv2_d[l_ac].fabv017,2) RETURNING g_sub_success,g_errno,g_fabv2_d[l_ac].fabv017
