{<section id="afat490.state_change" >}
   #應用 a09 樣板自動產生(Version:17)
#+ 確認碼變更 
PRIVATE FUNCTION afat490_statechange()
   #add-point:statechange段define(客製用) name="statechange.define_customerization"
   
   #end add-point  
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="statechange.define"
   DEFINE l_success    LIKE type_t.num5
   DEFINE r_docno      LIKE fabx_t.fabxdocno
   DEFINE l_fabxdocno  LIKE fabx_t.fabxdocno
   #end add-point  
   
   #add-point:Function前置處理 name="statechange.before"
   
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_fabx_m.fabxdocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN afat490_cl USING g_enterprise,g_fabx_m.fabxdocno
   IF STATUS THEN
      CLOSE afat490_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN afat490_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE afat490_master_referesh USING g_fabx_m.fabxdocno INTO g_fabx_m.fabxsite,g_fabx_m.fabx002, 
       g_fabx_m.fabxcomp,g_fabx_m.fabx001,g_fabx_m.fabxdocno,g_fabx_m.fabxdocdt,g_fabx_m.fabx003,g_fabx_m.fabx004, 
       g_fabx_m.fabx005,g_fabx_m.fabx006,g_fabx_m.fabx007,g_fabx_m.fabx008,g_fabx_m.fabx009,g_fabx_m.fabx010, 
       g_fabx_m.fabx011,g_fabx_m.fabx012,g_fabx_m.fabxstus,g_fabx_m.fabxownid,g_fabx_m.fabxowndp,g_fabx_m.fabxcrtid, 
       g_fabx_m.fabxcrtdp,g_fabx_m.fabxcrtdt,g_fabx_m.fabxmodid,g_fabx_m.fabxmoddt,g_fabx_m.fabxcnfid, 
       g_fabx_m.fabxcnfdt,g_fabx_m.fabxpstid,g_fabx_m.fabxpstdt,g_fabx_m.fabxsite_desc,g_fabx_m.fabx002_desc, 
       g_fabx_m.fabxcomp_desc,g_fabx_m.fabx003_desc,g_fabx_m.fabx004_desc,g_fabx_m.fabx005_desc,g_fabx_m.fabx006_desc, 
       g_fabx_m.fabx007_desc,g_fabx_m.fabx008_desc,g_fabx_m.fabxownid_desc,g_fabx_m.fabxowndp_desc,g_fabx_m.fabxcrtid_desc, 
       g_fabx_m.fabxcrtdp_desc,g_fabx_m.fabxmodid_desc,g_fabx_m.fabxcnfid_desc,g_fabx_m.fabxpstid_desc 
 
   
 
   #檢查是否允許此動作
   IF NOT afat490_action_chk() THEN
      CLOSE afat490_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_fabx_m.fabxsite,g_fabx_m.fabxsite_desc,g_fabx_m.fabx002,g_fabx_m.fabx002_desc,g_fabx_m.fabxcomp, 
       g_fabx_m.fabxcomp_desc,g_fabx_m.fabx001,g_fabx_m.fabxdocno,g_fabx_m.fabxdocdt,g_fabx_m.fabx003, 
       g_fabx_m.fabx003_desc,g_fabx_m.fabx004,g_fabx_m.fabx004_desc,g_fabx_m.fabx005,g_fabx_m.fabx005_desc, 
       g_fabx_m.fabx006,g_fabx_m.fabx006_desc,g_fabx_m.fabx007,g_fabx_m.fabx007_desc,g_fabx_m.fabx008, 
       g_fabx_m.fabx008_desc,g_fabx_m.fabx009,g_fabx_m.fabx010,g_fabx_m.fabx011,g_fabx_m.fabx012,g_fabx_m.fabxstus, 
       g_fabx_m.fabxownid,g_fabx_m.fabxownid_desc,g_fabx_m.fabxowndp,g_fabx_m.fabxowndp_desc,g_fabx_m.fabxcrtid, 
       g_fabx_m.fabxcrtid_desc,g_fabx_m.fabxcrtdp,g_fabx_m.fabxcrtdp_desc,g_fabx_m.fabxcrtdt,g_fabx_m.fabxmodid, 
       g_fabx_m.fabxmodid_desc,g_fabx_m.fabxmoddt,g_fabx_m.fabxcnfid,g_fabx_m.fabxcnfid_desc,g_fabx_m.fabxcnfdt, 
       g_fabx_m.fabxpstid,g_fabx_m.fabxpstid_desc,g_fabx_m.fabxpstdt
 
   CASE g_fabx_m.fabxstus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
      WHEN "S"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/posted.png")
      WHEN "Z"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/unposted.png")
      WHEN "X"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   IF NOT cl_null(g_fabx_m.fabxdocdt) THEN 
      IF NOT s_afa_date_chk('',g_fabx_m.fabxcomp,g_fabx_m.fabxdocdt) THEN 
         CLOSE afat490_cl
         CALL s_transaction_end('N','0')
         RETURN
      END IF 
   END IF 
   LET l_success = TRUE
   CALL cl_err_collect_init()
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_fabx_m.fabxstus
            
            WHEN "N"
               HIDE OPTION "unconfirmed"
            WHEN "Y"
               HIDE OPTION "confirmed"
            WHEN "S"
               HIDE OPTION "posted"
            WHEN "Z"
               HIDE OPTION "unposted"
            WHEN "X"
               HIDE OPTION "invalid"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      CALL cl_set_act_visible("unconfirmed,invalid,confirmed,posted,unposted",TRUE)
      CASE g_fabx_m.fabxstus
         WHEN "X"
            CALL cl_set_act_visible("unconfirmed,confirmed,invalid,posted,unposted",FALSE)
            CLOSE afat490_cl
            CALL s_transaction_end('N','0')
            RETURN
         WHEN "Y"
            CALL cl_set_act_visible("invalid,confirmed,unposted",FALSE)
         WHEN "N"
            CALL cl_set_act_visible("unconfirmed,posted,unposted",FALSE)
         WHEN "S"
            CALL cl_set_act_visible("unconfirmed,invalid,confirmed,posted",FALSE)
      END CASE
      #end add-point
      
      
	  
      ON ACTION unconfirmed
         IF cl_auth_chk_act("unconfirmed") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.unconfirmed"
            CALL s_afat490_unconf_chk(g_fabx_m.fabxdocno) RETURNING l_success
            
            IF l_success = TRUE THEN 
               CALL s_afat490_unconfirm(g_fabx_m.fabxdocno) RETURNING l_success
            END IF
            
            IF l_success = FALSE THEN 
               CALL cl_err_collect_show()
               CALL s_transaction_end('N','0') 
               RETURN
            ELSE
               CALL cl_err_collect_show()  
               CALL s_transaction_end('Y','0')
            END IF
            #end add-point
         END IF
         EXIT MENU
      ON ACTION confirmed
         IF cl_auth_chk_act("confirmed") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.confirmed"
            CALL s_afat490_conf_chk(g_fabx_m.fabxdocno) RETURNING l_success
            
            IF l_success = TRUE THEN 
               CALL s_afat490_confirm(g_fabx_m.fabxdocno) RETURNING r_docno,l_success
            END IF
            
            IF l_success = FALSE THEN 
               CALL cl_err_collect_show()
               CALL s_transaction_end('N','0') 
               RETURN
            ELSE
               CALL cl_err_collect_show()  
               CALL s_transaction_end('Y','0')
            END IF
            #end add-point
         END IF
         EXIT MENU
      ON ACTION posted
         IF cl_auth_chk_act("posted") THEN
            LET lc_state = "S"
            #add-point:action控制 name="statechange.posted"
            CALL s_afat490_post(g_fabx_m.fabxdocno) RETURNING l_success
            
            IF g_fabx_m.fabx009 = 'N' AND l_success = TRUE THEN 
               SELECT fabxdocno INTO l_fabxdocno
                 FROM fabx_t
                WHERE fabxent = g_enterprise
                  AND fabx001 = '32'
                  AND fabx010 = g_fabx_m.fabx010
                  
               CALL s_afat491_post(l_fabxdocno) RETURNING l_success
            END IF

            IF l_success = FALSE THEN 
               CALL cl_err_collect_show()
               CALL s_transaction_end('N','0') 
               RETURN
            ELSE
               CALL cl_err_collect_show()  
               CALL s_transaction_end('Y','0')
            END IF
            #end add-point
         END IF
         EXIT MENU
      ON ACTION unposted
         IF cl_auth_chk_act("unposted") THEN
            LET lc_state = "Z"
            #add-point:action控制 name="statechange.unposted"
            LET lc_state = "Y"
            
            CALL s_afat490_unpost_chk(g_fabx_m.fabxdocno) RETURNING l_success
            
            IF l_success = TRUE THEN 
               CALL s_afat490_unpost(g_fabx_m.fabxdocno) RETURNING l_success
            END IF
            
            IF l_success = FALSE THEN 
               CALL cl_err_collect_show()
               CALL s_transaction_end('N','0') 
               RETURN
            ELSE
               CALL cl_err_collect_show()  
               CALL s_transaction_end('Y','0')
            END IF
            #end add-point
         END IF
         EXIT MENU
      ON ACTION invalid
         IF cl_auth_chk_act("invalid") THEN
            LET lc_state = "X"
            #add-point:action控制 name="statechange.invalid"
            
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      AND lc_state <> "S"
      AND lc_state <> "Z"
      AND lc_state <> "X"
      ) OR 
      g_fabx_m.fabxstus = lc_state OR cl_null(lc_state) THEN
      CLOSE afat490_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   
   #end add-point
   
   LET g_fabx_m.fabxmodid = g_user
   LET g_fabx_m.fabxmoddt = cl_get_current()
   LET g_fabx_m.fabxstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE fabx_t 
      SET (fabxstus,fabxmodid,fabxmoddt) 
        = (g_fabx_m.fabxstus,g_fabx_m.fabxmodid,g_fabx_m.fabxmoddt)     
    WHERE fabxent = g_enterprise AND fabxdocno = g_fabx_m.fabxdocno
 
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "S"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/posted.png")
         WHEN "Z"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unposted.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE afat490_master_referesh USING g_fabx_m.fabxdocno INTO g_fabx_m.fabxsite,g_fabx_m.fabx002, 
          g_fabx_m.fabxcomp,g_fabx_m.fabx001,g_fabx_m.fabxdocno,g_fabx_m.fabxdocdt,g_fabx_m.fabx003, 
          g_fabx_m.fabx004,g_fabx_m.fabx005,g_fabx_m.fabx006,g_fabx_m.fabx007,g_fabx_m.fabx008,g_fabx_m.fabx009, 
          g_fabx_m.fabx010,g_fabx_m.fabx011,g_fabx_m.fabx012,g_fabx_m.fabxstus,g_fabx_m.fabxownid,g_fabx_m.fabxowndp, 
          g_fabx_m.fabxcrtid,g_fabx_m.fabxcrtdp,g_fabx_m.fabxcrtdt,g_fabx_m.fabxmodid,g_fabx_m.fabxmoddt, 
          g_fabx_m.fabxcnfid,g_fabx_m.fabxcnfdt,g_fabx_m.fabxpstid,g_fabx_m.fabxpstdt,g_fabx_m.fabxsite_desc, 
          g_fabx_m.fabx002_desc,g_fabx_m.fabxcomp_desc,g_fabx_m.fabx003_desc,g_fabx_m.fabx004_desc,g_fabx_m.fabx005_desc, 
          g_fabx_m.fabx006_desc,g_fabx_m.fabx007_desc,g_fabx_m.fabx008_desc,g_fabx_m.fabxownid_desc, 
          g_fabx_m.fabxowndp_desc,g_fabx_m.fabxcrtid_desc,g_fabx_m.fabxcrtdp_desc,g_fabx_m.fabxmodid_desc, 
          g_fabx_m.fabxcnfid_desc,g_fabx_m.fabxpstid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_fabx_m.fabxsite,g_fabx_m.fabxsite_desc,g_fabx_m.fabx002,g_fabx_m.fabx002_desc, 
          g_fabx_m.fabxcomp,g_fabx_m.fabxcomp_desc,g_fabx_m.fabx001,g_fabx_m.fabxdocno,g_fabx_m.fabxdocdt, 
          g_fabx_m.fabx003,g_fabx_m.fabx003_desc,g_fabx_m.fabx004,g_fabx_m.fabx004_desc,g_fabx_m.fabx005, 
          g_fabx_m.fabx005_desc,g_fabx_m.fabx006,g_fabx_m.fabx006_desc,g_fabx_m.fabx007,g_fabx_m.fabx007_desc, 
          g_fabx_m.fabx008,g_fabx_m.fabx008_desc,g_fabx_m.fabx009,g_fabx_m.fabx010,g_fabx_m.fabx011, 
          g_fabx_m.fabx012,g_fabx_m.fabxstus,g_fabx_m.fabxownid,g_fabx_m.fabxownid_desc,g_fabx_m.fabxowndp, 
          g_fabx_m.fabxowndp_desc,g_fabx_m.fabxcrtid,g_fabx_m.fabxcrtid_desc,g_fabx_m.fabxcrtdp,g_fabx_m.fabxcrtdp_desc, 
          g_fabx_m.fabxcrtdt,g_fabx_m.fabxmodid,g_fabx_m.fabxmodid_desc,g_fabx_m.fabxmoddt,g_fabx_m.fabxcnfid, 
          g_fabx_m.fabxcnfid_desc,g_fabx_m.fabxcnfdt,g_fabx_m.fabxpstid,g_fabx_m.fabxpstid_desc,g_fabx_m.fabxpstdt 
 
   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
   IF NOT cl_null(r_docno) THEN 
      INITIALIZE la_param.* TO NULL
      LET la_param.prog = 'afat491'
      LET la_param.param[1] = 'u'
      LET la_param.param[2] = r_docno
      LET ls_js = util.JSON.stringify( la_param )
      CALL cl_cmdrun_wait(ls_js)
      
      IF s_transaction_chk("N",0) THEN
         CALL s_transaction_begin()
      END IF
      CALL cl_err_collect_init()
      CALL s_afat491_conf_chk(r_docno) RETURNING l_success 
    
      IF l_success = TRUE THEN 
         CALL s_afat491_confirm(r_docno) RETURNING l_success  
      END IF
      
      IF l_success = FALSE THEN 
         CLOSE afat490_cl
         CALL cl_err_collect_show()
         CALL s_transaction_end('N','0') 
         RETURN
      ELSE
         CLOSE afat490_cl
         CALL cl_err_collect_show()  
         CALL s_transaction_end('Y','0')
         RETURN
      END IF
   END IF
   #end add-point
 
   #add-point:statechange段結束前 name="statechange.after"
   
   #end add-point  
 
   CLOSE afat490_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL afat490_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION
 
 
 
 
