         CALL cl_err() 
         RETURN r_success
      END IF 
   END FOREACH 
   #刪除資產變更歷程檔
   LET l_sql = "DELETE FROM fabf_t WHERE fabfent = '",g_enterprise,"' AND fabf008 = '",g_fabg_m.fabgpstdt,"'"
   PREPARE del_fabf FROM l_sql
   EXECUTE del_fabf
   IF SQLCA.SQLCODE THEN
      LET r_success = FALSE
      INITIALIZE g_errparam TO NULL
      LET g_errparam.extend = g_fabg_m.fabgdocno
      LET g_errparam.code   = sqlca.sqlcode
      LET g_errparam.popup  = TRUE 
      LET r_success = FALSE    
      CALL cl_err() 
      RETURN r_success
   END IF 
   #更新狀態碼=過帳, 過帳人=g_user,過帳日期=g_today
   UPDATE fabg_t SET fabgstus = 'Y',
                     fabgpstid = '',
                     fabgpstdt = ''
               WHERE fabgent = g_enterprise
                 AND fabgld = g_fabg_m.fabgld
                 AND fabgdocno = g_fabg_m.fabgdocno
   IF SQLCA.SQLCODE THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.extend = g_fabg_m.fabgdocno
      LET g_errparam.code   = sqlca.sqlcode
      LET g_errparam.popup  = TRUE 
      CALL cl_err() 
      LET r_success = FALSE
   END IF 
   RETURN r_success
END FUNCTION

################################################################################
# Descriptions...: 取消审核检查
# Memo...........:
# Usage..........: CALL afat511_unconf_chk()
# Input parameter: 无
# Return code....: 无
# Date & Author..: 2015/05/05 By yangxf
# Modify.........:
################################################################################
PRIVATE FUNCTION afat511_unconf_chk()
   DEFINE l_n       LIKE type_t.num5
   DEFINE l_fabp000 LIKE fabp_t.fabp000
   DEFINE l_fabp001 LIKE fabp_t.fabp001
   DEFINE l_fabp002 LIKE fabp_t.fabp002
   DEFINE l_sql     STRING 
   DEFINE r_success LIKE type_t.num5
   DEFINE l_faal003 LIKE faal_t.faal003  
   DEFINE l_faah006 LIKE faah_t.faah006
   DEFINE l_year    LIKE type_t.num5
   DEFINE l_month   LIKE type_t.num5 
   DEFINE l_site    LIKE ooef_t.ooef001
   DEFINE l_ooab002 LIKE ooab_t.ooab002
   
   LET r_success = TRUE   
   #檢核是否已經關帳
   SELECT glaacomp INTO l_site FROM glaa_t
   WHERE glaaent = g_enterprise AND glaald = g_fabg_m.fabgld
    
   CALL cl_get_para(g_enterprise,l_site,'S-FIN-9003') RETURNING l_ooab002
   IF g_fabg_m.fabgdocdt <= l_ooab002 THEN 
      INITIALIZE g_errparam TO NULL
      LET g_errparam.extend = g_fabg_m.fabgdocno
      LET g_errparam.code   = 'afa-00061'
      LET g_errparam.popup  = TRUE 
      CALL cl_err() 
      LET r_success = FALSE
      RETURN r_success
   END IF
   
   LET l_sql = " SELECT fabp000,fabp001,fabp002 FROM fabp_t",
                  " WHERE fabpent = '",g_enterprise,"'",
                  "   AND fabpld = '",g_fabg_m.fabgld,"'",
                  "   AND fabpdocno = '",g_fabg_m.fabgdocno,"'"
   PREPARE sel_fabp_pre FROM l_sql
   DECLARE sel_fabp_cs CURSOR FOR sel_fabp_pre
   FOREACH sel_fabp_cs INTO l_fabp000,l_fabp001,l_fabp002
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.extend = 'foreach:'
         LET g_errparam.code   = SQLCA.SQLCODE
         LET g_errparam.popup  = TRUE 
         CALL cl_err() 
         LET r_success = FALSE         
         RETURN r_success
      END IF
      LET l_n = 0
      SELECT COUNT(*) INTO l_n 
        FROM fabg_t,fabp_t
       WHERE fabgent = fabpent 
         AND fabgld = fabpld
         AND fabgdocno = fabpdocno
         AND fabgent = g_enterprise
         AND fabgld  = g_fabg_m.fabgld  
         AND fabgdocdt > g_fabg_m.fabgdocdt
         AND fabp000 = l_fabp000
         AND fabp001 = l_fabp001
         AND fabp002 = l_fabp002
         AND (fabgstus = 'Y'
          OR fabgstus = 'S')      
      IF l_n > 0 THEN 
         INITIALIZE g_errparam TO NULL
         LET g_errparam.extend = g_fabg_m.fabgdocno
         LET g_errparam.code   = 'afa-00332'
         LET g_errparam.popup  = TRUE 
         CALL cl_err() 
         LET r_success = FALSE
         RETURN r_success
      END IF 
      
      SELECT faah006 INTO l_faah006 FROM faah_t
       WHERE faahent = g_enterprise
         AND faah003 = l_fabp001
         AND faah004 = l_fabp002
         AND faah001 = l_fabp000
      #当月异动是否先计提折旧，N则不可取消
      SELECT faal003 INTO l_faal003 FROM faal_t 
       WHERE faalent = g_enterprise 
         AND faalld = g_fabg_m.fabgld   
         AND faal001 = l_faah006   
      IF l_faal003 = 'N' THEN
         #检查是否已有计提档
         LET l_n = 0
         LET l_year = YEAR(g_fabg_m.fabgdocdt)
         LET l_month = MONTH(g_fabg_m.fabgdocdt)         
         SELECT COUNT(*) INTO l_n FROM faam_t
           WHERE faament = g_enterprise
             AND faamld = g_fabg_m.fabgld
             AND faam000 = l_fabp000
             AND faam001 = l_fabp001
             AND faam002 = l_fabp002
             AND faam004 = l_year
             AND faam005 = l_month 
         IF l_n > 0 THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.extend = l_fabp000
            LET g_errparam.code   = 'afa-00331'
            LET g_errparam.popup  = TRUE 
            CALL cl_err() 
            LET r_success = FALSE
            RETURN r_success
         END IF   
      END IF
   END FOREACH    
   RETURN r_success   
END FUNCTION

################################################################################
# Descriptions...: 审核检查
# Memo...........:
# Usage..........: CALL afat511_conf_chk()
# Input parameter: 无
# Return code....: 无
# Date & Author..: 2015/05/05 By yangxf
# Modify.........:
################################################################################
PRIVATE FUNCTION afat511_conf_chk()
   DEFINE l_n       LIKE type_t.num5
   DEFINE r_success LIKE type_t.num5
   DEFINE l_fabp000 LIKE fabp_t.fabp000
   DEFINE l_fabp001 LIKE fabp_t.fabp001
   DEFINE l_fabp002 LIKE fabp_t.fabp002
   DEFINE l_sql     STRING 
   
   LET r_success = TRUE
   #有單頭資料但無單身資料返回报错
   LET l_n = 0
   SELECT count(*) INTO l_n FROM fabp_t
    WHERE fabpent = g_enterprise
      AND fabpld  = g_fabg_m.fabgld
      AND fabpdocno = g_fabg_m.fabgdocno
   IF l_n = 0  THEN
      LET r_success = FALSE
      INITIALIZE g_errparam TO NULL
      LET g_errparam.extend = g_fabg_m.fabgdocno
      LET g_errparam.code   = 'agl-00066'
      LET g_errparam.popup  = TRUE
      CALL cl_err()        
      LET r_success = FALSE
      RETURN r_success
   END IF
   #帐套法人设定检查
   IF NOT s_afat503_sys_chk(g_fabg_m.fabgld,g_fabg_m.fabgsite,g_fabg_m.fabgdocdt) THEN
      RETURN FALSE
   END IF
   #关账日期检查
   IF NOT s_afat503_docdt_chk(g_fabg_m.fabgld,g_fabg_m.fabgsite,g_fabg_m.fabgdocdt) THEN
      RETURN FALSE
   END IF
   
   CALL cl_err_collect_init()
   LET l_sql = " SELECT fabp000,fabp001,fabp002 FROM fabp_t",
                  " WHERE fabpent = '",g_enterprise,"'",
                  "   AND fabpld = '",g_fabg_m.fabgld,"'",
                  "   AND fabpdocno = '",g_fabg_m.fabgdocno,"'"
   PREPARE sel_fabp_pre2 FROM l_sql
   DECLARE sel_fabp_cs2 CURSOR FOR sel_fabp_pre2
   FOREACH sel_fabp_cs2 INTO l_fabp000,l_fabp001,l_fabp002
      CALL s_afat503_conf_ins_tab_chk(g_fabg_m.fabgdocno,g_fabg_m.fabgld,l_fabp001,l_fabp002,l_fabp000) RETURNING r_success
   END FOREACH 
   CALL cl_err_collect_show() 
   RETURN r_success
END FUNCTION

 
