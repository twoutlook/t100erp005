#+ QBE資料查詢
PRIVATE FUNCTION afai020_query()
   #add-point:query段define(客製用) name="query.define_customerization"
   
   #end add-point
   DEFINE ls_wc      LIKE type_t.chr500
   DEFINE ls_return  STRING
   DEFINE ls_result  STRING 
   #add-point:query段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="query.define"
   
   #end add-point 
   
   #add-point:Function前置處理  name="query.pre_function"
   
   #end add-point
   
   LET INT_FLAG = 0
   CLEAR FORM
   CALL g_faac_d.clear()
   
   LET g_qryparam.state = "c"
   
   #wc備份
   LET ls_wc = g_wc2
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      CONSTRUCT g_wc2 ON faacstus,faac001,faacl003,faac017,faac002,faac003,faac016,faac004,faac005,faac006, 
          faac007,faac008,faac009,faac010,faac011,faac012,faac013,faac014,faac015,faacownid,faacowndp, 
          faaccrtid,faaccrtdp,faaccrtdt,faacmodid,faacmoddt 
 
         FROM s_detail1[1].faacstus,s_detail1[1].faac001,s_detail1[1].faacl003,s_detail1[1].faac017, 
             s_detail1[1].faac002,s_detail1[1].faac003,s_detail1[1].faac016,s_detail1[1].faac004,s_detail1[1].faac005, 
             s_detail1[1].faac006,s_detail1[1].faac007,s_detail1[1].faac008,s_detail1[1].faac009,s_detail1[1].faac010, 
             s_detail1[1].faac011,s_detail1[1].faac012,s_detail1[1].faac013,s_detail1[1].faac014,s_detail1[1].faac015, 
             s_detail2[1].faacownid,s_detail2[1].faacowndp,s_detail2[1].faaccrtid,s_detail2[1].faaccrtdp, 
             s_detail2[1].faaccrtdt,s_detail2[1].faacmodid,s_detail2[1].faacmoddt 
      
         #應用 a11 樣板自動產生(Version:3)
         #共用欄位查詢處理  
         ##----<<faaccrtdt>>----
         AFTER FIELD faaccrtdt
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
 
         #----<<faacmoddt>>----
         AFTER FIELD faacmoddt
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
