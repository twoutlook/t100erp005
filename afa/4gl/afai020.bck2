PRIVATE FUNCTION afai020_open_afai021()
   DEFINE la_param  RECORD
          prog      STRING,
          param     DYNAMIC ARRAY OF STRING
                    END RECORD
   DEFINE ls_js     STRING

   IF cl_null(g_faac_d[l_ac].faac001) THEN
