PRIVATE FUNCTION afat516_b_fill()
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   
   #end add-point     
   DEFINE p_wc2      STRING
   DEFINE li_idx     LIKE type_t.num10
   #add-point:b_fill段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   
   #end add-point     
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   #清空第一階單身
   CALL g_fabq_d.clear()
 
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   
   #end add-point
   
   #判斷是否填充
   IF afat516_fill_chk(1) THEN
      #切換上下筆時不重組SQL
      IF (g_action_choice = "query" OR cl_null(g_action_choice))
      #add-point:b_fill段long_sql_if name="b_fill.long_sql_if"
      
      #end add-point
      THEN
         LET g_sql = "SELECT  DISTINCT fabqseq,fabq021,fabq022,fabq002,fabq019,fabq005,fabq006,fabq007, 
             fabq008,fabq009,fabq012,fabq013,fabq014,fabq015,fabq017  FROM fabq_t",   
                     " INNER JOIN faba_t ON fabaent = " ||g_enterprise|| " AND fabadocno = fabqdocno ",
 
                     #"",
                     
                     "",
                     #下層單身所需的join條件
 
                     
                     " WHERE fabqent=? AND fabqdocno=?"
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before name="b_fill.body.fill_sql"
         
         #end add-point
         IF NOT cl_null(g_wc2_table1) THEN
            LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY fabq_t.fabqseq"
         
         #add-point:單身填充控制 name="b_fill.sql"
         
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE afat516_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR afat516_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_faba_m.fabadocno
                                               
      FOREACH b_fill_cs INTO g_fabq_d[l_ac].fabqseq,g_fabq_d[l_ac].fabq021,g_fabq_d[l_ac].fabq022,g_fabq_d[l_ac].fabq002, 
          g_fabq_d[l_ac].fabq019,g_fabq_d[l_ac].fabq005,g_fabq_d[l_ac].fabq006,g_fabq_d[l_ac].fabq007, 
          g_fabq_d[l_ac].fabq008,g_fabq_d[l_ac].fabq009,g_fabq_d[l_ac].fabq012,g_fabq_d[l_ac].fabq013, 
          g_fabq_d[l_ac].fabq014,g_fabq_d[l_ac].fabq015,g_fabq_d[l_ac].fabq017
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充 name="b_fill.fill"
         SELECT fabq014,fabq015 INTO g_fabq_d[l_ac].l_fabq014,g_fabq_d[l_ac].l_fabq015
           FROM fabq_t
          WHERE fabqent = g_enterprise
            AND fabqdocno = g_fabq_d[l_ac].fabq021
            AND fabqseq = g_fabq_d[l_ac].fabq022
            
         #160426-00014#23--add--s--
         #根据财遍+卡片+附号 带出 规格型号
         SELECT faah013 INTO g_fabq_d[l_ac].faah013
           FROM faah_t
          WHERE faahent = g_enterprise
            AND faah003 = g_fabq_d[l_ac].fabq005
            AND faah004 = g_fabq_d[l_ac].fabq006
            AND faah001 = g_fabq_d[l_ac].fabq007 
         #160426-00014#23--add--e--   
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF
            EXIT FOREACH
         END IF
         
         LET l_ac = l_ac + 1
      END FOREACH
      LET g_error_show = 0
   
   END IF
    
 
   
   #add-point:browser_fill段其他table處理 name="browser_fill.other_fill"
   
   #end add-point
   
   CALL g_fabq_d.deleteElement(g_fabq_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE afat516_pb
 
   
   LET li_idx = l_ac
   
   #遮罩相關處理
   FOR l_ac = 1 TO g_fabq_d.getLength()
      LET g_fabq_d_mask_o[l_ac].* =  g_fabq_d[l_ac].*
      CALL afat516_fabq_t_mask()
      LET g_fabq_d_mask_n[l_ac].* =  g_fabq_d[l_ac].*
   END FOR
   
 
   
   LET l_ac = li_idx
   
   CALL cl_ap_performance_next_end()
 
