   ERROR ""
  
   LET g_fabadocno_t = g_faba_m.fabadocno
 
   CALL s_transaction_begin()
   
   OPEN afat516_cl USING g_enterprise,g_faba_m.fabadocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN afat516_cl:" 
