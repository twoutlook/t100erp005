# 审核/取消审核
PRIVATE FUNCTION afat516_confirm(p_state)
   DEFINE p_state        LIKE type_t.chr5
   DEFINE l_sql          STRING
   DEFINE l_fabq021      LIKE fabq_t.fabq021
   DEFINE l_fabq022      LIKE fabq_t.fabq022
   DEFINE l_fabq018      LIKE fabq_t.fabq018
   DEFINE l_fabq005      LIKE fabq_t.fabq005
   DEFINE l_fabq006      LIKE fabq_t.fabq006
   DEFINE l_fabq007      LIKE fabq_t.fabq007
   DEFINE l_fabq009      LIKE fabq_t.fabq009
   DEFINE l_faah018      LIKE faah_t.faah018
   DEFINE l_faaj033      LIKE faaj_t.faaj033
   DEFINE l_faah037      LIKE faah_t.faah037
   DEFINE l_fabacnfdt    LIKE faba_t.fabacnfdt
   DEFINE r_success      LIKE type_t.num5
   
   LET r_success = TRUE
   
   LET l_sql = "SELECT fabq021,fabq022,fabq005,fabq006,fabq007,fabq009 ",
               "  FROM fabq_t ",
               " WHERE fabqent = '",g_enterprise,"'",
               "   AND fabqdocno = '",g_faba_m.fabadocno,"'"
               
   PREPARE afat516_confirm_pre FROM l_sql
   DECLARE afat516_confirm_cur CURSOR FOR afat516_confirm_pre
   
   FOREACH afat516_confirm_cur INTO l_fabq021,l_fabq022,l_fabq005,l_fabq006,l_fabq007,l_fabq009
      SELECT faah018 INTO l_faah018
        FROM faah_t
       WHERE faahent = g_enterprise
         AND faah003 = l_fabq005
         AND faah004 = l_fabq006
         AND faah001 = l_fabq007
         
      SELECT faaj033 INTO l_faaj033
        FROM faaj_t
       WHERE faajent = g_enterprise
         AND faajld = g_glaa.glaald
         AND faaj001 = l_fabq005
         AND faaj002 = l_fabq006
         AND faaj037 = l_fabq007
     
      IF p_state = 'c' THEN 
         LET l_fabq018 = '3'
      END IF
      
      IF p_state = 'u' THEN 
         LET l_fabq018 = '2'
      END IF
      
      UPDATE fabq_t
         SET fabq018 = l_fabq018
       WHERE fabqent = g_enterprise
         AND fabqdocno = l_fabq021
         AND fabqseq = l_fabq022
         
      IF p_state = 'c' THEN    
         UPDATE faah_t SET faah037 = '1'
          WHERE faahent = g_enterprise
            AND faah003 = l_fabq005
            AND faah004 = l_fabq006
            AND faah001 = l_fabq007
      END IF
      
      IF p_state = 'u' THEN    
         IF l_fabq009 < l_faah018 - l_faaj033 THEN 
            LET l_faah037 = '3'
         END IF
         
         IF l_fabq009 = l_faah018 - l_faaj033 THEN  
            LET l_faah037 = '4'
         END IF
         
         UPDATE faah_t SET faah037 = l_faah037
          WHERE faahent = g_enterprise
            AND faah003 = l_fabq005
            AND faah004 = l_fabq006
            AND faah001 = l_fabq007 
      END IF
         
      IF SQLCA.SQLcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "upd fabq_t"
         LET g_errparam.popup = TRUE
         CALL cl_err()
      
         LET r_success = FALSE
      END IF
   END FOREACH
   
   IF p_state = 'c' THEN 
      LET l_fabacnfdt = cl_get_current()
      UPDATE faba_t SET fabacnfid = g_user,
                        fabacnfdt = l_fabacnfdt
       WHERE fabaent = g_enterprise 
         AND fabadocno = g_faba_m.fabadocno
   ELSE
      UPDATE faba_t SET fabacnfid = '',
             fabacnfdt = ''
       WHERE fabaent = g_enterprise 
         AND fabadocno = g_faba_m.fabadocno
   END IF
   
   IF SQLCA.SQLCODE THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "upd faba_t" 
      LET g_errparam.code   = SQLCA.sqlcode
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      LET r_success = FALSE
   END IF

   RETURN r_success
END FUNCTION
# 抵押单号+项次检查
PRIVATE FUNCTION afat516_fabq021_chk()
   DEFINE l_n          LIKE type_t.num5
   DEFINE l_fabq018    LIKE fabq_t.fabq018
   
   LET g_errno = ''
   
   #抵押单号+项次是否存在
   LET l_n = 0
   SELECT COUNT(*) INTO l_n
     FROM faba_t,fabq_t
    WHERE fabaent = fabqent
      AND fabadocno = fabqdocno
      AND faba003 = '28'
      AND fabadocno = g_fabq_d[l_ac].fabq021
      AND fabqseq = g_fabq_d[l_ac].fabq022
      
   IF l_n = 0 THEN 
      LET g_errno = 'afa-01011'
      RETURN
   END IF
   
   #抵押单号+项次状态是否为'已抵押'
   SELECT fabq018 INTO l_fabq018
     FROM faba_t,fabq_t
    WHERE fabaent = fabqent
      AND fabadocno = fabqdocno
      AND faba003 = '28'
      AND fabadocno = g_fabq_d[l_ac].fabq021
      AND fabqseq = g_fabq_d[l_ac].fabq022
      
   IF l_fabq018 <> '2' THEN 
      LET g_errno = 'afa-01012'
      RETURN
   END IF
   
   #抵押单号+项次是否重复
   LET l_n = 0
   SELECT COUNT(*) INTO l_n
     FROM fabq_t
    WHERE fabqent = g_enterprise
      AND fabq021 = g_fabq_d[l_ac].fabq021
      AND fabq022 = g_fabq_d[l_ac].fabq022
      AND fabqseq <> g_fabq_d[l_ac].fabqseq
   
   IF l_n > 0 THEN 
      LET g_errno = 'afa-01014'
      RETURN
   END IF
END FUNCTION
