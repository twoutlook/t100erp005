# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL afat516_immediately_conf()
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 2015/12/16 By 07166
# Modify.........:
################################################################################
PRIVATE FUNCTION afat516_immediately_conf()
#151125-00006#3--s
   DEFINE l_success         LIKE type_t.num5
   DEFINE l_doc_success     LIKE type_t.num5
   DEFINE l_slip            LIKE ooba_t.ooba002
   DEFINE l_dfin0031        LIKE type_t.chr1
   DEFINE l_count           LIKE type_t.num5
   DEFINE g_glaald          LIKE glaa_t.glaald

   SELECT glaald INTO g_glaald
     FROM glaa_t
    WHERE glaaent = g_enterprise 
      AND glaacomp = g_faba_m.fabacomp
      AND glaa014 = 'Y'

   IF cl_null(g_glaald)           THEN RETURN END IF   #無資料直接返回不做處理
   IF cl_null(g_faba_m.fabacomp)  THEN RETURN END IF   #無資料直接返回不做處理
   IF cl_null(g_faba_m.fabadocno) THEN RETURN END IF   #無資料直接返回不做處理
    
   LET l_count = 0
   SELECT COUNT(*) INTO l_count FROM fabq_t WHERE fabqent = g_enterprise
      AND fabqdocno = g_faba_m.fabadocno
      
   IF cl_null(l_count) THEN LET l_count = 0 END IF
   IF l_count = 0 THEN
      RETURN 
   END IF                   #单身無資料直接返回不做處理
   
   #取得單別
   CALL s_aooi200_fin_get_slip(g_faba_m.fabadocno) RETURNING l_success,l_slip
   #取得單別設置的"是否直接審核"參數
   CALL s_fin_get_doc_para(g_glaald,g_faba_m.fabacomp,l_slip,'D-FIN-0031') RETURNING l_dfin0031

   IF cl_null(l_dfin0031) OR l_dfin0031 MATCHES '[Nn]' THEN RETURN END IF #參數值為空或為N則不做直接審核邏輯
   IF NOT cl_ask_confirm('aap-00403') THEN RETURN END IF  #询问是否立即审核?
   
   
   CALL s_transaction_begin()
   CALL cl_err_collect_init()
   LET l_doc_success = TRUE
        
   IF g_faba_m.fabastus <> 'N' THEN 
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 'afa-01010'
               LET g_errparam.extend = g_faba_m.fabadocno
               LET g_errparam.popup = TRUE
               CALL cl_err()
               LET l_doc_success = FALSE
               CALL cl_err_collect_show()
               RETURN
            END IF
            
            CALL afat516_confirm('c') RETURNING l_success
   
   #異動狀態碼欄位/修改人/修改日期
   LET g_faba_m.fabamoddt = cl_get_current()
   LET g_faba_m.fabacnfdt = cl_get_current()
   UPDATE faba_t SET fabastus = 'Y',
                     fabamodid= g_user,
                     fabamoddt= g_faba_m.fabamoddt,
                     fabacnfid= g_user,
                     fabacnfdt= g_faba_m.fabacnfdt
    WHERE fabaent = g_enterprise AND fabacomp = g_faba_m.fabacomp
      AND fabadocno = g_faba_m.fabadocno
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      LET l_doc_success = FALSE
   END IF
   IF l_doc_success = TRUE THEN
      CALL s_transaction_end('Y',1)
   ELSE
      CALL s_transaction_end('N',1)
      CALL cl_err_collect_show()
   END IF
#151125-00006#3--e
END FUNCTION

 
