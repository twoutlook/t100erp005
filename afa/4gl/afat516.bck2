               LET g_errparam.popup = TRUE
               CALL cl_err()
               CALL cl_err_collect_show()
               CALL s_transaction_end('N','0')     #160812-00017#7 Add By Ken 160816
               RETURN
            END IF
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
 
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      AND lc_state <> "A"
      AND lc_state <> "D"
      AND lc_state <> "R"
      AND lc_state <> "W"
