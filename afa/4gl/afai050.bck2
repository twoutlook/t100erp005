   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define name="main.define_sql"
               
   #end add-point
   LET g_forupd_sql = " SELECT faaeld,'','','','','','',''", 
                      " FROM faae_t",
                      " WHERE faaeent= ? AND faaeld=? FOR UPDATE"
   #add-point:SQL_define name="main.after_define_sql"
               
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE afai050_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.faaeld,t1.glaal002",
               " FROM faae_t t0",
                              " LEFT JOIN glaal_t t1 ON t1.glaalent="||g_enterprise||" AND t1.glaalld=t0.faaeld AND t1.glaal001='"||g_dlang||"' ",
 
               " WHERE t0.faaeent = " ||g_enterprise|| " AND t0.faaeld = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define name="main.after_refresh_sql"
   
   #end add-point
   PREPARE afai050_master_referesh FROM g_sql
 
    
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call name="main.servicecall"
                              
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_afai050 WITH FORM cl_ap_formpath("afa",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL afai050_init()   
 
      #進入選單 Menu (="N")
      CALL afai050_ui_dialog() 
      
      #add-point:畫面關閉前 name="main.before_close"
                              
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_afai050
      
   END IF 
   
   CLOSE afai050_cl
   
   
 
   #add-point:作業離開前 name="main.exit"
               
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
