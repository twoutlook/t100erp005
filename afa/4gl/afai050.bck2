# Descriptions...: 整批删除
# Memo...........:
# Usage..........: CALL afai050_faae_del_1()
# Input parameter: 无
# Return code....: 无
# Date & Author..: 2015/01/21 By yangxf
# Modify.........:
################################################################################
PRIVATE FUNCTION afai050_faae_del_1()
   DEFINE l_success   LIKE type_t.chr1
   
   #詢問是否確認執行
   IF NOT cl_ask_confirm('afa-00405') THEN 
      RETURN 
   END IF 
   LET l_success = 'Y' 
   #開啟事務
   CALL s_transaction_begin()
   LET g_sql = "DELETE FROM faae_t WHERE faaeent = '",g_enterprise,"' AND ",g_wc3 CLIPPED 
   PREPARE del_faae_pre2 FROM g_sql
   EXECUTE del_faae_pre2
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = 'delete faae_t ' 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      LET l_success = 'N'
   END IF
   IF l_success = 'Y' THEN
      #執行成功
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = '' 
      LET g_errparam.code   = 'adz-00217'
      LET g_errparam.popup  = TRUE 
      CALL cl_err()  
      CALL s_transaction_end('Y','0')
   ELSE
      #執行失敗
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = '' 
      LET g_errparam.code   = 'adz-00218'
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CALL s_transaction_end('N','0')
   END IF 
END FUNCTION

 
