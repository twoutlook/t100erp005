################################################################################
PRIVATE FUNCTION afai050_faaeld_desc()
   SELECT glaacomp,glaa004,glaa008,glaa014
     INTO g_faae_m.glaacomp,g_faae_m.glaa004,g_faae_m.glaa008,g_faae_m.glaa014
     FROM glaa_t
    WHERE glaaent = g_enterprise
      AND glaald  = g_faae_m.faaeld
      AND glaastus = 'Y'
      AND (glaa008  = 'Y' OR glaa014 ='Y')
   DISPLAY BY NAME g_faae_m.glaacomp,g_faae_m.glaa004,g_faae_m.glaa008,g_faae_m.glaa014
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_faae_m.faaeld
   CALL ap_ref_array2(g_ref_fields,"SELECT glaal002 FROM glaal_t WHERE glaalent='"||g_enterprise||"' AND glaalld=? AND glaal001='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_faae_m.faaeld_desc = '', g_rtn_fields[1] , ''
   DISPLAY BY NAME g_faae_m.faaeld_desc
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_faae_m.glaacomp
   CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_faae_m.glaacomp_desc = '', g_rtn_fields[1] , ''
   DISPLAY BY NAME g_faae_m.glaacomp_desc
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_faae_m.glaa004
   CALL ap_ref_array2(g_ref_fields,"SELECT ooall004 FROM ooall_t WHERE ooallent='"||g_enterprise||"' AND ooall001='0' AND ooall002=? AND ooall003='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_faae_m.glaa004_desc = '', g_rtn_fields[1] , ''
   DISPLAY BY NAME g_faae_m.glaa004_desc

END FUNCTION
