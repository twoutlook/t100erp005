   DROP TABLE afai050_detail
   
   #add-point:單身複製前1 name="detail_reproduce.body.table1.b_insert"
               
   #end add-point
   
   #CREATE TEMP TABLE
   SELECT * FROM faae_t
    WHERE faaeent = g_enterprise AND faaeld = g_faaeld_t
 
       INTO TEMP afai050_detail
   
   #將key修正為調整後   
   UPDATE afai050_detail 
      #更新key欄位
      SET faaeld = g_faae_m.faaeld
 
      #更新共用欄位
      #應用 a13 樣板自動產生(Version:4)
       , faaeownid = g_user 
       , faaeowndp = g_dept
       , faaecrtid = g_user
       , faaecrtdp = g_dept 
       , faaecrtdt = ld_date
       , faaemodid = g_user
       , faaemoddt = ld_date
      #, faaestus = "Y" 
 
 
 
                                       
   #將資料塞回原table   
   INSERT INTO faae_t SELECT * FROM afai050_detail
   
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "reproduce:",SQLERRMESSAGE 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #add-point:單身複製中1 name="detail_reproduce.body.table1.m_insert"
               
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE afai050_detail
   
   #add-point:單身複製後1 name="detail_reproduce.body.table1.a_insert"
            
  DELETE FROM faae_t
   WHERE faaeent = g_enterprise
     AND faaeld  = g_faae_m.faaeld
     AND (faae003 NOT IN (SELECT glac002 FROM glac_t WHERE glacent=g_enterprise AND glac001=g_faae_m.glaa004)
     OR faae001 NOT IN (SELECT DISTINCT ooef001 FROM ooef_t WHERE ooefent=g_enterprise AND ooef017=g_faae_m.glaacomp))  
   
  SELECT COUNT(faaeld) INTO l_n 
    FROM faae_t
   WHERE faaeent = g_enterprise
     AND faaeld  = g_faae_m.faaeld
  IF l_n = 0 THEN
     INITIALIZE g_errparam TO NULL
     LET g_errparam.code = 'afa-00057'
     LET g_errparam.extend = ''
     LET g_errparam.popup = TRUE
     CALL cl_err()

  END IF
                                      
   #end add-point
 
 
   
 
   
   #多語言複製段落
   
   
   CALL s_transaction_end('Y','0')
   
   #已新增完, 調整資料內容(修改時使用)
   LET g_faaeld_t = g_faae_m.faaeld
 
   
   DROP TABLE afai050_detail
   
END FUNCTION
 
