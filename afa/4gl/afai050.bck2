# Input parameter: 無
# Return code....: 無
# Date & Author..: 2015/01/20 By yangxf
# Modify.........:
################################################################################
PRIVATE FUNCTION afai050_faae_upd()
   DEFINE l_faae      RECORD LIKE faae_t.*
   DEFINE l_sql       STRING 
   DEFINE l_n         LIKE type_t.num5
   DEFINE l_chk       LIKE type_t.chr1
   DEFINE l_success   LIKE type_t.chr1
   
   #詢問是否確認執行
   IF NOT cl_ask_confirm('afa-00405') THEN 
      RETURN 
   END IF 
   LET g_chk = 'N'
   LET l_success = 'Y'
   INITIALIZE l_faae.* TO NULL
   #開啟事務
   CALL s_transaction_begin()
   #預設值
   LET l_faae.faaemodid = g_user
   LET l_faae.faaemoddt = cl_get_current()
   LET l_faae.faaestus = 'Y'
   LET l_faae.faaeld = tm.faaeld
   LET l_faae.faae002 = tm.faae002
   LET l_faae.faae003 = tm.faae003
   #160923-00011#1--mark--str--lujh
   #LET g_sql = " SELECT ooeg001 FROM ooeg_t ",
   #            "  WHERE ooegent = '",g_enterprise,"'",
   #            "    AND ooegstus = 'Y' ",
   #            "    AND ooeg009 = '",tm.glaacomp,"'",
   #            "    AND ooeg006 <= '",g_today,"' AND (ooeg007 IS NULL OR ooeg007 > '",g_today,"')",
   #            "    AND ",g_wc3 CLIPPED,
   #            "  ORDER BY ooeg001 "
   #160923-00011#1--mark--end--lujh
               
   #160923-00011#1--add--str--lujh
   LET g_wc3 = cl_replace_str(g_wc3,"ooeg001","faae001")
   LET g_wc4 = cl_replace_str(g_wc4,"faac001","faae002")
   LET g_sql = " SELECT faae001,faae002 FROM faae_t ",
               "  WHERE faaeent = '",g_enterprise,"'",
               "    AND faaeld = '",l_faae.faaeld,"'",
               "    AND ",g_wc3 CLIPPED,
               "    AND ",g_wc4 CLIPPED,
