      LET l_ac = 1
      
   #  OPEN b_fill_cs USING g_enterprise,g_faba_m.fabadocno   #(ver:78)
                                               
      FOREACH b_fill_cs USING g_enterprise,g_faba_m.fabadocno INTO g_fabn_d[l_ac].fabnseq,g_fabn_d[l_ac].fabn001, 
          g_fabn_d[l_ac].fabn002,g_fabn_d[l_ac].fabn003,g_fabn_d[l_ac].fabn004,g_fabn_d[l_ac].fabn005, 
          g_fabn_d[l_ac].fabn018,g_fabn_d[l_ac].fabn006,g_fabn_d[l_ac].fabn007,g_fabn_d[l_ac].fabn008, 
          g_fabn_d[l_ac].fabn009,g_fabn_d[l_ac].fabn010,g_fabn_d[l_ac].fabn011,g_fabn_d[l_ac].fabn012, 
          g_fabn_d[l_ac].fabn013,g_fabn_d[l_ac].fabn014,g_fabn_d[l_ac].fabn015,g_fabn_d[l_ac].fabn016, 
          g_fabn_d[l_ac].fabn017,g_fabn_d[l_ac].fabn017_desc   #(ver:78)
         IF SQLCA.SQLCODE THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code = SQLCA.SQLCODE 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充 name="b_fill.fill"
         #160426-00014#23 add --add--s--
         INITIALIZE g_ref_fields TO NULL 
         LET g_ref_fields[1] = g_fabn_d[l_ac].fabn003
         LET g_ref_fields[2] = g_fabn_d[l_ac].fabn001   
         LET g_ref_fields[3] = g_fabn_d[l_ac].fabn002
         CALL ap_ref_array2(g_ref_fields," SELECT faah012,faah013 FROM faah_t WHERE faahent = "||g_enterprise||" AND faah001 = ? AND faah003 = ? AND faah004 = ? ","") RETURNING g_rtn_fields 
         LET g_fabn_d[l_ac].faah012 = g_rtn_fields[1] 
         LET g_fabn_d[l_ac].faah013 = g_rtn_fields[2] 
         DISPLAY BY NAME g_fabn_d[l_ac].faah012,g_fabn_d[l_ac].faah013
         #160426-00014#23 add --add--e--
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code = 9035 
               LET g_errparam.popup = TRUE 
               CALL cl_err()
            END IF
            EXIT FOREACH
         END IF
         
         LET l_ac = l_ac + 1
      END FOREACH
      LET g_error_show = 0
   
   END IF
    
 
   
   #add-point:browser_fill段其他table處理 name="browser_fill.other_fill"
   
   #end add-point
   
   CALL g_fabn_d.deleteElement(g_fabn_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE afat400_pb
 
   
   LET li_idx = l_ac
   
   #遮罩相關處理
   FOR l_ac = 1 TO g_fabn_d.getLength()
      LET g_fabn_d_mask_o[l_ac].* =  g_fabn_d[l_ac].*
      CALL afat400_fabn_t_mask()
      LET g_fabn_d_mask_n[l_ac].* =  g_fabn_d[l_ac].*
   END FOR
   
 
   
   LET l_ac = li_idx
   
   CALL cl_ap_performance_next_end()
 
END FUNCTION
 
