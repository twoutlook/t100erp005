PUBLIC FUNCTION afat400_site_comp_chk(p_site,p_faah001,p_faah003,p_faah004)
   DEFINE p_site           LIKE faba_t.fabasite
   DEFINE p_faah001        LIKE faah_t.faah001
   DEFINE p_faah003        LIKE faah_t.faah003
   DEFINE p_faah004        LIKE faah_t.faah004
   DEFINE l_faah028        LIKE faah_t.faah028   #所有组织
   DEFINE l_faah030        LIKE faah_t.faah030   #管理组织
   DEFINE l_result         LIKE type_t.num5
   DEFINE l_result1        LIKE type_t.num5   
  
   #根据财编+卡片编号+附号获取所有组织+管理组织
   LET l_result=0
   LET l_result1=0
   SELECT faah028,faah030 INTO l_faah028,l_faah030 FROM faah_t WHERE faahent=g_enterprise AND faah001=p_faah001 AND faah003=p_faah003 AND faah004=p_faah004
   
   SELECT COUNT(*) INTO l_result FROM ooed_t WHERE (ooed005=p_site OR ooed002=p_site) AND ooedent=g_enterprise AND ooed004=
      (select ooef017 FROM ooef_t WHERE ooefent=g_enterprise AND ooef001=l_faah028 )
  SELECT COUNT(*) INTO l_result1 FROM ooed_t WHERE (ooed005=p_site OR ooed002=p_site) AND ooedent=g_enterprise AND ooed004=
      (select ooef017 FROM ooef_t WHERE ooefent=g_enterprise AND ooef001=l_faah030 )
   IF l_result>0 AND l_result1>0 THEN
      LET l_result=1
   ELSE 
      LET l_result=0
   END IF
   RETURN l_result
         
END FUNCTION

