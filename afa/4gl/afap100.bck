# Memo...........:
# Usage..........: CALL s_aooi150_ins (传入参数)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者:20150504 By dujuan
# Modify.........:
################################################################################
PRIVATE FUNCTION afap100_get_ooef001_wc(p_wc)
   DEFINE p_wc       STRING
   DEFINE r_wc       STRING
   DEFINE tok        base.StringTokenizer
   DEFINE l_str      STRING

   LET tok = base.StringTokenizer.create(p_wc,",")
   WHILE tok.hasMoreTokens()
      IF cl_null(l_str) THEN
         LET l_str = tok.nextToken()
      ELSE
         LET l_str = l_str,"','",tok.nextToken()
      END IF      
   END WHILE   
   LET r_wc = "('",l_str,"')"
   
   RETURN r_wc
END FUNCTION

################################################################################
# Descriptions...: 描述说明:获得查询的数据
# Memo...........:
# Usage..........: CALL s_aooi150_ins (传入参数)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者：20150504 By  dujuan
# Modify.........:
################################################################################
PRIVATE FUNCTION afap100_get_date()
   DEFINE l_origin_str  STRING
   DEFINE l_sql         STRING
   DEFINE l_count       LIKE type_t.num5
   DEFINE l_faahstus    LIKE faah_t.faahstus 
   

   IF cl_null(g_master.wc) THEN LET g_master.wc = " 1=1" END IF
   
   CALL s_fin_create_account_center_tmp()
   CALL s_fin_account_center_sons_query('5',g_master.faah030,g_today,'')
   CALL s_fin_account_center_sons_str()RETURNING l_origin_str
   LET l_origin_str = l_origin_str,",",g_master.faah030
   CALL afap100_get_ooef001_wc(l_origin_str)RETURNING l_origin_str
   
   LET l_sql = "SELECT faah000,faah001,faah003,faah004,faah032 ",
               "  FROM faah_t ",
               " WHERE faahent = '",g_enterprise,"'  ",
               "   AND faahstus = 'N' ",
               "   AND faah030 IN ",l_origin_str,
               "   AND ",g_master.wc
   PREPARE afap100_prep FROM l_sql
   DECLARE afap100_curs CURSOR FOR afap100_prep

   LET l_sql = "SELECT COUNT(*) FROM (",l_sql,")"
   PREPARE afap100_count_prep FROM l_sql

   LET l_count = 0
   EXECUTE afap100_count_prep INTO l_count
   IF cl_null(l_count) THEN LET l_count = 0 END IF
   IF l_count = 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.extend = 'ins temp table:'
      LET g_errparam.code   = 'sub-01321' #'axm-00276' #160318-00005#8 mod
      LET g_errparam.popup  = TRUE
      CALL cl_err()
      LET g_success = 'N'
   END IF            
               
END FUNCTION

################################################################################
# Descriptions...: 描述说明:获得数据后进行update
# Memo...........:
# Usage..........: CALL s_aooi150_ins (传入参数)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者:20150504 By dujuan
# Modify.........:
################################################################################
PRIVATE FUNCTION afap100_run_conf()
   #150812-00010#2 mod--s
   DEFINE l_faah000 LIKE faah_t.faah000
   DEFINE l_faah001 LIKE faah_t.faah001
   DEFINE l_faah003 LIKE faah_t.faah003
   DEFINE l_faah004 LIKE faah_t.faah004
   DEFINE l_faah032 LIKE faah_t.faah032
   #150812-00010#2 mod--e
   DEFINE l_glaald  LIKE glaa_t.glaald
   DEFINE l_faahmodid LIKE faah_t.faahmodid
   DEFINE l_faahmoddt LIKE faah_t.faahmoddt
   DEFINE l_faajmodid LIKE faaj_t.faajmodid
   DEFINE l_faajmoddt LIKE faaj_t.faajmoddt
   DEFINE l_success   LIKE type_t.num5
   DEFINE l_success_1 LIKE type_t.num5
   
   LET l_faahmodid = g_user
   LET l_faahmoddt = cl_get_current()
   LET l_faajmodid = g_user
   LET l_faajmoddt = cl_get_current()
   LET g_totsuccess = 'Y'
   FOREACH afap100_curs INTO l_faah000,l_faah001,l_faah003,l_faah004,l_faah032
 
      CALL s_afai100_confirm_chk(l_faah000,l_faah001,l_faah003,l_faah004) RETURNING l_success
      IF l_success = TRUE THEN 
          CALL s_afai100_confirm(l_faah000,l_faah001,l_faah003,l_faah004) RETURNING l_success_1
          IF NOT l_success_1 THEN
            LET g_success = 'N'
          END IF
      ELSE
         LET g_success = 'N'
      END IF
      
      IF g_success = 'N' THEN
         LET g_totsuccess = 'N'
      ELSE
         UPDATE faah_t 
         SET (faahstus,faahmodid,faahmoddt) 
           = ('Y',l_faahmodid,l_faahmoddt)     
         WHERE faahent = g_enterprise 
           AND faah000 = l_faah000
           AND faah001 = l_faah001      
           AND faah003 = l_faah003      
           AND faah004 = l_faah004
         
         #獲取法人對應的主帳套
         SELECT glaald INTO l_glaald
           FROM glaa_t
          WHERE glaaent = g_enterprise
            AND glaacomp = l_faah032
            AND glaa014 = 'Y'
         UPDATE faaj_t 
         SET (faajstus,faajmodid,faajmoddt) 
           = ('Y',l_faajmodid,l_faajmoddt)   
         WHERE faajent = g_enterprise 
           AND faaj000 = l_faah000
           AND faaj037 = l_faah001      
           AND faaj001 = l_faah003
           AND faaj002 = l_faah004
           AND faajld =  l_glaald      
      END IF

   END FOREACH
   IF g_totsuccess = 'N' THEN LET g_success = 'N' END IF
END FUNCTION

