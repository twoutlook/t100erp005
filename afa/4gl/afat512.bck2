                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = 'afa-01033'
                  LET g_errparam.replace[1] = l_docno
                  LET g_errparam.extend = ''
                  LET g_errparam.popup = TRUE
                  CALL cl_err()  
               END IF
            END IF 
            
            CALL cl_err_collect_show()
            #20150701--add--end--lujh
            #end add-point
         END IF
         EXIT MENU
    
      
      
      #add-point:stus控制

      #end add-point
      
   END MENU
   
   IF (lc_state <> "N" 
      AND lc_state <> "X"
      AND lc_state <> "Y"
      ) OR 
      cl_null(lc_state) THEN
      CALL s_transaction_end('N','0')   #160812-00017#6 20160822 add by beckxie
