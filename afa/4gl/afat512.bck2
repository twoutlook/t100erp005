PRIVATE FUNCTION afat512_action_chk()


   SELECT fabrstus  INTO g_fabr_m.fabrstus
     FROM fabr_t
    WHERE fabrent = g_enterprise
      AND fabr003 = g_fabr_m.fabr003
      AND fabr004 = g_fabr_m.fabr004

   IF (g_action_choice="modify" OR g_action_choice="delete" OR g_action_choice="modify_detail")  THEN
      LET g_errno = NULL
      CASE g_fabr_m.fabrstus
        WHEN 'Z'
           LET g_errno = 'sub-01351'
        WHEN 'W'
           LET g_errno = 'sub-00180'
        WHEN 'X'
           LET g_errno = 'sub-00229'
        WHEN 'Y'
           LET g_errno = 'sub-00178'
        WHEN 'S'
           LET g_errno = 'sub-00230'
     END CASE

     IF NOT cl_null(g_errno) THEN
        INITIALIZE g_errparam TO NULL
        LET g_errparam.code = g_errno
        LET g_errparam.extend = g_fabr_m.fabr003
        LET g_errparam.popup = TRUE
        CALL cl_err()
        LET g_errno = NULL
        CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
        IF g_fabr_m.fabrstus = 'N' THEN
           CALL cl_set_act_visible("modify,delete", TRUE)
        ELSE
           CALL cl_set_act_visible("modify,delete", FALSE)
        END IF
        CALL afat512_show()
        RETURN FALSE
     END IF
   END IF

        
   #end add-point
      
   RETURN TRUE
END FUNCTION

 
