# Usage..........: CALL afat512_change_to_sql(p_wc)

# Date & Author..: 日期 By 作者 02481
# Modify.........:
################################################################################
PRIVATE FUNCTION afat512_change_to_sql(p_wc)
  DEFINE p_wc       STRING
   DEFINE r_wc       STRING
   DEFINE tok        base.StringTokenizer
   DEFINE l_str      STRING

   LET tok = base.StringTokenizer.create(p_wc,",")
   WHILE tok.hasMoreTokens()
      IF cl_null(l_str) THEN
         LET l_str = tok.nextToken()
      ELSE
         LET l_str = l_str,"','",tok.nextToken()
      END IF
   END WHILE
   LET r_wc = "'",l_str,"'"

   RETURN r_wc
END FUNCTION
# 如果有部门，人员，存放位置的信息资料的变更,产生一笔afat421的资料
# 20150701 add lujh
PRIVATE FUNCTION afat512_ins_afat421()
   DEFINE l_docno          LIKE faba_t.fabadocno
   DEFINE l_docdt          LIKE faba_t.fabadocdt   
   DEFINE l_ld             LIKE glaa_t.glaald
   DEFINE l_success        LIKE type_t.num5
   DEFINE l_faba           RECORD LIKE faba_t.*
   DEFINE l_fabb           RECORD LIKE fabb_t.*
   DEFINE r_success        LIKE type_t.num5
   DEFINE r_docno          LIKE faba_t.fabadocno
   
   LET r_success = TRUE
   LET r_docno = ''
   
   CALL afat512_02(g_fabr_m.fabrcomp) RETURNING l_ld,l_docno,l_docdt
   
   IF cl_null(l_docno) AND cl_null(l_docdt) THEN 
      RETURN r_success,r_docno
   END IF
   
   #插入单头
   LET l_faba.fabaent = g_enterprise
   LET l_faba.fabasite = g_fabr_m.fabr001
   LET l_faba.faba001 = g_user
   LET l_faba.fabacomp = g_fabr_m.fabrcomp
   LET l_faba.faba004 = g_user
   LET l_faba.faba005 = g_dept
   LET l_faba.faba006 = g_today
   LET l_faba.faba003 = '27'
   LET l_faba.fabadocdt = l_docdt
   LET l_faba.fabastus = 'N'
   CALL s_aooi200_fin_gen_docno(l_ld,'','',l_docno,l_faba.fabadocdt,'afat421')
   RETURNING l_success,l_faba.fabadocno
   IF l_success  = 0  THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'apm-00003'
      LET g_errparam.extend = l_docno
      LET g_errparam.popup = TRUE
      CALL cl_err()
    
      LET r_success = FALSE
      RETURN r_success,r_docno
   END IF 
   
   INSERT INTO faba_t VALUES(l_faba.*)
   
   IF SQLCA.SQLcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = 'ins faba_t'
      LET g_errparam.code   = SQLCA.sqlcode
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
  
      LET r_success = FALSE
      RETURN r_success,r_docno                           
   END IF 
   
   #插入单身
   LET l_fabb.fabbent = g_enterprise 
   LET l_fabb.fabbcomp = g_fabr_m.fabrcomp
   LET l_fabb.fabbdocno = l_faba.fabadocno
   SELECT MAX(fabbseq) + 1 
     INTO l_fabb.fabbseq
     FROM fabb_t
    WHERE fabbent = g_enterprise
      AND fabbdocno = l_fabb.fabbdocno
      
   IF cl_null(l_fabb.fabbseq) THEN 
      LET l_fabb.fabbseq = 1
   END IF
   
   LET l_fabb.fabblegl = l_fabb.fabbcomp
   LET l_fabb.fabb000 = g_fabr_m.fabr007
   LET l_fabb.fabb001 = g_fabr_m.fabr005
   LET l_fabb.fabb002 = g_fabr_m.fabr006 
   LET l_fabb.fabb010 = g_fabr_m.fabr014
   LET l_fabb.fabb003 = g_fabr_m.fabr024
   LET l_fabb.fabb011 = g_fabr_m.fabr015
   LET l_fabb.fabb004 = g_fabr_m.fabr025
   LET l_fabb.fabb012 = g_fabr_m.fabr016
   LET l_fabb.fabb009 = g_fabr_m.fabr026
   LET l_fabb.fabb023 = g_fabr_m.fabr019
   LET l_fabb.fabb024 = g_fabr_m.fabr028
   LET l_fabb.fabb025 = g_fabr_m.fabr018
   LET l_fabb.fabb026 = g_fabr_m.fabr027
   LET l_fabb.fabb027 = g_fabr_m.fabr020
   LET l_fabb.fabb028 = g_fabr_m.fabr029
   
   INSERT INTO fabb_t VALUES(l_fabb.*)
   
   IF SQLCA.SQLcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = 'ins fabb_t'
      LET g_errparam.code   = SQLCA.sqlcode
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
  
      LET r_success = FALSE
      RETURN r_success,r_docno                           
   END IF 
   
   LET r_docno = l_faba.fabadocno
   
   RETURN r_success,r_docno
END FUNCTION
