            LET lc_state = "N"
            #add-point:action控制

            #end add-point
         END IF
         EXIT MENU
      ON ACTION invalid
         IF cl_auth_chk_act("invalid") THEN
            LET lc_state = "X"
            #add-point:action控制

            #end add-point
         END IF
         EXIT MENU
      ON ACTION confirmed
         IF cl_auth_chk_act("confirmed") THEN
            LET lc_state = "Y"
            #add-point:action控制
            #20150701--add--str--lujh
            CALL cl_err_collect_init()

            #160405-00001#1 add--str---
            LET l_cnt = 0
            SELECT COUNT(*) INTO l_cnt
              FROM fabr_t
             WHERE fabrent = g_enterprise
               AND fabr003 = g_fabr_m.fabr003           
            #160405-00001#1 add--end---

            SELECT COUNT(*) INTO l_n
              FROM fabr_t
             WHERE fabrent = g_enterprise
               AND fabr003 = g_fabr_m.fabr003
               AND fabr023 - fabr012 <> 0
            
            #CALL s_transaction_begin() #160329-00025#1 
            
            IF l_n > 0  THEN 
               IF cl_ask_confirm('afa-01029') THEN
                  CALL afat512_confirm() RETURNING l_success,l_docno1,l_docno2
               END IF
            #160405-00001#1 add--str---   
            ELSE
               IF l_n = 0 AND l_cnt > 0 THEN  #此盘点编号下的所有数量与盘点数量相等时不需提示afa-01029
                  LET l_success = TRUE   
               END IF 
            #160405-00001#1 add--end---               
            END IF   
            
            IF g_fabr_m.fabr014 <> g_fabr_m.fabr024 OR 
               g_fabr_m.fabr015 <> g_fabr_m.fabr025 OR 
               g_fabr_m.fabr016 <> g_fabr_m.fabr026 OR 
               g_fabr_m.fabr017 <> g_fabr_m.fabr036 OR 
               g_fabr_m.fabr018 <> g_fabr_m.fabr027 OR 
               g_fabr_m.fabr019 <> g_fabr_m.fabr028 OR 
               g_fabr_m.fabr020 <> g_fabr_m.fabr029 OR 
               g_fabr_m.fabr021 <> g_fabr_m.fabr030 THEN 
               
               IF cl_ask_confirm('afa-01034') THEN
                  CALL afat512_ins_afat421() RETURNING l_success,l_docno
               END IF
            END IF
            
            IF l_success = FALSE THEN 
               CALL s_transaction_end('N','0')
               CALL cl_err_collect_init()
               RETURN
            ELSE 
               #CALL s_transaction_end('Y','0')  #160329-00025#1
               IF l_n > 0 THEN #160405-00001#1 add
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = 'azz-00322'
                  LET g_errparam.extend = ''
                  LET g_errparam.popup = TRUE
                  CALL cl_err()
               END IF  #160405-00001#1 add 
               
               IF NOT cl_null(l_docno1) THEN 
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = 'afa-01031'
                  LET g_errparam.replace[1] = l_docno1
                  LET g_errparam.extend = ''
                  LET g_errparam.popup = TRUE
                  CALL cl_err()  
               END IF
               
               IF NOT cl_null(l_docno2) THEN 
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = 'afa-01032'
                  LET g_errparam.replace[1] = l_docno2
                  LET g_errparam.extend = ''
                  LET g_errparam.popup = TRUE
                  CALL cl_err()  
               END IF
               
