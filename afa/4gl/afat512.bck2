{<section id="afat512.modify" >}
#+ 資料修改
PRIVATE FUNCTION afat512_modify()
   #add-point:modify段define

   #end add-point
   
   #先確定key值無遺漏
   IF g_fabr_m.fabr003 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
 
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_fabr003_t = g_fabr_m.fabr003
   LET g_fabr004_t = g_fabr_m.fabr004
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN afat512_cl USING g_enterprise,g_fabr_m.fabr003,g_fabr_m.fabr004
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN afat512_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
 
      CLOSE afat512_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
