      LET g_wc = g_wc, " ORDER BY faaeld"
   ELSE
      LET g_wc = g_wc, " ORDER BY ", g_searchcol
   END IF 
 
   CALL afai050_browser_fill("F")
   RETURN TRUE
 
END FUNCTION
