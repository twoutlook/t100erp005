  <section id="afai050.modify" ver="3" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION afai050_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_faae_m.faaeld IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE afai050_master_referesh USING g_faae_m.faaeld INTO g_faae_m.faaeld,g_faae_m.faaeld_desc
 
   ERROR ""
  
   LET g_faaeld_t = g_faae_m.faaeld
 
   CALL s_transaction_begin()
   
   OPEN afai050_cl USING g_enterprise,g_faae_m.faaeld
 
   IF STATUS THEN
      CALL cl_err("OPEN afai050_cl:", STATUS, 1)
      CLOSE afai050_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH afai050_cl INTO g_faae_m.faaeld,g_faae_m.faaeld_desc,g_faae_m.glaacomp,g_faae_m.glaacomp_desc, 
       g_faae_m.glaa004,g_faae_m.glaa004_desc,g_faae_m.glaa014,g_faae_m.glaa008
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_faae_m.faaeld,SQLCA.sqlcode,0)
      CLOSE afai050_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL afai050_show()
   WHILE TRUE
      LET g_faaeld_t = g_faae_m.faaeld
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL afai050_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_faae_m.* = g_faae_m_t.*
         CALL afai050_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_faae_m.faaeld != g_faaeld_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE faae_t SET faaeld = g_faae_m.faaeld
 
          WHERE faaeent = g_enterprise AND faaeld = g_faaeld_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("faae_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("faae_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_faae_m.faaeld,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE afai050_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_faae_m.faaeld,'U')
 
   CALL afai050_b_fill("1=1")
   
END FUNCTION   
]]>
  </section>
