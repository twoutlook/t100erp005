  <point name="function.afai050_faae001_chk" order="5" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 检查部门是否归属在单头的所属法人
################################################################################
PRIVATE FUNCTION afai050_faae001_chk()
DEFINE l_ooeg004  LIKE ooeg_t.ooeg004
DEFINE l_n        LIKE type_t.num5
   LET g_errno = ''
   
     
   SELECT COUNT(*) INTO l_n
     FROM ooef_t
    WHERE ooefent = g_enterprise
      AND ooef001 = g_faae_d[l_ac].faae001
      AND ooef017 = g_faae_m.glaacomp
   IF l_n = 0 THEN
      LET g_errno = 'afa-00054'
   END IF
   
END FUNCTION]]>
