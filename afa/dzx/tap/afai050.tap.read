            #add-point:ON ACTION controlp INFIELD faae003
            {<point name="input.c.page1.faae003" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_faae_d[l_ac].* = g_faae_d_t.*
               CLOSE afai050_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_faae_d[l_ac].faae001,-263,1)
               LET g_faae_d[l_ac].* = g_faae_d_t.*
            ELSE
               #寫入修改者/修改日期資訊
               LET g_faae2_d[l_ac].faaemodid = g_user 
LET g_faae2_d[l_ac].faaemoddt = cl_get_current()
 
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
         
               UPDATE faae_t SET (faaeld,faaestus,faae001,faae002,faae003,faaeownid,faaeowndp,faaecrtid, 
                   faaecrtdp,faaecrtdt,faaemodid,faaemoddt) = (g_faae_m.faaeld,g_faae_d[l_ac].faaestus, 
                   g_faae_d[l_ac].faae001,g_faae_d[l_ac].faae002,g_faae_d[l_ac].faae003,g_faae2_d[l_ac].faaeownid, 
                   g_faae2_d[l_ac].faaeowndp,g_faae2_d[l_ac].faaecrtid,g_faae2_d[l_ac].faaecrtdp,g_faae2_d[l_ac].faaecrtdt, 
                   g_faae2_d[l_ac].faaemodid,g_faae2_d[l_ac].faaemoddt)
                WHERE faaeent = g_enterprise AND faaeld = g_faae_m.faaeld 
 
                 AND faae001 = g_faae_d_t.faae001 #項次   
