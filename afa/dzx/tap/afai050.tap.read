  <section id="afai050.b_fill" ver="9" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION afai050_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_faae_d.clear()
   CALL g_faae2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE faaestus,faae001,faae002,faae003,faae001,faae002,faaeownid,faaeowndp, 
       faaecrtid,faaecrtdp,faaecrtdt,faaemodid,faaemoddt,t1.ooefl003 ,t2.faacl003 ,t3.oofa011 ,t4.ooefl003 , 
       t5.oofa011 ,t6.ooefl003 ,t7.oofa011 FROM faae_t",   
               "",
               
                              " LEFT JOIN ooefl_t t1 ON t1.ooeflent='"||g_enterprise||"' AND t1.ooefl001=faae001 AND t1.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN faacl_t t2 ON t2.faaclent='"||g_enterprise||"' AND t2.faacl001=faae002 AND t2.faacl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t3 ON t3.oofaent='"||g_enterprise||"' AND t3.oofa002='2' AND t3.oofa003=faaeownid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=faaeowndp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t5 ON t5.oofaent='"||g_enterprise||"' AND t5.oofa002='2' AND t5.oofa003=faaecrtid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent='"||g_enterprise||"' AND t6.ooefl001=faaecrtdp AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t7 ON t7.oofaent='"||g_enterprise||"' AND t7.oofa002='2' AND t7.oofa003=faaemodid  ",
 
               " WHERE faaeent= ? AND faaeld=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("faae_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF afai050_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY faae_t.faae001,faae_t.faae002"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE afai050_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR afai050_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_faae_m.faaeld
                                               
      FOREACH b_fill_cs INTO g_faae_d[l_ac].faaestus,g_faae_d[l_ac].faae001,g_faae_d[l_ac].faae002,g_faae_d[l_ac].faae003, 
          g_faae2_d[l_ac].faae001,g_faae2_d[l_ac].faae002,g_faae2_d[l_ac].faaeownid,g_faae2_d[l_ac].faaeowndp, 
          g_faae2_d[l_ac].faaecrtid,g_faae2_d[l_ac].faaecrtdp,g_faae2_d[l_ac].faaecrtdt,g_faae2_d[l_ac].faaemodid, 
          g_faae2_d[l_ac].faaemoddt,g_faae_d[l_ac].faae001_desc,g_faae_d[l_ac].faae002_desc,g_faae2_d[l_ac].faaeownid_desc, 
          g_faae2_d[l_ac].faaeowndp_desc,g_faae2_d[l_ac].faaecrtid_desc,g_faae2_d[l_ac].faaecrtdp_desc, 
          g_faae2_d[l_ac].faaemodid_desc
                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_faae2_d[l_ac].faaeownid_desc = cl_get_username(g_faae2_d[l_ac].faaeownid)
      #LET g_faae2_d[l_ac].faaeowndp_desc = cl_get_deptname(g_faae2_d[l_ac].faaeowndp)
      #LET g_faae2_d[l_ac].faaecrtid_desc = cl_get_username(g_faae2_d[l_ac].faaecrtid)
      #LET g_faae2_d[l_ac].faaecrtdp_desc = cl_get_deptname(g_faae2_d[l_ac].faaecrtdp)
      #LET g_faae2_d[l_ac].faaemodid_desc = cl_get_username(g_faae2_d[l_ac].faaemodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_faae_d.deleteElement(g_faae_d.getLength())
      CALL g_faae2_d.deleteElement(g_faae2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE afai050_pb   
   
END FUNCTION
]]>
  </section>
