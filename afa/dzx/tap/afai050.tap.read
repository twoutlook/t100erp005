  <section id="afai050.delete" ver="6" status="" src="s">
    <![CDATA[#+ 資料刪除
PRIVATE FUNCTION afai050_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_faae_m.faaeld IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE afai050_master_referesh USING g_faae_m.faaeld INTO g_faae_m.faaeld,g_faae_m.faaeld_desc
   
   CALL afai050_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN afai050_cl USING g_enterprise,g_faae_m.faaeld
 
   IF STATUS THEN
      CALL cl_err("OPEN afai050_cl:", STATUS, 1)
      CLOSE afai050_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH afai050_cl INTO g_faae_m.faaeld,g_faae_m.faaeld_desc,g_faae_m.glaacomp,g_faae_m.glaacomp_desc, 
       g_faae_m.glaa004,g_faae_m.glaa004_desc,g_faae_m.glaa014,g_faae_m.glaa008
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_faae_m.faaeld,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL afai050_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM faae_t WHERE faaeent = g_enterprise AND faaeld = g_faae_m.faaeld
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("faae_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_faae_d.clear() 
      CALL g_faae2_d.clear()       
 
     
      CALL afai050_ui_browser_refresh()  
      CALL afai050_ui_headershow()  
      CALL afai050_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL afai050_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL afai050_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE afai050_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_faae_m.faaeld,'D')
    
END FUNCTION
]]>
