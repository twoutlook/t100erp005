  <point name="detail_reproduce.body.table1.a_insert" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[            
  DELETE FROM faae_t
   WHERE faaeent = g_enterprise
     AND faaeld  = g_faae_m.faaeld
     AND (faae003 NOT IN (SELECT glac002 FROM glac_t WHERE glacent=g_enterprise AND glac001=g_faae_m.glaa004)
     OR faae001 NOT IN (SELECT DISTINCT ooef001 FROM ooef_t WHERE ooefent=g_enterprise AND ooef017=g_faae_m.glaacomp))
  SELECT COUNT(faaeld) INTO l_n 
    FROM faae_t
   WHERE faaeent = g_enterprise
     AND faaeld  = g_faae_m.faaeld
  IF l_n = 0 THEN
     CALL cl_err('','afa-00057',1)
  END IF
                                      ]]>
  </point>
