  <point name="function.afat400_get_fabasite_desc" order="1" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[# 資產中心名稱
PRIVATE FUNCTION afat400_get_fabasite_desc()
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_faba_m.fabasite
   CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_faba_m.fabasite_desc = '', g_rtn_fields[1] , ''
   DISPLAY BY NAME g_faba_m.fabasite_desc
END FUNCTION]]>
