  <section id="afat400.b_fill" ver="10" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION afat400_b_fill()
   DEFINE p_wc2      STRING
   DEFINE li_idx     LIKE type_t.num10
   #add-point:b_fill段define
   {<point name="b_fill.define" edit="s"/>}
   #end add-point     
   #add-point:b_fill段define(客製用)
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point     
 
   CALL g_fabn_d.clear()    #g_fabn_d 單頭及單身 
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF afat400_fill_chk(1) THEN
      #切換上下筆時不重組SQL
      IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
         LET g_sql = "SELECT  UNIQUE fabnseq,fabn001,fabn002,fabn003,fabn004,fabn005,fabn018,fabn006, 
             fabn007,fabn008,fabn009,fabn010,fabn011,fabn012,fabn013,fabn014,fabn015,fabn016,fabn017 , 
             t1.oocql004 FROM fabn_t",   
                     " INNER JOIN faba_t ON fabadocno = fabndocno ",
 
                     #"",
                     
                     "",
                                    " LEFT JOIN oocql_t t1 ON t1.oocqlent='"||g_enterprise||"' AND t1.oocql001='3904' AND t1.oocql002=fabn017 AND t1.oocql003='"||g_dlang||"' ",
 
                     " WHERE fabnent=? AND fabndocno=?"
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before
         {<point name="b_fill.body.fill_sql"/>}
         #end add-point
         IF NOT cl_null(g_wc2_table1) THEN
            LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY fabn_t.fabnseq"
         
         #add-point:單身填充控制
         {<point name="b_fill.sql"/>}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE afat400_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR afat400_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_faba_m.fabadocno
                                               
      FOREACH b_fill_cs INTO g_fabn_d[l_ac].fabnseq,g_fabn_d[l_ac].fabn001,g_fabn_d[l_ac].fabn002,g_fabn_d[l_ac].fabn003, 
          g_fabn_d[l_ac].fabn004,g_fabn_d[l_ac].fabn005,g_fabn_d[l_ac].fabn018,g_fabn_d[l_ac].fabn006, 
          g_fabn_d[l_ac].fabn007,g_fabn_d[l_ac].fabn008,g_fabn_d[l_ac].fabn009,g_fabn_d[l_ac].fabn010, 
          g_fabn_d[l_ac].fabn011,g_fabn_d[l_ac].fabn012,g_fabn_d[l_ac].fabn013,g_fabn_d[l_ac].fabn014, 
          g_fabn_d[l_ac].fabn015,g_fabn_d[l_ac].fabn016,g_fabn_d[l_ac].fabn017,g_fabn_d[l_ac].fabn017_desc 

         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF
            EXIT FOREACH
         END IF
         
         LET l_ac = l_ac + 1
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
 
   
   #add-point:browser_fill段其他table處理
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_fabn_d.deleteElement(g_fabn_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE afat400_pb
 
   
   LET li_idx = l_ac
   
   #遮罩相關處理
   FOR l_ac = 1 TO g_fabn_d.getLength()
      LET g_fabn_d_mask_o[l_ac].* =  g_fabn_d[l_ac].*
      CALL afat400_fabn_t_mask()
      LET g_fabn_d_mask_n[l_ac].* =  g_fabn_d[l_ac].*
   END FOR
   
 
   
   LET l_ac = li_idx
   
   CALL cl_ap_performance_next_end()
 
END FUNCTION
]]>
