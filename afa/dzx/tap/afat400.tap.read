  <point name="function.afat400_get_faah" order="3" ver="2" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[# 根据卡片+財產編號+附號自動帶默認值
PRIVATE FUNCTION afat400_get_faah()
  ############################add by huangtao
   IF cl_null(g_fabn_d[l_ac].fabn001) OR cl_null(g_fabn_d[l_ac].fabn003) THEN
      RETURN
   END IF
   IF cl_null(g_fabn_d[l_ac].fabn002) THEN
      LET g_fabn_d[l_ac].fabn002 = ' '
   END IF
   #############################
   SELECT faah014,faah020,faah022,faah024,faah025,faah026,faah027,faah030,faah028,faah031 
     INTO g_fabn_d[l_ac].fabn006,g_fabn_d[l_ac].fabn007,
          g_fabn_d[l_ac].fabn009,g_fabn_d[l_ac].fabn010,
          g_fabn_d[l_ac].fabn011,g_fabn_d[l_ac].fabn012,
          g_fabn_d[l_ac].fabn013,g_fabn_d[l_ac].fabn014,
          g_fabn_d[l_ac].fabn015,g_fabn_d[l_ac].fabn016
     FROM faah_t
    WHERE faahent = g_enterprise  
      AND faah003 = g_fabn_d[l_ac].fabn001   
      AND faah004 = g_fabn_d[l_ac].fabn002
      AND faah001 = g_fabn_d[l_ac].fabn003
      
      
    
END FUNCTION]]>
  </point>
  <point name="function.afat400_confirm" order="4" ver="5" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[# 資料審核
PRIVATE FUNCTION afat400_confirm()
   DEFINE l_fabn       RECORD  LIKE fabn_t.*
   DEFINE l_faah000    LIKE faah_t.faah000
   DEFINE l_faah001    LIKE faah_t.faah001
   DEFINE l_year       STRING
   DEFINE l_month      STRING
   DEFINE l_str        STRING
   DEFINE ls_sql       STRING
   DEFINE l_ooab002      LIKE ooab_t.ooab002
   DEFINE l_ooab002_1      LIKE ooab_t.ooab002
   DEFINE l_sql        STRING
   
   CALL s_transaction_begin()
   #CALL cl_showmsg_init()
   LET g_success = 'Y'
   
    #檢核是否已經關帳
   CALL cl_get_para(g_enterprise,g_faba_m.fabacomp,'S-FIN-9003') RETURNING l_ooab002
  
   IF g_faba_m.fabadocdt <= l_ooab002 THEN 
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'afa-00060'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()
      LET g_success = 'N'   #add by huangtao
      RETURN 
   END IF  
   
    CALL cl_get_para(g_enterprise,g_faba_m.fabacomp,'S-FIN-9005') RETURNING l_ooab002_1
    
   LET g_sql = "SELECT * FROM fabn_t",
               " WHERE fabnent = '",g_enterprise,"'",
               "   AND fabndocno = '",g_faba_m.fabadocno,"'"
   PREPARE afat400_pre FROM g_sql
   DECLARE afat400_cus CURSOR FOR afat400_pre
   
   FOREACH afat400_cus INTO l_fabn.*
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "foreach:"
         LET g_errparam.popup = TRUE
         CALL cl_err()
         LET g_success = 'N'   #add by huangtao
      END IF
      
      #回寫取得日期
      UPDATE faah_t SET faah014 = l_fabn.fabn006
          WHERE faahent = g_enterprise
            AND faah003 = l_fabn.fabn001
            AND faah004 = l_fabn.fabn002
      
      IF SQLCA.SQLcode  THEN
         #CALL cl_errmsg('',l_fabn.fabn001,l_fabn.fabn002,SQLCA.sqlcode,1)
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = 'upd faah014'
         LET g_errparam.popup = TRUE
         CALL cl_err()
         LET g_success = 'N'
      END IF
      
      
      #財產編號=正式財產編號 并且附號=正式附號,直接更新faah_t 狀態為資本化
      IF l_fabn.fabn001 = l_fabn.fabn004 AND ((l_fabn.fabn002= ' ' AND  cl_null(l_fabn.fabn005))
                                           OR (l_fabn.fabn002 <>' ' AND l_fabn.fabn002 IS NOT NULL AND  l_fabn.fabn002 = l_fabn.fabn005)) 
                                         AND  l_fabn.fabn003 = l_fabn.fabn018  THEN 
         UPDATE faah_t SET faah015 = '1' 
          WHERE faahent = g_enterprise
            AND faah003 = l_fabn.fabn001
            AND faah004 = l_fabn.fabn002
            AND faah001 = l_fabn.fabn003
            
         IF SQLCA.SQLcode  THEN
           # CALL cl_errmsg('',l_fabn.fabn001,l_fabn.fabn002,SQLCA.sqlcode,1)
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "upd faah015"
            LET g_errparam.popup = TRUE
            CALL cl_err()
            LET g_success = 'N'
         END IF
         
         UPDATE faai_t SET faai023 = '1' 
          WHERE faaient = g_enterprise
            AND faai002 = l_fabn.fabn001
            AND faai003 = l_fabn.fabn002
            AND faai001 = l_fabn.fabn003
            
         IF SQLCA.SQLcode  THEN
            #CALL cl_errmsg('',l_fabn.fabn001,l_fabn.fabn002,SQLCA.sqlcode,1)
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "upd faai023"
            LET g_errparam.popup = TRUE
            CALL cl_err()
            LET g_success = 'N'
         END IF
      END IF
      
      #財產編號<>正式財產編號 或附號<>正式附號
      #update faah_t中的此财编的状态为被资本化 ,对应的faaj与faai的资产状态均为被资本化，不参加折旧
      #且后台新增一笔以正式财产编号+附号+单身的信息栏位为准的，其它资料与原财产编号相同的一笔faah档
      #但资产状态为资本化
      IF l_fabn.fabn001 <> l_fabn.fabn004 OR (l_fabn.fabn002 = ' ' AND NOT cl_null(l_fabn.fabn005))
                                              OR (l_fabn.fabn002 <>' ' AND l_fabn.fabn002 IS NOT NULL AND NOT cl_null(l_fabn.fabn005) AND l_fabn.fabn002 <> l_fabn.fabn005)
                                              OR (l_fabn.fabn002 <>' ' AND l_fabn.fabn002 IS NOT NULL AND cl_null(l_fabn.fabn005))
                                          OR l_fabn.fabn003 <> l_fabn.fabn018   THEN 
         UPDATE faah_t SET faah015 = '10' 
          WHERE faahent = g_enterprise
            AND faah003 = l_fabn.fabn001
            AND faah004 = l_fabn.fabn002
            AND faah001 = l_fabn.fabn003
            
         IF SQLCA.SQLcode  THEN
            #CALL cl_errmsg('',l_fabn.fabn001,l_fabn.fabn002,SQLCA.sqlcode,1)
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "upd faah015"
            LET g_errparam.popup = TRUE
            CALL cl_err()
            LET g_success = 'N'
         END IF
         
         UPDATE faai_t SET faai023 = '10' 
          WHERE faaient = g_enterprise
            AND faai002 = l_fabn.fabn001
            AND faai003 = l_fabn.fabn002
            AND faai001 = l_fabn.fabn003
            
         IF SQLCA.SQLcode  THEN
            #CALL cl_errmsg('',l_fabn.fabn001,l_fabn.fabn002,SQLCA.sqlcode,1)
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "upd faai023"
            LET g_errparam.popup = TRUE
            CALL cl_err()
            LET g_success = 'N'
         END IF
         
         DROP TABLE afat400_tmp
         DROP TABLE afat400_tmp1
         DROP TABLE afat400_tmp2
         

         #重新產生一筆新的faah_t資料
         #CREATE TEMP TABLE
         LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE afat400_tmp AS ",
                      "SELECT * FROM faah_t "
         PREPARE afat400_tmp_pre FROM ls_sql
         EXECUTE afat400_tmp_pre
         FREE afat400_tmp_pre
                      
         #將符合條件的資料丟入TEMP TABLE
         INSERT INTO afat400_tmp SELECT * FROM faah_t 
                                  WHERE faahent = g_enterprise 
                                    AND faah003 = l_fabn.fabn001
                                    AND faah004 = l_fabn.fabn002
                                    AND faah001 = l_fabn.fabn003
         
        IF l_ooab002_1 = 'Y' THEN    
         ################################mark by huangtao        
         # #卡片編號,生成批號自動+1
         # SELECT lpad((MAX(faah001) + 1),10,'0') INTO l_fabn.fabn018
         #   FROM faah_t
         #  WHERE faahent = g_enterprise
         LET l_sql =  " SELECT lpad((MAX(CAST(faah001 AS NUMBER(10,0))) + 1),10,'0') FROM faah_t ",
                  "  WHERE faahent ='",g_enterprise,"'",
                  "    AND regexp_substr(faah001,'[0-9]+') IS NOT NULL"               
         PREPARE sel_faah001 FROM l_sql
         EXECUTE sel_faah001 INTO l_fabn.fabn018      
         ################################mark by huangtao 
           IF cl_null(l_fabn.fabn018) THEN 
              LET l_fabn.fabn018 = '0000000001'
           END IF       
         END IF
         
         LET l_year = YEAR(g_today)
         LET l_month = MONTH(g_today)
         IF l_month < 10 THEN 
            LET l_month = '0' CLIPPED,l_month
         END IF
         
         LET l_str = l_year CLIPPED,l_month
         
         LET g_sql = "SELECT MAX(faah000) ",
                     "  FROM faah_t ",
                     " WHERE faahent = '",g_enterprise,"'",
                     "   AND faah000 LIKE '",l_str,"%'"
         PREPARE faah000_pre FROM g_sql
         EXECUTE faah000_pre INTO l_faah000
         IF cl_null(l_faah000) THEN 
            LET l_faah000 = l_str,'0001'
         ELSE
            LET g_sql = "SELECT lpad((lpad((substr(MAX(faah000),7,10) + 1),4,'0')),10,'",l_str,"')",
                        "  FROM faah_t ",
                        " WHERE faahent = '",g_enterprise,"'",
                        "   AND faah000 LIKE '",l_str,"%'"
            PREPARE faah000_pre1 FROM g_sql
            EXECUTE faah000_pre1 INTO l_faah000
         END IF
         IF l_fabn.fabn005 IS NULL THEN
            LET l_fabn.fabn005 = ' '
         END IF
         #將財產編號,附號修改為正式財產編號,正式附號,其他欄位以單身信息為準
         UPDATE afat400_tmp 
            SET faah000 = l_faah000,
                faah001 = l_fabn.fabn018,
                faah003 = l_fabn.fabn004,
                faah004 = l_fabn.fabn005,
                
                faah014 = l_fabn.fabn006,
                faah020 = l_fabn.fabn007, 
                faah022 = l_fabn.fabn009, 
                faah024 = l_fabn.fabn010, 
                faah025 = l_fabn.fabn011,
                faah026 = l_fabn.fabn012, 
                faah027 = l_fabn.fabn013,
                faah030 = l_fabn.fabn014,
                faah028 = l_fabn.fabn015,                
                faah031 = l_fabn.fabn016,
                faah015 = '1',
                ###########################################
                faahownid = g_faba_m.fabaownid,    
                faahowndp = g_faba_m.fabaowndp,    
                faahcrtid = g_faba_m.fabacrtid,    
                faahcrtdp = g_faba_m.fabacrtdp,    
                faahcrtdt = g_faba_m.fabacrtdt,   
                faahmodid = g_faba_m.fabacnfid,    
                faahmoddt = g_faba_m.fabacnfdt     
                ###########################################
         #將資料塞回原table   
         INSERT INTO faah_t SELECT * FROM afat400_tmp
         
         IF SQLCA.sqlcode THEN
            #CALL cl_errmsg('',l_fabn.fabn001,l_fabn.fabn002,SQLCA.sqlcode,1)
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "ins faah"
            LET g_errparam.popup = TRUE
            CALL cl_err()
            LET g_success = 'N'
         END IF
         
         
         #重新產生新的faai_t資料
         #CREATE TEMP TABLE
         LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE afat400_tmp1 AS ",
                      "SELECT * FROM faai_t "
         PREPARE afat400_tmp_pre1 FROM ls_sql
         EXECUTE afat400_tmp_pre1
         FREE afat400_tmp_pre1
                      
         #將符合條件的資料丟入TEMP TABLE
         INSERT INTO afat400_tmp1 SELECT * FROM faai_t 
                                  WHERE faaient = g_enterprise 
                                    AND faai002 = l_fabn.fabn001
                                    AND faai003 = l_fabn.fabn002
                                    AND faai001 = l_fabn.fabn003
         #將財產編號,附號修改為正式財產編號,正式附號,其他欄位以單身信息為準
         UPDATE afat400_tmp1 
            SET faai000 = l_faah000,
                faai001 = l_fabn.fabn018,
                faai002 = l_fabn.fabn004,
                faai003 = l_fabn.fabn005,
                faai023 = '1'                
         
         #將資料塞回原table   
         INSERT INTO faai_t SELECT * FROM afat400_tmp1
         
         IF SQLCA.sqlcode THEN
            #CALL cl_errmsg('',l_fabn.fabn001,l_fabn.fabn002,SQLCA.sqlcode,1)
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "ins faai"
            LET g_errparam.popup = TRUE
            CALL cl_err()
            LET g_success = 'N'
         END IF
         
         #重新產生新的faaj_t資料
         #CREATE TEMP TABLE
         LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE afat400_tmp2 AS ",
                      "SELECT * FROM faaj_t "
         PREPARE afat400_tmp_pre2 FROM ls_sql
         EXECUTE afat400_tmp_pre2
         FREE afat400_tmp_pre2
                      
         #將符合條件的資料丟入TEMP TABLE
         INSERT INTO afat400_tmp2 SELECT * FROM faaj_t 
                                  WHERE faajent = g_enterprise 
                                    AND faaj001 = l_fabn.fabn001
                                    AND faaj002 = l_fabn.fabn002
                                    AND faaj037 = l_fabn.fabn003

         #將財產編號,附號修改為正式財產編號,正式附號,其他欄位以單身信息為準
         UPDATE afat400_tmp2 
            SET faaj000 = l_faah000,
                faaj037 = l_fabn.fabn018,
                faaj001 = l_fabn.fabn004,
                faaj002 = l_fabn.fabn005                      

         #將資料塞回原table   
         INSERT INTO faaj_t SELECT * FROM afat400_tmp2
         
         IF SQLCA.sqlcode THEN
           #CALL cl_errmsg('',l_fabn.fabn001,l_fabn.fabn002,SQLCA.sqlcode,1)
           INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "ins faaj"
            LET g_errparam.popup = TRUE
            CALL cl_err()
            LET g_success = 'N'
         END IF
         
         ##回写单身
         IF l_ooab002_1 = 'Y' THEN
            UPDATE fabn_t
               SET fabn018 = l_fabn.fabn018
             WHERE fabnent = g_enterprise AND fabndocno = l_fabn.fabndocno AND fabnseq = l_fabn.fabnseq
         END IF   
         
         #刪除TEMP TABLE
         DROP TABLE afat400_tmp
         DROP TABLE afat400_tmp1
         DROP TABLE afat400_tmp2
      END IF
   END FOREACH 
   
END FUNCTION]]>
  </point>
  <point name="function.afat400_unconfirm" order="5" ver="5" cite_std="N" new="Y" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[# 取消審核
PRIVATE FUNCTION afat400_unconfirm()
   DEFINE l_ooab002      LIKE ooab_t.ooab002
   DEFINE l_faah015      LIKE faah_t.faah015
   DEFINE l_fabn         RECORD  LIKE fabn_t.*
   
   CALL s_transaction_begin()
   #CALL cl_showmsg_init()
   LET g_success = 'Y'
   
   #檢核是否已經關帳
   CALL cl_get_para(g_enterprise,g_faba_m.fabacomp,'S-FIN-9003') RETURNING l_ooab002
   IF g_faba_m.fabadocdt <= l_ooab002 THEN 
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'afa-00061'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()
      LET g_success = 'N'   #add by huangtao
      RETURN 
   END IF
   ##################################财编在当前的单据日期后如有异动，则不可取消审核

   
   ###################################
   
   LET g_sql = "SELECT * FROM fabn_t",
               " WHERE fabnent = '",g_enterprise,"'",
               "   AND fabndocno = '",g_faba_m.fabadocno,"'"
   PREPARE afat400_pre1 FROM g_sql
   DECLARE afat400_cus1 CURSOR FOR afat400_pre1
   
   FOREACH afat400_cus1 INTO l_fabn.*
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "foreach:"
         LET g_errparam.popup = TRUE
         CALL cl_err()
         LET g_success = 'N'   #add by huangtao
         EXIT FOREACH
      END IF
      
      #檢核財編是否已開始折舊,即狀態是否為資本化的狀態
      SELECT faah015 INTO l_faah015
        FROM faah_t
       WHERE faahent = g_enterprise
         AND faah003 = l_fabn.fabn001
         AND faah004 = l_fabn.fabn002
      
      IF l_faah015 <> '1' AND l_faah015 <> '10' THEN 
         #CALL cl_errmsg('',l_fabn.fabn001,l_fabn.fabn002,'afa-00128',1)
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'afa-00128'
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()
         LET g_success = 'N'
      END IF
      
      #財產編號=正式財產編號 并且附號=正式附號,直接还原状态为取得
      IF l_fabn.fabn001 = l_fabn.fabn004 AND  ((l_fabn.fabn002= ' ' AND  cl_null(l_fabn.fabn005))
                                           OR (l_fabn.fabn002 <>' ' AND l_fabn.fabn002 IS NOT NULL AND  l_fabn.fabn002 = l_fabn.fabn005)) 
                                           AND  l_fabn.fabn003 = l_fabn.fabn018  THEN 
         UPDATE faah_t SET faah015 = '0' 
          WHERE faahent = g_enterprise
            AND faah003 = l_fabn.fabn001
            AND faah004 = l_fabn.fabn002
            
         IF SQLCA.SQLcode  THEN
            #CALL cl_errmsg('',l_fabn.fabn001,l_fabn.fabn002,SQLCA.sqlcode,1)
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = 'upd faah015'
            LET g_errparam.popup = TRUE
            CALL cl_err()
            LET g_success = 'N'
         END IF
      END IF
      
      #財產編號<>正式財產編號 或附號<>正式附號
      #删除新生成的资本化的正式财产编号资料
      #还原原财产编号的资产状态为取得
      IF l_fabn.fabn001 <> l_fabn.fabn004 OR (l_fabn.fabn002 = ' ' AND NOT cl_null(l_fabn.fabn005))
                                              OR (l_fabn.fabn002 <>' ' AND l_fabn.fabn002 IS NOT NULL AND NOT cl_null(l_fabn.fabn005) AND l_fabn.fabn002 <> l_fabn.fabn005)
                                              OR (l_fabn.fabn002 <>' ' AND l_fabn.fabn002 IS NOT NULL AND cl_null(l_fabn.fabn005)) 
                                          OR  l_fabn.fabn003 <> l_fabn.fabn018  THEN 
         UPDATE faah_t SET faah015 = '0' 
          WHERE faahent = g_enterprise
            AND faah003 = l_fabn.fabn001
            AND faah004 = l_fabn.fabn002
            
         IF SQLCA.SQLcode  THEN
            #CALL cl_errmsg('',l_fabn.fabn001,l_fabn.fabn002,SQLCA.sqlcode,1)
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = 'upd faah015'
            LET g_errparam.popup = TRUE
            CALL cl_err()
            LET g_success = 'N'
         END IF
         
         IF cl_null(l_fabn.fabn005) THEN
            LET l_fabn.fabn005 = ' '
         END IF
         #刪除faah_t
         DELETE FROM faah_t WHERE faahent = g_enterprise
                              AND faah003 = l_fabn.fabn004
                              AND faah004 = l_fabn.fabn005 
                              AND faah001 = l_fabn.fabn018
                              
         IF SQLCA.SQLcode  THEN
            #CALL cl_errmsg('',l_fabn.fabn001,l_fabn.fabn002,SQLCA.sqlcode,1)
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = 'del faah'
            LET g_errparam.popup = TRUE
            CALL cl_err()
