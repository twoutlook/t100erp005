  <point name="function.afat503_fabh001_chk" order="7" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 財產編號檢查
# Memo...........:
# Usage..........: CALL afat503_fabh001_chk()
# Modify.........:
################################################################################
PRIVATE FUNCTION afat503_fabh001_chk()
   DEFINE l_cnt       LIKE type_t.num5
   DEFINE l_faah015   LIKE faah_t.faah015
   DEFINE l_fabh000   LIKE fabh_t.fabh000
   DEFINE l_fabh001   LIKE fabh_t.fabh001
   DEFINE l_fabh002   LIKE fabh_t.fabh002
   DEFINE l_flag      LIKE type_t.num5
   DEFINE l_sql       STRING
   
   LET g_errno=""
   IF NOT cl_null(g_fabh_d[l_ac].fabh001) AND NOT cl_null(g_fabh_d[l_ac].fabh002) 
      AND NOT cl_null(g_fabh_d[l_ac].fabh000) THEN 
      #卡片+财编+附號需存在於faah_t固定資產基礎資料檔中   
      SELECT faah015 INTO l_faah015 FROM faah_t
       WHERE faahent = g_enterprise
         AND faah003 = g_fabh_d[l_ac].fabh001
         AND faah004 = g_fabh_d[l_ac].fabh002 
         AND faah001 = g_fabh_d[l_ac].fabh000
         
      IF SQLCA.SQLCODE = 100 THEN 
         LET g_errno= 'afa-00076'
      END IF 
      #財產狀態不可為0：取得、5：出售、6：銷賬、10：被資本化
      IF l_faah015='0' OR l_faah015='5' OR l_faah015='6' OR l_faah015='8' OR l_faah015='10' THEN
         LET g_errno='afa-00135'
      END IF
      #卡片+财编+附號需存在於faaj_t固定資產帳套折舊明細資料檔中   
      LET l_cnt = 0 
      SELECT COUNT(*) INTO l_cnt FROM faaj_t
       WHERE faajent = g_enterprise
         AND faajld  = g_fabg_m.fabgld 
         AND faaj001 = g_fabh_d[l_ac].fabh001
         AND faaj002 = g_fabh_d[l_ac].fabh002 
         AND faaj037 = g_fabh_d[l_ac].fabh000
      IF l_cnt = 0 THEN 
         LET g_errno= 'afa-00134'
      END IF
      
      #當前重估單，卡片+財產編號+附號不可重複
      LET l_cnt = 0
      SELECT COUNT(*) INTO l_cnt FROM fabh_t
       WHERE fabhent = g_enterprise
         AND fabhdocno = g_fabg_m.fabgdocno
         AND fabh001 = g_fabh_d[l_ac].fabh001 
         AND fabh002 = g_fabh_d[l_ac].fabh002
         AND fabh000 = g_fabh_d[l_ac].fabh000
         AND fabhseq <> g_fabh_d[l_ac].fabhseq
      IF l_cnt>0 THEN
         LET g_errno= 'afa-00078'
      END IF 
   END IF  
   
   #若不是開窗，直接輸入欄位時，需默認帶出相應的卡片+附號
   #當前重估單中未使用的資料                  
   IF cl_null(g_fabh_d[l_ac].fabh000) AND cl_null(g_fabh_d[l_ac].fabh002) 
      AND NOT cl_null(g_fabh_d[l_ac].fabh001) THEN 
      LET l_sql = " SELECT faah001,faah003,faah004 FROM faah_t,faaj_t ",
                  "  WHERE faahent = faajent AND faah000 = faaj000 ",
                  "    AND faah001 = faaj037 AND faah003 = faaj001 ",
                  "    AND faah004 = faaj002 AND faajld='",g_fabg_m.fabgld,"' ",
                  "    AND faahent = '",g_enterprise,"'",
                  "    AND faah003 = '",g_fabh_d[l_ac].fabh001,"'", 
                  "    AND faah015 NOT IN ('0','5','6','8','10') ",
                  "  ORDER BY faah001,faah003,faah004 "
      PREPARE afat503_pb4 FROM l_sql
      DECLARE afat503_cs4 CURSOR FOR afat503_pb4
      LET l_flag=FALSE #記錄是否抓取到合適資料  
      
      FOREACH afat503_cs4 INTO l_fabh000,l_fabh001,l_fabh002 
         IF SQLCA.sqlcode THEN
            LET g_errno=SQLCA.sqlcode
            EXIT FOREACH
         END IF             
         LET l_cnt = 0
         SELECT COUNT(*) INTO l_cnt FROM fabh_t 
          WHERE fabhent = g_enterprise
            AND fabhld  = g_fabg_m.fabgld
            AND fabhdocno = g_fabg_m.fabgdocno
            AND fabh001 = l_fabh001 
            AND fabh002 = l_fabh002
            AND fabh000 = l_fabh000
            AND fabhseq <>g_fabh_d[l_ac].fabhseq
         IF l_cnt>0 THEN 
            CONTINUE FOREACH
         ELSE 
            LET l_flag=TRUE
            LET g_fabh_d[l_ac].fabh000 = l_fabh000
            LET g_fabh_d[l_ac].fabh002 = l_fabh002
            EXIT FOREACH
         END IF 
      END FOREACH  
      IF l_flag=FALSE THEN  #排除輸入的不符合條件的資料
         LET g_errno = 'afa-00079'
      END IF    
   END IF  
END FUNCTION]]>
  </point>
  <point name="function.afat503_get_faah" order="8" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 根据卡片+財產編號+附號自動帶出成本等相关资料
# Memo...........:
# Usage..........: CALL afat503_get_faah()
# Modify.........:
################################################################################
PRIVATE FUNCTION afat503_get_faah()
   DEFINE l_faah006        LIKE faah_t.faah006
   DEFINE l_faaj012        LIKE faaj_t.faaj012
   DEFINE l_faaj013        LIKE faaj_t.faaj013
   DEFINE l_fabh006_1      LIKE fabh_t.fabh006  #20141215 add by chenying
   DEFINE l_fabh006_2      LIKE fabh_t.fabh006  #20141215 add by chenying
   
   #20141215 mod by chenying 数量开放修改，默认值带faah数量-faaj处置数量，且检查数量不可大于faah数量-faaj处置数量
   LET l_fabh006_1 = 0  #20141215 add by chenying
   LET l_fabh006_2 = 0  #20141215 add by chenying
   SELECT faah017,faah018,faah006,faah015
     INTO g_fabh_d[l_ac].fabh005,l_fabh006_1,l_faah006,g_fabh_d[l_ac].fabh003  #20141215 mod by chenying g_fabh_d[l_ac].fabh006-->l_fabh006_1
     FROM faah_t
    WHERE faahent=g_enterprise AND faah001=g_fabh_d[l_ac].fabh000
      AND faah003=g_fabh_d[l_ac].fabh001 AND faah004=g_fabh_d[l_ac].fabh002
      
   SELECT faaj016,faaj028,faaj017,faaj005,faaj019,faaj003,faaj012,faaj013,faaj023,faaj024,faaj026,
          faaj103,faaj104,faaj105,faaj153,faaj154,faaj155,faaj033    #20141215 add faaj033 by chenying
     INTO g_fabh_d[l_ac].fabh008,g_fabh_d[l_ac].fabh004,g_fabh_d[l_ac].fabh011,
          g_fabh_d[l_ac].fabh013,g_fabh_d[l_ac].fabh015,g_faaj003,l_faaj012,l_faaj013,
          g_fabh_d[l_ac].fabh024,g_fabh_d[l_ac].fabh023,g_fabh_d[l_ac].fabh025,
          g_fabh3_d[l_ac].fabh102,g_fabh3_d[l_ac].fabh104,g_fabh3_d[l_ac].fabh106,
          g_fabh3_d[l_ac].fabh152,g_fabh3_d[l_ac].fabh154,g_fabh3_d[l_ac].fabh156,l_fabh006_2 #20141215 add l_fabh006_2  by chenying        
     FROM faaj_t
    WHERE faajent=g_enterprise AND faajld=g_fabg_m.fabgld
      AND faaj001=g_fabh_d[l_ac].fabh001 AND faaj002=g_fabh_d[l_ac].fabh002
      AND faaj037=g_fabh_d[l_ac].fabh000
   
   LET g_fabh_d[l_ac].fabh006 = l_fabh006_1 - l_fabh006_2 #20141215 add by chenying
   LET g_fabh_d[l_ac].fabh012 = g_fabh_d[l_ac].fabh011
   LET g_fabh_d[l_ac].fabh014 = g_fabh_d[l_ac].fabh013
   LET g_fabh3_d[l_ac].fabh105 = g_fabh3_d[l_ac].fabh104
   LET g_fabh3_d[l_ac].fabh155 = g_fabh3_d[l_ac].fabh154
   #当資產性質為7：折畢再提時，預留殘值=折畢再提預留殘值；未使用年限=折畢再提未使用年限
   IF g_fabh_d[l_ac].fabh003='7' THEN
      LET g_fabh_d[l_ac].fabh013=l_faaj012
      LET g_fabh_d[l_ac].fabh015=l_faaj013
   END IF
   LET g_fabh_d[l_ac].fabh016 = g_fabh_d[l_ac].fabh015
   DISPLAY BY NAME g_fabh_d[l_ac].fabh005,g_fabh_d[l_ac].fabh006,
                   g_fabh_d[l_ac].fabh008,g_fabh_d[l_ac].fabh004,g_fabh_d[l_ac].fabh011,
                   g_fabh_d[l_ac].fabh012,g_fabh_d[l_ac].fabh013,g_fabh_d[l_ac].fabh015,
                   g_fabh3_d[l_ac].fabh102,g_fabh3_d[l_ac].fabh104,g_fabh3_d[l_ac].fabh106,
                   g_fabh3_d[l_ac].fabh152,g_fabh3_d[l_ac].fabh154,g_fabh3_d[l_ac].fabh156
  
   IF cl_null(g_fabh_d[l_ac].fabh014) AND g_faaj003="6" THEN
      LET g_fabh_d[l_ac].fabh014=0
      DISPLAY BY NAME g_fabh_d[l_ac].fabh014
   END IF
   
