  <point name="function.afat503_site_ld_chk" order="5" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 資產中心、帳務人員、帳套之間的關聯檢查
# Memo...........:
# Usage..........: CALL afat503_site_ld_chk(p_site,p_user,p_ld)
# Modify.........:
################################################################################
PRIVATE FUNCTION afat503_site_ld_chk(p_site,p_user,p_ld)
   DEFINE p_site         LIKE fabg_t.fabgsite
   DEFINE p_user      LIKE fabg_t.fabg001
   DEFINE p_ld           LIKE fabg_t.fabgld
   DEFINE l_ooeastus     LIKE ooea_t.ooeastus
   DEFINE l_faabstus     LIKE faab_t.faabstus
   DEFINE l_faab007      LIKE faab_t.faab007
   DEFINE l_ooagstus     LIKE ooag_t.ooagstus
   DEFINE l_cnt          LIKE type_t.num5
   DEFINE l_success      LIKE type_t.num5
   
   LET g_errno = ' '
   LET l_cnt = 0
   
   #帳套下屬於法人否檢查
#    SELECT COUNT(*) INTO l_cnt FROM glaa_t,ooef_t,faab_t 
#    WHERE glaald   = p_ld
#      AND glaacomp = ooef017 
#      AND glaaent  = ooefent 
#      AND ooef001  = faab004 
#      AND ooefent  = faabent 
#      AND faab002  = p_site
#      AND faab001  = '4'
#      AND glaaent = g_enterprise
   SELECT COUNT(*) INTO l_cnt FROM ooag_t
    WHERE ooagent = g_enterprise AND ooag001 = g_fabg_m.fabg001
      AND ooag004 IN (SELECT ooed004 FROM ooed_t WHERE ooedent = g_enterprise
      AND ooed001 = '5' AND ooed002 = g_fabg_m.fabgsite)
   #檢查是否該員工所屬營運據點在當前資產中心有權限   
   IF l_cnt = 0 THEN 
      LET g_errno = 'afa-00020'
      RETURN
   END IF     
   
#   IF l_cnt < 1 OR cl_null(l_cnt) THEN
#      LET g_errno = 'axr-00089' RETURN
#   END IF
      
   #人員對應帳套使用權限檢查
   IF NOT cl_null(p_ld) AND NOT cl_null(p_user) THEN
      CALL s_ld_chk_authorization(p_user,p_ld) RETURNING l_success
      IF l_success = FALSE THEN
         LET g_errno = 'axr-00022' 
         RETURN
      END IF
   END IF
END FUNCTION]]>
  </point>
