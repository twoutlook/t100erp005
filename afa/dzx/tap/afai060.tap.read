  <section id="afai060.input.body" ver="5" status="" src="s">
    <![CDATA[   
      #Page1 預設值產生於此處
      INPUT ARRAY g_faag_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_faag_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL afai060_b_fill()
            LET g_rec_b = g_faag_d.getLength()
            #add-point:資料輸入前
            {<point name="input.d.before_input"/>}
            #end add-point
         
         BEFORE ROW
            #add-point:modify段before row2
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET l_insert = FALSE
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET g_detail_idx = l_ac
            
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
         
            CALL s_transaction_begin()
            OPEN afai060_cl USING g_enterprise,g_faaf_m.faafld,g_faaf_m.faaf001,g_faaf_m.faaf002,g_faaf_m.faaf003
            IF STATUS THEN
               CALL cl_err("OPEN afai060_cl:", STATUS, 1)
               CLOSE afai060_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            
            LET g_rec_b = g_faag_d.getLength()
            
            IF g_rec_b >= l_ac 
               AND g_faag_d[l_ac].faag004 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_faag_d_t.* = g_faag_d[l_ac].*  #BACKUP
               CALL afai060_set_entry_b(l_cmd)
               #add-point:modify段after_set_entry_b
               {<point name="input.body.after_set_entry_b"/>}
               #end add-point  
               CALL afai060_set_no_entry_b(l_cmd)
               IF NOT afai060_lock_b("faag_t","'1'") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH afai060_bcl INTO g_faag_d[l_ac].faag004,g_faag_d[l_ac].faag005,g_faag_d[l_ac].faag006, 
                      g_faag_d[l_ac].faag007,g_faag_d[l_ac].faag008,g_faag_d[l_ac].faag009,g_faag_d[l_ac].faag010 

                  IF SQLCA.sqlcode THEN
                     CALL cl_err(g_faag_d_t.faag004,SQLCA.sqlcode,1)
                     LET l_lock_sw = "Y"
                  END IF
                  
                  LET g_bfill = "N"
                  CALL afai060_show()
                  LET g_bfill = "Y"
                  
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
        
         BEFORE INSERT
            
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_faag_d[l_ac].* TO NULL 
                  LET g_faag_d[l_ac].faag008 = "1"
 
            LET g_faag_d_t.* = g_faag_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL afai060_set_entry_b(l_cmd)
            #add-point:modify段after_set_entry_b
            {<point name="input.body.insert.after_set_entry_b"/>}
            #end add-point
            CALL afai060_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_faag_d[li_reproduce_target].* = g_faag_d[li_reproduce].*
 
               LET g_faag_d[li_reproduce_target].faag004 = NULL
 
            END IF
            #公用欄位給值(單身)
            
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            #add-point:單身新增
            {<point name="input.body.b_a_insert"/>}
            #end add-point
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM faag_t 
             WHERE faagent = g_enterprise AND faagld = g_faaf_m.faafld
               AND faag001 = g_faaf_m.faaf001
               AND faag002 = g_faaf_m.faaf002
               AND faag003 = g_faaf_m.faaf003
 
               AND faag004 = g_faag_d[l_ac].faag004
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前
               {<point name="input.body.b_insert"/>}
               #end add-point
            
               #同步新增到同層的table
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_faaf_m.faafld
               LET gs_keys[2] = g_faaf_m.faaf001
               LET gs_keys[3] = g_faaf_m.faaf002
               LET gs_keys[4] = g_faaf_m.faaf003
               LET gs_keys[5] = g_faag_d[g_detail_idx].faag004
               CALL afai060_insert_b('faag_t',gs_keys,"'1'")
                           
               #add-point:單身新增後
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_faag_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("faag_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL afai060_b_fill()
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert2"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR 'INSERT O.K'
               LET g_rec_b = g_rec_b + 1
            END IF
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_faag_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_faag_d.deleteElement(l_ac)
               NEXT FIELD faag004
            END IF
         
            IF g_faag_d[l_ac].faag004 IS NOT NULL
 
               THEN 
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete" mark="Y"/>}
               #end add-point 
               
               DELETE FROM faag_t
                WHERE faagent = g_enterprise AND faagld = g_faaf_m.faafld AND
                                          faag001 = g_faaf_m.faaf001 AND
                                          faag002 = g_faaf_m.faaf002 AND
                                          faag003 = g_faaf_m.faaf003 AND
 
                      faag004 = g_faag_d_t.faag004
 
                  
               #add-point:單身刪除中
               {<point name="input.body.m_delete"/>}
               #end add-point 
               
               IF SQLCA.sqlcode THEN
                  CALL cl_err("faag_t",SQLCA.sqlcode,1)
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               ELSE
                  LET g_rec_b = g_rec_b-1
                  
                  #add-point:單身刪除後
                  {<point name="input.body.a_delete"/>}
                  #end add-point
                  CALL s_transaction_end('Y','0')
               END IF 
               CLOSE afai060_bcl
               LET l_count = g_faag_d.getLength()
            END IF 
            
                           INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_faaf_m.faafld
