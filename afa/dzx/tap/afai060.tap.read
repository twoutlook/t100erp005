   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("afa","")
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT faafld,'',faaf001,faaf003,'','',faaf002,faaf004,'','',faafstus,faafownid, 
       '',faafowndp,'',faafcrtid,'',faafcrtdp,'',faafcrtdt,faafmodid,'',faafmoddt", 
                      " FROM faaf_t",
                      " WHERE faafent= ? AND faafld=? AND faaf001=? AND faaf002=? AND faaf003=? FOR  
                          UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE afai060_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE faafld,faaf001,faaf003,faaf002,faaf004,faafstus,faafownid,faafowndp,faafcrtid, 
       faafcrtdp,faafcrtdt,faafmodid,faafmoddt,t1.glaal002 ,t2.oofa011 ,t3.ooefl003 ,t4.oofa011 ,t5.ooefl003 , 
       t6.oofa011",
               " FROM faaf_t",
                              " LEFT JOIN glaal_t t1 ON t1.glaalent='"||g_enterprise||"' AND t1.glaalld=faafld AND t1.glaal001='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t2 ON t2.oofaent='"||g_enterprise||"' AND t2.oofa002='2' AND t2.oofa003=faafownid  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent='"||g_enterprise||"' AND t3.ooefl001=faafowndp AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t4 ON t4.oofaent='"||g_enterprise||"' AND t4.oofa002='2' AND t4.oofa003=faafcrtid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=faafcrtdp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t6 ON t6.oofaent='"||g_enterprise||"' AND t6.oofa002='2' AND t6.oofa003=faafmodid  ",
 
               " WHERE faafent = '" ||g_enterprise|| "' AND faafld = ? AND faaf001 = ? AND faaf002 = ? AND faaf003 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE afai060_master_referesh FROM g_sql
 
 
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_afai060 WITH FORM cl_ap_formpath("afa",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL afai060_init()   
 
      #進入選單 Menu (="N")
      CALL afai060_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_afai060
      
   END IF 
   
   CLOSE afai060_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
   
END MAIN
 
 
]]>
  </section>
  <section id="afai060.modify" ver="8" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION afai060_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_faaf_m.faafld IS NULL
   OR g_faaf_m.faaf001 IS NULL
   OR g_faaf_m.faaf002 IS NULL
   OR g_faaf_m.faaf003 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE afai060_master_referesh USING g_faaf_m.faafld,g_faaf_m.faaf001,g_faaf_m.faaf002,g_faaf_m.faaf003 INTO g_faaf_m.faafld, 
       g_faaf_m.faaf001,g_faaf_m.faaf003,g_faaf_m.faaf002,g_faaf_m.faaf004,g_faaf_m.faafstus,g_faaf_m.faafownid, 
       g_faaf_m.faafowndp,g_faaf_m.faafcrtid,g_faaf_m.faafcrtdp,g_faaf_m.faafcrtdt,g_faaf_m.faafmodid, 
       g_faaf_m.faafmoddt,g_faaf_m.faafld_desc,g_faaf_m.faafownid_desc,g_faaf_m.faafowndp_desc,g_faaf_m.faafcrtid_desc, 
       g_faaf_m.faafcrtdp_desc,g_faaf_m.faafmodid_desc
 
   ERROR ""
  
   LET g_faafld_t = g_faaf_m.faafld
   LET g_faaf001_t = g_faaf_m.faaf001
   LET g_faaf002_t = g_faaf_m.faaf002
   LET g_faaf003_t = g_faaf_m.faaf003
 
   CALL s_transaction_begin()
   
   OPEN afai060_cl USING g_enterprise,g_faaf_m.faafld,g_faaf_m.faaf001,g_faaf_m.faaf002,g_faaf_m.faaf003
   IF STATUS THEN
      CALL cl_err("OPEN afai060_cl:", STATUS, 1)
      CLOSE afai060_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE afai060_master_referesh USING g_faaf_m.faafld,g_faaf_m.faaf001,g_faaf_m.faaf002,g_faaf_m.faaf003 INTO g_faaf_m.faafld, 
       g_faaf_m.faaf001,g_faaf_m.faaf003,g_faaf_m.faaf002,g_faaf_m.faaf004,g_faaf_m.faafstus,g_faaf_m.faafownid, 
       g_faaf_m.faafowndp,g_faaf_m.faafcrtid,g_faaf_m.faafcrtdp,g_faaf_m.faafcrtdt,g_faaf_m.faafmodid, 
       g_faaf_m.faafmoddt,g_faaf_m.faafld_desc,g_faaf_m.faafownid_desc,g_faaf_m.faafowndp_desc,g_faaf_m.faafcrtid_desc, 
       g_faaf_m.faafcrtdp_desc,g_faaf_m.faafmodid_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_faaf_m.faafld,SQLCA.sqlcode,0)
      CLOSE afai060_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   CALL afai060_show()
   WHILE TRUE
      LET g_faafld_t = g_faaf_m.faafld
      LET g_faaf001_t = g_faaf_m.faaf001
      LET g_faaf002_t = g_faaf_m.faaf002
      LET g_faaf003_t = g_faaf_m.faaf003
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_faaf_m.faafmodid = g_user 
LET g_faaf_m.faafmoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      CALL afai060_input("u")
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_faaf_m.* = g_faaf_m_t.*
         CALL afai060_show()
         CALL cl_err('',9001,0)
         CALL s_transaction_end('N','0')
         EXIT WHILE
      END IF 
      
      #若有modid跟moddt則進行update
      UPDATE faaf_t SET (faafmodid,faafmoddt) = (g_faaf_m.faafmodid,g_faaf_m.faafmoddt)
       WHERE faafent = g_enterprise AND faafld = g_faafld_t
         AND faaf001 = g_faaf001_t
         AND faaf002 = g_faaf002_t
         AND faaf003 = g_faaf003_t
 
                  
      #若單頭key欄位有變更
      IF g_faaf_m.faafld != g_faafld_t 
      OR g_faaf_m.faaf001 != g_faaf001_t 
      OR g_faaf_m.faaf002 != g_faaf002_t 
      OR g_faaf_m.faaf003 != g_faaf003_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE faag_t SET faagld = g_faaf_m.faafld
                                       ,faag001 = g_faaf_m.faaf001
                                       ,faag002 = g_faaf_m.faaf002
                                       ,faag003 = g_faaf_m.faaf003
 
          WHERE faagent = g_enterprise AND faagld = g_faafld_t
            AND faag001 = g_faaf001_t
            AND faag002 = g_faaf002_t
            AND faag003 = g_faaf003_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("faag_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("faag_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_faaf_m.faafld,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE afai060_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_faaf_m.faafld,'U')
 
END FUNCTION   
]]>
