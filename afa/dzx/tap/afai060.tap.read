  <section id="afai060.b_fill" ver="6" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION afai060_b_fill()
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_faag_d.clear()    #g_faag_d 單頭及單身 
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF afai060_fill_chk(1) THEN
   
      LET g_sql = "SELECT  UNIQUE faag004,faag005,faag006,faag007,faag008,faag009,faag010 ,t1.ooefl003 FROM faag_t", 
             
                  " INNER JOIN faaf_t ON faafld = faagld ",
                  " AND faaf001 = faag001 ",
                  " AND faaf002 = faag002 ",
                  " AND faaf003 = faag003 ",
 
                  #"",
                  
                  "",
                                 " LEFT JOIN ooefl_t t1 ON t1.ooeflent='"||g_enterprise||"' AND t1.ooefl001=faag005 AND t1.ooefl002='"||g_dlang||"' ",
 
                  " WHERE faagent=? AND faagld=? AND faag001=? AND faag002=? AND faag003=?"
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      #add-point:b_fill段sql_before
      {<point name="b_fill.body.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table1) THEN
         LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
      END IF
      
      #子單身的WC
      
      
      LET g_sql = g_sql, " ORDER BY faag_t.faag004"
      
      #add-point:單身填充控制
      {<point name="b_fill.sql"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE afai060_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR afai060_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_faaf_m.faafld,g_faaf_m.faaf001,g_faaf_m.faaf002,g_faaf_m.faaf003
                                               
