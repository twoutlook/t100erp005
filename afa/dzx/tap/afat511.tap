         #更新單身key值
         UPDATE fabp_t SET fabpld = g_fabg_m.fabgld
                                       ,fabpdocno = g_fabg_m.fabgdocno
 
          WHERE fabpent = g_enterprise AND fabpld = g_fabgld_t
            AND fabpdocno = g_fabgdocno_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
