  <point name="function.afat511_faid_chk" order="11" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 財產編號、附號、卡片編號檢查
# Memo...........:
# Usage..........: CALL afat511_faid_chk()
#                  RETURNING r_success
# Input parameter: 
# Return code....: r_success      執行成功否     TRUE/FALSE
# Date & Author..: 2014/10/20 By liuym
# Modify.........:
################################################################################
PRIVATE FUNCTION afat511_faid_chk()
   DEFINE l_faah015         LIKE faah_t.faah015
   DEFINE l_n1              LIKE type_t.num5 
   DEFINE r_success         LIKE type_t.num5
   DEFINE l_n               LIKE type_t.num5
   DEFINE l_faah006         LIKE faah_t.faah006 
   DEFINE l_faah032         LIKE faah_t.faah032 
   DEFINE l_ooef017         LIKE ooef_t.ooef017 
   DEFINE l_faal003         LIKE faal_t.faal003 
   DEFINE l_year            LIKE type_t.num5
   DEFINE l_month           LIKE type_t.num5
   
   LET r_success = TRUE

   #當前折畢再單，卡片+財產編號+附號不可重複
   LET l_n1 = 0
   SELECT COUNT(*) INTO l_n1 FROM fabp_t
    WHERE fabpent = g_enterprise
      AND fabpdocno = g_fabg_m.fabgdocno
      AND fabp001 = g_fabp_d[l_ac].fabp001
      AND fabp002 = g_fabp_d[l_ac].fabp002
      AND fabp000 = g_fabp_d[l_ac].fabp000
      AND fabpseq <> g_fabp_d[l_ac].fabpseq
   IF l_n1 > 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'afa-00142'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()


      LET g_fabp_d[l_ac].fabp001 = g_fabp_d_t.fabp001
      LET g_fabp_d[l_ac].fabp002 = g_fabp_d_t.fabp002
      LET g_fabp_d[l_ac].fabp000 = g_fabp_d_t.fabp000
      LET r_success = FALSE
      RETURN r_success
   END IF

   RETURN r_success

END FUNCTION]]>
  </point>
  <point name="function.afat511_faid_def" order="12" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 財產編號、附號、卡片編號檢查
# Memo...........:
# Usage..........: CALL afat511_faid_chk()
#                  RETURNING r_success
# Input parameter: 
# Return code....: r_success      執行成功否     TRUE/FALSE
# Date & Author..: 2014/10/20 By liuym
# Modify.........:
################################################################################
PRIVATE FUNCTION afat511_faid_def()
   DEFINE l_fabp000         LIKE fabp_t.fabp000
   DEFINE l_sql             STRING
   DEFINE l_faah015         LIKE faah_t.faah015

   LET l_sql = " SELECT faaj037, faaj002, faaj001 FROM faaj_t WHERE faajent = '",g_enterprise,"'",
               "   AND NOT EXISTS (SELECT fabp000 FROM fabp_t WHERE fabpent = '",g_enterprise,"'",
               "   AND fabpdocno = '",g_fabg_m.fabgdocno,"'                                     ",
               "   AND fabp001 = faaj001                                                        ",
               "   AND fabp002 = faaj002                                                        ",
               "   AND fabp000 = faaj037)                                                       ",
