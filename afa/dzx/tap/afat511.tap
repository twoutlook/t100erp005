  <section id="afat511.b_fill" ver="25" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION afat511_b_fill()
   DEFINE p_wc2      STRING
   DEFINE li_idx     LIKE type_t.num10
   #add-point:b_fill段define
   {<point name="b_fill.define" edit="s"/>}
   #end add-point     
   #add-point:b_fill段define(客製用)
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point     
 
   CALL g_fabp_d.clear()    #g_fabp_d 單頭及單身 
   CALL g_fabp4_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF afat511_fill_chk(1) THEN
      #切換上下筆時不重組SQL
      IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
         LET g_sql = "SELECT  UNIQUE fabpseq,fabp001,fabp002,fabp000,fabp004,fabp005,fabp003,fabp006, 
             fabp007,fabp010,fabp011,fabp012,fabp013,fabp014,fabp015,fabp016,fabp020,fabp021,fabp022, 
             fabp023,fabp024,fabp025,fabp026,fabpseq,fabp100,fabp101,fabp102,fabp103,fabp110,fabp111, 
             fabp112,fabp113 ,t1.faacl003 ,t2.faadl003 ,t3.oocql004 ,t4.glacl004 ,t5.glacl004 ,t6.glacl004 , 
             t7.glacl004 ,t8.faacl003 ,t9.faadl003 ,t10.oocql004 ,t11.glacl004 ,t12.glacl004 ,t13.glacl004 , 
             t14.glacl004 FROM fabp_t",   
                     " INNER JOIN fabg_t ON fabgld = fabpld ",
                     " AND fabgdocno = fabpdocno ",
 
                     #"",
                     
                     "",
                                    " LEFT JOIN faacl_t t1 ON t1.faaclent='"||g_enterprise||"' AND t1.faacl001=fabp010 AND t1.faacl002='"||g_dlang||"' ",
               " LEFT JOIN faadl_t t2 ON t2.faadlent='"||g_enterprise||"' AND t2.faadl001=fabp011 AND t2.faadl002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t3 ON t3.oocqlent='"||g_enterprise||"' AND t3.oocql001='3903' AND t3.oocql002=fabp012 AND t3.oocql003='"||g_dlang||"' ",
               " LEFT JOIN glacl_t t4 ON t4.glaclent='"||g_enterprise||"' AND t4.glacl001='' AND t4.glacl002=fabp013 AND t4.glacl003='"||g_dlang||"' ",
               " LEFT JOIN glacl_t t5 ON t5.glaclent='"||g_enterprise||"' AND t5.glacl001='' AND t5.glacl002=fabp014 AND t5.glacl003='"||g_dlang||"' ",
               " LEFT JOIN glacl_t t6 ON t6.glaclent='"||g_enterprise||"' AND t6.glacl001='' AND t6.glacl002=fabp015 AND t6.glacl003='"||g_dlang||"' ",
               " LEFT JOIN glacl_t t7 ON t7.glaclent='"||g_enterprise||"' AND t7.glacl001='' AND t7.glacl002=fabp016 AND t7.glacl003='"||g_dlang||"' ",
               " LEFT JOIN faacl_t t8 ON t8.faaclent='"||g_enterprise||"' AND t8.faacl001=fabp020 AND t8.faacl002='"||g_dlang||"' ",
               " LEFT JOIN faadl_t t9 ON t9.faadlent='"||g_enterprise||"' AND t9.faadl001=fabp021 AND t9.faadl002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t10 ON t10.oocqlent='"||g_enterprise||"' AND t10.oocql001='3903' AND t10.oocql002=fabp022 AND t10.oocql003='"||g_dlang||"' ",
               " LEFT JOIN glacl_t t11 ON t11.glaclent='"||g_enterprise||"' AND t11.glacl001='' AND t11.glacl002=fabp023 AND t11.glacl003='"||g_dlang||"' ",
               " LEFT JOIN glacl_t t12 ON t12.glaclent='"||g_enterprise||"' AND t12.glacl001='' AND t12.glacl002=fabp024 AND t12.glacl003='"||g_dlang||"' ",
               " LEFT JOIN glacl_t t13 ON t13.glaclent='"||g_enterprise||"' AND t13.glacl001='' AND t13.glacl002=fabp025 AND t13.glacl003='"||g_dlang||"' ",
               " LEFT JOIN glacl_t t14 ON t14.glaclent='"||g_enterprise||"' AND t14.glacl001='' AND t14.glacl002=fabp026 AND t14.glacl003='"||g_dlang||"' ",
 
                     " WHERE fabpent=? AND fabpld=? AND fabpdocno=?"
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before
         {<point name="b_fill.body.fill_sql"/>}
         #end add-point
         IF NOT cl_null(g_wc2_table1) THEN
            LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY fabp_t.fabpseq"
         
         #add-point:單身填充控制
         {<point name="b_fill.sql"/>}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE afat511_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR afat511_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_fabg_m.fabgld,g_fabg_m.fabgdocno
                                               
      FOREACH b_fill_cs INTO g_fabp_d[l_ac].fabpseq,g_fabp_d[l_ac].fabp001,g_fabp_d[l_ac].fabp002,g_fabp_d[l_ac].fabp000, 
          g_fabp_d[l_ac].fabp004,g_fabp_d[l_ac].fabp005,g_fabp_d[l_ac].fabp003,g_fabp_d[l_ac].fabp006, 
          g_fabp_d[l_ac].fabp007,g_fabp_d[l_ac].fabp010,g_fabp_d[l_ac].fabp011,g_fabp_d[l_ac].fabp012, 
          g_fabp_d[l_ac].fabp013,g_fabp_d[l_ac].fabp014,g_fabp_d[l_ac].fabp015,g_fabp_d[l_ac].fabp016, 
          g_fabp_d[l_ac].fabp020,g_fabp_d[l_ac].fabp021,g_fabp_d[l_ac].fabp022,g_fabp_d[l_ac].fabp023, 
          g_fabp_d[l_ac].fabp024,g_fabp_d[l_ac].fabp025,g_fabp_d[l_ac].fabp026,g_fabp4_d[l_ac].fabpseq, 
