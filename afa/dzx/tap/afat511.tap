  <section id="afat511.update_b" ver="25" status="" src="s" readonly="">
    <![CDATA[#+ 修改單身後其他table連動
PRIVATE FUNCTION afat511_update_b(ps_table,ps_keys,ps_keys_bak,ps_page)
   DEFINE ps_table         STRING
   DEFINE ps_page          STRING
   DEFINE ps_keys          DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ps_keys_bak      DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ls_group         STRING
   DEFINE li_idx           LIKE type_t.num10 
   DEFINE lb_chk           BOOLEAN
   DEFINE l_new_key        DYNAMIC ARRAY OF STRING
   DEFINE l_old_key        DYNAMIC ARRAY OF STRING
   DEFINE l_field_key      DYNAMIC ARRAY OF STRING
   #add-point:update_b段define
   {<point name="update_b.define" edit="s"/>}
   #end add-point   
   #add-point:update_b段define(客製用)
   {<point name="update_b.define_customerization" edit="c"/>}
   #end add-point   
   
   LET g_update = TRUE   
   
   #判斷key是否有改變
   LET lb_chk = TRUE
   FOR li_idx = 1 TO ps_keys.getLength()
      IF ps_keys[li_idx] <> ps_keys_bak[li_idx] THEN
         LET lb_chk = FALSE
         EXIT FOR
      END IF
   END FOR
   
   #不需要做處理
   IF lb_chk THEN
      RETURN
   END IF
   
   #判斷是否是同一群組的table
   LET ls_group = "'1','2',"
   IF ls_group.getIndexOf(ps_page,1) > 0 AND ps_table <> "fabp_t" THEN
      #add-point:update_b段修改前
      {<point name="update_b.before_update" mark="Y"/>}
      #end add-point 
      
      #將遮罩欄位還原
      CALL afat511_fabp_t_mask_restore('restore_mask_o')
               
      UPDATE fabp_t 
         SET (fabpld,fabpdocno,
              fabpseq
              ,fabp001,fabp002,fabp000,fabp004,fabp005,fabp003,fabp006,fabp007,fabp010,fabp011,fabp012,fabp013,fabp014,fabp015,fabp016,fabp020,fabp021,fabp022,fabp023,fabp024,fabp025,fabp026,fabp100,fabp101,fabp102,fabp103,fabp110,fabp111,fabp112,fabp113) 
              = 
             (ps_keys[1],ps_keys[2],ps_keys[3]
              ,g_fabp_d[g_detail_idx].fabp001,g_fabp_d[g_detail_idx].fabp002,g_fabp_d[g_detail_idx].fabp000, 
                  g_fabp_d[g_detail_idx].fabp004,g_fabp_d[g_detail_idx].fabp005,g_fabp_d[g_detail_idx].fabp003, 
                  g_fabp_d[g_detail_idx].fabp006,g_fabp_d[g_detail_idx].fabp007,g_fabp_d[g_detail_idx].fabp010, 
                  g_fabp_d[g_detail_idx].fabp011,g_fabp_d[g_detail_idx].fabp012,g_fabp_d[g_detail_idx].fabp013, 
                  g_fabp_d[g_detail_idx].fabp014,g_fabp_d[g_detail_idx].fabp015,g_fabp_d[g_detail_idx].fabp016, 
                  g_fabp_d[g_detail_idx].fabp020,g_fabp_d[g_detail_idx].fabp021,g_fabp_d[g_detail_idx].fabp022, 
                  g_fabp_d[g_detail_idx].fabp023,g_fabp_d[g_detail_idx].fabp024,g_fabp_d[g_detail_idx].fabp025, 
                  g_fabp_d[g_detail_idx].fabp026,g_fabp4_d[g_detail_idx].fabp100,g_fabp4_d[g_detail_idx].fabp101, 
                  g_fabp4_d[g_detail_idx].fabp102,g_fabp4_d[g_detail_idx].fabp103,g_fabp4_d[g_detail_idx].fabp110, 
                  g_fabp4_d[g_detail_idx].fabp111,g_fabp4_d[g_detail_idx].fabp112,g_fabp4_d[g_detail_idx].fabp113)  

         WHERE fabpent = g_enterprise AND fabpld = ps_keys_bak[1] AND fabpdocno = ps_keys_bak[2] AND fabpseq = ps_keys_bak[3]
      #add-point:update_b段修改中
      {<point name="update_b.m_update"/>}
      #end add-point   
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "fabp_t" 
            LET g_errparam.code   = "std-00009" 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            CALL s_transaction_end('N','0')
         WHEN SQLCA.sqlcode #其他錯誤
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "fabp_t" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            CALL s_transaction_end('N','0')
         OTHERWISE
            
      END CASE
      
      #將遮罩欄位進行遮蔽
      CALL afat511_fabp_t_mask_restore('restore_mask_n')
               
      #add-point:update_b段修改後
      {<point name="update_b.after_update"/>}
      #end add-point  
   END IF
   
 
   
 
   
   #add-point:update_b段other
   {<point name="update_b.other"/>}
   #end add-point  
   
END FUNCTION
]]>
