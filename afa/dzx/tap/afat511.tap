  <point name="function.afat511_get_faah" order="7" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 根据卡片+財產編號+附號自動帶出成本等相关资料
# Memo...........:
# Usage..........: CALL afat511_get_faah()
# Modify.........:
################################################################################
PRIVATE FUNCTION afat511_get_faah()

   #      资产状态/名称/规格/成本
   SELECT faah015,faah012,faah013,faaj016,
   #      累折/主要类型/次要类型/资产组
          faaj017,faah006,faah007,faah008,
   #      资产科目/累折科目/折旧科目/减值准备科目
          faaj023,faaj024,faaj025,faaj026
     INTO g_fabp_d[l_ac].fabp003,g_fabp_d[l_ac].fabp004,g_fabp_d[l_ac].fabp005,g_fabp_d[l_ac].fabp006,
          g_fabp_d[l_ac].fabp007,g_fabp_d[l_ac].fabp010,g_fabp_d[l_ac].fabp011,g_fabp_d[l_ac].fabp012,
          g_fabp_d[l_ac].fabp013,g_fabp_d[l_ac].fabp014,g_fabp_d[l_ac].fabp015,g_fabp_d[l_ac].fabp016
     FROM faah_t LEFT JOIN faaj_t ON faajent = faahent 
                                 AND faaj000 = faah000
                                 AND faaj001 = faah003
                                 AND faaj002 = faah004
                                 AND faaj037 = faah001
    WHERE faahent = g_enterprise 
      AND faah001 = g_fabp_d[l_ac].fabp000
      AND faah003 = g_fabp_d[l_ac].fabp001
      AND faah004 = g_fabp_d[l_ac].fabp002
      AND faajld = g_fabg_m.fabgld

   #预设新值
   LET g_fabp_d[l_ac].fabp020 = g_fabp_d[l_ac].fabp010
   LET g_fabp_d[l_ac].fabp021 = g_fabp_d[l_ac].fabp011
   LET g_fabp_d[l_ac].fabp022 = g_fabp_d[l_ac].fabp012
   LET g_fabp_d[l_ac].fabp023 = g_fabp_d[l_ac].fabp013
   LET g_fabp_d[l_ac].fabp024 = g_fabp_d[l_ac].fabp014
   LET g_fabp_d[l_ac].fabp025 = g_fabp_d[l_ac].fabp015
   LET g_fabp_d[l_ac].fabp026 = g_fabp_d[l_ac].fabp016
   #显示名称
   CALL afat511_fabp010_desc()
   CALL afat511_fabp011_desc()
   CALL afat511_fabp012_desc()
   CALL afat511_fabp013_desc()
   CALL afat511_fabp014_desc()
   CALL afat511_fabp015_desc()
   CALL afat511_fabp016_desc()
   CALL afat511_fabp020_desc()
   CALL afat511_fabp021_desc()
   CALL afat511_fabp022_desc()
   CALL afat511_fabp023_desc()
   CALL afat511_fabp024_desc()
   CALL afat511_fabp025_desc()
   CALL afat511_fabp026_desc()
   #获取本位币二、三币别、汇率、成本
   CALL afat511_get_rate() 
END FUNCTION]]>
  </point>
  <point name="function.afat511_get_rate" order="8" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 获取本位币二三幣別和汇率
# Memo...........:
# Usage..........: CALL afat511_get_rate()
# Modify.........:
################################################################################
PRIVATE FUNCTION afat511_get_rate()
   DEFINE l_curr       LIKE glaa_t.glaa001 #來源幣別
   
   IF g_glaa.glaa015 = 'Y' THEN 
      #帳套本位幣二幣別 
      LET g_fabp4_d[l_ac].fabp100 = g_glaa.glaa016       
      
      IF g_glaa.glaa017='1' THEN #依交易原幣轉換
         LET l_curr=g_fabp4_d[l_ac].fabp100
      ELSE
         LET l_curr=g_glaa.glaa001
      END IF
      #主帳套本位幣二匯率
                               #匯率參照表;帳套;           日期;      來源幣別
      CALL s_aooi160_get_exrate('2',g_glaa.glaald,g_fabg_m.fabgdocdt,l_curr,
                                #目的幣別;          交易金額; 匯類類型
                                g_glaa.glaa016 ,0,g_glaa.glaa018)
      RETURNING g_fabp4_d[l_ac].fabp101
      LET g_fabp4_d[l_ac].fabp102 = g_fabp4_d[l_ac].fabp101 * g_fabp_d[l_ac].fabp006
   END IF
   IF g_glaa.glaa019 = 'Y' THEN 
      #帳套本位幣三幣別 
      LET g_fabp4_d[l_ac].fabp110 = g_glaa.glaa020       
      
      IF g_glaa.glaa021='1' THEN #依交易原幣轉換
         LET l_curr=g_fabp4_d[l_ac].fabp110
      ELSE
         LET l_curr=g_glaa.glaa001
      END IF
      #主帳套本位幣三匯率
                               #匯率參照表;帳套;           日期;      來源幣別
      CALL s_aooi160_get_exrate('2',g_glaa.glaald,g_fabg_m.fabgdocdt,l_curr,
                                #目的幣別;          交易金額; 匯類類型
                                g_glaa.glaa020 ,0,g_glaa.glaa022)
      RETURNING g_fabp4_d[l_ac].fabp111     
      LET g_fabp4_d[l_ac].fabp112 = g_fabp4_d[l_ac].fabp111 * g_fabp_d[l_ac].fabp006      
   END IF
END FUNCTION]]>
  </point>
