            LET g_rec_b = g_fabp_d.getLength()
            #add-point:資料輸入前
            {<point name="input.d.before_input"/>}
            #end add-point
         
         BEFORE ROW
            #add-point:modify段before row2
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET l_insert = FALSE
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET g_detail_idx = l_ac
            LET g_current_page = 1
            
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
         
            CALL s_transaction_begin()
            OPEN afat511_cl USING g_enterprise,g_fabg_m.fabgld,g_fabg_m.fabgdocno
            IF STATUS THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "OPEN afat511_cl:" 
               LET g_errparam.code   = STATUS 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CLOSE afat511_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            
            LET g_rec_b = g_fabp_d.getLength()
            
            IF g_rec_b >= l_ac 
               AND g_fabp_d[l_ac].fabpseq IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_fabp_d_t.* = g_fabp_d[l_ac].*  #BACKUP
               LET g_fabp_d_o.* = g_fabp_d[l_ac].*  #BACKUP
               CALL afat511_set_entry_b(l_cmd)
               #add-point:modify段after_set_entry_b
               {<point name="input.body.after_set_entry_b"/>}
               #end add-point  
               CALL afat511_set_no_entry_b(l_cmd)
               IF NOT afat511_lock_b("fabp_t","'1'") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH afat511_bcl INTO g_fabp_d[l_ac].fabpseq,g_fabp_d[l_ac].fabp001,g_fabp_d[l_ac].fabp002, 
                      g_fabp_d[l_ac].fabp000,g_fabp_d[l_ac].fabp004,g_fabp_d[l_ac].fabp005,g_fabp_d[l_ac].fabp003, 
                      g_fabp_d[l_ac].fabp006,g_fabp_d[l_ac].fabp007,g_fabp_d[l_ac].fabp010,g_fabp_d[l_ac].fabp011, 
                      g_fabp_d[l_ac].fabp012,g_fabp_d[l_ac].fabp013,g_fabp_d[l_ac].fabp014,g_fabp_d[l_ac].fabp015, 
                      g_fabp_d[l_ac].fabp016,g_fabp_d[l_ac].fabp020,g_fabp_d[l_ac].fabp021,g_fabp_d[l_ac].fabp022, 
                      g_fabp_d[l_ac].fabp023,g_fabp_d[l_ac].fabp024,g_fabp_d[l_ac].fabp025,g_fabp_d[l_ac].fabp026, 
                      g_fabp4_d[l_ac].fabpseq,g_fabp4_d[l_ac].fabp100,g_fabp4_d[l_ac].fabp101,g_fabp4_d[l_ac].fabp102, 
                      g_fabp4_d[l_ac].fabp103,g_fabp4_d[l_ac].fabp110,g_fabp4_d[l_ac].fabp111,g_fabp4_d[l_ac].fabp112, 
                      g_fabp4_d[l_ac].fabp113
                  IF SQLCA.sqlcode THEN
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = g_fabp_d_t.fabpseq 
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     LET l_lock_sw = "Y"
                  END IF
                  
                  #遮罩相關處理
                  LET g_fabp_d_mask_o[l_ac].* =  g_fabp_d[l_ac].*
                  CALL afat511_fabp_t_mask()
                  LET g_fabp_d_mask_n[l_ac].* =  g_fabp_d[l_ac].*
                  
                  LET g_bfill = "N"
                  CALL afat511_show()
                  LET g_bfill = "Y"
                  
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
        
         BEFORE INSERT
            
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_fabp_d[l_ac].* TO NULL 
            INITIALIZE g_fabp_d_t.* TO NULL 
            INITIALIZE g_fabp_d_o.* TO NULL 
            #公用欄位給值(單身)
            
