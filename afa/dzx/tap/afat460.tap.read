    <![CDATA[
################################################################################
# Descriptions...: 融資
# Memo...........:
# Usage..........: CALL afat460_get_fmaz()
# Input parameter:  
# Date & Author..: 2014/8/14 By 01531
# Modify.........:
################################################################################
PRIVATE FUNCTION afat460_get_fmaz()
   #利率
   SELECT fman005 INTO g_fabj_d[l_ac].fabj009 
     FROM fman_t LEFT OUTER JOIN fmaz_t
       ON fman001 = fmaz004 AND fman009 = fmaz011
    WHERE fmaz011 = g_fabj_d[l_ac].fabj007
      AND fmaz004 = g_fabj_d[l_ac].fabj008

   #幣別
   SELECT fmaz006 INTO g_fabj_d[l_ac].fabj010 
     FROM fmaz_t
    WHERE fmaz011 = g_fabj_d[l_ac].fabj007
      AND fmaz004 = g_fabj_d[l_ac].fabj008 

   #原幣金額
   SELECT SUM(fmaz010) INTO g_fabj_d[l_ac].fabj012 FROM fmaz_t
    WHERE fmaz011 = g_fabj_d[l_ac].fabj007
      AND fmaz004 = g_fabj_d[l_ac].fabj008 
      AND fmaz003 <= MONTH(g_faba_m.fabadocdt)
   
   #本幣金額   
   IF NOT cl_null(g_fabj_d[l_ac].fabj011) THEN
      LET g_fabj_d[l_ac].fabj013 = g_fabj_d[l_ac].fabj012 * g_fabj_d[l_ac].fabj011
      DISPLAY BY NAME g_fabj_d[l_ac].fabj013
   END IF   

   DISPLAY BY NAME g_fabj_d[l_ac].fabj009 
   DISPLAY BY NAME g_fabj_d[l_ac].fabj010
   DISPLAY BY NAME g_fabj_d[l_ac].fabj011   
END FUNCTION]]>
