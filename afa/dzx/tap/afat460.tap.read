################################################################################
PUBLIC FUNCTION afat460_fabj001_fabj002_fabj003_chk(p_cmd)
DEFINE p_cmd       LIKE type_t.chr1
DEFINE l_cnt       LIKE type_t.num5
DEFINE l_cnt1      LIKE type_t.num5   
DEFINE l_fabj003   LIKE fabj_t.fabj003
DEFINE l_fabj001   LIKE fabj_t.fabj001
DEFINE l_fabj002   LIKE fabj_t.fabj002
DEFINE l_flag      LIKE type_t.num5
DEFINE l_sql       STRING
DEFINE l_yy        LIKE type_t.chr4
DEFINE l_mm        LIKE type_t.chr2
DEFINE l_n         LIKE type_t.num5 
DEFINE  l_faak032  LIKE faak_t.faak032

   LET g_errno=""
   IF cl_null(g_fabj_d[l_ac].fabj001) OR cl_null(g_fabj_d[l_ac].fabj003) THEN
      RETURN TRUE   
   END IF
   IF cl_null(g_fabj_d[l_ac].fabj002) THEN
       LET g_fabj_d[l_ac].fabj002 = ' '
   END IF
   
   INITIALIZE g_chkparam.* TO NULL
   LET g_chkparam.arg1 = g_fabj_d[l_ac].fabj001
   LET g_chkparam.arg2 = g_fabj_d[l_ac].fabj002
   LET g_chkparam.arg3 = g_fabj_d[l_ac].fabj003
   IF NOT cl_chk_exist("v_faak003_1") THEN
      RETURN FALSE
   END IF
  
      #當前利息資本化單，卡片+財產編號+附號不可重複
      LET l_n = 0
      IF p_cmd = 'a' THEN
         SELECT COUNT(*) INTO l_n FROM fabj_t
         WHERE fabjent = g_enterprise
           AND fabjdocno = g_faba_m.fabadocno
           AND fabj001 = g_fabj_d[l_ac].fabj001 
           AND fabj002 = g_fabj_d[l_ac].fabj002
           AND fabj003 = g_fabj_d[l_ac].fabj003
      ELSE
          SELECT COUNT(*) INTO l_n FROM fabj_t
         WHERE fabjent = g_enterprise
           AND fabjdocno = g_faba_m.fabadocno
           AND fabj001 = g_fabj_d[l_ac].fabj001 
           AND fabj002 = g_fabj_d[l_ac].fabj002
           AND fabj003 = g_fabj_d[l_ac].fabj003
           AND fabjseq <> g_fabj_d_t.fabjseq
      END IF
      IF l_n > 0   THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'afa-00151'
         LET g_errparam.extend = g_fabj_d[l_ac].fabj001||"|"||' '||"|"||g_fabj_d[l_ac].fabj003
         LET g_errparam.popup = FALSE
         CALL cl_err()
         RETURN FALSE            
      END IF  
      
   #相同年月中，底稿编号不可重复
   #LET l_cnt1 = 0
   #LET l_mm = MONTH(g_faba_m.fabadocdt) USING '&&'
   #LET l_yy = YEAR(g_faba_m.fabadocdt) USING '&&&&'
   #SELECT COUNT(*) INTO l_cnt1 
   #  FROM fabj_t,faba_t
   # WHERE fabjent = fabaent 
   #   AND fabjdocno = fabadocno
   #   AND fabjent = g_enterprise
   #   AND TO_CHAR(fabadocdt,'MM') = l_mm
   #   AND TO_CHAR(fabadocdt,'YYYY') = l_yy   
   #   AND fabj001 = g_fabj_d[l_ac].fabj001  AND fabj003 = g_fabj_d[l_ac].fabj003 
   #   AND fabj002 = ' ' AND fabastus = 'Y'
   #   IF l_cnt1>0 THEN
   #      INITIALIZE g_errparam TO NULL
   #      LET g_errparam.code = 'afa-00173'
   #      LET g_errparam.extend = g_fabj_d[l_ac].fabj001||"|"||' '||"|"||g_fabj_d[l_ac].fabj003 
   #      LET g_errparam.popup = FALSE
   #      CALL cl_err()
   #      RETURN FALSE
   #   END IF 
 
               
    #检查财编+附号+卡号 的归属法人和单头的归属法人一致
     SELECT faak032 INTO l_faak032 FROM faak_t
      WHERE faakent = g_enterprise 
        AND faak003 = g_fabj_d[l_ac].fabj001
        AND faak004 = g_fabj_d[l_ac].fabj002
        AND faak001 = g_fabj_d[l_ac].fabj003
     IF l_faak032 <> g_faba_m.fabacomp THEN
        INITIALIZE g_errparam TO NULL
        LET g_errparam.code = 'afa-00296'
        LET g_errparam.extend = ''
        LET g_errparam.popup = TRUE
        CALL cl_err() 
        RETURN FALSE
     END IF 
     
    #带出默认值
            
    CALL afat460_get_faak()
    #CALL afat460_get_rate()
    RETURN TRUE  
END FUNCTION]]>
