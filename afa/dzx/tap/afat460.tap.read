  <point name="function.afat460_unconf_upd_faba" order="6" ver="3" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...:  
# Memo...........:
# Usage..........: CALL afat460_unconf_upd_faba()
# Input parameter:  
# Date & Author..: 2014/8/10 By 01531
# Modify.........:
################################################################################
PRIVATE FUNCTION afat460_unconf_upd_faba()
   DEFINE r_success           LIKE type_t.num5
   DEFINE l_ooab002           LIKE ooab_t.ooab002

   WHENEVER ERROR CONTINUE
   
   LET r_success  = TRUE
   
   #刪除faak附屬費用資料
   DELETE FROM faak_t 
    WHERE faakent = g_enterprise
      AND faak002 = '3'
      AND faak041 = g_faba_m.fabadocno      

   IF SQLCA.sqlcode THEN
      #CALL cl_errmsg('del faak ','','',SQLCA.sqlcode,1)
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend = 'del faak '
      LET g_errparam.popup = TRUE
      CALL cl_err()
      LET r_success = FALSE  
   END IF 
   
   #清空fabj轉固定財產日期
   UPDATE fabj_t SET fabj014 = ''                
               WHERE fabjent = g_enterprise
                 AND fabjdocno = g_faba_m.fabadocno
   IF SQLCA.sqlcode THEN
     # CALL cl_errmsg('upd fabj ','','',SQLCA.sqlcode,1)
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend = 'upd fabj  '
      LET g_errparam.popup = TRUE
      CALL cl_err()
      LET r_success = FALSE  
   END IF                  
   
   CALL cl_get_para(g_enterprise,g_faba_m.fabacomp,'S-FIN-9005') RETURNING l_ooab002
   IF l_ooab002 = 'Y' THEN
      UPDATE fabj_t SET fabj017 = ''                
               WHERE fabjent = g_enterprise
                 AND fabjdocno = g_faba_m.fabadocno
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = 'upd fabj  '
         LET g_errparam.popup = TRUE
         CALL cl_err()
         LET r_success = FALSE  
      END IF                    
   END IF
     
   RETURN r_success
END FUNCTION]]>
  </point>
