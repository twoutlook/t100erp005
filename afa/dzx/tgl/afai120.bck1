      LET g_faak_m.faakmodid = g_user
      LET g_faak_m.faakmoddt = cl_get_current()
      LET g_faak_m.faakstus = 'N'
 
 
 
   
   CALL s_transaction_begin()
   
   #add-point:複製輸入前 name="reproduce.head.b_input"
   {<point name="reproduce.head.b_input"/>}
   #end add-point
   
