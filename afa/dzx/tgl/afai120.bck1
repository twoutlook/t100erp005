   LET g_faakld_t = g_faak_m.faakld
   LET g_faak000_t = g_faak_m.faak000
   LET g_faak001_t = g_faak_m.faak001
   LET g_faak003_t = g_faak_m.faak003
   LET g_faak004_t = g_faak_m.faak004
 
   
   #組合新增資料的條件
