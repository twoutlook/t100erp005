                      g_faak_m.faakstus,g_faak_m.faak014,g_faak_m.faak015,g_faak_m.faak016,g_faak_m.faak017, 
                      g_faak_m.faak018,g_faak_m.faak019,g_faak_m.faak021,g_faak_m.faak022,g_faak_m.faak020, 
