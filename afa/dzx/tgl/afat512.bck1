{<section id="afat512.state_change" >}
   #+ 此段落由子樣板a09產生
#+ 確認碼變更
PRIVATE FUNCTION afat512_statechange()
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define
{<point name="statechange.define"/>}
   #end add-point  
   
   #add-point:statechange段開始前
{<point name="statechange.before"/>}
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_fabr_m.fabr003 IS NULL
      OR g_fabr_m.fabr004 IS NULL
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
 
      RETURN
   END IF
   
   #20150701--add--str--lujh
   CALL s_transaction_begin()
   
   OPEN afat512_cl USING g_enterprise,g_fabr_m.fabr003,g_fabr_m.fabr004
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN afat512_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE afat512_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE afat512_master_referesh USING g_fabr_m.fabr003,g_fabr_m.fabr004 INTO g_fabr_m.fabr001,g_fabr_m.fabr002, 
       g_fabr_m.fabrcomp,g_fabr_m.fabr003,g_fabr_m.fabr035,g_fabr_m.fabrstus,g_fabr_m.fabr004,g_fabr_m.fabr007, 
       g_fabr_m.fabr005,g_fabr_m.fabr006,g_fabr_m.fabr033,g_fabr_m.fabr034,g_fabr_m.fabr008,g_fabr_m.fabr009, 
       g_fabr_m.fabr010,g_fabr_m.fabr011,g_fabr_m.fabr012,g_fabr_m.fabr013,g_fabr_m.fabr015,g_fabr_m.fabr014, 
       g_fabr_m.fabr016,g_fabr_m.fabr019,g_fabr_m.fabr017,g_fabr_m.fabr018,g_fabr_m.fabr020,g_fabr_m.fabr021, 
       g_fabr_m.fabr031,g_fabr_m.fabr032,g_fabr_m.fabr022,g_fabr_m.fabr023,g_fabr_m.fabr025,g_fabr_m.fabr024, 
       g_fabr_m.fabr026,g_fabr_m.fabr028,g_fabr_m.fabr036,g_fabr_m.fabr027,g_fabr_m.fabr029,g_fabr_m.fabr030, 
       g_fabr_m.fabrownid,g_fabr_m.fabrowndp,g_fabr_m.fabrcrtid,g_fabr_m.fabrcrtdp,g_fabr_m.fabrcrtdt, 
       g_fabr_m.fabrmodid,g_fabr_m.fabrmoddt,g_fabr_m.fabrcnfid,g_fabr_m.fabrcnfdt,g_fabr_m.fabrpstid, 
       g_fabr_m.fabrpstdt,g_fabr_m.fabr037,g_fabr_m.fabr038,g_fabr_m.fabr039,g_fabr_m.fabr040,
       g_fabr_m.fabr041,g_fabr_m.fabr042,g_fabr_m.fabr043,     
       g_fabr_m.fabr001_desc,g_fabr_m.fabr002_desc,g_fabr_m.fabrcomp_desc,g_fabr_m.fabr010_desc, 
       g_fabr_m.fabr011_desc,g_fabr_m.fabr015_desc,g_fabr_m.fabr014_desc,g_fabr_m.fabr016_desc,g_fabr_m.fabr019_desc, 
       g_fabr_m.fabr017_desc,g_fabr_m.fabr018_desc,g_fabr_m.fabr020_desc,g_fabr_m.fabr021_desc,g_fabr_m.fabr032_desc, 
       g_fabr_m.fabr022_desc,g_fabr_m.fabr025_desc,g_fabr_m.fabr024_desc,g_fabr_m.fabr026_desc,g_fabr_m.fabr028_desc, 
       g_fabr_m.fabr036_desc,g_fabr_m.fabr027_desc,g_fabr_m.fabr029_desc,g_fabr_m.fabr030_desc,g_fabr_m.fabrownid_desc, 
       g_fabr_m.fabrowndp_desc,g_fabr_m.fabrcrtid_desc,g_fabr_m.fabrcrtdp_desc,g_fabr_m.fabrmodid_desc, 
       g_fabr_m.fabrcnfid_desc,g_fabr_m.fabrpstid_desc,g_fabr_m.fabr044,g_fabr_m.fabr045,g_fabr_m.fabr046
       
   #將資料顯示到畫面上
   DISPLAY BY NAME g_fabr_m.fabr001,g_fabr_m.fabr002,g_fabr_m.fabrcomp,g_fabr_m.fabr003,g_fabr_m.fabrstus, 
       g_fabr_m.fabr004,g_fabr_m.fabr007,g_fabr_m.fabr005,g_fabr_m.fabr006,g_fabr_m.fabr033,g_fabr_m.fabr034, 
       g_fabr_m.fabr008,g_fabr_m.fabr010,g_fabr_m.fabr011,g_fabr_m.fabr012,g_fabr_m.fabr013,g_fabr_m.fabr015, 
       g_fabr_m.fabr014,g_fabr_m.fabr016,g_fabr_m.fabr019,g_fabr_m.fabr017,g_fabr_m.fabr018,g_fabr_m.fabr020, 
       g_fabr_m.fabr021,g_fabr_m.fabr031,g_fabr_m.fabr032,g_fabr_m.fabr022,g_fabr_m.fabr023,g_fabr_m.fabr025, 
       g_fabr_m.fabr024,g_fabr_m.fabr026,g_fabr_m.fabr028,g_fabr_m.fabr036,g_fabr_m.fabr027,g_fabr_m.fabr029, 
       g_fabr_m.fabr030,g_fabr_m.fabr037,g_fabr_m.fabr038,g_fabr_m.fabr039,g_fabr_m.fabr040,g_fabr_m.fabr041,
       g_fabr_m.fabr042,g_fabr_m.fabr043,g_fabr_m.fabr044,g_fabr_m.fabr045,g_fabr_m.fabr046
   
   CASE g_fabr_m.fabrstus
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         
      END CASE
   #20150701--add--end--lujh
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_fabr_m.fabrstus
            WHEN "N"
               HIDE OPTION "unconfirmed"
            WHEN "X"
               HIDE OPTION "invalid"
            WHEN "Y"
               HIDE OPTION "confirmed"
            
         END CASE
     
      #add-point:menu前
{<point name="statechange.before_menu"/>}
      #end add-point
      
      ON ACTION unconfirmed
         IF cl_auth_chk_act("unconfirmed") THEN
            LET lc_state = "N"
            #add-point:action控制
{<point name="statechange.unconfirmed"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION invalid
         IF cl_auth_chk_act("invalid") THEN
            LET lc_state = "X"
            #add-point:action控制
{<point name="statechange.invalid"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION confirmed
         IF cl_auth_chk_act("confirmed") THEN
            LET lc_state = "Y"
            #add-point:action控制
{<point name="statechange.confirmed"/>}
            #end add-point
         END IF
         EXIT MENU
    
      
      
      #add-point:stus控制
{<point name="statechange.more_control"/>}
      #end add-point
      
   END MENU
   
   IF (lc_state <> "N" 
      AND lc_state <> "X"
      AND lc_state <> "Y"
      ) OR 
      cl_null(lc_state) THEN
      CALL s_transaction_end('N','0')   #160812-00017#6 20160822 add by beckxie
      RETURN
   END IF
   
   #add-point:stus修改前
{<point name="statechange.b_update"/>}
   #end add-point
      
   UPDATE fabr_t SET fabrstus = lc_state,
                     fabrcnfid = g_fabr_m.fabrcnfid,
                      fabrcnfdt = g_fabr_m.fabrcnfdt                   
    WHERE fabrent = g_enterprise AND fabr003 = g_fabr_m.fabr003
      AND fabr004 = g_fabr_m.fabr004
  
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
 
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         
      END CASE
      LET g_fabr_m.fabrstus = lc_state
      DISPLAY BY NAME g_fabr_m.fabrstus
   END IF
 
   #add-point:stus修改後
{<point name="statechange.a_update"/>}
   #end add-point
 
   #add-point:statechange段結束前
{<point name="statechange.after"/>}
   #end add-point  
   
   #20150701--add--str--lujh
   CLOSE afat512_cl
   CALL s_transaction_end('Y','0')
   #20150701--add--end--lujh
END FUNCTION
 
 

