               " LEFT JOIN ooag_t t6 ON t6.ooagent="||g_enterprise||" AND t6.ooag001=t0.faafmodid  ",

               " WHERE t0.faafent = " ||g_enterprise|| " AND t0.faafld = ? AND t0.faaf001 = ? AND t0.faaf002 = ? AND t0.faaf003 = ?"
