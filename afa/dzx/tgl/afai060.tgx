               #將遮罩欄位還原
               CALL afai060_faag_t_mask_restore('restore_mask_o')
      
               UPDATE faag_t SET (faagld,faag001,faag002,faag003,faag004,faag005,faag006,faag007,faag008,faag009,faag010) = (g_faaf_m.faafld,g_faaf_m.faaf001,g_faaf_m.faaf002,g_faaf_m.faaf003,g_faag_d[l_ac].faag004,g_faag_d[l_ac].faag005,g_faag_d[l_ac].faag006,g_faag_d[l_ac].faag007,g_faag_d[l_ac].faag008,g_faag_d[l_ac].faag009,g_faag_d[l_ac].faag010)
                WHERE faagent = g_enterprise AND faagld = g_faaf_m.faafld 
                  AND faag001 = g_faaf_m.faaf001 
                  AND faag002 = g_faaf_m.faaf002 
                  AND faag003 = g_faaf_m.faaf003 

                  AND faag004 = g_faag_d_t.faag004 #項次   

                  
               #add-point:單身修改中 name="input.body.m_update"
               {<point name="input.body.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     LET g_faag_d[l_ac].* = g_faag_d_t.*
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "faag_t" 
                     LET g_errparam.code = "std-00009" 
                     LET g_errparam.popup = TRUE 
                     CALL s_transaction_end('N','0')
                     CALL cl_err()
                     
                  WHEN SQLCA.SQLCODE #其他錯誤
                     LET g_faag_d[l_ac].* = g_faag_d_t.*  
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "faag_t:",SQLERRMESSAGE 
                     LET g_errparam.code = SQLCA.SQLCODE 
                     LET g_errparam.popup = TRUE 
                     CALL s_transaction_end('N','0')
                     CALL cl_err()                   
                     
                  OTHERWISE
                     #資料多語言用-增/改
                     
