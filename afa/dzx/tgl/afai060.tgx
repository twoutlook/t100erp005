   OPEN afai060_cl USING g_enterprise,g_faaf_m.faafld,g_faaf_m.faaf001,g_faaf_m.faaf002,g_faaf_m.faaf003
   IF SQLCA.SQLCODE THEN   #(ver:78)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN afai060_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE   #(ver:78)
      LET g_errparam.popup = TRUE 
      CLOSE afai060_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE afai060_master_referesh USING g_faaf_m.faafld,g_faaf_m.faaf001,g_faaf_m.faaf002,g_faaf_m.faaf003 INTO g_faaf_m.faafld,g_faaf_m.faaf001,g_faaf_m.faaf003,g_faaf_m.faaf002,g_faaf_m.faaf004,g_faaf_m.faafstus,g_faaf_m.faafownid,g_faaf_m.faafowndp,g_faaf_m.faafcrtid,g_faaf_m.faafcrtdp,g_faaf_m.faafcrtdt,g_faaf_m.faafmodid,g_faaf_m.faafmoddt,g_faaf_m.faafld_desc,g_faaf_m.faafownid_desc,g_faaf_m.faafowndp_desc,g_faaf_m.faafcrtid_desc,g_faaf_m.faafcrtdp_desc,g_faaf_m.faafmodid_desc
   
   #檢查是否允許此動作
   IF NOT afai060_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_faaf_m_mask_o.* =  g_faaf_m.*
   CALL afai060_faaf_t_mask()
   LET g_faaf_m_mask_n.* =  g_faaf_m.*
   
   
   
   #add-point:modify段show之前 name="modify.before_show"
   {<point name="modify.before_show"/>}
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"


   
   CALL afai060_show()
   #add-point:modify段show之後 name="modify.after_show"
   {<point name="modify.after_show"/>}
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1


    
   WHILE TRUE
      LET g_faafld_t = g_faaf_m.faafld
      LET g_faaf001_t = g_faaf_m.faaf001
      LET g_faaf002_t = g_faaf_m.faaf002
      LET g_faaf003_t = g_faaf_m.faaf003

      
      #寫入修改者/修改日期資訊(單頭)
      LET g_faaf_m.faafmodid = g_user 
LET g_faaf_m.faafmoddt = cl_get_current()
LET g_faaf_m.faafmodid_desc = cl_get_username(g_faaf_m.faafmodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      LET g_master_commit = "N"
      CALL afai060_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後 name="modify.after_input"
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE faaf_t SET (faafmodid,faafmoddt) = (g_faaf_m.faafmodid,g_faaf_m.faafmoddt)
          WHERE faafent = g_enterprise AND faafld = g_faafld_t
            AND faaf001 = g_faaf001_t
            AND faaf002 = g_faaf002_t
            AND faaf003 = g_faaf003_t

      END IF
    
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         #若單頭無commit則還原
         IF g_master_commit = "N" THEN
            LET g_faaf_m.* = g_faaf_m_t.*
            CALL afai060_show()
         END IF
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code = 9001 
         LET g_errparam.popup = FALSE 
         CALL cl_err()
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_faaf_m.faafld != g_faaf_m_t.faafld
      OR g_faaf_m.faaf001 != g_faaf_m_t.faaf001
      OR g_faaf_m.faaf002 != g_faaf_m_t.faaf002
      OR g_faaf_m.faaf003 != g_faaf_m_t.faaf003

      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前 name="modify.body.b_fk_update"
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE faag_t SET faagld = g_faaf_m.faafld
                                       ,faag001 = g_faaf_m.faaf001
                                       ,faag002 = g_faaf_m.faaf002
                                       ,faag003 = g_faaf_m.faaf003

          WHERE faagent = g_enterprise AND faagld = g_faaf_m_t.faafld
            AND faag001 = g_faaf_m_t.faaf001
            AND faag002 = g_faaf_m_t.faaf002
            AND faag003 = g_faaf_m_t.faaf003

            
         #add-point:單身fk修改中 name="modify.body.m_fk_update"
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            #   INITIALIZE g_errparam TO NULL 
            #   LET g_errparam.extend = "faag_t" 
            #   LET g_errparam.code = "std-00009" 
            #   LET g_errparam.popup = TRUE 
            #   CALL cl_err()
            #   CALL s_transaction_end('N','0')
            #   CONTINUE WHILE
            WHEN SQLCA.SQLCODE #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "faag_t:",SQLERRMESSAGE 
               LET g_errparam.code = SQLCA.SQLCODE 
               LET g_errparam.popup = TRUE 
               CALL s_transaction_end('N','0')
               CALL cl_err()
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後 name="modify.body.a_fk_update"
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         

         

         
         #UPDATE 多語言table key值
         

         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL afai060_set_act_visible()   
   CALL afai060_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " faafent = " ||g_enterprise|| " AND",
                      " faafld = '", g_faaf_m.faafld, "' "
