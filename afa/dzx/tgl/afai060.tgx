      LET g_errparam.extend = "" 
      LET g_errparam.code = "std-00003" 
      LET g_errparam.popup = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_faafld_t = g_faaf_m.faafld
   LET g_faaf001_t = g_faaf_m.faaf001
   LET g_faaf002_t = g_faaf_m.faaf002
   LET g_faaf003_t = g_faaf_m.faaf003

