                      ," AND faaf003 = '", g_faaf_m.faaf003, "' "

   #填到對應位置
   CALL afai060_browser_fill("")
 
   CLOSE afai060_cl
   
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
