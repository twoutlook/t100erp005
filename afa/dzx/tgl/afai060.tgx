               LET gs_keys_bak[1] = g_faafld_t
               LET gs_keys[2] = g_faaf_m.faaf001
               LET gs_keys_bak[2] = g_faaf001_t
               LET gs_keys[3] = g_faaf_m.faaf002
               LET gs_keys_bak[3] = g_faaf002_t
               LET gs_keys[4] = g_faaf_m.faaf003
               LET gs_keys_bak[4] = g_faaf003_t
               LET gs_keys[5] = g_faag_d[g_detail_idx].faag004
               LET gs_keys_bak[5] = g_faag_d_t.faag004
               CALL afai060_update_b('faag_t',gs_keys,gs_keys_bak,"'1'")
               END CASE
 
               #將遮罩欄位進行遮蔽
               CALL afai060_faag_t_mask_restore('restore_mask_n')
               
