      CALL afat505_01_fabo_t_mask()
      LET g_fabo7_d_mask_n[l_ac].* =  g_fabo7_d[l_ac].*
   END FOR
 
   
   LET l_ac = g_cnt
 
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
