   END FOR
   
   IF g_cnt > g_fabo_d.getLength() THEN
      LET l_ac = g_fabo_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_fabo_d.getLength()
