   DEFINE l_oldno     LIKE faah_t.faah000 
   DEFINE l_newno02     LIKE faah_t.faah001 
   DEFINE l_oldno02     LIKE faah_t.faah001 
   DEFINE l_newno03     LIKE faah_t.faah003 
   DEFINE l_oldno03     LIKE faah_t.faah003 
