   CALL cl_mask_set_no_entry()
 
   #add-point:modify段修改前 name="modify.before_input"
   {<point name="modify.before_input"/>}
   #end add-point
 
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #Page1 預設值產生於此處
      INPUT ARRAY g_faac_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_detail_cnt,WITHOUT DEFAULTS, #MAXCOUNT = g_max_rec,
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         

         #應用 a43 樣板自動產生(Version:4)
         ON ACTION update_item
            LET g_action_choice="update_item"
            IF cl_auth_chk_act("update_item") THEN
               #該樣板不需此段落
               
               #add-point:ON ACTION update_item name="input.detail_input.page1.update_item"
               {<point name="input.detail_input.page1.update_item" mark="Y"/>}
               #END add-point
               #該樣板不需此段落
               #該樣板不需此段落
            END IF
 



         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_faac_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 

            CALL afai020_b_fill(g_wc2)
            LET g_detail_cnt = g_faac_d.getLength()
         
         BEFORE ROW
            #add-point:modify段before row name="input.body.before_row2"
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_cmd = ''
            LET l_ac_t = l_ac 
            LET l_ac = g_detail_idx
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            DISPLAY g_faac_d.getLength() TO FORMONLY.cnt
         
            CALL s_transaction_begin()
            LET g_detail_cnt = g_faac_d.getLength()
            
            IF g_detail_cnt >= l_ac 
               AND g_faac_d[l_ac].faac001 IS NOT NULL

            THEN
               LET l_cmd='u'
               LET g_faac_d_t.* = g_faac_d[l_ac].*  #BACKUP
               LET g_faac_d_o.* = g_faac_d[l_ac].*  #BACKUP
               IF NOT afai020_lock_b("faac_t") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH afai020_bcl INTO g_faac_d[l_ac].faacstus,g_faac_d[l_ac].faac001,g_faac_d[l_ac].faac017,g_faac_d[l_ac].faac002,g_faac_d[l_ac].faac003,g_faac_d[l_ac].faac016,g_faac_d[l_ac].faac004,g_faac_d[l_ac].faac005,g_faac_d[l_ac].faac006,g_faac_d[l_ac].faac007,g_faac_d[l_ac].faac008,g_faac_d[l_ac].faac009,g_faac_d[l_ac].faac010,g_faac_d[l_ac].faac011,g_faac_d[l_ac].faac012,g_faac_d[l_ac].faac013,g_faac_d[l_ac].faac014,g_faac_d[l_ac].faac015,g_faac2_d[l_ac].faac001,g_faac2_d[l_ac].faacownid,g_faac2_d[l_ac].faacowndp,g_faac2_d[l_ac].faaccrtid,g_faac2_d[l_ac].faaccrtdp,g_faac2_d[l_ac].faaccrtdt,g_faac2_d[l_ac].faacmodid,g_faac2_d[l_ac].faacmoddt
                  IF SQLCA.SQLCODE THEN
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = g_faac_d_t.faac001,":",SQLERRMESSAGE  
                     LET g_errparam.code = SQLCA.SQLCODE
                     LET g_errparam.popup = TRUE 
                     CALL cl_err()
                     LET l_lock_sw = "Y"
                  END IF
                  
                  #遮罩相關處理
                  LET g_faac_d_mask_o[l_ac].* =  g_faac_d[l_ac].*
                  CALL afai020_faac_t_mask()
                  LET g_faac_d_mask_n[l_ac].* =  g_faac_d[l_ac].*
                  
                  CALL afai020_detail_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            CALL afai020_set_entry_b(l_cmd)
            CALL afai020_set_no_entry_b(l_cmd)
            #add-point:modify段before row name="input.body.before_row"
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
LET g_detail_multi_table_t.faacl001 = g_faac_d[l_ac].faac001
LET g_detail_multi_table_t.faacl002 = g_dlang
LET g_detail_multi_table_t.faacl003 = g_faac_d[l_ac].faacl003


            #其他table進行lock
            
            INITIALIZE l_var_keys TO NULL 
            INITIALIZE l_field_keys TO NULL 
            LET l_field_keys[01] = 'faaclent'
            LET l_var_keys[01] = g_enterprise
            LET l_field_keys[02] = 'faacl001'
            LET l_var_keys[02] = g_faac_d[l_ac].faac001
            LET l_field_keys[03] = 'faacl002'
            LET l_var_keys[03] = g_dlang
            IF NOT cl_multitable_lock(l_var_keys,l_field_keys,'faacl_t') THEN
               RETURN 
            END IF 


        
         BEFORE INSERT
            
            LET l_insert = TRUE
            IF s_transaction_chk("N",0) THEN
               CALL s_transaction_begin()
            END IF
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_faac_d_t.* TO NULL
            INITIALIZE g_faac_d_o.* TO NULL
            INITIALIZE g_faac_d[l_ac].* TO NULL 
            #公用欄位給值(單身)
            #應用 a14 樣板自動產生(Version:5)    
      #公用欄位新增給值  
      LET g_faac2_d[l_ac].faacownid = g_user
      LET g_faac2_d[l_ac].faacowndp = g_dept
      LET g_faac2_d[l_ac].faaccrtid = g_user
      LET g_faac2_d[l_ac].faaccrtdp = g_dept 
      LET g_faac2_d[l_ac].faaccrtdt = cl_get_current()
      LET g_faac2_d[l_ac].faacmodid = g_user
      LET g_faac2_d[l_ac].faacmoddt = cl_get_current()
      LET g_faac_d[l_ac].faacstus = ''



            #自定義預設值(單身2)
                  LET g_faac_d[l_ac].faacstus = "Y"
      LET g_faac_d[l_ac].faac017 = "2"
      LET g_faac_d[l_ac].faac005 = "N"
      LET g_faac_d[l_ac].faac006 = "0"
      LET g_faac_d[l_ac].faac007 = "Y"
      LET g_faac_d[l_ac].faac008 = "1"
      LET g_faac_d[l_ac].faac009 = "1"
      LET g_faac_d[l_ac].faac010 = "1"
      LET g_faac_d[l_ac].faac011 = "1"
      LET g_faac_d[l_ac].faac012 = "0"

            #add-point:modify段before備份 name="input.body.before_bak"
            {<point name="input.body.before_bak"/>}
            #end add-point
            LET g_faac_d_t.* = g_faac_d[l_ac].*     #新輸入資料
            LET g_faac_d_o.* = g_faac_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_faac_d[li_reproduce_target].* = g_faac_d[li_reproduce].*
               LET g_faac2_d[li_reproduce_target].* = g_faac2_d[li_reproduce].*

               LET g_faac_d[g_faac_d.getLength()].faac001 = NULL

            END IF
            
LET g_detail_multi_table_t.faacl001 = g_faac_d[l_ac].faac001
LET g_detail_multi_table_t.faacl002 = g_dlang
LET g_detail_multi_table_t.faacl003 = g_faac_d[l_ac].faacl003


            CALL afai020_set_entry_b(l_cmd)
            CALL afai020_set_no_entry_b(l_cmd)
            #add-point:modify段before insert name="input.body.before_insert"
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(1) INTO l_count FROM faac_t 
             WHERE faacent = g_enterprise AND faac001 = g_faac_d[l_ac].faac001

                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前 name="input.body.b_insert"
               {<point name="input.body.b_insert"/>}
               #end add-point
            
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_faac_d[g_detail_idx].faac001
               CALL afai020_insert_b('faac_t',gs_keys,"'1'")
                           
               #add-point:單身新增後 name="input.body.a_insert"
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE    
               INITIALIZE g_faac_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = 'INSERT' 
               LET g_errparam.code   = "std-00006" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLCODE THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "faac_t:",SQLERRMESSAGE 
               LET g_errparam.code = SQLCA.SQLCODE
               LET g_errparam.popup = TRUE 
               CALL s_transaction_end('N','0')                    
               CALL cl_err()
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL afai020_b_fill(g_wc2)
               #資料多語言用-增/改
                        INITIALIZE l_var_keys TO NULL 
         INITIALIZE l_field_keys TO NULL 
         INITIALIZE l_vars TO NULL 
         INITIALIZE l_fields TO NULL 
         INITIALIZE l_var_keys_bak TO NULL 
         IF g_faac_d[l_ac].faac001 = g_detail_multi_table_t.faacl001 AND
         g_faac_d[l_ac].faacl003 = g_detail_multi_table_t.faacl003 THEN
         ELSE 
            LET l_var_keys[01] = g_enterprise
            LET l_field_keys[01] = 'faaclent'
            LET l_var_keys_bak[01] = g_enterprise
            LET l_var_keys[02] = g_faac_d[l_ac].faac001
            LET l_field_keys[02] = 'faacl001'
            LET l_var_keys_bak[02] = g_detail_multi_table_t.faacl001
            LET l_var_keys[03] = g_dlang
            LET l_field_keys[03] = 'faacl002'
            LET l_var_keys_bak[03] = g_detail_multi_table_t.faacl002
            LET l_vars[01] = g_faac_d[l_ac].faacl003
            LET l_fields[01] = 'faacl003'
            CALL cl_multitable(l_var_keys,l_field_keys,l_vars,l_fields,l_var_keys_bak,'faacl_t')
         END IF 

               #add-point:input段-after_insert name="input.body.a_insert2"
               {<point name="input.body.a_insert2"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ##ERROR 'INSERT O.K'
               LET g_detail_cnt = g_detail_cnt + 1
               
               LET g_wc2 = g_wc2, " OR (faac001 = '", g_faac_d[l_ac].faac001, "' "

                                  ,")"
            END IF                
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' THEN
               LET l_cmd='d'
            ELSE
               #add-point:單身刪除ask前 name="input.body.b_delete_ask"
               {<point name="input.body.b_delete_ask"/>}
               #end add-point   
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "" 
                  LET g_errparam.code   = -263 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  CANCEL DELETE
               END IF
               
               #add-point:單身刪除前 name="input.body.b_delete"
               {<point name="input.body.b_delete" mark="Y"/>}
               #end add-point   
               
               DELETE FROM faac_t
                WHERE faacent = g_enterprise AND 
