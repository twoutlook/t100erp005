{<section id="afai020.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION afai020_b_fill(p_wc2)              #BODY FILL UP
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   {<point name="b_fill.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE p_wc2            STRING
   DEFINE ls_owndept_list  STRING  #(ver:35) add
   DEFINE ls_ownuser_list  STRING  #(ver:35) add
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   {<point name="b_fill.define"/>}
   #end add-point
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   {<point name="b_fill.pre_function"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前 name="b_fill.sql_before"
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  DISTINCT t0.faacstus,t0.faac001,t0.faac017,t0.faac002,t0.faac003,t0.faac016,t0.faac004,t0.faac005,t0.faac006,t0.faac007,t0.faac008,t0.faac009,t0.faac010,t0.faac011,t0.faac012,t0.faac013,t0.faac014,t0.faac015,t0.faac001,t0.faacownid,t0.faacowndp,t0.faaccrtid,t0.faaccrtdp,t0.faaccrtdt,t0.faacmodid,t0.faacmoddt ,t1.ooag011 ,t2.ooefl003 ,t3.ooag011 ,t4.ooefl003 ,t5.ooag011 FROM faac_t t0",
               " LEFT JOIN faacl_t ON faaclent = "||g_enterprise||" AND faac001 = faacl001 AND faacl002 = '",g_dlang,"'",
                              " LEFT JOIN ooag_t t1 ON t1.ooagent="||g_enterprise||" AND t1.ooag001=t0.faacownid  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent="||g_enterprise||" AND t2.ooefl001=t0.faacowndp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent="||g_enterprise||" AND t3.ooag001=t0.faaccrtid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent="||g_enterprise||" AND t4.ooefl001=t0.faaccrtdp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=t0.faacmodid  ",

               " WHERE t0.faacent= ?  AND  1=1 AND (", p_wc2, ") "
 
   #(ver:35) ---add start---
      #應用 a68 樣板自動產生(Version:1)
   #若是修改，須視權限加上條件
   IF g_action_choice = "modify" OR g_action_choice = "modify_detail" THEN
      LET ls_owndept_list = NULL
      LET ls_ownuser_list = NULL

      #若有設定部門權限
      CALL cl_get_owndept_list("faac_t","modify") RETURNING ls_owndept_list
      IF NOT cl_null(ls_owndept_list) THEN
         LET g_sql = g_sql, " AND faacowndp IN (",ls_owndept_list,")"
      END IF

      #若有設定個人權限
      CALL cl_get_ownuser_list("faac_t","modify") RETURNING ls_ownuser_list
      IF NOT cl_null(ls_ownuser_list) THEN
         LET g_sql = g_sql," AND faacownid IN (",ls_ownuser_list,")"
      END IF
   END IF



   #(ver:35) --- add end ---
 
   #add-point:b_fill段sql wc name="b_fill.sql_wc"
   {<point name="b_fill.sql_wc"/>}
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("faac_t"),
                      " ORDER BY t0.faac001"
   
   #add-point:b_fill段sql之後 name="b_fill.sql_after"
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"faac_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE afai020_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR afai020_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_faac_d.clear()
   CALL g_faac2_d.clear()   

 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_faac_d[l_ac].faacstus,g_faac_d[l_ac].faac001,g_faac_d[l_ac].faac017,g_faac_d[l_ac].faac002,g_faac_d[l_ac].faac003,g_faac_d[l_ac].faac016,g_faac_d[l_ac].faac004,g_faac_d[l_ac].faac005,g_faac_d[l_ac].faac006,g_faac_d[l_ac].faac007,g_faac_d[l_ac].faac008,g_faac_d[l_ac].faac009,g_faac_d[l_ac].faac010,g_faac_d[l_ac].faac011,g_faac_d[l_ac].faac012,g_faac_d[l_ac].faac013,g_faac_d[l_ac].faac014,g_faac_d[l_ac].faac015,g_faac2_d[l_ac].faac001,g_faac2_d[l_ac].faacownid,g_faac2_d[l_ac].faacowndp,g_faac2_d[l_ac].faaccrtid,g_faac2_d[l_ac].faaccrtdp,g_faac2_d[l_ac].faaccrtdt,g_faac2_d[l_ac].faacmodid,g_faac2_d[l_ac].faacmoddt,g_faac2_d[l_ac].faacownid_desc,g_faac2_d[l_ac].faacowndp_desc,g_faac2_d[l_ac].faaccrtid_desc,g_faac2_d[l_ac].faaccrtdp_desc,g_faac2_d[l_ac].faacmodid_desc
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH b_fill_curs:",SQLERRMESSAGE 
         LET g_errparam.code = SQLCA.SQLCODE
         LET g_errparam.popup = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充 name="b_fill.fill"
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL afai020_detail_show()      
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
 
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   LET g_error_show = 0
   

   
   CALL g_faac_d.deleteElement(g_faac_d.getLength())   
   CALL g_faac2_d.deleteElement(g_faac2_d.getLength())

   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_faac_d.getLength()
      LET g_faac2_d[l_ac].faac001 = g_faac_d[l_ac].faac001 

      #add-point:b_fill段key值相關欄位 name="b_fill.keys.fill"
      {<point name="b_fill.keys.fill"/>}
      #end add-point
   END FOR
   
   IF g_cnt > g_faac_d.getLength() THEN
      LET l_ac = g_faac_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_faac_d.getLength()
      LET g_faac_d_mask_o[l_ac].* =  g_faac_d[l_ac].*
      CALL afai020_faac_t_mask()
      LET g_faac_d_mask_n[l_ac].* =  g_faac_d[l_ac].*
   END FOR
   
   LET g_faac2_d_mask_o.* =  g_faac2_d.*
   FOR l_ac = 1 TO g_faac2_d.getLength()
      LET g_faac2_d_mask_o[l_ac].* =  g_faac2_d[l_ac].*
      CALL afai020_faac_t_mask()
      LET g_faac2_d_mask_n[l_ac].* =  g_faac2_d[l_ac].*
   END FOR

   
   LET l_ac = g_cnt
 
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   {<point name="b_fill.others.fill"/>}
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_faac_d.getLength()
   DISPLAY g_detail_idx TO FORMONLY.idx
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE afai020_pb
   
END FUNCTION
