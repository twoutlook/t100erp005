             select glab005 
                 where glabld   = xrcald 
                   and glab001  = '14' 
                   and glab002 = '2'   #-銷項
                   and glab003 = xrcd002 #--稅別

             if 取不到會科時表示以常用會科設定 (agli160) 
　　　　　　　　select glab005 from glab_t 
                   where glabld = xrcald 
                     and glab001 ='12' 
                     and glab002  ='9711' 
                     and glab003 = '9711_91' 
             end if 
               
    帶值:會科名稱
]]>
