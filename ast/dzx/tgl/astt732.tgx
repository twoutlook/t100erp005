   LET g_sql = "SELECT '',stdfstus,stdfsite,'',stdf001,'',stdf002,stdf003,stdf004,'',stdf005,stdf006,stdf007,stdf037,stdf038,stdf008,'','',stdf009,stdf010,stdf011,'',stdf012,stdf013,'',stdf014,'',stdf015,'',stdf016,stdf017,stdf018,stdf019,stdf029,stdf030,stdf031,stdf032,stdf033,stdf034,stdf020,'',stdf021,'',stdf022,'',stdf023,'',stdf024,'',stdf025,'',stdf026,'',stdf027,'',stdf039,'',stdf040,'',stdf041,'',stdf028,stdf035,stdf036",
