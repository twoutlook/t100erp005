   CALL astt802_idx_chk()
   
   LET g_data_owner = g_stem_m.stemownid      
   LET g_data_dept  = g_stem_m.stemowndp
   
   #功能已完成,通報訊息中心
   CALL astt802_msgcentre_notify('reproduce')
 
