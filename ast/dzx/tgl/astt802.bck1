      RETURN
   END IF
    
   LET g_stemdocno_t = g_stem_m.stemdocno
 
    
   LET g_stem_m.stemdocno = ""
 
 
   CALL cl_set_head_visible("","YES")
 
   #公用欄位給予預設值
   #應用 a14 樣板自動產生(Version:5)    
      #公用欄位新增給值  
