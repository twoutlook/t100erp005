  <section id="astq330.detail_show" ver="5" status="" src="s" readonly="">
    <![CDATA[#+ 顯示相關資料
PRIVATE FUNCTION astq330_detail_show()
   #add-point:show段define
   {<point name="detail_show.define" edit="s"/>}
