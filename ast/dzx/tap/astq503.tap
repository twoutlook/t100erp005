  <point name="b_fill.rank_sql_after" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   LET ls_wc = ls_wc CLIPPED," AND ",l_where
   LET ls_sql_rank = "SELECT  UNIQUE    'N',stgcsite,stgc001,stgc002,imaal003 stgc002_desc,                                                         ",
                     "              stgc003,inayl003 stgc003_desc,stgc004,mhael023 stgc004_desc,stgc005,                                            ",
                     "pmaal004 stgc005_desc,stgc007,stgc008,stgc009,stgc006,                                                                        ",
                     "stael003 stgc006_desc,stgc012,stgc010,stgc011,stgc013,                                                                        ",
                     "              stgc014,                                                                                                        ",  
                     "DENSE_RANK() OVER( ORDER BY stgc_t.stgcsite,stgc_t.stgc001,stgc_t.stgc002,stgc_t.stgc003) AS RANK                             ", 
                     "  FROM stgc_t                                                                                                                 ",
                     "       LEFT JOIN imaal_t ON stgcent=imaalent AND stgc002=imaal001 AND imaal002 = '"||g_dlang||"'                              ",
                     "       LEFT JOIN inayl_t ON stgcent=inaylent AND stgc003=inayl001 AND inayl002 = '"||g_dlang||"'                              ",
                     "       LEFT JOIN mhael_t ON stgcent=mhaelent AND stgcsite=mhaelsite AND stgc004=mhael001 AND mhael022 = '"||g_dlang||"'       ",
                     "       LEFT JOIN pmaal_t ON stgcent=pmaalent AND stgc005=pmaal001 AND pmaal002 = '"||g_dlang||"'                              ",
                     "       LEFT JOIN stael_t ON stgcent=staelent AND stgc006=stael001 AND stael002 = '"||g_dlang||"'                              ",
                     " WHERE stgcent= ? AND 1=1 AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("stgc_t"),
                     " ORDER BY stgc_t.stgcsite,stgc_t.stgc001,stgc_t.stgc002,stgc_t.stgc003"
]]>
  </point>
  <point name="b_fill.sql_after" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   LET g_sql = "SELECT 'N',stgcsite,stgc001,stgc002,stgc002_desc,     ",
               "  stgc003,stgc003_desc,stgc004,stgc004_desc,stgc005,  ",
               "  stgc005_desc,stgc007,stgc008,stgc009,stgc006,       ",
               "  stgc006_desc,stgc012,stgc010,stgc011,stgc013,       ",
               "  stgc014                                             ",
               " FROM (",ls_sql_rank,")                               ",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page]]>
