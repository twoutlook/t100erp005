  <section id="asti204.detail_reproduce" ver="2" status="" src="s">
    <![CDATA[#+ 單身自動複製
PRIVATE FUNCTION asti204_detail_reproduce()
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE staf_t.*
 
 
   #add-point:delete段define
   {<point name="detail_reproduce.define"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE asti204_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE asti204_detail AS ",
                "SELECT * FROM staf_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO asti204_detail SELECT * FROM staf_t 
                                         WHERE stafent = g_enterprise AND staf001 = g_staf001_t
                                         AND staf002 = g_staf002_t
 
   
   #將key修正為調整後   
   UPDATE asti204_detail 
      #更新key欄位
      SET staf001 = g_staf_m.staf001
          , staf002 = g_staf_m.staf002
 
      #更新共用欄位
      #此段落由子樣板a13產生
       , stafownid = g_user
       , stafowndp = g_dept
       , stafcrtid = g_user
       , stafcrtdp = g_dept 
       , stafcrtdt = ld_date
       , stafmodid = "" 
       , stafmoddt = "" 
      #, stafstus = "Y"
 
 
                                       
  
   #將資料塞回原table   
   INSERT INTO staf_t SELECT * FROM asti204_detail
   
   IF SQLCA.sqlcode THEN
      CALL cl_err("reproduce",SQLCA.sqlcode,1)
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE asti204_detail
   
   #add-point:單身複製後1
   {<point name="detail_reproduce.body.table1.a_insert"/>}
   #end add-point
 
 
   
 
   
   #多語言複製段落
   
   
