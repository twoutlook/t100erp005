  <section id="asti204.modify" ver="2" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION asti204_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_staf_m.staf001 IS NULL
   OR g_staf_m.staf002 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE asti204_master_referesh USING g_staf_m.staf001,g_staf_m.staf002 INTO g_staf_m.staf001,g_staf_m.staf002, 
       g_staf_m.staf001_desc
 
   ERROR ""
  
   LET g_staf001_t = g_staf_m.staf001
   LET g_staf002_t = g_staf_m.staf002
 
   CALL s_transaction_begin()
   
   OPEN asti204_cl USING g_enterprise,g_staf_m.staf001
                                                       ,g_staf_m.staf002
 
   IF STATUS THEN
      CALL cl_err("OPEN asti204_cl:", STATUS, 1)
      CLOSE asti204_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH asti204_cl INTO g_staf_m.staf001,g_staf_m.staf001_desc,g_staf_m.staf002
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_staf_m.staf001,SQLCA.sqlcode,0)
      CLOSE asti204_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL asti204_show()
   WHILE TRUE
      LET g_staf001_t = g_staf_m.staf001
      LET g_staf002_t = g_staf_m.staf002
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL asti204_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_staf_m.* = g_staf_m_t.*
         CALL asti204_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_staf_m.staf001 != g_staf001_t 
      OR g_staf_m.staf002 != g_staf002_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE staf_t SET staf001 = g_staf_m.staf001
                                       , staf002 = g_staf_m.staf002
 
          WHERE stafent = g_enterprise AND staf001 = g_staf001_t
            AND staf002 = g_staf002_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("staf_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("staf_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_staf_m.staf001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE asti204_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_staf_m.staf001,'U')
 
   CALL asti204_b_fill("1=1")
   
END FUNCTION   
]]>
  </section>
