  <section id="asti204.delete" ver="2" status="" src="s">
    <![CDATA[#+ 資料刪除
PRIVATE FUNCTION asti204_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_staf_m.staf001 IS NULL
   OR g_staf_m.staf002 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE asti204_master_referesh USING g_staf_m.staf001,g_staf_m.staf002 INTO g_staf_m.staf001,g_staf_m.staf002, 
       g_staf_m.staf001_desc
   
   CALL asti204_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN asti204_cl USING g_enterprise,g_staf_m.staf001
                                                       ,g_staf_m.staf002
 
   IF STATUS THEN
      CALL cl_err("OPEN asti204_cl:", STATUS, 1)
      CLOSE asti204_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH asti204_cl INTO g_staf_m.staf001,g_staf_m.staf001_desc,g_staf_m.staf002
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_staf_m.staf001,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL asti204_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM staf_t WHERE stafent = g_enterprise AND staf001 = g_staf_m.staf001
                                                               AND staf002 = g_staf_m.staf002
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("staf_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_staf_d.clear() 
      CALL g_staf2_d.clear()       
 
     
      CALL asti204_ui_browser_refresh()  
      CALL asti204_ui_headershow()  
      CALL asti204_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL asti204_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL asti204_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE asti204_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_staf_m.staf001,'D')
    
END FUNCTION
]]>
