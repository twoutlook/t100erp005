   CALL astt601_show()
   WHILE TRUE
      LET g_stcadocno_t = g_stca_m.stcadocno
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_stca_m.stcamodid = g_user 
LET g_stca_m.stcamoddt = cl_get_current()
 
      
