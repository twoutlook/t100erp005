        FROM stael_t
       WHERE staelent = g_enterprise AND stael001 = g_stae_d[l_ac].stae008 AND stael002 = g_dlang
   END IF
   DISPLAY BY NAME g_stae_d[l_ac].stae008_desc
END FUNCTION]]>
