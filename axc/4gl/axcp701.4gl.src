         RETURN r_success,r_start_no,r_end_no   
      END IF
   
      CALL axcp701_ins_glap() RETURNING r_success,r_start_no,r_end_no  #内有事务开始，结束在这层function做
   END IF
   
   LET g_input.start_no = r_start_no
