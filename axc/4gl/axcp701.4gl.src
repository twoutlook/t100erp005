      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_axcp701 WITH FORM cl_ap_formpath("axc",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL axcp701_init()   
 
      #進入選單 Menu (="N")
      CALL axcp701_ui_dialog() 
 
      #add-point:畫面關閉前 name="main.before_close"
      
      #end add-point
      #畫面關閉
      CLOSE WINDOW w_axcp701
   END IF 
   
   #add-point:作業離開前 name="main.exit"
   DROP TABLE vouc_grp_tmp01;           #160727-00019#22 Mod  s_voucher_xc_group--> vouc_grp_tmp01
   DROP TABLE axcp701_tmp;
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
END MAIN
 
