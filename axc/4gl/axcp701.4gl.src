   DEFINE l_xceadocno     LIKE xcea_t.xceadocno
   DEFINE l_xcea002       LIKE xcea_t.xcea002
   DEFINE l_cnt           LIKE type_t.num5
   DEFINE l_sql           STRING
   DEFINE l_success       LIKE type_t.num5
   DEFINE l_msg           STRING #160328-00022#9
   
   
   #160629-00013#1---mod---s
   LET r_success  = TRUE
   LET r_start_no = ''
   LET r_end_no   = ''         
   #160629-00013#1---mod---e
         
   #160328-00022#9-s-add
