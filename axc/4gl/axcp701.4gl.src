         LET r_start_no = ''
         LET r_end_no   = ''
         RETURN r_success,r_start_no,r_end_no         
         #160629-00013#1---mod---e
      END IF
   END IF
   CALL cl_err_collect_init() #汇总错误讯息初始化      
   IF g_glaa121 = 'Y' THEN    #先mark，要使用的话，要先定义元件用的s_pre_voucher_tmp和group_tmp
      IF NOT s_pre_voucher_cre_tmp_table() THEN
