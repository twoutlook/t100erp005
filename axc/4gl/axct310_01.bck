      LET l_xcco.xccomodid = ""
      LET l_xcco.xccomoddt = ""
      LET l_xcco.xccocnfid = g_user
      LET l_xcco.xccocnfdt = cl_get_current()
      LET l_xcco.xccopstid = ""
      LET l_xcco.xccopstdt = "" 
      LET l_xcco.xccostus = "Y"
      
      SELECT xcaz010 INTO l_xcaz010
        FROM xcaz_t 
       WHERE xcazent = g_enterprise
         AND xcazld = l_xcco.xccold
         AND xcaz001 = l_xcco.xcco003
      
      IF cl_null(l_xcaz010) THEN
         LET l_xcaz010 = 'N'
      END IF
      
      #根據匯率來源取匯率
      CASE l_glaa028
         WHEN '1'   #日匯率
            CALL s_aooi160_get_exrate('2',g_xcco_m.xccold,l_date_e,l_pmdt.pmds037,l_glaa001,0,l_glaa025)
                 RETURNING l_rate
         WHEN '2'   #月匯率
            CALL s_aooi160_get_exrate_avg('2',g_xcco_m.xccold,l_date_e,l_pmdt.pmds037,l_glaa001,0,l_glaa025)
                 RETURNING l_success,l_errno,l_rate
         OTHERWISE
            CALL s_aooi160_get_exrate('2',g_xcco_m.xccold,l_date_e,l_pmdt.pmds037,l_glaa001,0,l_glaa025)
                 RETURNING l_rate
      END CASE
      
      IF l_xcaz010 = 'N' THEN
         LET l_amount = l_pmdt.pmdt038 * l_rate
      ELSE
         LET l_amount = l_pmdt.pmdt039 * l_rate
      END IF
      
      CASE g_xcco_m.xcco102_type
         WHEN '1'
            LET l_xcco.xcco102a = l_amount
         WHEN '2'
            LET l_xcco.xcco102b = l_amount
         WHEN '3'
            LET l_xcco.xcco102c = l_amount
         WHEN '4'
            LET l_xcco.xcco102d = l_amount
         WHEN '5'
            LET l_xcco.xcco102e = l_amount
         WHEN '6'
            LET l_xcco.xcco102f = l_amount
         WHEN '7'
            LET l_xcco.xcco102g = l_amount
         WHEN '8'
            LET l_xcco.xcco102h = l_amount
      END CASE
      
