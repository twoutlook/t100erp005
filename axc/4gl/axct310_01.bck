      #成本域      
      CASE l_sys_6002
           WHEN '1'  #组织
              SELECT xcbf001 INTO l_xcco.xcco002
                FROM xcbf_t 
                WHERE xcbfent  = g_enterprise
                  AND xcbfcomp = g_xcco_m.xccocomp
