      LET l_xcco.xcco008 = l_pmdt.pmdt018
      LET l_xcco.xcco009 = l_pmdt.pmdtdocno
      LET l_xcco.xcco010 = '1'
      LET l_xcco.xcco011 = l_pmdt.pmdt051_desc
      LET l_xcco.xccoownid = g_user
      LET l_xcco.xccoowndp = g_dept
      LET l_xcco.xccocrtid = g_user
      LET l_xcco.xccocrtdp = g_dept 
