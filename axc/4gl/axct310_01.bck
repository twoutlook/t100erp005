         CALL cl_err()  
         LET l_success = FALSE
         EXIT FOREACH
      END IF
      LET l_count = l_count + 1
   END FOREACH
   IF l_success THEN
      CALL s_transaction_end('Y','0')
   ELSE
      CALL s_transaction_end('N','0')
      LET l_count = 0
   END IF
   IF l_count = 0 THEN
      LET l_error = 'axc-00821'
   ELSE
      LET l_error = 'axc-00820'
   END IF
   INITIALIZE g_errparam TO NULL
   LET g_errparam.code = l_error
   LET g_errparam.popup = TRUE
   CALL cl_err()  
   #end add-point    
   
END FUNCTION
 
