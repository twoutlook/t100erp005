                        "                     AND inaj002 = pmdtseq ",
                        "                     AND inaj022 BETWEEN '",l_date_s,"' AND '",l_date_e,"' ",
                        "                     AND inaj036 IN ('102','103','104','105','106','107')) "                        
   END IF
   PREPARE axct310_01_pmdt_p FROM l_sql
   DECLARE axct310_01_pmdt_c CURSOR FOR axct310_01_pmdt_p
   
   LET l_count = 0
   
   #帳套法人取匯率相關設定
   SELECT glaa001,glaa002,glaa025,glaa028 INTO l_glaa001,l_glaa002,l_glaa025,l_glaa028
     FROM glaa_t
    WHERE glaaent = g_enterprise
      AND glaald = g_xcco_m.xccold
      AND glaacomp = g_xcco_m.xccocomp
   
   FOREACH axct310_01_pmdt_c INTO l_pmdt.pmds037,l_pmdt.pmdtsite,l_pmdt.pmdtdocno,l_pmdt.pmdtseq,l_pmdt.pmdt006,
                                  l_pmdt.pmdt007,l_pmdt.pmdt016,l_pmdt.pmdt018,l_pmdt.pmdt038,l_pmdt.pmdt039,
                                  l_pmdt.pmdt051_desc,l_pmdt.pmdt063
