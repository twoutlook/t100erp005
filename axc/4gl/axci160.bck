      LET g_sfaa_d[l_ac].tot = g_sfaa_d[l_ac].tot - l_inaj011
      
      CASE g_item
        WHEN '1'  #完工已達產量工單未結案
          IF g_sfaa_d[l_ac].sfaa012 > g_sfaa_d[l_ac].tot OR g_sfaa_d[l_ac].sfaastus = 'M' THEN
             CONTINUE FOREACH
          END IF
          
