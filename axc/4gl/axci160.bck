################################################################################
# Descriptions...: 成本結案日檢查
################################################################################
PRIVATE FUNCTION axci160_sfaa048_chk()
DEFINE l_sfaa046  LIKE sfaa_t.sfaa046
DEFINE l_bdate    LIKE type_t.dat
DEFINE l_edate    LIKE type_t.dat
DEFINE l_closday  LIKE type_t.dat
DEFINE l_firstday LIKE type_t.dat  #160914-00014#1

   LET g_errno = ''
   SELECT sfaa046 INTO l_sfaa046
     FROM sfaa_t
    WHERE sfaaent = g_enterprise
      AND sfaadocno = g_sfaa_d[l_ac].sfaadocno
   IF g_sfaa_d[l_ac].sfaa048 < l_sfaa046 THEN
      LET g_errno = 'axc-00103'
   END IF
   
   #160914-00014#1---s
   #成本结案日期需在当前期別的日期之间，并且不能小于工单结案日和工单开立日；
   #比如当前期別是6，那成本结案日期只能在6月期间6/1~6/30
   #IF g_sfaa_d[l_ac].sfaa048 <= g_lastday THEN
   #   LET g_errno = 'axc-00117'
   #END IF
   LET l_firstday=MDY(g_mm,1,g_yy)
   IF g_sfaa_d[l_ac].sfaa048 < l_firstday OR g_sfaa_d[l_ac].sfaa048 > g_lastday THEN
      LET g_errno = 'aap-00225'
   END IF
   IF g_sfaa_d[l_ac].sfaa048 < g_sfaa_d[l_ac].sfaadocdt THEN
      LET g_errno = 'axc-00788'
   END IF
   IF g_sfaa_d[l_ac].sfaa048 < g_sfaa_d[l_ac].sfaa047 THEN
      LET g_errno = 'axc-00789'
   END IF
   
   #160914-00014#1---e
   
   CALL cl_get_para(g_enterprise,g_ooef001,'S-FIN-6012') RETURNING l_closday   #fengmy150116
   CALL s_date_get_ymtodate(g_yy,g_mm,g_yy,g_mm) RETURNING l_bdate,l_edate
#   IF g_sfaa_d_t.sfaa048  < l_bdate THEN  #fengmy150116
   IF g_sfaa_d_t.sfaa048  < l_closday THEN #fengmy150116
      LET g_errno = 'axc-00118'
   END IF       
END FUNCTION
################################################################################
# Descriptions...: 法人組織欄位檢查
################################################################################
PRIVATE FUNCTION axci160_ooef001_chk()
DEFINE l_n  LIKE type_t.num5
   SELECT COUNT(ooef001) INTO l_n FROM ooef_t WHERE ooefent = g_enterprise
                                       AND ooef001 = g_ooef001
   IF l_n <=0 THEN
      LET g_errno = 'axc-00104'
   END IF
   SELECT COUNT(ooef001) INTO l_n FROM ooef_t WHERE ooefent = g_enterprise
                                       AND ooef001 = g_ooef001
                                       AND ooefstus = 'Y'
   IF l_n <=0 THEN
      LET g_errno = 'axc-00105'
   END IF
   SELECT COUNT(ooef001) INTO l_n
     FROM ooef_t 
    WHERE ooefent = g_enterprise
      AND ooef003 = 'Y'
      AND ooef001 = g_ooef001
   IF l_n <=0 THEN
      LET g_errno = 'axc-00106'
   END IF   
END FUNCTION

