   OPEN b_fill_curs USING g_enterprise
 
   CALL g_sfaa_d.clear()
   CALL g_sfaa2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_sfaa_d[l_ac].sfaasite,g_sfaa_d[l_ac].sfaadocno,g_sfaa_d[l_ac].sfaadocdt, 
       g_sfaa_d[l_ac].sfaa003,g_sfaa_d[l_ac].sfaastus,g_sfaa_d[l_ac].sfaa010,g_sfaa_d[l_ac].sfaa011, 
       g_sfaa_d[l_ac].sfaa012,g_sfaa_d[l_ac].sfaa042,g_sfaa_d[l_ac].sfaa048,g_sfaa_d[l_ac].sfaa047,g_sfaa_d[l_ac].sfaa065, 
       g_sfaa2_d[l_ac].sfaadocno,g_sfaa2_d[l_ac].sfaaownid,g_sfaa2_d[l_ac].sfaaowndp,g_sfaa2_d[l_ac].sfaacrtid, 
