          IF g_sfaa_d[l_ac].sfaa012 <= g_sfaa_d[l_ac].tot OR g_sfaa_d[l_ac].sfaastus <> 'M' THEN
             CONTINUE FOREACH
          END IF        
          
        WHEN '3'  #未達產量工單未結案
          IF g_sfaa_d[l_ac].sfaa012 <= g_sfaa_d[l_ac].tot OR g_sfaa_d[l_ac].sfaastus = 'M' THEN
             CONTINUE FOREACH
          END IF                   
      END CASE      
      #--160201-00012--add--(e)
      
      SELECT xcbg004 INTO g_sfaa_d[l_ac].xcbg004 FROM xcbg_t
       WHERE xcbgent  = g_enterprise
         AND xcbgcomp = g_ooef001
         AND xcbg001  = g_yy
         AND xcbg002  = g_mm
         AND xcbg003  = g_sfaa_d[l_ac].sfaadocno   

      IF cl_null(g_sfaa_d[l_ac].xcbg004) THEN LET g_sfaa_d[l_ac].xcbg004 = 0 END IF
      #end add-point
      
      CALL axci160_detail_show()      
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
 
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   LET g_error_show = 0
   
 
   
   CALL g_sfaa_d.deleteElement(g_sfaa_d.getLength())   
   CALL g_sfaa2_d.deleteElement(g_sfaa2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_sfaa_d.getLength()
      LET g_sfaa2_d[l_ac].sfaadocno = g_sfaa_d[l_ac].sfaadocno 
 
      #add-point:b_fill段key值相關欄位 name="b_fill.keys.fill"
      
      #end add-point
   END FOR
   
   IF g_cnt > g_sfaa_d.getLength() THEN
      LET l_ac = g_sfaa_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_sfaa_d.getLength()
      LET g_sfaa_d_mask_o[l_ac].* =  g_sfaa_d[l_ac].*
      CALL axci160_sfaa_t_mask()
      LET g_sfaa_d_mask_n[l_ac].* =  g_sfaa_d[l_ac].*
   END FOR
   
   LET g_sfaa2_d_mask_o.* =  g_sfaa2_d.*
   FOR l_ac = 1 TO g_sfaa2_d.getLength()
      LET g_sfaa2_d_mask_o[l_ac].* =  g_sfaa2_d[l_ac].*
      CALL axci160_sfaa_t_mask()
      LET g_sfaa2_d_mask_n[l_ac].* =  g_sfaa2_d[l_ac].*
   END FOR
 
   
   LET l_ac = g_cnt
 
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
 
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_sfaa_d.getLength()
   DISPLAY g_detail_idx TO FORMONLY.idx
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE axci160_pb
   
END FUNCTION
 
