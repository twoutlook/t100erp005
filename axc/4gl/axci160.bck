         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充 name="b_fill.fill"
      EXECUTE axci160_tot USING g_sfaa_d[l_ac].sfaadocno  INTO g_sfaa_d[l_ac].tot
      #--160201-00012--add--(s)
      LET l_inaj011 = 0
