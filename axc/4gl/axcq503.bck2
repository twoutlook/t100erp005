PRIVATE FUNCTION axcq503_get_info()
   #根據參數顯示隱藏成本域 和 料件特性
   IF cl_null(g_xccd_m.xccdcomp) THEN
      CALL cl_get_para(g_enterprise,g_site,'S-FIN-6001') RETURNING g_para_data   #采用成本域否
      CALL cl_get_para(g_enterprise,g_site,'S-FIN-6013') RETURNING g_para_data1   #采用特性否       
   ELSE
      CALL cl_get_para(g_enterprise,g_xccd_m.xccdcomp,'S-FIN-6001') RETURNING g_para_data   #采用成本域否
      CALL cl_get_para(g_enterprise,g_xccd_m.xccdcomp,'S-FIN-6013') RETURNING g_para_data1   #采用特性否       
   END IF            
   IF g_para_data = 'Y' THEN
      CALL cl_set_comp_visible('xccd002,xccf002,xccf002_2,xccf002_3',TRUE)                    
   ELSE
      CALL cl_set_comp_visible('xccd002,xccf002,xccf002_2,xccf002_3',FALSE)                
   END IF    
   IF g_para_data1 = 'Y' THEN
      CALL cl_set_comp_visible('xccd008,xccf008,xccf008_2,xccf008_3',TRUE)                    
   ELSE
      CALL cl_set_comp_visible('xccd008,xccf008,xccf008_2,xccf008_3',FALSE)                
   END IF   
   
   #根据工单抓返工否，结案日期
   SELECT sfaa042,sfaa048 INTO g_xccd_m.sfaa042,g_xccd_m.sfaa048
     FROM sfaa_t
    WHERE sfaaent   = g_enterprise
      AND sfaadocno = g_xccd_m.xccd006

   #根据料号抓品名规格，成本单位（主件）
   SELECT imag014 INTO g_xccd_m.imag014
     FROM imag_t
    WHERE imagent  = g_enterprise
      AND imagsite = g_xccd_m.xccdcomp
      AND imag001  = g_xccd_m.xccd007
   
   CALL s_desc_get_unit_desc(g_xccd_m.imag014)
         RETURNING g_xccd_m.imag014_desc 
         
   CALL s_desc_get_item_desc(g_xccd_m.xccd007)
         RETURNING g_xccd_m.xccd007_desc,g_xccd_m.xccd007_desc_1         
#以下是元件的
   SELECT imag014 INTO g_xccd_m.imag014_1
     FROM imag_t
    WHERE imagent  = g_enterprise
      AND imagsite = g_xccd_m.xccdcomp
      AND imag001  = g_xccd_m.xccf007
   
   CALL s_desc_get_unit_desc(g_xccd_m.imag014_1)
         RETURNING g_xccd_m.imag014_1_desc 

   CALL s_desc_get_item_desc(g_xccd_m.xccf007)
         RETURNING g_xccd_m.xccf007_desc,g_xccd_m.xccf007_desc_1
         
   #抓成本阶
   SELECT DISTINCT xcbb006 INTO g_xccd_m.xcbb006
     FROM xcbb_t
    WHERE xcbbent  = g_enterprise
      AND xcbbcomp = g_xccd_m.xccdcomp
      AND xcbb001  = g_xccd_m.xccd004
      AND xcbb002  = g_xccd_m.xccd005
      AND xcbb003  = g_xccd_m.xccd007   #不写特性，因为相同料号不同特性的xcbb成本阶一样的
      
   SELECT DISTINCT xcbb006 INTO g_xccd_m.xcbb006_1
     FROM xcbb_t
    WHERE xcbbent  = g_enterprise
      AND xcbbcomp = g_xccd_m.xccdcomp
      AND xcbb001  = g_xccd_m.xccd004
      AND xcbb002  = g_xccd_m.xccd005
      AND xcbb003  = g_xccd_m.xccf007   #不写特性，因为相同料号不同特性的xcbb成本阶一样的      

   #抓取料件类别
   SELECT imaa004 INTO g_xccd_m.imaa004 FROM imaa_t WHERE imaaent=g_enterprise AND imaa001=g_xccd_m.xccf007
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_xccd_m.xccdcomp
   CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_xccd_m.xccdcomp_desc = '', g_rtn_fields[1] , ''

   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_xccd_m.xccdld
   CALL ap_ref_array2(g_ref_fields,"SELECT glaal002 FROM glaal_t WHERE glaalent='"||g_enterprise||"' AND glaalld=? AND glaal001='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_xccd_m.xccdld_desc = '', g_rtn_fields[1] , ''
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_xccd_m.xccd003
   CALL ap_ref_array2(g_ref_fields,"SELECT xcatl003 FROM xcatl_t WHERE xcatlent='"||g_enterprise||"' AND xcatl001=? AND xcatl002='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_xccd_m.xccd003_desc = '', g_rtn_fields[1] , ''
END FUNCTION

