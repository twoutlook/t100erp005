################################################################################
PRIVATE FUNCTION axcq533_detail_fill()
  DEFINE l_sql   STRING
  
   CALL g_detail.clear()
   LET l_ac = 1
   
   LET l_sql = "SELECT item,cost FROM axcq533_temp01 ORDER BY item"       #160727-00019#21 Mod  axcq533_cost_temp--> axcq533_temp01
   
   LET l_sql = cl_sql_add_mask(l_sql)              #遮蔽特定資料
   PREPARE axcq533_pb FROM l_sql
   DECLARE axcq533_fill_curs CURSOR FOR axcq533_pb 
   
   FOREACH axcq533_fill_curs INTO g_detail[l_ac].gzcb002,g_detail[l_ac].account

      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "FOREACH:"
         LET g_errparam.popup = TRUE
         CALL cl_err()
         EXIT FOREACH
      END IF
      CALL s_desc_gzcbl004_desc('8545',g_detail[l_ac].gzcb002) RETURNING g_detail[l_ac].gzcb002_desc
      

      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
         END IF
         EXIT FOREACH
      END IF      
   END FOREACH
   
   CALL g_detail.deleteElement(g_detail.getLength())
 

 
   LET g_error_show = 0
 
   #單身總筆數顯示
   LET g_detail_cnt = g_detail.getLength()
   LET g_tot_cnt = g_detail.getLength()
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL axcq533_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL axcq533_detail_action_trans()
   
   
   
END FUNCTION
