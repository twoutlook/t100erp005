################################################################################
PRIVATE FUNCTION axcq533_bpage_visible()
   CALL cl_set_comp_visible("bpage_1",FALSE)
   CALL cl_set_comp_visible("bpage_2",FALSE)
   IF g_master.type_1 = 'Y' THEN
      CALL cl_set_comp_visible("bpage_1",TRUE)
   END IF  
   IF g_master.type_2 = 'Y' THEN
      CALL cl_set_comp_visible("bpage_2",TRUE)  
   END IF      
END FUNCTION

################################################################################
# Descriptions...: 起始年度期別不可大於截止年度期別
# Memo...........:
# Usage..........: CALL axcq533_date_chk()
#                  RETURNING TRUE/FALSE
# Input parameter: 
# Return code....: RETURN TRUE/FALSE
# Date & Author..: 2015/11/16 By Dorislai
# Modify.........:
################################################################################
PRIVATE FUNCTION axcq533_date_chk()
   DEFINE l_chk LIKE type_t.num5
   
   LET l_chk = TRUE
   IF NOT cl_null(g_master.xccc004) AND NOT cl_null(g_master.xccc004_1) THEN
      IF g_master.xccc004 > g_master.xccc004_1 THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'acr-00064'
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()
         LET l_chk = FALSE
      END IF
   END IF
  
   IF NOT cl_null(g_master.xccc005) AND NOT cl_null(g_master.xccc005_1) THEN
      IF g_master.xccc005 > g_master.xccc005_1 AND g_master.xccc004 = g_master.xccc004_1 THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'acr-00067'
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         CALL cl_err()
         LET l_chk = FALSE
      END IF
   END IF
   
   RETURN l_chk
END FUNCTION

 
