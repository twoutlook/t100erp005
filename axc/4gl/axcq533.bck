# Descriptions...: 在製原料明細顯示
# Memo...........:
# Usage..........: CALL axcq533_detail2_fill()
#                  RETURNING 
# Input parameter: 
# Return code....: 
# Date & Author..: 151109 By Polly
# Modify.........:
################################################################################
PRIVATE FUNCTION axcq533_detail2_fill()
  DEFINE l_sql   STRING
  
   CALL g_detail2.clear()
   LET l_ac = 1
   
   LET l_sql = "SELECT imag011,item,cost FROM axcq533_temp02 ORDER BY l_order,item"   #160727-00019#21 Mod  axcq533_remk_temp--> axcq533_temp02  
   
   LET l_sql = cl_sql_add_mask(l_sql)              #遮蔽特定資料
   PREPARE axcq533_pb2 FROM l_sql
   DECLARE axcq533_fill2_curs CURSOR FOR axcq533_pb2 
   
   FOREACH axcq533_fill2_curs INTO g_detail2[l_ac].imag011,g_detail2[l_ac].project,g_detail2[l_ac].amount

      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "FOREACH:"
         LET g_errparam.popup = TRUE
         CALL cl_err()
         EXIT FOREACH
      END IF
      CALL s_desc_get_acc_desc('206',g_detail2[l_ac].imag011) RETURNING g_detail2[l_ac].imag011_desc
      CALL s_desc_gzcbl004_desc('8547',g_detail2[l_ac].project) RETURNING g_detail2[l_ac].project_desc

      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
         END IF
         EXIT FOREACH
      END IF      
   END FOREACH
   
   CALL g_detail2.deleteElement(g_detail.getLength())
 

 
   LET g_error_show = 0
 
   #單身總筆數顯示
   LET g_detail2_cnt = g_detail2.getLength() - 1

   #調整單身index指標，避免翻頁後指到空白筆數
   CALL axcq533_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL axcq533_detail_action_trans()
END FUNCTION
