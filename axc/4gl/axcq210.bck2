   PREPARE axcq210_pb3 FROM g_sql
   DECLARE b_fill_curs3 CURSOR FOR axcq210_pb3
   
   FOREACH b_fill_curs3 INTO  
                             g_xcbz2_d[l_ac].xcbzsite,g_xcbz2_d[l_ac].xcbz003,g_xcbz2_d[l_ac].xcbz004,g_xcbz2_d[l_ac].xcbz003_2_desc,g_xcbz2_d[l_ac].xcbz003_2_desc_desc,
                             g_xcbz2_d[l_ac].xcbz005,g_xcbz2_d[l_ac].xcbz006,g_xcbz2_d[l_ac].xcbz007,g_xcbz2_d[l_ac].xcbz008,g_xcbz2_d[l_ac].xcbz009, 
                             g_xcbz2_d[l_ac].xcbz101,g_xcbz2_d[l_ac].xcbz201,g_xcbz2_d[l_ac].xcbz202,g_xcbz2_d[l_ac].xcbz203,g_xcbz2_d[l_ac].xcbz204,
                             g_xcbz2_d[l_ac].xcbz205,g_xcbz2_d[l_ac].xcbz206,g_xcbz2_d[l_ac].xcbz207,g_xcbz2_d[l_ac].xcbz208,g_xcbz2_d[l_ac].xcbz209,
                             g_xcbz2_d[l_ac].xcbz301,g_xcbz2_d[l_ac].xcbz302,g_xcbz2_d[l_ac].xcbz303,g_xcbz2_d[l_ac].xcbz304,
                             g_xcbz2_d[l_ac].xcbz305,g_xcbz2_d[l_ac].xcbz306,g_xcbz2_d[l_ac].xcbz901
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
      
      
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
   END FOREACH
   
   CLOSE b_fill_curs3
   FREE axcq210_pb3
   LET g_error_show = 0
  
