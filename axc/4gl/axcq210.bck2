   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
    CALL g_xcbz2_d.clear()   
 
   LET g_sql = "SELECT UNIQUE xcbzsite,xcbz003,xcbz004,imaal003,imaal004,xcbz005,xcbz006,xcbz007,xcbz008,xcbz009, ",  #add imaal004 fengmy150122
               "              xcbz101,xcbz201,xcbz202,xcbz203,xcbz204,xcbz205,xcbz206,xcbz207,xcbz208,xcbz209, ",
