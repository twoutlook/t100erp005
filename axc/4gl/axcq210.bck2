                        " WHERE xcbzent= ",g_enterprise,
                        "   AND xcbzcomp='",l_xcbz_tmp_m[li].xcbzcomp,"' ",
                        "   AND xcbzld ='",l_xcbz_tmp_m[li].xcbzld,"' ",
                        "   AND xcbz001= ",l_xcbz_tmp_m[li].xcbz001,
                        "   AND xcbz002= ",l_xcbz_tmp_m[li].xcbz002 ,
                        "   AND ",ls_wc   #单身需根据条件筛选资料
      LET g_detail_sql = g_detail_sql, cl_sql_add_filter("xcbz_t"),
               " ORDER BY xcbzsite,xcbz003,xcbz004,xcbz005,xcbz006,xcbz007,xcbz008,xcbz009"
      LET g_detail_sql = cl_sql_add_mask(g_detail_sql)              #遮蔽特定資料
      PREPARE axcq210_tmp_pre_d FROM g_detail_sql      #預備一下
      DECLARE axcq210_tmp_curs_d  CURSOR FOR axcq210_tmp_pre_d    #宣告成可捲動的
      LET li_d=1
      FOREACH axcq210_tmp_curs_d INTO l_xcbz_tmp_d[li_d].xcbzsite,l_xcbz_tmp_d[li_d].xcbz003,l_xcbz_tmp_d[li_d].xcbz004,l_xcbz_tmp_d[li_d].xcbz003_desc,l_xcbz_tmp_d[li_d].xcbz003_desc_2, 
