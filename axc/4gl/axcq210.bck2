#查询前预设
PRIVATE FUNCTION axcq210_default()
DEFINE l_xcbk001    LIKE xcbk_t.xcbk001

   #预设当前site的法人，主账套，年度期别，#成本计算类型   #150805-00001#3add加IF，截止年期赋值
   IF cl_null(tm.xcbzcomp) AND cl_null(tm.xcbzld) AND cl_null(tm.xcbz001) AND cl_null(tm.xcbz002) THEN
      CALL s_axc_set_site_default() RETURNING tm.xcbzcomp,tm.xcbzld,tm.xcbz001,tm.xcbz002,l_xcbk001
      LET tm.xcbz001_1=tm.xcbz001
      LET tm.xcbz002_1=tm.xcbz002
   END IF
   #fengmy150122---begin
   IF cl_null(tm.xcbzcomp) THEN
      CALL cl_get_para(g_enterprise,g_site,'S-FIN-6013') RETURNING g_para_data1   #采用特性否       
   ELSE
      CALL cl_get_para(g_enterprise,tm.xcbzcomp,'S-FIN-6013') RETURNING g_para_data1   #采用特性否       
   END IF
   IF g_para_data1 = 'Y' THEN
      CALL cl_set_comp_visible('b_xcbz004,xcbz004_2',TRUE)                    
   ELSE                           
      CALL cl_set_comp_visible('b_xcbz004,xcbz004_2',FALSE)                
   END IF 
  #fengmy150122---end  
   #说明栏位
   CALL s_desc_get_department_desc(tm.xcbzcomp) RETURNING tm.xcbzcomp_desc #法人
   CALL s_desc_get_ld_desc(tm.xcbzld) RETURNING tm.xcbzld_desc #帳別編號
