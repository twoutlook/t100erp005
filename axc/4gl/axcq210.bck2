         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
      LET g_detail_sql= "SELECT UNIQUE xcbzsite,xcbz003,xcbz004,imaal003,imaal004,xcbz005,xcbz006,xcbz007,xcbz008,xcbz009, ",  #add imaal004 fengmy150122
                        "              xcbz101,0,0,xcbz901, ",
