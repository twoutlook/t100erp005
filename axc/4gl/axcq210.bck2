#+ 單身陣列填充
PRIVATE FUNCTION axcq210_b_fill()
   #add-point:b_fill段define-客製 name="b_fill.define_customerization"
   
   #end add-point
   DEFINE ls_wc           STRING
   DEFINE ls_wc2          STRING
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準  (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
 DEFINE l_num           LIKE type_t.num5
 DEFINE l_num1          LIKE type_t.num5
 DEFINE l_num2          LIKE type_t.num5
   #end add-point
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   LET g_wc = g_wc," AND (xcbz001*12+xcbz002 BETWEEN ",tm.xcbz001,"*12+",tm.xcbz002," AND ",tm.xcbz001_1,"*12+",tm.xcbz002_1,")"    #150805-00001#3add 

   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   #add--160802-00020#7 By shiun--(S)
   #---增加帳套權限控管
   IF NOT cl_null(g_wc_cs_ld) THEN
      LET ls_wc = ls_wc ," AND xcbzld IN ",g_wc_cs_ld
    END IF
   #---增加法人過濾條件
   IF NOT cl_null(g_wc_cs_comp) THEN
      LET ls_wc = ls_wc ," AND xcbzcomp IN ",g_wc_cs_comp
   END IF
   #add--160802-00020#7 By shiun--(E)
   #显示资料
   #抓出单头资料
   LET g_sql = "SELECT UNIQUE xcbzcomp,ooefl003,xcbzld,glaal002, ",
               "            '",tm.xcbz001,"','",tm.xcbz002,"','",tm.xcbz001_1,"','",tm.xcbz002_1,"'",
               "  FROM xcbz_t LEFT JOIN ooefl_t ON ooeflent='"||g_enterprise||"' AND ooefl001=xcbzcomp AND ooefl002='"||g_dlang||"' ",
               "              LEFT JOIN glaal_t ON glaalent='"||g_enterprise||"' AND glaalld=xcbzld AND glaal001='"||g_dlang||"' ",
               " WHERE xcbzent = ",g_enterprise,
               "   AND ",ls_wc
   LET g_sql = g_sql, cl_sql_add_filter("xcbz_t"),  #額外的過濾條件
              " ORDER BY xcbzcomp,xcbzld"
              
   LET g_head_sql= g_sql                   #2015/3/24 liuym 
   
   PREPARE axcq210_prepare FROM g_sql      #預備一下
   DECLARE axcq210_b_curs                  #宣告成可捲動的
       SCROLL CURSOR WITH HOLD FOR axcq210_prepare

   #抓出資料筆數
   #LET g_sql = "SELECT COUNT(DISTINCT xcbzcomp||xcbzld||xcbz001||xcbz002) ",
    LET g_sql = "SELECT COUNT(DISTINCT xcbzcomp||xcbzld) ",
               "  FROM xcbz_t",
               " WHERE xcbzent = ",g_enterprise,
               "   AND ",ls_wc
   LET g_sql = g_sql, cl_sql_add_filter("xcbz_t")   #額外的過濾條件
   PREPARE axcq210_precount FROM g_sql
   DECLARE axcq210_count CURSOR FOR axcq210_precount

   OPEN axcq210_b_curs         #抓出单头唯一性资料
   IF SQLCA.sqlcode THEN                    #有問題
      INITIALIZE g_errparam TO NULL
      LET g_errparam.extend = "open axcq210_b_curs"
      LET g_errparam.code   = SQLCA.sqlcode
      LET g_errparam.popup  = FALSE
      CALL cl_err()
      INITIALIZE tm.* TO NULL
   ELSE
      #显示总页数
      OPEN axcq210_count
      FETCH axcq210_count INTO g_page_cnt
      DISPLAY g_page_cnt TO FORMONLY.h_count  #总页数


      LET g_fetch = 'F'
      CALL axcq210_fetch()  #显示第一笔
      
      #未获取资料
      IF g_page_cnt = 0 THEN
         LET g_page_idx = ''  #当页
         DISPLAY g_page_idx TO FORMONLY.h_index  #当页
      END IF
   END IF
   
   RETURN
   #end add-point
 
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   LET ls_sql_rank = "SELECT  UNIQUE xcbzsite,xcbz003,xcbz004,'','',xcbz005,xcbz006,'',xcbz007,xcbz008, 
       xcbz009,xcbz101,xcbz201,xcbz301,xcbz901,'','','','','','','','','','','','','','','','','','', 
       '','','','','','','','','',''  ,DENSE_RANK() OVER( ORDER BY xcbz_t.xcbzsite,xcbz_t.xcbzld,xcbz_t.xcbz001, 
       xcbz_t.xcbz002,xcbz_t.xcbz003,xcbz_t.xcbz004,xcbz_t.xcbz005,xcbz_t.xcbz006,xcbz_t.xcbz007,xcbz_t.xcbz008, 
       xcbz_t.xcbz009) AS RANK FROM xcbz_t",
 
 
                     "",
                     " WHERE xcbzent= ? AND 1=1 AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("xcbz_t"),
                     " ORDER BY xcbz_t.xcbzsite,xcbz_t.xcbzld,xcbz_t.xcbz001,xcbz_t.xcbz002,xcbz_t.xcbz003,xcbz_t.xcbz004,xcbz_t.xcbz005,xcbz_t.xcbz006,xcbz_t.xcbz007,xcbz_t.xcbz008,xcbz_t.xcbz009"
 
   #add-point:b_fill段rank_sql_after name="b_fill.rank_sql_after"
   LET ls_sql_rank = "SELECT  UNIQUE xcbzsite,xcbz003,xcbz004,'','',xcbz005,xcbz006,'',xcbz007,xcbz008, 
       xcbz009,xcbz101,sum(xcbz201),xcbz301,xcbz901,'','','','','','','','','','','','','','','','','','', 
       '','','','','','','','','',''  ,DENSE_RANK() OVER( ORDER BY xcbz_t.xcbzsite,xcbz_t.xcbzld,xcbz_t.xcbz001, 
       xcbz_t.xcbz002,xcbz_t.xcbz003,xcbz_t.xcbz004,xcbz_t.xcbz005,xcbz_t.xcbz006,xcbz_t.xcbz007,xcbz_t.xcbz008, 
       xcbz_t.xcbz009) AS RANK FROM xcbz_t",
 
 
                     "",
                     " WHERE xcbzent= ? AND 1=1 AND ", ls_wc
    
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("xcbz_t"),
                     " ORDER BY xcbz_t.xcbz001,xcbz_t.xcbz002,xcbz_t.xcbzsite,xcbz_t.xcbzld,xcbz_t.xcbz003,xcbz_t.xcbz004,xcbz_t.xcbz005,xcbz_t.xcbz006,xcbz_t.xcbz007,xcbz_t.xcbz008,xcbz_t.xcbz009"
 
   #end add-point
 
   LET g_sql = "SELECT COUNT(1) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   #add-point:b_fill段rank_sql_after_count name="b_fill.rank_sql_after_count"
   
   #end add-point
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT xcbzsite,xcbz003,xcbz004,'','',xcbz005,xcbz006,'',xcbz007,xcbz008,xcbz009,xcbz101, 
       xcbz201,xcbz301,xcbz901,'','','','','','','','','','','','','','','','','','','','','','','', 
       '','','','',''",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after name="b_fill.sql_after"
 LET g_sql = "SELECT xcbzsite,xcbz003,xcbz004,'','',xcbz005,xcbz006,'',xcbz007,xcbz008,xcbz009,xcbz101, 
       sum(xcbz201),sum(xcbz301),xcbz901,'','','','','','','','','','','','','','','','','','','','','','','', 
       '','','','',''",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE axcq210_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR axcq210_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_xcbz_d.clear()
   CALL g_xcbz2_d.clear()   
 
 
   #add-point:陣列清空 name="b_fill.array_clear"
   
