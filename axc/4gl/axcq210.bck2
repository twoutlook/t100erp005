#筛选，pattern不支持功能需求，用此代替pattern
PRIVATE FUNCTION axcq210_filter2()

   LET l_ac = 1
   LET g_detail_idx = 1
   LET g_detail_idx2 = 1
 
   LET INT_FLAG = 0
 
   LET g_qryparam.state = 'c'
 
   #備份
   LET g_wc_filter_t = g_wc_filter
   LET g_wc_t = g_wc
   
   LET g_wc = cl_replace_str(g_wc, g_wc_filter, '')
 
   LET g_wc_filter       = ''
   
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      CONSTRUCT g_wc_filter ON xcbzcomp,xcbzld,xcbz001,xcbz002,xcbz001,xcbz002,
                               xcbzsite,xcbz003,xcbz006
           FROM b_xcbzcomp,b_xcbzld,b_xcbz001,b_xcbz002,b_xcbz001_1,b_xcbz002_1,
                s_detail1[1].b_xcbzsite,s_detail1[1].b_xcbz003,s_detail1[1].b_xcbz006
                      
         BEFORE CONSTRUCT
            DISPLAY axcq210_filter_parser('xcbzcomp') TO s_detail1[1].b_xcbzcomp
            DISPLAY axcq210_filter_parser('xcbzld') TO s_detail1[1].b_xcbzld
            DISPLAY axcq210_filter_parser('xcbz001') TO s_detail1[1].b_xcbz001
            DISPLAY axcq210_filter_parser('xcbz002') TO s_detail1[1].b_xcbz002
            DISPLAY axcq210_filter_parser('xcbzsite') TO s_detail1[1].b_xcbzsite
            DISPLAY axcq210_filter_parser('xcbz003') TO s_detail1[1].b_xcbz003
            DISPLAY axcq210_filter_parser('xcbz006') TO s_detail1[1].b_xcbz006

         ON ACTION controlp INFIELD b_xcbzcomp #法人组织
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c'
            LET g_qryparam.reqry = FALSE
            #LET g_qryparam.where = " ooefstus = 'Y'"
            #add--160802-00020#7 By shiun--(S)
      	   #增加法人過濾條件
            IF NOT cl_null(g_wc_cs_comp) THEN
               LET g_qryparam.where = " ooef001 IN ",g_wc_cs_comp
            END IF
            #add--160802-00020#7 By shiun--(E)
            CALL q_ooef001_2()                      #呼叫開窗
            DISPLAY g_qryparam.return1 TO b_xcbzcomp  #顯示到畫面上
            NEXT FIELD b_xcbzcomp                     #返回原欄位

         ON ACTION controlp INFIELD b_xcbzld   #账别编号
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c'
            LET g_qryparam.reqry = FALSE
            LET g_qryparam.arg1 = g_user
            LET g_qryparam.arg2 = g_dept
            #add--160802-00020#7 By shiun--(S)
            LET g_qryparam.arg1 = g_user          #人員權限
            LET g_qryparam.arg2 = g_dept          #部門權限
            #增加帳套權限控制
            IF NOT cl_null(g_wc_cs_ld) THEN
               LET g_qryparam.where = " glaald IN ",g_wc_cs_ld
            END IF
            #add--160802-00020#7 By shiun--(E)
            CALL q_authorised_ld()                #呼叫開窗
            DISPLAY g_qryparam.return1 TO b_xcbzld  #顯示到畫面上
            NEXT FIELD b_xcbzld                     #返回原欄位
 
         ON ACTION controlp INFIELD b_xcbzsite  #营运据点
            #開窗c段
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c'
            LET g_qryparam.reqry = FALSE
#            CALL q_ooef001_14()                          #呼叫開窗   #161019-00017#7 marked
            CALL q_ooef001_1()                           #呼叫開窗   #161019-00017#7 add
            DISPLAY g_qryparam.return1 TO b_xcbzsite  #顯示到畫面上
            NEXT FIELD b_xcbzsite                     #返回原欄位
 
         ON ACTION controlp INFIELD b_xcbz003   #料件编号
            #開窗c段
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c'
            LET g_qryparam.reqry = FALSE
            CALL q_imaa001()                           #呼叫開窗
            DISPLAY g_qryparam.return1 TO b_xcbz003  #顯示到畫面上
            NEXT FIELD b_xcbz003                     #返回原欄位

         ON ACTION controlp INFIELD b_xcbz006   #仓库编号
            #開窗c段
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c'
            LET g_qryparam.reqry = FALSE
            CALL q_inaa001_23()                           #呼叫開窗
            DISPLAY g_qryparam.return1 TO b_xcbz006  #顯示到畫面上
            NEXT FIELD b_xcbz006                     #返回原欄位

       
      END CONSTRUCT
      #end add-point
 
      BEFORE DIALOG
         #add-point:filter段b_dialog

         #end add-point  
 
      ON ACTION accept
         ACCEPT DIALOG 
 
      ON ACTION cancel
         LET INT_FLAG = 1
         EXIT DIALOG 
 
      #交談指令共用ACTION
      &include "common_action.4gl" 
         CONTINUE DIALOG
 
   END DIALOG
 
   IF NOT INT_FLAG THEN
      LET g_wc_filter = g_wc_filter, " "
   ELSE
      LET g_wc_filter = g_wc_filter_t
      LET g_wc = g_wc_t
   END IF
   
   CALL axcq210_filter_show('xcbzcomp','b_xcbzcomp')
   CALL axcq210_filter_show('xcbzld','b_xcbzld')
   CALL axcq210_filter_show('xcbz001','b_xcbz001')
   CALL axcq210_filter_show('xcbz002','b_xcbz002')
   CALL axcq210_filter_show('xcbzsite','b_xcbzsite')
   CALL axcq210_filter_show('xcbz003','b_xcbz003')
   CALL axcq210_filter_show('xcbz006','b_xcbz006')
   CALL axcq210_filter_show('xcbz001','b_xcbz001_1')
   CALL axcq210_filter_show('xcbz002','b_xcbz002_1')
   
   INITIALIZE tm.* TO NULL
   CALL axcq210_b_fill()
   
END FUNCTION

