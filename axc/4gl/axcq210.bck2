                                      l_xcbz_tmp_d[li_d].xcbz301,l_xcbz_tmp_d[li_d].xcbz302,l_xcbz_tmp_d[li_d].xcbz303,l_xcbz_tmp_d[li_d].xcbz304,
                                      l_xcbz_tmp_d[li_d].xcbz305,l_xcbz_tmp_d[li_d].xcbz306,l_xcbz_tmp_d[li_d].xcbz901
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH DETAIL:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
         
         #抓取营运据点说明
         SELECT ooefl003 INTO l_xcbzsite_desc FROM ooefl_t WHERE ooeflent=g_enterprise AND ooefl001=l_xcbz_tmp_d[li_d].xcbzsite AND ooefl002=g_dlang 
         #抓取仓库说明
         SELECT inayl003 INTO l_xcbz006_desc FROM inayl_t WHERE inaylent=g_enterprise AND inayl001=l_xcbz_tmp_d[li_d].xcbz006 AND inayl002=g_dlang
         
