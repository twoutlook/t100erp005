   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1   
 
   FOREACH b_fill_curs INTO g_xcbz_d[l_ac].xcbzsite,g_xcbz_d[l_ac].xcbz003,g_xcbz_d[l_ac].xcbz004,g_xcbz_d[l_ac].xcbz003_desc, 
       g_xcbz_d[l_ac].xcbz003_desc_desc,g_xcbz_d[l_ac].xcbz005,g_xcbz_d[l_ac].xcbz006,g_xcbz_d[l_ac].xcbz006_desc, 
       g_xcbz_d[l_ac].xcbz007,g_xcbz_d[l_ac].xcbz008,g_xcbz_d[l_ac].xcbz009,g_xcbz_d[l_ac].xcbz101,g_xcbz_d[l_ac].xcbz201, 
       g_xcbz_d[l_ac].xcbz301,g_xcbz_d[l_ac].xcbz901,g_xcbz2_d[l_ac].xcbzsite,g_xcbz2_d[l_ac].xcbz003, 
       g_xcbz2_d[l_ac].xcbz004,g_xcbz2_d[l_ac].xcbz003_2_desc,g_xcbz2_d[l_ac].xcbz003_2_desc_desc,g_xcbz2_d[l_ac].xcbz005, 
       g_xcbz2_d[l_ac].xcbz006,g_xcbz2_d[l_ac].xcbz006_2_desc,g_xcbz2_d[l_ac].xcbz007,g_xcbz2_d[l_ac].xcbz008, 
       g_xcbz2_d[l_ac].xcbz009,g_xcbz2_d[l_ac].xcbz101,g_xcbz2_d[l_ac].xcbz201,g_xcbz2_d[l_ac].xcbz202, 
       g_xcbz2_d[l_ac].xcbz203,g_xcbz2_d[l_ac].xcbz204,g_xcbz2_d[l_ac].xcbz205,g_xcbz2_d[l_ac].xcbz206, 
       g_xcbz2_d[l_ac].xcbz207,g_xcbz2_d[l_ac].xcbz208,g_xcbz2_d[l_ac].xcbz209,g_xcbz2_d[l_ac].xcbz301, 
       g_xcbz2_d[l_ac].xcbz302,g_xcbz2_d[l_ac].xcbz303,g_xcbz2_d[l_ac].xcbz304,g_xcbz2_d[l_ac].xcbz305, 
       g_xcbz2_d[l_ac].xcbz306,g_xcbz2_d[l_ac].xcbz901
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #LET g_xcbz_d[l_ac].statepic = cl_get_actipic(g_xcbz_d[l_ac].statepic)
 
      
 
      #add-point:b_fill段資料填充 name="b_fill.fill"
      
      #end add-point
 
      CALL axcq210_detail_show("'1'")      
 
      CALL axcq210_xcbz_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
