PRIVATE FUNCTION axcq210_ins_temp()
TYPE type_xcbz_tmp_m RECORD
                     xcbzcomp   LIKE xcbz_t.xcbzcomp,  #法人組織
                     xcbzcomp_desc LIKE type_t.chr100,
                     xcbzld     LIKE xcbz_t.xcbzld  ,  #帳別編號
                     xcbzld_desc   LIKE type_t.chr100,
                     xcbz001    LIKE xcbz_t.xcbz001 ,  #年度
                     xcbz002    LIKE xcbz_t.xcbz002    #期別
                     END RECORD
DEFINE l_xcbz_tmp_m          DYNAMIC ARRAY OF type_xcbz_tmp_m                     
TYPE type_xcbz_tmp_d RECORD                     
                     xcbzsite       LIKE xcbz_t.xcbzsite,            
                     xcbz003        LIKE xcbz_t.xcbz003, 
                     xcbz004        LIKE xcbz_t.xcbz004,
                     xcbz003_desc   LIKE type_t.chr500, 
                     xcbz003_desc_2 LIKE type_t.chr500, 
