DEFINE li_d                  LIKE type_t.num5
DEFINE l_xcbz006_desc        LIKE type_t.chr100    #仓库说明 
DEFINE l_xcbzsite_desc       LIKE type_t.chr100    #据点说明
DEFINE l_null                LIKE type_t.chr100    #存放无用栏位
DEFINE ls_wc                STRING

   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   PREPARE axcq210_tmp_pre_m FROM g_head_sql      #預備一下
   DECLARE axcq210_tmp_curs_m  CURSOR FOR axcq210_tmp_pre_m    #宣告成可捲動的
   LET li=1
   FOREACH axcq210_tmp_curs_m INTO  l_xcbz_tmp_m[li].xcbzcomp,l_xcbz_tmp_m[li].xcbzcomp_desc,
                                    l_xcbz_tmp_m[li].xcbzld,l_xcbz_tmp_m[li].xcbzld_desc,
