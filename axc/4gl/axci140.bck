{<section id="axci140.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION axci140_b_fill(p_wc2)
   #add-point:b_fill段define name="b_fill.define_customerization"
   
   #end add-point
   DEFINE p_wc2      STRING
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   DEFINE l_ecbb004  LIKE ecbb_t.ecbb004
   #end add-point     
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   #先清空單身變數內容
   CALL g_xcbe_d.clear()
   CALL g_xcbe2_d.clear()
 
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   
   #end add-point
   
   LET g_sql = "SELECT  DISTINCT xcbestus,xcbe003,xcbe004,xcbe005,xcbe006,xcbe007,xcbe003,xcbe004,xcbe005, 
       xcbe006,xcbeownid,xcbeowndp,xcbecrtid,xcbecrtdp,xcbecrtdt,xcbemodid,xcbemoddt,t1.ooag011 ,t2.ooefl003 , 
       t3.ooag011 ,t4.ooefl003 ,t5.ooag011 FROM xcbe_t",   
               "",
               
                              " LEFT JOIN ooag_t t1 ON t1.ooagent="||g_enterprise||" AND t1.ooag001=xcbeownid  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent="||g_enterprise||" AND t2.ooefl001=xcbeowndp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent="||g_enterprise||" AND t3.ooag001=xcbecrtid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent="||g_enterprise||" AND t4.ooefl001=xcbecrtdp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=xcbemodid  ",
 
               " WHERE xcbeent= ? AND xcbesite=? AND xcbe001=? AND xcbe002=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("xcbe_t")
   END IF
   
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF axci140_fill_chk(1) THEN
      IF (g_action_choice = "query" OR cl_null(g_action_choice))
         #add-point:b_fill段long_sql_if name="b_fill.long_sql_if"
         
         #end add-point
      THEN
         LET g_sql = g_sql, " ORDER BY xcbe_t.xcbe003,xcbe_t.xcbe004,xcbe_t.xcbe005,xcbe_t.xcbe006"
         #add-point:b_fill段fill_before name="b_fill.fill_before"
         
         #end add-point
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE axci140_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR axci140_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_xcbe_m.xcbesite,g_xcbe_m.xcbe001,g_xcbe_m.xcbe002
                                               
      FOREACH b_fill_cs INTO g_xcbe_d[l_ac].xcbestus,g_xcbe_d[l_ac].xcbe003,g_xcbe_d[l_ac].xcbe004,g_xcbe_d[l_ac].xcbe005, 
          g_xcbe_d[l_ac].xcbe006,g_xcbe_d[l_ac].xcbe007,g_xcbe2_d[l_ac].xcbe003,g_xcbe2_d[l_ac].xcbe004, 
          g_xcbe2_d[l_ac].xcbe005,g_xcbe2_d[l_ac].xcbe006,g_xcbe2_d[l_ac].xcbeownid,g_xcbe2_d[l_ac].xcbeowndp, 
          g_xcbe2_d[l_ac].xcbecrtid,g_xcbe2_d[l_ac].xcbecrtdp,g_xcbe2_d[l_ac].xcbecrtdt,g_xcbe2_d[l_ac].xcbemodid, 
          g_xcbe2_d[l_ac].xcbemoddt,g_xcbe2_d[l_ac].xcbeownid_desc,g_xcbe2_d[l_ac].xcbeowndp_desc,g_xcbe2_d[l_ac].xcbecrtid_desc, 
          g_xcbe2_d[l_ac].xcbecrtdp_desc,g_xcbe2_d[l_ac].xcbemodid_desc
                             
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充 name="b_fill.fill"
         CALL axci140_ecbb004_get()
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #應用 a12 樣板自動產生(Version:4)
 
 
 
 
        
         #add-point:單身資料抓取 name="bfill.foreach"
         
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF 
            EXIT FOREACH
         END IF
      
         LET l_ac = l_ac + 1
         
      END FOREACH
 
            CALL g_xcbe_d.deleteElement(g_xcbe_d.getLength())
      CALL g_xcbe2_d.deleteElement(g_xcbe2_d.getLength())
 
      
   END IF
   
   #add-point:b_fill段more name="b_fill.more"
   
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_xcbe_d.getLength()
      LET g_xcbe_d_mask_o[l_ac].* =  g_xcbe_d[l_ac].*
      CALL axci140_xcbe_t_mask()
      LET g_xcbe_d_mask_n[l_ac].* =  g_xcbe_d[l_ac].*
   END FOR
   
   LET g_xcbe2_d_mask_o.* =  g_xcbe2_d.*
   FOR l_ac = 1 TO g_xcbe2_d.getLength()
      LET g_xcbe2_d_mask_o[l_ac].* =  g_xcbe2_d[l_ac].*
      CALL axci140_xcbe_t_mask()
      LET g_xcbe2_d_mask_n[l_ac].* =  g_xcbe2_d[l_ac].*
   END FOR
 
 
   FREE axci140_pb   
   
END FUNCTION
 
