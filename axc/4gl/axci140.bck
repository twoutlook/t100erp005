# 单个制程料号的最後一道工序约当值需=100%
PRIVATE FUNCTION axci140_xcbe006_chk()
   DEFINE l_xcbe003              LIKE xcbe_t.xcbe003
   DEFINE l_xcbe005              LIKE xcbe_t.xcbe005
   DEFINE l_xcbe006              LIKE xcbe_t.xcbe006
   DEFINE l_xcbe007              LIKE xcbe_t.xcbe007
   DEFINE l_success              LIKE type_t.num5
   
   
   CALL cl_showmsg_init() 
   LET l_success = TRUE
   LET g_sql = "SELECT xcbe003,xcbe004,MAX(xcbe005) ",
               "  FROM xcbe_t ",
               " WHERE xcbeent = '",g_enterprise,"'",
               "   AND xcbesite = '",g_xcbe_m.xcbesite,"'",
               "   AND xcbe001 = '",g_xcbe_m.xcbe001,"'",
               "   AND xcbe002 = '",g_xcbe_m.xcbe002,"'",
               " GROUP BY xcbe003,xcbe004"
               
   PREPARE axci140_pre FROM g_sql
   DECLARE axci140_cure CURSOR FOR axci140_pre
   FOREACH axci140_cure INTO l_xcbe003,l_xcbe005,l_xcbe006
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = sqlca.sqlcode
         LET g_errparam.extend = ''
         LET g_errparam.popup = FALSE
         CALL cl_err()

         EXIT FOREACH
      END IF
      
      SELECT xcbe007 INTO l_xcbe007
        FROM xcbe_t
       WHERE xcbeent = g_enterprise
         AND xcbesite = g_xcbe_m.xcbesite
         AND xcbe001 = g_xcbe_m.xcbe001
         AND xcbe002 = g_xcbe_m.xcbe002
         AND xcbe003 = l_xcbe003
         AND xcbe005 = l_xcbe005
         AND xcbe006 = l_xcbe006
      
      IF l_xcbe007 <> 100 THEN 
         CALL cl_errmsg('',l_xcbe003,'','axc-00292',1) 
         LET l_success = FALSE
      END IF
   END FOREACH 
   IF l_success = FALSE THEN
      CALL cl_err_showmsg()
      CALL axci140_input('u')
   END IF
END FUNCTION
# 製程複製
PRIVATE FUNCTION axci140_copy()
   DEFINE l_ecbb001       LIKE ecbb_t.ecbb001
   DEFINE l_ecbb002       LIKE ecbb_t.ecbb002
   DEFINE l_ecbb003       LIKE ecbb_t.ecbb003
   DEFINE l_ecbb028       LIKE ecbb_t.ecbb028
   DEFINE l_n             LIKE type_t.num5
   DEFINE l_xcbeownid     LIKE xcbe_t.xcbeownid
   DEFINE l_xcbeowndp     LIKE xcbe_t.xcbeowndp
   DEFINE l_xcbecrtid     LIKE xcbe_t.xcbecrtid 
   DEFINE l_xcbecrtdp     LIKE xcbe_t.xcbecrtdp
   DEFINE l_xcbecrtdt     LIKE xcbe_t.xcbecrtdt
   DEFINE l_xcbemodid     LIKE xcbe_t.xcbemodid
   DEFINE l_xcbemoddt     LIKE xcbe_t.xcbemoddt 
   
   CALL s_transaction_begin()
   LET g_success = 'Y'
   
   LET g_sql = "SELECT DISTINCT ecbb001,ecbb002,ecbb003,ecbb028 ",
               "  FROM ecba_t,ecbb_t ",
               " WHERE ecbbent = '",g_enterprise,"'",
               "   AND ecbbsite = '",g_xcbe_m.xcbesite,"'",
               "   AND ecbaent = ecbbent ",
               "   AND ecbasite = ecbbsite ",
               "   AND ecba001 = ecbb001 ",
               "   AND ecba002 = ecbb002 ",
               "   AND ecbastus = 'Y'"               
   PREPARE axci140_pre1 FROM g_sql
   DECLARE axci140_cure1 CURSOR FOR axci140_pre1
   LET l_n = 0
   FOREACH axci140_cure1 INTO l_ecbb001,l_ecbb002,l_ecbb003,l_ecbb028
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "foreach:"
         LET g_errparam.popup = TRUE
         CALL cl_err()

         EXIT FOREACH
      END IF
      
      SELECT COUNT(*) INTO l_n
        FROM xcbe_t
       WHERE xcbeent = g_enterprise
         AND xcbesite = g_xcbe_m.xcbesite
         AND xcbe001 = g_xcbe_m.xcbe001
         AND xcbe002 = g_xcbe_m.xcbe002
         AND xcbe003 = l_ecbb001
         AND xcbe005 = l_ecbb002
         AND xcbe006 = l_ecbb003
         
      IF l_n > 0 THEN 
         CONTINUE FOREACH
      END IF 
      
      IF cl_null(l_ecbb028) THEN 
         LET l_ecbb028 = 0
      END IF
      
      LET l_xcbeownid = g_user
      LET l_xcbeowndp = g_dept
      LET l_xcbecrtid = g_user
      LET l_xcbecrtdp = g_dept 
      LET l_xcbecrtdt = cl_get_current()
      LET l_xcbemodid = ""
      LET l_xcbemoddt = ""
      
      
      INSERT INTO xcbe_t(xcbeent,xcbesite,xcbe001,xcbe002,xcbe003,xcbe004,xcbe005,xcbe006,xcbe007,xcbestus,
                         xcbeownid,xcbeowndp,xcbecrtid,xcbecrtdp,xcbecrtdt,xcbemodid,xcbemoddt)
                  VALUES(g_enterprise,g_xcbe_m.xcbesite,g_xcbe_m.xcbe001,g_xcbe_m.xcbe002,
                         l_ecbb001,' ',l_ecbb002,l_ecbb003,l_ecbb028,'Y',l_xcbeownid,l_xcbeowndp,l_xcbecrtid,
                         l_xcbecrtdp,l_xcbecrtdt,l_xcbemodid,l_xcbemoddt)
                         
      IF SQLCA.SQLcode  THEN
         LET g_success = 'N'
      ELSE
         LET g_success = 'Y'
      END IF
   END FOREACH 
   
   IF g_success = 'N' THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend = "xcbe_t"
      LET g_errparam.popup = TRUE
      CALL cl_err()
  
      CALL s_transaction_end('N','1') 
   ELSE
      CALL s_transaction_end('Y','1')
   END IF
   
  
END FUNCTION

 
