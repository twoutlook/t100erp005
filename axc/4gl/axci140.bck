{<section id="axci140.other_function" readonly="Y" >}
# xcbe003+xbe004+xcbe005欄位檢查
PRIVATE FUNCTION axci140_xcbe_chk()
   DEFINE l_n            LIKE type_t.num5
   DEFINE l_ecbastus     LIKE ecba_t.ecbastus
   
   LET g_errno = ''
   
   IF NOT cl_null(g_xcbe_d[l_ac].xcbe003) THEN 
      LET l_n = 0
      
      SELECT COUNT(*) INTO l_n
        FROM ecba_t
       WHERE ecbaent = g_enterprise
         AND ecbasite = g_xcbe_m.xcbesite
         AND ecba001 = g_xcbe_d[l_ac].xcbe003
         
      CASE
         WHEN l_n = 0   LET g_errno = 'aec-00011'
      END CASE
   END IF
   
   IF NOT cl_null(g_xcbe_d[l_ac].xcbe003) AND NOT cl_null(g_xcbe_d[l_ac].xcbe005) THEN 
      LET l_ecbastus = ''
      
      SELECT ecbastus INTO l_ecbastus
        FROM ecba_t
       WHERE ecbaent = g_enterprise
         AND ecbasite = g_xcbe_m.xcbesite
         AND ecba001 = g_xcbe_d[l_ac].xcbe003
         AND ecba002 = g_xcbe_d[l_ac].xcbe005
         
      CASE
         WHEN SQLCA.SQLCODE = 100    LET g_errno = 'aim-00155'
         WHEN l_ecbastus = 'N'       LET g_errno = 'aim-00156'
      END CASE
   END IF 
   
   IF NOT cl_null(g_xcbe_d[l_ac].xcbe003) AND NOT cl_null(g_xcbe_d[l_ac].xcbe005) AND NOT cl_null(g_xcbe_d[l_ac].xcbe006) THEN
      LET l_ecbastus = ''
      
      SELECT ecbastus INTO l_ecbastus
        FROM ecba_t,ecbb_t
       WHERE ecbaent = g_enterprise
         AND ecbasite = g_xcbe_m.xcbesite
         AND ecba001 = g_xcbe_d[l_ac].xcbe003
         AND ecba002 = g_xcbe_d[l_ac].xcbe005
         AND ecbaent = ecbbent
         AND ecbasite = ecbbsite
         AND ecba001 = ecbb001
         AND ecba002 = ecbb002
         AND ecbb003 = g_xcbe_d[l_ac].xcbe006
         
      CASE
         WHEN SQLCA.SQLCODE = 100    LET g_errno = 'axc-00107'
         WHEN l_ecbastus = 'N'       LET g_errno = 'axc-00108'
      END CASE
   END IF
END FUNCTION
