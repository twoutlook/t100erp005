PRIVATE FUNCTION axct300_02_xccacomp_desc()
   SELECT ooefl003 INTO g_xcca_m.xccacomp_desc FROM ooefl_t 
    WHERE ooeflent=g_enterprise AND ooefl001=g_xcca_m.xccacomp AND ooefl002=g_dlang

