   #CALL axci005_xcaf_b_fill()  #160427-00007#1
   #160621-00013#1---mark---e
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_xcae_d.getLength()
      LET g_xcae_d_mask_o[l_ac].* =  g_xcae_d[l_ac].*
      CALL axci005_xcae_t_mask()
      LET g_xcae_d_mask_n[l_ac].* =  g_xcae_d[l_ac].*
   END FOR
   
   LET g_xcae2_d_mask_o.* =  g_xcae2_d.*
   FOR l_ac = 1 TO g_xcae2_d.getLength()
      LET g_xcae2_d_mask_o[l_ac].* =  g_xcae2_d[l_ac].*
      CALL axci005_xcae_t_mask()
      LET g_xcae2_d_mask_n[l_ac].* =  g_xcae2_d[l_ac].*
   END FOR
 
 
   FREE axci005_pb   
   
END FUNCTION
 
