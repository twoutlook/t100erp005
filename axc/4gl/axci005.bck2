   INSERT INTO axci005_detail1 SELECT * FROM xcaf_t 
                                         WHERE xcafent = g_enterprise AND xcaf001 = g_xcae001_t
                                         AND xcaf002 = g_xcae002_t
 
 
   
   #將key修正為調整後   
   UPDATE axci005_detail1 
