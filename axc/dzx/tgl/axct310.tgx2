   LET g_forupd_sql = " SELECT xccocomp,'',xcco004,xcco005,xccold,'',xcco003,'',xcco009,xcco010", 
                      " FROM xcco_t",
                      " WHERE xccoent= ? AND xccold=? AND xcco003=? AND xcco004=? AND xcco005=? AND  
                          xcco009=? FOR UPDATE"
   #add-point:SQL_define name="main.after_define_sql"
