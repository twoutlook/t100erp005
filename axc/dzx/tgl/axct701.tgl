   #LET g_wc2_table1 = l_wc2_table1
   #LET  g_wc2_table2 = l_wc2_table2 
 
 
 
    
   WHILE TRUE
      LET g_xceald_t = g_xcea_m.xceald
      LET g_xceadocno_t = g_xcea_m.xceadocno
 
      
