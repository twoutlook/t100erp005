       g_xcea_m.xcea006_desc,g_xcea_m.xcea003_desc,g_xcea_m.xceaownid_desc,g_xcea_m.xceaowndp_desc,g_xcea_m.xceacrtid_desc, 
       g_xcea_m.xceacrtdp_desc,g_xcea_m.xceamodid_desc,g_xcea_m.xceacnfid_desc,g_xcea_m.xceapstid_desc 

   
   #檢查是否允許此動作
   IF NOT axct701_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_xcea_m_mask_o.* =  g_xcea_m.*
   CALL axct701_xcea_t_mask()
   LET g_xcea_m_mask_n.* =  g_xcea_m.*
   
   
   
