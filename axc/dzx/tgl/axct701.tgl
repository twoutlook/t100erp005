      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_xceald_t = g_xcea_m.xceald
   LET g_xceadocno_t = g_xcea_m.xceadocno
 
   CALL s_transaction_begin()
   
   OPEN axct701_cl USING g_enterprise,g_xcea_m.xceald,g_xcea_m.xceadocno
   IF SQLCA.SQLCODE THEN   #(ver:78)
