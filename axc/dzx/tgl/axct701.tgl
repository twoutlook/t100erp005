      LET INT_FLAG = 0
      #CALL g_xceb_d.clear()
      #CALL g_xceb4_d.clear()
      #CALL g_xceb8_d.clear()
 
 
      LET g_rec_b = 0
      CALL s_transaction_end('Y','0')
      EXIT WHILE
        
   END WHILE
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL axct701_set_act_visible()   
