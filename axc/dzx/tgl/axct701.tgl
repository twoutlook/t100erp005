       g_xcea_m.xcea003,g_xcea_m.xcea101,g_xcea_m.xceastus,g_xcea_m.xceaownid,g_xcea_m.xceaowndp,g_xcea_m.xceacrtid, 
       g_xcea_m.xceacrtdp,g_xcea_m.xceacrtdt,g_xcea_m.xceamodid,g_xcea_m.xceamoddt,g_xcea_m.xceacnfid, 
       g_xcea_m.xceacnfdt,g_xcea_m.xceapstid,g_xcea_m.xceapstdt,g_xcea_m.xceald_desc,g_xcea_m.xceacomp_desc, 
