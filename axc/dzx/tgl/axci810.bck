               LET gs_keys_bak[6] = g_xcfg005_t
               LET gs_keys[7] = g_xcfg_m.xcfg006
               LET gs_keys_bak[7] = g_xcfg006_t
               LET gs_keys[8] = g_xcfg_m.xcfg007
               LET gs_keys_bak[8] = g_xcfg007_t
               LET gs_keys[9] = g_xcfg_m.xcfg008
               LET gs_keys_bak[9] = g_xcfg008_t
               LET gs_keys[10] = g_xcfh_d[g_detail_idx].xcfh009
               LET gs_keys_bak[10] = g_xcfh_d_t.xcfh009
               LET gs_keys[11] = g_xcfh_d[g_detail_idx].xcfhseq
               LET gs_keys_bak[11] = g_xcfh_d_t.xcfhseq
               CALL axci810_update_b('xcfh_t',gs_keys,gs_keys_bak,"'1'")
               END CASE
 
               #將遮罩欄位進行遮蔽
               CALL axci810_xcfh_t_mask_restore('restore_mask_n')
               
               #判斷key是否有改變
               INITIALIZE gs_keys TO NULL
               IF NOT(g_xcfh_d[g_detail_idx].xcfh009 = g_xcfh_d_t.xcfh009 
                  AND g_xcfh_d[g_detail_idx].xcfhseq = g_xcfh_d_t.xcfhseq 
 
                  ) THEN
                  LET gs_keys[01] = g_xcfg_m.xcfgld
                  LET gs_keys[gs_keys.getLength()+1] = g_xcfg_m.xcfg001
                  LET gs_keys[gs_keys.getLength()+1] = g_xcfg_m.xcfg002
                  LET gs_keys[gs_keys.getLength()+1] = g_xcfg_m.xcfg003
                  LET gs_keys[gs_keys.getLength()+1] = g_xcfg_m.xcfg004
                  LET gs_keys[gs_keys.getLength()+1] = g_xcfg_m.xcfg005
                  LET gs_keys[gs_keys.getLength()+1] = g_xcfg_m.xcfg006
                  LET gs_keys[gs_keys.getLength()+1] = g_xcfg_m.xcfg007
                  LET gs_keys[gs_keys.getLength()+1] = g_xcfg_m.xcfg008
 
                  LET gs_keys[gs_keys.getLength()+1] = g_xcfh_d_t.xcfh009
                  LET gs_keys[gs_keys.getLength()+1] = g_xcfh_d_t.xcfhseq
 
                  CALL axci810_key_update_b(gs_keys,'xcfh_t')
               END IF
               
               #修改歷程記錄(單身修改)
               LET g_log1 = util.JSON.stringify(g_xcfg_m),util.JSON.stringify(g_xcfh_d_t)
               LET g_log2 = util.JSON.stringify(g_xcfg_m),util.JSON.stringify(g_xcfh_d[l_ac])
               IF NOT cl_log_modified_record_d(g_log1,g_log2) THEN 
                  CALL s_transaction_end('N','0')
               END IF
               
               #add-point:單身修改後 name="input.body.a_update"
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            #add-point:單身after_row name="input.body.after_row"
            {<point name="input.body.after_row"/>}
            #end add-point
            CALL axci810_unlock_b("xcfh_t","'1'")
