                   g_xcfg_m.xcfg004,g_xcfg_m.xcfg005,g_xcfg_m.xcfg006,g_xcfg_m.xcfg007,g_xcfg_m.xcfg008, 
                   g_xcfh_d[l_ac].xcfhseq,g_xcfh_d[l_ac].xcfh012,g_xcfh_d[l_ac].xcfh010,g_xcfh_d[l_ac].xcfh011, 
                   g_xcfh_d[l_ac].xcfh013,g_xcfh_d[l_ac].xcfh014,g_xcfh_d[l_ac].xcfh015,g_xcfh_d[l_ac].xcfh017, 
