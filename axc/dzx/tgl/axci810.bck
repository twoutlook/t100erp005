   LET g_xcfg005_t = g_xcfg_m.xcfg005
   LET g_xcfg006_t = g_xcfg_m.xcfg006
   LET g_xcfg007_t = g_xcfg_m.xcfg007
   LET g_xcfg008_t = g_xcfg_m.xcfg008
 
   CALL s_transaction_begin()
   
   OPEN axci810_cl USING g_enterprise,g_xcfg_m.xcfgld,g_xcfg_m.xcfg001,g_xcfg_m.xcfg002,g_xcfg_m.xcfg003,g_xcfg_m.xcfg004,g_xcfg_m.xcfg005,g_xcfg_m.xcfg006,g_xcfg_m.xcfg007,g_xcfg_m.xcfg008
   IF SQLCA.SQLCODE THEN   #(ver:78)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN axci810_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE   #(ver:78)
      LET g_errparam.popup = TRUE 
      CLOSE axci810_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE axci810_master_referesh USING g_xcfg_m.xcfgld,g_xcfg_m.xcfg001,g_xcfg_m.xcfg002,g_xcfg_m.xcfg003, 
       g_xcfg_m.xcfg004,g_xcfg_m.xcfg005,g_xcfg_m.xcfg006,g_xcfg_m.xcfg007,g_xcfg_m.xcfg008 INTO g_xcfg_m.xcfgcomp, 
       g_xcfg_m.xcfg006,g_xcfg_m.xcfgld,g_xcfg_m.xcfg004,g_xcfg_m.xcfg005,g_xcfg_m.xcfg003,g_xcfg_m.xcfg007, 
       g_xcfg_m.xcfg001,g_xcfg_m.xcfg002,g_xcfg_m.xcfg008,g_xcfg_m.xcfg015,g_xcfg_m.xcfg016,g_xcfg_m.xcfg017, 
       g_xcfg_m.xcfg018,g_xcfg_m.xcfg019,g_xcfg_m.xcfg023,g_xcfg_m.xcfg025,g_xcfg_m.xcfg028,g_xcfg_m.xcfg029, 
       g_xcfg_m.xcfgcomp_desc,g_xcfg_m.xcfg006_desc,g_xcfg_m.xcfgld_desc,g_xcfg_m.xcfg006_desc_desc, 
       g_xcfg_m.xcfg003_desc,g_xcfg_m.xcfg002_desc
   
   #檢查是否允許此動作
   IF NOT axci810_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_xcfg_m_mask_o.* =  g_xcfg_m.*
   CALL axci810_xcfg_t_mask()
   LET g_xcfg_m_mask_n.* =  g_xcfg_m.*
   
   
   
   #add-point:modify段show之前 name="modify.before_show"
   {<point name="modify.before_show"/>}
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
 
   
   CALL axci810_show()
   #add-point:modify段show之後 name="modify.after_show"
   {<point name="modify.after_show"/>}
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
 
 
    
   WHILE TRUE
      LET g_xcfgld_t = g_xcfg_m.xcfgld
      LET g_xcfg001_t = g_xcfg_m.xcfg001
      LET g_xcfg002_t = g_xcfg_m.xcfg002
      LET g_xcfg003_t = g_xcfg_m.xcfg003
      LET g_xcfg004_t = g_xcfg_m.xcfg004
      LET g_xcfg005_t = g_xcfg_m.xcfg005
      LET g_xcfg006_t = g_xcfg_m.xcfg006
      LET g_xcfg007_t = g_xcfg_m.xcfg007
      LET g_xcfg008_t = g_xcfg_m.xcfg008
 
      
      #寫入修改者/修改日期資訊(單頭)
      
      
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      LET g_master_commit = "N"
      CALL axci810_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後 name="modify.after_input"
      {<point name="modify.after_input"/>}
      #end add-point
      
 
    
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         #若單頭無commit則還原
         IF g_master_commit = "N" THEN
            LET g_xcfg_m.* = g_xcfg_m_t.*
            CALL axci810_show()
         END IF
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code = 9001 
         LET g_errparam.popup = FALSE 
         CALL cl_err()
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_xcfg_m.xcfgld != g_xcfg_m_t.xcfgld
      OR g_xcfg_m.xcfg001 != g_xcfg_m_t.xcfg001
      OR g_xcfg_m.xcfg002 != g_xcfg_m_t.xcfg002
      OR g_xcfg_m.xcfg003 != g_xcfg_m_t.xcfg003
      OR g_xcfg_m.xcfg004 != g_xcfg_m_t.xcfg004
      OR g_xcfg_m.xcfg005 != g_xcfg_m_t.xcfg005
      OR g_xcfg_m.xcfg006 != g_xcfg_m_t.xcfg006
      OR g_xcfg_m.xcfg007 != g_xcfg_m_t.xcfg007
      OR g_xcfg_m.xcfg008 != g_xcfg_m_t.xcfg008
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前 name="modify.body.b_fk_update"
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE xcfh_t SET xcfhld = g_xcfg_m.xcfgld
                                       ,xcfh001 = g_xcfg_m.xcfg001
                                       ,xcfh002 = g_xcfg_m.xcfg002
                                       ,xcfh003 = g_xcfg_m.xcfg003
                                       ,xcfh004 = g_xcfg_m.xcfg004
                                       ,xcfh005 = g_xcfg_m.xcfg005
                                       ,xcfh006 = g_xcfg_m.xcfg006
                                       ,xcfh007 = g_xcfg_m.xcfg007
                                       ,xcfh008 = g_xcfg_m.xcfg008
 
          WHERE xcfhent = g_enterprise AND xcfhld = g_xcfg_m_t.xcfgld
            AND xcfh001 = g_xcfg_m_t.xcfg001
            AND xcfh002 = g_xcfg_m_t.xcfg002
            AND xcfh003 = g_xcfg_m_t.xcfg003
            AND xcfh004 = g_xcfg_m_t.xcfg004
            AND xcfh005 = g_xcfg_m_t.xcfg005
            AND xcfh006 = g_xcfg_m_t.xcfg006
            AND xcfh007 = g_xcfg_m_t.xcfg007
            AND xcfh008 = g_xcfg_m_t.xcfg008
 
            
         #add-point:單身fk修改中 name="modify.body.m_fk_update"
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            #   INITIALIZE g_errparam TO NULL 
            #   LET g_errparam.extend = "xcfh_t" 
            #   LET g_errparam.code = "std-00009" 
            #   LET g_errparam.popup = TRUE 
            #   CALL cl_err()
            #   CALL s_transaction_end('N','0')
            #   CONTINUE WHILE
            WHEN SQLCA.SQLCODE #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "xcfh_t:",SQLERRMESSAGE 
               LET g_errparam.code = SQLCA.SQLCODE 
               LET g_errparam.popup = TRUE 
               CALL s_transaction_end('N','0')
               CALL cl_err()
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後 name="modify.body.a_fk_update"
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL axci810_set_act_visible()   
   CALL axci810_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " xcfgent = " ||g_enterprise|| " AND",
                      " xcfgld = '", g_xcfg_m.xcfgld, "' "
                      ," AND xcfg001 = '", g_xcfg_m.xcfg001, "' "
                      ," AND xcfg002 = '", g_xcfg_m.xcfg002, "' "
                      ," AND xcfg003 = '", g_xcfg_m.xcfg003, "' "
                      ," AND xcfg004 = '", g_xcfg_m.xcfg004, "' "
                      ," AND xcfg005 = '", g_xcfg_m.xcfg005, "' "
                      ," AND xcfg006 = '", g_xcfg_m.xcfg006, "' "
                      ," AND xcfg007 = '", g_xcfg_m.xcfg007, "' "
                      ," AND xcfg008 = '", g_xcfg_m.xcfg008, "' "
 
   #填到對應位置
   CALL axci810_browser_fill("")
 
   CLOSE axci810_cl
   
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
