   {<point name="main.define"/>}
   #end add-point   
   
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("axc","")
 
   #add-point:作業初始化 name="main.init"
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define name="main.define_sql"
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT xcfgcomp,'',xcfg006,xcfgld,'','',xcfg004,xcfg005,'',xcfg003,'',xcfg007, 
       xcfg001,xcfg002,'',xcfg008,xcfg015,xcfg016,xcfg017,xcfg018,'','','','',xcfg019,xcfg023,xcfg025, 
       xcfg028,xcfg029", 
                      " FROM xcfg_t",
                      " WHERE xcfgent= ? AND xcfgld=? AND xcfg001=? AND xcfg002=? AND xcfg003=? AND  
                          xcfg004=? AND xcfg005=? AND xcfg006=? AND xcfg007=? AND xcfg008=? FOR UPDATE" 

   #add-point:SQL_define name="main.after_define_sql"
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE axci810_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.xcfgcomp,t0.xcfg006,t0.xcfgld,t0.xcfg004,t0.xcfg005,t0.xcfg003,t0.xcfg007, 
       t0.xcfg001,t0.xcfg002,t0.xcfg008,t0.xcfg015,t0.xcfg016,t0.xcfg017,t0.xcfg018,t0.xcfg019,t0.xcfg023, 
       t0.xcfg025,t0.xcfg028,t0.xcfg029,t1.ooefl003 ,t2.imaal003 ,t3.glaal002 ,t4.imaal004 ,t5.xcatl003 , 
       t6.xcbfl003",
               " FROM xcfg_t t0",
                              " LEFT JOIN ooefl_t t1 ON t1.ooeflent="||g_enterprise||" AND t1.ooefl001=t0.xcfgcomp AND t1.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t2 ON t2.imaalent="||g_enterprise||" AND t2.imaal001=t0.xcfg006 AND t2.imaal002='"||g_dlang||"' ",
               " LEFT JOIN glaal_t t3 ON t3.glaalent="||g_enterprise||" AND t3.glaalld=t0.xcfgld AND t3.glaal001='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t4 ON t4.imaalent="||g_enterprise||" AND t4.imaal001=t0.xcfg006 AND t4.imaal002='"||g_dlang||"' ",
               " LEFT JOIN xcatl_t t5 ON t5.xcatlent="||g_enterprise||" AND t5.xcatl001=t0.xcfg003 AND t5.xcatl002='"||g_dlang||"' ",
               " LEFT JOIN xcbfl_t t6 ON t6.xcbflent="||g_enterprise||" AND t6.xcbflcomp=t0.xcfgcomp AND t6.xcbfl001=t0.xcfg002 AND t6.xcbfl002='"||g_dlang||"' ",
 
               " WHERE t0.xcfgent = " ||g_enterprise|| " AND t0.xcfgld = ? AND t0.xcfg001 = ? AND t0.xcfg002 = ? AND t0.xcfg003 = ? AND t0.xcfg004 = ? AND t0.xcfg005 = ? AND t0.xcfg006 = ? AND t0.xcfg007 = ? AND t0.xcfg008 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define name="main.after_refresh_sql"
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE axci810_master_referesh FROM g_sql
 
    
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call name="main.servicecall"
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_axci810 WITH FORM cl_ap_formpath("axc",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL axci810_init()   
 
      #進入選單 Menu (="N")
