   {<point name="modify.pre_function"/>}
   #end add-point
   
   IF g_xccp_m.xccpld IS NULL
   OR g_xccp_m.xccp003 IS NULL
   OR g_xccp_m.xccp004 IS NULL
   OR g_xccp_m.xccp005 IS NULL
   OR g_xccp_m.xccp006 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL  
