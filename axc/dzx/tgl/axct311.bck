   LET g_xccp_m_mask_n.* =  g_xccp_m.*
   
   CALL axct311_show()
 
   #單頭多語言資料備份
   
   
   IF cl_ask_del_master() THEN              #確認一下
      #應用 a47 樣板自動產生(Version:4)
