{<section id="axct306.delete" >}
#+ 資料刪除
PRIVATE FUNCTION axct306_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_xcdj_m.xcdjld IS NULL
   OR g_xcdj_m.xcdj003 IS NULL
   OR g_xcdj_m.xcdj004 IS NULL
   OR g_xcdj_m.xcdj005 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
 
      RETURN
   END IF
 
   EXECUTE axct306_master_referesh USING g_xcdj_m.xcdjld,g_xcdj_m.xcdj003,g_xcdj_m.xcdj004,g_xcdj_m.xcdj005 INTO g_xcdj_m.xcdjcomp, 
       g_xcdj_m.xcdj004,g_xcdj_m.xcdj005,g_xcdj_m.xcdjld,g_xcdj_m.xcdj003,g_xcdj_m.xcdjcomp_desc,g_xcdj_m.xcdjld_desc, 
       g_xcdj_m.xcdj003_desc
   
   CALL axct306_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN axct306_cl USING g_enterprise,g_xcdj_m.xcdjld
                                                       ,g_xcdj_m.xcdj003
                                                       ,g_xcdj_m.xcdj004
                                                       ,g_xcdj_m.xcdj005
 
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN axct306_cl:" 
      LET g_errparam.code   =  STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
 
      CLOSE axct306_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH axct306_cl INTO g_xcdj_m.xcdjcomp,g_xcdj_m.xcdjcomp_desc,g_xcdj_m.xcdj004,g_xcdj_m.xcdj005, 
       g_xcdj_m.xcdjld,g_xcdj_m.xcdjld_desc,g_xcdj_m.xcdj003,g_xcdj_m.xcdj003_desc
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = g_xcdj_m.xcdjld 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
 
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL axct306_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM xcdj_t WHERE xcdjent = g_enterprise AND xcdjld = g_xcdj_m.xcdjld
                                                               AND xcdj003 = g_xcdj_m.xcdj003
                                                               AND xcdj004 = g_xcdj_m.xcdj004
                                                               AND xcdj005 = g_xcdj_m.xcdj005
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "xcdj_t" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
 
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_xcdj_d.clear() 
      CALL g_xcdj2_d.clear()       
      CALL g_xcdj3_d.clear()       
 
     
      CALL axct306_ui_browser_refresh()  
      CALL axct306_ui_headershow()  
      CALL axct306_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL axct306_fetch('P')
      ELSE
         #LET g_wc = " 1=1"
         #LET g_wc2 = " 1=1"
         #CALL axct306_browser_fill("F")
         CLEAR FORM
      END IF
       
   END IF
 
   CLOSE axct306_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_xcdj_m.xcdjld,'D')
    
END FUNCTION

{</section>}

