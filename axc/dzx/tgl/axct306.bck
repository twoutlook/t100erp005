{<section id="axct306.show" >}
#+ 單頭資料重新顯示及單身資料重抓
PRIVATE FUNCTION axct306_show()
   #add-point:show段define
   {<point name="show.define"/>}
   #end add-point
   
   #add-point:show段之前
   {<point name="show.before"/>}
   #end add-point
   
   IF g_bfill = "Y" THEN
      CALL axct306_b_fill(g_wc2) #單身填充
      CALL axct306_b_fill2('0') #單身填充
   END IF
   
   
 
   #顯示followup圖示
   #+ 此段落由子樣板a48產生
   CALL axct306_set_pk_array()
   #add-point:ON ACTION agendum
   {<point name="show.follow_pic"/>}
   #END add-point
   CALL cl_user_overview_set_follow_pic()
 
 
   
   DISPLAY BY NAME g_xcdj_m.xcdjcomp,g_xcdj_m.xcdjcomp_desc,g_xcdj_m.xcdj004,g_xcdj_m.xcdj005,g_xcdj_m.xcdjld, 
       g_xcdj_m.xcdjld_desc,g_xcdj_m.xcdj003,g_xcdj_m.xcdj003_desc
   CALL axct306_b_fill(g_wc2_table1)                 #單身
   CALL axct306_b_fill2('0') #單身填充
 
   CALL axct306_ref_show()
 
   #移動上下筆可以連動切換資料
   CALL cl_show_fld_cont()  
 
   #add-point:show段之後
   {<point name="show.after"/>}
   #end add-point   
   
END FUNCTION

