{<section id="axct306.modify" >}
#+ 資料修改
PRIVATE FUNCTION axct306_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_xcdj_m.xcdjld IS NULL
   OR g_xcdj_m.xcdj003 IS NULL
   OR g_xcdj_m.xcdj004 IS NULL
   OR g_xcdj_m.xcdj005 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
 
      RETURN
   END IF
   
   EXECUTE axct306_master_referesh USING g_xcdj_m.xcdjld,g_xcdj_m.xcdj003,g_xcdj_m.xcdj004,g_xcdj_m.xcdj005 INTO g_xcdj_m.xcdjcomp, 
       g_xcdj_m.xcdj004,g_xcdj_m.xcdj005,g_xcdj_m.xcdjld,g_xcdj_m.xcdj003,g_xcdj_m.xcdjcomp_desc,g_xcdj_m.xcdjld_desc, 
       g_xcdj_m.xcdj003_desc
 
   ERROR ""
  
   LET g_xcdjld_t = g_xcdj_m.xcdjld
   LET g_xcdj003_t = g_xcdj_m.xcdj003
   LET g_xcdj004_t = g_xcdj_m.xcdj004
   LET g_xcdj005_t = g_xcdj_m.xcdj005
 
   CALL s_transaction_begin()
   
   OPEN axct306_cl USING g_enterprise,g_xcdj_m.xcdjld
                                                       ,g_xcdj_m.xcdj003
                                                       ,g_xcdj_m.xcdj004
                                                       ,g_xcdj_m.xcdj005
 
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN axct306_cl:" 
      LET g_errparam.code   =  STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
 
      CLOSE axct306_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH axct306_cl INTO g_xcdj_m.xcdjcomp,g_xcdj_m.xcdjcomp_desc,g_xcdj_m.xcdj004,g_xcdj_m.xcdj005, 
       g_xcdj_m.xcdjld,g_xcdj_m.xcdjld_desc,g_xcdj_m.xcdj003,g_xcdj_m.xcdj003_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = g_xcdj_m.xcdjld 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
 
      CLOSE axct306_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL axct306_show()
   WHILE TRUE
      LET g_xcdjld_t = g_xcdj_m.xcdjld
      LET g_xcdj003_t = g_xcdj_m.xcdj003
      LET g_xcdj004_t = g_xcdj_m.xcdj004
      LET g_xcdj005_t = g_xcdj_m.xcdj005
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL axct306_input("u")     #欄位更改
      
