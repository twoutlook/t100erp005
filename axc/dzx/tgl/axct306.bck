PRIVATE FUNCTION axct306_detail_reproduce()
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE xcdj_t.*
 
 
   #add-point:delete段define
   {<point name="detail_reproduce.define"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE axct306_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE axct306_detail AS ",
                "SELECT * FROM xcdj_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO axct306_detail SELECT * FROM xcdj_t 
                                         WHERE xcdjent = g_enterprise AND xcdjld = g_xcdjld_t
                                         AND xcdj003 = g_xcdj003_t
                                         AND xcdj004 = g_xcdj004_t
                                         AND xcdj005 = g_xcdj005_t
 
   
   #將key修正為調整後   
   UPDATE axct306_detail 
      #更新key欄位
      SET xcdjld = g_xcdj_m.xcdjld
          , xcdj003 = g_xcdj_m.xcdj003
          , xcdj004 = g_xcdj_m.xcdj004
          , xcdj005 = g_xcdj_m.xcdj005
 
      #更新共用欄位
      
                                       
   #將資料塞回原table   
   INSERT INTO xcdj_t SELECT * FROM axct306_detail
   
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "reproduce" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
 
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE axct306_detail
   
   #add-point:單身複製後1
   {<point name="detail_reproduce.body.table1.a_insert"/>}
   #end add-point
 
 
   
 
   
   #多語言複製段落
   
   
   CALL s_transaction_end('Y','0')
   
   #已新增完, 調整資料內容(修改時使用)
   LET g_xcdjld_t = g_xcdj_m.xcdjld
   LET g_xcdj003_t = g_xcdj_m.xcdj003
   LET g_xcdj004_t = g_xcdj_m.xcdj004
   LET g_xcdj005_t = g_xcdj_m.xcdj005
 
   
   DROP TABLE axct306_detail
   
END FUNCTION

