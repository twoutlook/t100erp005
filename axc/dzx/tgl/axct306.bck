{<section id="axct306.reproduce" >}
#+ 資料複製
PRIVATE FUNCTION axct306_reproduce()
   DEFINE l_newno     LIKE xcdj_t.xcdjld 
   DEFINE l_oldno     LIKE xcdj_t.xcdjld 
   DEFINE l_newno02     LIKE xcdj_t.xcdj003 
   DEFINE l_oldno02     LIKE xcdj_t.xcdj003 
   DEFINE l_newno03     LIKE xcdj_t.xcdj004 
   DEFINE l_oldno03     LIKE xcdj_t.xcdj004 
   DEFINE l_newno04     LIKE xcdj_t.xcdj005 
   DEFINE l_oldno04     LIKE xcdj_t.xcdj005 
 
   DEFINE l_master    RECORD LIKE xcdj_t.*
   DEFINE l_detail    RECORD LIKE xcdj_t.*
 
   DEFINE l_cnt       LIKE type_t.num5
   #add-point:reproduce段define
   {<point name="reproduce.define"/>}
   #end add-point
 
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF     
 
   IF g_xcdj_m.xcdjld IS NULL
      OR g_xcdj_m.xcdj003 IS NULL
      OR g_xcdj_m.xcdj004 IS NULL
      OR g_xcdj_m.xcdj005 IS NULL
 
      THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
 
      RETURN
   END IF
   
   LET g_xcdjld_t = g_xcdj_m.xcdjld
   LET g_xcdj003_t = g_xcdj_m.xcdj003
   LET g_xcdj004_t = g_xcdj_m.xcdj004
   LET g_xcdj005_t = g_xcdj_m.xcdj005
 
   
   LET g_xcdj_m.xcdjld = ""
   LET g_xcdj_m.xcdj003 = ""
   LET g_xcdj_m.xcdj004 = ""
   LET g_xcdj_m.xcdj005 = ""
 
    
   CALL axct306_set_entry('a')
   CALL axct306_set_no_entry('a')
   
   CALL cl_set_head_visible("","YES")
   
   CALL s_transaction_begin()
   
   #add-point:複製輸入前
   {<point name="reproduce.head.b_input"/>}
   #end add-point
   
   CALL axct306_input("r")
   
      LET g_xcdj_m.xcdjld_desc = ''
   DISPLAY BY NAME g_xcdj_m.xcdjld_desc
   LET g_xcdj_m.xcdj003_desc = ''
   DISPLAY BY NAME g_xcdj_m.xcdj003_desc
 
    
   IF INT_FLAG THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = '' 
      LET g_errparam.code   = 9001 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
      DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
      INITIALIZE g_xcdj_m.* TO NULL
      INITIALIZE g_xcdj_d TO NULL
      INITIALIZE g_xcdj2_d TO NULL
      INITIALIZE g_xcdj3_d TO NULL
 
      CALL axct306_show()
      LET INT_FLAG = 0
      RETURN
   END IF
   
   #將新增的資料併入搜尋條件中
   LET g_state = "insert"
   
   LET g_xcdjld_t = g_xcdj_m.xcdjld
   LET g_xcdj003_t = g_xcdj_m.xcdj003
   LET g_xcdj004_t = g_xcdj_m.xcdj004
   LET g_xcdj005_t = g_xcdj_m.xcdj005
 
   
   #組合新增資料的條件
   LET g_add_browse = " xcdjent = '" ||g_enterprise|| "' AND",
                      " xcdjld = '", g_xcdj_m.xcdjld CLIPPED, "' "
                      ," AND xcdj003 = '", g_xcdj_m.xcdj003 CLIPPED, "' "
                      ," AND xcdj004 = '", g_xcdj_m.xcdj004 CLIPPED, "' "
                      ," AND xcdj005 = '", g_xcdj_m.xcdj005 CLIPPED, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL axct306_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   #add-point:完成複製段落後
