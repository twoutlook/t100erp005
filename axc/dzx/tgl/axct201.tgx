   LET g_sql = "SELECT  DISTINCT xcblseq,xcbl010,xcbl023,xcbl011,xcbl012,xcbl013,xcbl024,xcbl025,xcbl014,xcbl015,xcbl016,xcbl017,xcbl018,xcbl019,xcbl020,xcbl021,xcbl022,xcbl100,xcbl110,xcbl120,xcblseq,xcblownid,xcblowndp,xcblcrtid,xcblcrtdp,xcblcrtdt,xcblmodid,xcblmoddt,t1.xcaul003 ,t2.ooefl003 ,t3.ooefl003 ,t4.pmaal004 ,t5.pmaal004 ,t6.ooag011 ,t7.oocql004 ,t8.oocql004 ,t9.ooefl003 ,t10.gzcbl004 ,t11.oocql004 ,t12.rtaxl003 ,t13.oocql004 ,t14.pjbal003 ,t15.pjbbl004 ,t16.ooag011 ,t17.ooefl003 ,t18.ooag011 ,t19.ooefl003 ,t20.ooag011 FROM xcbl_t",   
