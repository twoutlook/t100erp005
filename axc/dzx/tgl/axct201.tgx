               " LEFT JOIN glaal_t t2 ON t2.glaalent="||g_enterprise||" AND t2.glaalld=t0.xcblld AND t2.glaal001='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent="||g_enterprise||" AND t3.ooefl001=t0.xcbl004 AND t3.ooefl002='"||g_dlang||"' ",

               " WHERE t0.xcblent = " ||g_enterprise|| " AND t0.xcblld = ? AND t0.xcbl001 = ? AND t0.xcbl002 = ? AND t0.xcbl003 = ? AND t0.xcbl004 = ? AND t0.xcbl005 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define name="main.after_refresh_sql"
   {<point name="main.after_refresh_sql"/>}
