   LET g_forupd_sql = " SELECT xcblcomp,'',xcblld,'',xcbl002,xcbl003,xcbl004,'',xcbl001,xcbl005", 
                      " FROM xcbl_t",
                      " WHERE xcblent= ? AND xcblld=? AND xcbl001=? AND xcbl002=? AND xcbl003=? AND xcbl004=? AND xcbl005=? FOR UPDATE"
   #add-point:SQL_define name="main.after_define_sql"
