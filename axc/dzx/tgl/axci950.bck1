#+ QBE資料查詢
PRIVATE FUNCTION axci950_query()
   #add-point:query段define(客製用) name="query.define_customerization"
   {<point name="query.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE ls_wc      LIKE type_t.chr500
   DEFINE ls_return  STRING
   DEFINE ls_result  STRING 
   #add-point:query段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="query.define"
   {<point name="query.define"/>}
   #end add-point 
   
   #add-point:Function前置處理  name="query.pre_function"
   {<point name="query.pre_function"/>}
   #end add-point
   
   LET INT_FLAG = 0
   CLEAR FORM
   CALL g_xcja_d.clear()
   
   LET g_qryparam.state = "c"
   
   #wc備份
   LET ls_wc = g_wc2
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      CONSTRUCT g_wc2 ON xcja001,xcjal003,xcja002,xcja008,xcja005,xcja003,xcja009,xcja004,xcja011,xcja006, 
          xcja007,xcja010,xcjastus,xcjaownid,xcjaowndp,xcjacrtid,xcjacrtdp,xcjacrtdt,xcjamodid,xcjamoddt  

 
         FROM s_detail1[1].xcja001,s_detail1[1].xcjal003,s_detail1[1].xcja002,s_detail1[1].xcja008,s_detail1[1].xcja005, 
             s_detail1[1].xcja003,s_detail1[1].xcja009,s_detail1[1].xcja004,s_detail1[1].xcja011,s_detail1[1].xcja006, 
             s_detail1[1].xcja007,s_detail1[1].xcja010,s_detail1[1].xcjastus,s_detail2[1].xcjaownid, 
             s_detail2[1].xcjaowndp,s_detail2[1].xcjacrtid,s_detail2[1].xcjacrtdp,s_detail2[1].xcjacrtdt, 
             s_detail2[1].xcjamodid,s_detail2[1].xcjamoddt 
      
