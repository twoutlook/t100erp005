   {<point name="b_fill.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE p_wc2    STRING
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   {<point name="b_fill.define"/>}
   #end add-point
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   {<point name="b_fill.pre_function"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前 name="b_fill.sql_before"
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  DISTINCT t0.xcja001,t0.xcja002,t0.xcja008,t0.xcja005,t0.xcja003,t0.xcja009,t0.xcja004, 
       t0.xcja011,t0.xcja006,t0.xcja007,t0.xcja010,t0.xcjastus,t0.xcja001,t0.xcjaownid,t0.xcjaowndp, 
       t0.xcjacrtid,t0.xcjacrtdp,t0.xcjacrtdt,t0.xcjamodid,t0.xcjamoddt ,t1.xcatl003 ,t2.ooail003 ,t3.ooefl003 , 
       t4.ooag011 ,t5.ooefl003 ,t6.ooag011 ,t7.ooefl003 ,t8.ooag011 FROM xcja_t t0",
               " LEFT JOIN xcjal_t ON xcjalent = "||g_enterprise||" AND xcja001 = xcjal001 AND xcjal002 = '",g_dlang,"'",
                              " LEFT JOIN xcatl_t t1 ON t1.xcatlent="||g_enterprise||" AND t1.xcatl001=t0.xcja009 AND t1.xcatl002='"||g_dlang||"' ",
               " LEFT JOIN ooail_t t2 ON t2.ooailent="||g_enterprise||" AND t2.ooail001=t0.xcja006 AND t2.ooail002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent="||g_enterprise||" AND t3.ooefl001=t0.xcja007 AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t4 ON t4.ooagent="||g_enterprise||" AND t4.ooag001=t0.xcjaownid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent="||g_enterprise||" AND t5.ooefl001=t0.xcjaowndp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent="||g_enterprise||" AND t6.ooag001=t0.xcjacrtid  ",
               " LEFT JOIN ooefl_t t7 ON t7.ooeflent="||g_enterprise||" AND t7.ooefl001=t0.xcjacrtdp AND t7.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t8 ON t8.ooagent="||g_enterprise||" AND t8.ooag001=t0.xcjamodid  ",
 
               " WHERE t0.xcjaent= ?  AND  1=1 AND (", p_wc2, ") " 
   #add-point:b_fill段sql wc name="b_fill.sql_wc"
   {<point name="b_fill.sql_wc"/>}
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("xcja_t"),
                      " ORDER BY t0.xcja001"
   
   #add-point:b_fill段sql之後 name="b_fill.sql_after"
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"xcja_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE axci950_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR axci950_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_xcja_d.clear()
   CALL g_xcja2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_xcja_d[l_ac].xcja001,g_xcja_d[l_ac].xcja002,g_xcja_d[l_ac].xcja008,g_xcja_d[l_ac].xcja005, 
       g_xcja_d[l_ac].xcja003,g_xcja_d[l_ac].xcja009,g_xcja_d[l_ac].xcja004,g_xcja_d[l_ac].xcja011,g_xcja_d[l_ac].xcja006, 
       g_xcja_d[l_ac].xcja007,g_xcja_d[l_ac].xcja010,g_xcja_d[l_ac].xcjastus,g_xcja2_d[l_ac].xcja001, 
       g_xcja2_d[l_ac].xcjaownid,g_xcja2_d[l_ac].xcjaowndp,g_xcja2_d[l_ac].xcjacrtid,g_xcja2_d[l_ac].xcjacrtdp, 
       g_xcja2_d[l_ac].xcjacrtdt,g_xcja2_d[l_ac].xcjamodid,g_xcja2_d[l_ac].xcjamoddt,g_xcja_d[l_ac].xcja009_desc, 
       g_xcja_d[l_ac].xcja006_desc,g_xcja_d[l_ac].xcja007_desc,g_xcja2_d[l_ac].xcjaownid_desc,g_xcja2_d[l_ac].xcjaowndp_desc, 
       g_xcja2_d[l_ac].xcjacrtid_desc,g_xcja2_d[l_ac].xcjacrtdp_desc,g_xcja2_d[l_ac].xcjamodid_desc
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充 name="b_fill.fill"
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL axci950_detail_show()      
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
 
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   LET g_error_show = 0
   
 
   
   CALL g_xcja_d.deleteElement(g_xcja_d.getLength())   
   CALL g_xcja2_d.deleteElement(g_xcja2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_xcja_d.getLength()
      LET g_xcja2_d[l_ac].xcja001 = g_xcja_d[l_ac].xcja001 
 
      #add-point:b_fill段key值相關欄位 name="b_fill.keys.fill"
      {<point name="b_fill.keys.fill"/>}
      #end add-point
   END FOR
   
   IF g_cnt > g_xcja_d.getLength() THEN
      LET l_ac = g_xcja_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_xcja_d.getLength()
      LET g_xcja_d_mask_o[l_ac].* =  g_xcja_d[l_ac].*
      CALL axci950_xcja_t_mask()
      LET g_xcja_d_mask_n[l_ac].* =  g_xcja_d[l_ac].*
   END FOR
   
   LET g_xcja2_d_mask_o.* =  g_xcja2_d.*
   FOR l_ac = 1 TO g_xcja2_d.getLength()
      LET g_xcja2_d_mask_o[l_ac].* =  g_xcja2_d[l_ac].*
      CALL axci950_xcja_t_mask()
      LET g_xcja2_d_mask_n[l_ac].* =  g_xcja2_d[l_ac].*
   END FOR
 
   
   LET l_ac = g_cnt
 
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   {<point name="b_fill.others.fill"/>}
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_xcja_d.getLength()
   DISPLAY g_detail_idx TO FORMONLY.idx
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE axci950_pb
   
END FUNCTION

