  <section id="axci111.b_fill" ver="6" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION axci111_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2    STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE xcaustus,xcau001,xcau002,xcau003,xcau001,xcauownid,xcauowndp,xcaucrtid, 
       xcaucrtdp,xcaucrtdt,xcaumodid,xcaumoddt ,t1.xcavl003 ,t2.oofa011 ,t3.ooefl003 ,t4.oofa011 ,t5.ooefl003 , 
       t6.oofa011 FROM xcau_t",
               " LEFT JOIN xcaul_t ON xcau001 = xcaul001 AND xcaul002 = '",g_lang,"'",
                              " LEFT JOIN xcavl_t t1 ON t1.xcavlent='"||g_enterprise||"' AND t1.xcavl001=xcau002 AND t1.xcavl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t2 ON t2.oofaent='"||g_enterprise||"' AND t2.oofa002='2' AND t2.oofa003=xcauownid  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent='"||g_enterprise||"' AND t3.ooefl001=xcauowndp AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t4 ON t4.oofaent='"||g_enterprise||"' AND t4.oofa002='2' AND t4.oofa003=xcaucrtid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=xcaucrtdp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t6 ON t6.oofaent='"||g_enterprise||"' AND t6.oofa002='2' AND t6.oofa003=xcaumodid  ",
 
               " WHERE xcauent= ? AND 1=1 AND ", p_wc2 
    
   LET g_sql = g_sql, cl_sql_add_filter("xcau_t"),
                      " ORDER BY xcau_t.xcau001"
   
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE axci111_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR axci111_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_xcau_d.clear()
   CALL g_xcau2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_xcau_d[l_ac].xcaustus,g_xcau_d[l_ac].xcau001,g_xcau_d[l_ac].xcau002,g_xcau_d[l_ac].xcau003, 
       g_xcau2_d[l_ac].xcau001,g_xcau2_d[l_ac].xcauownid,g_xcau2_d[l_ac].xcauowndp,g_xcau2_d[l_ac].xcaucrtid, 
       g_xcau2_d[l_ac].xcaucrtdp,g_xcau2_d[l_ac].xcaucrtdt,g_xcau2_d[l_ac].xcaumodid,g_xcau2_d[l_ac].xcaumoddt, 
       g_xcau_d[l_ac].xcau002_desc,g_xcau2_d[l_ac].xcauownid_desc,g_xcau2_d[l_ac].xcauowndp_desc,g_xcau2_d[l_ac].xcaucrtid_desc, 
       g_xcau2_d[l_ac].xcaucrtdp_desc,g_xcau2_d[l_ac].xcaumodid_desc
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL axci111_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec THEN
      IF g_error_show = 1 THEN
         CALL cl_err("xcau_t",9035,1)
      END IF
   END IF 
   LET g_error_show = 0
   
 
   
   CALL g_xcau_d.deleteElement(g_xcau_d.getLength())   
   CALL g_xcau2_d.deleteElement(g_xcau2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_xcau_d.getLength()
      LET g_xcau2_d[l_ac].xcau001 = g_xcau_d[l_ac].xcau001 
 
   END FOR
   
   IF g_cnt > g_xcau_d.getLength() THEN
      LET l_ac = g_xcau_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_xcau_d.getLength()
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE axci111_pb
   
END FUNCTION
]]>
  </section>
  <section id="axci111.default_search" ver="3" status="" src="s">
    <![CDATA[#+ 外部參數搜尋
PRIVATE FUNCTION axci111_default_search()
   DEFINE li_idx  LIKE type_t.num5
   DEFINE li_cnt  LIKE type_t.num5
   DEFINE ls_wc   STRING
   #add-point:default_search段define
