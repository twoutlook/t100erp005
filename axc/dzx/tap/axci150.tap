                  #   LET g_errparam.code   = SQLCA.sqlcode 
                  #   LET g_errparam.popup  = TRUE 
                  #   CALL cl_err()
                  #   CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_xcbf_m.xcbfcomp
               LET gs_keys_bak[1] = g_xcbfcomp_t
               LET gs_keys[2] = g_xcbf_m.xcbf003
               LET gs_keys_bak[2] = g_xcbf003_t
               LET gs_keys[3] = g_xcbf_d[g_detail_idx].xcbf002
               LET gs_keys_bak[3] = g_xcbf_d_t.xcbf002
               CALL axci150_update_b('xcbf_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
                     #修改歷程記錄(單身修改)
                     LET g_log1 = util.JSON.stringify(g_xcbf_m),util.JSON.stringify(g_xcbf_d_t)
                     LET g_log2 = util.JSON.stringify(g_xcbf_m),util.JSON.stringify(g_xcbf_d[l_ac])
                     IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                        CALL s_transaction_end('N','0')
                     END IF
                     
               END CASE
               
               #將遮罩欄位進行遮蔽
               CALL axci150_xcbf_t_mask_restore('restore_mask_n')
               
               #若Key欄位有變動
               LET ls_keys[01] = g_xcbf_m.xcbfcomp
               LET ls_keys[ls_keys.getLength()+1] = g_xcbf_m.xcbf003
 
               LET ls_keys[ls_keys.getLength()+1] = g_xcbf_d_t.xcbf002
 
               CALL axci150_key_update_b(ls_keys)
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
            END IF
 
         AFTER ROW
            #add-point:input段after row
            {<point name="input.body.after_row"/>}
            #end add-point  
            LET l_ac = ARR_CURR()
            LET l_ac_t = l_ac
