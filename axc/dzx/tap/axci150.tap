      CALL axci150_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM xcbf_t WHERE xcbfent = g_enterprise AND xcbfcomp = g_xcbf_m.xcbfcomp
                                                               AND xcbf003 = g_xcbf_m.xcbf003
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "xcbf_t" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
      END IF
 
      INITIALIZE l_var_keys_bak TO NULL 
   INITIALIZE l_field_keys   TO NULL 
   LET l_var_keys_bak[01] = g_enterprise
   LET l_field_keys[01] = 'xcbflent'
   LET l_var_keys_bak[02] = g_master_multi_table_t.xcbflcomp
   LET l_field_keys[02] = 'xcbflcomp'
   LET l_var_keys_bak[03] = g_master_multi_table_t.xcbfl001
   LET l_field_keys[03] = 'xcbfl001'
   CALL cl_multitable_delete(l_field_keys,l_var_keys_bak,'xcbfl_t')
 
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      #單頭多語言刪除
      INITIALIZE l_var_keys_bak TO NULL 
   INITIALIZE l_field_keys   TO NULL 
   LET l_var_keys_bak[01] = g_enterprise
   LET l_field_keys[01] = 'xcbflent'
   LET l_var_keys_bak[02] = g_master_multi_table_t.xcbflcomp
   LET l_field_keys[02] = 'xcbflcomp'
   LET l_var_keys_bak[03] = g_master_multi_table_t.xcbfl001
   LET l_field_keys[03] = 'xcbfl001'
   CALL cl_multitable_delete(l_field_keys,l_var_keys_bak,'xcbfl_t')
 
      
      #單身多語言刪除
      
 
 
   
      #add-point:多語言刪除
      {<point name="delete.lang.delete"/>}
      #end add-point
      
      #頭先不紀錄
      #IF NOT cl_log_modified_record('','') THEN 
      #   CLOSE axci150_cl
      #   CALL s_transaction_end('N','0')
      #   RETURN 
      #END IF
      
      CLEAR FORM
      CALL g_xcbf_d.clear() 
      CALL g_xcbf2_d.clear()       
 
     
      CALL axci150_ui_browser_refresh()  
      #CALL axci150_ui_headershow()  
      #CALL axci150_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL axci150_fetch('P')
      ELSE
         #LET g_wc = " 1=1"
         #LET g_wc2 = " 1=1"
         #CALL axci150_browser_fill("F")
