  <section id="axci150.input" ver="12" status="" src="s" readonly="">
    <![CDATA[#+ 資料輸入
PRIVATE FUNCTION axci150_input(p_cmd)
   DEFINE  p_cmd                 LIKE type_t.chr1
   DEFINE  l_cmd_t               LIKE type_t.chr1
   DEFINE  l_cmd                 LIKE type_t.chr1
   DEFINE  l_ac_t                LIKE type_t.num10               #未取消的ARRAY CNT 
   DEFINE  l_n                   LIKE type_t.num10               #檢查重複用  
   DEFINE  l_cnt                 LIKE type_t.num10               #檢查重複用  
   DEFINE  l_lock_sw             LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert        LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete        LIKE type_t.num5                #可刪除否  
   DEFINE  l_count               LIKE type_t.num10
   DEFINE  l_i                   LIKE type_t.num10
   DEFINE  l_insert              BOOLEAN
   DEFINE  ls_return             STRING
   DEFINE  l_var_keys            DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys          DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                DYNAMIC ARRAY OF STRING
   DEFINE  l_fields              DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak        DYNAMIC ARRAY OF STRING
   DEFINE  lb_reproduce          BOOLEAN
   DEFINE  li_reproduce          LIKE type_t.num10
   DEFINE  li_reproduce_target   LIKE type_t.num10
   DEFINE  ls_keys               DYNAMIC ARRAY OF VARCHAR(500)
   #add-point:input段define
   {<point name="input.define" edit="s"/>}
   #end add-point
   #add-point:input段define
   {<point name="input.define_customerization" edit="c"/>}
   #end add-point   
   
   #先做狀態判定
   IF p_cmd = 'r' THEN
      LET l_cmd_t = 'r'
      LET p_cmd   = 'a'
   ELSE
      LET l_cmd_t = p_cmd
   END IF   
   
   #將資料輸出到畫面上
   DISPLAY BY NAME g_xcbf_m.xcbfcomp,g_xcbf_m.xcbfcomp_desc,g_xcbf_m.xcbf003,g_xcbf_m.xcbf004,g_xcbf_m.xcbf001, 
       g_xcbf_m.xcbfl003
   
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF  
 
   CALL cl_set_head_visible("","YES")  
 
   LET l_insert = FALSE
   LET g_action_choice = ""
 
   #add-point:input段define_sql
   {<point name="input.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT xcbfstus,xcbf002,xcbf002,xcbfownid,xcbfowndp,xcbfcrtid,xcbfcrtdp,xcbfcrtdt, 
       xcbfmodid,xcbfmoddt FROM xcbf_t WHERE xcbfent=? AND xcbfcomp=? AND xcbf003=? AND xcbf002=? FOR  
       UPDATE"
   #add-point:input段define_sql
   {<point name="input.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE axci150_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR
 
 
   
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   LET g_qryparam.state = 'i'
   
   #控制key欄位可否輸入
   CALL axci150_set_entry(p_cmd)
   #add-point:set_entry後
   {<point name="input.after_set_entry"/>}
   #end add-point
   CALL axci150_set_no_entry(p_cmd)
   #add-point:set_no_entry後
   {<point name="input.after_set_no_entry"/>}
   #end add-point
 
   DISPLAY BY NAME g_xcbf_m.xcbfcomp,g_xcbf_m.xcbf003,g_xcbf_m.xcbf004,g_xcbf_m.xcbf001,g_xcbf_m.xcbfl003 

   
   LET lb_reproduce = FALSE
   
   #關閉被遮罩相關欄位輸入, 無法確定USER是否會需要輸入此欄位
   #因此先行關閉, 若有需要可於下方add-point中自行開啟
   CALL cl_mask_set_no_entry()
   
   #add-point:進入修改段前
   {<point name="input.before_input"/>}
   #end add-point
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
]]>
  </section>
  <section id="axci150.input.body" ver="19" status="" src="s" readonly="">
    <![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_xcbf_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,WITHOUT DEFAULTS, #MAXCOUNT = g_max_rec,
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂單身ACTION
         
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_xcbf_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL axci150_b_fill(g_wc2) #test 
            #如果一直都在單頭則控制筆數位置
            IF g_loc = 'm' THEN
               CALL FGL_SET_ARR_CURR(g_detail_idx)
            END IF
            LET g_loc = 'm' 
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            #add-point:modify段before row
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            LET g_curr_diag = ui.DIALOG.getCurrent()
            CALL axci150_idx_chk()
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN axci150_cl USING g_enterprise,g_xcbf_m.xcbfcomp,g_xcbf_m.xcbf003                          
               IF STATUS THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "OPEN axci150_cl:" 
                  LET g_errparam.code   =  STATUS 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  CLOSE axci150_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            IF g_rec_b >= l_ac 
               AND g_xcbf_d[l_ac].xcbf002 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_xcbf_d_t.* = g_xcbf_d[l_ac].*  #BACKUP
               LET g_xcbf_d_o.* = g_xcbf_d[l_ac].*  #BACKUP
               CALL axci150_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL axci150_set_no_entry_b(l_cmd)
               OPEN axci150_bcl USING g_enterprise,g_xcbf_m.xcbfcomp,
                                                g_xcbf_m.xcbf003,
 
                                                g_xcbf_d_t.xcbf002
 
               IF STATUS THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "OPEN axci150_bcl:" 
                  LET g_errparam.code   =  STATUS 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  LET l_lock_sw='Y'
               ELSE
                  FETCH axci150_bcl INTO g_xcbf_d[l_ac].xcbfstus,g_xcbf_d[l_ac].xcbf002,g_xcbf2_d[l_ac].xcbf002, 
                      g_xcbf2_d[l_ac].xcbfownid,g_xcbf2_d[l_ac].xcbfowndp,g_xcbf2_d[l_ac].xcbfcrtid, 
                      g_xcbf2_d[l_ac].xcbfcrtdp,g_xcbf2_d[l_ac].xcbfcrtdt,g_xcbf2_d[l_ac].xcbfmodid, 
                      g_xcbf2_d[l_ac].xcbfmoddt
                  IF SQLCA.sqlcode THEN
                      INITIALIZE g_errparam TO NULL 
                      LET g_errparam.extend = g_xcbf_d_t.xcbf002 
                      LET g_errparam.code   = SQLCA.sqlcode 
                      LET g_errparam.popup  = TRUE 
                      CALL cl_err()
                      LET l_lock_sw = "Y"
                  END IF
                  
                  #遮罩相關處理
                  LET g_xcbf_d_mask_o[l_ac].* =  g_xcbf_d[l_ac].*
                  CALL axci150_xcbf_t_mask()
                  LET g_xcbf_d_mask_n[l_ac].* =  g_xcbf_d[l_ac].*
                  
                  CALL axci150_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
        
         BEFORE INSERT
            
            INITIALIZE g_xcbf_d_t.* TO NULL
            INITIALIZE g_xcbf_d_o.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_xcbf_d[l_ac].* TO NULL
            #公用欄位預設值
            #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_xcbf2_d[l_ac].xcbfownid = g_user
      LET g_xcbf2_d[l_ac].xcbfowndp = g_dept
      LET g_xcbf2_d[l_ac].xcbfcrtid = g_user
      LET g_xcbf2_d[l_ac].xcbfcrtdp = g_dept 
      LET g_xcbf2_d[l_ac].xcbfcrtdt = cl_get_current()
      LET g_xcbf2_d[l_ac].xcbfmodid = g_user
      LET g_xcbf2_d[l_ac].xcbfmoddt = cl_get_current()
      LET g_xcbf_d[l_ac].xcbfstus = ''
 
  
            #一般欄位預設值
                  LET g_xcbf_d[l_ac].xcbfstus = "Y"
 
            
            #add-point:modify段before備份
            {<point name="input.body.before_insert.before_bak"/>}
            #end add-point
            LET g_xcbf_d_t.* = g_xcbf_d[l_ac].*     #新輸入資料
            LET g_xcbf_d_o.* = g_xcbf_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL axci150_set_entry_b(l_cmd)
            #add-point:set_entry_b後
            {<point name="input.body.before_insert.set_entry_b"/>}
            #end add-point
            CALL axci150_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_xcbf_d[li_reproduce_target].* = g_xcbf_d[li_reproduce].*
               LET g_xcbf2_d[li_reproduce_target].* = g_xcbf2_d[li_reproduce].*
 
               LET g_xcbf_d[g_xcbf_d.getLength()].xcbf002 = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
 
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM xcbf_t 
             WHERE xcbfent = g_enterprise AND xcbfcomp = g_xcbf_m.xcbfcomp
               AND xcbf003 = g_xcbf_m.xcbf003
 
               AND xcbf002 = g_xcbf_d[l_ac].xcbf002
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               CALL s_transaction_begin()
               #add-point:單身新增前
               {<point name="input.body.b_insert" mark="Y"/>}
               #end add-point
               INSERT INTO xcbf_t
                           (xcbfent,
                            xcbfcomp,xcbf003,xcbf004,xcbf001,
                            xcbf002
                            ,xcbfstus,xcbfownid,xcbfowndp,xcbfcrtid,xcbfcrtdp,xcbfcrtdt,xcbfmodid,xcbfmoddt) 
                     VALUES(g_enterprise,
                            g_xcbf_m.xcbfcomp,g_xcbf_m.xcbf003,g_xcbf_m.xcbf004,g_xcbf_m.xcbf001,
                            g_xcbf_d[l_ac].xcbf002
                            ,g_xcbf_d[l_ac].xcbfstus,g_xcbf2_d[l_ac].xcbfownid,g_xcbf2_d[l_ac].xcbfowndp, 
                                g_xcbf2_d[l_ac].xcbfcrtid,g_xcbf2_d[l_ac].xcbfcrtdp,g_xcbf2_d[l_ac].xcbfcrtdt, 
                                g_xcbf2_d[l_ac].xcbfmodid,g_xcbf2_d[l_ac].xcbfmoddt)
               #add-point:單身新增中
               {<point name="input.body.m_insert"/>}
               #end add-point
               LET p_cmd = 'u'
               LET g_master_insert = TRUE
            ELSE    
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = 'INSERT' 
               LET g_errparam.code   = "std-00006" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               INITIALIZE g_xcbf_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "xcbf_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               #ERROR 'INSERT O.K'
               LET g_rec_b=g_rec_b+1
               DISPLAY g_rec_b TO FORMONLY.cnt
            END IF
            
            #add-point:單身新增後
            {<point name="input.body.after_insert"/>}
            #end add-point
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' THEN
               LET l_cmd='d'
            ELSE
               #add-point:單身刪除前
               {<point name="input.body.b_delete"/>}
               #end add-point
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "" 
                  LET g_errparam.code   =  -263 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  CANCEL DELETE
               END IF
               IF axci150_before_delete() THEN 
                  
                  CALL s_transaction_end('Y','0')
               ELSE 
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               END IF 
               CLOSE axci150_bcl
               LET l_count = g_xcbf_d.getLength()
            END IF 
            
            #add-point:單身刪除後
            {<point name="input.body.a_delete"/>}
            #end add-point
              
         AFTER DELETE 
            IF l_cmd <> 'd' THEN
               #add-point:單身AFTER DELETE 
               {<point name="input.body.after_delete"/>}
               #end add-point
            END IF
            #如果是最後一筆
            IF l_ac = (g_xcbf_d.getLength() + 1) THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
            END IF
            
                  #應用 a01 樣板自動產生(Version:1)
         BEFORE FIELD xcbfstus
            #add-point:BEFORE FIELD xcbfstus
            {<point name="input.b.page1.xcbfstus" />}
            #END add-point
 
         #應用 a02 樣板自動產生(Version:1)
         AFTER FIELD xcbfstus
            
            #add-point:AFTER FIELD xcbfstus
            {<point name="input.a.page1.xcbfstus" />}
            #END add-point
            
 
         #應用 a04 樣板自動產生(Version:2)
         ON CHANGE xcbfstus
            #add-point:ON CHANGE xcbfstus
            {<point name="input.g.page1.xcbfstus" />}
            #END add-point 
 
         #應用 a02 樣板自動產生(Version:1)
         AFTER FIELD xcbf002
            
            #add-point:AFTER FIELD xcbf002
            {<point name="input.a.page1.xcbf002" />}
            #END add-point
            
 
         #應用 a01 樣板自動產生(Version:1)
         BEFORE FIELD xcbf002
            #add-point:BEFORE FIELD xcbf002
            {<point name="input.b.page1.xcbf002" />}
            #END add-point
 
