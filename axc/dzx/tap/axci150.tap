  <section id="axci150.modify" ver="15" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION axci150_modify()
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point  
   #add-point:modify段define
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point    
   
   IF g_xcbf_m.xcbfcomp IS NULL
   OR g_xcbf_m.xcbf003 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
   LET g_xcbfcomp_t = g_xcbf_m.xcbfcomp
   LET g_xcbf003_t = g_xcbf_m.xcbf003
 
   CALL s_transaction_begin()
   
   OPEN axci150_cl USING g_enterprise,g_xcbf_m.xcbfcomp,g_xcbf_m.xcbf003
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN axci150_cl:" 
      LET g_errparam.code   =  STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE axci150_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE axci150_master_referesh USING g_xcbf_m.xcbfcomp,g_xcbf_m.xcbf003 INTO g_xcbf_m.xcbfcomp,g_xcbf_m.xcbf003, 
       g_xcbf_m.xcbf004,g_xcbf_m.xcbf001,g_xcbf_m.xcbfcomp_desc
   
   #遮罩相關處理
   LET g_xcbf_m_mask_o.* =  g_xcbf_m.*
   CALL axci150_xcbf_t_mask()
   LET g_xcbf_m_mask_n.* =  g_xcbf_m.*
   
   CALL s_transaction_end('Y','0')
 
   CALL axci150_show()
   WHILE TRUE
      LET g_xcbfcomp_t = g_xcbf_m.xcbfcomp
      LET g_xcbf003_t = g_xcbf_m.xcbf003
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL axci150_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_xcbf_m.* = g_xcbf_m_t.*
         CALL axci150_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更(更新單身table的key欄位值)
      IF g_xcbf_m.xcbfcomp != g_xcbfcomp_t 
      OR g_xcbf_m.xcbf003 != g_xcbf003_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("modify,modify_detail,delete,reproduce", TRUE)
   CALL axci150_set_act_visible()
   CALL axci150_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " xcbfent = '" ||g_enterprise|| "' AND",
                      " xcbfcomp = '", g_xcbf_m.xcbfcomp, "' "
                      ," AND xcbf003 = '", g_xcbf_m.xcbf003, "' "
 
   #填到對應位置
   CALL axci150_browser_fill("")
 
   CALL axci150_idx_chk()
 
   CLOSE axci150_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL axci150_msgcentre_notify('')
   
END FUNCTION   
]]>
  </section>
