            CLOSE axci150_bcl
            CALL s_transaction_end('Y','0')
 
         AFTER INPUT
            #若單身還沒有輸入資料, 強制切換至單身
            IF g_xcbf_d.getLength() = 0 THEN
               NEXT FIELD xcbf002
