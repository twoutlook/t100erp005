  <point name="input.a.page1.xcbf002" order="" ver="7" cite_std="N" new="N" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[            #此段落由子樣板a05產生
            #mark 150527-00044#1  wangxina20150529
            #IF  g_xcbf_m.xcbfcomp IS NOT NULL AND g_xcbf_m.xcbf001 IS NOT NULL AND g_xcbf_d[g_detail_idx].xcbf002 IS NOT NULL THEN 
               #IF l_cmd = 'a' OR ( l_cmd = 'u' AND (g_xcbf_m.xcbfcomp != g_xcbfcomp_t OR g_xcbf_m.xcbf001 != g_xcbf001_t OR g_xcbf_d[g_detail_idx].xcbf002 != g_xcbf_d_t.xcbf002)) THEN   
                   #IF NOT ap_chk_notDup("","SELECT COUNT(*) FROM xcbf_t WHERE "||"xcbfent = '" ||g_enterprise|| "' AND "||"xcbfcomp = '"||g_xcbf_m.xcbfcomp ||"' AND "|| "xcbf001 = '"||g_xcbf_m.xcbf001 ||"' AND "|| "xcbf002 = '"||g_xcbf_d[g_detail_idx].xcbf002 ||"'",'std-00004',0) THEN 
            #add  150527-00044#1  wangxina20150529
            IF  g_xcbf_m.xcbfcomp IS NOT NULL AND g_xcbf_m.xcbf003 IS NOT NULL AND g_xcbf_d[g_detail_idx].xcbf002 IS NOT NULL THEN 
               IF l_cmd = 'a' OR ( l_cmd = 'u' AND (g_xcbf_m.xcbfcomp != g_xcbfcomp_t OR g_xcbf_m.xcbf003 != g_xcbf003_t OR g_xcbf_d[g_detail_idx].xcbf002 != g_xcbf_d_t.xcbf002)) THEN    
                  IF NOT ap_chk_notDup("","SELECT COUNT(*) FROM xcbf_t WHERE "||"xcbfent = '" ||g_enterprise|| "' AND "||"xcbfcomp = '"||g_xcbf_m.xcbfcomp ||"' AND "|| "xcbf003 = '"||g_xcbf_m.xcbf003 ||"' AND "|| "xcbf002 = '"||g_xcbf_d[g_detail_idx].xcbf002 ||"'",'std-00004',0) THEN 
                     NEXT FIELD CURRENT
                  END IF
               END IF
            END IF
            IF  NOT cl_null(g_xcbf_d[g_detail_idx].xcbf002) THEN
                #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
               INITIALIZE g_chkparam.* TO NULL

               IF g_xcbf_m.xcbf003 = '1' THEN
                  #設定g_chkparam.*的參數
                  LET g_chkparam.arg1 = g_xcbf_d[g_detail_idx].xcbf002
                  LET g_chkparam.arg2 = g_xcbf_m.xcbfcomp
			   
                  #呼叫檢查存在並帶值的library
                  IF cl_chk_exist("v_ooef001_9") THEN
			   
                  ELSE
                     #檢查失敗時後續處理
                     LET g_xcbf_d[g_detail_idx].xcbf002 = g_xcbf_d_t.xcbf002
                     LET g_chkparam.arg1 = g_xcbf_d[g_detail_idx].xcbf002
                     CALL cl_ref_val("v_ooef001_10")
                     LET g_xcbf_d[g_detail_idx].xcbf002_desc = g_chkparam.return1          {#ADP版次:1#}
                     NEXT FIELD xcbf002
                  END IF
                  LET g_chkparam.arg1 = g_xcbf_d[g_detail_idx].xcbf002
                  CALL cl_ref_val("v_ooef001_10")
                  LET g_xcbf_d[g_detail_idx].xcbf002_desc = g_chkparam.return1          {#ADP版次:1#}
               ELSE
                  LET l_n = 0
                  SELECT COUNT(*) INTO l_n FROM inaa_t
                   WHERE inaaent = g_enterprise AND inaa001 = g_xcbf_d[g_detail_idx].xcbf002
                  IF l_n = 0 THEN
                     #檢查失敗時後續處理
                     INITIALIZE g_errparam TO NULL
