  <section id="axci150.detail_reproduce" ver="8" status="" src="s" readonly="">
    <![CDATA[#+ 單身自動複製
PRIVATE FUNCTION axci150_detail_reproduce()
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE xcbf_t.*
 
 
   #add-point:delete段define
   {<point name="detail_reproduce.define" edit="s"/>}
   #end add-point    
   #add-point:delete段define
   {<point name="detail_reproduce.define_customerization" edit="c"/>}
   #end add-point 
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE axci150_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE axci150_detail AS ",
                "SELECT * FROM xcbf_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO axci150_detail SELECT * FROM xcbf_t 
                                         WHERE xcbfent = g_enterprise AND xcbfcomp = g_xcbfcomp_t
                                         AND xcbf003 = g_xcbf003_t
 
   
   #將key修正為調整後   
   UPDATE axci150_detail 
      #更新key欄位
      SET xcbfcomp = g_xcbf_m.xcbfcomp
          , xcbf003 = g_xcbf_m.xcbf003
 
      #更新共用欄位
      #應用 a13 樣板自動產生(Version:3)
       , xcbfownid = g_user 
       , xcbfowndp = g_dept
       , xcbfcrtid = g_user
       , xcbfcrtdp = g_dept 
       , xcbfcrtdt = ld_date
       , xcbfmodid = g_user
       , xcbfmoddt = ld_date
      #, xcbfstus = "Y" 
 
 
                                       
   #將資料塞回原table   
   INSERT INTO xcbf_t SELECT * FROM axci150_detail
   
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "reproduce" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE axci150_detail
   
   #add-point:單身複製後1
   {<point name="detail_reproduce.body.table1.a_insert"/>}
   #end add-point
 
 
   
 
   
   #多語言複製段落
   
   
   CALL s_transaction_end('Y','0')
   
   #已新增完, 調整資料內容(修改時使用)
   LET g_xcbfcomp_t = g_xcbf_m.xcbfcomp
   LET g_xcbf003_t = g_xcbf_m.xcbf003
 
   
   DROP TABLE axci150_detail
   
END FUNCTION
]]>
