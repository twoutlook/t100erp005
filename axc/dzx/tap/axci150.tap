               UPDATE xcbf_t SET (xcbfcomp,xcbf003,xcbfstus,xcbf002,xcbfownid,xcbfowndp,xcbfcrtid,xcbfcrtdp, 
                   xcbfcrtdt,xcbfmodid,xcbfmoddt) = (g_xcbf_m.xcbfcomp,g_xcbf_m.xcbf003,g_xcbf_d[l_ac].xcbfstus, 
