                   g_xcbf2_d[l_ac].xcbfcrtdp,g_xcbf2_d[l_ac].xcbfcrtdt,g_xcbf2_d[l_ac].xcbfmodid,g_xcbf2_d[l_ac].xcbfmoddt) 

                WHERE xcbfent = g_enterprise AND xcbfcomp = g_xcbf_m.xcbfcomp 
