  <section id="axci001.modify" ver="1" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION axci001_modify()
   {<Local define>}
   DEFINE  l_cmd                  LIKE type_t.chr1
   DEFINE  l_ac_t                 LIKE type_t.num5                #未取消的ARRAY CNT 
   DEFINE  l_n                    LIKE type_t.num5                #檢查重複用  
   DEFINE  l_cnt                  LIKE type_t.num5                #檢查重複用  
   DEFINE  l_lock_sw              LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert         LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete         LIKE type_t.num5                #可刪除否  
   DEFINE  l_count                LIKE type_t.num5
   DEFINE  l_i                    LIKE type_t.num5
   DEFINE  ls_return              STRING
   DEFINE  l_var_keys             DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys           DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                 DYNAMIC ARRAY OF STRING
   DEFINE  l_fields               DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak         DYNAMIC ARRAY OF STRING
   DEFINE  li_reproduce           LIKE type_t.num5
   DEFINE  li_reproduce_target    LIKE type_t.num5
   DEFINE  lb_reproduce           BOOLEAN
   {</Local define>}
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point 
   
   LET g_action_choice = ""
   
   LET g_qryparam.state = "i"
 
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
 
   #add-point:modify段define_sql
   {<point name="modify.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT xcaastus,xcaa001,'',xcaa002,xcaa003,'',xcaamodid,'',xcaamoddt,xcaaownid, 
       '',xcaaowndp,'',xcaacrtid,'',xcaacrtdp,'',xcaacrtdt FROM xcaa_t WHERE xcaaent=? AND xcaa001=?  
       FOR UPDATE"
   #add-point:modify段define_sql
   {<point name="modify.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   DECLARE axci001_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR
   
 
   
   LET INT_FLAG = FALSE
   LET lb_reproduce = FALSE
 
   #add-point:modify段修改前
   {<point name="modify.before_input"/>}
   #end add-point
 
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #Page1 預設值產生於此處
      INPUT ARRAY g_xcaa_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_detail_cnt,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
 
 
         ON ACTION update_item
            #add-point:ON ACTION update_item
            {<point name="input.detail_input.page1.update_item" />}
            #END add-point
 
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_xcaa_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL axci001_b_fill(g_wc2)
            LET g_detail_cnt = g_xcaa_d.getLength()
         
         BEFORE ROW
            #add-point:modify段before row
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            DISPLAY g_xcaa_d.getLength() TO FORMONLY.cnt
         
            CALL s_transaction_begin()
            LET g_detail_cnt = g_xcaa_d.getLength()
            
            IF g_detail_cnt >= l_ac 
               AND g_xcaa_d[l_ac].xcaa001 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_xcaa_d_t.* = g_xcaa_d[l_ac].*  #BACKUP
               IF NOT axci001_lock_b("xcaa_t") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH axci001_bcl INTO g_xcaa_d[l_ac].xcaastus,g_xcaa_d[l_ac].xcaa001,g_xcaa_d[l_ac].xcaal003, 
                      g_xcaa_d[l_ac].xcaa002,g_xcaa_d[l_ac].xcaa003,g_xcaa2_d[l_ac].xcaa001,g_xcaa2_d[l_ac].xcaamodid, 
                      g_xcaa2_d[l_ac].xcaaownid_desc,g_xcaa2_d[l_ac].xcaamoddt,g_xcaa2_d[l_ac].xcaaownid, 
                      g_xcaa2_d[l_ac].xcaamodid_desc,g_xcaa2_d[l_ac].xcaaowndp,g_xcaa2_d[l_ac].xcaaowndp_desc, 
                      g_xcaa2_d[l_ac].xcaacrtid,g_xcaa2_d[l_ac].xcaacrtid_desc,g_xcaa2_d[l_ac].xcaacrtdp, 
                      g_xcaa2_d[l_ac].xcaacrtdp_desc,g_xcaa2_d[l_ac].xcaacrtdt
                  IF SQLCA.sqlcode THEN
                     CALL cl_err(g_xcaa_d_t.xcaa001,SQLCA.sqlcode,1)
                     LET l_lock_sw = "Y"
                  END IF
                  
                  CALL axci001_detail_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            LET g_detail_multi_table_t.xcaal001 = g_xcaa_d[l_ac].xcaa001
LET g_detail_multi_table_t.xcaal002 = g_dlang
LET g_detail_multi_table_t.xcaal003 = g_xcaa_d[l_ac].xcaal003
 
            #其他table進行lock
                        INITIALIZE l_var_keys TO NULL 
            INITIALIZE l_field_keys TO NULL 
            LET l_field_keys[01] = 'xcaal001'
            LET l_var_keys[01] = g_xcaa_d[l_ac].xcaa001
            LET l_field_keys[02] = 'xcaal002'
            LET l_var_keys[02] = g_dlang
            IF NOT cl_multitable_lock(l_var_keys,l_field_keys,'xcaal_t') THEN
               RETURN 
            END IF 
 
        
         BEFORE INSERT
            
            CALL s_transaction_begin()
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_xcaa_d_t.* TO NULL
            INITIALIZE g_xcaa_d[l_ac].* TO NULL 
                  LET g_xcaa_d[l_ac].xcaastus = "Y"
      LET g_xcaa_d[l_ac].xcaa002 = "N"
      LET g_xcaa_d[l_ac].xcaa003 = "Y"
 
 
            LET g_xcaa_d_t.* = g_xcaa_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL axci001_set_entry_b("a")
            CALL axci001_set_no_entry_b("a")
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_xcaa_d[li_reproduce_target].* = g_xcaa_d[li_reproduce].*
               LET g_xcaa2_d[li_reproduce_target].* = g_xcaa2_d[li_reproduce].*
 
               LET g_xcaa_d[g_xcaa_d.getLength()].xcaa001 = NULL
 
            END IF
            #公用欄位給值(單身)
            #此段落由子樣板a14產生    
      LET g_xcaa2_d[l_ac].xcaaownid = g_user
      LET g_xcaa2_d[l_ac].xcaaowndp = g_dept
      LET g_xcaa2_d[l_ac].xcaacrtid = g_user
      LET g_xcaa2_d[l_ac].xcaacrtdp = g_dept 
      LET g_xcaa2_d[l_ac].xcaacrtdt = cl_get_current()
      LET g_xcaa2_d[l_ac].xcaamodid = ""
      LET g_xcaa2_d[l_ac].xcaamoddt = ""
      #LET g_xcaa_d[l_ac].xcaastus = ""
 
 
            LET g_detail_multi_table_t.xcaal001 = g_xcaa_d[l_ac].xcaa001
LET g_detail_multi_table_t.xcaal002 = g_dlang
LET g_detail_multi_table_t.xcaal003 = g_xcaa_d[l_ac].xcaal003
 
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM xcaa_t 
             WHERE xcaaent = g_enterprise AND xcaa001 = g_xcaa_d[l_ac].xcaa001
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前
               {<point name="input.body.b_insert"/>}
               #end add-point
            
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_xcaa_d[g_detail_idx].xcaa001
               CALL axci001_insert_b('xcaa_t',gs_keys,"'1'")
                           
               #add-point:單身新增後
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_xcaa_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("xcaa_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL axci001_b_fill(g_wc2)
               #資料多語言用-增/改
                        INITIALIZE l_var_keys TO NULL 
         INITIALIZE l_field_keys TO NULL 
         INITIALIZE l_vars TO NULL 
         INITIALIZE l_fields TO NULL 
         INITIALIZE l_var_keys_bak TO NULL 
         IF g_xcaa_d[l_ac].xcaa001 = g_detail_multi_table_t.xcaal001 AND
