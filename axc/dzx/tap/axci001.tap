  <section id="axci001.b_fill" ver="1" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION axci001_b_fill(p_wc2)              #BODY FILL UP
   {<Local define>}
   DEFINE p_wc2           STRING
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE xcaastus,xcaa001,'',xcaa002,xcaa003,'',xcaamodid,'',xcaamoddt,xcaaownid, 
       '',xcaaowndp,'',xcaacrtid,'',xcaacrtdp,'',xcaacrtdt FROM xcaa_t",
               " LEFT JOIN xcaal_t ON xcaa001 = xcaal001 AND xcaal002 = '",g_lang,"'",
               " WHERE xcaaent= ? AND 1=1 AND ", p_wc2
    
   LET g_sql = g_sql, " ORDER BY xcaa_t.xcaa001"
  
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
  
   PREPARE axci001_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR axci001_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_xcaa_d.clear()
   CALL g_xcaa2_d.clear()   
 
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_xcaa_d[l_ac].xcaastus,g_xcaa_d[l_ac].xcaa001,g_xcaa_d[l_ac].xcaal003,g_xcaa_d[l_ac].xcaa002, 
       g_xcaa_d[l_ac].xcaa003,g_xcaa2_d[l_ac].xcaa001,g_xcaa2_d[l_ac].xcaamodid,g_xcaa2_d[l_ac].xcaaownid_desc, 
       g_xcaa2_d[l_ac].xcaamoddt,g_xcaa2_d[l_ac].xcaaownid,g_xcaa2_d[l_ac].xcaamodid_desc,g_xcaa2_d[l_ac].xcaaowndp, 
       g_xcaa2_d[l_ac].xcaaowndp_desc,g_xcaa2_d[l_ac].xcaacrtid,g_xcaa2_d[l_ac].xcaacrtid_desc,g_xcaa2_d[l_ac].xcaacrtdp, 
       g_xcaa2_d[l_ac].xcaacrtdp_desc,g_xcaa2_d[l_ac].xcaacrtdt
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL axci001_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec AND g_error_show = 1 THEN
      CALL cl_err("xcaa_t",9035,1)
   END IF 
   LET g_error_show = 0
   
 
  
   
   CALL g_xcaa_d.deleteElement(g_xcaa_d.getLength())   
   CALL g_xcaa2_d.deleteElement(g_xcaa2_d.getLength())
 
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_xcaa_d.getLength()
      LET g_xcaa2_d[l_ac].xcaa001 = g_xcaa_d[l_ac].xcaa001 
 
 
   END FOR
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = l_ac - 1
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE axci001_pb
   
END FUNCTION
]]>
  </section>
  <section id="axci001.default_search" ver="1" status="" src="s">
    <![CDATA[#+ 外部參數搜尋, 施工中
PRIVATE FUNCTION axci001_default_search()
   {<Local define>}
   DEFINE li_idx  LIKE type_t.num5
   DEFINE li_cnt  LIKE type_t.num5
   DEFINE ls_wc   STRING
   {</Local define>}
   #add-point:default_search段define
   {<point name="default_search.define"/>}
   #end add-point  
   
   IF NOT cl_null(g_argv[1]) THEN
      LET ls_wc = ls_wc, " xcaa001 = '", g_argv[1], "' AND "
   END IF
   
 
   
   IF NOT cl_null(ls_wc) THEN
      LET ls_wc = ls_wc.subString(1,ls_wc.getLength()-5)
