  <section id="axci114.detail_reproduce" ver="2" status="" src="s">
<![CDATA[#+ 單身自動複製
PRIVATE FUNCTION axci114_detail_reproduce()
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE xcay_t.*
 
 
   #add-point:delete段define
   {<point name="detail_reproduce.define"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE axci114_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE axci114_detail AS ",
                "SELECT * FROM xcay_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO axci114_detail SELECT * FROM xcay_t 
                                         WHERE xcayent = g_enterprise AND xcayld = g_xcayld_t
 
   
   #將key修正為調整後   
   UPDATE axci114_detail 
      #更新key欄位
      SET xcayld = g_xcay_m.xcayld
 
      #更新共用欄位
      #此段落由子樣板a13產生
       , xcayownid = g_user
       , xcayowndp = g_dept
       , xcaycrtid = g_user
       , xcaycrtdp = g_dept 
       , xcaycrtdt = ld_date
       , xcaymodid = "" 
       , xcaymoddt = "" 
      #, xcaystus = "Y"
 
 
                                       
  
   #將資料塞回原table   
   INSERT INTO xcay_t SELECT * FROM axci114_detail
   
   IF SQLCA.sqlcode THEN
      CALL cl_err("reproduce",SQLCA.sqlcode,1)
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE axci114_detail
   
   #add-point:單身複製後1
   {<point name="detail_reproduce.body.table1.a_insert"/>}
   #end add-point
 
 
   
 
   
   #多語言複製段落
   
   
   CALL s_transaction_end('Y','0')
   
   #已新增完, 調整資料內容(修改時使用)
   LET g_xcayld_t = g_xcay_m.xcayld
 
   
   DROP TABLE axci114_detail
   
END FUNCTION
]]>
