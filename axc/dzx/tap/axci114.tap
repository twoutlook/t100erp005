  <section id="axci114.delete" ver="5" status="" src="s">
<![CDATA[#+ 資料刪除
PRIVATE FUNCTION axci114_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_xcay_m.xcayld IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE axci114_master_referesh USING g_xcay_m.xcayld INTO g_xcay_m.xcayld,g_xcay_m.xcayld_desc
   
   CALL axci114_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN axci114_cl USING g_enterprise,g_xcay_m.xcayld
 
   IF STATUS THEN
      CALL cl_err("OPEN axci114_cl:", STATUS, 1)
      CLOSE axci114_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH axci114_cl INTO g_xcay_m.xcayld,g_xcay_m.xcayld_desc
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_xcay_m.xcayld,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL axci114_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM xcay_t WHERE xcayent = g_enterprise AND xcayld = g_xcay_m.xcayld
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("xcay_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_xcay_d.clear() 
      CALL g_xcay2_d.clear()       
 
     
      CALL axci114_ui_browser_refresh()  
      CALL axci114_ui_headershow()  
      CALL axci114_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL axci114_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL axci114_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE axci114_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_xcay_m.xcayld,'D')
    
END FUNCTION
]]>
</section>
