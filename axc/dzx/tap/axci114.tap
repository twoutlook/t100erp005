   DEFINE  l_var_keys            DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys          DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                DYNAMIC ARRAY OF STRING
   DEFINE  l_fields              DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak        DYNAMIC ARRAY OF STRING
   DEFINE  lb_reproduce          BOOLEAN
   DEFINE  li_reproduce          LIKE type_t.num5
   DEFINE  li_reproduce_target   LIKE type_t.num5
   #add-point:input段define
   {<point name="input.define"/>}
   #end add-point    
   
   #先做狀態判定
   IF p_cmd = 'r' THEN
      LET l_cmd_t = 'r'
      LET p_cmd   = 'a'
   ELSE
      LET l_cmd_t = p_cmd
   END IF   
   
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF  
 
   CALL cl_set_head_visible("","YES")  
 
   LET l_insert = FALSE
   LET g_action_choice = ""
 
   #add-point:input段define_sql
   {<point name="input.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT xcaystus,xcay001,xcay002,xcay001,xcayownid,xcayowndp,xcaycrtid,xcaycrtdp, 
       xcaycrtdt,xcaymodid,xcaymoddt FROM xcay_t WHERE xcayent=? AND xcayld=? AND xcay001=? FOR UPDATE" 

   #add-point:input段define_sql
   {<point name="input.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE axci114_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR
 
 
   
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   LET g_qryparam.state = 'i'
   
   #控制key欄位可否輸入
   CALL axci114_set_entry(p_cmd)
   #add-point:set_entry後
   {<point name="input.after_set_entry"/>}
   #end add-point
   CALL axci114_set_no_entry(p_cmd)
   #add-point:set_no_entry後
   {<point name="input.after_set_no_entry"/>}
   #end add-point
 
   DISPLAY BY NAME g_xcay_m.xcayld
   
   LET lb_reproduce = FALSE
   
   #add-point:進入修改段前
   {<point name="input.before_input"/>}
   #end add-point
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
]]>
</section>
  <section id="axci114.input.body" ver="5" status="" src="s">
<![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_xcay_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂單身ACTION
         
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_xcay_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL axci114_b_fill(g_wc2) 
            IF g_rec_b != 0 THEN
               CALL fgl_set_arr_curr(l_ac)
            END IF
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN axci114_cl USING g_enterprise,
                                               g_xcay_m.xcayld
 
                                               
               IF STATUS THEN
                  CALL cl_err("OPEN axci114_cl:", STATUS, 1)
                  CLOSE axci114_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_xcay_d[l_ac].xcay001 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_xcay_d_t.* = g_xcay_d[l_ac].*  #BACKUP
               CALL axci114_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL axci114_set_no_entry_b(l_cmd)
               OPEN axci114_bcl USING g_enterprise,g_xcay_m.xcayld,
 
                                                g_xcay_d_t.xcay001
 
               IF STATUS THEN
                  CALL cl_err("OPEN axci114_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH axci114_bcl INTO g_xcay_d[l_ac].xcaystus,g_xcay_d[l_ac].xcay001,g_xcay_d[l_ac].xcay002 

                  IF SQLCA.sqlcode THEN
                      CALL cl_err(g_xcay_d_t.xcay001,SQLCA.sqlcode,1)
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL axci114_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
        
         BEFORE INSERT
            
            INITIALIZE g_xcay_d_t.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_xcay_d[l_ac].* TO NULL
            
            #公用欄位預設值
            #此段落由子樣板a14產生    
      LET g_xcay2_d[l_ac].xcayownid = g_user
      LET g_xcay2_d[l_ac].xcayowndp = g_dept
      LET g_xcay2_d[l_ac].xcaycrtid = g_user
      LET g_xcay2_d[l_ac].xcaycrtdp = g_dept 
      LET g_xcay2_d[l_ac].xcaycrtdt = cl_get_current()
      LET g_xcay2_d[l_ac].xcaymodid = ""
      LET g_xcay2_d[l_ac].xcaymoddt = ""
      LET g_xcay_d[l_ac].xcaystus = ""
 
  
            
            #一般欄位預設值
                  LET g_xcay_d[l_ac].xcaystus = "Y"
 
            
            
            LET g_xcay_d_t.* = g_xcay_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL axci114_set_entry_b(l_cmd)
            #add-point:set_entry_b後
            {<point name="input.body.before_insert.set_entry_b"/>}
            #end add-point
            CALL axci114_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_xcay_d[li_reproduce_target].* = g_xcay_d[li_reproduce].*
               LET g_xcay2_d[li_reproduce_target].* = g_xcay2_d[li_reproduce].*
 
               LET g_xcay_d[g_xcay_d.getLength()].xcay001 = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
 
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM xcay_t 
             WHERE xcayent = g_enterprise AND xcayld = g_xcay_m.xcayld
 
               AND xcay001 = g_xcay_d[l_ac].xcay001
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               CALL s_transaction_begin()
               #add-point:單身新增前
               {<point name="input.body.b_insert" mark="Y"/>}
               #end add-point
               INSERT INTO xcay_t
                           (xcayent,
                            xcayld,
                            xcay001
                            ,xcaystus,xcay002,xcayownid,xcayowndp,xcaycrtid,xcaycrtdp,xcaycrtdt,xcaymodid,xcaymoddt) 
                     VALUES(g_enterprise,
                            g_xcay_m.xcayld,
                            g_xcay_d[l_ac].xcay001
                            ,g_xcay_d[l_ac].xcaystus,g_xcay_d[l_ac].xcay002,g_xcay2_d[l_ac].xcayownid, 
                                g_xcay2_d[l_ac].xcayowndp,g_xcay2_d[l_ac].xcaycrtid,g_xcay2_d[l_ac].xcaycrtdp, 
                                g_xcay2_d[l_ac].xcaycrtdt,g_xcay2_d[l_ac].xcaymodid,g_xcay2_d[l_ac].xcaymoddt) 

               #add-point:單身新增中
               {<point name="input.body.m_insert"/>}
               #end add-point
               LET p_cmd = 'u'
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_xcay_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("xcay_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR "INSERT O.K"
               LET g_rec_b=g_rec_b+1
               DISPLAY g_rec_b TO FORMONLY.cnt
            END IF
            
            #add-point:單身新增後
            {<point name="input.body.after_insert"/>}
            #end add-point
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_xcay_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_xcay_d.deleteElement(l_ac)
               NEXT FIELD xcay001
            END IF
            IF g_xcay_d_t.xcay001 IS NOT NULL
 
               THEN
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete"/>}
               #end add-point
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               IF axci114_before_delete() THEN 
                  CALL s_transaction_end('Y','0')
               ELSE 
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               END IF 
               CLOSE axci114_bcl
               LET l_count = g_xcay_d.getLength()
            END IF 
            
            #add-point:單身刪除後
            {<point name="input.body.a_delete"/>}
            #end add-point
              
         AFTER DELETE 
            #add-point:單身AFTER DELETE 
            {<point name="input.body.after_delete"/>}
            #end add-point
 
                  #此段落由子樣板a01產生
         BEFORE FIELD xcaystus
            #add-point:BEFORE FIELD xcaystus
            {<point name="input.b.page1.xcaystus" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD xcaystus
            
            #add-point:AFTER FIELD xcaystus
            {<point name="input.a.page1.xcaystus" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE xcaystus
            #add-point:ON CHANGE xcaystus
            {<point name="input.g.page1.xcaystus" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD xcay001
            
            #add-point:AFTER FIELD xcay001
            {<point name="input.a.page1.xcay001" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD xcay001
            #add-point:BEFORE FIELD xcay001
            {<point name="input.b.page1.xcay001" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE xcay001
            #add-point:ON CHANGE xcay001
            {<point name="input.g.page1.xcay001" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD xcay002
            
            #add-point:AFTER FIELD xcay002
            {<point name="input.a.page1.xcay002" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD xcay002
            #add-point:BEFORE FIELD xcay002
            {<point name="input.b.page1.xcay002" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE xcay002
            #add-point:ON CHANGE xcay002
            {<point name="input.g.page1.xcay002" />}
            #END add-point
 
 
                  #Ctrlp:input.c.page1.xcaystus
         ON ACTION controlp INFIELD xcaystus
            #add-point:ON ACTION controlp INFIELD xcaystus
            {<point name="input.c.page1.xcaystus" />}
            #END add-point
 
         #Ctrlp:input.c.page1.xcay001
         ON ACTION controlp INFIELD xcay001
            #add-point:ON ACTION controlp INFIELD xcay001
            {<point name="input.c.page1.xcay001" />}
            #END add-point
 
         #Ctrlp:input.c.page1.xcay002
         ON ACTION controlp INFIELD xcay002
            #add-point:ON ACTION controlp INFIELD xcay002
            {<point name="input.c.page1.xcay002" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_xcay_d[l_ac].* = g_xcay_d_t.*
               CLOSE axci114_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_xcay_d[l_ac].xcay001,-263,1)
               LET g_xcay_d[l_ac].* = g_xcay_d_t.*
            ELSE
               #寫入修改者/修改日期資訊
               LET g_xcay2_d[l_ac].xcaymodid = g_user 
LET g_xcay2_d[l_ac].xcaymoddt = cl_get_current()
 
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
         
               UPDATE xcay_t SET (xcayld,xcaystus,xcay001,xcay002,xcayownid,xcayowndp,xcaycrtid,xcaycrtdp, 
                   xcaycrtdt,xcaymodid,xcaymoddt) = (g_xcay_m.xcayld,g_xcay_d[l_ac].xcaystus,g_xcay_d[l_ac].xcay001, 
                   g_xcay_d[l_ac].xcay002,g_xcay2_d[l_ac].xcayownid,g_xcay2_d[l_ac].xcayowndp,g_xcay2_d[l_ac].xcaycrtid, 
                   g_xcay2_d[l_ac].xcaycrtdp,g_xcay2_d[l_ac].xcaycrtdt,g_xcay2_d[l_ac].xcaymodid,g_xcay2_d[l_ac].xcaymoddt) 

                WHERE xcayent = g_enterprise AND xcayld = g_xcay_m.xcayld 
 
                 AND xcay001 = g_xcay_d_t.xcay001 #項次   
 
                 
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
               
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("xcay_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                  #WHEN SQLCA.sqlcode #其他錯誤
                  #   CALL cl_err("xcay_t",SQLCA.sqlcode,1)  
                  #   CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_xcay_m.xcayld
               LET gs_keys_bak[1] = g_xcayld_t
               LET gs_keys[2] = g_xcay_d[g_detail_idx].xcay001
               LET gs_keys_bak[2] = g_xcay_d_t.xcay001
               CALL axci114_update_b('xcay_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
                     LET g_log1 = util.JSON.stringify(g_xcay_m),util.JSON.stringify(g_xcay_d_t)
                     LET g_log2 = util.JSON.stringify(g_xcay_m),util.JSON.stringify(g_xcay_d[l_ac])
                     IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                        CALL s_transaction_end('N','0')
                     END IF
               END CASE
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
            END IF
 
         AFTER INPUT
            #若單身還沒有輸入資料, 強制切換至單身
            IF g_xcay_d.getLength() = 0 THEN
               NEXT FIELD xcay001
            END IF
            #add-point:input段after input 
            {<point name="input.body.after_input"/>}
            #end add-point    
            
         ON ACTION controlo   
            CALL FGL_SET_ARR_CURR(g_xcay_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_xcay_d.getLength()+1
            
      END INPUT
 
 
      
      DISPLAY ARRAY g_xcay2_d TO s_detail2.* ATTRIBUTES(COUNT=g_rec_b)  
      
         BEFORE ROW
            CALL axci114_b_fill(g_wc2) 
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail2")
            LET l_ac = g_detail_idx
            DISPLAY g_detail_idx TO FORMONLY.idx
            CALL axci114_ui_detailshow()
        
         BEFORE DISPLAY 
            CALL FGL_SET_ARR_CURR(g_detail_idx)      
         
         #add-point:page2自定義行為
         {<point name="input.body2.action"/>}
         #end add-point
         
      END DISPLAY
 
      
 
      
      #add-point:input段more_input
      {<point name="input.more_inputarray"/>}
      #end add-point    
      
      BEFORE DIALOG
         #add-point:input段before_dialog
         {<point name="input.before_dialog"/>}
         #end add-point 
         #新增時強制從單頭開始填
         IF p_cmd = 'a' THEN
            NEXT FIELD xcayld
        ELSE
            CASE g_aw
               WHEN "s_detail1"
                  NEXT FIELD xcaystus
               WHEN "s_detail2"
                  NEXT FIELD xcay001_2
 
            END CASE
         END IF
   
      ON ACTION controlf
         CALL cl_set_focus_form(ui.Interface.getRootNode()) RETURNING g_fld_name,g_frm_name
         CALL cl_fldhelp(g_frm_name,g_fld_name,g_lang)
 
      ON ACTION controlr
         CALL cl_show_req_fields()
 
      ON ACTION controls
         IF g_header_hidden THEN
            CALL gfrm_curr.setElementHidden("vb_master",0)
            CALL gfrm_curr.setElementImage("controls","small/arr-u.png")
            LET g_header_hidden = 0     #visible
         ELSE
            CALL gfrm_curr.setElementHidden("vb_master",1)
            CALL gfrm_curr.setElementImage("controls","small/arr-d.png")
            LET g_header_hidden = 1     #hidden     
         END IF
 
      ON ACTION accept
         ACCEPT DIALOG
        
      ON ACTION cancel      #在dialog button (放棄)
         LET g_action_choice=""
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION close       #在dialog 右上角 (X)
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION exit        #toolbar 離開
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      #交談指令共用ACTION
      &include "common_action.4gl" 
         CONTINUE DIALOG 
   END DIALOG
   
   #add-point:input段after_input
   {<point name="input.after_input"/>}
   #end add-point  
   
END FUNCTION
]]>
</section>
  <section id="axci114.input.head" ver="3" status="" src="s">
<![CDATA[   
      #單頭段
      INPUT BY NAME g_xcay_m.xcayld 
         ATTRIBUTE(WITHOUT DEFAULTS)
         
         #自訂單頭ACTION
         
         
         BEFORE INPUT
            IF s_transaction_chk("N",0) THEN
               CALL s_transaction_begin()
            END IF
            
            IF l_cmd_t = 'r' THEN
               
            END IF
            #add-point:單頭input前
            {<point name="input.head.b_input"/>}
            #end add-point
          
                  #此段落由子樣板a02產生
         AFTER FIELD xcayld
            
            #add-point:AFTER FIELD xcayld
            {<point name="input.a.xcayld" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD xcayld
            #add-point:BEFORE FIELD xcayld
            {<point name="input.b.xcayld" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE xcayld
            #add-point:ON CHANGE xcayld
            {<point name="input.g.xcayld" />}
            #END add-point
 
 #欄位檢查
                  #Ctrlp:input.c.xcayld
         ON ACTION controlp INFIELD xcayld
            #add-point:ON ACTION controlp INFIELD xcayld
            {<point name="input.c.xcayld" />}
            #END add-point
 
 #欄位開窗
 
         AFTER INPUT
            IF INT_FLAG THEN
               EXIT DIALOG
            END IF
            
            IF s_transaction_chk("N",0) THEN
                CALL s_transaction_begin()
            END IF
 
            #多語言處理
            
                
            CALL cl_showmsg()
            DISPLAY BY NAME g_xcay_m.xcayld             
 
 
            IF p_cmd = 'u' THEN
               #add-point:單頭修改前
               {<point name="input.head.b_update" mark="Y"/>}
               #end add-point
            
               UPDATE xcay_t SET (xcayld) = (g_xcay_m.xcayld)
                WHERE xcayent = g_enterprise AND xcayld = g_xcayld_t
 
               #add-point:單頭修改中
               {<point name="input.head.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("xcay_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                  WHEN SQLCA.sqlcode #其他錯誤
                     CALL cl_err("xcay_t",SQLCA.sqlcode,1)  
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                     #資料多語言用-增/改
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_xcay_m.xcayld
               LET gs_keys_bak[1] = g_xcayld_t
               LET gs_keys[2] = g_xcay_d[g_detail_idx].xcay001
               LET gs_keys_bak[2] = g_xcay_d_t.xcay001
               CALL axci114_update_b('xcay_t',gs_keys,gs_keys_bak,"'1'")
                     
                     LET g_xcayld_t = g_xcay_m.xcayld
 
                     #add-point:單頭修改後
                     {<point name="input.head.a_update"/>}
                     #end add-point
                     
                     LET g_log1 = util.JSON.stringify(g_xcay_m_t)
                     LET g_log2 = util.JSON.stringify(g_xcay_m)
                     IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                        CALL s_transaction_end('N','0')
                     ELSE
                        CALL s_transaction_end('Y','0')
                     END IF
               END CASE
            
            ELSE    
               #add-point:單頭新增
               {<point name="input.head.a_insert"/>}
               #end add-point
                                 
               IF l_cmd_t = 'r' AND p_cmd = 'a' THEN
                  CALL axci114_detail_reproduce()
               END IF
            END IF
           #controlp
                     
           LET g_xcayld_t = g_xcay_m.xcayld
 
           
           #若單身還沒有輸入資料, 強制切換至單身
           #IF cl_null(g_xcay_d[1].xcay001) THEN
           #   CALL g_xcay_d.deleteElement(1)
           #   NEXT FIELD xcay001
           #END IF
           
           IF g_xcay_d.getLength() = 0 THEN
              NEXT FIELD xcay001
           END IF
 
      END INPUT
]]>
