  <section id="axci114.b_fill" ver="8" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION axci114_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_xcay_d.clear()
   CALL g_xcay2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE xcaystus,xcay001,xcay002,xcay001,xcayownid,xcayowndp,xcaycrtid,xcaycrtdp, 
       xcaycrtdt,xcaymodid,xcaymoddt,t1.xcaul003 ,t2.oofa011 ,t3.ooefl003 ,t4.oofa011 ,t5.ooefl003 , 
       t6.oofa011 FROM xcay_t",   
               "",
               
                              " LEFT JOIN xcaul_t t1 ON t1.xcaulent='"||g_enterprise||"' AND t1.xcaul001=xcay002 AND t1.xcaul002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t2 ON t2.oofaent='"||g_enterprise||"' AND t2.oofa002='2' AND t2.oofa003=xcayownid  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent='"||g_enterprise||"' AND t3.ooefl001=xcayowndp AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t4 ON t4.oofaent='"||g_enterprise||"' AND t4.oofa002='2' AND t4.oofa003=xcaycrtid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=xcaycrtdp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t6 ON t6.oofaent='"||g_enterprise||"' AND t6.oofa002='2' AND t6.oofa003=xcaymodid  ",
 
               " WHERE xcayent= ? AND xcayld=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("xcay_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF axci114_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY xcay_t.xcay001"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE axci114_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR axci114_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_xcay_m.xcayld
                                               
      FOREACH b_fill_cs INTO g_xcay_d[l_ac].xcaystus,g_xcay_d[l_ac].xcay001,g_xcay_d[l_ac].xcay002,g_xcay2_d[l_ac].xcay001, 
          g_xcay2_d[l_ac].xcayownid,g_xcay2_d[l_ac].xcayowndp,g_xcay2_d[l_ac].xcaycrtid,g_xcay2_d[l_ac].xcaycrtdp, 
          g_xcay2_d[l_ac].xcaycrtdt,g_xcay2_d[l_ac].xcaymodid,g_xcay2_d[l_ac].xcaymoddt,g_xcay_d[l_ac].xcay002_desc, 
          g_xcay2_d[l_ac].xcayownid_desc,g_xcay2_d[l_ac].xcayowndp_desc,g_xcay2_d[l_ac].xcaycrtid_desc, 
          g_xcay2_d[l_ac].xcaycrtdp_desc,g_xcay2_d[l_ac].xcaymodid_desc
                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_xcay2_d[l_ac].xcayownid_desc = cl_get_username(g_xcay2_d[l_ac].xcayownid)
      #LET g_xcay2_d[l_ac].xcayowndp_desc = cl_get_deptname(g_xcay2_d[l_ac].xcayowndp)
      #LET g_xcay2_d[l_ac].xcaycrtid_desc = cl_get_username(g_xcay2_d[l_ac].xcaycrtid)
      #LET g_xcay2_d[l_ac].xcaycrtdp_desc = cl_get_deptname(g_xcay2_d[l_ac].xcaycrtdp)
      #LET g_xcay2_d[l_ac].xcaymodid_desc = cl_get_username(g_xcay2_d[l_ac].xcaymodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_xcay_d.deleteElement(g_xcay_d.getLength())
      CALL g_xcay2_d.deleteElement(g_xcay2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE axci114_pb   
   
END FUNCTION
]]>
</section>
