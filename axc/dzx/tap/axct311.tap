   LET g_xccpld_t = g_xccp_m.xccpld
   LET g_xccp003_t = g_xccp_m.xccp003
   LET g_xccp004_t = g_xccp_m.xccp004
   LET g_xccp005_t = g_xccp_m.xccp005
   LET g_xccp006_t = g_xccp_m.xccp006
 
   
