  <section id="axct311.b_fill" ver="16" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION axct311_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define" edit="s"/>}
   #end add-point     
   #add-point:b_fill段define
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
   
   #先清空單身變數內容
   CALL g_xccp_d.clear()
   CALL g_xccp2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE xccp001,xccp007,xccp009,xccp101,xccp102a,xccp102b,xccp102c,xccp102d,xccp102e, 
       xccp102f,xccp102g,xccp102h,xccp102,xccp001,xccp007,xccpownid,xccpowndp,xccpcrtid,xccpcrtdp,xccpcrtdt, 
       xccpmodid,xccpmoddt,xccpcnfid,xccpcnfdt,xccppstid,xccppstdt,t1.ooag011 ,t2.ooefl003 ,t3.ooag011 , 
       t4.ooefl003 ,t5.ooag011 ,t6.ooag011 ,t7.ooag011 FROM xccp_t",   
               "",
               
                              " LEFT JOIN ooag_t t1 ON t1.ooagent='"||g_enterprise||"' AND t1.ooag001=xccpownid  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent='"||g_enterprise||"' AND t2.ooefl001=xccpowndp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent='"||g_enterprise||"' AND t3.ooag001=xccpcrtid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=xccpcrtdp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent='"||g_enterprise||"' AND t5.ooag001=xccpmodid  ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent='"||g_enterprise||"' AND t6.ooag001=xccpcnfid  ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent='"||g_enterprise||"' AND t7.ooag001=xccppstid  ",
 
               " WHERE xccpent= ? AND xccpld=? AND xccp003=? AND xccp004=? AND xccp005=? AND xccp006=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("xccp_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF axct311_fill_chk(1) THEN
      IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
         LET g_sql = g_sql, " ORDER BY xccp_t.xccp001,xccp_t.xccp007"
         #add-point:b_fill段fill_before
         {<point name="b_fill.fill_before"/>}
         #end add-point
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE axct311_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR axct311_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_xccp_m.xccpld,g_xccp_m.xccp003,g_xccp_m.xccp004,g_xccp_m.xccp005,g_xccp_m.xccp006
                                               
      FOREACH b_fill_cs INTO g_xccp_d[l_ac].xccp001,g_xccp_d[l_ac].xccp007,g_xccp_d[l_ac].xccp009,g_xccp_d[l_ac].xccp101, 
          g_xccp_d[l_ac].xccp102a,g_xccp_d[l_ac].xccp102b,g_xccp_d[l_ac].xccp102c,g_xccp_d[l_ac].xccp102d, 
          g_xccp_d[l_ac].xccp102e,g_xccp_d[l_ac].xccp102f,g_xccp_d[l_ac].xccp102g,g_xccp_d[l_ac].xccp102h, 
          g_xccp_d[l_ac].xccp102,g_xccp2_d[l_ac].xccp001,g_xccp2_d[l_ac].xccp007,g_xccp2_d[l_ac].xccpownid, 
          g_xccp2_d[l_ac].xccpowndp,g_xccp2_d[l_ac].xccpcrtid,g_xccp2_d[l_ac].xccpcrtdp,g_xccp2_d[l_ac].xccpcrtdt, 
          g_xccp2_d[l_ac].xccpmodid,g_xccp2_d[l_ac].xccpmoddt,g_xccp2_d[l_ac].xccpcnfid,g_xccp2_d[l_ac].xccpcnfdt, 
