      CONSTRUCT g_wc2_table1 ON xccp001,xccp007,xccp009,xccp101,xccp102a,xccp102b,xccp102c,xccp102d, 
          xccp102e,xccp102f,xccp102g,xccp102h,xccp102,xccpownid,xccpowndp,xccpcrtid,xccpcrtdp,xccpcrtdt, 
