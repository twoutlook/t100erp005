  <section id="axci120.input.body" ver="5" status="" src="s">
    <![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_xcbb_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂單身ACTION
         
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_xcbb_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL axci120_b_fill(g_wc2) 
            IF g_rec_b != 0 THEN
               CALL fgl_set_arr_curr(l_ac)
            END IF
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN axci120_cl USING g_enterprise,
                                               g_xcbb_m.xcbbsite
                                               ,g_xcbb_m.xcbb001
                                               ,g_xcbb_m.xcbb002
 
                                               
               IF STATUS THEN
                  CALL cl_err("OPEN axci120_cl:", STATUS, 1)
                  CLOSE axci120_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_xcbb_d[l_ac].xcbb003 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_xcbb_d_t.* = g_xcbb_d[l_ac].*  #BACKUP
               CALL axci120_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL axci120_set_no_entry_b(l_cmd)
               OPEN axci120_bcl USING g_enterprise,g_xcbb_m.xcbbsite,
                                                g_xcbb_m.xcbb001,
                                                g_xcbb_m.xcbb002,
 
                                                g_xcbb_d_t.xcbb003
 
               IF STATUS THEN
                  CALL cl_err("OPEN axci120_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH axci120_bcl INTO g_xcbb_d[l_ac].xcbbstus,g_xcbb_d[l_ac].xcbb003,g_xcbb_d[l_ac].xcbb004, 
                      g_xcbb_d[l_ac].xcbb011,g_xcbb_d[l_ac].xcbb005,g_xcbb_d[l_ac].xcbb007,g_xcbb_d[l_ac].xcbb006, 
                      g_xcbb_d[l_ac].xcbb010,g_xcbb_d[l_ac].xcbb009,g_xcbb_d[l_ac].xcbb008
                  IF SQLCA.sqlcode THEN
                      CALL cl_err(g_xcbb_d_t.xcbb003,SQLCA.sqlcode,1)
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL axci120_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
        
         BEFORE INSERT
            
            INITIALIZE g_xcbb_d_t.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_xcbb_d[l_ac].* TO NULL
            
            #公用欄位預設值
            #此段落由子樣板a14產生    
      LET g_xcbb2_d[l_ac].xcbbownid = g_user
      LET g_xcbb2_d[l_ac].xcbbowndp = g_dept
      LET g_xcbb2_d[l_ac].xcbbcrtid = g_user
      LET g_xcbb2_d[l_ac].xcbbcrtdp = g_dept 
      LET g_xcbb2_d[l_ac].xcbbcrtdt = cl_get_current()
      LET g_xcbb2_d[l_ac].xcbbmodid = ""
      LET g_xcbb2_d[l_ac].xcbbmoddt = ""
      LET g_xcbb_d[l_ac].xcbbstus = ""
 
  
            
            #一般欄位預設值
                  LET g_xcbb_d[l_ac].xcbbstus = "Y"
      LET g_xcbb_d[l_ac].xcbb008 = "N"
 
            
            
            LET g_xcbb_d_t.* = g_xcbb_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL axci120_set_entry_b(l_cmd)
            #add-point:set_entry_b後
            {<point name="input.body.before_insert.set_entry_b"/>}
            #end add-point
            CALL axci120_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_xcbb_d[li_reproduce_target].* = g_xcbb_d[li_reproduce].*
               LET g_xcbb2_d[li_reproduce_target].* = g_xcbb2_d[li_reproduce].*
 
               LET g_xcbb_d[g_xcbb_d.getLength()].xcbb003 = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
 
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM xcbb_t 
             WHERE xcbbent = g_enterprise AND xcbbsite = g_xcbb_m.xcbbsite
               AND xcbb001 = g_xcbb_m.xcbb001
               AND xcbb002 = g_xcbb_m.xcbb002
 
               AND xcbb003 = g_xcbb_d[l_ac].xcbb003
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
