  <section id="axci120.b_fill" ver="8" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION axci120_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_xcbb_d.clear()
   CALL g_xcbb2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE xcbbstus,xcbb003,xcbb004,xcbb011,xcbb005,xcbb007,xcbb006,xcbb010,xcbb009, 
       xcbb008,xcbb003,xcbbownid,xcbbowndp,xcbbcrtid,xcbbcrtdp,xcbbcrtdt,xcbbmodid,xcbbmoddt,t1.oofa011 , 
       t2.ooefl003 ,t3.oofa011 ,t4.ooefl003 ,t5.oofa011 FROM xcbb_t",   
               "",
               
                              " LEFT JOIN oofa_t t1 ON t1.oofaent='"||g_enterprise||"' AND t1.oofa002='2' AND t1.oofa003=xcbbownid  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent='"||g_enterprise||"' AND t2.ooefl001=xcbbcrtdp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t3 ON t3.oofaent='"||g_enterprise||"' AND t3.oofa002='2' AND t3.oofa003=xcbbcrtid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=xcbbcrtdp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t5 ON t5.oofaent='"||g_enterprise||"' AND t5.oofa002='2' AND t5.oofa003=xcbbcrtid  ",
 
               " WHERE xcbbent= ? AND xcbbsite=? AND xcbb001=? AND xcbb002=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("xcbb_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF axci120_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY xcbb_t.xcbb003"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE axci120_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR axci120_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_xcbb_m.xcbbsite,g_xcbb_m.xcbb001,g_xcbb_m.xcbb002
                                               
      FOREACH b_fill_cs INTO g_xcbb_d[l_ac].xcbbstus,g_xcbb_d[l_ac].xcbb003,g_xcbb_d[l_ac].xcbb004,g_xcbb_d[l_ac].xcbb011, 
          g_xcbb_d[l_ac].xcbb005,g_xcbb_d[l_ac].xcbb007,g_xcbb_d[l_ac].xcbb006,g_xcbb_d[l_ac].xcbb010, 
          g_xcbb_d[l_ac].xcbb009,g_xcbb_d[l_ac].xcbb008,g_xcbb2_d[l_ac].xcbb003,g_xcbb2_d[l_ac].xcbbownid, 
          g_xcbb2_d[l_ac].xcbbowndp,g_xcbb2_d[l_ac].xcbbcrtid,g_xcbb2_d[l_ac].xcbbcrtdp,g_xcbb2_d[l_ac].xcbbcrtdt, 
          g_xcbb2_d[l_ac].xcbbmodid,g_xcbb2_d[l_ac].xcbbmoddt,g_xcbb2_d[l_ac].xcbbownid_desc,g_xcbb2_d[l_ac].xcbbowndp_desc, 
          g_xcbb2_d[l_ac].xcbbcrtid_desc,g_xcbb2_d[l_ac].xcbbcrtdp_desc,g_xcbb2_d[l_ac].xcbbmodid_desc 

                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_xcbb2_d[l_ac].xcbbownid_desc = cl_get_username(g_xcbb2_d[l_ac].xcbbownid)
      #LET g_xcbb2_d[l_ac].xcbbowndp_desc = cl_get_deptname(g_xcbb2_d[l_ac].xcbbowndp)
      #LET g_xcbb2_d[l_ac].xcbbcrtid_desc = cl_get_username(g_xcbb2_d[l_ac].xcbbcrtid)
      #LET g_xcbb2_d[l_ac].xcbbcrtdp_desc = cl_get_deptname(g_xcbb2_d[l_ac].xcbbcrtdp)
      #LET g_xcbb2_d[l_ac].xcbbmodid_desc = cl_get_username(g_xcbb2_d[l_ac].xcbbmodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_xcbb_d.deleteElement(g_xcbb_d.getLength())
      CALL g_xcbb2_d.deleteElement(g_xcbb2_d.getLength())
 
   END IF
   
