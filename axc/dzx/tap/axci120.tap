               #end add-point
            
               UPDATE xcbb_t SET (xcbbsite,xcbb001,xcbb002) = (g_xcbb_m.xcbbsite,g_xcbb_m.xcbb001,g_xcbb_m.xcbb002) 

                WHERE xcbbent = g_enterprise AND xcbbsite = g_xcbbsite_t
                  AND xcbb001 = g_xcbb001_t
                  AND xcbb002 = g_xcbb002_t
 
               #add-point:單頭修改中
               {<point name="input.head.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("xcbb_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                  WHEN SQLCA.sqlcode #其他錯誤
                     CALL cl_err("xcbb_t",SQLCA.sqlcode,1)  
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                     #資料多語言用-增/改
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_xcbb_m.xcbbsite
               LET gs_keys_bak[1] = g_xcbbsite_t
               LET gs_keys[2] = g_xcbb_m.xcbb001
               LET gs_keys_bak[2] = g_xcbb001_t
               LET gs_keys[3] = g_xcbb_m.xcbb002
               LET gs_keys_bak[3] = g_xcbb002_t
               LET gs_keys[4] = g_xcbb_d[g_detail_idx].xcbb003
               LET gs_keys_bak[4] = g_xcbb_d_t.xcbb003
               CALL axci120_update_b('xcbb_t',gs_keys,gs_keys_bak,"'1'")
                     
                     LET g_xcbbsite_t = g_xcbb_m.xcbbsite
                     LET g_xcbb001_t = g_xcbb_m.xcbb001
                     LET g_xcbb002_t = g_xcbb_m.xcbb002
 
                     #add-point:單頭修改後
                     {<point name="input.head.a_update"/>}
                     #end add-point
                     
                     LET g_log1 = util.JSON.stringify(g_xcbb_m_t)
                     LET g_log2 = util.JSON.stringify(g_xcbb_m)
                     IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                        CALL s_transaction_end('N','0')
                     ELSE
                        CALL s_transaction_end('Y','0')
                     END IF
               END CASE
            
            ELSE    
               #add-point:單頭新增
               {<point name="input.head.a_insert"/>}
               #end add-point
                                 
               IF l_cmd_t = 'r' AND p_cmd = 'a' THEN
                  CALL axci120_detail_reproduce()
               END IF
            END IF
           #controlp
                     
           LET g_xcbbsite_t = g_xcbb_m.xcbbsite
           LET g_xcbb001_t = g_xcbb_m.xcbb001
           LET g_xcbb002_t = g_xcbb_m.xcbb002
 
           
           #若單身還沒有輸入資料, 強制切換至單身
           #IF cl_null(g_xcbb_d[1].xcbb003) THEN
           #   CALL g_xcbb_d.deleteElement(1)
           #   NEXT FIELD xcbb003
           #END IF
           
           IF g_xcbb_d.getLength() = 0 THEN
              NEXT FIELD xcbb003
           END IF
 
      END INPUT
]]>
