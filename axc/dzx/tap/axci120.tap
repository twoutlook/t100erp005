   LET g_forupd_sql = "SELECT xcbbstus,xcbb003,xcbb004,xcbb011,xcbb005,xcbb007,xcbb006,xcbb010,xcbb009, 
       xcbb008,xcbb003,xcbbownid,xcbbowndp,xcbbcrtid,xcbbcrtdp,xcbbcrtdt,xcbbmodid,xcbbmoddt FROM xcbb_t  
       WHERE xcbbent=? AND xcbbsite=? AND xcbb001=? AND xcbb002=? AND xcbb003=? FOR UPDATE"
   #add-point:input段define_sql
