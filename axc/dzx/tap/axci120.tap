  <section id="axci120.detail_reproduce" ver="3" status="" src="s">
    <![CDATA[#+ 單身自動複製
PRIVATE FUNCTION axci120_detail_reproduce()
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE xcbb_t.*
 
 
   #add-point:delete段define
   {<point name="detail_reproduce.define"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE axci120_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE axci120_detail AS ",
                "SELECT * FROM xcbb_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO axci120_detail SELECT * FROM xcbb_t 
                                         WHERE xcbbent = g_enterprise AND xcbbsite = g_xcbbsite_t
                                         AND xcbb001 = g_xcbb001_t
                                         AND xcbb002 = g_xcbb002_t
 
   
   #將key修正為調整後   
   UPDATE axci120_detail 
      #更新key欄位
      SET xcbbsite = g_xcbb_m.xcbbsite
          , xcbb001 = g_xcbb_m.xcbb001
          , xcbb002 = g_xcbb_m.xcbb002
 
      #更新共用欄位
      #此段落由子樣板a13產生
       , xcbbownid = g_user
       , xcbbowndp = g_dept
       , xcbbcrtid = g_user
       , xcbbcrtdp = g_dept 
       , xcbbcrtdt = ld_date
       , xcbbmodid = "" 
       , xcbbmoddt = "" 
      #, xcbbstus = "Y"
 
 
                                       
  
   #將資料塞回原table   
   INSERT INTO xcbb_t SELECT * FROM axci120_detail
   
   IF SQLCA.sqlcode THEN
      CALL cl_err("reproduce",SQLCA.sqlcode,1)
      RETURN
   END IF
   
