  <section id="axci120.delete" ver="6" status="" src="s">
    <![CDATA[#+ 資料刪除
PRIVATE FUNCTION axci120_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_xcbb_m.xcbbsite IS NULL
   OR g_xcbb_m.xcbb001 IS NULL
   OR g_xcbb_m.xcbb002 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE axci120_master_referesh USING g_xcbb_m.xcbbsite,g_xcbb_m.xcbb001,g_xcbb_m.xcbb002 INTO g_xcbb_m.xcbbsite, 
       g_xcbb_m.xcbb001,g_xcbb_m.xcbb002,g_xcbb_m.xcbbsite_desc
   
   CALL axci120_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN axci120_cl USING g_enterprise,g_xcbb_m.xcbbsite
                                                       ,g_xcbb_m.xcbb001
                                                       ,g_xcbb_m.xcbb002
 
   IF STATUS THEN
      CALL cl_err("OPEN axci120_cl:", STATUS, 1)
      CLOSE axci120_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH axci120_cl INTO g_xcbb_m.xcbbsite,g_xcbb_m.xcbbsite_desc,g_xcbb_m.xcbb001,g_xcbb_m.xcbb002
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_xcbb_m.xcbbsite,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL axci120_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM xcbb_t WHERE xcbbent = g_enterprise AND xcbbsite = g_xcbb_m.xcbbsite
                                                               AND xcbb001 = g_xcbb_m.xcbb001
                                                               AND xcbb002 = g_xcbb_m.xcbb002
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("xcbb_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_xcbb_d.clear() 
      CALL g_xcbb2_d.clear()       
 
     
      CALL axci120_ui_browser_refresh()  
      CALL axci120_ui_headershow()  
      CALL axci120_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL axci120_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL axci120_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE axci120_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_xcbb_m.xcbbsite,'D')
    
END FUNCTION
