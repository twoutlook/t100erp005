  <point name="set_no_entry_b.define" order="" ver="5" cite_std="N" new="N" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[   DEFINE l_is_sfaastus  LIKE type_t.num5   #add 150610 zhangllc是否可修改sfaastus栏位
   DEFINE l_success      LIKE type_t.num5   #add 150610 zhangllc
   DEFINE l_year         LIKE type_t.num5   #add 150610 zhangllc
   DEFINE l_period       LIKE type_t.num5   #add 150610 zhangllc
   DEFINE l_glaald       LIKE glaa_t.glaald #add 150610 zhangllc
   DEFINE l_cnt          LIKE type_t.num5   #add 150610 zhangllc]]>
  </point>
  <point name="set_no_entry_b.set_no_entry_b" order="" ver="5" cite_std="N" new="N" status="u" src="s" readonly="" mark_hard="N">
    <![CDATA[   #fengmy--begin 20141230
   IF NOT cl_null(g_sfaa_d[l_ac].sfaa048) THEN
   
      CALL axci160_sfaa048_chk()
      IF NOT cl_null(g_errno) THEN
         CALL cl_set_comp_entry("sfaa042",FALSE)
#         CALL cl_set_comp_entry("sfaa048",FALSE)
      END IF
   END IF
   #fengmy--end 20141230
   
   #add 150610 zhangllc 
   #sfaastus可开放修改的控制条件
   LET l_is_sfaastus = TRUE
   #--只有M才开放此栏位修改
   IF g_sfaa_d_t.sfaastus != 'M' THEN  #这里用旧值，以防M改成C后想改回M改不了
      LET l_is_sfaastus = FALSE
   END IF
   
   IF NOT cl_null(g_sfaa_d[l_ac].sfaa048) THEN
      #--成本结案日要和现行成本计算年期(g_yy,g_mm)一致才能修改状态
      #获取帐别
      SELECT glaald INTO l_glaald FROM glaa_t
       WHERE glaaent = g_enterprise
         AND glaacomp= g_ooef001
         AND glaa014 ='Y'
      #成本结案日所在年期
      CALL s_fin_date_get_period_value('',l_glaald,g_sfaa_d[l_ac].sfaa048) RETURNING l_success,l_year,l_period
      IF l_year != g_yy OR l_period != g_mm OR l_success = FALSE THEN
         LET l_is_sfaastus = FALSE
      END IF
   
      #--如果成本结案日的次月成本计算（xccc、xcck等）已经有了，也不能修改
      #成本结案日的次期
      CALL s_fin_date_get_next_period('',l_glaald,l_year,l_period) RETURNING l_success,l_year,l_period
      SELECT COUNT(*) INTO l_cnt FROM xccc_t
      WHERE xcccent = g_enterprise
        AND xcccld  = l_glaald
        AND xccc004 = l_year
        AND xccc005 = l_period
      IF l_cnt > 0 THEN
         LET l_is_sfaastus = FALSE
      END IF
   END IF
   
   IF l_is_sfaastus = FALSE THEN
      CALL cl_set_comp_entry("sfaastus",FALSE)
   END IF

   IF g_sfaa_d[l_ac].sfaastus !='M' THEN
      CALL cl_set_comp_entry("sfaa048",FALSE)
   END IF
   #add 150610 end]]>
  </point>
  <section id="axci160.b_fill" ver="21" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION axci160_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2    STRING
   #add-point:b_fill段define
   {<point name="b_fill.define" edit="s"/>}
   #end add-point
   #add-point:b_fill段define(客製用)
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE t0.sfaasite,t0.sfaadocno,t0.sfaadocdt,t0.sfaa003,t0.sfaastus,t0.sfaa010, 
       t0.sfaa012,t0.sfaa042,t0.sfaa048,t0.sfaadocno,t0.sfaaownid,t0.sfaaowndp,t0.sfaacrtid,t0.sfaacrtdp, 
       t0.sfaacrtdt,t0.sfaamodid,t0.sfaamoddt,t0.sfaacnfid,t0.sfaacnfdt,t0.sfaapstid,t0.sfaapstdt ,t1.ooefl003 , 
       t2.imaal003 ,t3.imaal004 ,t4.ooag011 ,t5.ooefl003 ,t6.ooag011 ,t7.ooefl003 ,t8.ooag011 ,t9.ooag011 , 
       t10.ooag011 FROM sfaa_t t0",
               "",
                              " LEFT JOIN ooefl_t t1 ON t1.ooeflent='"||g_enterprise||"' AND t1.ooefl001=t0.sfaasite AND t1.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t2 ON t2.imaalent='"||g_enterprise||"' AND t2.imaal001=t0.sfaa010 AND t2.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t3 ON t3.imaalent='"||g_enterprise||"' AND t3.imaal001=t0.sfaa010 AND t3.imaal002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t4 ON t4.ooagent='"||g_enterprise||"' AND t4.ooag001=t0.sfaaownid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=t0.sfaaowndp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent='"||g_enterprise||"' AND t6.ooag001=t0.sfaacrtid  ",
               " LEFT JOIN ooefl_t t7 ON t7.ooeflent='"||g_enterprise||"' AND t7.ooefl001=t0.sfaacrtdp AND t7.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t8 ON t8.ooagent='"||g_enterprise||"' AND t8.ooag001=t0.sfaamodid  ",
               " LEFT JOIN ooag_t t9 ON t9.ooagent='"||g_enterprise||"' AND t9.ooag001=t0.sfaacnfid  ",
               " LEFT JOIN ooag_t t10 ON t10.ooagent='"||g_enterprise||"' AND t10.ooag001=t0.sfaapstid  ",
 
               " WHERE t0.sfaaent= ?  AND  1=1 AND (", p_wc2, ") " 
   #add-point:b_fill段sql wc
   {<point name="b_fill.sql_wc"/>}
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("sfaa_t"),
                      " ORDER BY t0.sfaadocno"
   
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"sfaa_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE axci160_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR axci160_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_sfaa_d.clear()
   CALL g_sfaa2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_sfaa_d[l_ac].sfaasite,g_sfaa_d[l_ac].sfaadocno,g_sfaa_d[l_ac].sfaadocdt, 
       g_sfaa_d[l_ac].sfaa003,g_sfaa_d[l_ac].sfaastus,g_sfaa_d[l_ac].sfaa010,g_sfaa_d[l_ac].sfaa012, 
       g_sfaa_d[l_ac].sfaa042,g_sfaa_d[l_ac].sfaa048,g_sfaa2_d[l_ac].sfaadocno,g_sfaa2_d[l_ac].sfaaownid, 
       g_sfaa2_d[l_ac].sfaaowndp,g_sfaa2_d[l_ac].sfaacrtid,g_sfaa2_d[l_ac].sfaacrtdp,g_sfaa2_d[l_ac].sfaacrtdt, 
       g_sfaa2_d[l_ac].sfaamodid,g_sfaa2_d[l_ac].sfaamoddt,g_sfaa2_d[l_ac].sfaacnfid,g_sfaa2_d[l_ac].sfaacnfdt, 
       g_sfaa2_d[l_ac].sfaapstid,g_sfaa2_d[l_ac].sfaapstdt,g_sfaa_d[l_ac].sfaasite_desc,g_sfaa_d[l_ac].sfaa010_desc, 
       g_sfaa_d[l_ac].sfaa010_desc_desc,g_sfaa2_d[l_ac].sfaaownid_desc,g_sfaa2_d[l_ac].sfaaowndp_desc, 
       g_sfaa2_d[l_ac].sfaacrtid_desc,g_sfaa2_d[l_ac].sfaacrtdp_desc,g_sfaa2_d[l_ac].sfaamodid_desc, 
       g_sfaa2_d[l_ac].sfaacnfid_desc,g_sfaa2_d[l_ac].sfaapstid_desc
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL axci160_detail_show()      
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
 
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   LET g_error_show = 0
   
 
   
   CALL g_sfaa_d.deleteElement(g_sfaa_d.getLength())   
   CALL g_sfaa2_d.deleteElement(g_sfaa2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_sfaa_d.getLength()
      LET g_sfaa2_d[l_ac].sfaadocno = g_sfaa_d[l_ac].sfaadocno 
 
      #add-point:b_fill段key值相關欄位
      {<point name="b_fill.keys.fill"/>}
      #end add-point
   END FOR
   
   IF g_cnt > g_sfaa_d.getLength() THEN
      LET l_ac = g_sfaa_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_sfaa_d.getLength()
      LET g_sfaa_d_mask_o[l_ac].* =  g_sfaa_d[l_ac].*
      CALL axci160_sfaa_t_mask()
      LET g_sfaa_d_mask_n[l_ac].* =  g_sfaa_d[l_ac].*
   END FOR
   
   LET g_sfaa2_d_mask_o.* =  g_sfaa2_d.*
   FOR l_ac = 1 TO g_sfaa2_d.getLength()
      LET g_sfaa2_d_mask_o[l_ac].* =  g_sfaa2_d[l_ac].*
      CALL axci160_sfaa_t_mask()
