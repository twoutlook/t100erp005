      LET g_errparam.code   = STATUS
      LET g_errparam.popup  = TRUE
      CALL cl_err()
      LET g_rep_success = 'N' 
   END IF
   DECLARE axcq530_x01_curs CURSOR FOR axcq530_x01_prepare
 
END FUNCTION
