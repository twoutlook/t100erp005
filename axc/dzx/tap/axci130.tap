  <section id="axci130.main" ver="10" status="" src="s">
<![CDATA[#+ 此段落由子樣板a26產生
#OPTIONS SHORT CIRCUIT
#+ 作業開始 
MAIN
   #add-point:main段define
   {<point name="main.define"/>}
   #end add-point   
 
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("axc","")
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT xcbosite,'',xcbo001,'',''", 
                      " FROM xcbo_t",
                      " WHERE xcboent= ? AND xcbosite=? AND xcbo001=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE axci130_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE xcbosite,xcbo001,t1.ooefl003",
               " FROM xcbo_t",
                              " LEFT JOIN ooefl_t t1 ON t1.ooeflent='"||g_enterprise||"' AND t1.ooefl001=xcbosite AND t1.ooefl002='"||g_dlang||"' ",
 
               " WHERE xcboent = '" ||g_enterprise|| "' AND xcbosite = ? AND xcbo001 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE axci130_master_referesh FROM g_sql
 
 
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_axci130 WITH FORM cl_ap_formpath("axc",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL axci130_init()   
 
      #進入選單 Menu (="N")
      CALL axci130_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_axci130
      
   END IF 
   
   CLOSE axci130_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
   
END MAIN
 
 
]]>
</section>
  <section id="axci130.modify" ver="8" status="" src="s">
<![CDATA[#+ 資料修改
PRIVATE FUNCTION axci130_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_xcbo_m.xcbosite IS NULL
   OR g_xcbo_m.xcbo001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE axci130_master_referesh USING g_xcbo_m.xcbosite,g_xcbo_m.xcbo001 INTO g_xcbo_m.xcbosite,g_xcbo_m.xcbo001, 
       g_xcbo_m.xcbosite_desc
 
   ERROR ""
  
   LET g_xcbosite_t = g_xcbo_m.xcbosite
   LET g_xcbo001_t = g_xcbo_m.xcbo001
 
   CALL s_transaction_begin()
   
   OPEN axci130_cl USING g_enterprise,g_xcbo_m.xcbosite
                                                       ,g_xcbo_m.xcbo001
 
   IF STATUS THEN
      CALL cl_err("OPEN axci130_cl:", STATUS, 1)
      CLOSE axci130_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH axci130_cl INTO g_xcbo_m.xcbosite,g_xcbo_m.xcbosite_desc,g_xcbo_m.xcbo001,g_xcbo_m.imag014, 
       g_xcbo_m.imaal003
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_xcbo_m.xcbosite,SQLCA.sqlcode,0)
      CLOSE axci130_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL axci130_show()
   WHILE TRUE
      LET g_xcbosite_t = g_xcbo_m.xcbosite
      LET g_xcbo001_t = g_xcbo_m.xcbo001
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL axci130_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_xcbo_m.* = g_xcbo_m_t.*
         CALL axci130_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_xcbo_m.xcbosite != g_xcbosite_t 
      OR g_xcbo_m.xcbo001 != g_xcbo001_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE xcbo_t SET xcbosite = g_xcbo_m.xcbosite
                                       , xcbo001 = g_xcbo_m.xcbo001
 
          WHERE xcboent = g_enterprise AND xcbosite = g_xcbosite_t
            AND xcbo001 = g_xcbo001_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("xcbo_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("xcbo_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_xcbo_m.xcbosite,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE axci130_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_xcbo_m.xcbosite,'U')
 
   CALL axci130_b_fill("1=1")
   
END FUNCTION   
]]>
</section>
