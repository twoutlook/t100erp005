  <point name="input.a.xcbosite" cite_std="N" status="" ver="1" src="s" new="N" mark_hard="N">
<![CDATA[            #此段落由子樣板a05產生
            IF  NOT cl_null(g_xcbo_m.xcbosite) AND NOT cl_null(g_xcbo_m.xcbo001)  THEN 
               IF p_cmd = 'a' OR ( p_cmd = 'u' AND (g_xcbo_m.xcbosite != g_xcbosite_t  OR g_xcbo_m.xcbo001 != g_xcbo001_t  )) THEN 
                  IF NOT ap_chk_notDup("","SELECT COUNT(*) FROM xcbo_t WHERE "||"xcboent = '" ||g_enterprise|| "' AND "||"xcbosite = '"||g_xcbo_m.xcbosite ||"' AND "|| "xcbo001 = '"||g_xcbo_m.xcbo001 ||"'",'std-00004',0) THEN 
                     NEXT FIELD CURRENT
                  END IF
               END IF
            END IF
            IF NOT cl_null(g_xcbo_m.xcbosite) AND NOT cl_null(g_xcbo_m.xcbo001) THEN
               LET l_n = 0 
               SELECT COUNT(*) INTO l_n FROM imag_t 
                WHERE imagent = g_enterprise AND imagsite = g_xcbo_m.xcbosite
                  AND imag001 = g_xcbo_m.xcbo001
               IF l_n = 0 THEN
                  LET g_xcbo_m.xcbosite = g_xcbo_m_t.xcbosite
                  CALL cl_err('','axc-00200',0)
                  NEXT FIELD xcbosite
               END IF
            END IF
            IF NOT cl_null(g_xcbo_m.xcbosite) AND cl_null(g_xcbo_m.xcbo001) THEN
               #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
               INITIALIZE g_chkparam.* TO NULL

               #設定g_chkparam.*的參數
               LET g_chkparam.arg1 = g_xcbo_m.xcbosite

               #呼叫檢查存在並帶值的library
               IF cl_chk_exist('v_ooef001_13') THEN
                  IF NOT cl_chk_exist('v_imagsite') THEN
                     #檢查失敗時後續處理
                     LET g_xcbo_m.xcbosite = g_xcbo_m_t.xcbosite
                     INITIALIZE g_ref_fields TO NULL
                     LET g_ref_fields[1] = g_xcbo_m.xcbosite
                     CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
                     LET g_xcbo_m.xcbosite_desc = '', g_rtn_fields[1] , ''
                     DISPLAY BY NAME g_xcbo_m.xcbosite_desc
                     NEXT FIELD xcbosite
                  END IF
               ELSE
                  #檢查失敗時後續處理
                  LET g_xcbo_m.xcbosite = g_xcbo_m_t.xcbosite
                  INITIALIZE g_ref_fields TO NULL
                  LET g_ref_fields[1] = g_xcbo_m.xcbosite
                  CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
                  LET g_xcbo_m.xcbosite_desc = '', g_rtn_fields[1] , ''
                  DISPLAY BY NAME g_xcbo_m.xcbosite_desc
                  NEXT FIELD xcbosite
               END IF
            END IF


            INITIALIZE g_ref_fields TO NULL
            LET g_ref_fields[1] = g_xcbo_m.xcbosite
            CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
            LET g_xcbo_m.xcbosite_desc = '', g_rtn_fields[1] , ''
            DISPLAY BY NAME g_xcbo_m.xcbosite_desc
]]>
</point>
