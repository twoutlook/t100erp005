  <section id="axci130.b_fill" ver="10" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION axci130_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_xcbo_d.clear()
   CALL g_xcbo2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE xcbo002,xcbo003,xcbo004,xcbo002,xcboownid,xcboowndp,xcbocrtid,xcbocrtdp, 
       xcbocrtdt,xcbomodid,xcbomoddt,t1.oofa011 ,t2.ooefl003 ,t3.oofa011 ,t4.ooefl003 ,t5.oofa011 FROM xcbo_t", 
          
               "",
               
                              " LEFT JOIN oofa_t t1 ON t1.oofaent='"||g_enterprise||"' AND t1.oofa002='2' AND t1.oofa003=xcboownid  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent='"||g_enterprise||"' AND t2.ooefl001=xcboowndp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t3 ON t3.oofaent='"||g_enterprise||"' AND t3.oofa002='2' AND t3.oofa003=xcbocrtid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=xcbocrtdp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t5 ON t5.oofaent='"||g_enterprise||"' AND t5.oofa002='2' AND t5.oofa003=xcbomodid  ",
 
               " WHERE xcboent= ? AND xcbosite=? AND xcbo001=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("xcbo_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF axci130_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY xcbo_t.xcbo002"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE axci130_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR axci130_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_xcbo_m.xcbosite,g_xcbo_m.xcbo001
                                               
      FOREACH b_fill_cs INTO g_xcbo_d[l_ac].xcbo002,g_xcbo_d[l_ac].xcbo003,g_xcbo_d[l_ac].xcbo004,g_xcbo2_d[l_ac].xcbo002, 
          g_xcbo2_d[l_ac].xcboownid,g_xcbo2_d[l_ac].xcboowndp,g_xcbo2_d[l_ac].xcbocrtid,g_xcbo2_d[l_ac].xcbocrtdp, 
          g_xcbo2_d[l_ac].xcbocrtdt,g_xcbo2_d[l_ac].xcbomodid,g_xcbo2_d[l_ac].xcbomoddt,g_xcbo2_d[l_ac].xcboownid_desc, 
          g_xcbo2_d[l_ac].xcboowndp_desc,g_xcbo2_d[l_ac].xcbocrtid_desc,g_xcbo2_d[l_ac].xcbocrtdp_desc, 
          g_xcbo2_d[l_ac].xcbomodid_desc
                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_xcbo2_d[l_ac].xcboownid_desc = cl_get_username(g_xcbo2_d[l_ac].xcboownid)
      #LET g_xcbo2_d[l_ac].xcboowndp_desc = cl_get_deptname(g_xcbo2_d[l_ac].xcboowndp)
      #LET g_xcbo2_d[l_ac].xcbocrtid_desc = cl_get_username(g_xcbo2_d[l_ac].xcbocrtid)
      #LET g_xcbo2_d[l_ac].xcbocrtdp_desc = cl_get_deptname(g_xcbo2_d[l_ac].xcbocrtdp)
      #LET g_xcbo2_d[l_ac].xcbomodid_desc = cl_get_username(g_xcbo2_d[l_ac].xcbomodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_xcbo_d.deleteElement(g_xcbo_d.getLength())
      CALL g_xcbo2_d.deleteElement(g_xcbo2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE axci130_pb   
   
END FUNCTION
]]>
</section>
