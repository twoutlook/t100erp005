  <section id="axci130.delete" ver="9" status="" src="s">
<![CDATA[#+ 資料刪除
PRIVATE FUNCTION axci130_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_xcbo_m.xcbosite IS NULL
   OR g_xcbo_m.xcbo001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE axci130_master_referesh USING g_xcbo_m.xcbosite,g_xcbo_m.xcbo001 INTO g_xcbo_m.xcbosite,g_xcbo_m.xcbo001, 
       g_xcbo_m.xcbosite_desc
   
   CALL axci130_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN axci130_cl USING g_enterprise,g_xcbo_m.xcbosite
                                                       ,g_xcbo_m.xcbo001
 
   IF STATUS THEN
      CALL cl_err("OPEN axci130_cl:", STATUS, 1)
      CLOSE axci130_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH axci130_cl INTO g_xcbo_m.xcbosite,g_xcbo_m.xcbosite_desc,g_xcbo_m.xcbo001,g_xcbo_m.imag014, 
       g_xcbo_m.imaal003
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_xcbo_m.xcbosite,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL axci130_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM xcbo_t WHERE xcboent = g_enterprise AND xcbosite = g_xcbo_m.xcbosite
                                                               AND xcbo001 = g_xcbo_m.xcbo001
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("xcbo_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_xcbo_d.clear() 
      CALL g_xcbo2_d.clear()       
 
     
      CALL axci130_ui_browser_refresh()  
      CALL axci130_ui_headershow()  
      CALL axci130_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL axci130_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL axci130_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE axci130_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_xcbo_m.xcbosite,'D')
    
END FUNCTION
]]>
</section>
