  <section id="axci130.construct" ver="8" status="" src="s">
<![CDATA[#+ QBE資料查詢
PRIVATE FUNCTION axci130_construct()
   DEFINE ls_return   STRING
   DEFINE ls_result   STRING 
   DEFINE ls_wc       STRING 
   #add-point:cs段define
   {<point name="cs.define"/>}
   #end add-point    
 
   #清除畫面上相關資料
   CLEAR FORM                 
   INITIALIZE g_xcbo_m.* TO NULL
   CALL g_xcbo_d.clear()
   CALL g_xcbo2_d.clear()
 
   INITIALIZE g_wc TO NULL
   INITIALIZE g_wc2 TO NULL
   LET g_action_choice = ""
    
   LET g_qryparam.state = 'c'
   
   #add-point:cs段construct外
   {<point name="cs.head.before"/>}
   #end add-point 
   
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
      
      #單頭
      CONSTRUCT BY NAME g_wc ON xcbosite,xcbo001
 
         BEFORE CONSTRUCT
            #add-point:cs段more_construct
            {<point name="cs.head.before_construct"/>}
            #end add-point 
            
                 #Ctrlp:construct.c.xcbosite
         ON ACTION controlp INFIELD xcbosite
            #add-point:ON ACTION controlp INFIELD xcbosite
            {<point name="construct.c.xcbosite" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD xcbosite
            #add-point:BEFORE FIELD xcbosite
            {<point name="construct.b.xcbosite" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD xcbosite
            
            #add-point:AFTER FIELD xcbosite
            {<point name="construct.a.xcbosite" />}
            #END add-point
            
 
         #Ctrlp:construct.c.xcbo001
         ON ACTION controlp INFIELD xcbo001
            #add-point:ON ACTION controlp INFIELD xcbo001
            {<point name="construct.c.xcbo001" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD xcbo001
            #add-point:BEFORE FIELD xcbo001
            {<point name="construct.b.xcbo001" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD xcbo001
            
            #add-point:AFTER FIELD xcbo001
            {<point name="construct.a.xcbo001" />}
            #END add-point
            
 
 
         
      END CONSTRUCT
 
      CONSTRUCT g_wc2_table1 ON xcbo002,xcbo002_desc,xcbo003,xcbo004,xcboownid,xcboowndp,xcbocrtid,xcbocrtdp, 
          xcbocrtdt,xcbomodid,xcbomoddt
           FROM s_detail1[1].xcbo002,s_detail1[1].xcbo002_desc,s_detail1[1].xcbo003,s_detail1[1].xcbo004, 
               s_detail2[1].xcboownid,s_detail2[1].xcboowndp,s_detail2[1].xcbocrtid,s_detail2[1].xcbocrtdp, 
               s_detail2[1].xcbocrtdt,s_detail2[1].xcbomodid,s_detail2[1].xcbomoddt
                      
         BEFORE CONSTRUCT
            #add-point:cs段more_construct
            {<point name="cs.body.before_construct"/>}
            #end add-point 
            
         #單身公用欄位開窗相關處理
         #此段落由子樣板a11產生
         ##----<<xcbocrtdt>>----
         AFTER FIELD xcbocrtdt
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
         
         #----<<xcbomoddt>>----
         AFTER FIELD xcbomoddt
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
         
         #----<<xcbocnfdt>>----
         
         #----<<xcbopstdt>>----
 
 
           
         #單身一般欄位開窗相關處理
            
       
      END CONSTRUCT
  
 
  
      #add-point:cs段more_construct
      {<point name="cs.more_construct"/>}
      #end add-point
 
      BEFORE DIALOG
         CALL cl_qbe_init()
         #add-point:ui_dialog段b_dialog
         {<point name="cs.before_dialog"/>}
         #end add-point
      
      #查詢方案列表
      ON ACTION qbe_select
         LET ls_wc = ""
         CALL cl_qbe_list("c") RETURNING ls_wc
    
      #條件儲存為方案
      ON ACTION qbe_save
         CALL cl_qbe_save()
 
      ON ACTION accept
         ACCEPT DIALOG
 
      ON ACTION cancel
         LET INT_FLAG = 1
         EXIT DIALOG 
 
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE DIALOG
   END DIALOG
   
   #add-point:cs段after_construct
   {<point name="cs.after_construct"/>}
   #end add-point
   
   #組合g_wc2
   LET g_wc2 = g_wc2_table1
 
 
 
   
   LET g_current_row = 1
 
   IF INT_FLAG THEN
      RETURN
   END IF
   
   LET g_wc_filter = ""
 
END FUNCTION
]]>
