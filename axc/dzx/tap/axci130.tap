  <point name="input.a.page1.xcbo002" cite_std="N" status="" ver="1" src="s" new="N" mark_hard="N">
<![CDATA[            #此段落由子樣板a05產生
            IF  g_xcbo_m.xcbosite IS NOT NULL AND g_xcbo_m.xcbo001 IS NOT NULL AND g_xcbo_d[g_detail_idx].xcbo002 IS NOT NULL THEN 
               IF l_cmd = 'a' OR ( l_cmd = 'u' AND (g_xcbo_m.xcbosite != g_xcbosite_t OR g_xcbo_m.xcbo001 != g_xcbo001_t OR g_xcbo_d[g_detail_idx].xcbo002 != g_xcbo_d_t.xcbo002)) THEN 
                  IF NOT ap_chk_notDup("","SELECT COUNT(*) FROM xcbo_t WHERE "||"xcboent = '" ||g_enterprise|| "' AND "||"xcbosite = '"||g_xcbo_m.xcbosite ||"' AND "|| "xcbo001 = '"||g_xcbo_m.xcbo001 ||"' AND "|| "xcbo002 = '"||g_xcbo_d[g_detail_idx].xcbo002 ||"'",'std-00004',0) THEN 
                     NEXT FIELD CURRENT
                  END IF
               END IF
            END IF
            IF NOT cl_null(g_xcbo_d[g_detail_idx].xcbo002) AND NOT cl_null(g_xcbo_m.xcbosite) THEN
            #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
               INITIALIZE g_chkparam.* TO NULL
			   
               #設定g_chkparam.*的參數
               LET g_chkparam.arg1 = g_xcbo_m.xcbosite
               LET g_chkparam.arg2 = g_xcbo_d[g_detail_idx].xcbo002
			   
               #呼叫檢查存在並帶值的library
               IF cl_chk_exist('v_imag001') THEN
               ELSE
                  #檢查失敗時後續處理
                  LET g_xcbo_d[g_detail_idx].xcbo002 = g_xcbo_d_t.xcbo002
                  SELECT imaal003 INTO g_xcbo_d[g_detail_idx].xcbo002_desc FROM imaal_t
                   WHERE imaalent = g_enterprise AND imaal001 = g_xcbo_d[g_detail_idx].xcbo002 AND imaal002 = g_dlang
                  SELECT imag014 INTO g_xcbo_d[g_detail_idx].xcbo003 FROM imag_t
                   WHERE imagent = g_enterprise AND imag001 = g_xcbo_d[g_detail_idx].xcbo002
                     AND imagsite = g_xcbo_m.xcbosite
                  NEXT FIELD xcbo002 
               END IF
            END IF
            SELECT imaal003 INTO g_xcbo_d[g_detail_idx].xcbo002_desc FROM imaal_t
             WHERE imaalent = g_enterprise AND imaal001 = g_xcbo_d[g_detail_idx].xcbo002 AND imaal002 = g_dlang
            SELECT imag014 INTO g_xcbo_d[g_detail_idx].xcbo003 FROM imag_t
             WHERE imagent = g_enterprise AND imag001 = g_xcbo_d[g_detail_idx].xcbo002
               AND imagsite = g_xcbo_m.xcbosite]]>
</point>
  <point name="input.a.page1.xcbo004" cite_std="N" status="" ver="1" src="s" new="N" mark_hard="N">
<![CDATA[            IF NOT cl_null(g_xcbo_d[l_ac].xcbo004) THEN 
