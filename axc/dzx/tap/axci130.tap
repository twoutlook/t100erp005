  <section id="axci130.input" ver="7" status="" src="s">
<![CDATA[#+ 資料輸入
PRIVATE FUNCTION axci130_input(p_cmd)
   DEFINE  p_cmd                 LIKE type_t.chr1
   DEFINE  l_cmd_t               LIKE type_t.chr1
   DEFINE  l_cmd                 LIKE type_t.chr1
   DEFINE  l_ac_t                LIKE type_t.num5                #未取消的ARRAY CNT 
   DEFINE  l_n                   LIKE type_t.num5                #檢查重複用  
   DEFINE  l_cnt                 LIKE type_t.num5                #檢查重複用  
   DEFINE  l_lock_sw             LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert        LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete        LIKE type_t.num5                #可刪除否  
   DEFINE  l_count               LIKE type_t.num5
   DEFINE  l_i                   LIKE type_t.num5
   DEFINE  l_insert              BOOLEAN
   DEFINE  ls_return             STRING
   DEFINE  l_var_keys            DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys          DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                DYNAMIC ARRAY OF STRING
   DEFINE  l_fields              DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak        DYNAMIC ARRAY OF STRING
   DEFINE  lb_reproduce          BOOLEAN
   DEFINE  li_reproduce          LIKE type_t.num5
   DEFINE  li_reproduce_target   LIKE type_t.num5
   #add-point:input段define
   {<point name="input.define"/>}
   #end add-point    
   
   #先做狀態判定
   IF p_cmd = 'r' THEN
      LET l_cmd_t = 'r'
      LET p_cmd   = 'a'
   ELSE
      LET l_cmd_t = p_cmd
   END IF   
   
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF  
 
   CALL cl_set_head_visible("","YES")  
 
   LET l_insert = FALSE
   LET g_action_choice = ""
 
   #add-point:input段define_sql
   {<point name="input.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT xcbo002,xcbo003,xcbo004,xcbo002,xcboownid,xcboowndp,xcbocrtid,xcbocrtdp, 
       xcbocrtdt,xcbomodid,xcbomoddt FROM xcbo_t WHERE xcboent=? AND xcbosite=? AND xcbo001=? AND xcbo002=?  
       FOR UPDATE"
   #add-point:input段define_sql
   {<point name="input.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE axci130_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR
 
 
   
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   LET g_qryparam.state = 'i'
   
   #控制key欄位可否輸入
   CALL axci130_set_entry(p_cmd)
   #add-point:set_entry後
   {<point name="input.after_set_entry"/>}
   #end add-point
   CALL axci130_set_no_entry(p_cmd)
   #add-point:set_no_entry後
   {<point name="input.after_set_no_entry"/>}
   #end add-point
 
   DISPLAY BY NAME g_xcbo_m.xcbosite,g_xcbo_m.xcbo001
   
   LET lb_reproduce = FALSE
   
   #add-point:進入修改段前
   {<point name="input.before_input"/>}
   #end add-point
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
]]>
</section>
  <section id="axci130.input.body" ver="12" status="" src="s">
<![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_xcbo_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂單身ACTION
         
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_xcbo_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL axci130_b_fill(g_wc2) 
            IF g_rec_b != 0 THEN
               CALL fgl_set_arr_curr(l_ac)
            END IF
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN axci130_cl USING g_enterprise,
                                               g_xcbo_m.xcbosite
                                               ,g_xcbo_m.xcbo001
 
                                               
               IF STATUS THEN
                  CALL cl_err("OPEN axci130_cl:", STATUS, 1)
                  CLOSE axci130_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_xcbo_d[l_ac].xcbo002 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_xcbo_d_t.* = g_xcbo_d[l_ac].*  #BACKUP
               CALL axci130_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL axci130_set_no_entry_b(l_cmd)
               OPEN axci130_bcl USING g_enterprise,g_xcbo_m.xcbosite,
                                                g_xcbo_m.xcbo001,
 
                                                g_xcbo_d_t.xcbo002
 
               IF STATUS THEN
                  CALL cl_err("OPEN axci130_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH axci130_bcl INTO g_xcbo_d[l_ac].xcbo002,g_xcbo_d[l_ac].xcbo003,g_xcbo_d[l_ac].xcbo004 

                  IF SQLCA.sqlcode THEN
                      CALL cl_err(g_xcbo_d_t.xcbo002,SQLCA.sqlcode,1)
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL axci130_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
        
         BEFORE INSERT
            
            INITIALIZE g_xcbo_d_t.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_xcbo_d[l_ac].* TO NULL
            
            #公用欄位預設值
            #此段落由子樣板a14產生    
      LET g_xcbo2_d[l_ac].xcboownid = g_user
      LET g_xcbo2_d[l_ac].xcboowndp = g_dept
      LET g_xcbo2_d[l_ac].xcbocrtid = g_user
      LET g_xcbo2_d[l_ac].xcbocrtdp = g_dept 
      LET g_xcbo2_d[l_ac].xcbocrtdt = cl_get_current()
      LET g_xcbo2_d[l_ac].xcbomodid = ""
      LET g_xcbo2_d[l_ac].xcbomoddt = ""
 
  
            
            #一般欄位預設值
            
            
            
            LET g_xcbo_d_t.* = g_xcbo_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL axci130_set_entry_b(l_cmd)
            #add-point:set_entry_b後
            {<point name="input.body.before_insert.set_entry_b"/>}
            #end add-point
            CALL axci130_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_xcbo_d[li_reproduce_target].* = g_xcbo_d[li_reproduce].*
               LET g_xcbo2_d[li_reproduce_target].* = g_xcbo2_d[li_reproduce].*
 
               LET g_xcbo_d[g_xcbo_d.getLength()].xcbo002 = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
 
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM xcbo_t 
             WHERE xcboent = g_enterprise AND xcbosite = g_xcbo_m.xcbosite
               AND xcbo001 = g_xcbo_m.xcbo001
 
               AND xcbo002 = g_xcbo_d[l_ac].xcbo002
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               CALL s_transaction_begin()
               #add-point:單身新增前
               {<point name="input.body.b_insert" mark="Y"/>}
               #end add-point
               INSERT INTO xcbo_t
                           (xcboent,
                            xcbosite,xcbo001,
                            xcbo002
                            ,xcbo003,xcbo004,xcboownid,xcboowndp,xcbocrtid,xcbocrtdp,xcbocrtdt,xcbomodid,xcbomoddt) 
                     VALUES(g_enterprise,
                            g_xcbo_m.xcbosite,g_xcbo_m.xcbo001,
                            g_xcbo_d[l_ac].xcbo002
                            ,g_xcbo_d[l_ac].xcbo003,g_xcbo_d[l_ac].xcbo004,g_xcbo2_d[l_ac].xcboownid, 
                                g_xcbo2_d[l_ac].xcboowndp,g_xcbo2_d[l_ac].xcbocrtid,g_xcbo2_d[l_ac].xcbocrtdp, 
                                g_xcbo2_d[l_ac].xcbocrtdt,g_xcbo2_d[l_ac].xcbomodid,g_xcbo2_d[l_ac].xcbomoddt) 

               #add-point:單身新增中
               {<point name="input.body.m_insert"/>}
               #end add-point
               LET p_cmd = 'u'
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_xcbo_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("xcbo_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR "INSERT O.K"
               LET g_rec_b=g_rec_b+1
               DISPLAY g_rec_b TO FORMONLY.cnt
            END IF
            
            #add-point:單身新增後
            {<point name="input.body.after_insert"/>}
            #end add-point
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_xcbo_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_xcbo_d.deleteElement(l_ac)
               NEXT FIELD xcbo002
            END IF
            IF g_xcbo_d_t.xcbo002 IS NOT NULL
 
               THEN
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete"/>}
               #end add-point
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               IF axci130_before_delete() THEN 
                  CALL s_transaction_end('Y','0')
               ELSE 
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               END IF 
               CLOSE axci130_bcl
               LET l_count = g_xcbo_d.getLength()
            END IF 
            
            #add-point:單身刪除後
            {<point name="input.body.a_delete"/>}
            #end add-point
              
         AFTER DELETE 
            #add-point:單身AFTER DELETE 
            {<point name="input.body.after_delete"/>}
            #end add-point
 
                  #此段落由子樣板a01產生
         BEFORE FIELD xcbo002
            #add-point:BEFORE FIELD xcbo002
            {<point name="input.b.page1.xcbo002" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD xcbo002
            
            #add-point:AFTER FIELD xcbo002
            {<point name="input.a.page1.xcbo002" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE xcbo002
            #add-point:ON CHANGE xcbo002
            {<point name="input.g.page1.xcbo002" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD xcbo002_desc
            #add-point:BEFORE FIELD xcbo002_desc
            {<point name="input.b.page1.xcbo002_desc" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD xcbo002_desc
            
            #add-point:AFTER FIELD xcbo002_desc
            {<point name="input.a.page1.xcbo002_desc" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE xcbo002_desc
            #add-point:ON CHANGE xcbo002_desc
            {<point name="input.g.page1.xcbo002_desc" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD xcbo003
            #add-point:BEFORE FIELD xcbo003
            {<point name="input.b.page1.xcbo003" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD xcbo003
            
            #add-point:AFTER FIELD xcbo003
            {<point name="input.a.page1.xcbo003" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE xcbo003
            #add-point:ON CHANGE xcbo003
            {<point name="input.g.page1.xcbo003" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD xcbo004
            #此段落由子樣板a15產生
            IF NOT cl_ap_chk_Range(g_xcbo_d[l_ac].xcbo004,"0","1","100","1","azz-00087",1) THEN
               NEXT FIELD xcbo004
            END IF
 
 
            #add-point:AFTER FIELD xcbo004
            {<point name="input.a.page1.xcbo004" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD xcbo004
            #add-point:BEFORE FIELD xcbo004
            {<point name="input.b.page1.xcbo004" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE xcbo004
            #add-point:ON CHANGE xcbo004
            {<point name="input.g.page1.xcbo004" />}
            #END add-point
 
 
                  #Ctrlp:input.c.page1.xcbo002
         ON ACTION controlp INFIELD xcbo002
            #add-point:ON ACTION controlp INFIELD xcbo002
            {<point name="input.c.page1.xcbo002" />}
            #END add-point
 
         #Ctrlp:input.c.page1.xcbo002_desc
         ON ACTION controlp INFIELD xcbo002_desc
            #add-point:ON ACTION controlp INFIELD xcbo002_desc
            {<point name="input.c.page1.xcbo002_desc" />}
            #END add-point
 
         #Ctrlp:input.c.page1.xcbo003
         ON ACTION controlp INFIELD xcbo003
            #add-point:ON ACTION controlp INFIELD xcbo003
            {<point name="input.c.page1.xcbo003" />}
            #END add-point
 
         #Ctrlp:input.c.page1.xcbo004
         ON ACTION controlp INFIELD xcbo004
            #add-point:ON ACTION controlp INFIELD xcbo004
            {<point name="input.c.page1.xcbo004" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_xcbo_d[l_ac].* = g_xcbo_d_t.*
               CLOSE axci130_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_xcbo_d[l_ac].xcbo002,-263,1)
               LET g_xcbo_d[l_ac].* = g_xcbo_d_t.*
            ELSE
               #寫入修改者/修改日期資訊
               LET g_xcbo2_d[l_ac].xcbomodid = g_user 
LET g_xcbo2_d[l_ac].xcbomoddt = cl_get_current()
 
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
         
               UPDATE xcbo_t SET (xcbosite,xcbo001,xcbo002,xcbo003,xcbo004,xcboownid,xcboowndp,xcbocrtid, 
                   xcbocrtdp,xcbocrtdt,xcbomodid,xcbomoddt) = (g_xcbo_m.xcbosite,g_xcbo_m.xcbo001,g_xcbo_d[l_ac].xcbo002, 
                   g_xcbo_d[l_ac].xcbo003,g_xcbo_d[l_ac].xcbo004,g_xcbo2_d[l_ac].xcboownid,g_xcbo2_d[l_ac].xcboowndp, 
                   g_xcbo2_d[l_ac].xcbocrtid,g_xcbo2_d[l_ac].xcbocrtdp,g_xcbo2_d[l_ac].xcbocrtdt,g_xcbo2_d[l_ac].xcbomodid, 
                   g_xcbo2_d[l_ac].xcbomoddt)
                WHERE xcboent = g_enterprise AND xcbosite = g_xcbo_m.xcbosite 
                 AND xcbo001 = g_xcbo_m.xcbo001 
 
                 AND xcbo002 = g_xcbo_d_t.xcbo002 #項次   
 
                 
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
               
               CASE
