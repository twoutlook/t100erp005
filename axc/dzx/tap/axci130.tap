   {<point name="reproduce.define"/>}
   #end add-point
 
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF     
 
   IF g_xcbo_m.xcbosite IS NULL
      OR g_xcbo_m.xcbo001 IS NULL
 
      THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   LET g_xcbosite_t = g_xcbo_m.xcbosite
   LET g_xcbo001_t = g_xcbo_m.xcbo001
 
   
   LET g_xcbo_m.xcbosite = ""
   LET g_xcbo_m.xcbo001 = ""
 
    
   CALL axci130_set_entry('a')
   CALL axci130_set_no_entry('a')
   
   CALL cl_set_head_visible("","YES")
   
   CALL s_transaction_begin()
   
   #add-point:複製輸入前
   {<point name="reproduce.head.b_input"/>}
   #end add-point
   
   CALL axci130_input("r")
   
      LET g_xcbo_m.xcbosite_desc = ''
   DISPLAY BY NAME g_xcbo_m.xcbosite_desc
 
   
   IF INT_FLAG THEN
      LET INT_FLAG = 0
      RETURN
   END IF
   
   LET g_state = "Y"
   LET g_current_idx = 1 
   
   LET g_wc = g_wc,  
              " OR (",
              " xcbosite = '", l_newno CLIPPED, "' "
              ," AND xcbo001 = '", l_newno02 CLIPPED, "' "
 
              , ") "
   
   #add-point:完成複製段落後
   {<point name="reproduce.after_reproduce" />}
   #end add-point
   
END FUNCTION
