               " LEFT JOIN oocal_t t4 ON t4.oocalent="||g_enterprise||" AND t4.oocal001=indf007 AND t4.oocal002='"||g_dlang||"' ",
               " LEFT JOIN inayl_t t5 ON t5.inaylent="||g_enterprise||" AND t5.inayl001=indf022 AND t5.inayl002='"||g_dlang||"' ",
               " LEFT JOIN inab_t t6 ON t6.inabent="||g_enterprise||" AND t6.inabsite=indfsite AND t6.inab001=indf022 AND t6.inab002=indf023  ",
               " LEFT JOIN inayl_t t7 ON t7.inaylent="||g_enterprise||" AND t7.inayl001=indf032 AND t7.inayl002='"||g_dlang||"' ",
               " LEFT JOIN inab_t t8 ON t8.inabent="||g_enterprise||" AND t8.inabsite=indfsite AND t8.inab001=indf032 AND t8.inab002=indf033  ",
               " LEFT JOIN oocql_t t9 ON t9.oocqlent="||g_enterprise||" AND t9.oocql001='2102' AND t9.oocql002=indf042 AND t9.oocql003='"||g_dlang||"' ",
 
                     " WHERE indfent=? AND indfdocno=?"
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before name="b_fill.body.fill_sql"
 
         #end add-point
         IF NOT cl_null(g_wc2_table1) THEN
            LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY indf_t.indfseq"
         
         #add-point:單身填充控制 name="b_fill.sql"
         
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE aint520_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR aint520_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
   #  OPEN b_fill_cs USING g_enterprise,g_inde_m.indedocno   #(ver:78)
                                               
      FOREACH b_fill_cs USING g_enterprise,g_inde_m.indedocno INTO g_indf_d[l_ac].indfsite,g_indf_d[l_ac].indfunit, 
          g_indf_d[l_ac].indfseq,g_indf_d[l_ac].indf001,g_indf_d[l_ac].indf009,g_indf_d[l_ac].indf003, 
          g_indf_d[l_ac].indf002,g_indf_d[l_ac].indf004,g_indf_d[l_ac].indf005,g_indf_d[l_ac].indf006, 
          g_indf_d[l_ac].indf007,g_indf_d[l_ac].indf008,g_indf_d[l_ac].indf020,g_indf_d[l_ac].indf021, 
          g_indf_d[l_ac].indf022,g_indf_d[l_ac].indf023,g_indf_d[l_ac].indf024,g_indf_d[l_ac].indf102, 
          g_indf_d[l_ac].indf030,g_indf_d[l_ac].indf031,g_indf_d[l_ac].indf032,g_indf_d[l_ac].indf033, 
          g_indf_d[l_ac].indf034,g_indf_d[l_ac].indf040,g_indf_d[l_ac].indf041,g_indf_d[l_ac].indf045, 
          g_indf_d[l_ac].indf046,g_indf_d[l_ac].indf047,g_indf_d[l_ac].indf048,g_indf_d[l_ac].indf042, 
          g_indf_d[l_ac].indf043,g_indf_d[l_ac].indf002_desc,g_indf_d[l_ac].indf002_desc_desc,g_indf_d[l_ac].indf006_desc, 
          g_indf_d[l_ac].indf007_desc,g_indf_d[l_ac].indf022_desc,g_indf_d[l_ac].indf023_desc,g_indf_d[l_ac].indf032_desc, 
          g_indf_d[l_ac].indf033_desc,g_indf_d[l_ac].indf042_desc   #(ver:78)
         IF SQLCA.SQLCODE THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code = SQLCA.SQLCODE 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充 name="b_fill.fill"
         
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code = 9035 
               LET g_errparam.popup = TRUE 
               CALL cl_err()
            END IF
            EXIT FOREACH
         END IF
         
         LET l_ac = l_ac + 1
      END FOREACH
      LET g_error_show = 0
   
   END IF
    
 
   
   #add-point:browser_fill段其他table處理 name="browser_fill.other_fill"
   
   #end add-point
   
   CALL g_indf_d.deleteElement(g_indf_d.getLength())
 
   
 
   LET l_ac = g_cnt
