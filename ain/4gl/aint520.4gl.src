   #LET g_wc2_table1 = l_wc2_table1
 
 
    
   WHILE TRUE
      LET g_indedocno_t = g_inde_m.indedocno
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_inde_m.indemodid = g_user 
LET g_inde_m.indemoddt = cl_get_current()
LET g_inde_m.indemodid_desc = cl_get_username(g_inde_m.indemodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      #「D抽單 / R已拒絕」狀況碼更改資料後，需還原為「N未確認」
      IF g_inde_m.indestus MATCHES "[DR]" THEN 
         LET g_inde_m.indestus = "N"
      END IF

      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      LET g_master_commit = "N"
      CALL aint520_input("u")
