      THEN
         LET g_sql = "SELECT  DISTINCT indfsite,indfunit,indfseq,indf001,indf009,indf003,indf002,indf004, 
             indf005,indf006,indf007,indf008,indf020,indf021,indf022,indf023,indf024,indf102,indf030, 
