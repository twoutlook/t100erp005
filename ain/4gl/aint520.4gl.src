   DEFINE l_master    RECORD LIKE inde_t.* #此變數樣板目前無使用
   DEFINE l_detail    RECORD LIKE indf_t.* #此變數樣板目前無使用
 
 
   DEFINE l_cnt       LIKE type_t.num10
   #add-point:reproduce段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="reproduce.define"
   DEFINE r_success     LIKE type_t.num5
   DEFINE r_doctype     LIKE rtai_t.rtai004
   #end add-point   
   
   #add-point:Function前置處理  name="reproduce.pre_function"
   
   #end add-point
   
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF
   
   LET g_master_insert = FALSE
   
   IF g_inde_m.indedocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code = "std-00003" 
      LET g_errparam.popup = FALSE 
      CALL cl_err()
      RETURN
   END IF
    
   LET g_indedocno_t = g_inde_m.indedocno
 
    
   LET g_inde_m.indedocno = ""
 
 
   CALL cl_set_head_visible("","YES")
 
   #公用欄位給予預設值
   #應用 a14 樣板自動產生(Version:5)    
      #公用欄位新增給值  
      LET g_inde_m.indeownid = g_user
      LET g_inde_m.indeowndp = g_dept
      LET g_inde_m.indecrtid = g_user
      LET g_inde_m.indecrtdp = g_dept 
      LET g_inde_m.indecrtdt = cl_get_current()
      LET g_inde_m.indemodid = g_user
      LET g_inde_m.indemoddt = cl_get_current()
      LET g_inde_m.indestus = 'N'
 
 
 
   
   CALL s_transaction_begin()
   
   #add-point:複製輸入前 name="reproduce.head.b_input"
   #预设单别
   LET r_success = ''
   LET r_doctype = ''
