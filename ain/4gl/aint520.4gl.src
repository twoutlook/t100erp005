             indf043 ,t1.imaal003 ,t2.imaal004 ,t3.oocal003 ,t4.oocal003 ,t5.inayl003 ,t6.inab003 ,t7.inayl003 , 
             t8.inab003 ,t9.oocql004 FROM indf_t",   
                     " INNER JOIN inde_t ON indeent = " ||g_enterprise|| " AND indedocno = indfdocno ",
 
                     #"",
                     
                     "",
