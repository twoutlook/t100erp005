         #end add-point 
         CALL aint520_show()
         RETURN
      END IF
      
      LET INT_FLAG = 0
      #CALL g_indf_d.clear()
 
 
      LET g_rec_b = 0
      CALL s_transaction_end('Y','0')
      EXIT WHILE
        
