      LET g_errparam.code = "std-00003" 
      LET g_errparam.popup = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_indedocno_t = g_inde_m.indedocno
 
   CALL s_transaction_begin()
   
   OPEN aint520_cl USING g_enterprise,g_inde_m.indedocno
   IF SQLCA.SQLCODE THEN   #(ver:78)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aint520_cl:",SQLERRMESSAGE 
