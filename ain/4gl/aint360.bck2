PRIVATE FUNCTION aint360_action_chk()
   #add-point:action_chk段define(客製用) name="action_chk.define_customerization"
   
   #end add-point
   #add-point:action_chk段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="action_chk.define"
   
   #end add-point
   
   #add-point:action_chk段action_chk name="action_chk.action_chk"
   #160818-00017#16-s
   SELECT indestus INTO g_inde_m.indestus FROM inde_t
    WHERE indeent = g_enterprise
      AND indesite = g_site
      AND indedocno = g_inde_m.indedocno
   IF (g_action_choice="modify" OR g_action_choice="delete" OR g_action_choice="modify_detail")  THEN
     LET g_errno = NULL
     CASE g_inde_m.indestus
        WHEN 'W'
           LET g_errno = 'sub-01347'
        WHEN 'X'
           LET g_errno = 'sub-00229'
        WHEN 'Y'
           LET g_errno = 'sub-00178'
        WHEN 'S'
           LET g_errno = 'sub-00230'
     END CASE

     IF NOT cl_null(g_errno) THEN
        INITIALIZE g_errparam TO NULL
        LET g_errparam.code = g_errno
        LET g_errparam.extend = g_inde_m.indedocno
        LET g_errparam.popup = TRUE
        CALL cl_err()
        LET g_errno = NULL
        CALL aint360_set_act_visible()
        CALL aint360_set_act_no_visible()
        CALL aint360_show()
        RETURN FALSE
     END IF
   END IF      
   #160818-00017#16-e
   #end add-point
      
   RETURN TRUE
   
END FUNCTION
 
