################################################################################
# Descriptions...: 自動產生單身
# Memo...........:
# Usage..........: CALL aint360_auto_gene_detail()
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By lixh
# Modify.........:
################################################################################
PRIVATE FUNCTION aint360_auto_gene_detail()
DEFINE   l_sql      STRING 
DEFINE   l_indd     RECORD LIKE indd_t.*
DEFINE   l_indf     RECORD LIKE indf_t.*
DEFINE   l_i        LIKE type_t.num5
DEFINE   r_success  LIKE type_t.num5
DEFINE   r_rate     LIKE inaj_t.inaj014

   LET l_sql = " SELECT * FROM indd_t ",
               "  WHERE inddent = '",g_enterprise,"'",
               "    AND inddsite = '",g_site,"'",
               "    AND indddocno = '",g_inde_m.inde001,"'"              
   PREPARE aint360_pre_01 FROM l_sql
   DECLARE aint360_indf_01 CURSOR FOR aint360_pre_01   
   LET l_i = 1     
   FOREACH aint360_indf_01 INTO l_indd.*
      #160202-00004#2 add by lixh 20160203
      LET l_indf.indf040 = l_indd.indd110
      IF l_indf.indf040 = 0 THEN
         CONTINUE FOREACH
      END IF 
      #160202-00004#2 add by lixh 20160203
      LET l_indf.indfent = g_enterprise
      LET l_indf.indfsite = g_site
      LET l_indf.indfdocno = g_inde_m.indedocno
      LET l_indf.indfseq = l_i
      LET l_indf.indf101 = l_indd.indddocno
      LET l_indf.indf001 = l_indd.inddseq
      LET l_indf.indf002 = l_indd.indd002
      LET l_indf.indf004 = l_indd.indd004
      LET l_indf.indf102 = l_indd.indd102
      LET l_indf.indf024 = l_indd.indd024
      LET l_indf.indf042 = l_indd.indd111
      LET l_indf.indf007 = l_indd.indd006
      LET l_indf.indf040 = l_indd.indd110
      LET l_indf.indf034 = l_indd.indd034
      LET l_indf.indf022 = l_indd.indd022
      LET l_indf.indf023 = l_indd.indd023
      LET l_indf.indf104 = l_indd.indd104
      LET l_indf.indf044 = l_indd.indd041 
      IF l_indf.indf042 = '1' THEN   #撥出方
      　 LET l_indf.indf041 = l_indd.indd110 - l_indd.indd112
      END IF
      IF l_indf.indf042 = '2' THEN   #撥入方
      　 LET l_indf.indf103 = l_indd.indd110 - l_indd.indd112
      END IF  
      IF NOT cl_null(l_indf.indf104) AND NOT cl_null(l_indf.indf007) THEN
#         CALL s_aimi190_get_convert(l_indf.indf002,l_indf.indf007,l_indf.indf104) 
#              RETURNING r_success,r_rate     #xj mod
         IF cl_null(l_indf.indf007) THEN
            CALL s_aooi250_convert_qty(l_indf.indf002,l_indf.indf007,l_indf.indf104,l_indf.indf041)
               RETURNING r_success,l_indf.indf105 
            IF r_success THEN           
#               LET l_indf.indf105 = l_indf.indf041 * r_rate  # xj mod
               CALL aint360_unit_round(l_indf.indf104,l_indf.indf105) RETURNING l_indf.indf105      
               LET l_indf.indf106 = l_indf.indf103 * r_rate 
               CALL aint360_unit_round(l_indf.indf104,l_indf.indf106) RETURNING l_indf.indf106             
            END IF 
         END IF         
      END IF       
      INSERT INTO indf_t
                  (indfent,indfsite,
                   indfdocno,
                   indfseq,
                   indf101,indf001,indf002,indf004,indf102,indf024,indf042,indf007,indf040,indf041,indf104,indf105,indf022,indf023,indf103,indf106,indf032,indf033,indf043,indf044) 
            VALUES(g_enterprise,l_indf.indfsite,
                   g_inde_m.indedocno,l_indf.indfseq,l_indf.indf101,l_indf.indf001,l_indf.indf002,l_indf.indf004,l_indf.indf102,
                   l_indf.indf024,l_indf.indf042,l_indf.indf007,l_indf.indf040,l_indf.indf041,l_indf.indf104,l_indf.indf105,
                   l_indf.indf022,l_indf.indf023,l_indf.indf103,l_indf.indf106,l_indf.indf032,l_indf.indf033,l_indf.indf043,l_indf.indf044)                   
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = 'insert indf_t'
         LET g_errparam.popup = TRUE
         CALL cl_err()

         EXIT FOREACH
      END IF      
      LET l_i = l_i + 1       
   END FOREACH   
END FUNCTION

################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL aint360_unit_round(p_indf104,p_indf105)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By lixh
# Modify.........:
################################################################################
PRIVATE FUNCTION aint360_unit_round(p_indf104,p_indf105)
DEFINE p_indf104   LIKE indf_t.indf104
DEFINE p_indf105   LIKE indf_t.indf105
DEFINE l_success   LIKE type_t.num5
DEFINE l_ooca002   LIKE ooca_t.ooca002      #小数位数
DEFINE l_ooca004   LIKE ooca_t.ooca004      #舍入类型 
DEFINE r_indf105   LIKE indf_t.indf105      #數量

       LET l_success = NULL
       LET l_ooca002 = 0
       LET l_ooca004 = NULL
       LET r_indf105 = 0
       
       IF cl_null(p_indf104) OR cl_null(p_indf105) THEN
          RETURN p_indf105
       END IF
       
       #抓取单位档中的小数位数和舍入类型
       IF NOT cl_null(p_indf104) THEN
          CALL s_aooi250_get_msg(p_indf104) RETURNING l_success,l_ooca002,l_ooca004
          IF l_success THEN
             IF NOT cl_null(p_indf105) THEN
                CALL s_num_round(l_ooca004,p_indf105,l_ooca002) RETURNING r_indf105
                RETURN r_indf105
             END IF
          END IF
       END IF
       RETURN r_indf105
END FUNCTION

################################################################################
# Descriptions...: 輸入數量後預帶其他相關數量
# Memo...........:
# Usage..........: CALL aint360_amount_default()
# Input parameter: 
# Return code....: 
# Date & Author..: 150526 By whitney
# Modify.........:
################################################################################
PRIVATE FUNCTION aint360_amount_default()
DEFINE l_success    LIKE type_t.num5

   IF g_indf_d[l_ac].indf041 = 0 OR cl_null(g_indf_d[l_ac].indf041) THEN
      LET g_indf_d[l_ac].indf022 = ''
      LET g_indf_d[l_ac].indf022_desc = ''
      LET g_indf_d[l_ac].indf023 = ''
      LET g_indf_d[l_ac].indf023_desc = ''
   END IF
   
   IF g_indf_d[l_ac].indf103 = 0 OR cl_null(g_indf_d[l_ac].indf103) THEN
      LET g_indf_d[l_ac].indf032 = ''
      LET g_indf_d[l_ac].indf032_desc = ''
      LET g_indf_d[l_ac].indf033 = ''
      LET g_indf_d[l_ac].indf033_desc = ''
   END IF
   
   IF cl_null(g_indf_d[l_ac].indf032) AND g_indf_d[l_ac].indf103 > 0 THEN
      SELECT indd032,indd033 INTO g_indf_d[l_ac].indf032,g_indf_d[l_ac].indf033
        FROM indd_t
       WHERE inddent = g_enterprise
         AND indddocno = g_indf_d[l_ac].indf101
         AND inddseq = g_indf_d[l_ac].indf001
      CALL s_desc_get_stock_desc(g_inde_m.inde004,g_indf_d[l_ac].indf032) RETURNING g_indf_d[l_ac].indf032_desc
      CALL s_desc_get_locator_desc(g_inde_m.inde004,g_indf_d[l_ac].indf032,g_indf_d[l_ac].indf033) RETURNING g_indf_d[l_ac].indf033_desc
   END IF
   
   IF cl_null(g_indf_d[l_ac].indf022) AND g_indf_d[l_ac].indf041 > 0 THEN
      SELECT indd022,indd023 INTO g_indf_d[l_ac].indf022,g_indf_d[l_ac].indf023
        FROM indd_t
       WHERE inddent = g_enterprise
         AND indddocno = g_indf_d[l_ac].indf101
         AND inddseq = g_indf_d[l_ac].indf001
      CALL s_desc_get_stock_desc(g_site,g_indf_d[l_ac].indf022) RETURNING g_indf_d[l_ac].indf022_desc
      CALL s_desc_get_locator_desc(g_site,g_indf_d[l_ac].indf022,g_indf_d[l_ac].indf023) RETURNING g_indf_d[l_ac].indf023_desc
   END IF
   
   IF NOT cl_null(g_indf_d[l_ac].indf002) AND NOT cl_null(g_indf_d[l_ac].indf104) THEN
      IF NOT cl_null(g_indf_d[l_ac].indf007) AND NOT cl_null(g_indf_d[l_ac].indf041) THEN
         CALl s_aooi250_convert_qty(g_indf_d[l_ac].indf002,g_indf_d[l_ac].indf007,g_indf_d[l_ac].indf104,g_indf_d[l_ac].indf041)
              RETURNING l_success,g_indf_d[l_ac].indf105
      END IF
      IF NOT cl_null(g_indf_d[l_ac].indf044) AND NOT cl_null(g_indf_d[l_ac].indf103) THEN
         CALl s_aooi250_convert_qty(g_indf_d[l_ac].indf002,g_indf_d[l_ac].indf044,g_indf_d[l_ac].indf104,g_indf_d[l_ac].indf103)
              RETURNING l_success,g_indf_d[l_ac].indf106                       
      END IF
   END IF

END FUNCTION

