   IF l_ac <> 0 THEN
      LET l_sql = " SELECT inaoseq,inaoseq2,inao001,'','',inao008,inao009,inao010,inao012 FROM inao_t ",
                  "  WHERE inaoent = ",g_enterprise,
                  "    AND inaodocno = '",g_inde_m.indedocno,"'",
                  "    AND inaoseq = ",g_indf_d[l_ac].indfseq,
                  "    AND inao000 = '2'",
                  "    AND inao013 = '1'"
                  
      LET l_sql2 = l_sql,"    AND inaoseq1 = 0 "
                                                  
      PREPARE aint360_inao_pre FROM l_sql2
      DECLARE aint360_inao_cs CURSOR FOR aint360_inao_pre
    
      LET l_sql2 = ''
      LET l_sql2 = l_sql,"    AND inaoseq1 = 1 "
   
    
      PREPARE aint360_inao_pre2 FROM l_sql2
      DECLARE aint360_inao_cs2 CURSOR FOR aint360_inao_pre2
      
      LET l_ac1 = 1
      FOREACH aint360_inao_cs INTO g_inao_d[l_ac1].*
      
         CALL s_desc_get_item_desc(g_inao_d[l_ac1].inao001) RETURNING g_inao_d[l_ac1].inao001_desc,g_inao_d[l_ac1].inao001_desc_desc
