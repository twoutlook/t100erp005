   #add by lixh 20150930
   #end add-point
   
   CALL g_indf_d.deleteElement(g_indf_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE aint360_pb
 
   
   LET li_idx = l_ac
   
   #遮罩相關處理
   FOR l_ac = 1 TO g_indf_d.getLength()
      LET g_indf_d_mask_o[l_ac].* =  g_indf_d[l_ac].*
      CALL aint360_indf_t_mask()
      LET g_indf_d_mask_n[l_ac].* =  g_indf_d[l_ac].*
