               AND inddsite = g_site
               AND indddocno = g_indf_d[l_ac].indf101
               AND inddseq = g_indf_d[l_ac].indf001
            IF NOT cl_null(l_indd032) THEN
               LET l_inaa010 = ''
               SELECT inaa010 INTO l_inaa010 FROM inaa_t
                WHERE inaaent = g_enterprise
                  AND inaasite = g_inde_m.inde004
                  AND inaa001 = l_indd032
               LET l_inaa010_new = '' 
               SELECT inaa010 INTO l_inaa010_new FROM inaa_t
                WHERE inaaent = g_enterprise
                  AND inaasite = g_inde_m.inde004
                  AND inaa001 = g_indf_d[l_ac].indf032
               IF l_inaa010 = l_inaa010_new THEN
               ELSE
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = ''
                  LET g_errparam.extend = g_indf_d[l_ac].indf032
                  LET g_errparam.popup = TRUE
                  CALL cl_err()

                  RETURN FALSE
               END IF
            END IF
            RETURN TRUE
END FUNCTION

