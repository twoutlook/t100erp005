# Modify.........:
################################################################################
PRIVATE FUNCTION aint360_indf101_chk()
DEFINE  l_cnt   LIKE type_t.num5
DEFINE  l_sql   STRING
   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt FROM indc_t
    WHERE indcent = g_enterprise
      AND indcsite = g_site
      AND indcdocno = g_indf_d[l_ac].indf101
      AND indc006 = g_inde_m.inde004
      AND indc007 = g_inde_m.inde005
      AND indc109 = g_inde_m.inde102
   IF cl_null(l_cnt) THEN LET l_cnt = 0 END IF
   IF l_cnt < 1 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'ain-00242'
      LET g_errparam.extend = g_indf_d[l_ac].indf101
      LET g_errparam.popup = TRUE
      CALL cl_err()

      RETURN FALSE      
   END IF   
   LET l_cnt = 0
   LET l_sql = " SELECT COUNT(*) FROM indd_t ",
               "  WHERE inddent = '",g_enterprise,"'",
               "    AND inddsite = '",g_site,"'",
               "    AND indddocno = '",g_indf_d[l_ac].indf101,"'",
               "    AND indd110 <> 0 ",
               "    AND indd112 <> indd110 "
   IF NOT cl_null(g_indf_d[l_ac].indf001) THEN
      LET l_sql = l_sql," AND inddseq = '",g_indf_d[l_ac].indf001,"'"
   END IF   
