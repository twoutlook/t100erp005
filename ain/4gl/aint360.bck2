               " LEFT JOIN inayl_t t6 ON t6.inaylent="||g_enterprise||" AND t6.inayl001=indf022 AND t6.inayl002='"||g_dlang||"' ",
               " LEFT JOIN inab_t t7 ON t7.inabent="||g_enterprise||" AND t7.inabsite=indfsite AND t7.inab001=indf022 AND t7.inab002=indf023  ",
               " LEFT JOIN inayl_t t8 ON t8.inaylent="||g_enterprise||" AND t8.inayl001=indf032 AND t8.inayl002='"||g_dlang||"' ",
               " LEFT JOIN inab_t t9 ON t9.inabent="||g_enterprise||" AND t9.inab001=indf032 AND t9.inab002=indf033  ",
 
                     " WHERE indfent=? AND indfdocno=?"
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before name="b_fill.body.fill_sql"
         
         #end add-point
         IF NOT cl_null(g_wc2_table1) THEN
            LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY indf_t.indfseq"
         
         #add-point:單身填充控制 name="b_fill.sql"
  
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE aint360_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR aint360_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_inde_m.indedocno
                                               
      FOREACH b_fill_cs INTO g_indf_d[l_ac].indfsite,g_indf_d[l_ac].indfseq,g_indf_d[l_ac].indf101,g_indf_d[l_ac].indf001, 
          g_indf_d[l_ac].indf002,g_indf_d[l_ac].indf004,g_indf_d[l_ac].indf024,g_indf_d[l_ac].indf102, 
          g_indf_d[l_ac].indf042,g_indf_d[l_ac].indf007,g_indf_d[l_ac].indf040,g_indf_d[l_ac].indf044, 
          g_indf_d[l_ac].indf041,g_indf_d[l_ac].indf104,g_indf_d[l_ac].indf105,g_indf_d[l_ac].indf022, 
          g_indf_d[l_ac].indf023,g_indf_d[l_ac].indf103,g_indf_d[l_ac].indf106,g_indf_d[l_ac].indf032, 
          g_indf_d[l_ac].indf033,g_indf_d[l_ac].indf043,g_indf_d[l_ac].indf002_desc,g_indf_d[l_ac].indf002_desc_desc, 
          g_indf_d[l_ac].indf007_desc,g_indf_d[l_ac].indf044_desc,g_indf_d[l_ac].indf104_desc,g_indf_d[l_ac].indf022_desc, 
          g_indf_d[l_ac].indf023_desc,g_indf_d[l_ac].indf032_desc,g_indf_d[l_ac].indf033_desc
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充 name="b_fill.fill"
         
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF
            EXIT FOREACH
         END IF
         
         LET l_ac = l_ac + 1
      END FOREACH
      LET g_error_show = 0
   
   END IF
    
 
   
   #add-point:browser_fill段其他table處理 name="browser_fill.other_fill"
   #add by lixh 20150930
   LET l_ac = g_cnt 
