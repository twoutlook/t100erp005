{<section id="aint360.state_change" >}
   #應用 a09 樣板自動產生(Version:17)
#+ 確認碼變更 
PRIVATE FUNCTION aint360_statechange()
   #add-point:statechange段define(客製用) name="statechange.define_customerization"
   
   #end add-point  
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="statechange.define"
   DEFINE r_success   LIKE type_t.num5
   #end add-point  
   
   #add-point:Function前置處理 name="statechange.before"
   
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_inde_m.indedocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN aint360_cl USING g_enterprise,g_inde_m.indedocno
   IF STATUS THEN
      CLOSE aint360_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aint360_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE aint360_master_referesh USING g_inde_m.indedocno INTO g_inde_m.indedocno,g_inde_m.indedocdt, 
       g_inde_m.inde002,g_inde_m.inde101,g_inde_m.indestus,g_inde_m.inde001,g_inde_m.inde004,g_inde_m.inde005, 
       g_inde_m.inde102,g_inde_m.inde006,g_inde_m.indesite,g_inde_m.indeownid,g_inde_m.indeowndp,g_inde_m.indecrtid, 
       g_inde_m.indecrtdp,g_inde_m.indecrtdt,g_inde_m.indemodid,g_inde_m.indemoddt,g_inde_m.indecnfid, 
       g_inde_m.indecnfdt,g_inde_m.inde002_desc,g_inde_m.inde101_desc,g_inde_m.inde004_desc,g_inde_m.inde005_desc, 
       g_inde_m.inde102_desc,g_inde_m.indeownid_desc,g_inde_m.indeowndp_desc,g_inde_m.indecrtid_desc, 
       g_inde_m.indecrtdp_desc,g_inde_m.indemodid_desc,g_inde_m.indecnfid_desc
   
 
   #檢查是否允許此動作
   IF NOT aint360_action_chk() THEN
      CLOSE aint360_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_inde_m.indedocno,g_inde_m.indedocdt,g_inde_m.inde002,g_inde_m.inde002_desc,g_inde_m.indedocno_desc, 
       g_inde_m.inde101,g_inde_m.inde101_desc,g_inde_m.indestus,g_inde_m.inde001,g_inde_m.inde004,g_inde_m.inde004_desc, 
       g_inde_m.inde005,g_inde_m.inde005_desc,g_inde_m.inde102,g_inde_m.inde102_desc,g_inde_m.inde006, 
       g_inde_m.indesite,g_inde_m.indeownid,g_inde_m.indeownid_desc,g_inde_m.indeowndp,g_inde_m.indeowndp_desc, 
       g_inde_m.indecrtid,g_inde_m.indecrtid_desc,g_inde_m.indecrtdp,g_inde_m.indecrtdp_desc,g_inde_m.indecrtdt, 
       g_inde_m.indemodid,g_inde_m.indemodid_desc,g_inde_m.indemoddt,g_inde_m.indecnfid,g_inde_m.indecnfid_desc, 
       g_inde_m.indecnfdt
 
   CASE g_inde_m.indestus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
      WHEN "X"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_inde_m.indestus
            
            WHEN "N"
               HIDE OPTION "unconfirmed"
            WHEN "Y"
               HIDE OPTION "confirmed"
            WHEN "X"
               HIDE OPTION "invalid"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      CALL cl_set_act_visible("unconfirmed,invalid,confirmed",TRUE)
      CALL cl_set_act_visible("signing,withdraw",FALSE)  #2015-4-24 zhujing add
      CASE g_inde_m.indestus
         WHEN "N"
            CALL cl_set_act_visible("unconfirmed",FALSE)
            #2015-4-24 zhujing add--(S)
            #需提交至BPM時，則顯示「提交」功能並隱藏「確認」功能
            IF cl_bpm_chk() THEN
               CALL cl_set_act_visible("signing",TRUE)
               CALL cl_set_act_visible("confirmed",FALSE)
            END IF
         WHEN "R"    #保留修改的功能(如作廢)，隱藏其他應用功能(如: 確認、未確認、留置、過帳…)
            CALL cl_set_act_visible("confirmed,unconfirmed,hold",FALSE)
         WHEN "D"    #保留修改的功能(如作廢)，隱藏其他應用功能(如: 確認、未確認、留置、過帳…)
            CALL cl_set_act_visible("confirmed,unconfirmed,hold",FALSE)
         WHEN "W"    #只能顯示抽單;其餘應用功能皆隱藏
            CALL cl_set_act_visible("withdraw",TRUE)
            CALL cl_set_act_visible("unconfirmed,invalid,confirmed,hold",FALSE)
         WHEN "A"    #只能顯示確認; 其餘應用功能皆隱藏
            CALL cl_set_act_visible("confirmed",TRUE)
            CALL cl_set_act_visible("unconfirmed,invalid,hold",FALSE)
            #2015-4-24 zhujing add--(E)
         WHEN "X"
            CALL cl_set_act_visible("invalid,confirmed,unconfirmed",FALSE)
            
         WHEN "Y"
            CALL cl_set_act_visible("invalid,confirmed",FALSE)        
            
      END CASE      
      #end add-point
      
      
	  
      ON ACTION unconfirmed
         IF cl_auth_chk_act("unconfirmed") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.unconfirmed"
            CALL s_transaction_begin()
            IF lc_state = 'N' THEN
               CALL s_aint360_unconf_chk(g_inde_m.indedocno) RETURNING r_success
               IF NOT r_success THEN
                  CALL s_transaction_end('N','0')   #160812-00017#2 20160815 add by beckxie
                  RETURN
               ELSE
                  IF NOT cl_ask_confirm('aim-00110') THEN
                     CALL s_transaction_end('N','0')   #160812-00017#2 20160815 add by beckxie
                     RETURN
                  ELSE
                     CALL s_aint360_unconf_upd(g_inde_m.indedocno) RETURNING r_success
                     IF NOT r_success THEN
                        CALL s_transaction_end('N','0')
                        RETURN
                     ELSE
                        CALL s_transaction_end('Y','0')
                     END IF
                  END IF
               END IF
            END IF
            #end add-point
         END IF
         EXIT MENU
      ON ACTION confirmed
         IF cl_auth_chk_act("confirmed") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.confirmed"
 
            #end add-point
         END IF
         EXIT MENU
      ON ACTION invalid
         IF cl_auth_chk_act("invalid") THEN
            LET lc_state = "X"
            #add-point:action控制 name="statechange.invalid"
            
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
 
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      AND lc_state <> "X"
      ) OR 
      g_inde_m.indestus = lc_state OR cl_null(lc_state) THEN
      CLOSE aint360_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
      CALL s_transaction_begin()
      IF lc_state = 'Y' THEN
         CALL s_aint360_conf_chk(g_inde_m.indedocno) RETURNING r_success
         IF NOT r_success THEN
            CALL s_transaction_end('N','0')   #160812-00017#2 20160815 add by beckxie
            RETURN
         ELSE
            IF NOT cl_ask_confirm('aim-00108') THEN
               CALL s_transaction_end('N','0')   #160812-00017#2 20160815 add by beckxie
               RETURN
            ELSE
               CALL s_aint360_conf_upd(g_inde_m.indedocno) RETURNING r_success
               IF NOT r_success THEN
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','0')
               END IF
            END IF
         END IF
      END IF
#      IF lc_state = 'N' THEN
#         CALL s_aint360_unconf_chk(g_inde_m.indedocno) RETURNING r_success
#         IF NOT r_success THEN
#            RETURN
#         ELSE
#            IF NOT cl_ask_confirm('aim-00110') THEN
#               RETURN
#            ELSE
#               CALL s_aint360_unconf_upd(g_inde_m.indedocno) RETURNING r_success
#               IF NOT r_success THEN
#                  CALL s_transaction_end('N','0')
#                  RETURN
#               ELSE
#                  CALL s_transaction_end('Y','0')
#               END IF
#            END IF
#         END IF
#      END IF
      IF lc_state = 'X' THEN
         CALL s_aint360_void_chk(g_inde_m.indedocno) RETURNING r_success
         IF NOT r_success THEN
            CALL s_transaction_end('N','0')   #160812-00017#2 20160815 add by beckxie
            RETURN
         ELSE
            IF NOT cl_ask_confirm('aim-00109') THEN
               CALL s_transaction_end('N','0')   #160812-00017#2 20160815 add by beckxie
               RETURN
            ELSE
               CALL s_aint360_void_upd(g_inde_m.indedocno) RETURNING r_success
               IF NOT r_success THEN
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','0')
               END IF
            END IF
         END IF
      END IF
   #end add-point
   
   LET g_inde_m.indemodid = g_user
   LET g_inde_m.indemoddt = cl_get_current()
   LET g_inde_m.indestus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE inde_t 
      SET (indestus,indemodid,indemoddt) 
        = (g_inde_m.indestus,g_inde_m.indemodid,g_inde_m.indemoddt)     
    WHERE indeent = g_enterprise AND indedocno = g_inde_m.indedocno
 
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE aint360_master_referesh USING g_inde_m.indedocno INTO g_inde_m.indedocno,g_inde_m.indedocdt, 
