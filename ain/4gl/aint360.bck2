      LET g_errparam.code = 'ain-00241'
      LET g_errparam.extend = g_indf_d[l_ac].indf101
      LET g_errparam.popup = TRUE
      CALL cl_err()

      RETURN FALSE
   END IF   
   RETURN TRUE
END FUNCTION

