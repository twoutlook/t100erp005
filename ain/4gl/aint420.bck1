{<section id="aint420.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION aint420_b_fill()
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   
   #end add-point     
   DEFINE p_wc2      STRING
   DEFINE li_idx     LIKE type_t.num10
   #add-point:b_fill段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   
   #end add-point     
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   #清空第一階單身
   CALL g_infh_d.clear()
 
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   
   #end add-point
   
   #判斷是否填充
   IF aint420_fill_chk(1) THEN
      #切換上下筆時不重組SQL
      IF (g_action_choice = "query" OR cl_null(g_action_choice))
      #add-point:b_fill段long_sql_if name="b_fill.long_sql_if"
      
      #end add-point
      THEN
         LET g_sql = "SELECT  DISTINCT infhseq,infh012,infh013,infh002,infh001,infh003,infh004,infh005, 
             infh006,infh007,infh008,infh009,infh010,infh011,infhsite,infhunit ,t1.imaal003 ,t2.imaal004 , 
             t4.oocal003 ,t5.infc002 ,t7.infc002 ,t9.infc002 FROM infh_t",   
                     " INNER JOIN infg_t ON infgent = " ||g_enterprise|| " AND infgdocno = infhdocno ",
 
                     #"",
                     
                     "",
                     #下層單身所需的join條件
 
                                    " LEFT JOIN imaal_t t1 ON t1.imaalent="||g_enterprise||" AND t1.imaal001=infh001 AND t1.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t2 ON t2.imaalent="||g_enterprise||" AND t2.imaal001=infh001 AND t2.imaal002='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t4 ON t4.oocalent="||g_enterprise||" AND t4.oocal001=infh004 AND t4.oocal002='"||g_dlang||"' ",
               " LEFT JOIN infc_t t5 ON t5.infcent="||g_enterprise||" AND t5.infcsite=infhsite AND t5.infc001=infh005  ",
               " LEFT JOIN infc_t t7 ON t7.infcent="||g_enterprise||" AND t7.infcsite=infhsite AND t7.infc001=infh006  ",
               " LEFT JOIN infc_t t9 ON t9.infcent="||g_enterprise||" AND t9.infcsite=infhsite AND t9.infc001=infh008  ",
 
                     " WHERE infhent=? AND infhdocno=?"
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before name="b_fill.body.fill_sql"
         
         #end add-point
         IF NOT cl_null(g_wc2_table1) THEN
            LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY infh_t.infhseq"
         
         #add-point:單身填充控制 name="b_fill.sql"
         
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE aint420_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR aint420_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
   #  OPEN b_fill_cs USING g_enterprise,g_infg_m.infgdocno   #(ver:78)
                                               
      FOREACH b_fill_cs USING g_enterprise,g_infg_m.infgdocno INTO g_infh_d[l_ac].infhseq,g_infh_d[l_ac].infh012, 
          g_infh_d[l_ac].infh013,g_infh_d[l_ac].infh002,g_infh_d[l_ac].infh001,g_infh_d[l_ac].infh003, 
          g_infh_d[l_ac].infh004,g_infh_d[l_ac].infh005,g_infh_d[l_ac].infh006,g_infh_d[l_ac].infh007, 
          g_infh_d[l_ac].infh008,g_infh_d[l_ac].infh009,g_infh_d[l_ac].infh010,g_infh_d[l_ac].infh011, 
          g_infh_d[l_ac].infhsite,g_infh_d[l_ac].infhunit,g_infh_d[l_ac].infh001_desc,g_infh_d[l_ac].infh001_desc_desc, 
          g_infh_d[l_ac].infh004_desc,g_infh_d[l_ac].infh005_desc,g_infh_d[l_ac].infh006_desc,g_infh_d[l_ac].infh008_desc  
            #(ver:78)
         IF SQLCA.SQLCODE THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code = SQLCA.SQLCODE 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充 name="b_fill.fill"
         INITIALIZE g_ref_fields TO NULL
         LET g_ref_fields[1] = g_infh_d[l_ac].infh001
         CALL ap_ref_array2(g_ref_fields,"SELECT imaa009 FROM imaa_t WHERE imaaent='"||g_enterprise||"' AND imaa001=? ","") RETURNING g_rtn_fields
         LET g_infh_d[l_ac].infh001_desc_1 = '', g_rtn_fields[1] , ''
         
         INITIALIZE g_ref_fields TO NULL
         LET g_ref_fields[1] = g_infh_d[l_ac].infh001_desc_1
         CALL ap_ref_array2(g_ref_fields,"SELECT rtaxl003 FROM rtaxl_t WHERE rtaxlent='"||g_enterprise||"' AND rtaxl001=? AND rtaxl002='"||g_dlang||"'","") RETURNING g_rtn_fields
         LET g_infh_d[l_ac].infh001_desc_1_desc = '', g_rtn_fields[1] , ''
         
         CALL aint420_infh005_desc_ref(g_infh_d[l_ac].infh005_desc) RETURNING g_infh_d[l_ac].infh005_desc_desc
         DISPLAY BY NAME g_infh_d[l_ac].infh005_desc_desc
            
         CALL aint420_infh005_desc_ref(g_infh_d[l_ac].infh006_desc) RETURNING g_infh_d[l_ac].infh006_desc_desc
         DISPLAY BY NAME g_infh_d[l_ac].infh006_desc_desc
         
         CALL aint420_infh005_desc_ref(g_infh_d[l_ac].infh008_desc) RETURNING g_infh_d[l_ac].infh008_desc_desc
         DISPLAY BY NAME g_infh_d[l_ac].infh008_desc_desc
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code = 9035 
               LET g_errparam.popup = TRUE 
               CALL cl_err()
            END IF
            EXIT FOREACH
         END IF
         
         LET l_ac = l_ac + 1
      END FOREACH
      LET g_error_show = 0
   
   END IF
    
 
   
   #add-point:browser_fill段其他table處理 name="browser_fill.other_fill"
   
   #end add-point
   
   CALL g_infh_d.deleteElement(g_infh_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE aint420_pb
 
   
   LET li_idx = l_ac
   
   #遮罩相關處理
   FOR l_ac = 1 TO g_infh_d.getLength()
      LET g_infh_d_mask_o[l_ac].* =  g_infh_d[l_ac].*
      CALL aint420_infh_t_mask()
      LET g_infh_d_mask_n[l_ac].* =  g_infh_d[l_ac].*
   END FOR
   
 
   
   LET l_ac = li_idx
   
   CALL cl_ap_performance_next_end()
 
END FUNCTION
 
