{<section id="aint420.state_change" >}
   #應用 a09 樣板自動產生(Version:17)
#+ 確認碼變更 
PRIVATE FUNCTION aint420_statechange()
   #add-point:statechange段define(客製用) name="statechange.define_customerization"
   
   #end add-point  
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="statechange.define"
   DEFINE l_success   LIKE type_t.num5
   #end add-point  
   
   #add-point:Function前置處理 name="statechange.before"
   
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_infg_m.infgdocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN aint420_cl USING g_enterprise,g_infg_m.infgdocno
   IF STATUS THEN
      CLOSE aint420_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aint420_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE aint420_master_referesh USING g_infg_m.infgdocno INTO g_infg_m.infgsite,g_infg_m.infgdocdt, 
       g_infg_m.infgdocno,g_infg_m.infg001,g_infg_m.infg002,g_infg_m.infg003,g_infg_m.infg004,g_infg_m.infg005, 
       g_infg_m.infgunit,g_infg_m.infgstus,g_infg_m.infgownid,g_infg_m.infgowndp,g_infg_m.infgcrtid, 
       g_infg_m.infgcrtdp,g_infg_m.infgcrtdt,g_infg_m.infgmodid,g_infg_m.infgmoddt,g_infg_m.infgcnfid, 
       g_infg_m.infgcnfdt,g_infg_m.infgsite_desc,g_infg_m.infg004_desc,g_infg_m.infg005_desc,g_infg_m.infgownid_desc, 
       g_infg_m.infgowndp_desc,g_infg_m.infgcrtid_desc,g_infg_m.infgcrtdp_desc,g_infg_m.infgmodid_desc, 
