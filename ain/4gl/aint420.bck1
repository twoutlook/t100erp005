   IF cl_null(g_infh_d[l_ac].infh012) OR cl_null(g_infh_d[l_ac].infh013) THEN
      RETURN TRUE
   END IF
   
   LET l_n = 0
   CASE g_infg_m.infg002
      WHEN '2'
         SELECT COUNT(*) INTO l_n
           FROM pmdt_t
          WHERE pmdtent = g_enterprise
            AND pmdtdocno = g_infh_d[l_ac].infh012
            AND pmdtseq = g_infh_d[l_ac].infh013
            AND pmdt006 = g_infh_d[l_ac].infh001
            AND pmdt200 = g_infh_d[l_ac].infh002
      WHEN '4'
