                g_infh_d[l_ac].infh010,g_infh_d[l_ac].infh011
           FROM pmdt_t
          WHERE pmdtent = g_enterprise
            AND pmdtdocno = g_infh_d[l_ac].infh012
            AND pmdtseq = g_infh_d[l_ac].infh013
      WHEN '5'
         SELECT indd002,indd003,indd004,indd006,indd021,indd021
           INTO g_infh_d[l_ac].infh001,g_infh_d[l_ac].infh002,g_infh_d[l_ac].infh003,g_infh_d[l_ac].infh004,
                g_infh_d[l_ac].infh010,g_infh_d[l_ac].infh011
           FROM indd_t
          WHERE inddent = g_enterprise
            AND indddocno = g_infh_d[l_ac].infh012
            AND inddseq = g_infh_d[l_ac].infh013
      WHEN '6'
         SELECT infh001,infh002,infh003,infh004,infh006,infh008,infh010,infh011 
           INTO g_infh_d[l_ac].infh001,g_infh_d[l_ac].infh002,g_infh_d[l_ac].infh003,g_infh_d[l_ac].infh004,
                g_infh_d[l_ac].infh005,g_infh_d[l_ac].infh006,g_infh_d[l_ac].infh010,g_infh_d[l_ac].infh011
           FROM infh_t
          WHERE infhent = g_enterprise
            AND infhdocno = g_infh_d[l_ac].infh012
            AND infhseq = g_infh_d[l_ac].infh013
         CALL aint420_infh005_ref(g_infh_d[l_ac].infh005) RETURNING g_infh_d[l_ac].infh005_desc
         CALL aint420_infh005_desc_ref(g_infh_d[l_ac].infh005_desc) RETURNING g_infh_d[l_ac].infh005_desc_desc
         CALL aint420_infh005_ref(g_infh_d[l_ac].infh006) RETURNING g_infh_d[l_ac].infh006_desc
         CALL aint420_infh005_desc_ref(g_infh_d[l_ac].infh006_desc) RETURNING g_infh_d[l_ac].infh006_desc_desc
      OTHERWISE
         SELECT indd002,indd003,indd004,indd006,indd031,indd031
           INTO g_infh_d[l_ac].infh001,g_infh_d[l_ac].infh002,g_infh_d[l_ac].infh003,g_infh_d[l_ac].infh004,
                g_infh_d[l_ac].infh010,g_infh_d[l_ac].infh011
           FROM indd_t
          WHERE inddent = g_enterprise
            AND indddocno = g_infh_d[l_ac].infh012
            AND inddseq = g_infh_d[l_ac].infh013
   END CASE
   #上架時，自動帶出銷售類貨架至目標貨架
   IF g_infg_m.infg001 = '1' THEN
      #161228-00033#1 Mark By Ken 170104(S)
      #SELECT inff005 INTO g_infh_d[l_ac].infh006
      #  FROM inff_t
      # WHERE inffent = g_enterprise
