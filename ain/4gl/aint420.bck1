#            AND indn001 = g_infh_d[l_ac].infh001
#            AND indn002 = g_infh_d[l_ac].infh002
         SELECT COUNT(*) INTO l_n
           FROM pmdt_t
          WHERE pmdtent = g_enterprise
            AND pmdtdocno = g_infh_d[l_ac].infh012
            AND pmdtseq = g_infh_d[l_ac].infh013
            AND pmdt006 = g_infh_d[l_ac].infh001
            AND pmdt200 = g_infh_d[l_ac].infh002
      WHEN '6'
         SELECT COUNT(*) INTO l_n
           FROM infh_t
          WHERE infhent = g_enterprise
            AND infhdocno = g_infh_d[l_ac].infh012
            AND infhseq = g_infh_d[l_ac].infh013  
            AND infh001 = g_infh_d[l_ac].infh001
            AND infh002 = g_infh_d[l_ac].infh002            
      OTHERWISE
         SELECT COUNT(*) INTO l_n
           FROM indd_t
          WHERE inddent = g_enterprise
            AND indddocno = g_infh_d[l_ac].infh012
            AND inddseq = g_infh_d[l_ac].infh013
            AND indd002 = g_infh_d[l_ac].infh001
            AND indd003 = g_infh_d[l_ac].infh002
   END CASE
   IF l_n < 1 THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = '' 
      LET g_errparam.code   = 'ain-00471'
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN FALSE
   END IF
   
   RETURN TRUE

END FUNCTION

