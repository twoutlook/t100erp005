               END IF
            ELSE
               CALL s_transaction_end('N','0')   #160812-00017#2 20160815 add by beckxie
               RETURN
            END IF
         ELSE
            CALL s_transaction_end('N','0')   #160812-00017#2 20160815 add by beckxie
            RETURN
         END IF
      OTHERWISE
         CALL s_transaction_end('N','0')   #160812-00017#2 20160815 add by beckxie
         RETURN
   END CASE
   #end add-point
   
   LET g_infg_m.infgmodid = g_user
   LET g_infg_m.infgmoddt = cl_get_current()
   LET g_infg_m.infgstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE infg_t 
      SET (infgstus,infgmodid,infgmoddt) 
        = (g_infg_m.infgstus,g_infg_m.infgmodid,g_infg_m.infgmoddt)     
    WHERE infgent = g_enterprise AND infgdocno = g_infg_m.infgdocno
 
    
   IF SQLCA.sqlcode THEN
