         LET g_errparam.code   = 'ain-00465'
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         RETURN FALSE,'',''
      END IF
   END IF
   
   RETURN TRUE,l_infh005,l_infh006
   
END FUNCTION

