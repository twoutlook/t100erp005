      #   AND inff006 IN (SELECT oocq002 FROM oocq_t WHERE oocqent = g_enterprise AND oocq001 = '2116' AND oocq019 = '1')
      #   AND ROWNUM = 1
      #161228-00033#1 Mark By Ken 170104(E)
      #161228-00033#1 Add By Ken 170104(S)
      LET l_sql = " SELECT inff005 ",
                  "   FROM inff_t ",
                  "  WHERE inffent = ",g_enterprise,
                  "    AND inffsite = '", g_infg_m.infgsite ,"' ",
                  "    AND inff001 = '", g_infh_d[l_ac].infh001 ,"' ",
                  "    AND inff006 IN (SELECT oocq002 FROM oocq_t WHERE oocqent = g_enterprise AND oocq001 = '2116' AND oocq019 = '1') "
      PREPARE aint420_inff_sel FROM l_sql
      DECLARE aint420_inff_sel_cur SCROLL CURSOR FOR aint420_inff_sel
      OPEN aint420_inff_sel_cur
      FETCH FIRST aint420_inff_sel_cur INTO g_infh_d[l_ac].infh006
      FREE aint420_inff_sel
      CLOSE aint420_inff_sel_cur        
      #161228-00033#1 Add By Ken 170104(E)         
   END IF
  
   CALL aint420_infh001_ref()
   
END FUNCTION

