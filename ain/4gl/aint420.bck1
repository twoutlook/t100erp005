################################################################################
# Descriptions...: 來源單號檢查
# Memo...........:
# Usage..........: CALL aint420_chk_infg003(p_docno)
# Date & Author..: 20150208 By dongsz
# Modify.........:
################################################################################
PRIVATE FUNCTION aint420_chk_infg003(p_docno)
DEFINE p_docno    LIKE infg_t.infg003
DEFINE l_n        LIKE type_t.num5
DEFINE l_errno    LIKE type_t.chr10

   LET l_errno = ''
   LET l_n = 0
   #判斷來源單號是否符合來源類型
   CASE g_infg_m.infg002
      WHEN '2'
         SELECT COUNT(*) INTO l_n
           FROM pmds_t
          WHERE pmdsent = g_enterprise
            AND pmdsdocno = p_docno
            AND pmds000 IN ('3','4','6')
            AND pmdsstus = 'S'
         IF l_n < 1 THEN
            LET l_errno = 'ain-00445'
         END IF
      WHEN '3'
         SELECT COUNT(*) INTO l_n
           FROM indc_t
          WHERE indcent = g_enterprise
            AND indcdocno = p_docno
            AND indc005 <> indc006
            AND indc006 = g_infg_m.infgsite
            AND indcstus IN ('P','C')
         IF l_n < 1 THEN
            LET l_errno = 'ain-00447'
         END IF
      WHEN '4'
#         SELECT COUNT(*) INTO l_n
#           FROM indm_t
#          WHERE indment = g_enterprise
#            AND indmdocno = g_infg_m.infg003
#            AND indmstus = 'T'
         SELECT COUNT(*) INTO l_n
           FROM pmds_t
          WHERE pmdsent = g_enterprise
            AND pmdsdocno = p_docno
            AND pmds000 IN ('7')
            AND pmdsstus = 'S'
         IF l_n < 1 THEN
            LET l_errno = 'ain-00446'
         END IF
      WHEN '5'
         SELECT COUNT(*) INTO l_n
           FROM indc_t
          WHERE indcent = g_enterprise
            AND indcdocno = p_docno
            AND indc005 <> indc006
            AND indc005 = g_infg_m.infgsite
            AND indcstus IN ('O','P','C')
         IF l_n < 1 THEN
            LET l_errno = 'ain-00448'
         END IF
      WHEN '6'
         SELECT COUNT(*) INTO l_n
           FROM infg_t,infh_t
          WHERE infgent = infhent
            AND infgdocno = infhdocno
            AND infgent = g_enterprise
            AND infhsite = g_infg_m.infgsite
            AND infhdocno = p_docno
            AND infh007 <= g_today
            AND infh009 = '1'
            AND infgstus = 'Y'
         IF l_n < 1 THEN
            LET l_errno = 'ain-00463'
         END IF
      WHEN '7'
         SELECT COUNT(*) INTO l_n
           FROM indc_t
          WHERE indcent = g_enterprise
            AND indcdocno = p_docno
            AND indc005 = indc006
            AND indc005 = g_infg_m.infgsite
            AND indcstus IN ('P','C')
         IF l_n < 1 THEN
            LET l_errno = 'ain-00449'
         END IF
   END CASE
   
   #判斷來源單號是否做過商品上下架處理
   IF g_infg_m.infg002 MATCHES '[23457]' THEN
      SELECT COUNT(*) INTO l_n
        FROM infg_t
       WHERE infgent = g_enterprise
         AND infg003 = p_docno
         AND infgdocno <> g_infg_m.infgdocno
         AND infgstus <> 'X'
      IF l_n > 0 THEN
         LET l_errno = 'ain-00450'
      END IF
   END IF
   
   IF cl_null(l_errno) THEN
      RETURN TRUE
   ELSE
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = p_docno 
      LET g_errparam.code   = l_errno
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN FALSE
   END IF

END FUNCTION

