   LET g_data_owner = g_infg_m.infgownid      
   LET g_data_dept  = g_infg_m.infgowndp
   
   #重新顯示   
   CALL aint420_show()
 
   #應用 a56 樣板自動產生(Version:3)
   #檢查此單據是否需顯示BPM簽核狀況按鈕 
   IF cl_bpm_chk() THEN
