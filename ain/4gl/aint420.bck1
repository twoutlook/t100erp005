                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','1')
               END IF
            ELSE
               CALL s_transaction_end('N','0')   #160812-00017#2 20160815 add by beckxie
               RETURN
            END IF
         ELSE
            CALL s_transaction_end('N','0')   #160812-00017#2 20160815 add by beckxie
            RETURN
         END IF
      WHEN lc_state = 'N'
         CALL cl_err_collect_init()
         CALL s_aint420_unconf_chk(g_infg_m.infgdocno,g_infg_m.infgstus) RETURNING l_success
         CALL cl_err_collect_show()
         IF l_success THEN
            IF cl_ask_confirm('aim-00110') THEN
               CALL s_transaction_begin()
               CALL s_aint420_unconf_upd(g_infg_m.infgdocno) RETURNING l_success,g_errno
               IF NOT l_success THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = g_errno
                  LET g_errparam.extend = g_infg_m.infgdocno
                  LET g_errparam.popup = TRUE
