#          WHERE indnent = g_enterprise
#            AND indndocno = g_infh_d[l_ac].infh012
#            AND indnseq = g_infh_d[l_ac].infh013   
         SELECT pmdt006,pmdt200,pmdt007,pmdt019,pmdt020,pmdt020 
