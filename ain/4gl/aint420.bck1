         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE aint420_master_referesh USING g_infg_m.infgdocno INTO g_infg_m.infgsite,g_infg_m.infgdocdt, 
          g_infg_m.infgdocno,g_infg_m.infg001,g_infg_m.infg002,g_infg_m.infg003,g_infg_m.infg004,g_infg_m.infg005, 
          g_infg_m.infgunit,g_infg_m.infgstus,g_infg_m.infgownid,g_infg_m.infgowndp,g_infg_m.infgcrtid, 
          g_infg_m.infgcrtdp,g_infg_m.infgcrtdt,g_infg_m.infgmodid,g_infg_m.infgmoddt,g_infg_m.infgcnfid, 
          g_infg_m.infgcnfdt,g_infg_m.infgsite_desc,g_infg_m.infg004_desc,g_infg_m.infg005_desc,g_infg_m.infgownid_desc, 
          g_infg_m.infgowndp_desc,g_infg_m.infgcrtid_desc,g_infg_m.infgcrtdp_desc,g_infg_m.infgmodid_desc, 
          g_infg_m.infgcnfid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_infg_m.infgsite,g_infg_m.infgsite_desc,g_infg_m.infgdocdt,g_infg_m.infgdocno, 
          g_infg_m.infg001,g_infg_m.infg002,g_infg_m.infg003,g_infg_m.infg004,g_infg_m.infg004_desc, 
          g_infg_m.infg005,g_infg_m.infg005_desc,g_infg_m.infgunit,g_infg_m.infgstus,g_infg_m.infgownid, 
          g_infg_m.infgownid_desc,g_infg_m.infgowndp,g_infg_m.infgowndp_desc,g_infg_m.infgcrtid,g_infg_m.infgcrtid_desc, 
          g_infg_m.infgcrtdp,g_infg_m.infgcrtdp_desc,g_infg_m.infgcrtdt,g_infg_m.infgmodid,g_infg_m.infgmodid_desc, 
          g_infg_m.infgmoddt,g_infg_m.infgcnfid,g_infg_m.infgcnfid_desc,g_infg_m.infgcnfdt
   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
   SELECT infgmodid,infgmoddt,infgcnfid,infgcnfdt
     INTO g_infg_m.infgmodid,g_infg_m.infgmoddt,
          g_infg_m.infgcnfid,g_infg_m.infgcnfdt
     FROM infg_t
    WHERE infgent = g_enterprise AND infgdocno = g_infg_m.infgdocno
   DISPLAY BY NAME g_infg_m.infgmodid,g_infg_m.infgmoddt,
                   g_infg_m.infgcnfid,g_infg_m.infgcnfdt
   #end add-point
 
   #add-point:statechange段結束前 name="statechange.after"
   
   #end add-point  
 
   CLOSE aint420_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aint420_msgcentre_notify('statechange:'||lc_state)
   
