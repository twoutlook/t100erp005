#         SELECT COUNT(*) INTO l_n
#           FROM indn_t
#          WHERE indnent = g_enterprise
#            AND indndocno = g_infh_d[l_ac].infh012
#            AND indnseq = g_infh_d[l_ac].infh013    
         SELECT COUNT(*) INTO l_n
           FROM pmdt_t
          WHERE pmdtent = g_enterprise
            AND pmdtdocno = g_infh_d[l_ac].infh012
            AND pmdtseq = g_infh_d[l_ac].infh013
      WHEN '6'
         SELECT COUNT(*) INTO l_n
           FROM infh_t
          WHERE infhent = g_enterprise
            AND infhdocno = g_infh_d[l_ac].infh012
            AND infhseq = g_infh_d[l_ac].infh013           
      OTHERWISE
         SELECT COUNT(*) INTO l_n
           FROM indd_t
          WHERE inddent = g_enterprise
            AND indddocno = g_infh_d[l_ac].infh012
            AND inddseq = g_infh_d[l_ac].infh013
   END CASE
   IF l_n < 1 THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = '' 
      LET g_errparam.code   = 'ain-00468'
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN FALSE
   END IF
   
   #檢查單號+項次是否做過商品上下架處理
   IF g_infg_m.infg002 <> '6' THEN
      SELECT COUNT(*) INTO l_n
        FROM infh_t,infg_t
       WHERE infhent = infgent
         AND infhdocno = infgdocno
         AND infhent = g_enterprise
         AND infhdocno <> g_infg_m.infgdocno
         AND infgstus <> 'X'
         AND infh012 = g_infh_d[l_ac].infh012
         AND infh013 = g_infh_d[l_ac].infh013
      IF l_n > 0 THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 'ain-00450'
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         RETURN FALSE
      END IF
   ELSE
      #檢查是否為到期待轉移的
      SELECT infh007,infh009 INTO l_infh007,l_infh009
        FROM infh_t
       WHERE infhent = g_enterprise
         AND infhdocno = g_infh_d[l_ac].infh012
         AND infhseq = g_infh_d[l_ac].infh013
      IF cl_null(l_infh007) OR l_infh007 > g_today OR l_infh009 <> '1' THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 'ain-00472'
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         RETURN FALSE
      END IF
   END IF
    
   RETURN TRUE
END FUNCTION

