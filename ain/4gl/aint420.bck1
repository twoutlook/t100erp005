PRIVATE FUNCTION aint420_get_para()
DEFINE l_infh005    LIKE infh_t.infh005
DEFINE l_infh006    LIKE infh_t.infh006

   IF g_infg_m.infg001 = '1' THEN
      #獲取參數設定的收貨暫存貨架
      LET l_infh005 = cl_get_para(g_enterprise,g_infg_m.infgsite,'S-CIR-2001')
      IF cl_null(l_infh005) OR l_infh005 = "NOT_DEFINED" OR l_infh005 = '" "' THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = g_infg_m.infgsite
         LET g_errparam.code   = 'ain-00464'
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         RETURN FALSE,'',''
      END IF
   END IF
   IF g_infg_m.infg001 = '2' THEN
      #獲取參數設定的退貨暫存貨架
      LET l_infh006 = cl_get_para(g_enterprise,g_infg_m.infgsite,'S-CIR-2002')
