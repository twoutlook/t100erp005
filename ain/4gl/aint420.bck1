      LET g_infg_m.infgmodid = g_user
      LET g_infg_m.infgmoddt = cl_get_current()
      LET g_infg_m.infgstus = 'N'
 
 
 
 
      #append欄位給值
      
     
      #一般欄位給值
            LET g_infg_m.infg001 = "1"
      LET g_infg_m.infg002 = "1"
 
  
      #add-point:單頭預設值 name="insert.default"
      CALL cl_set_combo_scc_part('infg002','6757','2,3')
      LET g_site_flag = FALSE
      CALL s_aooi500_default(g_prog,'infgsite',g_infg_m.infgsite)
         RETURNING l_insert,g_infg_m.infgsite
      IF NOT l_insert THEN
         RETURN
      END IF
      LET g_infg_m.infgunit = g_infg_m.infgsite
      LET g_infg_m.infg002 = "2"
      LET g_infg_m.infgdocdt = g_today
      #預設單據的單別
      LET r_success = ''
      LET r_doctype = ''
      CALL s_arti200_get_def_doc_type(g_infg_m.infgsite,g_prog,'1')
           RETURNING r_success,r_doctype
      LET g_infg_m.infgdocno = r_doctype
      LET g_infg_m.infg004 = g_user
      LET g_infg_m.infg005 = g_dept
      
      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_infg_m.infgsite
      CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
      LET g_infg_m.infgsite_desc = '', g_rtn_fields[1] , ''
      DISPLAY BY NAME g_infg_m.infgsite_desc
      
      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = g_infg_m.infg004
      CALL ap_ref_array2(g_ref_fields,"SELECT ooag011 FROM ooag_t WHERE ooagent='"||g_enterprise||"' AND ooag001=? ","") RETURNING g_rtn_fields
      LET g_infg_m.infg004_desc = '', g_rtn_fields[1] , ''
      DISPLAY BY NAME g_infg_m.infg004_desc

