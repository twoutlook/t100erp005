         WHEN "D"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/withdraw.png")
         WHEN "R"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/rejection.png")
         WHEN "W"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/signing.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE aint530_master_referesh USING g_indm_m.indmdocno INTO g_indm_m.indmsite,g_indm_m.indmdocdt, 
          g_indm_m.indmdocno,g_indm_m.indmunit,g_indm_m.indm008,g_indm_m.indm001,g_indm_m.indm002,g_indm_m.indm003, 
          g_indm_m.indm006,g_indm_m.indm007,g_indm_m.indm004,g_indm_m.indm005,g_indm_m.indm009,g_indm_m.indm010, 
          g_indm_m.indm011,g_indm_m.indm012,g_indm_m.indmstus,g_indm_m.indmownid,g_indm_m.indmowndp, 
          g_indm_m.indmcrtid,g_indm_m.indmcrtdp,g_indm_m.indmcrtdt,g_indm_m.indmmodid,g_indm_m.indmmoddt, 
