DEFINE l_success   LIKE type_t.num5
DEFINE l_imaa104   LIKE imaa_t.imaa104
DEFINE l_rate      LIKE indn_t.indn009

   #取库存单位
   SELECT imaa104 INTO l_imaa104
     FROM imaa_t
    WHERE imaaent = g_enterprise AND imaa001 = p_indn.indn001 AND imaastus = 'Y'
    
   #取退货单位与库存单位的转换率
   CALL s_aimi190_get_convert(p_indn.indn001,p_indn.indn007,l_imaa104) 
      RETURNING l_success,l_rate
   
   #计算出退货数量
   #例：
   #    库存单位A，包装单位B，退货单位C，其中10A=B(即件装数=10)，2A=C(即退货单位对库存单位的转换率为2)
   #    则包装数量 = 退货数量*转换率/件装数
   LET p_indn.indn010 = p_indn.indn011*l_rate/p_indn.indn009
   
   #按照单位设置截位
   CALL s_aooi250_take_decimals(p_indn.indn008,p_indn.indn010) RETURNING l_success,p_indn.indn010
   
   #赋值indn012/indn013/indn014/indn015
   LET p_indn.indn012 = p_indn.indn010
   LET p_indn.indn013 = p_indn.indn011
   LET p_indn.indn014 = p_indn.indn010 - p_indn.indn012
   LET p_indn.indn015 = p_indn.indn011 - p_indn.indn013
   
   LET r_indn.* = p_indn.*
   
   RETURN r_indn.*

END FUNCTION

 
