     END IF
   END IF  
   #151224-00024#2 20151228 s983961--add(E) A門店可幫B門店確認
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_indm_m.indmdocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN aint530_cl USING g_enterprise,g_indm_m.indmdocno
   IF STATUS THEN
      CLOSE aint530_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aint530_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE aint530_master_referesh USING g_indm_m.indmdocno INTO g_indm_m.indmsite,g_indm_m.indmdocdt, 
       g_indm_m.indmdocno,g_indm_m.indmunit,g_indm_m.indm008,g_indm_m.indm001,g_indm_m.indm002,g_indm_m.indm003, 
