PRIVATE FUNCTION aint530_indn010_init()
DEFINE l_success   LIKE type_t.num5
DEFINE l_imaa104   LIKE imaa_t.imaa104
DEFINE l_rate      LIKE indn_t.indn009

   #判断是否为空
   IF cl_null(g_indn_d[l_ac].indn007) OR cl_null(g_indn_d[l_ac].indn008) OR 
      cl_null(g_indn_d[l_ac].indn009) OR cl_null(g_indn_d[l_ac].indn010) THEN
      RETURN
   END IF
   
   #取库存单位
   SELECT imaa104 INTO l_imaa104
     FROM imaa_t
    WHERE imaaent = g_enterprise AND imaa001 = g_indn_d[l_ac].indn001 AND imaastus = 'Y'
    
   #取退货单位与库存单位的转换率
   CALL s_aimi190_get_convert(g_indn_d[l_ac].indn001,g_indn_d[l_ac].indn007,l_imaa104) 
      RETURNING l_success,l_rate
   
   #计算出退货数量
   #例：
   #    库存单位A，包装单位B，退货单位C，其中10A=B(即件装数=10)，2A=C(即退货单位对库存单位的转换率为2)
   #    则退货数量 = 包装数量*件装数/转换率
   LET g_indn_d[l_ac].indn011 = g_indn_d[l_ac].indn010*g_indn_d[l_ac].indn009/l_rate
   
   #按照单位设置截位
   CALL s_aooi250_take_decimals(g_indn_d[l_ac].indn007,g_indn_d[l_ac].indn011) RETURNING l_success,g_indn_d[l_ac].indn011
   DISPLAY BY NAME g_indn_d[l_ac].indn011
END FUNCTION
