{<section id="aint530.set_no_entry" >}
#+ 單頭欄位關閉設定
PRIVATE FUNCTION aint530_set_no_entry(p_cmd)
   #add-point:set_no_entry段define(客製用) name="set_no_entry.define_customerization"
   
   #end add-point     
   DEFINE p_cmd   LIKE type_t.chr1   
   #add-point:set_no_entry段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="set_no_entry.define"
   
   #end add-point     
   
   #add-point:Function前置處理  name="set_no_entry.pre_function"
   
   #end add-point
   
   IF p_cmd = 'u' AND g_chkey = 'N' THEN
      CALL cl_set_comp_entry("indmdocno",FALSE)
      #根據azzi850使用者身分開關特定欄位
      IF NOT cl_null(g_no_entry) THEN
         CALL cl_set_comp_entry(g_no_entry,FALSE)
      END IF
      #add-point:set_no_entry段欄位控制 name="set_no_entry.field_control"
      CALL cl_set_comp_entry("indmsite",FALSE)
      #end add-point 
   END IF 
   
   IF p_cmd = 'u' THEN  #docno,ld欄位確認是絕對關閉
      CALL cl_set_comp_entry("indmdocno",FALSE)
   END IF 
 
#  IF p_cmd = 'u' THEN  #docdt欄位依照設定關閉(FALSE則為設定不同意修正) #(ver:78)
      IF NOT cl_chk_update_docdt() THEN
         CALL cl_set_comp_entry("indmdocdt",FALSE)
      END IF
#  END IF 
   
   #add-point:set_no_entry段欄位控制後 name="set_no_entry.after_control"
   #aooi500設定的欄位控卡   
   IF NOT s_aooi500_comp_entry(g_prog,'indmsite') OR g_site_flag THEN
      CALL cl_set_comp_entry("indmsite",FALSE)
   END IF
   
   IF g_site = g_indm_m.indm003 AND g_indm_m.indmstus = 'Y' THEN
      CALL cl_set_comp_entry("indm001,indm002,indm006,indm009,indm010",FALSE)
   END IF
   #151224-00024#2 s983961--add(s)
   IF NOT (g_site = g_indm_m.indm003 AND g_indm_m.indmstus = 'Y') THEN
      CALL cl_set_comp_entry("indm003",FALSE)
   END IF
   #151224-00024#2 s983961--add(e)  
   
   #判斷據點參數若不使用產品特徵時，則產品特徵需隱藏不可以維護(據點參數:S-BAS-0036)
   IF NOT cl_null(g_indm_m.indmsite) THEN
      IF cl_get_para(g_enterprise,g_indm_m.indmsite,'S-BAS-0036') = 'N' THEN
         CALL cl_set_comp_visible("indn003,indn003_desc",FALSE)
      ELSE
         CALL cl_set_comp_visible("indn003,indn003_desc",TRUE)
      END IF
   END IF
   #end add-point 
 
END FUNCTION
 
