{<section id="aint530.set_no_entry_b" >}
#+ 單身欄位關閉設定
PRIVATE FUNCTION aint530_set_no_entry_b(p_cmd)
   #add-point:set_no_entry_b段define(客製用) name="set_no_entry_b.define_customerization"
   
   #end add-point    
   DEFINE p_cmd   LIKE type_t.chr1   
   #add-point:set_no_entry_b段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="set_no_entry_b.define"
   DEFINE l_imaa005   LIKE imaa_t.imaa005
   #end add-point    
   
   #add-point:Function前置處理  name="set_no_entry_b.pre_function"
   
   #end add-point
   
   IF p_cmd = 'u' AND g_chkey = 'N' THEN
      CALL cl_set_comp_entry("",FALSE)
      #add-point:set_no_entry_b段欄位控制 name="set_no_entry_b.field_control"
      
      #end add-point 
   END IF 
   
   #add-point:set_no_entry_b段 name="set_no_entry_b.set_no_entry_b"
   #判断采购类型若为0.自订货则设置供应商不为空，若不为0则设置配送中心不为空
   CASE 
      WHEN g_indn_d[l_ac].indn018 = '0' OR g_indn_d[l_ac].indn018 = '1'
         CALL cl_set_comp_entry("indn020",TRUE)
         CALL cl_set_comp_entry("indn019",FALSE)
      OTHERWISE
         CALL cl_set_comp_entry("indn019",TRUE)
         CALL cl_set_comp_entry("indn020",FALSE)
   END CASE
   
   #若当前组织为核准组织且为已审核状态，则只能维护核准件数&数量
   IF g_site = g_indm_m.indm003 AND g_indm_m.indmstus = 'Y' THEN
      CALL cl_set_comp_entry("indnseq,indn025,indn026,indn002,indn001,indn004,indn005,indn006,indn007,indn010,indn011,indn019,indn020,indn021,indn027,indn028",FALSE) #150324-00007#5 2015/04/10 By sakura add indn028
   END IF
   
   #若为未审核状态，不可维护核准件数&数量
   IF g_indm_m.indmstus = 'N' THEN
      CALL cl_set_comp_entry("indn012,indn013",FALSE)
   END IF
   
   #单头理由码不为空，则设置单身理由码不可维护
   IF NOT cl_null(g_indm_m.indm009) THEN
      CALL cl_set_comp_entry("indn021",FALSE)
   END IF
   
   #收货单号与收货项次都不为空时，设置相关栏位不可维护
   IF NOT cl_null(g_indn_d[l_ac].indn025) AND NOT cl_null(g_indn_d[l_ac].indn026) THEN
      CALL cl_set_comp_entry("indn001,indn002,indn004,indn005,indn006,indn007,indn028",FALSE) #150324-00007#5 2015/04/10 By sakura add indn028 
   END IF
   
   #收货单号不为空时，设置项次必输
   IF NOT cl_null(g_indn_d[l_ac].indn025) THEN
      CALL cl_set_comp_required("indn026",TRUE)
   END IF
   #收货项次不为空时，设置单号必输
   IF NOT cl_null(g_indn_d[l_ac].indn026) THEN
      CALL cl_set_comp_required("indn025",TRUE)
   END IF
   
   #若退货数量不为空，则不可维护包装数量
   IF NOT cl_null(g_indn_d[l_ac].indn011) THEN
      CALL cl_set_comp_entry("indn010",FALSE)
   END IF
   
   #若核准退货数量不为空，则不可维护核准包装数量
   IF NOT cl_null(g_indn_d[l_ac].indn013) THEN
      CALL cl_set_comp_entry("indn012",FALSE)
   END IF
   
   #判斷料件是否存在產品特徵功能
   LET l_imaa005 = ''
   SELECT imaa005 INTO l_imaa005 FROM imaa_t WHERE imaaent = g_enterprise AND imaa001 = g_indn_d[l_ac].indn001
   IF cl_null(l_imaa005) THEN
      CALL cl_set_comp_entry("indn003",FALSE)
      LET g_indn_d[l_ac].indn003 = ' '
   ELSE
      IF cl_null(g_indn_d[l_ac].indn003) THEN
         LET g_indn_d[l_ac].indn003 = ''
      END IF
   END IF
   #end add-point     
END FUNCTION
 
