               " LEFT JOIN oocql_t t8 ON t8.oocqlent="||g_enterprise||" AND t8.oocql001='313' AND t8.oocql002=indn021 AND t8.oocql003='"||g_dlang||"' ",
 
                     " WHERE indnent=? AND indndocno=?"
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before name="b_fill.body.fill_sql"
         
         #end add-point
         IF NOT cl_null(g_wc2_table1) THEN
            LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY indn_t.indnseq"
         
         #add-point:單身填充控制 name="b_fill.sql"
         
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE aint530_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR aint530_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
   #  OPEN b_fill_cs USING g_enterprise,g_indm_m.indmdocno   #(ver:78)
                                               
      FOREACH b_fill_cs USING g_enterprise,g_indm_m.indmdocno INTO g_indn_d[l_ac].indnsite,g_indn_d[l_ac].indnunit, 
          g_indn_d[l_ac].indnseq,g_indn_d[l_ac].indn025,g_indn_d[l_ac].indn026,g_indn_d[l_ac].indn002, 
          g_indn_d[l_ac].indn001,g_indn_d[l_ac].indn003,g_indn_d[l_ac].indn004,g_indn_d[l_ac].indn005, 
          g_indn_d[l_ac].indn006,g_indn_d[l_ac].indn028,g_indn_d[l_ac].indn008,g_indn_d[l_ac].indn009, 
          g_indn_d[l_ac].indn010,g_indn_d[l_ac].indn007,g_indn_d[l_ac].indn011,g_indn_d[l_ac].indn012, 
          g_indn_d[l_ac].indn013,g_indn_d[l_ac].indn014,g_indn_d[l_ac].indn015,g_indn_d[l_ac].indn018, 
          g_indn_d[l_ac].indn019,g_indn_d[l_ac].indn020,g_indn_d[l_ac].indn021,g_indn_d[l_ac].indn029, 
          g_indn_d[l_ac].indn030,g_indn_d[l_ac].indn022,g_indn_d[l_ac].indn023,g_indn_d[l_ac].indn024, 
          g_indn_d[l_ac].indn027,g_indn_d[l_ac].indn001_desc,g_indn_d[l_ac].indn004_desc,g_indn_d[l_ac].indn005_desc, 
          g_indn_d[l_ac].indn008_desc,g_indn_d[l_ac].indn007_desc,g_indn_d[l_ac].indn019_desc,g_indn_d[l_ac].indn020_desc, 
          g_indn_d[l_ac].indn021_desc   #(ver:78)
         IF SQLCA.SQLCODE THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code = SQLCA.SQLCODE 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充 name="b_fill.fill"
         CALL aint530_indn001_ref()
         CALL s_feature_description(g_indn_d[l_ac].indn001,g_indn_d[l_ac].indn003) RETURNING l_success,g_indn_d[l_ac].indn003_desc
         DISPLAY BY NAME g_indn_d[l_ac].indn003_desc
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code = 9035 
               LET g_errparam.popup = TRUE 
               CALL cl_err()
            END IF
            EXIT FOREACH
         END IF
         
         LET l_ac = l_ac + 1
      END FOREACH
      LET g_error_show = 0
   
   END IF
    
 
   
   #add-point:browser_fill段其他table處理 name="browser_fill.other_fill"
   
   #end add-point
   
   CALL g_indn_d.deleteElement(g_indn_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE aint530_pb
 
   
   LET li_idx = l_ac
   
   #遮罩相關處理
   FOR l_ac = 1 TO g_indn_d.getLength()
