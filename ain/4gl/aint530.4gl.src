PRIVATE FUNCTION aint530_indn025_chk()
DEFINE l_pmdsstus   LIKE pmds_t.pmdsstus
DEFINE l_pmdssite   LIKE pmds_t.pmdssite
DEFINE l_pmdt054    LIKE pmdt_t.pmdt054
DEFINE l_indn011    LIKE indn_t.indn011
DEFINE l_pmdt020_2  LIKE pmdt_t.pmdt020

   INITIALIZE g_errno TO NULL
   INITIALIZE l_pmdsstus TO NULL

   IF cl_null(g_indn_d[l_ac].indn025) THEN RETURN END IF

   IF cl_null(g_indn_d[l_ac].indn026) THEN
      SELECT pmdsstus,pmdssite INTO l_pmdsstus,l_pmdssite
        FROM pmds_t
       WHERE pmdsent = g_enterprise AND pmdsdocno = g_indn_d[l_ac].indn025
         AND pmds000 IN ('1','2','3','4')
   ELSE
      SELECT DISTINCT pmdsstus,pmdssite INTO l_pmdsstus,l_pmdssite
        FROM pmds_t,pmdt_t
       WHERE pmdsent = pmdtent AND pmdsdocno = pmdtdocno
         AND pmdtent = g_enterprise 
         AND pmdtdocno = g_indn_d[l_ac].indn025
         AND pmdtseq = g_indn_d[l_ac].indn026
         AND pmds000 IN ('1','2','3','4')
   END IF
   
   CASE 
      WHEN SQLCA.sqlcode = 100 LET g_errno = 'ain-00498' #输入的单号或单号+项次不存在于采购收货单中
      WHEN l_pmdsstus <> 'S' AND l_pmdsstus <> 'Y'  LET g_errno = 'ain-00499' #输入的单据未审核或过账
      WHEN l_pmdssite <> g_indm_m.indmsite LET g_errno = 'ain-00510' #输入收货单的收货组织与退货组织不一致
   END CASE
   
   IF cl_null(g_errno) AND NOT cl_null(g_indn_d[l_ac].indn026) AND NOT cl_null(g_indn_d[l_ac].indn026) THEN
      #收货数量
      SELECT pmdt054 INTO l_pmdt054
        FROM pmdt_t
       WHERE pmdtent = g_enterprise AND pmdtdocno = g_indn_d[l_ac].indn025 AND pmdtseq = g_indn_d[l_ac].indn026
      IF cl_null(l_pmdt054) THEN LET l_pmdt054 = 0 END IF
      
      #非作废的退货申请数量
      SELECT SUM(indn011) INTO l_indn011
        FROM indn_t,indm_t
       WHERE indnent = indment AND indndocno = indmdocno 
         AND indnent = g_enterprise 
         AND indn025 = g_indn_d[l_ac].indn025 AND indn026 = g_indn_d[l_ac].indn026
         AND NOT (indndocno = g_indm_m.indmdocno AND indnseq = g_indn_d[l_ac].indnseq)
         AND indmstus <> 'X'
      IF cl_null(l_indn011) THEN LET l_indn011 = 0 END IF
      
      #非作废的退货单数量（非退货申请来源）
      SELECT SUM(pmdt020) INTO l_pmdt020_2
        FROM pmdt_t,pmds_t
       WHERE pmdtent = pmdsent AND pmdtdocno = pmdsdocno
         AND pmdtent = g_enterprise
         AND pmdt027 = g_indn_d[l_ac].indn025 AND pmdt028 = g_indn_d[l_ac].indn026
         AND pmds000 IN ('7') #退厂
         AND pmdsstus <> 'X'
         AND (pmdt216 IS NULL OR pmdt217 IS NULL)
      IF cl_null(l_pmdt020_2) THEN LET l_pmdt020_2 = 0 END IF
      
      IF l_pmdt054 - l_indn011 - l_pmdt020_2 <= 0 THEN
         LET g_errno = 'ain-00514' #该收货单号+项次已无未退数量（收货数量-退货申请数量-非退货申请来源的退货单数量）
      END IF
   END IF
END FUNCTION
