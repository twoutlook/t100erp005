          g_indm_m.indmunit,g_indm_m.indm008,g_indm_m.indm001,g_indm_m.indm001_desc,g_indm_m.indm002, 
          g_indm_m.indm002_desc,g_indm_m.indm003,g_indm_m.indm003_desc,g_indm_m.indm006,g_indm_m.indm007, 
          g_indm_m.indm004,g_indm_m.indm004_desc,g_indm_m.indm005,g_indm_m.indm005_desc,g_indm_m.indm009, 
          g_indm_m.indm009_desc,g_indm_m.indm010,g_indm_m.indm011,g_indm_m.indm012,g_indm_m.indmstus, 
          g_indm_m.indmownid,g_indm_m.indmownid_desc,g_indm_m.indmowndp,g_indm_m.indmowndp_desc,g_indm_m.indmcrtid, 
          g_indm_m.indmcrtid_desc,g_indm_m.indmcrtdp,g_indm_m.indmcrtdp_desc,g_indm_m.indmcrtdt,g_indm_m.indmmodid, 
          g_indm_m.indmmodid_desc,g_indm_m.indmmoddt,g_indm_m.indmcnfid,g_indm_m.indmcnfid_desc,g_indm_m.indmcnfdt 
 
   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
   IF NOT l_success THEN
