                  g_indn_d[g_detail_idx].indn006,g_indn_d[g_detail_idx].indn028,g_indn_d[g_detail_idx].indn008, 
                  g_indn_d[g_detail_idx].indn009,g_indn_d[g_detail_idx].indn010,g_indn_d[g_detail_idx].indn007, 
                  g_indn_d[g_detail_idx].indn011,g_indn_d[g_detail_idx].indn012,g_indn_d[g_detail_idx].indn013, 
                  g_indn_d[g_detail_idx].indn014,g_indn_d[g_detail_idx].indn015,g_indn_d[g_detail_idx].indn018, 
                  g_indn_d[g_detail_idx].indn019,g_indn_d[g_detail_idx].indn020,g_indn_d[g_detail_idx].indn021, 
                  g_indn_d[g_detail_idx].indn029,g_indn_d[g_detail_idx].indn030,g_indn_d[g_detail_idx].indn022, 
                  g_indn_d[g_detail_idx].indn023,g_indn_d[g_detail_idx].indn024,g_indn_d[g_detail_idx].indn027)  
 
         WHERE indnent = g_enterprise AND indndocno = ps_keys_bak[1] AND indnseq = ps_keys_bak[2]
      #add-point:update_b段修改中 name="update_b.m_update"
      
      #end add-point   
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "indn_t" 
            LET g_errparam.code = "std-00009" 
            LET g_errparam.popup = TRUE 
            CALL s_transaction_end('N','0')
            CALL cl_err()
            
         WHEN SQLCA.SQLCODE #其他錯誤
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "indn_t:",SQLERRMESSAGE 
            LET g_errparam.code = SQLCA.SQLCODE 
            LET g_errparam.popup = TRUE 
            CALL s_transaction_end('N','0')
            CALL cl_err()
            
         OTHERWISE
 
