PUBLIC FUNCTION aint700_sync_multi(p_multi_flag)
DEFINE p_multi_flag      LIKE type_t.chr1
   CASE p_multi_flag
      WHEN 'Y' 
         LET g_indj_d[l_ac].indj018 = 'Y'
         LET g_indj_d[l_ac].indj012 = ''
         LET g_indj_d[l_ac].indj012_desc = ''
         LET g_indj_d[l_ac].indj013 = ''
         LET g_indj_d[l_ac].indj013_desc = ''
         LET g_indj_d[l_ac].indj014 = ''
         LET g_indj_d[l_ac].indj019 = ''
         LET g_indj_d[l_ac].indj015 = ''
         LET g_indj_d[l_ac].indj015_desc = ''
         LET g_indj_d[l_ac].indj016 = ''
         LET g_indj_d[l_ac].indj016_desc = ''
         LET g_indj_d[l_ac].indj017 = ''
         SELECT SUM(indk006) INTO g_indj_d[l_ac].indj010 FROM indk_t
          WHERE indkent = g_enterprise
            AND indkdocno = g_indi_m.indidocno
            AND indkseq = g_indj_d[l_ac].indjseq
      WHEN 'N'
         LET g_indj_d[l_ac].indj018 = 'N'
         SELECT indk006,
                indk008,indk009,indk010,indk015,
                indk012,indk013,indk014
           INTO g_indj_d[l_ac].indj010,
                g_indj_d[l_ac].indj012,g_indj_d[l_ac].indj013,g_indj_d[l_ac].indj014,g_indj_d[l_ac].indj019,
                g_indj_d[l_ac].indj015,g_indj_d[l_ac].indj016,g_indj_d[l_ac].indj017
           FROM indk_t
          WHERE indkent = g_enterprise
            AND indkdocno = g_indi_m.indidocno
            AND indkseq = g_indj_d[l_ac].indjseq
            
         CALL s_desc_get_stock_desc(g_indj_d[l_ac].indjsite,g_indj_d[l_ac].indj012)
          RETURNING g_indj_d[l_ac].indj012_desc
          
         CALL s_desc_get_locator_desc(g_indj_d[l_ac].indjsite,g_indj_d[l_ac].indj012,g_indj_d[l_ac].indj013)
          RETURNING g_indj_d[l_ac].indj013_desc
          
         CALL s_desc_get_stock_desc(g_indj_d[l_ac].indjunit,g_indj_d[l_ac].indj015)
          RETURNING g_indj_d[l_ac].indj015_desc
         
         CALL s_desc_get_locator_desc(g_indj_d[l_ac].indjunit,g_indj_d[l_ac].indj015,g_indj_d[l_ac].indj016)
          RETURNING g_indj_d[l_ac].indj016_desc
      WHEN 'X'
         LET g_indj_d[l_ac].indj018 = 'N'
   END CASE
   DISPLAY BY NAME g_indj_d[l_ac].indj018
END FUNCTION

