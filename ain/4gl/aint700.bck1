################################################################################
# Descriptions...: 取得需求组织库存
# Memo...........: 161103-00060#1
# Usage..........: CALL aint700_get_inag009 (传入参数)
#                  RETURNING 回传参数
# Input parameter: p_ent   企业编号
#                : p_site  营运据点
#                : p_item  料件编号
#                : p_feature 产品特征
#                : p_store 库位编号
# Return code....: l_inag009   库存数量
# Date & Author..: 2016/11/03 By tangyi
# Modify.........:
################################################################################
PRIVATE FUNCTION aint700_get_inag009(p_ent,p_site,p_item,p_feature,p_store)
DEFINE p_ent     LIKE inag_t.inagent,
       p_site    LIKE inag_t.inagsite,
       p_item    LIKE inag_t.inag001,
       p_feature LIKE inag_t.inag002,
       p_store   LIKE inag_t.inag004,
       l_inag009 LIKE inag_t.inag009,
       l_sql     STRING
       
      IF p_store IS NULL THEN
         SELECT ooef123 INTO p_store
           FROM ooef_t
          WHERE ooefent = g_enterprise
            AND ooef001 = p_site
            AND ooefstus = 'Y'
      END IF 
       
      LET l_sql="SELECT sum(inag009) FROM inag_t
                 WHERE inagent=",p_ent,"
                  AND inagsite='",p_site,"'
                  AND inag001= '",p_item,"'"
      LET l_sql=l_sql," AND inag004='",p_store,"'" #add by geza 201611206 161124-00039#1             
      IF NOT cl_null(p_feature) THEN
         LET l_sql=l_sql," AND inag002='",p_feature,"'"
      END IF 
      #mark by geza 201611206 161124-00039#1(S)
#      IF NOT cl_null(p_store) THEN
#         LET l_sql=l_sql," AND inag004='",p_store,"'"
#      ELSE
#         LET l_sql=l_sql," AND inag004 IN (SELECT inaa001 FROM inaa_t
#                                            WHERE inaa008='Y' 
#                                              AND inaaent=",p_ent,"
#                                              AND inaasite='",p_site,"')"
#      END IF 
      #mark by geza 201611206 161124-00039#1(e)
      PREPARE aint700_get_inag009 FROM l_sql
      EXECUTE aint700_get_inag009 INTO l_inag009
      
      IF l_inag009 IS NULL THEN
         LET l_inag009 = 0
      END IF
      
      RETURN l_inag009

END FUNCTION

################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL aint700_get_inas011(p_imaa001,p_site,p_imay012,p_inag007)
#                  RETURNING 回传参数
# Input parameter: p_imaa001 料号
#                : p_site    配送中心
#                : p_imay012 产品特征
#                : p_inag007 單位
# Date & Author..: 20170117 By geza
# Modify.........:
################################################################################
PRIVATE FUNCTION aint700_get_inas011(p_imaa001,p_site,p_imay012,p_inag007)
DEFINE p_imaa001             LIKE imaa_t.imaa001     #料号
DEFINE p_site                LIKE pmda_t.pmdasite    #配送中心
DEFINE p_imay012             LIKE imay_t.imay012     #产品特征 
DEFINE l_inag009             LIKE inag_t.inag009     #数量  
DEFINE p_inag007             LIKE inag_t.inag007     #库存单位   
     
   LET l_inag009 = 0
   SELECT COALESCE(SUM(inas011),0) INTO l_inag009
     FROM inas_t 
    WHERE inasent = g_enterprise
      AND inassite = p_site
      AND inas009 = p_imaa001
      AND inas010 = p_imay012
      AND inas013 = p_inag007
      
   RETURN  l_inag009   
END FUNCTION

 
