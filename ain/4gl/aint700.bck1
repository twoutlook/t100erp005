################################################################################
# Descriptions...: 子畫面程式碼
# Memo...........:
# Usage..........: CALL aint700_open_aint700_s01()
#                  RETURNING r_success
# Input parameter: 
# Return code....: r_success      TRUE/FALSE
# Date & Author..: 2016/09/07 By Ken
# Modify.........:
################################################################################
PRIVATE FUNCTION aint700_open_aint700_s01()
   DEFINE tm      RECORD                   
          l_type          LIKE type_t.chr1
                  END RECORD     
   DEFINE l_errno         LIKE type_t.chr10
   DEFINE r_success       LIKE type_t.num5     
   
   LET r_success = TRUE
   
   INITIALIZE tm.* TO NULL  
   
   OPEN WINDOW w_aint700_s01 WITH FORM cl_ap_formpath("ain","aint700_s01")
   #畫面初始化
   CALL cl_ui_init()
   WHILE TRUE
      DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)     
       
         INPUT tm.l_type FROM l_type ATTRIBUTE(WITHOUT DEFAULTS)
            BEFORE INPUT 
         END INPUT
         
         BEFORE DIALOG
#            LET tm.l_type = '1'
            LET tm.l_type = '2'    #161102-00026#24  mod by yany 2016/11/14
            DISPLAY BY NAME tm.l_type
               
         ON ACTION confirm
            ACCEPT DIALOG
      
         ON ACTION cancel1     
            LET INT_FLAG = TRUE
            EXIT DIALOG
      
         ON ACTION exit
            LET INT_FLAG = TRUE
            EXIT DIALOG
      
         ON ACTION close
            LET INT_FLAG = TRUE
            EXIT DIALOG  
            
      END DIALOG 
      
      IF NOT INT_FLAG THEN      
         #161109-00078#3 Mark By ken 161114(S)
         #IF NOT aint700_ins_inbm(tm.l_type) THEN
         #   LET r_success = FALSE
         #END IF 
         #161109-00078#3 Mark By ken 161114(E)
           
         #161109-00078#3 Add By ken 161114(S)           
         #IF NOT s_aint700_gen_aint701(tm.l_type,'','') THEN   #161220-00037#4 Mark By ken 161227
         IF NOT s_aint700_gen_aint701(tm.l_type,'','',0) THEN  #161220-00037#4 Add By ken 161227
            LET r_success = FALSE
         END IF          
         #161109-00078#3 Add By ken 161114(E)
         EXIT WHILE
      ELSE
         LET r_success = FALSE
         LET INT_FLAG = FALSE
         EXIT WHILE
      END IF
      
   END WHILE
   
   CLOSE WINDOW w_aint700_s01

   RETURN r_success
END FUNCTION

