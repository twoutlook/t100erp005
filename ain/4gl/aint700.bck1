{<section id="aint700.state_change" >}
   #應用 a09 樣板自動產生(Version:17)
#+ 確認碼變更 
PRIVATE FUNCTION aint700_statechange()
   #add-point:statechange段define(客製用) name="statechange.define_customerization"
   
   #end add-point  
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="statechange.define"
   DEFINE l_success    LIKE type_t.num5
   #161017-00051#18 -s by 08172
   DEFINE l_sys         LIKE type_t.chr80       #是否启用pos_b2b
   DEFINE lc_state_o    LIKE type_t.chr5        #备份lc_state
   DEFINE l_indistus_o  LIKE type_t.chr5        #备份indcstus
   DEFINE p_cnt         LIKE type_t.num10 
   DEFINE l_wc          STRING
   #161017-00051#18 -e by 08172
   #end add-point  
   
   #add-point:Function前置處理 name="statechange.before"
   IF g_indi_m.indistus = 'X' THEN
      RETURN
   END IF
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_indi_m.indidocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN aint700_cl USING g_enterprise,g_indi_m.indidocno
   IF STATUS THEN
      CLOSE aint700_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aint700_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE aint700_master_referesh USING g_indi_m.indidocno INTO g_indi_m.indisite,g_indi_m.indidocdt, 
       g_indi_m.indidocno,g_indi_m.indi006,g_indi_m.indi000,g_indi_m.indiunit,g_indi_m.indi001,g_indi_m.indi002, 
       g_indi_m.indi007,g_indi_m.indi003,g_indi_m.indi004,g_indi_m.indi008,g_indi_m.indi005,g_indi_m.indistus, 
       g_indi_m.indiownid,g_indi_m.indiowndp,g_indi_m.indicrtid,g_indi_m.indicrtdp,g_indi_m.indicrtdt, 
       g_indi_m.indimodid,g_indi_m.indimoddt,g_indi_m.indicnfid,g_indi_m.indicnfdt,g_indi_m.indisite_desc, 
       g_indi_m.indi006_desc,g_indi_m.indiunit_desc,g_indi_m.indi002_desc,g_indi_m.indi003_desc,g_indi_m.indi004_desc, 
       g_indi_m.indiownid_desc,g_indi_m.indiowndp_desc,g_indi_m.indicrtid_desc,g_indi_m.indicrtdp_desc, 
       g_indi_m.indimodid_desc,g_indi_m.indicnfid_desc
   
 
   #檢查是否允許此動作
   IF NOT aint700_action_chk() THEN
      CLOSE aint700_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_indi_m.indisite,g_indi_m.indisite_desc,g_indi_m.indidocdt,g_indi_m.indidocno,g_indi_m.indi006, 
       g_indi_m.indi006_desc,g_indi_m.indi000,g_indi_m.indiunit,g_indi_m.indiunit_desc,g_indi_m.indi001, 
       g_indi_m.indi002,g_indi_m.indi002_desc,g_indi_m.indi007,g_indi_m.indi003,g_indi_m.indi003_desc, 
       g_indi_m.indi004,g_indi_m.indi004_desc,g_indi_m.indi008,g_indi_m.indi005,g_indi_m.indistus,g_indi_m.indiownid, 
       g_indi_m.indiownid_desc,g_indi_m.indiowndp,g_indi_m.indiowndp_desc,g_indi_m.indicrtid,g_indi_m.indicrtid_desc, 
       g_indi_m.indicrtdp,g_indi_m.indicrtdp_desc,g_indi_m.indicrtdt,g_indi_m.indimodid,g_indi_m.indimodid_desc, 
       g_indi_m.indimoddt,g_indi_m.indicnfid,g_indi_m.indicnfid_desc,g_indi_m.indicnfdt
 
   CASE g_indi_m.indistus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
      WHEN "A"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/approved.png")
      WHEN "D"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/withdraw.png")
      WHEN "R"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/rejection.png")
      WHEN "W"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/signing.png")
      WHEN "X"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_indi_m.indistus
            
            WHEN "N"
               HIDE OPTION "unconfirmed"
            WHEN "Y"
               HIDE OPTION "confirmed"
            WHEN "A"
               HIDE OPTION "approved"
            WHEN "D"
               HIDE OPTION "withdraw"
            WHEN "R"
               HIDE OPTION "rejection"
            WHEN "W"
               HIDE OPTION "signing"
            WHEN "X"
               HIDE OPTION "invalid"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      CALL aint700_set_act_visible()
      CALL aint700_set_act_no_visible()
      #end add-point
      
      #應用 a36 樣板自動產生(Version:5)
      #提交
      ON ACTION signing
         IF cl_auth_chk_act("signing") THEN
            IF NOT aint700_send() THEN
               CALL s_transaction_end('N','0')
            ELSE
               CALL s_transaction_end('Y','0')
            END IF
            #因應簽核行為, 該動作完成後不再進行後續處理
            #於此處直接返回
            CLOSE aint700_cl
            RETURN
         END IF
    
      #抽單
      ON ACTION withdraw
         IF cl_auth_chk_act("withdraw") THEN
            IF NOT aint700_draw_out() THEN
               CALL s_transaction_end('N','0')
            ELSE
               CALL s_transaction_end('Y','0')
            END IF
            #因應簽核行為, 該動作完成後不再進行後續處理
            #於此處直接返回
            CLOSE aint700_cl
