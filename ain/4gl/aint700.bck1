PRIVATE FUNCTION aint700_browser_fill(ps_page_action)
   #add-point:browser_fill段define(客製用) name="browser_fill.define_customerization"
   
   #end add-point  
   DEFINE ps_page_action    STRING
   DEFINE l_wc              STRING
   DEFINE l_wc2             STRING
   DEFINE l_sql             STRING
   DEFINE l_sub_sql         STRING
   DEFINE l_sql_rank        STRING
   #add-point:browser_fill段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="browser_fill.define"
   DEFINE l_where           STRING
   DEFINE l_indi008         LIKE indi_t.indi008
   DEFINE l_sql_tmp         STRING
   #end add-point    
   
   #add-point:Function前置處理 name="browser_fill.before_browser_fill"
   LET l_where = s_aooi500_sql_where(g_prog,'indisite')
   IF cl_null(g_wc) THEN
      LET g_wc = l_where
   ELSE
      LET g_wc = g_wc CLIPPED," AND ",l_where
   END IF
   
   #161109-00078#3 Add By ken 161115(S)
   IF NOT cl_null(g_indi008) THEN 
      IF g_indi008 = 'Y' THEN
         LET g_wc =  cl_replace_str(g_wc,'indi008',"'Y'")
      END IF
      IF g_indi008 = 'N' THEN
         LET g_wc =  cl_replace_str(g_wc,'indi008',"'N'")
      END IF   
      IF g_indi008 = 'P' THEN
         LET g_wc =  cl_replace_str(g_wc,'indi008',"'P'")
      END IF        
      LET l_sql_tmp = " AND EXISTS(SELECT 1 ",
                      "              FROM (SELECT indient indient_n,indidocno indidocno_n,sum(indj021_1) indj021_1,SUM(indj021_2) indj021_2 FROM( ",
                      "                     SELECT indient,indidocno, ",
                      "                    (CASE WHEN COALESCE(indj021,'N')=  'Y' THEN 1 ELSE 0 END) indj021_1, ",
                      "                    (CASE WHEN COALESCE(indj021,'N') = 'N' THEN 1 ELSE 0 END) indj021_2  ",
                      "    FROM indi_t LEFT JOIN indj_t ON indient=indjent AND indidocno=indjdocno ",
                      "           ) GROUP BY indient,indidocno) ",
                      "WHERE indient_n = indient AND indidocno_n = indidocno ",
                      "  AND   ( CASE  WHEN indj021_1 >0 AND indj021_2 = 0  THEN 'Y' ",
                      "          WHEN indj021_1 =0 AND indj021_2 > 0  THEN 'N' ",
                      "          WHEN indj021_1 >0 AND indj021_2 > 0  THEN 'P' ",
                      "           END) = '",g_indi008,"') "
      LET g_wc = g_wc CLIPPED, l_sql_tmp
      LET g_indi008 = ''
   END IF
   #161109-00078#3 Add By ken 161115(E)
   #end add-point
   
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   LET l_wc  = g_wc.trim() 
   LET l_wc2 = g_wc2.trim()
 
   #add-point:browser_fill,foreach前 name="browser_fill.before_foreach"
   #151113-00003#6 Add By Ken 151120(S)
   #IF cl_null(g_wc) THEN
   #   LET g_wc = " indisite = '",g_site,"' "
   #ELSE
   #   LET g_wc = g_wc," AND indisite = '",g_site,"' "
   #END IF
   
   LET g_wc = g_wc," AND indi000 = '", g_argv[1], "' "
   
   LET l_wc  = g_wc.trim() 
   #151113-00003#6 Add By Ken 151120(E)
   #end add-point
   
   IF g_wc2 <> " 1=1" THEN
      #單身有輸入搜尋條件                      
      LET l_sub_sql = " SELECT DISTINCT indidocno ",
                      " FROM indi_t ",
                      " ",
                      " LEFT JOIN indj_t ON indjent = indient AND indidocno = indjdocno ", "  ",
                      #add-point:browser_fill段sql(indj_t1) name="browser_fill.cnt.join.}"
                      
                      #end add-point
