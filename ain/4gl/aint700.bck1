#當多庫儲批的flag = 'N' 的時候 維持indk_t和indj_t之間的資料同步
PUBLIC FUNCTION aint700_modify_indk()
   DEFINE r_success   LIKE type_t.num5
   DEFINE l_success   LIKE type_t.num5
   DEFINE l_cnt       LIKE type_t.num5
   DEFINE l_indk      RECORD
            indkent       LIKE indk_t.indkent,
            indksite      LIKE indk_t.indksite,
            indkunit      LIKE indk_t.indkunit,
            indkdocno     LIKE indk_t.indkdocno,
            indkseq       LIKE indk_t.indkseq,
            indkseq1      LIKE indk_t.indkseq1,
            indk001       LIKE indk_t.indk001,
            indk002       LIKE indk_t.indk002,
            indk003       LIKE indk_t.indk003,
            indk004       LIKE indk_t.indk004,
            indk005       LIKE indk_t.indk005,
            indk006       LIKE indk_t.indk006,
            indk007       LIKE indk_t.indk007,
            indk008       LIKE indk_t.indk008,
            indk009       LIKE indk_t.indk009,
            indk010       LIKE indk_t.indk010,
            indk011       LIKE indk_t.indk011,
            indk012       LIKE indk_t.indk012,
            indk013       LIKE indk_t.indk013,
            indk014       LIKE indk_t.indk014,
            indk015       LIKE indk_t.indk015
                      END RECORD          
   
   LET r_success = TRUE
   
   IF g_indj_d[l_ac].indj018 = 'N' THEN
      DELETE FROM indk_t 
       WHERE indkent = g_enterprise
         AND indkdocno = g_indi_m.indidocno
         AND indkseq = g_indj_d[l_ac].indjseq
   ELSE 
      RETURN r_success
   END IF
   
   LET l_indk.indkent = g_enterprise
   LET l_indk.indksite = g_indj_d[l_ac].indjsite
   LET l_indk.indkunit = g_indj_d[l_ac].indjunit
   LET l_indk.indkdocno = g_indi_m.indidocno
   LET l_indk.indkseq = g_indj_d[l_ac].indjseq
   LET l_indk.indkseq1 = 1
   LET l_indk.indk001 = g_indj_d[l_ac].indj004
   LET l_indk.indk002 = g_indj_d[l_ac].indj005
   LET l_indk.indk003 = g_indj_d[l_ac].indj006
   LET l_indk.indk005 = g_indj_d[l_ac].indj008
   LET l_indk.indk006 = g_indj_d[l_ac].indj010
   CALL s_aooi250_convert_qty(l_indk.indk001,l_indk.indk005,l_indk.indk003,l_indk.indk006) 
    RETURNING l_success,l_indk.indk004
   IF g_argv[1] = '0' THEN   #151113-00003#6 Add By Ken 151120
      LET l_indk.indk007 = g_indj_d[l_ac].indjsite
   END IF
   IF g_argv[1] = '1' THEN   #151113-00003#6 Add By Ken 151120
      LET l_indk.indk007 = g_indi_m.indisite
   END IF
   LET l_indk.indk008 = g_indj_d[l_ac].indj012
   LET l_indk.indk009 = g_indj_d[l_ac].indj013
   LET l_indk.indk010 = g_indj_d[l_ac].indj014
   LET l_indk.indk015 = g_indj_d[l_ac].indj019
   LET l_indk.indk011 = g_indj_d[l_ac].indjunit
   LET l_indk.indk012 = g_indj_d[l_ac].indj015
   LET l_indk.indk013 = g_indj_d[l_ac].indj016
   LET l_indk.indk014 = g_indj_d[l_ac].indj017
   
   #特徵碼為空時 給一格空白
   IF cl_null(l_indk.indk002) THEN
      LET l_indk.indk002 = ' '
   END IF
   #儲位、批號為空時 給一格空白
   IF cl_null(l_indk.indk009) THEN
      LET l_indk.indk009 = ' '
   END IF 
   IF cl_null(l_indk.indk010) THEN
      LET l_indk.indk010 = ' '
   END IF
   IF cl_null(l_indk.indk013) THEN
      LET l_indk.indk013 = ' '
   END IF
   IF cl_null(l_indk.indk014) THEN
      LET l_indk.indk014 = ' '
   END IF
   #庫存管理特徵為空時 給一格空白
   IF cl_null(l_indk.indk015) THEN
      LET l_indk.indk015 = ' '
   END IF
   INSERT INTO indk_t (indkent, indksite, indkunit, indkdocno,
                       indkseq, indkseq1,
                       indk001, indk002, indk003, indk004, indk005, 
                       indk006, indk007, indk008, indk009, indk010, indk015,
                       indk011, indk012, indk013, indk014  )
        VALUES(l_indk.indkent, l_indk.indksite, l_indk.indkunit, l_indk.indkdocno,
               l_indk.indkseq, l_indk.indkseq1,
               l_indk.indk001, l_indk.indk002, l_indk.indk003, l_indk.indk004, l_indk.indk005, 
               l_indk.indk006, l_indk.indk007, l_indk.indk008, l_indk.indk009, l_indk.indk010, l_indk.indk015,
               l_indk.indk011, l_indk.indk012, l_indk.indk013, l_indk.indk014) 
   IF SQLCA.sqlcode OR SQLCA.sqlerrd[3] = 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'sub-00034'
      LET g_errparam.extend = 'INSERT indk_t'
      LET g_errparam.popup = TRUE
      CALL cl_err()
      LET r_success = FALSE
   END IF
   
   RETURN r_success
END FUNCTION
