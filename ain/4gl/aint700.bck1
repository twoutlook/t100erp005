                      #add-point:browser_fill段sql(indk_t1) name="browser_fill.cnt.join.indk_t1"
                      
                      #end add-point
 
 
 
                      " ", 
                      " ", 
                      " ",                      
 
 
 
                      " WHERE indient = " ||g_enterprise|| " AND indjent = " ||g_enterprise|| " AND ",l_wc, " AND ", l_wc2, cl_sql_add_filter("indi_t")
