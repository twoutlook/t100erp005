                        LET g_errparam.extend = 'upd indi_t'
                        LET g_errparam.popup = TRUE
                        CALL cl_err()
                        LET l_success = FALSE       
                     END IF   
                     #add by geza 20161102 #161024-00023#19(E)
                     IF NOT l_success THEN
                        CALL s_transaction_end('N','0')
                        CALL cl_err_collect_show()
                        RETURN
                     ELSE
                        CALL s_transaction_end('Y','0')
                        CALL cl_err_collect_show()
                        CALL aint700_b_fill()
                     END IF
                     #add by geza 20161102 #161024-00023#19(S)
                     SELECT indi008 INTO g_indi_m.indi008
                       FROM indi_t 
                      WHERE indient = g_enterprise
                        AND indidocno = g_indi_m.indidocno
                     DISPLAY BY NAME g_indi_m.indi008
                     #add by geza 20161102 #161024-00023#19(E)
                  END IF 
               END IF
            END IF
         END IF
      END IF
   END IF

   IF lc_state = 'N' THEN
      CALL s_aint700_unconf_chk(g_indi_m.indidocno,g_indi_m.indistus) RETURNING l_success
      IF NOT l_success THEN
         CALL cl_err_collect_show()
         CALL s_transaction_end('N','0')
         RETURN
      ELSE
         IF NOT cl_ask_confirm('aim-00110') THEN
            CALL cl_err_collect_show()
            CALL s_transaction_end('N','0')
            RETURN
         ELSE
            CALL s_aint700_unconf_upd(g_indi_m.indidocno) RETURNING l_success
            IF NOT l_success THEN
               CALL s_transaction_end('N','0')
               CALL cl_err_collect_show()
               RETURN
            ELSE
               CALL s_transaction_end('Y','0')
               CALL cl_err_collect_show()
               CALL aint700_b_fill()
            END IF
         END IF
      END IF
   END IF

   IF lc_state = 'X' THEN
      CALL s_aint700_invalid_chk(g_indi_m.indidocno,g_indi_m.indistus) RETURNING l_success
      IF NOT l_success THEN
         CALL cl_err_collect_show()
         CALL s_transaction_end('N','0')
         RETURN
      ELSE
         IF NOT cl_ask_confirm('aim-00109') THEN
            CALL cl_err_collect_show()
            CALL s_transaction_end('N','0')
            RETURN
         ELSE
            CALL s_aint700_invalid_upd(g_indi_m.indidocno) RETURNING l_success
            IF NOT l_success THEN
               CALL s_transaction_end('N','0')
               CALL cl_err_collect_show()
               RETURN
            ELSE
               CALL s_transaction_end('Y','0')
               CALL cl_err_collect_show()
            END IF
         END IF
      END IF
   END IF
   #end add-point
   
   LET g_indi_m.indimodid = g_user
   LET g_indi_m.indimoddt = cl_get_current()
   LET g_indi_m.indistus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE indi_t 
      SET (indistus,indimodid,indimoddt) 
        = (g_indi_m.indistus,g_indi_m.indimodid,g_indi_m.indimoddt)     
    WHERE indient = g_enterprise AND indidocno = g_indi_m.indidocno
 
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "A"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/approved.png")
         WHEN "D"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/withdraw.png")
         WHEN "R"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/rejection.png")
         WHEN "W"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/signing.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE aint700_master_referesh USING g_indi_m.indidocno INTO g_indi_m.indisite,g_indi_m.indidocdt, 
          g_indi_m.indidocno,g_indi_m.indi006,g_indi_m.indi000,g_indi_m.indiunit,g_indi_m.indi001,g_indi_m.indi002, 
          g_indi_m.indi007,g_indi_m.indi003,g_indi_m.indi004,g_indi_m.indi008,g_indi_m.indi005,g_indi_m.indistus, 
          g_indi_m.indiownid,g_indi_m.indiowndp,g_indi_m.indicrtid,g_indi_m.indicrtdp,g_indi_m.indicrtdt, 
          g_indi_m.indimodid,g_indi_m.indimoddt,g_indi_m.indicnfid,g_indi_m.indicnfdt,g_indi_m.indisite_desc, 
          g_indi_m.indi006_desc,g_indi_m.indiunit_desc,g_indi_m.indi002_desc,g_indi_m.indi003_desc,g_indi_m.indi004_desc, 
          g_indi_m.indiownid_desc,g_indi_m.indiowndp_desc,g_indi_m.indicrtid_desc,g_indi_m.indicrtdp_desc, 
          g_indi_m.indimodid_desc,g_indi_m.indicnfid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_indi_m.indisite,g_indi_m.indisite_desc,g_indi_m.indidocdt,g_indi_m.indidocno, 
          g_indi_m.indi006,g_indi_m.indi006_desc,g_indi_m.indi000,g_indi_m.indiunit,g_indi_m.indiunit_desc, 
          g_indi_m.indi001,g_indi_m.indi002,g_indi_m.indi002_desc,g_indi_m.indi007,g_indi_m.indi003, 
          g_indi_m.indi003_desc,g_indi_m.indi004,g_indi_m.indi004_desc,g_indi_m.indi008,g_indi_m.indi005, 
          g_indi_m.indistus,g_indi_m.indiownid,g_indi_m.indiownid_desc,g_indi_m.indiowndp,g_indi_m.indiowndp_desc, 
          g_indi_m.indicrtid,g_indi_m.indicrtid_desc,g_indi_m.indicrtdp,g_indi_m.indicrtdp_desc,g_indi_m.indicrtdt, 
          g_indi_m.indimodid,g_indi_m.indimodid_desc,g_indi_m.indimoddt,g_indi_m.indicnfid,g_indi_m.indicnfid_desc, 
          g_indi_m.indicnfdt
   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
   #161017-00051#18 -s by 08172
   CALL cl_get_para(g_enterprise,g_site,'E-BAS-0013') RETURNING l_sys 
   IF l_sys = 'Y' THEN
      IF lc_state_o = 'Y' AND l_indistus_o = 'N'  THEN
         CALL cl_err_collect_init()
         LET l_wc = " indc003 = '",g_indi_m.indidocno,"'"
         CALL s_ainp512_send(l_wc)  RETURNING l_success,p_cnt 
         CALL cl_err_collect_show()
      END IF
   END IF
   #161017-00051#18 -e by 08172
   #end add-point
 
   #add-point:statechange段結束前 name="statechange.after"
   CLOSE aint700_cl
   #end add-point  
 
   CLOSE aint700_cl
   CALL s_transaction_end('Y','0')
 
