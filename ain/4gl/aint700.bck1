                              t6.oocal003 ,t7.ooefl003 ,t8.inayl003 ,t9.inab003 ,t10.ooefl003 ,t11.inayl003 ,t12.inab003, ",
             "                indj022,indj023 ",    #161220-00037#2 Add By Ken 161223
             "           FROM indj_t", 
                
