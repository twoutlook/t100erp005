          g_indj_d_total[l_ac5].indj011,g_indj_d_total[l_ac5].indj018,g_indj_d_total[l_ac5].indjsite,g_indj_d_total[l_ac5].indj012, 
          g_indj_d_total[l_ac5].indj013,g_indj_d_total[l_ac5].indj014,g_indj_d_total[l_ac5].indj019,g_indj_d_total[l_ac5].indjunit, 
          g_indj_d_total[l_ac5].indj015,g_indj_d_total[l_ac5].indj016,g_indj_d_total[l_ac5].indj017, g_indj_d_total[l_ac5].indj021, 
          g_indj_d_total[l_ac5].indj003_desc, 
          g_indj_d_total[l_ac5].indj020_desc,g_indj_d_total[l_ac5].indj004_desc,g_indj_d_total[l_ac5].indj004_desc_desc,g_indj_d_total[l_ac5].indj006_desc, 
          g_indj_d_total[l_ac5].indj008_desc,g_indj_d_total[l_ac5].indjsite_desc,g_indj_d_total[l_ac5].indj012_desc,g_indj_d_total[l_ac5].indj013_desc, 
          g_indj_d_total[l_ac5].indjunit_desc,g_indj_d_total[l_ac5].indj015_desc,g_indj_d_total[l_ac5].indj016_desc,
          g_indj_d_total[l_ac5].indj022,g_indj_d_total[l_ac5].indj023    #161220-00037#2 Add By Ken 161223
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充 name="b_fill.fill"
         #160825-00021#3 Add By Ken 160825(S)   #取pmcz的需求對象欄位的說明欄位
         IF g_indi_m.indi007 != '2' THEN   #來源非訂單
            CALL s_desc_get_department_desc(g_indj_d_total[l_ac5].indj020) 
             RETURNING g_indj_d_total[l_ac5].indj020_desc      
         END IF
         #160825-00021#3 Add By Ken 160825(E)
         #161029-00003#1 -str-
         LET l_pmcz063 =''
         SELECT pmcz063 INTO l_pmcz063 FROM pmcz_t
          WHERE pmcz001=g_indj_d_total[l_ac5].indj001
            AND pmcz002=g_indj_d_total[l_ac5].indj002
            AND pmcz062=g_indj_d_total[l_ac5].indj020
            AND pmczent = g_enterprise #add by geza 20170123 #170122-00004#1
         CASE l_pmcz063
           WHEN '1'  #门店
              CALL s_desc_get_department_desc(g_indj_d_total[l_ac5].indj020) 
              RETURNING g_indj_d_total[l_ac5].indj020_desc 
           WHEN '2'  #客户
              #161129-00027#7 add by yany 2016/12/29 --str
              CALL s_desc_get_trading_partner_full_desc(g_indj_d_total[l_ac5].indj020) 
                   RETURNING g_indj_d_total[l_ac5].indj020_desc 
              #161129-00027#7 add by yany 2016/12/29 --end    
              
         END CASE
         #161029-00003#1 -end-
         CALL aint700_get_inag009(g_enterprise,g_indj_d_total[l_ac5].indj003,g_indj_d_total[l_ac5].indj004,   #161103-00060#1 add
                                  g_indj_d_total[l_ac5].indj005,g_indj_d_total[l_ac5].indj015)  #add by geza 201611206 161124-00039#1 indj015
          RETURNING g_indj_d_total[l_ac5].inag009
         #end add-point
      
         IF l_ac5 > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac5
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF
            EXIT FOREACH
         END IF
         
         LET l_ac5 = l_ac5 + 1
      END FOREACH                                         
   
      LET g_error_show = 0
   
   END IF
   
   CALL g_indj_d_total.deleteElement(g_indj_d_total.getLength())
   LET l_ac5 = l_ac5-1
   LET g_rec_b5=l_ac5
   FREE b_fill_cs_total
   
END FUNCTION

