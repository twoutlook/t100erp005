PUBLIC FUNCTION aint700_analyse_str_array(p_str_array)
DEFINE p_str_array    DYNAMIC ARRAY OF STRING
DEFINE l_i            LIKE type_t.num5
DEFINE l_cnt          LIKE type_t.num5
DEFINE l_count        LIKE type_t.num5
DEFINE tok            base.StringTokenizer
DEFINE l_str1         LIKE indj_t.indj001
DEFINE l_str2         LIKE indj_t.indj002
DEFINE l_success      LIKE type_t.num5
DEFINE l_errno        LIKE type_t.chr10
DEFINE l_ac_t         LIKE type_t.num5
DEFINE l_detail_cnt   LIKE type_t.num5
DEFINE l_indjseq      LIKE indj_t.indjseq
DEFINE l_detail_idx_t LIKE type_t.num5
DEFINE l_del_flag     LIKE type_t.chr1
   LET l_count = 0
   LET l_detail_cnt = g_indj_d.getLength()
   LET l_indjseq = g_indj_d[l_ac].indjseq
   
   LET l_ac_t = l_ac
   LET l_detail_idx_t = g_detail_idx
   LET g_detail_idx = l_ac
   FOR l_i = 1 TO p_str_array.getLength()
      LET tok = base.StringTokenizer.create(p_str_array[l_i],"|")
      LET l_cnt = 0 
      WHILE tok.hasMoreTokens()
         LET l_cnt = l_cnt + 1 
         CASE l_cnt 
            WHEN '1'
               LET l_str1 = tok.nextToken()
            WHEN '2'
               LET l_str2 = tok.nextToken()
            OTHERWISE 
         END CASE 
      END WHILE

      LET g_indj_d[l_ac].indjseq = l_indjseq
      CALL s_aint700_chk_indj001_indj002(l_str1,l_str2,g_indi_m.indidocno,g_indj_d[l_ac].indjseq) 
       RETURNING l_success,l_errno
      IF NOT l_success THEN
         CONTINUE FOR
      END IF
      LET g_indj_d[l_ac].indj001 = l_str1 
      LET g_indj_d[l_ac].indj002 = l_str2
      
      IF l_count > 0 THEN
         LET g_indj_d[l_ac].indj011 = "0"
         LET g_indj_d[l_ac].indj018 = "N"
         LET g_indj_d[l_ac].indjsite = g_indi_m.indisite
         CALL aint700_carry_pmdb()
         CALL s_desc_get_department_desc(g_indj_d[l_ac].indjsite)
          RETURNING g_indj_d[l_ac].indjsite_desc

         #benson_m
         IF cl_null(g_indj_d[l_ac].indj012) OR cl_null(g_indj_d[l_ac].indj015) THEN
            CALL g_indj_d.deleteElement(l_ac) 
            CONTINUE FOR
         END IF 
         
         INITIALIZE gs_keys TO NULL
         LET gs_keys[1] = g_indi_m.indidocno
         LET gs_keys[2] = g_indj_d[l_ac].indjseq
         CALL aint700_insert_b('indj_t',gs_keys,"'1'")
         IF SQLCA.SQLcode  THEN
            CALL g_indj_d.deleteElement(l_ac) 
            CONTINUE FOR
         END IF
         
         CALL aint700_modify_indk()
          RETURNING l_success
         IF NOT l_success THEN
            CALL g_indj_d.deleteElement(l_ac) 
            CONTINUE FOR
         END IF
      END IF
      LET l_count = l_count + 1 
      LET l_ac = l_ac + 1
      LET l_indjseq = l_indjseq + 1
      LET g_detail_idx = l_ac
   END FOR
   
   LET l_ac = l_ac_t
   LET g_detail_idx = l_detail_idx_t
   IF l_count = 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = l_errno
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()
   END IF
   LET l_del_flag = 'N'
   IF cl_null(g_indj_d[g_indj_d.getLength()].indj001) THEN
      LET l_del_flag = 'Y'
   END IF
   IF l_del_flag = 'Y' THEN   
      CALL g_indj_d.deleteElement(g_indj_d.getLength()) 
   END IF
END FUNCTION

