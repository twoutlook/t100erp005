      LET l_ac5 = 1
      
      OPEN b_fill_cs_total USING g_enterprise,g_indi_m.indidocno
      FOREACH b_fill_cs_total INTO g_indj_d_total[l_ac5].indjseq,g_indj_d_total[l_ac5].indj001,g_indj_d_total[l_ac5].indj002,g_indj_d_total[l_ac5].indj003, 
