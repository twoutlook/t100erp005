   DEFINE l_cnt     LIKE type_t.num5
   #end add-point     
   
   #add-point:Function前置處理  name="set_no_entry.pre_function"
   
   #end add-point
   
   IF p_cmd = 'u' AND g_chkey = 'N' THEN
      CALL cl_set_comp_entry("indidocno",FALSE)
      #根據azzi850使用者身分開關特定欄位
      IF NOT cl_null(g_no_entry) THEN
         CALL cl_set_comp_entry(g_no_entry,FALSE)
      END IF
      #add-point:set_no_entry段欄位控制 name="set_no_entry.field_control"
      
      #end add-point 
   END IF 
   
   IF p_cmd = 'u' THEN  #docno,ld欄位確認是絕對關閉
      CALL cl_set_comp_entry("indidocno",FALSE)
   END IF 
 
#  IF p_cmd = 'u' THEN  #docdt欄位依照設定關閉(FALSE則為設定不同意修正) #(ver:78)
      IF NOT cl_chk_update_docdt() THEN
         CALL cl_set_comp_entry("indidocdt",FALSE)
      END IF
#  END IF 
   
   #add-point:set_no_entry段欄位控制後 name="set_no_entry.after_control"
   IF p_cmd = 'u' THEN
      CALL cl_set_comp_entry("indisite,indidocno,indidocdt",FALSE)
   END IF
   
   IF NOT s_aooi500_comp_entry(g_prog,'indisite') OR g_site_flag THEN
      CALL cl_set_comp_entry("indisite",FALSE)
   END IF
   IF NOT s_aooi500_comp_entry(g_prog,'indiunit') THEN
      CALL cl_set_comp_entry("indiunit",FALSE)
   END IF
   
   LET l_cnt = 0 
   SELECT COUNT(*) INTO l_cnt FROM indj_t
    WHERE indjent = g_enterprise
      AND indjdocno = g_indi_m.indidocno
   IF l_cnt > 0 THEN
       CALL cl_set_comp_entry("indiunit,indi001,indi002",FALSE)
   END IF
   #end add-point 
 
END FUNCTION
 
