         CALL s_transaction_end('N','0')
         RETURN
      END IF
             
      CLEAR FORM
      CALL g_inbe_d.clear() 
 
     
      CALL aint160_ui_browser_refresh()  
      #CALL aint160_ui_headershow()  
      #CALL aint160_ui_detailshow()
 
      #add-point:多語言刪除 name="delete.lang.before_delete"
      
