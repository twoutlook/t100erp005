      LET g_errparam.extend = "" 
      LET g_errparam.code = "std-00003" 
      LET g_errparam.popup = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_inbddocno_t = g_inbd_m.inbddocno
 
   CALL s_transaction_begin()
   
   OPEN aint160_cl USING g_enterprise,g_inbd_m.inbddocno
   IF SQLCA.SQLCODE THEN   #(ver:78)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aint160_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE   #(ver:78)
      LET g_errparam.popup = TRUE 
      CLOSE aint160_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aint160_master_referesh USING g_inbd_m.inbddocno INTO g_inbd_m.inbdsite,g_inbd_m.inbddocno, 
       g_inbd_m.inbddocdt,g_inbd_m.inbd001,g_inbd_m.inbd002,g_inbd_m.inbd003,g_inbd_m.inbd005,g_inbd_m.inbd006, 
       g_inbd_m.inbdstus,g_inbd_m.inbdownid,g_inbd_m.inbdowndp,g_inbd_m.inbdcrtid,g_inbd_m.inbdcrtdp, 
       g_inbd_m.inbdcrtdt,g_inbd_m.inbdmodid,g_inbd_m.inbdmoddt,g_inbd_m.inbdcnfid,g_inbd_m.inbdcnfdt, 
       g_inbd_m.inbd001_desc,g_inbd_m.inbd002_desc,g_inbd_m.inbdownid_desc,g_inbd_m.inbdowndp_desc,g_inbd_m.inbdcrtid_desc, 
       g_inbd_m.inbdcrtdp_desc,g_inbd_m.inbdmodid_desc,g_inbd_m.inbdcnfid_desc
   
   #檢查是否允許此動作
   IF NOT aint160_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_inbd_m_mask_o.* =  g_inbd_m.*
   CALL aint160_inbd_t_mask()
   LET g_inbd_m_mask_n.* =  g_inbd_m.*
   
   
   
   #add-point:modify段show之前 name="modify.before_show"
   
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
 
   
   CALL aint160_show()
   #add-point:modify段show之後 name="modify.after_show"
   
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
 
 
    
   WHILE TRUE
      LET g_inbddocno_t = g_inbd_m.inbddocno
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_inbd_m.inbdmodid = g_user 
LET g_inbd_m.inbdmoddt = cl_get_current()
LET g_inbd_m.inbdmodid_desc = cl_get_username(g_inbd_m.inbdmodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      IF g_inbd_m.inbdstus MATCHES "[DR]" THEN 
         LET g_inbd_m.inbdstus = "N"
      END IF

      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      LET g_master_commit = "N"
      CALL aint160_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後 name="modify.after_input"
      
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE inbd_t SET (inbdmodid,inbdmoddt) = (g_inbd_m.inbdmodid,g_inbd_m.inbdmoddt)
          WHERE inbdent = g_enterprise AND inbddocno = g_inbddocno_t
 
      END IF
    
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         #若單頭無commit則還原
         IF g_master_commit = "N" THEN
            LET g_inbd_m.* = g_inbd_m_t.*
            CALL aint160_show()
         END IF
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code = 9001 
         LET g_errparam.popup = FALSE 
         CALL cl_err()
         RETURN
