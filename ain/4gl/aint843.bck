# Memo...........:
# Usage..........: CALL aint843_inpd001_default()
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By lixh
# Modify.........:
################################################################################
PRIVATE FUNCTION aint843_inpd001_default()
   IF NOT cl_null(g_inpd_d[l_ac].inpd001) AND (g_inpd_d[l_ac].inpd001 <> g_inpd_d_o.inpd001 OR cl_null(g_inpd_d_o.inpd001)) THEN
      LET g_inpd_d[l_ac].inpd002 = ''
      LET g_inpd_d[l_ac].inpd002_desc = ''
      LET g_inpd_d[l_ac].inpd003 = ''
      LET g_inpd_d[l_ac].inpd004 = ''
      LET g_inpd_d[l_ac].inpd012 = ''
      LET g_inpd_d[l_ac].inpd012_desc = ''
      LET g_inpd_d[l_ac].inpd010 = ''
      LET g_inpd_d[l_ac].inpd010_desc = ''
      LET g_inpd_d[l_ac].inpd005 = ''
      LET g_inpd_d[l_ac].inpd005_desc = ''
      LET g_inpd_d[l_ac].inpd006 = ''
      LET g_inpd_d[l_ac].inpd006_desc = ''
      LET g_inpd_d[l_ac].inpd004 = ''
      LET g_inpd_d[l_ac].inpd004_desc = ''  
      LET g_inpd_d[l_ac].inpd001_desc = ''
      LET g_inpd_d[l_ac].inpd001_desc_desc = ''
            
      SELECT imaf015,imaf053,imaf091,imaf092,imaf157 INTO g_inpd_d[l_ac].inpd012,g_inpd_d[l_ac].inpd010,g_inpd_d[l_ac].inpd005,
                                                          g_inpd_d[l_ac].inpd006,g_inpd_d[l_ac].inpd004
        FROM imaf_t
       WHERE imafent = g_enterprise
         AND imafsite = g_site
         AND imaf001 = g_inpd_d[l_ac].inpd001
      CALL s_desc_get_unit_desc(g_inpd_d[l_ac].inpd010) RETURNING g_inpd_d[l_ac].inpd010_desc
      CALL s_desc_get_unit_desc(g_inpd_d[l_ac].inpd012) RETURNING g_inpd_d[l_ac].inpd012_desc
      CALL s_desc_get_stock_desc(g_site,g_inpd_d[l_ac].inpd005) RETURNING g_inpd_d[l_ac].inpd005_desc
      CALL s_desc_get_locator_desc(g_site,g_inpd_d[l_ac].inpd005,g_inpd_d[l_ac].inpd006) RETURNING g_inpd_d[l_ac].inpd006_desc
      CALL aint843_inpd004_ref()   
   END IF   
   DISPLAY BY NAME g_inpd_d[l_ac].inpd012,g_inpd_d[l_ac].inpd010,g_inpd_d[l_ac].inpd005,g_inpd_d[l_ac].inpd006,g_inpd_d[l_ac].inpd004,g_inpd_d[l_ac].inpd002_desc
   DISPLAY BY NAME g_inpd_d[l_ac].inpd012_desc,g_inpd_d[l_ac].inpd010_desc,g_inpd_d[l_ac].inpd005_desc,g_inpd_d[l_ac].inpd006_desc,g_inpd_d[l_ac].inpd004_desc  
   CALL aint843_inpd001_ref() 
END FUNCTION

 
