{<section id="aint843.set_no_entry_b" >}
#+ 單身欄位關閉設定
PRIVATE FUNCTION aint843_set_no_entry_b(p_cmd)                                               
   #add-point:set_no_entry_b段define(客製用) name="set_no_entry_b.define_customerization"
   
   #end add-point   
   DEFINE p_cmd   LIKE type_t.chr1           
   #add-point:set_no_entry_b段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="set_no_entry_b.define"
   DEFINE l_imaf055  LIKE imaf_t.imaf055
   DEFINE l_imaf056  LIKE imaf_t.imaf056
   DEFINE l_imaa005  LIKE imaa_t.imaa005
   DEFINE l_inaa007  LIKE inaa_t.inaa007
   DEFINE l_imaf061  LIKE imaf_t.imaf061 
   DEFINE l_inpd034  LIKE inpd_t.inpd034
   DEFINE l_inpd040  LIKE inpd_t.inpd040
   DEFINE l_inpd054  LIKE inpd_t.inpd054   
   DEFINE l_inpdstus LIKE inpd_t.inpdstus   
   #end add-point
   
   IF p_cmd = 'u' AND g_chkey = 'N' THEN
      CALL cl_set_comp_entry("",FALSE)
      #根據azzi850使用者身分開關特定欄位
      IF NOT cl_null(g_no_entry) THEN
         CALL cl_set_comp_entry(g_no_entry,FALSE)
      END IF
      #add-point:set_no_entry_b段欄位控制 name="set_no_entry_b.field_control"
      
      #end add-point 
   END IF
   
   #add-point:set_no_entry_b段control name="set_no_entry_b.set_no_entry_b"
   CALL cl_set_comp_entry("inpdsite,inpddocno,inpdseq",FALSE)
   IF l_ac = 0 THEN
      RETURN
   END IF
   IF l_ac > 0 THEN
      IF g_inpd_d[l_ac].inpd009 = 'N' THEN
         CALL cl_set_comp_entry("inpd001,inpd002,inpd003,inpd004,inpd010,inpd005,inpd006,inpd007",FALSE)
      END IF
   END IF
   SELECT imaf055,imaf056 INTO l_imaf055,l_imaf056 FROM imaf_t
    WHERE imafent = g_enterprise
      AND imafsite = g_site
      AND imaf001 = g_inpd_d[l_ac].inpd001 
   IF l_imaf055 = '2' THEN
      CALL cl_set_comp_entry("inpd003",FALSE)
   END IF  
   #料件是否做產品特徵管理
   SELECT imaa005 INTO l_imaa005 FROM imaa_t
    WHERE imaaent = g_enterprise
      AND imaa001 = g_inpd_d[l_ac].inpd001
   IF cl_null(l_imaa005) THEN
      CALL cl_set_comp_entry("inpd002",FALSE) 
   END IF  
  
   IF cl_null(g_inpd_d[l_ac].inpd012) THEN 
      CALL cl_set_comp_entry("inpd057",FALSE)    
   END IF    
   IF NOT cl_null(g_inpd_d[l_ac].inpd005) THEN
      LET l_inaa007 = ''
      SELECT inaa007 INTO l_inaa007 FROM inaa_t
       WHERE inaaent = g_enterprise
         AND inaasite = g_site
         AND inaa001 = g_inpd_d[l_ac].inpd005
      IF l_inaa007 = '5' THEN
         CALL cl_set_comp_entry("inpd006",FALSE)   
         LET g_inpd_d[l_ac].inpd006 = ' '
         DISPLAY BY NAME g_inpd_d[l_ac].inpd006         
      END IF      
   END IF
   IF NOT cl_null(g_inpd_d[l_ac].inpd001) THEN
      SELECT imaf061 INTO l_imaf061 FROM imaf_t
       WHERE imafent = g_enterprise
         AND imafsite = g_site
         AND imaf001 = g_inpd_d[l_ac].inpd001
      IF l_imaf061 = '2' THEN
         CALL cl_set_comp_entry("inpd007",FALSE)    
      END IF
   END IF 
   SELECT inpdstus,inpd034,inpd040,inpd054 INTO l_inpdstus,l_inpd034,l_inpd040,l_inpd054 FROM inpd_t 
    WHERE inpdent = g_enterprise
      AND inpdsite = g_site
      AND inpddocno = g_inpd_m.inpddocno
      AND inpdseq = g_inpd_m.inpdseq
   IF (l_inpdstus = '6' OR l_inpdstus = 'S') AND NOT cl_null(g_inpd_d[l_ac].inpd001) AND (NOT cl_null(l_inpd034) OR NOT cl_null(l_inpd040) OR NOT cl_null(l_inpd054))THEN
      CALL cl_set_comp_entry("inpd001,inpd002,inpd003,inpd004,inpd005,inpd006,inpd007,inpd010",FALSE)
   END IF 
   #end add-point       
                                                                                
END FUNCTION
 
