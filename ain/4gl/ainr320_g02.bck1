################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL s_aooi150_ins (传入参数)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 2016/07/20 By yany
# Modify.........: 2016/07/21 By yany
################################################################################
PRIVATE FUNCTION ainr320_g02_ins_erwei_temp(p_indadocno,p_imaal003,p_i,p_condition,p_item)
#str--add by yany 2016/07/20
#--計算交叉表數值區資料總和--
DEFINE p_indadocno  LIKE inda_t.indadocno
DEFINE p_imaal003   LIKE imaal_t.imaal003
DEFINE p_condition  LIKE type_t.chr1000
DEFINE p_item       LIKE type_t.chr100
DEFINE p_i          LIKE type_t.num5
DEFINE l_n          LIKE type_t.num5
DEFINE l_i          LIKE type_t.num5
DEFINE l_max        LIKE type_t.num5
DEFINE l_max2       LIKE type_t.num5
DEFINE l_a          LIKE type_t.num5
DEFINE l_b          LIKE type_t.num5
DEFINE l_sql        STRING
DEFINE l_indb008    STRING
DEFINE l_vi         varchar(3)
DEFINE i            LIKE type_t.num5

   CALL g_inam014.clear()
   
   LET l_n = 0
   SELECT COUNT(DISTINCT inam014) INTO l_n FROM ainr320_g02_temp01
    WHERE indadocno = p_indadocno 
      #AND imaal003 = p_imaal003   #161021-00007#3 mark
      #AND item = p_item           #161021-00007#3 mark
      AND condition = p_condition

   LET l_i = (l_n-1)/10
   LET l_i = l_i +1
   IF l_i = p_i THEN
      LET l_max = l_n
   ELSE
      LET l_max = p_i*10
   END IF

   IF l_max mod 10 = 0 THEN 
      LET l_max2 = 10
   ELSE
      LET l_max2 = l_max mod 10
   END IF

   LET l_a = p_i*10-9
   LET l_b = p_i*10
   LET l_sql = " SELECT inam014 FROM inam014_tmp",
               "  WHERE i >=? and i<=? ",
               "  ORDER BY i "
   PREPARE inam014_pr FROM l_sql
   DECLARE inam014_cs CURSOR FOR inam014_pr
   OPEN inam014_cs USING l_a,l_b
   LET i = 1
   FOREACH inam014_cs INTO g_inam014[i]
      IF i = l_max2 THEN
         EXIT FOREACH
      ELSE
         LET i = i +1
      END IF
   END FOREACH
   
   LET l_sql = " INSERT INTO indb_print_tmp(indadocno,item,imaal003,imaal004,inam012,inam014,inam018,indb105,condition  "
   FOR i=1 to l_max2
      CASE i WHEN 1  LET l_indb008 = 'indb0081'
             WHEN 2  LET l_indb008 = 'indb0082'
             WHEN 3  LET l_indb008 = 'indb0083'
             WHEN 4  LET l_indb008 = 'indb0084'
             WHEN 5  LET l_indb008 = 'indb0085'
             WHEN 6  LET l_indb008 = 'indb0086'
             WHEN 7  LET l_indb008 = 'indb0087'
             WHEN 8  LET l_indb008 = 'indb0088'
             WHEN 9  LET l_indb008 = 'indb0089'           
             WHEN 10 LET l_indb008 = 'indb0080'
      END CASE
      LET l_sql = l_sql , ",",l_indb008
   END FOR
   
   LET l_sql = l_sql," ) SELECT indadocno,item,imaal003,imaal004,inam012,'','',indb105,condition "
   FOR i=1 to l_max2
      LET l_vi = i
      LET l_sql = l_sql," ,sum(A",l_vi,")"
   END FOR
   LET l_sql = l_sql,"  FROM ( SELECT indadocno,item,imaal003,imaal004,inam012,inam014,inam018,indb105,condition "
 
   FOR i=1 to l_max2
      LET l_vi = i
      LET l_sql = l_sql,",CASE WHEN inam014 = '",g_inam014[i],"'",
                        " THEN indb008 ELSE 0 END A",l_vi
   END FOR
   LET l_sql = l_sql,  "  FROM ainr320_g02_temp01 ",
                       " WHERE indadocno='",p_indadocno,"'",
                       "   AND imaal003 = '",p_imaal003,"'",
                       "   AND condition = '",p_condition,"'",
                       "   AND item = '",p_item,"'",
                       "   ) ",
                       " GROUP BY indadocno,item,imaal003,imaal004,inam012,indb105,condition "
                              
   PREPARE indb_print_pr FROM l_sql
  # DELETE FROM indb_print_tmp WHERE 1=1    #161021-00007#mark
   EXECUTE indb_print_pr
   
   UPDATE indb_print_tmp  SET l_skip = "N"
    WHERE indadocno = p_indadocno 
      AND imaal003 = p_imaal003 
      AND inam014 = g_inam014[l_max2] 
      AND item = p_item
      
#end--add by yany 2016/07/20
END FUNCTION

 
