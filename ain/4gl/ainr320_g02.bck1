            LET l_sum.sum01=l_sum.sum01+ sr5.indb0081 
            LET l_sum.sum02=l_sum.sum02+ sr5.indb0082 
            LET l_sum.sum03=l_sum.sum03+ sr5.indb0083 
            LET l_sum.sum04=l_sum.sum04+ sr5.indb0084 
            LET l_sum.sum05=l_sum.sum05+ sr5.indb0085 
            LET l_sum.sum06=l_sum.sum06+ sr5.indb0086 
            LET l_sum.sum07=l_sum.sum07+ sr5.indb0087 
            LET l_sum.sum08=l_sum.sum08+ sr5.indb0088 
            LET l_sum.sum09=l_sum.sum09+ sr5.indb0089 
            LET l_sum.sum10=l_sum.sum10+ sr5.indb0080 
