       g_inph_m.inphowndp_desc,g_inph_m.inphcrtid_desc,g_inph_m.inphcrtdp_desc,g_inph_m.inphmodid_desc 
 
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = g_inph_m.inphsite 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      CLOSE aini910_cl
      CALL s_transaction_end('N','0')
      RETURN
