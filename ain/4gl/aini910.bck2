   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
 
   
   CALL aini910_show()
   #add-point:modify段show之後
   
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
 
 
    
   WHILE TRUE
      LET g_inphsite_t = g_inph_m.inphsite
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_inph_m.inphmodid = g_user 
LET g_inph_m.inphmoddt = cl_get_current()
LET g_inph_m.inphmodid_desc = cl_get_username(g_inph_m.inphmodid)
      
      #add-point:modify段修改前
      
      #end add-point
      
      #欄位更改
