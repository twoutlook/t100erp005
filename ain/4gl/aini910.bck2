      #若有modid跟moddt則進行update
      UPDATE inph_t SET (inphmodid,inphmoddt) = (g_inph_m.inphmodid,g_inph_m.inphmoddt)
       WHERE inphent = g_enterprise AND inphsite = g_inphsite_t
 
                  
