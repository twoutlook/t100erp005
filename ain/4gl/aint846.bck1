            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充 name="b_fill.fill"
         #產品特徵
         CALL s_feature_description(g_inpg_d[l_ac].inpg001,g_inpg_d[l_ac].inpg015) RETURNING r_success,g_inpg_d[l_ac].inpg015_desc 
         #end add-point
      
         IF l_ac > g_max_rec THEN
