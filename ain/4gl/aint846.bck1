       g_inpf_m.inpf003,g_inpf_m.imaal003,g_inpf_m.imaal004,g_inpf_m.inpf006,g_inpf_m.inpf006_desc,g_inpf_m.inpf007, 
       g_inpf_m.inpf002,g_inpf_m.inpf002_desc,g_inpf_m.sfaa061,g_inpf_m.inpf009,g_inpf_m.inpf009_desc, 
       g_inpf_m.inpfownid,g_inpf_m.inpfownid_desc,g_inpf_m.inpfowndp,g_inpf_m.inpfowndp_desc,g_inpf_m.inpfcrtid, 
