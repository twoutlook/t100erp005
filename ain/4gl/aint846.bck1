          g_inpg_d[l_ac].inpg007,g_inpg_d[l_ac].inpg053,g_inpg_d[l_ac].inpg054,g_inpg_d[l_ac].inpg055, 
          g_inpg_d[l_ac].inpg013,g_inpg_d[l_ac].inpg001_desc,g_inpg_d[l_ac].inpg001_desc_desc,g_inpg_d[l_ac].inpg007_desc, 
          g_inpg_d[l_ac].inpg054_desc   #(ver:78)
