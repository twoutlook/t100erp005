          g_inpf_m.inpfcrtid,g_inpf_m.inpfcrtid_desc,g_inpf_m.inpfcrtdp,g_inpf_m.inpfcrtdp_desc,g_inpf_m.inpfcrtdt, 
          g_inpf_m.inpfmodid,g_inpf_m.inpfmodid_desc,g_inpf_m.inpfmoddt,g_inpf_m.inpfcnfid,g_inpf_m.inpfcnfid_desc, 
          g_inpf_m.inpfcnfdt,g_inpf_m.inpfpstid,g_inpf_m.inpfpstid_desc,g_inpf_m.inpfpstdt
   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
