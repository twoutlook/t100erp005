      END IF
      #add by lixh 20150906
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "5"
      AND lc_state <> "6"
      AND lc_state <> "S"
      ) OR 
