                WHERE inpdent = g_enterprise AND 
                      inpdsite = g_inpd_d_t.inpdsite
                      AND inpddocno = g_inpd_d_t.inpddocno
                      AND inpdseq = g_inpd_d_t.inpdseq
 
                      
