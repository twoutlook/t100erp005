  <section id="aint930.modify" ver="5" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aint930_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   DEFINE l_wc2_table1          STRING
 
   DEFINE l_wc2_table2   STRING
 
 
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point    
   #add-point:modify段define(客製用)
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point    
   
   #保存單頭舊值
   LET g_inps_m_t.* = g_inps_m.*
   LET g_inps_m_o.* = g_inps_m.*
   
   IF g_inps_m.inpssite IS NULL
   OR g_inps_m.inpsdocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_inpssite_t = g_inps_m.inpssite
   LET g_inpsdocno_t = g_inps_m.inpsdocno
 
   CALL s_transaction_begin()
   
   OPEN aint930_cl USING g_enterprise,g_inps_m.inpssite,g_inps_m.inpsdocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aint930_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE aint930_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aint930_master_referesh USING g_inps_m.inpssite,g_inps_m.inpsdocno INTO g_inps_m.inpsdocno, 
       g_inps_m.inps007,g_inps_m.inps006,g_inps_m.inps001,g_inps_m.inps005,g_inps_m.inpssite,g_inps_m.inpsstus, 
       g_inps_m.inpsownid,g_inps_m.inpsowndp,g_inps_m.inpscrtid,g_inps_m.inpscrtdp,g_inps_m.inpscrtdt, 
       g_inps_m.inpsmodid,g_inps_m.inpsmoddt,g_inps_m.inpscnfid,g_inps_m.inpscnfdt,g_inps_m.inpspstid, 
       g_inps_m.inpspstdt,g_inps_m.inps006_desc,g_inps_m.inps005_desc,g_inps_m.inpsownid_desc,g_inps_m.inpsowndp_desc, 
       g_inps_m.inpscrtid_desc,g_inps_m.inpscrtdp_desc,g_inps_m.inpsmodid_desc,g_inps_m.inpscnfid_desc, 
       g_inps_m.inpspstid_desc
   
   #檢查是否允許此動作
   IF NOT aint930_action_chk() THEN
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_inps_m_mask_o.* =  g_inps_m.*
   CALL aint930_inps_t_mask()
   LET g_inps_m_mask_n.* =  g_inps_m.*
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
   #LET l_wc2_table2 = g_wc2_table2
   #LET l_wc2_table2 = " 1=1"
 
 
   
   CALL aint930_show()
   #add-point:modify段show之後
   {<point name="modify.after_show"/>}
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
 
   #LET  g_wc2_table2 = l_wc2_table2 
 
 
    
   WHILE TRUE
      LET g_inpssite_t = g_inps_m.inpssite
      LET g_inpsdocno_t = g_inps_m.inpsdocno
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_inps_m.inpsmodid = g_user 
LET g_inps_m.inpsmoddt = cl_get_current()
LET g_inps_m.inpsmodid_desc = cl_get_username(g_inps_m.inpsmodid)
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      CALL aint930_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE inps_t SET (inpsmodid,inpsmoddt) = (g_inps_m.inpsmodid,g_inps_m.inpsmoddt)
          WHERE inpsent = g_enterprise AND inpssite = g_inpssite_t
            AND inpsdocno = g_inpsdocno_t
 
      END IF
    
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_inps_m.* = g_inps_m_t.*
         CALL aint930_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_inps_m.inpssite != g_inpssite_t 
      OR g_inps_m.inpsdocno != g_inpsdocno_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE inpl_t SET inplsite = g_inps_m.inpssite
                                       ,inpldocno = g_inps_m.inpsdocno
 
          WHERE inplent = g_enterprise AND inplsite = g_inpssite_t
            AND inpldocno = g_inpsdocno_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "inpl_t" 
               LET g_errparam.code   = "std-00009" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "inpl_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
         #更新單身key值
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update2" mark="Y"/>}
         #end add-point
         UPDATE inpm_t
            SET inpmsite = g_inps_m.inpssite
               ,inpmdocno = g_inps_m.inpsdocno
 
          WHERE inpment = g_enterprise AND
                inpmsite = g_inpssite_t
            AND inpmdocno = g_inpsdocno_t
 
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update2"/>}
         #end add-point
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "inpm_t" 
               LET g_errparam.code   = "std-00009" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "inpm_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update2"/>}
         #end add-point
 
 
         
         #UPDATE 多語言table key值
         
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL aint930_set_act_visible()   
   CALL aint930_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " inpsent = '" ||g_enterprise|| "' AND",
                      " inpssite = '", g_inps_m.inpssite, "' "
                      ," AND inpsdocno = '", g_inps_m.inpsdocno, "' "
 
   #填到對應位置
   CALL aint930_browser_fill("")
 
   CLOSE aint930_cl
   
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aint930_msgcentre_notify('')
 
END FUNCTION   
]]>
