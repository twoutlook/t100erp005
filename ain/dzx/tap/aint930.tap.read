#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By lixh
# Modify.........:
################################################################################
PRIVATE FUNCTION aint930_set_no_entry_b2()
DEFINE  l_imaf071    LIKE imaf_t.imaf071
DEFINE  l_imaf081    LIKE imaf_t.imaf081
DEFINE  l_cnt        LIKE type_t.num5

   SELECT imaf071,imaf081 INTO l_imaf071,l_imaf081 FROM imaf_t
    WHERE imafent = g_enterprise
      AND imafsite = g_site
      AND imaf001 = g_inpl_d[g_detail_idx].inpl001
   IF l_imaf071 = '2' THEN
      CALL cl_set_comp_entry("inpm008",FALSE) 
   END IF 
   IF l_imaf081 = '2' THEN
      CALL cl_set_comp_entry("inpm009",FALSE) 
   END IF 
   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt FROM inai_t
    WHERE inaient = g_enterprise
      AND inaisite = g_site
      AND inai001 = g_inpl2_d[l_ac].inpm001
      AND inai002 = g_inpl_d[g_detail_idx].inpl002
      AND inai003 = g_inpl_d[g_detail_idx].inpl003
      AND inai004 = g_inpl_d[g_detail_idx].inpl005
      AND inai005 = g_inpl_d[g_detail_idx].inpl006
      AND inai006 = g_inpl_d[g_detail_idx].inpl007
      AND inai007 = g_inpl2_d[l_ac].inpm008
      AND inai008 = g_inpl2_d[l_ac].inpm009
      AND inai009 = g_inpl_d[g_detail_idx].inpl009  
   IF l_cnt > 0 THEN
      CALL cl_set_comp_entry("inpm008,inpm009",FALSE)
   END IF   
#   IF l_imaf081 <> '2' THEN  #初盤&復盤
#      CALL cl_set_comp_entry("inpm030,inpm050",FALSE)
#   END IF
END FUNCTION]]>
