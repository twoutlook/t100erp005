                     #製造批序號
                     SELECT imaf071,imaf081 INTO l_imaf071,l_imaf081 FROM imaf_t
                      WHERE imafent = g_enterprise
                        AND imafsite = g_site
                        AND imaf001 = l_inpl.inpl001
                        
                     IF l_imaf071 <> '2' OR l_imaf081 <> '2' THEN
                        LET l_sql = " SELECT * FROM inai_t ",
                                    "  WHERE inaient = '",g_enterprise,"'",
                                    "    AND inaisite = '",g_site,"'",
                                    "    AND inai001 = '",l_inpl.inpl001,"'",
                                    "    AND inai002 = '",l_inpl.inpl002,"'",
                                    "    AND inai003 = '",l_inpl.inpl003,"'",
                                    "    AND inai004 = '",l_inpl.inpl005,"'",
                                    "    AND inai005 = '",l_inpl.inpl006,"'",
                                    "    AND inai006 = '",l_inpl.inpl007,"'",
                                    "    AND inai009 = '",l_inpl.inpl009,"'"
                         PREPARE aint930_inai_pre FROM l_sql
