                            LET l_inpm.inpmseq = l_inpl.inplseq                                                        
                            LET l_inpm.inpm001 = l_inai.inai001
                            LET l_inpm.inpm002 = l_inai.inai002
                            LET l_inpm.inpm003 = l_inai.inai003
                            LET l_inpm.inpm004 = ''
                            LET l_inpm.inpm005 = l_inai.inai004  
                            LET l_inpm.inpm006 = l_inai.inai005
                            LET l_inpm.inpm007 = l_inai.inai006
                            LET l_inpm.inpm008 = l_inai.inai007
                            LET l_inpm.inpm009 = l_inai.inai008
                            LET l_inpm.inpm010 = l_inai.inai012
                            LET l_inpm.inpm012 = l_inai.inai010
                            LET l_inpm.inpm030 = l_inpm.inpm012
                            LET l_inpm.inpm031 = g_inps_m.inps006
                            LET l_inpm.inpm032 = g_inps_m.inps007
                            LET l_inpm.inpm033 = g_user
