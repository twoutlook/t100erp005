  <section id="aint930.b_fill" ver="2" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aint930_b_fill()
   DEFINE p_wc2      STRING
   DEFINE li_idx     LIKE type_t.num10
   #add-point:b_fill段define
   {<point name="b_fill.define" edit="s"/>}
   #end add-point     
   #add-point:b_fill段define(客製用)
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point     
 
   CALL g_inpl_d.clear()    #g_inpl_d 單頭及單身 
   CALL g_inpl2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF aint930_fill_chk(1) THEN
      #切換上下筆時不重組SQL
      IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
         LET g_sql = "SELECT  UNIQUE inplseq,inpl001,inpl005,inpl006,inpl007,inpl002,inpl003,inpl004, 
             inpl009,inpl010,inpl011,inpl012,inpl030,inpl031,inpl032,inpl033,inpl013,inpl050,inpl051, 
             inpl052,inpl053,inpl014 ,t1.imaal003 ,t2.imaal004 ,t3.inayl003 ,t4.inab003 ,t5.imaal003 , 
             t6.oocal003 ,t7.oocal003 ,t8.ooag011 ,t9.ooag011 FROM inpl_t",   
                     " INNER JOIN inps_t ON inpssite = inplsite ",
                     " AND inpsdocno = inpldocno ",
 
                     #"",
                     " LEFT JOIN inpm_t ON inplent = inpment AND inplsite = inpmsite AND inpldocno = inpmdocno AND inplseq = inpmseq ",
                     "",
                                    " LEFT JOIN imaal_t t1 ON t1.imaalent='"||g_enterprise||"' AND t1.imaal001=inpl001 AND t1.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t2 ON t2.imaalent='"||g_enterprise||"' AND t2.imaal001=inpl001 AND t2.imaal002='"||g_dlang||"' ",
               " LEFT JOIN inayl_t t3 ON t3.inaylent='"||g_enterprise||"' AND t3.inayl001=inpl005 AND t3.inayl002='"||g_dlang||"' ",
               " LEFT JOIN inab_t t4 ON t4.inabent='"||g_enterprise||"' AND t4.inabsite=inplsite AND t4.inab001=inpl005 AND t4.inab002=inpl006  ",
               " LEFT JOIN imaal_t t5 ON t5.imaalent='"||g_enterprise||"' AND t5.imaal001=inpl004 AND t5.imaal002='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t6 ON t6.oocalent='"||g_enterprise||"' AND t6.oocal001=inpl009 AND t6.oocal002='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t7 ON t7.oocalent='"||g_enterprise||"' AND t7.oocal001=inpl011 AND t7.oocal002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t8 ON t8.ooagent='"||g_enterprise||"' AND t8.ooag001=inpl032  ",
               " LEFT JOIN ooag_t t9 ON t9.ooagent='"||g_enterprise||"' AND t9.ooag001=inpl052  ",
 
                     " WHERE inplent=? AND inplsite=? AND inpldocno=?"
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before
         {<point name="b_fill.body.fill_sql"/>}
         #end add-point
         IF NOT cl_null(g_wc2_table1) THEN
            LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
         END IF
         
         #子單身的WC
            IF NOT cl_null(g_wc2_table2) THEN 
      LET g_sql = g_sql CLIPPED," AND ", g_wc2_table2 CLIPPED
   END IF 
 
         
         LET g_sql = g_sql, " ORDER BY inpl_t.inplseq"
         
         #add-point:單身填充控制
         {<point name="b_fill.sql"/>}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE aint930_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR aint930_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_inps_m.inpssite,g_inps_m.inpsdocno
                                               
      FOREACH b_fill_cs INTO g_inpl_d[l_ac].inplseq,g_inpl_d[l_ac].inpl001,g_inpl_d[l_ac].inpl005,g_inpl_d[l_ac].inpl006, 
          g_inpl_d[l_ac].inpl007,g_inpl_d[l_ac].inpl002,g_inpl_d[l_ac].inpl003,g_inpl_d[l_ac].inpl004, 
          g_inpl_d[l_ac].inpl009,g_inpl_d[l_ac].inpl010,g_inpl_d[l_ac].inpl011,g_inpl_d[l_ac].inpl012, 
          g_inpl_d[l_ac].inpl030,g_inpl_d[l_ac].inpl031,g_inpl_d[l_ac].inpl032,g_inpl_d[l_ac].inpl033, 
          g_inpl_d[l_ac].inpl013,g_inpl_d[l_ac].inpl050,g_inpl_d[l_ac].inpl051,g_inpl_d[l_ac].inpl052, 
          g_inpl_d[l_ac].inpl053,g_inpl_d[l_ac].inpl014,g_inpl_d[l_ac].inpl001_desc,g_inpl_d[l_ac].inpl001_desc_desc, 
          g_inpl_d[l_ac].inpl005_desc,g_inpl_d[l_ac].inpl006_desc,g_inpl_d[l_ac].inpl004_desc,g_inpl_d[l_ac].inpl009_desc, 
          g_inpl_d[l_ac].inpl011_desc,g_inpl_d[l_ac].inpl032_desc,g_inpl_d[l_ac].inpl052_desc
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF
            EXIT FOREACH
         END IF
         
         LET l_ac = l_ac + 1
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
 
   
   #add-point:browser_fill段其他table處理
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_inpl_d.deleteElement(g_inpl_d.getLength())
   CALL g_inpl2_d.deleteElement(g_inpl2_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE aint930_pb
 
   
   LET li_idx = l_ac
   
   #遮罩相關處理
   FOR l_ac = 1 TO g_inpl_d.getLength()
      LET g_inpl_d_mask_o[l_ac].* =  g_inpl_d[l_ac].*
      CALL aint930_inpl_t_mask()
      LET g_inpl_d_mask_n[l_ac].* =  g_inpl_d[l_ac].*
   END FOR
   
   LET g_inpl2_d_mask_o.* =  g_inpl2_d.*
   FOR l_ac = 1 TO g_inpl2_d.getLength()
      LET g_inpl2_d_mask_o[l_ac].* =  g_inpl2_d[l_ac].*
      CALL aint930_inpm_t_mask()
      LET g_inpl2_d_mask_n[l_ac].* =  g_inpl2_d[l_ac].*
   END FOR
 
   
   LET l_ac = li_idx
   
   CALL cl_ap_performance_next_end()
 
END FUNCTION
]]>
  </section>
  <section id="aint930.b_fill2" ver="4" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充2
PRIVATE FUNCTION aint930_b_fill2(pi_idx)
   DEFINE pi_idx          LIKE type_t.num10
   DEFINE li_ac           LIKE type_t.num10
   DEFINE ls_chk          LIKE type_t.chr1
   #add-point:b_fill2段define
   {<point name="b_fill2.define" edit="s"/>}
   #end add-point
   #add-point:b_fill2段define(客製用)
   {<point name="b_fill2.define_customerization" edit="c"/>}
   #end add-point
   
   LET li_ac = l_ac 
   
   IF g_detail_idx <= 0 THEN
      RETURN
   END IF
   
   IF aint930_fill_chk(2) THEN
      IF (pi_idx = 2 OR pi_idx = 0 ) AND g_inpl_d.getLength() > 0 THEN
               CALL g_inpl2_d.clear()
 
         LET g_sql = "SELECT  UNIQUE inpmseq1,inpm001,inpm002,inpm003,inpm004,inpm005,inpm006,inpm007, 
             inpm008,inpm009,inpm010,inpm011,inpm012,inpm030,inpm031,inpm032,inpm033,inpm034,inpm050, 
             inpm051,inpm052,inpm053,inpm054 ,t10.imaal003 ,t11.imaal004 ,t12.ooag011 ,t13.ooag011 , 
             t14.ooag011 ,t15.ooag011 FROM inpm_t",    
                     "",
                                    " LEFT JOIN imaal_t t10 ON t10.imaalent='"||g_enterprise||"' AND t10.imaal001=inpm001 AND t10.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t11 ON t11.imaalent='"||g_enterprise||"' AND t11.imaal001=inpm001 AND t11.imaal002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t12 ON t12.ooagent='"||g_enterprise||"' AND t12.ooag001=inpm031  ",
               " LEFT JOIN ooag_t t13 ON t13.ooagent='"||g_enterprise||"' AND t13.ooag001=inpm033  ",
               " LEFT JOIN ooag_t t14 ON t14.ooagent='"||g_enterprise||"' AND t14.ooag001=inpm051  ",
               " LEFT JOIN ooag_t t15 ON t15.ooagent='"||g_enterprise||"' AND t15.ooag001=inpm053  ",
 
                     " WHERE inpment=? AND inpmsite=? AND inpmdocno=? AND inpmseq=?"
         
         IF NOT cl_null(g_wc2_table2) THEN
            LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
         END IF
         
         LET g_sql = g_sql, " ORDER BY  inpm_t.inpmseq1" 
                            
         #add-point:單身填充前
         {<point name="b_fill2.before_fill2" />}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE aint930_pb2 FROM g_sql
         DECLARE b_fill_curs2 CURSOR FOR aint930_pb2
         
         OPEN b_fill_curs2 USING g_enterprise,g_inps_m.inpssite,g_inps_m.inpsdocno,g_inpl_d[g_detail_idx].inplseq 

         LET l_ac = 1
         FOREACH b_fill_curs2 INTO g_inpl2_d[l_ac].inpmseq1,g_inpl2_d[l_ac].inpm001,g_inpl2_d[l_ac].inpm002, 
             g_inpl2_d[l_ac].inpm003,g_inpl2_d[l_ac].inpm004,g_inpl2_d[l_ac].inpm005,g_inpl2_d[l_ac].inpm006, 
