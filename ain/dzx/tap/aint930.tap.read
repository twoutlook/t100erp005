      CALL cl_set_comp_visible('inpm050,inpm051,inpm052,inpm052_desc,inpm053',FALSE)      
   END IF   
   IF l_inpj015 = 'Y' THEN
      IF cl_get_para(g_enterprise,g_site,'S-BAS-0028') = 'Y' THEN   
