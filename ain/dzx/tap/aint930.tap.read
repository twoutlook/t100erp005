  <section id="aint930.insert" ver="7" status="" src="s" readonly="">
    <![CDATA[#+ 資料新增
PRIVATE FUNCTION aint930_insert()
   #add-point:insert段define
   {<point name="insert.define" edit="s"/>}
   #end add-point    
   #add-point:insert段define(客製用)
   {<point name="insert.define_customerization" edit="c"/>}
   #end add-point    
   
   #清畫面欄位內容
   CLEAR FORM                    
   CALL g_inpl_d.clear()   
   CALL g_inpl2_d.clear()  
 
 
   INITIALIZE g_inps_m.* LIKE inps_t.*             #DEFAULT 設定
   
   LET g_inpssite_t = NULL
   LET g_inpsdocno_t = NULL
 
   
   LET g_master_insert = FALSE
   
   #add-point:insert段before
   {<point name="insert.before"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   WHILE TRUE
      #公用欄位給值(單頭)
      #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_inps_m.inpsownid = g_user
      LET g_inps_m.inpsowndp = g_dept
      LET g_inps_m.inpscrtid = g_user
      LET g_inps_m.inpscrtdp = g_dept 
      LET g_inps_m.inpscrtdt = cl_get_current()
      LET g_inps_m.inpsmodid = g_user
      LET g_inps_m.inpsmoddt = cl_get_current()
      LET g_inps_m.inpsstus = 'N'
 
 
 
      #append欄位給值
      
     
      #一般欄位給值
      
  
      #add-point:單頭預設值
      {<point name="insert.default"/>}
      #end add-point 
      
      #保存單頭舊值(用於資料輸入錯誤還原預設值時使用)
      LET g_inps_m_t.* = g_inps_m.*
      LET g_inps_m_o.* = g_inps_m.*
      
      #顯示狀態(stus)圖片
            #應用 a21 樣板自動產生(Version:2)
	  #根據當下狀態碼顯示圖片
      CASE g_inps_m.inpsstus 
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "T"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/org_approved.png")
         WHEN "5"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/fconfirmed.png")
         WHEN "6"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/dconfirmed.png")
         WHEN "S"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/posted.png")
         WHEN "A"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/approved.png")
         WHEN "D"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/withdraw.png")
         WHEN "R"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/rejection.png")
         WHEN "W"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/signing.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
 
 
    
      CALL aint930_input("a")
      
      #add-point:單頭輸入後
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
      END IF
      
      IF NOT g_master_insert THEN
         DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
         DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
         INITIALIZE g_inps_m.* TO NULL
         INITIALIZE g_inpl_d TO NULL
         INITIALIZE g_inpl2_d TO NULL
 
         #add-point:取消新增後
         {<point name="insert.cancel"/>}
         #end add-point 
         CALL aint930_show()
         RETURN
      END IF
      
      LET INT_FLAG = 0
      #CALL g_inpl_d.clear()
      #CALL g_inpl2_d.clear()
 
 
      LET g_rec_b = 0
      CALL s_transaction_end('Y','0')
      EXIT WHILE
        
   END WHILE
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL aint930_set_act_visible()   
   CALL aint930_set_act_no_visible()
   
   #將新增的資料併入搜尋條件中
   LET g_inpssite_t = g_inps_m.inpssite
   LET g_inpsdocno_t = g_inps_m.inpsdocno
 
   
   #組合新增資料的條件
   LET g_add_browse = " inpsent = '" ||g_enterprise|| "' AND",
                      " inpssite = '", g_inps_m.inpssite, "' "
                      ," AND inpsdocno = '", g_inps_m.inpsdocno, "' "
 
                      
   #add-point:組合新增資料的條件後
   {<point name="insert.after.add_browse"/>}
   #end add-point
      
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL aint930_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   CLOSE aint930_cl
   
   CALL aint930_idx_chk()
   
   #撈取異動後的資料(主要是帶出reference)
   EXECUTE aint930_master_referesh USING g_inps_m.inpssite,g_inps_m.inpsdocno INTO g_inps_m.inpsdocno, 
       g_inps_m.inps007,g_inps_m.inps006,g_inps_m.inps001,g_inps_m.inps005,g_inps_m.inpssite,g_inps_m.inpsstus, 
       g_inps_m.inpsownid,g_inps_m.inpsowndp,g_inps_m.inpscrtid,g_inps_m.inpscrtdp,g_inps_m.inpscrtdt, 
       g_inps_m.inpsmodid,g_inps_m.inpsmoddt,g_inps_m.inpscnfid,g_inps_m.inpscnfdt,g_inps_m.inpspstid, 
       g_inps_m.inpspstdt,g_inps_m.inps006_desc,g_inps_m.inps005_desc,g_inps_m.inpsownid_desc,g_inps_m.inpsowndp_desc, 
       g_inps_m.inpscrtid_desc,g_inps_m.inpscrtdp_desc,g_inps_m.inpsmodid_desc,g_inps_m.inpscnfid_desc, 
       g_inps_m.inpspstid_desc
   
   #遮罩相關處理
   LET g_inps_m_mask_o.* =  g_inps_m.*
   CALL aint930_inps_t_mask()
   LET g_inps_m_mask_n.* =  g_inps_m.*
   
   #將資料顯示到畫面上
   DISPLAY BY NAME g_inps_m.inpsdocno,g_inps_m.inps007,g_inps_m.inps006,g_inps_m.inps006_desc,g_inps_m.inpsdocno_desc, 
       g_inps_m.inps001,g_inps_m.inps005,g_inps_m.inps005_desc,g_inps_m.inpssite,g_inps_m.inpsstus,g_inps_m.inpsownid, 
       g_inps_m.inpsownid_desc,g_inps_m.inpsowndp,g_inps_m.inpsowndp_desc,g_inps_m.inpscrtid,g_inps_m.inpscrtid_desc, 
       g_inps_m.inpscrtdp,g_inps_m.inpscrtdp_desc,g_inps_m.inpscrtdt,g_inps_m.inpsmodid,g_inps_m.inpsmodid_desc, 
       g_inps_m.inpsmoddt,g_inps_m.inpscnfid,g_inps_m.inpscnfid_desc,g_inps_m.inpscnfdt,g_inps_m.inpspstid, 
       g_inps_m.inpspstid_desc,g_inps_m.inpspstdt
   
   #add-point:新增結束後
   {<point name="insert.after"/>}
   #end add-point 
   
   #功能已完成,通報訊息中心
   CALL aint930_msgcentre_notify('')
   
END FUNCTION
]]>
  </section>
