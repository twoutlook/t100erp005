             g_inpl2_d[l_ac].inpm032,g_inpl2_d[l_ac].inpm033,g_inpl2_d[l_ac].inpm034,g_inpl2_d[l_ac].inpm050, 
             g_inpl2_d[l_ac].inpm051,g_inpl2_d[l_ac].inpm052,g_inpl2_d[l_ac].inpm053,g_inpl2_d[l_ac].inpm054, 
             g_inpl2_d[l_ac].inpm001_desc,g_inpl2_d[l_ac].inpm001_desc_desc,g_inpl2_d[l_ac].inpm031_desc, 
             g_inpl2_d[l_ac].inpm033_desc,g_inpl2_d[l_ac].inpm051_desc,g_inpl2_d[l_ac].inpm053_desc  

            IF SQLCA.sqlcode THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "FOREACH:" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               EXIT FOREACH
            END IF
            
            #add-point:b_fill段資料填充
