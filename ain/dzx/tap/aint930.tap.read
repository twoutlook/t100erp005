  <point name="input.head.m_insert" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
               
               #自動產生單身
               IF l_cmd_t <> 'r' THEN
                  LET l_cnt = 0
                  SELECT COUNT(*) INTO l_cnt FROM inpl_t
                   WHERE inplent = g_enterprise
                     AND inplsite = g_site
                     AND inpldocno = g_inps_m.inpsdocno
                  IF cl_null(l_cnt) THEN LET l_cnt = 0 END IF    
               IF cl_ask_confirm('ain-00186') AND l_cnt = 0 THEN
                  SELECT COUNT(*) INTO l_cnt FROM inpn_t   #產品分類
                   WHERE inpnent = g_enterprise
                     AND inpnsite = g_site
                     AND inpndocno = g_inps_m.inps001
                  IF cl_null(l_cnt) THEN LET l_cnt = 0 END IF  
                  SELECT COUNT(*) INTO l_cnt1 FROM inpk_t   #庫位
                   WHERE inpkent = g_enterprise
                     AND inpksite = g_site
                     AND inpkdocno = g_inps_m.inps001 
                  IF cl_null(l_cnt1) THEN LET l_cnt1 = 0 END IF   
                  SELECT inpj008,inpj009,inpj010,inpj011,inpj014 INTO l_inpj008,l_inpj009,l_inpj010,l_inpj011,l_inpj014 FROM inpj_t 
                   WHERE inpjent = g_enterprise
                     AND inpjsite = g_site
                     AND inpjdocno = g_inps_m.inps001 
                     
                  LET l_sql = " SELECT DISTINCT inag001,inag002,inag003,inag004,inag005,inag006,inag007,inag008,inag009,inag024,inag025 FROM inag_t,imaa_t ",
                              "  WHERE imaaent = inagent ",
                              "    AND imaa001 = inag001 ",
                              "    AND inagent = '",g_enterprise,"'",
                              "    AND inagsite = '",g_site,"'",
                              "    AND imaastus = 'Y'"
                  IF l_inpj011 = 'N' THEN   
                     LET l_sql = l_sql," AND inag008 > 0 "                  
                  END IF
                  IF l_inpj008 = 'Y' OR l_inpj009 = 'Y' OR l_inpj010 = 'Y' THEN
                     LET l_sql = " SELECT DISTINCT inag001,inag002,inag003,inag004,inag005,inag006,inag007,inag008,inag009,inag024,inag025 FROM inag_t,imaa_t ",
                                 "   LEFT OUTER JOIN imaf_t ON imafent = imaaent AND imaf001 = imaa001 ",
                                 "   AND imafsite = '",g_site,"'", 
                                 "  WHERE imaaent = inagent ",
                                 "    AND imaa001 = inag001 ",
                                 "    AND inagent = '",g_enterprise,"'",
                                 "    AND inagsite = '",g_site,"'",
                                 "    AND imaastus = 'Y'"
                                 
                     IF l_inpj011 = 'N' THEN   
                        LET l_sql = l_sql," AND inag008 > 0 "                  
                     END IF                                 
                     IF l_inpj008 = 'Y' THEN
                        LET l_sql = l_sql," AND (imaf057 = 'A'"
                     END IF   
                     IF l_inpj009 = 'Y' AND l_inpj008 = 'Y' THEN
                        LET l_sql = l_sql," OR imaf057 = 'B'"   
                     END IF   
                     IF l_inpj009 = 'Y' AND l_inpj008 = 'N' THEN
                        LET l_sql = l_sql," AND (imaf057 = 'B'"   
                     END IF                      
                     IF l_inpj010 = 'Y' AND (l_inpj009 = 'Y' OR l_inpj008 = 'Y') THEN
                        LET l_sql = l_sql," OR imaf057 = 'C' )"
                     END IF    
                     IF l_inpj010 = 'Y' AND l_inpj009 = 'N' OR l_inpj008 = 'N' THEN
                        LET l_sql = l_sql," AND  imaf057 = 'C' "
                     END IF                       
                  END IF                  
                  IF l_cnt > 0 THEN  #產品分類
                     LET l_sql = l_sql," AND imaa009 IN (",
                                       " SELECT inpn001 FROM inpn_t WHERE inpnent = '",g_enterprise,"'",
                                       "    AND inpnsite = '",g_site,"'",
                                       "    AND inpndocno = '",g_inps_m.inps001,"')"
                  END IF  
                  IF l_cnt1 > 0 THEN #庫位
                     LET l_sql = l_sql," AND inag004 IN (",
                                       " SELECT inpk001 FROM inpk_t WHERE inpkent = '",g_enterprise,"'",
                                       "    AND inpksite = '",g_site,"'",
                                       "    AND inpkdocno = '",g_inps_m.inps001,"')"                     
                  END IF
                  LET l_sql = l_sql," ORDER BY inag001,inag002,inag003,inag004,inag005,inag006,inag007 "
                  PREPARE aint930_inag_pre FROM l_sql
                  DECLARE b_fill_inag_cur CURSOR FOR aint930_inag_pre      
                  FOREACH b_fill_inag_cur INTO l_inag.inag001,l_inag.inag002,l_inag.inag003,l_inag.inag004,l_inag.inag005,l_inag.inag006,
                                               l_inag.inag007,l_inag.inag008,l_inag.inag009,l_inag.inag024,l_inag.inag025
                                               
                     SELECT MAX(inplseq)+1 INTO l_inpl.inplseq FROM inpl_t
                      WHERE inplent = g_enterprise
                        AND inplsite = g_site
                        AND inpldocno = g_inps_m.inpsdocno
                     IF cl_null(l_inpl.inplseq) THEN LET l_inpl.inplseq = 1 END IF
                     LET l_inpl.inpl001 = l_inag.inag001  
                     LET l_inpl.inpl002 = l_inag.inag002 
                     LET l_inpl.inpl003 = l_inag.inag003  
                     LET l_inpl.inpl004 = ''                     
                     LET l_inpl.inpl005 = l_inag.inag004 
                     LET l_inpl.inpl006 = l_inag.inag005  
                     LET l_inpl.inpl007 = l_inag.inag006  
                     LET l_inpl.inpl009 = l_inag.inag007  
                     LET l_inpl.inpl010 = l_inag.inag008  
                     LET l_inpl.inpl011 = l_inag.inag024  
                     LET l_inpl.inpl012 = l_inag.inag025
                     LET l_inpl.inpl030 = l_inag.inag008 
                     LET l_inpl.inpl031 = l_inag.inag025  
                     LET l_inpl.inpl032 = g_user
                     LET l_inpl.inpl033 = g_today   
                     IF l_inpj014 = 'Y' THEN  #復盤
                        LET l_inpl.inpl050 = l_inag.inag008
                        LET l_inpl.inpl051 = l_inag.inag025  
                        LET l_inpl.inpl052 = g_user
                        LET l_inpl.inpl053 = g_today                       
                     END IF    
                     INSERT INTO inpl_t (inplent,inplsite,inpldocno,inplseq,inpl008,inpl001,inpl002,inpl003,inpl004,inpl005,inpl006,
                                         inpl007,inpl009,inpl010,inpl011,inpl012,inpl030,inpl031,inpl032,inpl033,inpl050,inpl051,
                                         inpl052,inpl053)
                                  VALUES (g_enterprise,g_site,g_inps_m.inpsdocno,l_inpl.inplseq,g_inps_m.inps001,l_inpl.inpl001,l_inpl.inpl002,
