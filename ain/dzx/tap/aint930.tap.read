  <point name="function.aint930_inag_chk" order="3" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 是否存在庫存明細
# Memo...........:
# Usage..........: CALL aint930_inag_chk()
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By lixh
# Modify.........:
################################################################################
PRIVATE FUNCTION aint930_inag_chk()
DEFINE l_n          LIKE type_t.num5
DEFINE l_sql        STRING
   IF l_ac = 0 THEN
      RETURN TRUE
   END IF
   LET l_sql = "SELECT COUNT(*) FROM inag_t WHERE inagent = ",g_enterprise,
               " AND inagsite='",g_site,"' "
               
   IF NOT cl_null(g_inpl_d[l_ac].inpl001) THEN
      LET l_sql = l_sql," AND inag001 = '",g_inpl_d[l_ac].inpl001,"'"
   END IF  
   IF NOT cl_null(g_inpl_d[l_ac].inpl002) THEN
      LET l_sql = l_sql," AND inag002 = '",g_inpl_d[l_ac].inpl002,"'"
   END IF  
   IF NOT cl_null(g_inpl_d[l_ac].inpl003) THEN
      LET l_sql = l_sql," AND inag003 = '",g_inpl_d[l_ac].inpl003,"'"
   END IF  
   IF NOT cl_null(g_inpl_d[l_ac].inpl005) THEN
      LET l_sql = l_sql," AND inag004 = '",g_inpl_d[l_ac].inpl005,"'"
   END IF  
   IF NOT cl_null(g_inpl_d[l_ac].inpl006) THEN
      LET l_sql = l_sql," AND inag005 = '",g_inpl_d[l_ac].inpl006,"'"
   END IF  
   IF NOT cl_null(g_inpl_d[l_ac].inpl007) THEN
      LET l_sql = l_sql," AND inag006 = '",g_inpl_d[l_ac].inpl007,"'"
   END IF 
   IF NOT cl_null(g_inpl_d[l_ac].inpl009) THEN
      LET l_sql = l_sql," AND inag007 = '",g_inpl_d[l_ac].inpl009,"'"
   END IF    
   PREPARE sel_inag_chk FROM l_sql
   LET l_n = 0
   EXECUTE sel_inag_chk INTO l_n
   IF l_n = 0 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'ain-00119'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()

      RETURN FALSE
   END IF  
   RETURN TRUE  
END FUNCTION]]>
  </point>
