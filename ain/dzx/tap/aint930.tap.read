  <point name="function.aint930_inpl030_chk" order="7" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 數量檢查
# Memo...........:
# Usage..........: CALL aint930_inpl030_chk(p_qty)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By lixh
# Modify.........:
################################################################################
PRIVATE FUNCTION aint930_inpl030_chk(p_qty)
DEFINE p_qty        LIKE inpm_t.inpm050
DEFINE l_imaf071    LIKE imaf_t.imaf071
DEFINE l_imaf081    LIKE imaf_t.imaf081

   IF NOT cl_null(g_inpl2_d[l_ac].inpm001) THEN
      SELECT imaf071,imaf081 INTO l_imaf071,l_imaf081 FROM imaf_t
       WHERE imafent = g_enterprise
         AND imafsite = g_site
         AND imaf001 = g_inpl2_d[l_ac].inpm001
      IF l_imaf081 <> '2' THEN
         IF p_qty <> 0 AND p_qty <> 1 THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = 'ain-00506'
            LET g_errparam.extend = p_qty
            LET g_errparam.popup = TRUE
            CALL cl_err()         
            RETURN FALSE
         END IF
      END IF      
   END IF
   RETURN TRUE
END FUNCTION]]>
