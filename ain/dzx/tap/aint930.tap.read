  <point name="function.aint930_get_docno" order="1" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 獲取週期盤點單號
# Memo...........:
# Usage..........: CALL aint930_get_docno()
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By lixh
# Modify.........:
################################################################################
PRIVATE FUNCTION aint930_get_docno()
   IF NOT cl_null(g_inps_m.inps001) THEN
      SELECT inpj012 INTO g_inps_m.inpsdocno FROM inpj_t
       WHERE inpjent = g_enterprise
         AND inpjsite = g_site
         AND inpjdocno = g_inps_m.inps001
      CALL s_aooi200_get_slip_desc(g_inps_m.inpsdocno) RETURNING g_inps_m.inpsdocno_desc   
      DISPLAY BY NAME g_inps_m.inpsdocno,g_inps_m.inpsdocno_desc    
   ELSE
      LET g_inps_m.inpsdocno = ''
      LET g_inps_m.inpsdocno_desc = ''  
      DISPLAY BY NAME g_inps_m.inpsdocno,g_inps_m.inpsdocno_desc      
   END IF
END FUNCTION]]>
  </point>
