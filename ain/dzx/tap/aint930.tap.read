  <point name="function.aint930_inpl010_inpm_chk" order="10" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 庫存明細和批序號明細需要一致
# Memo...........:
# Usage..........: CALL aint930_inpl010_inpm_chk(p_type)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By lixh
# Modify.........:
################################################################################
PRIVATE FUNCTION aint930_inpl010_inpm_chk(p_type)
DEFINE  p_type     LIKE type_t.chr1
DEFINE  l_sum      LIKE inpl_t.inpl030

   IF p_type = '1' THEN   #初盤 
      SELECT SUM(inpm030) INTO l_sum FROM inpm_t
       WHERE inpment = g_enterprise
         AND inpmsite = g_site
         AND inpmdocno = g_inps_m.inpsdocno
         AND inpmseq = g_inpl_d[g_detail_idx].inplseq
         AND inpmseq1 <> g_inpl2_d[g_detail_idx2].inpmseq1
      IF l_sum + g_inpl2_d[g_detail_idx2].inpm030 > g_inpl_d[g_detail_idx].inpl030 THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = g_inpl_d[g_detail_idx].inpl030
         LET g_errparam.code   = "ain-00511" 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()         
         RETURN FALSE
      END IF      
   END IF   
   
   IF p_type = '2' THEN   #初盤 
      SELECT SUM(inpm050) INTO l_sum FROM inpm_t
       WHERE inpment = g_enterprise
         AND inpmsite = g_site
         AND inpmdocno = g_inps_m.inpsdocno
         AND inpmseq = g_inpl_d[g_detail_idx].inplseq
         AND inpmseq1 <> g_inpl2_d[g_detail_idx2].inpmseq1
      IF l_sum + g_inpl2_d[g_detail_idx2].inpm050 > g_inpl_d[g_detail_idx].inpl050 THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = g_inpl_d[g_detail_idx].inpl050
         LET g_errparam.code   = "ain-00512" 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()         
         RETURN FALSE
      END IF      
   END IF
   RETURN TRUE
END FUNCTION]]>
