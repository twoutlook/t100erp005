               #將遮罩欄位進行遮蔽
               CALL aint930_inps_t_mask_restore('restore_mask_n')
               
               #修改歷程記錄(單頭修改)
               LET g_log1 = util.JSON.stringify(g_inps_m_t)
               LET g_log2 = util.JSON.stringify(g_inps_m)
               IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                  CALL s_transaction_end('N','0')
