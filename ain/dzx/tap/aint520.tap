PRIVATE FUNCTION aint520_inde001_init()
   
   INITIALIZE g_inde_m.inde003,g_inde_m.inde004,g_inde_m.inde005 TO NULL
   IF NOT cl_null(g_inde_m.inde001) THEN
      SELECT indc005,indc006,indc007
        INTO g_inde_m.inde003,g_inde_m.inde004,g_inde_m.inde005
        FROM indc_t
       WHERE indcent = g_enterprise AND indcdocno = g_inde_m.inde001
   END IF
   DISPLAY BY NAME g_inde_m.inde003,g_inde_m.inde004,g_inde_m.inde005
   
   CALL aint520_inde003_ref()
   CALL aint520_inde004_ref()
   CALL aint520_inde005_ref()
END FUNCTION]]>
