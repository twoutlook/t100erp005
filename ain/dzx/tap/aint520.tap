   {<point name="input.define_customerization" edit="c"/>}
   #end add-point  
   
   #先做狀態判定
   IF p_cmd = 'r' THEN
      LET l_cmd_t = 'r'
      LET p_cmd   = 'a'
   ELSE
      LET l_cmd_t = p_cmd
   END IF   
   
   #將資料輸出到畫面上
   DISPLAY BY NAME g_inde_m.indesite,g_inde_m.indesite_desc,g_inde_m.indedocdt,g_inde_m.indedocno,g_inde_m.inde001, 
       g_inde_m.indeunit,g_inde_m.inde003,g_inde_m.inde003_desc,g_inde_m.inde004,g_inde_m.inde004_desc, 
       g_inde_m.inde005,g_inde_m.inde005_desc,g_inde_m.inde002,g_inde_m.inde002_desc,g_inde_m.inde101, 
       g_inde_m.inde101_desc,g_inde_m.inde006,g_inde_m.indestus,g_inde_m.indeownid,g_inde_m.indeownid_desc, 
       g_inde_m.indeowndp,g_inde_m.indeowndp_desc,g_inde_m.indecrtid,g_inde_m.indecrtid_desc,g_inde_m.indecrtdp, 
       g_inde_m.indecrtdp_desc,g_inde_m.indecrtdt,g_inde_m.indemodid,g_inde_m.indemodid_desc,g_inde_m.indemoddt, 
       g_inde_m.indecnfid,g_inde_m.indecnfid_desc,g_inde_m.indecnfdt
   
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF
 
   CALL cl_set_head_visible("","YES")  
 
   LET l_insert = FALSE
   LET g_action_choice = ""
 
   #add-point:input段define_sql
   {<point name="input.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT indfsite,indfunit,indfseq,indf001,indf009,indf003,indf002,indf004,indf005, 
       indf006,indf007,indf008,indf020,indf021,indf022,indf023,indf024,indf102,indf030,indf031,indf032, 
       indf033,indf034,indf040,indf041,indf042,indf043 FROM indf_t WHERE indfent=? AND indfdocno=? AND  
       indfseq=? FOR UPDATE"
   #add-point:input段define_sql
   {<point name="input.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE aint520_bcl CURSOR FROM g_forupd_sql
   
 
   
 
 
   #add-point:input段define_sql
   {<point name="input.other_sql"/>}
   #end add-point 
 
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   LET g_qryparam.state = 'i'
   
   #控制key欄位可否輸入
