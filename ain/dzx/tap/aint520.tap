  <section id="aint520.modify" ver="21" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aint520_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   DEFINE l_wc2_table1          STRING
 
 
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point    
   #add-point:modify段define(客製用)
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point    
   
   #保存單頭舊值
   LET g_inde_m_t.* = g_inde_m.*
   LET g_inde_m_o.* = g_inde_m.*
   
   IF g_inde_m.indedocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_indedocno_t = g_inde_m.indedocno
 
   CALL s_transaction_begin()
   
   OPEN aint520_cl USING g_enterprise,g_inde_m.indedocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aint520_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE aint520_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aint520_master_referesh USING g_inde_m.indedocno INTO g_inde_m.indesite,g_inde_m.indedocdt, 
       g_inde_m.indedocno,g_inde_m.inde001,g_inde_m.indeunit,g_inde_m.inde003,g_inde_m.inde004,g_inde_m.inde005, 
       g_inde_m.inde002,g_inde_m.inde101,g_inde_m.inde006,g_inde_m.indestus,g_inde_m.indeownid,g_inde_m.indeowndp, 
       g_inde_m.indecrtid,g_inde_m.indecrtdp,g_inde_m.indecrtdt,g_inde_m.indemodid,g_inde_m.indemoddt, 
       g_inde_m.indecnfid,g_inde_m.indecnfdt,g_inde_m.indesite_desc,g_inde_m.inde003_desc,g_inde_m.inde004_desc, 
       g_inde_m.inde005_desc,g_inde_m.inde002_desc,g_inde_m.inde101_desc,g_inde_m.indeownid_desc,g_inde_m.indeowndp_desc, 
       g_inde_m.indecrtid_desc,g_inde_m.indecrtdp_desc,g_inde_m.indemodid_desc,g_inde_m.indecnfid_desc 

   
   #檢查是否允許此動作
   IF NOT aint520_action_chk() THEN
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_inde_m_mask_o.* =  g_inde_m.*
   CALL aint520_inde_t_mask()
   LET g_inde_m_mask_n.* =  g_inde_m.*
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
 
   
   CALL aint520_show()
   #add-point:modify段show之後
   {<point name="modify.after_show"/>}
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
 
 
    
   WHILE TRUE
      LET g_indedocno_t = g_inde_m.indedocno
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_inde_m.indemodid = g_user 
LET g_inde_m.indemoddt = cl_get_current()
LET g_inde_m.indemodid_desc = cl_get_username(g_inde_m.indemodid)
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      CALL aint520_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE inde_t SET (indemodid,indemoddt) = (g_inde_m.indemodid,g_inde_m.indemoddt)
          WHERE indeent = g_enterprise AND indedocno = g_indedocno_t
 
      END IF
    
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_inde_m.* = g_inde_m_t.*
         CALL aint520_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_inde_m.indedocno != g_indedocno_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE indf_t SET indfdocno = g_inde_m.indedocno
 
          WHERE indfent = g_enterprise AND indfdocno = g_indedocno_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "indf_t" 
               LET g_errparam.code   = "std-00009" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "indf_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL aint520_set_act_visible()   
   CALL aint520_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " indeent = '" ||g_enterprise|| "' AND",
                      " indedocno = '", g_inde_m.indedocno, "' "
 
   #填到對應位置
   CALL aint520_browser_fill("")
 
   CLOSE aint520_cl
   
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aint520_msgcentre_notify('modify')
 
END FUNCTION   
]]>
