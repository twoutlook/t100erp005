  <section id="aint520.insert" ver="21" status="" src="s" readonly="">
    <![CDATA[#+ 資料新增
PRIVATE FUNCTION aint520_insert()
   #add-point:insert段define
   {<point name="insert.define" edit="s"/>}
   #end add-point    
   #add-point:insert段define(客製用)
   {<point name="insert.define_customerization" edit="c"/>}
   #end add-point    
   
   #清畫面欄位內容
   CLEAR FORM                    
   CALL g_indf_d.clear()   
 
 
   INITIALIZE g_inde_m.* LIKE inde_t.*             #DEFAULT 設定
   
   LET g_indedocno_t = NULL
 
   
   LET g_master_insert = FALSE
   
   #add-point:insert段before
   {<point name="insert.before"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   WHILE TRUE
      #公用欄位給值(單頭)
      #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_inde_m.indeownid = g_user
      LET g_inde_m.indeowndp = g_dept
      LET g_inde_m.indecrtid = g_user
      LET g_inde_m.indecrtdp = g_dept 
      LET g_inde_m.indecrtdt = cl_get_current()
      LET g_inde_m.indemodid = g_user
      LET g_inde_m.indemoddt = cl_get_current()
      LET g_inde_m.indestus = 'N'
 
 
 
      #append欄位給值
      
     
      #一般欄位給值
            LET g_inde_m.indestus = "N"
 
  
      #add-point:單頭預設值
      {<point name="insert.default"/>}
      #end add-point 
      
      #保存單頭舊值(用於資料輸入錯誤還原預設值時使用)
      LET g_inde_m_t.* = g_inde_m.*
      LET g_inde_m_o.* = g_inde_m.*
      
      #顯示狀態(stus)圖片
            #應用 a21 樣板自動產生(Version:2)
	  #根據當下狀態碼顯示圖片
      CASE g_inde_m.indestus 
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
 
 
    
      CALL aint520_input("a")
      
      #add-point:單頭輸入後
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
      END IF
      
      IF NOT g_master_insert THEN
         DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
         DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
         INITIALIZE g_inde_m.* TO NULL
         INITIALIZE g_indf_d TO NULL
 
         #add-point:取消新增後
         {<point name="insert.cancel"/>}
         #end add-point 
         CALL aint520_show()
         RETURN
      END IF
      
      LET INT_FLAG = 0
      #CALL g_indf_d.clear()
 
 
      LET g_rec_b = 0
      CALL s_transaction_end('Y','0')
      EXIT WHILE
        
   END WHILE
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL aint520_set_act_visible()   
   CALL aint520_set_act_no_visible()
   
   #將新增的資料併入搜尋條件中
   LET g_indedocno_t = g_inde_m.indedocno
 
   
   #組合新增資料的條件
   LET g_add_browse = " indeent = '" ||g_enterprise|| "' AND",
                      " indedocno = '", g_inde_m.indedocno, "' "
 
                      
   #add-point:組合新增資料的條件後
   {<point name="insert.after.add_browse"/>}
   #end add-point
      
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL aint520_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   CLOSE aint520_cl
   
   CALL aint520_idx_chk()
   
   #撈取異動後的資料(主要是帶出reference)
   EXECUTE aint520_master_referesh USING g_inde_m.indedocno INTO g_inde_m.indesite,g_inde_m.indedocdt, 
       g_inde_m.indedocno,g_inde_m.inde001,g_inde_m.indeunit,g_inde_m.inde003,g_inde_m.inde004,g_inde_m.inde005, 
       g_inde_m.inde002,g_inde_m.inde101,g_inde_m.inde006,g_inde_m.indestus,g_inde_m.indeownid,g_inde_m.indeowndp, 
       g_inde_m.indecrtid,g_inde_m.indecrtdp,g_inde_m.indecrtdt,g_inde_m.indemodid,g_inde_m.indemoddt, 
       g_inde_m.indecnfid,g_inde_m.indecnfdt,g_inde_m.indesite_desc,g_inde_m.inde003_desc,g_inde_m.inde004_desc, 
       g_inde_m.inde005_desc,g_inde_m.inde002_desc,g_inde_m.inde101_desc,g_inde_m.indeownid_desc,g_inde_m.indeowndp_desc, 
       g_inde_m.indecrtid_desc,g_inde_m.indecrtdp_desc,g_inde_m.indemodid_desc,g_inde_m.indecnfid_desc 

   
   #遮罩相關處理
   LET g_inde_m_mask_o.* =  g_inde_m.*
   CALL aint520_inde_t_mask()
   LET g_inde_m_mask_n.* =  g_inde_m.*
   
   #將資料顯示到畫面上
   DISPLAY BY NAME g_inde_m.indesite,g_inde_m.indesite_desc,g_inde_m.indedocdt,g_inde_m.indedocno,g_inde_m.inde001, 
       g_inde_m.indeunit,g_inde_m.inde003,g_inde_m.inde003_desc,g_inde_m.inde004,g_inde_m.inde004_desc, 
       g_inde_m.inde005,g_inde_m.inde005_desc,g_inde_m.inde002,g_inde_m.inde002_desc,g_inde_m.inde101, 
       g_inde_m.inde101_desc,g_inde_m.inde006,g_inde_m.indestus,g_inde_m.indeownid,g_inde_m.indeownid_desc, 
       g_inde_m.indeowndp,g_inde_m.indeowndp_desc,g_inde_m.indecrtid,g_inde_m.indecrtid_desc,g_inde_m.indecrtdp, 
       g_inde_m.indecrtdp_desc,g_inde_m.indecrtdt,g_inde_m.indemodid,g_inde_m.indemodid_desc,g_inde_m.indemoddt, 
       g_inde_m.indecnfid,g_inde_m.indecnfid_desc,g_inde_m.indecnfdt
   
   #add-point:新增結束後
   {<point name="insert.after"/>}
   #end add-point 
   
   #功能已完成,通報訊息中心
   CALL aint520_msgcentre_notify('insert')
   
END FUNCTION
]]>
