  <point name="function.aint520_inde001_chk" order="1" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#+
PRIVATE FUNCTION aint520_inde001_chk()
DEFINE l_cnt        LIKE type_t.num5
DEFINE l_indcstus   LIKE indc_t.indcstus
DEFINE l_indc005    LIKE indc_t.indc005
DEFINE l_indc000    LIKE indc_t.indc000
DEFINE l_sql        STRING
DEFINE l_indcsite   LIKE indc_t.indcsite
DEFINE l_ooed005    LIKE ooed_t.ooed005

   INITIALIZE g_errno TO NULL
   
   #检查录入的调拨单号是否存在于未审核的差异调整单中
   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt
     FROM inde_t
    WHERE indeent = g_enterprise AND inde001 = g_inde_m.inde001 AND indestus = 'N'
   IF l_cnt > 0 THEN
      LET g_errno = 'ain-00074' #该调拨单号存在于未审核的差异调整单中 -- 请至调拨差异调整单维护作业[aint520]查询后重新录入
      RETURN
   END IF
   
   #检查录入的调拨单号的状态是否符合要求
   SELECT indcstus,indcsite,indc005,indc000 INTO l_indcstus,l_indcsite,l_indc005,l_indc000
     FROM indc_t
    WHERE indcent = g_enterprise AND indcdocno = g_inde_m.inde001
   CASE
      WHEN SQLCA.sqlcode = 100 LET g_errno = 'ain-00075' #该调拨单不存在 -- 请至调拨单维护作业[aint510]查询后重新录入
      WHEN l_indcstus = 'N'    LET g_errno = 'ain-00076' #该调拨单未审核,不可做差异调整 -- 请至调拨单维护作业[aint510]查询后重新录入
      WHEN l_indcstus = 'O'    LET g_errno = 'ain-00077' #该调拨单为拨出审核,不可做差异调整 -- 请至调拨单维护作业[aint510]查询后重新录入
      WHEN l_indcstus = 'C'    LET g_errno = 'ain-00078' #该调拨单已结案,不可做差异调整 -- 请至调拨单维护作业[aint510]查询后重新录入
      WHEN l_indcstus = 'X'    LET g_errno = 'ain-00079' #该调拨单已作废,不可做差异调整 -- 请至调拨单维护作业[aint510]查询后重新录入
      WHEN l_indcsite <> l_indc005 OR l_indc000 <> '2' LET g_errno = 'ain-00167' #該調撥單不是下級需調整的調撥單
   END CASE
   
   #检查上下级关系
   IF cl_null(g_errno) THEN
      SELECT ooed005 INTO l_ooed005
        FROM ooed_t
       WHERE ooedent = g_enterprise AND ooed001 = '2' AND ooed006 <= g_today AND (ooed007 >= g_today OR ooed007 IS NULL)
         AND ooed004 = l_indc005
      IF l_ooed005 <> g_site THEN
         LET g_errno = 'ain-00162' #来源调拨单的拨出组织必须为当前组织或下级
         RETURN
      END IF
   END IF
 
END FUNCTION]]>
  </point>
