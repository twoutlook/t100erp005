          g_indf_d[l_ac].indf008,g_indf_d[l_ac].indf020,g_indf_d[l_ac].indf021,g_indf_d[l_ac].indf022, 
          g_indf_d[l_ac].indf023,g_indf_d[l_ac].indf024,g_indf_d[l_ac].indf102,g_indf_d[l_ac].indf030, 
          g_indf_d[l_ac].indf031,g_indf_d[l_ac].indf032,g_indf_d[l_ac].indf033,g_indf_d[l_ac].indf034, 
