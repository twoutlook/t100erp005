      LET g_inde_m.indemodid = g_user
      LET g_inde_m.indemoddt = cl_get_current()
      LET g_inde_m.indestus = 'N'
 
 
   
   CALL s_transaction_begin()
   
   #add-point:複製輸入前
   {<point name="reproduce.head.b_input"/>}
   #end add-point
   
   #顯示狀態(stus)圖片
