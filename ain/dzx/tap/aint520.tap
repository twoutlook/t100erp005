   DISPLAY BY NAME g_inde_m.indesite,g_inde_m.indedocdt,g_inde_m.indedocno,g_inde_m.inde001,g_inde_m.indeunit, 
       g_inde_m.inde003,g_inde_m.inde004,g_inde_m.inde005,g_inde_m.inde002,g_inde_m.inde101,g_inde_m.inde006, 
       g_inde_m.indestus
   
   LET lb_reproduce = FALSE
   
   #關閉被遮罩相關欄位輸入, 無法確定USER是否會需要輸入此欄位
   #因此先行關閉, 若有需要可於下方add-point中自行開啟
   CALL cl_mask_set_no_entry()
   
   #add-point:資料輸入前
   {<point name="input.before_input"/>}
   #end add-point
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
