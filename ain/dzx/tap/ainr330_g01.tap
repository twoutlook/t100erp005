  <point name="ins_data.foreach" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[       #調撥類型
       SELECT gzcbl004 INTO sr_s.l_indc000_desc
         FROM gzcbl_t 
        WHERE gzcbl001 = '2082'
          AND gzcbl002 = sr_s.indc000
          AND gzcbl003 = g_dlang  
        
       #來源類型  
       SELECT gzcbl004 INTO sr_s.l_indc002_desc
         FROM gzcbl_t
        WHERE gzcbl001 = '6016'
          AND gzcbl002 = sr_s.indc002
          AND gzcbl003 = g_dlang  
       
       #檢驗方式
       SELECT gzcbl004 INTO sr_s.l_indc102_desc
         FROM gzcbl_t
        WHERE gzcbl001 = '6016'
          AND gzcbl002 = sr_s.indc102
          AND gzcbl003 = g_dlang 
          
       #撥入據點 
       INITIALIZE sr_s.l_indc06_ooefl003  TO NULL
       SELECT ooefl003 INTO sr_s.l_indc06_ooefl003
         FROM ooefl_t
        WHERE ooeflent = sr_s.indcent
          AND ooefl001 = sr_s.indc006
          AND ooefl002 = g_dlang
       LET sr_s.l_indc06_ooefl003 = sr_s.indc006,".",sr_s.l_indc06_ooefl003
       
       #調撥部門 
       INITIALIZE sr_s.l_indc101_ooefl003  TO NULL
       SELECT ooefl003 INTO sr_s.l_indc101_ooefl003
         FROM ooefl_t
        WHERE ooeflent = sr_s.indcent
          AND ooefl001 = sr_s.indc101
          AND ooefl002 = g_dlang
       LET sr_s.l_indc101_ooefl003 = sr_s.indc101,".",sr_s.l_indc101_ooefl003
       
       #二階段調撥的撥入倉儲批
       IF sr_s.indc000 = '2' THEN
          SELECT indd032,indd033,indd034 
            INTO sr_s.indd032,sr_s.indd033,sr_s.indd034
            FROM indd_t
           WHERE indddocno = sr_s.indc001
             AND inddseq = sr_s.inddseq
       
          SELECT inaa002 INTO sr_s.l_indd032_inaa002 
            FROM inaa_t
           WHERE inaaent = sr_s.indcent        
             AND inaasite =sr_s.indcsite
             AND inaa001 = sr_s.indd032
             
         SELECT inab003 INTO sr_s.l_indd033_inab003 
            FROM inab_t
           WHERE inabent = sr_s.indcent        
             AND inabsite =sr_s.indcsite
             AND inab002 = sr_s.indd032    
                      
          LET sr_s.l_indd032_inaa002 = sr_s.indd032,'.',sr_s.l_indd032_inaa002
          LET sr_s.l_indd033_inab003 = sr_s.indd033,'.',sr_s.l_indd033_inab003
       
       END IF
       
          
       LET sr_s.l_in  = sr_s.l_indd022_inaa002,'/',sr_s.l_indd023_inab003,'/',sr_s.indd024,'/',sr_s.indd102   
       LET sr_s.l_out = sr_s.l_indd032_inaa002,'/',sr_s.l_indd033_inab003,'/',sr_s.indd034,'/',sr_s.indd102 
       
      ]]>
  </point>
