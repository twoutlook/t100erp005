            #add-point:BEFORE FIELD ined001
            {<point name="construct.b.ined001" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD ined001
            
            #add-point:AFTER FIELD ined001
            {<point name="construct.a.ined001" />}
            #END add-point
            
 
         #Ctrlp:construct.c.inedunit
         ON ACTION controlp INFIELD inedunit
            #add-point:ON ACTION controlp INFIELD inedunit
            {<point name="construct.c.inedunit" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD inedunit
            #add-point:BEFORE FIELD inedunit
            {<point name="construct.b.inedunit" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD inedunit
            
            #add-point:AFTER FIELD inedunit
            {<point name="construct.a.inedunit" />}
            #END add-point
            
 
