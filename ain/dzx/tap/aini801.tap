  <section id="aini801.detail_reproduce" ver="5" status="" src="s">
    <![CDATA[#+ 單身自動複製
PRIVATE FUNCTION aini801_detail_reproduce()
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE ined_t.*
 
 
   #add-point:delete段define
   {<point name="detail_reproduce.define"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE aini801_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE aini801_detail AS ",
                "SELECT * FROM ined_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO aini801_detail SELECT * FROM ined_t 
                                         WHERE inedent = g_enterprise AND inedsite = g_inedsite_t
                                         AND ined001 = g_ined001_t
 
   
   #將key修正為調整後   
   UPDATE aini801_detail 
      #更新key欄位
      SET inedsite = g_ined_m.inedsite
          , ined001 = g_ined_m.ined001
 
      #更新共用欄位
      #此段落由子樣板a13產生
       , inedownid = g_user
       , inedowndp = g_dept
       , inedcrtid = g_user
       , inedcrtdp = g_dept 
       , inedcrtdt = ld_date
       , inedmodid = "" 
       , inedmoddt = "" 
 
 
                                       
  
   #將資料塞回原table   
   INSERT INTO ined_t SELECT * FROM aini801_detail
   
   IF SQLCA.sqlcode THEN
      CALL cl_err("reproduce",SQLCA.sqlcode,1)
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE aini801_detail
   
   #add-point:單身複製後1
   {<point name="detail_reproduce.body.table1.a_insert"/>}
   #end add-point
 
 
   
 
   
   #多語言複製段落
   
   
   CALL s_transaction_end('Y','0')
   
   #已新增完, 調整資料內容(修改時使用)
   LET g_inedsite_t = g_ined_m.inedsite
   LET g_ined001_t = g_ined_m.ined001
 
   
   DROP TABLE aini801_detail
   
END FUNCTION
