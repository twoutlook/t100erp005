  <section id="aini801.modify" ver="5" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aini801_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_ined_m.inedsite IS NULL
   OR g_ined_m.ined001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE aini801_master_referesh USING g_ined_m.inedsite,g_ined_m.ined001 INTO g_ined_m.ined001,g_ined_m.inedunit, 
       g_ined_m.ined002,g_ined_m.inedsite,g_ined_m.ined001_desc,g_ined_m.inedunit_desc,g_ined_m.inedsite_desc 

 
   ERROR ""
  
   LET g_inedsite_t = g_ined_m.inedsite
   LET g_ined001_t = g_ined_m.ined001
 
   CALL s_transaction_begin()
   
   OPEN aini801_cl USING g_enterprise,g_ined_m.inedsite
                                                       ,g_ined_m.ined001
 
   IF STATUS THEN
      CALL cl_err("OPEN aini801_cl:", STATUS, 1)
      CLOSE aini801_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aini801_cl INTO g_ined_m.ined001,g_ined_m.ined001_desc,g_ined_m.inedunit,g_ined_m.inedunit_desc, 
       g_ined_m.ined002,g_ined_m.inedsite,g_ined_m.inedsite_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_ined_m.inedsite,SQLCA.sqlcode,0)
      CLOSE aini801_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL aini801_show()
   WHILE TRUE
      LET g_inedsite_t = g_ined_m.inedsite
      LET g_ined001_t = g_ined_m.ined001
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL aini801_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_ined_m.* = g_ined_m_t.*
         CALL aini801_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_ined_m.inedsite != g_inedsite_t 
      OR g_ined_m.ined001 != g_ined001_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE ined_t SET inedsite = g_ined_m.inedsite
                                       , ined001 = g_ined_m.ined001
 
          WHERE inedent = g_enterprise AND inedsite = g_inedsite_t
            AND ined001 = g_ined001_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("ined_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("ined_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
