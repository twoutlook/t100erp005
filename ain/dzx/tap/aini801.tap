  <section id="aini801.delete" ver="7" status="" src="s">
    <![CDATA[#+ 資料刪除
PRIVATE FUNCTION aini801_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_ined_m.inedsite IS NULL
   OR g_ined_m.ined001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE aini801_master_referesh USING g_ined_m.inedsite,g_ined_m.ined001 INTO g_ined_m.ined001,g_ined_m.inedunit, 
       g_ined_m.ined002,g_ined_m.inedsite,g_ined_m.ined001_desc,g_ined_m.inedunit_desc,g_ined_m.inedsite_desc 

   
   CALL aini801_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN aini801_cl USING g_enterprise,g_ined_m.inedsite
                                                       ,g_ined_m.ined001
 
   IF STATUS THEN
      CALL cl_err("OPEN aini801_cl:", STATUS, 1)
      CLOSE aini801_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aini801_cl INTO g_ined_m.ined001,g_ined_m.ined001_desc,g_ined_m.inedunit,g_ined_m.inedunit_desc, 
       g_ined_m.ined002,g_ined_m.inedsite,g_ined_m.inedsite_desc
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_ined_m.inedsite,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL aini801_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM ined_t WHERE inedent = g_enterprise AND inedsite = g_ined_m.inedsite
                                                               AND ined001 = g_ined_m.ined001
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("ined_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_ined_d.clear() 
      CALL g_ined2_d.clear()       
 
     
      CALL aini801_ui_browser_refresh()  
      CALL aini801_ui_headershow()  
      CALL aini801_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL aini801_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL aini801_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE aini801_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_ined_m.inedsite,'D')
    
END FUNCTION
