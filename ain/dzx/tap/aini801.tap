  <section id="aini801.b_fill" ver="12" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aini801_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_ined_d.clear()
   CALL g_ined2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE ined004,ined003,ined005,ined006,ined007,ined008,inedownid,inedowndp,inedcrtid, 
       inedcrtdp,inedcrtdt,inedmodid,inedmoddt,t1.oofa011 ,t2.oofa011 ,t3.ooefl003 ,t4.oofa011 ,t5.ooefl003 , 
       t6.oofa011 FROM ined_t",   
               "",
               
                              " LEFT JOIN oofa_t t1 ON t1.oofaent='"||g_enterprise||"' AND t1.oofa002='2' AND t1.oofa003=ined006  ",
               " LEFT JOIN oofa_t t2 ON t2.oofaent='"||g_enterprise||"' AND t2.oofa002='2' AND t2.oofa003=inedownid  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent='"||g_enterprise||"' AND t3.ooefl001=inedowndp AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t4 ON t4.oofaent='"||g_enterprise||"' AND t4.oofa002='2' AND t4.oofa003=inedcrtid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=inedcrtdp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t6 ON t6.oofaent='"||g_enterprise||"' AND t6.oofa002='2' AND t6.oofa003=inedmodid  ",
 
               " WHERE inedent= ? AND inedsite=? AND ined001=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("ined_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF aini801_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY ined_t.ined003"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE aini801_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR aini801_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_ined_m.inedsite,g_ined_m.ined001
                                               
      FOREACH b_fill_cs INTO g_ined_d[l_ac].ined004,g_ined_d[l_ac].ined003,g_ined_d[l_ac].ined005,g_ined_d[l_ac].ined006, 
          g_ined_d[l_ac].ined007,g_ined_d[l_ac].ined008,g_ined2_d[l_ac].inedownid,g_ined2_d[l_ac].inedowndp, 
          g_ined2_d[l_ac].inedcrtid,g_ined2_d[l_ac].inedcrtdp,g_ined2_d[l_ac].inedcrtdt,g_ined2_d[l_ac].inedmodid, 
          g_ined2_d[l_ac].inedmoddt,g_ined_d[l_ac].ined006_desc,g_ined2_d[l_ac].inedownid_desc,g_ined2_d[l_ac].inedowndp_desc, 
          g_ined2_d[l_ac].inedcrtid_desc,g_ined2_d[l_ac].inedcrtdp_desc,g_ined2_d[l_ac].inedmodid_desc 

                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_ined2_d[l_ac].inedownid_desc = cl_get_username(g_ined2_d[l_ac].inedownid)
      #LET g_ined2_d[l_ac].inedowndp_desc = cl_get_deptname(g_ined2_d[l_ac].inedowndp)
      #LET g_ined2_d[l_ac].inedcrtid_desc = cl_get_username(g_ined2_d[l_ac].inedcrtid)
      #LET g_ined2_d[l_ac].inedcrtdp_desc = cl_get_deptname(g_ined2_d[l_ac].inedcrtdp)
      #LET g_ined2_d[l_ac].inedmodid_desc = cl_get_username(g_ined2_d[l_ac].inedmodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_ined_d.deleteElement(g_ined_d.getLength())
      CALL g_ined2_d.deleteElement(g_ined2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE aini801_pb   
   
END FUNCTION
]]>
  </section>
