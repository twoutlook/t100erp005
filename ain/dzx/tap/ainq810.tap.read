   LET g_ineg_m.inegdocno = ""
 
    
   CALL ainq810_set_entry("a")
   CALL ainq810_set_no_entry("a")
   
   #公用欄位給予預設值
   
   
   #add-point:複製輸入前
   {<point name="reproduce.head.b_input"/>}
