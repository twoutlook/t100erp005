            #add-point:AFTER FIELD indesite
            {<point name="construct.a.page1.indesite" />}
            #END add-point
            
 
         #Ctrlp:construct.c.page1.indesite
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD indesite
            #add-point:ON ACTION controlp INFIELD indesite
            {<point name="construct.c.page1.indesite" />}
            #END add-point
 
         #應用 a01 樣板自動產生(Version:1)
         BEFORE FIELD indedocdt
            #add-point:BEFORE FIELD indedocdt
            {<point name="construct.b.page1.indedocdt" />}
            #END add-point
 
         #應用 a02 樣板自動產生(Version:1)
         AFTER FIELD indedocdt
            
            #add-point:AFTER FIELD indedocdt
            {<point name="construct.a.page1.indedocdt" />}
            #END add-point
            
 
