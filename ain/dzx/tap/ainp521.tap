            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_inde3_d.getLength()+1
 
      END INPUT
 
      
      DISPLAY ARRAY g_inde2_d TO s_detail2.*
         ATTRIBUTES(COUNT=g_detail_cnt)  
      
