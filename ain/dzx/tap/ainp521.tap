   LET g_qryparam.state = "c"
   
   #wc備份
   LET ls_wc = g_wc
   LET g_detail_idx = l_ac
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單身根據table分拆construct
      CONSTRUCT g_wc_table ON indestus,indesite,indedocdt,indedocno,inde001,inde002,inde003,inde004, 
          inde005,inde006,indeownid,indeowndp,indecrtid,indecrtdp,indecrtdt,indemodid,indemoddt,indecnfid, 
          indecnfdt
           FROM s_detail1[1].indestus,s_detail1[1].indesite,s_detail1[1].indedocdt,s_detail1[1].indedocno, 
               s_detail1[1].inde001,s_detail1[1].inde002,s_detail1[1].inde003,s_detail1[1].inde004,s_detail1[1].inde005, 
               s_detail1[1].inde006,s_detail2[1].indeownid,s_detail2[1].indeowndp,s_detail2[1].indecrtid, 
