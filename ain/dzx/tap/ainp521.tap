               END IF
               CLOSE ainp521_bcl2
               CALL s_transaction_end('N','0')
               CANCEL DIALOG 
            END IF
            
            #其他table進行unlock
            
 
            CALL ainp521_unlock_b("indf_t")
            CALL s_transaction_end('Y','0')
            LET l_cmd = ''
 
         AFTER INPUT
            #add-point:input段after input 
            {<point name="input.body3.after_input"/>}
            #end add-point   
 
         ON ACTION controlo   
