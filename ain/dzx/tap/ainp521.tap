  <section id="ainp521.update_b" ver="5" status="" src="s" readonly="">
    <![CDATA[#+ 修改單身後其他table連動
PRIVATE FUNCTION ainp521_update_b(ps_table,ps_keys,ps_keys_bak,ps_page)
   DEFINE ps_table         STRING
   DEFINE ps_page          STRING
   DEFINE ps_keys          DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ps_keys_bak      DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ls_group         STRING
   DEFINE li_idx           LIKE type_t.num10 
   DEFINE lb_chk           BOOLEAN
   DEFINE l_new_key        DYNAMIC ARRAY OF STRING
   DEFINE l_old_key        DYNAMIC ARRAY OF STRING
   DEFINE l_field_key      DYNAMIC ARRAY OF STRING
   #add-point:update_b段define
   {<point name="update_b.define" edit="s"/>}
   #end add-point     
   #add-point:update_b段define(客製用)
   {<point name="update_b.define_customerization" edit="c"/>}
   #end add-point     
   
   #判斷key是否有改變
   LET lb_chk = TRUE
   FOR li_idx = 1 TO ps_keys.getLength()
      IF ps_keys[li_idx] <> ps_keys_bak[li_idx] THEN
         LET lb_chk = FALSE
         EXIT FOR
      END IF
   END FOR
   
   #不需要做處理
   IF lb_chk THEN
      RETURN
   END IF
   
   #判斷是否是同一群組的table
   LET ls_group = "inde_t,"
   IF ls_group.getIndexOf(ps_table,1) > 0 AND ps_table <> "inde_t" THEN
   
      #add-point:update_b段修改前
      {<point name="update_b.before_update" mark="Y"/>}
      #end add-point     
   
      #將遮罩欄位還原
      CALL ainp521_inde_t_mask_restore('restore_mask_o')
               
      UPDATE inde_t 
         SET (indedocno
              ,indestus,indesite,indedocdt,inde001,inde002,inde003,inde004,inde005,inde006,indeownid,indeowndp,indecrtid,indecrtdp,indecrtdt,indemodid,indemoddt,indecnfid,indecnfdt) 
              = 
             (ps_keys[1]
              ,g_inde_d[g_detail_idx].indestus,g_inde_d[g_detail_idx].indesite,g_inde_d[g_detail_idx].indedocdt, 
                  g_inde_d[g_detail_idx].inde001,g_inde_d[g_detail_idx].inde002,g_inde_d[g_detail_idx].inde003, 
                  g_inde_d[g_detail_idx].inde004,g_inde_d[g_detail_idx].inde005,g_inde_d[g_detail_idx].inde006, 
                  g_inde2_d[g_detail_idx].indeownid,g_inde2_d[g_detail_idx].indeowndp,g_inde2_d[g_detail_idx].indecrtid, 
                  g_inde2_d[g_detail_idx].indecrtdp,g_inde2_d[g_detail_idx].indecrtdt,g_inde2_d[g_detail_idx].indemodid, 
                  g_inde2_d[g_detail_idx].indemoddt,g_inde2_d[g_detail_idx].indecnfid,g_inde2_d[g_detail_idx].indecnfdt)  

         WHERE indedocno = ps_keys_bak[1]
 
      #add-point:update_b段修改中
      {<point name="update_b.m_update"/>}
      #end add-point 
         
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "inde_t" 
            LET g_errparam.code   = "std-00009" 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            CALL s_transaction_end('N','0')
         WHEN SQLCA.sqlcode #其他錯誤
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "inde_t" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            CALL s_transaction_end('N','0')
         OTHERWISE
            
      END CASE
 
      #將遮罩欄位進行遮蔽
      CALL ainp521_inde_t_mask_restore('restore_mask_n')
               
      #add-point:update_b段修改後
      {<point name="update_b.after_update"/>}
      #end add-point
      
      RETURN
   END IF
   
 
   
   LET ls_group = "indf_t,"
   #判斷是否是同一群組的table
   IF ls_group.getIndexOf(ps_table,1) > 0 AND ps_table <> "indf_t" THEN
   
      #add-point:update_b段修改前
      {<point name="update_b.before_update2" mark="Y"/>}
      #end add-point    
      
      #將遮罩欄位還原
      CALL ainp521_indf_t_mask_restore('restore_mask_o')
      
      UPDATE indf_t 
         SET (indfdocno,indfseq
              ,indf001,indf003,indf002,indf006,indf040,indf041,indf042,indf043) 
              = 
             (ps_keys[1],ps_keys[2]
              ,g_inde3_d[g_detail_idx2].indf001,g_inde3_d[g_detail_idx2].indf003,g_inde3_d[g_detail_idx2].indf002, 
                  g_inde3_d[g_detail_idx2].indf006,g_inde3_d[g_detail_idx2].indf040,g_inde3_d[g_detail_idx2].indf041, 
                  g_inde3_d[g_detail_idx2].indf042,g_inde3_d[g_detail_idx2].indf043) 
         WHERE indfdocno = ps_keys_bak[1] AND indfseq = ps_keys_bak[2]
 
      #add-point:update_b段修改中
      {<point name="update_b.m_update2"/>}
      #end add-point 
         
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "indf_t" 
            LET g_errparam.code   = "std-00009" 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            CALL s_transaction_end('N','0')
         WHEN SQLCA.sqlcode #其他錯誤
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "indf_t" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            CALL s_transaction_end('N','0')
         OTHERWISE
            
      END CASE
      
      #將遮罩欄位進行遮蔽
      CALL ainp521_indf_t_mask_restore('restore_mask_n')
               
      #add-point:update_b段修改後
      {<point name="update_b.after_update2"/>}
      #end add-point 
      
      RETURN
   END IF
 
 
   
END FUNCTION
