  <section id="ainp521.b_fill" ver="15" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION ainp521_b_fill(p_wc2)
   DEFINE p_wc2           STRING
   DEFINE l_ac_t          LIKE type_t.num10
   #add-point:b_fill段define
   {<point name="b_fill.define" edit="s"/>}
   #end add-point
   #add-point:b_fill段define(客製用)
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE t0.indestus,t0.indesite,t0.indedocdt,t0.indedocno,t0.inde001,t0.inde002, 
       t0.inde003,t0.inde004,t0.inde005,t0.inde006,t0.indedocno,t0.indeownid,t0.indeowndp,t0.indecrtid, 
       t0.indecrtdp,t0.indecrtdt,t0.indemodid,t0.indemoddt,t0.indecnfid,t0.indecnfdt ,t1.ooefl003 ,t2.ooag011 , 
       t3.ooefl003 ,t4.ooefl003 ,t5.inayl003 ,t6.ooag011 ,t7.ooefl003 ,t8.ooag011 ,t9.ooefl003 ,t10.ooag011 , 
       t11.ooag011 FROM inde_t t0",
 
               " LEFT JOIN indf_t ON indfent = indeent AND indedocno = indfdocno",
 
 
               "",
                              " LEFT JOIN ooefl_t t1 ON t1.ooeflent='"||g_enterprise||"' AND t1.ooefl001=t0.indesite AND t1.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t2 ON t2.ooagent='"||g_enterprise||"' AND t2.ooag001=t0.inde002  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent='"||g_enterprise||"' AND t3.ooefl001=t0.inde003 AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=t0.inde004 AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN inayl_t t5 ON t5.inaylent='"||g_enterprise||"' AND t5.inayl001=t0.inde005 AND t5.inayl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent='"||g_enterprise||"' AND t6.ooag001=t0.indeownid  ",
               " LEFT JOIN ooefl_t t7 ON t7.ooeflent='"||g_enterprise||"' AND t7.ooefl001=t0.indeowndp AND t7.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t8 ON t8.ooagent='"||g_enterprise||"' AND t8.ooag001=t0.indecrtid  ",
               " LEFT JOIN ooefl_t t9 ON t9.ooeflent='"||g_enterprise||"' AND t9.ooefl001=t0.indecrtdp AND t9.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t10 ON t10.ooagent='"||g_enterprise||"' AND t10.ooag001=t0.indemodid  ",
               " LEFT JOIN ooag_t t11 ON t11.ooagent='"||g_enterprise||"' AND t11.ooag001=t0.indecnfid  ",
 
               " WHERE t0.indeent= ?  AND  1=1 AND (", p_wc2, ") "
   #add-point:b_fill段sql_wc
   {<point name="b_fill.sql_wc"/>}
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("inde_t"),
                      " ORDER BY t0.indedocno"
  
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
  
   #LET g_sql = cl_sql_add_tabid(g_sql,"inde_t")             #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料  
   PREPARE ainp521_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR ainp521_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_inde_d.clear()
   CALL g_inde2_d.clear()   
   CALL g_inde3_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_inde_d[l_ac].indestus,g_inde_d[l_ac].indesite,g_inde_d[l_ac].indedocdt, 
       g_inde_d[l_ac].indedocno,g_inde_d[l_ac].inde001,g_inde_d[l_ac].inde002,g_inde_d[l_ac].inde003, 
       g_inde_d[l_ac].inde004,g_inde_d[l_ac].inde005,g_inde_d[l_ac].inde006,g_inde2_d[l_ac].indedocno, 
       g_inde2_d[l_ac].indeownid,g_inde2_d[l_ac].indeowndp,g_inde2_d[l_ac].indecrtid,g_inde2_d[l_ac].indecrtdp, 
       g_inde2_d[l_ac].indecrtdt,g_inde2_d[l_ac].indemodid,g_inde2_d[l_ac].indemoddt,g_inde2_d[l_ac].indecnfid, 
       g_inde2_d[l_ac].indecnfdt,g_inde_d[l_ac].indesite_desc,g_inde_d[l_ac].inde002_desc,g_inde_d[l_ac].inde003_desc, 
       g_inde_d[l_ac].inde004_desc,g_inde_d[l_ac].inde005_desc,g_inde2_d[l_ac].indeownid_desc,g_inde2_d[l_ac].indeowndp_desc, 
       g_inde2_d[l_ac].indecrtid_desc,g_inde2_d[l_ac].indecrtdp_desc,g_inde2_d[l_ac].indemodid_desc, 
       g_inde2_d[l_ac].indecnfid_desc
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
      
      LET l_ac = l_ac + 1
      
   END FOREACH
   LET g_error_show = 0
   
 
   CALL g_inde_d.deleteElement(g_inde_d.getLength())   
   CALL g_inde2_d.deleteElement(g_inde2_d.getLength())
   CALL g_inde3_d.deleteElement(g_inde3_d.getLength())
 
   
   #確定指標無超過上限, 超過則指到最後一筆
   IF g_detail_idx > g_inde_d.getLength() THEN
      LET g_detail_idx = g_inde_d.getLength()
   END IF
   
   #將key欄位填到每個page
   LET l_ac_t = g_detail_idx
   FOR g_detail_idx = 1 TO g_inde_d.getLength()
      LET g_inde2_d[g_detail_idx].indedocno = g_inde_d[g_detail_idx].indedocno 
      #LET g_inde3_d[g_detail_idx2].indfseq = g_inde_d[g_detail_idx].indedocno 
 
   END FOR
   LET g_detail_idx = l_ac_t
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
    
   LET g_detail_cnt = l_ac - 1
   IF g_detail_cnt > 0 THEN
      DISPLAY g_detail_cnt TO FORMONLY.h_count
   END IF
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE ainp521_pb
   
   LET g_loc = 'm'
   CALL ainp521_detail_show() 
   
   LET l_ac = 1
   IF g_inde_d.getLength() > 0 THEN
      CALL ainp521_fetch()
   END IF
   
   #遮罩相關處理
   FOR l_ac = 1 TO g_inde_d.getLength()
      LET g_inde_d_mask_o[l_ac].* =  g_inde_d[l_ac].*
      CALL ainp521_inde_t_mask()
      LET g_inde_d_mask_n[l_ac].* =  g_inde_d[l_ac].*
   END FOR
   
   LET g_inde2_d_mask_o.* =  g_inde2_d.*
   FOR l_ac = 1 TO g_inde2_d.getLength()
      LET g_inde2_d_mask_o[l_ac].* =  g_inde2_d[l_ac].*
      CALL ainp521_inde_t_mask()
      LET g_inde2_d_mask_n[l_ac].* =  g_inde2_d[l_ac].*
   END FOR
   LET g_inde3_d_mask_o.* =  g_inde3_d.*
   FOR l_ac = 1 TO g_inde3_d.getLength()
      LET g_inde3_d_mask_o[l_ac].* =  g_inde3_d[l_ac].*
      CALL ainp521_indf_t_mask()
      LET g_inde3_d_mask_n[l_ac].* =  g_inde3_d[l_ac].*
   END FOR
 
   
   ERROR "" 
   
END FUNCTION
]]>
  </section>
