  <section id="ainp521.insert_b" ver="5" status="" src="s" readonly="">
    <![CDATA[#+ 新增單身後其他table連動
PRIVATE FUNCTION ainp521_insert_b(ps_table,ps_keys,ps_page)
   DEFINE ps_table    STRING
   DEFINE ps_page     STRING
   DEFINE ps_keys     DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ls_group    STRING
   #add-point:insert_b段define
   {<point name="insert_b.define" edit="s"/>}
   #end add-point
   #add-point:insert_b段define(客製用)
   {<point name="insert_b.define_customerization" edit="c"/>}
   #end add-point
 
   #判斷是否是同一群組的table
   LET ls_group = "inde_t,"
   IF ls_group.getIndexOf(ps_table,1) > 0 THEN
      #add-point:insert_b段新增前
      {<point name="insert_b.before_insert" mark="Y"/>}
      #end add-point
      INSERT INTO inde_t
                  (indeent,
                   indedocno
                   ,indestus,indesite,indedocdt,inde001,inde002,inde003,inde004,inde005,inde006,indeownid,indeowndp,indecrtid,indecrtdp,indecrtdt,indemodid,indemoddt,indecnfid,indecnfdt) 
            VALUES(g_enterprise,
                   ps_keys[1]
                   ,g_inde_d[g_detail_idx].indestus,g_inde_d[g_detail_idx].indesite,g_inde_d[g_detail_idx].indedocdt, 
                       g_inde_d[g_detail_idx].inde001,g_inde_d[g_detail_idx].inde002,g_inde_d[g_detail_idx].inde003, 
                       g_inde_d[g_detail_idx].inde004,g_inde_d[g_detail_idx].inde005,g_inde_d[g_detail_idx].inde006, 
                       g_inde2_d[g_detail_idx].indeownid,g_inde2_d[g_detail_idx].indeowndp,g_inde2_d[g_detail_idx].indecrtid, 
                       g_inde2_d[g_detail_idx].indecrtdp,g_inde2_d[g_detail_idx].indecrtdt,g_inde2_d[g_detail_idx].indemodid, 
                       g_inde2_d[g_detail_idx].indemoddt,g_inde2_d[g_detail_idx].indecnfid,g_inde2_d[g_detail_idx].indecnfdt) 

      #add-point:insert_b段新增中
      {<point name="insert_b.m_insert"/>}
      #end add-point
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "inde_t" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
      END IF
      #add-point:insert_b段新增後
      {<point name="insert_b.after_insert"/>}
      #end add-point
   END IF
   
 
   
   LET ls_group = "indf_t,"
   #判斷是否是同一群組的table
   IF ls_group.getIndexOf(ps_table,1) > 0 THEN
      #add-point:insert_b段新增前
      {<point name="insert_b.before_insert2" mark="Y"/>}
      #end add-point
      INSERT INTO indf_t
                  (indfent,
                   indfdocno,indfseq
                   ,indf001,indf003,indf002,indf006,indf040,indf041,indf042,indf043) 
            VALUES(g_enterprise,
                   ps_keys[1],ps_keys[2]
                   ,g_inde3_d[g_detail_idx2].indf001,g_inde3_d[g_detail_idx2].indf003,g_inde3_d[g_detail_idx2].indf002, 
                       g_inde3_d[g_detail_idx2].indf006,g_inde3_d[g_detail_idx2].indf040,g_inde3_d[g_detail_idx2].indf041, 
                       g_inde3_d[g_detail_idx2].indf042,g_inde3_d[g_detail_idx2].indf043)
      #add-point:insert_b段新增中
      {<point name="insert_b.m_insert2"/>}
      #end add-point
      IF SQLCA.sqlcode THEN
         RETURN
      END IF
      #add-point:insert_b段新增後
      {<point name="insert_b.after_insert2"/>}
      #end add-point
   END IF
 
 
   
END FUNCTION
