  <point name="function.ainp521_sel_init" order="12" ver="2" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#+
PRIVATE FUNCTION ainp521_sel_init()
DEFINE l_n   LIKE type_t.num5
   
   LET l_n = 0   
   SELECT COUNT(*) INTO l_n    
     FROM ainp521_tmp 
    WHERE tmp_indedocno = g_inde_d[l_ac].indedocno
   IF l_n = 0 THEN
      INSERT INTO ainp521_tmp(tmp_sel,tmp_indedocno) VALUES('N',g_inde_d[l_ac].indedocno)
   END IF
   
   SELECT tmp_sel INTO g_inde_d[l_ac].sel
     FROM ainp521_tmp 
    WHERE tmp_indedocno = g_inde_d[l_ac].indedocno
END FUNCTION]]>
