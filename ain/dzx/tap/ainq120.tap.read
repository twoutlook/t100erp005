  <section id="ainq120.b_fill" ver="1" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION ainq120_b_fill()
   {<Local define>}
   DEFINE ls_wc           STRING
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_detail_idx  = 1
   LET g_detail_idx2 = 1
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
 
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
 
   CALL g_inag_d.clear()
 
 
   
 
   LET g_cnt = l_ac
   LET l_ac = 1
   ERROR "Searching!"
 
   # b_fill段sql組成及FOREACH撰寫
      #+ 此段落由子樣板qs04產生
   #+ b_fill段資料取得(包含sql組成及FOREACH段撰寫)
   LET g_sql = "SELECT  UNIQUE inag001,inag002,inag003,inag004,'',inag005,'',inag006,inag007,inagsite, 
       inag018,inag027 FROM inag_t",
 
 
               "",
               " WHERE inagent= ? AND 1=1 AND ", ls_wc
 
   LET g_sql = g_sql, " ORDER BY inag_t.inagsite,inag_t.inag001,inag_t.inag002,inag_t.inag003,inag_t.inag004,inag_t.inag005,inag_t.inag006,inag_t.inag007"
 
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
 
   PREPARE ainq120_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR ainq120_pb
 
   FOREACH b_fill_curs INTO g_inag_d[l_ac].inag001,g_inag_d[l_ac].inag002,g_inag_d[l_ac].inag003,g_inag_d[l_ac].inag004, 
       g_inag_d[l_ac].inag004_desc,g_inag_d[l_ac].inag005,g_inag_d[l_ac].inag005_desc,g_inag_d[l_ac].inag006, 
       g_inag_d[l_ac].inag007,g_inag_d[l_ac].inagsite,g_inag_d[l_ac].inag018,g_inag_d[l_ac].inag027
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
 
      LET g_inag_d[l_ac].sel = "N"
 
      
 
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      CALL ainq120_detail_show()
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            CALL cl_err( "", 9035, 1 )
         END IF
         EXIT FOREACH
      END IF
 
   END FOREACH
 
 
 
   CALL g_inag_d.deleteElement(g_inag_d.getLength())
 
   LET g_error_show = 0
 
 
 
 
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = l_ac - 1
   DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
 
      #+ 此段落由子樣板qs06產生
   #+ b_fill段CURSOR釋放
   CLOSE b_fill_curs
   FREE ainq120_pb
 
 
 
 
   LET l_ac = 1
   CALL ainq120_b_fill2()
 
      CALL ainq120_filter_show('inag001','b_inag001')
   CALL ainq120_filter_show('inag002','b_inag002')
   CALL ainq120_filter_show('inag003','b_inag003')
   CALL ainq120_filter_show('inag004','b_inag004')
   CALL ainq120_filter_show('inag004_desc','b_inag004_desc')
   CALL ainq120_filter_show('inag005','b_inag005')
   CALL ainq120_filter_show('inag005_desc','b_inag005_desc')
   CALL ainq120_filter_show('inag006','b_inag006')
   CALL ainq120_filter_show('inag007','b_inag007')
   CALL ainq120_filter_show('inagsite','b_inagsite')
   CALL ainq120_filter_show('inag018','b_inag018')
   CALL ainq120_filter_show('inag027','b_inag027')
 
 
END FUNCTION
]]>
