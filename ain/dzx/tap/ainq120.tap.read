                              s_detail1[1].b_inagsite,s_detail1[1].b_inag018,s_detail1[1].b_inag027
 
         BEFORE CONSTRUCT
            CALL cl_qbe_init()
                     DISPLAY ainq120_filter_parser('inag001') TO s_detail1[1].b_inag001
            DISPLAY ainq120_filter_parser('inag002') TO s_detail1[1].b_inag002
            DISPLAY ainq120_filter_parser('inag003') TO s_detail1[1].b_inag003
            DISPLAY ainq120_filter_parser('inag004') TO s_detail1[1].b_inag004
            DISPLAY ainq120_filter_parser('inag004_desc') TO s_detail1[1].b_inag004_desc
            DISPLAY ainq120_filter_parser('inag005') TO s_detail1[1].b_inag005
            DISPLAY ainq120_filter_parser('inag005_desc') TO s_detail1[1].b_inag005_desc
