  <point name="function.aint340_unit_convert" order="1" ver="18" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL aint340_unit_convert()
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By lixh
# Modify.........:
################################################################################
PRIVATE FUNCTION aint340_unit_convert()
DEFINE   r_success   LIKE type_t.num5
DEFINE   r_rate      LIKE inaj_t.inaj014
              
   #單位取位
   CALL aint340_unit_round(g_indd_d[l_ac].indd006,g_indd_d[l_ac].indd103) RETURNING g_indd_d[l_ac].indd103
   LET g_indd_d[l_ac].indd021 = g_indd_d[l_ac].indd103 
   IF g_indc_m.indc102 = '2' THEN
      LET g_indd_d[l_ac].indd021 = 0   
   END IF
   IF NOT cl_null(g_indd_d[l_ac].indd002) AND
      NOT cl_null(g_indd_d[l_ac].indd006) AND
      NOT cl_null(g_indd_d[l_ac].indd041) AND
      NOT cl_null(g_indd_d[l_ac].indd103) THEN
      CALL s_aooi250_convert_qty(g_indd_d[l_ac].indd002,g_indd_d[l_ac].indd006,
                                 g_indd_d[l_ac].indd041,g_indd_d[l_ac].indd103)
      RETURNING r_success,g_indd_d[l_ac].indd031
      #add by lixh 20150330
      CALL s_aooi250_convert_qty(g_indd_d[l_ac].indd002,g_indd_d[l_ac].indd006,
                                 g_indd_d[l_ac].indd041,g_indd_d[l_ac].indd103)
      RETURNING r_success,g_indd_d[l_ac].indd107    
      #add by lixh 20150330      
   ELSE
      LET g_indd_d[l_ac].indd031 = 0
   END IF
   IF g_indc_m.indc102 <> '1' THEN
      LET g_indd_d[l_ac].indd031 = 0
   END IF                 
   IF ((g_indd_d[l_ac].indd103 <> g_indd_d_t.indd103 OR g_indd_d_t.indd103 IS NULL) OR (g_indd_d[l_ac].indd041 <> g_indd_d_t.indd041 OR g_indd_d_t.indd041 IS NULL)
       OR (g_indd_d[l_ac].indd006 <> g_indd_d_t.indd006 OR g_indd_d_t.indd006 IS NULL)) AND NOT cl_null(g_indd_d[l_ac].indd104) THEN

      CALL s_aooi250_convert_qty(g_indd_d[l_ac].indd002,g_indd_d[l_ac].indd006,g_indd_d[l_ac].indd104,g_indd_d[l_ac].indd103)
      RETURNING r_success,g_indd_d[l_ac].indd105                 
      IF r_success THEN
         CALL aint340_unit_round(g_indd_d[l_ac].indd104,g_indd_d[l_ac].indd105) RETURNING g_indd_d[l_ac].indd105
         LET g_indd_d[l_ac].indd106 = g_indd_d[l_ac].indd105
         LET g_indd_d[l_ac].indd108 = g_indd_d[l_ac].indd105   #add by lixh 20150330
         LET g_indd_d[l_ac].indd109 = g_indd_d[l_ac].indd105    
         IF g_indc_m.indc102 = '2' THEN
            LET g_indd_d[l_ac].indd106 = 0
            LET g_indd_d[l_ac].indd109 = 0
         END IF   
         IF g_indc_m.indc102 = '3' THEN
            LET g_indd_d[l_ac].indd106 = g_indd_d[l_ac].indd105
            LET g_indd_d[l_ac].indd109 = 0
         END IF             
      END IF  
   END IF 
END FUNCTION]]>
  </point>
