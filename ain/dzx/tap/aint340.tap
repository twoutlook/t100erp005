  <section id="aint340.filter" ver="7" status="" src="s" readonly="">
    <![CDATA[#應用 a50 樣板自動產生(Version:5)
#+ filter過濾功能
PRIVATE FUNCTION aint340_filter()
   #add-point:filter段define
   {<point name="filter.define" edit="s"/>}
   #end add-point   
   #add-point:filter段define
   {<point name="filter.define_customerization" edit="c"/>}
   #end add-point   
   
   #切換畫面
   IF NOT g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",1)
      CALL gfrm_curr.setElementHidden("worksheet",0)
      LET g_main_hidden = 1
   END IF   
 
   LET INT_FLAG = 0
 
   LET g_qryparam.state = 'c'
 
   LET g_wc_filter_t = g_wc_filter.trim()
   LET g_wc_t = g_wc
 
   LET g_wc = cl_replace_str(g_wc, g_wc_filter_t, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON indcdocno,indcdocdt,indc022,indc004,indc101
                          FROM s_browse[1].b_indcdocno,s_browse[1].b_indcdocdt,s_browse[1].b_indc022, 
                              s_browse[1].b_indc004,s_browse[1].b_indc101
 
         BEFORE CONSTRUCT
               DISPLAY aint340_filter_parser('indcdocno') TO s_browse[1].b_indcdocno
            DISPLAY aint340_filter_parser('indcdocdt') TO s_browse[1].b_indcdocdt
            DISPLAY aint340_filter_parser('indc022') TO s_browse[1].b_indc022
            DISPLAY aint340_filter_parser('indc004') TO s_browse[1].b_indc004
            DISPLAY aint340_filter_parser('indc101') TO s_browse[1].b_indc101
      
         #add-point:filter段cs_ctrl
