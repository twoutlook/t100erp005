    <![CDATA[
################################################################################
# Descriptions...: 檢查當前庫位與單身中的其他項次對應的庫位是否存在相同的控制組
# Memo...........:
# Usage..........: CALL aint340_control_group_indd022(p_site,p_indd022)
#                  RETURNING 回传参数
# Input parameter: p_site         營運據點
#                : p_indd022      撥入庫位
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By lixh
# Modify.........:
################################################################################
PRIVATE FUNCTION aint340_control_group_indd022(p_site,p_indd022)
DEFINE p_site       LIKE indc_t.indcsite
DEFINE p_indd022    LIKE indd_t.indd022
DEFINE l_indd022    LIKE indd_t.indd022
DEFINE l_oohl001    LIKE oohl_t.oohl001
DEFINE l_n          LIKE type_t.num5
DEFINE l_flag       LIKE type_t.num5
DEFINE r_success    LIKE type_t.num5
DEFINE l_inddseq    LIKE indd_t.inddseq
DEFINE l_i          LIKE type_t.num5
DEFINE l_cn         LIKE type_t.num5

    LET r_success = TRUE
    LET l_flag = TRUE
    LET l_i = 0
    LET l_cn = 0

    IF cl_null(g_indd_d_t.inddseq) THEN
       LET l_inddseq = g_indd_d[l_ac].inddseq
    ELSE
       LET l_inddseq = g_indd_d_t.inddseq
    END IF

    DECLARE indd022_cs1 CURSOR FOR
        SELECT DISTINCT indd022 FROM indd_t
          WHERE inddent = g_enterprise AND indddocno = g_indc_m.indcdocno
            AND inddseq <> l_inddseq     #考慮到修改資料時，修改前的資料不需要再進行檢查

    FOREACH indd022_cs1 INTO l_indd022

       LET l_cn = l_cn + 1  #單身是否有資料

       DECLARE indd022_cs2 CURSOR FOR
          SELECT oohl001 FROM oohl_t WHERE oohlent = g_enterprise AND oohl002 = g_site AND oohl003 = l_indd022
       FOREACH indd022_cs2 INTO l_oohl001   #獲得控制組編號

          LET l_i = l_i +1   #用于判断是否有走进foreach,即判断其他单身的库位是否存在控制组

          #判斷當前輸入的庫位編號是否存在其他庫位編號的控制組中
          LET l_n = 0
          SELECT COUNT(*) INTO l_n FROM oohl_t
             WHERE oohlent = g_enterprise AND oohl001 = l_oohl001
               AND oohl002 = g_site AND oohl003 = p_indd022
          IF l_n > 0 THEN  #存在相同的控制組編號
             LET l_flag = TRUE
             EXIT FOREACH   #存在一個相同的控制組編號，則其他的控制組編號無需再判斷
          ELSE
             LET l_flag = FALSE
          END IF

       END FOREACH
       IF NOT l_flag THEN   #如果已經有一筆庫位的控制組編號與當前庫位不存在相同的控制組，則其他的單身庫位也無需在進行檢查
          EXIT FOREACH
       END IF

    END FOREACH

    #如果其他单身的库位都不存在控制组中且單身有其他資料，再判断当前库位是否有控制组，若有，则报错
    IF l_i = 0 AND l_cn > 0 THEN
       LET l_n = 0
       SELECT COUNT(oohl001) INTO l_n FROM oohl_t
         WHERE oohlent = g_enterprise AND oohl002 = g_site AND oohl003 = p_indd022
       IF l_n > 0 THEN
          LET l_flag = FALSE
       END IF
    END IF

    IF NOT l_flag THEN
       INITIALIZE g_errparam TO NULL
       LET g_errparam.code = 'ain-00274'
       LET g_errparam.extend = p_indd022
       LET g_errparam.popup = TRUE
       CALL cl_err()

       LET r_success = FALSE
       RETURN r_success
    END IF

    RETURN r_success
END FUNCTION]]>
  </point>
