         #更新單身key值
         UPDATE indd_t SET indddocno = g_indc_m.indcdocno
 
          WHERE inddent = g_enterprise AND indddocno = g_indcdocno_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
