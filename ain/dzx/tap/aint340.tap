  <point name="statechange.b_update" order="" ver="13" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[      IF lc_state = 'O' AND g_indc_m.indcstus = 'N' THEN
         CALL s_transaction_begin()
         IF NOT cl_ask_confirm('aim-00108') THEN
            RETURN
         ELSE            
            IF NOT cl_null(g_indc_m.indc007) AND NOT cl_null(g_indc_m.indc109) THEN
            
            ELSE
               DECLARE aint340_indd022_cs CURSOR FOR SELECT DISTINCT indd022 FROM indd_t WHERE inddent = g_enterprise
                                                        AND indddocno = g_indc_m.indcdocno
               LET l_flag1 = 'N'
               LET l_flag2 = 'N'
               FOREACH aint340_indd022_cs INTO l_indd022
                  IF NOT cl_null(l_indd022) THEN
                     LET l_inaa010 = ''
                     SELECT inaa010 INTO l_inaa010 FROM inaa_t
                      WHERE inaaent = g_enterprise
                        AND inaasite = g_site
                        AND inaa001 = l_indd022
                     IF l_inaa010 = 'Y' THEN
                        LET l_flag1 = 'Y'
                     ELSE
                        LET l_flag2 = 'Y'     
                     END IF                        
                  END IF     
                  
               END FOREACH  
               IF (l_flag1 = 'Y' AND cl_null(g_indc_m.indc007)) OR (l_flag2 = 'Y' AND cl_null(g_indc_m.indc109)) THEN 

                  DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
                     INPUT BY NAME g_indc_m.indc007,g_indc_m.indc109
                           ATTRIBUTE(WITHOUT DEFAULTS)    
                        BEFORE INPUT
                           IF l_flag1 = 'Y' THEN
                              CALL cl_set_comp_required('indc007',TRUE)
                           END IF   
                           IF l_flag2 = 'Y' THEN
                              CALL cl_set_comp_required('indc109',TRUE)
                           END IF
                        AFTER FIELD indc007
                           CALL s_desc_get_stock_desc(g_site,g_indc_m.indc007) RETURNING g_indc_m.indc007_desc
                           DISPLAY BY NAME g_indc_m.indc007_desc
                           IF NOT cl_null(g_indc_m.indc007) THEN 
                              #此段落由子樣板a19產生
                              #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
                              INITIALIZE g_chkparam.* TO NULL
                              
                              #設定g_chkparam.*的參數
                              LET g_chkparam.arg1 = g_indc_m.indc007
                                 
                              #呼叫檢查存在並帶值的library
                              IF cl_chk_exist("v_inaa001_11") THEN
                                 #檢查成功時後續處理
                                 #LET  = g_chkparam.return1
                                 #DISPLAY BY NAME 
                              ELSE
                                 #檢查失敗時後續處理
                                 LET g_indc_m.indc007 = g_indc_m_t.indc007
                                 CALL s_desc_get_stock_desc(g_site,g_indc_m.indc007) RETURNING g_indc_m.indc007_desc 
                                 DISPLAY BY NAME g_indc_m.indc007_desc
                                 NEXT FIELD CURRENT
                              END IF
                              CALL s_desc_get_stock_desc(g_site,g_indc_m.indc007) RETURNING g_indc_m.indc007_desc
                              DISPLAY BY NAME g_indc_m.indc007_desc
                           END IF 
                     AFTER FIELD indc109
                        CALL s_desc_get_stock_desc(g_site,g_indc_m.indc109) RETURNING g_indc_m.indc109_desc
                        DISPLAY BY NAME g_indc_m.indc109_desc  
                        IF NOT cl_null(g_indc_m.indc109) THEN 
                           #此段落由子樣板a19產生
                           #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
                           INITIALIZE g_chkparam.* TO NULL
                           
                           #設定g_chkparam.*的參數
                           LET g_chkparam.arg1 = g_indc_m.indc109
                              
                           #呼叫檢查存在並帶值的library
                           IF cl_chk_exist("v_inaa001_12") THEN
                              #檢查成功時後續處理
                              #LET  = g_chkparam.return1
                              #DISPLAY BY NAME 
                           ELSE
                              #檢查失敗時後續處理
                              LET g_indc_m.indc109 = g_indc_m_t.indc109
                              CALL s_desc_get_stock_desc(g_site,g_indc_m.indc109) RETURNING g_indc_m.indc109_desc
                              DISPLAY BY NAME g_indc_m.indc109_desc                  
                              NEXT FIELD CURRENT
                           END IF
                           CALL s_desc_get_stock_desc(g_site,g_indc_m.indc109) RETURNING g_indc_m.indc109_desc
                           DISPLAY BY NAME g_indc_m.indc109_desc              
                        END IF         
                     ON ACTION controlp INFIELD indc007
                        #add-point:ON ACTION controlp INFIELD indc007
                        #此段落由子樣板a07產生            
                        #開窗i段
                        INITIALIZE g_qryparam.* TO NULL
                        LET g_qryparam.state = 'i'
                        LET g_qryparam.reqry = FALSE
                     
                        LET g_qryparam.default1 = g_indc_m.indc007             #給予default值
                     
                        #給予arg
                        
                        CALL q_inaa001_16()                                #呼叫開窗
                     
                        LET g_indc_m.indc007 = g_qryparam.return1              
                     
                        DISPLAY g_indc_m.indc007 TO indc007              #
                     
                        NEXT FIELD indc007   

                     ON ACTION controlp INFIELD indc109
                        #add-point:ON ACTION controlp INFIELD indc109
                        #此段落由子樣板a07產生            
                        #開窗i段
                        INITIALIZE g_qryparam.* TO NULL
                        LET g_qryparam.state = 'i'
                        LET g_qryparam.reqry = FALSE
                     
                        LET g_qryparam.default1 = g_indc_m.indc109             #給予default值
                     
                        #給予arg
#                        LET g_qryparam.arg1 = "" #
                     
                        
                        CALL q_inaa001_17()                                #呼叫開窗
                     
                        LET g_indc_m.indc109 = g_qryparam.return1              
                     
                        DISPLAY g_indc_m.indc109 TO indc109              #
                     
                        NEXT FIELD indc109                          #返回原欄位
                     END INPUT     
                     
                  BEFORE DIALOG      
                     ON ACTION accept
                        ACCEPT DIALOG
                  
                     ON ACTION cancel
                        LET INT_FLAG = 1
                        LET g_indc_m.indc007 = g_indc_m_t.indc007
                        LET g_indc_m.indc109 = g_indc_m_t.indc109
                        EXIT DIALOG   
                        
                  END DIALOG  
                  IF INT_FLAG THEN
                     RETURN 
                  END IF        
                  UPDATE indc_t SET indc007 = g_indc_m.indc007,
                                    indc109 = g_indc_m.indc109 
                   WHERE indcent = g_enterprise
                     AND indcdocno = g_indc_m.indcdocno 
                                    
              END IF
                              
            END IF   
            CALL cl_err_collect_init()
            IF NOT s_aint340_conf_chk(g_indc_m.indcdocno) THEN
               CALL cl_err_collect_show()
               CALL s_transaction_end('N','0')
               RETURN
            ELSE
               IF NOT s_aint340_conf_upd(g_indc_m.indcdocno) THEN
                  CALL cl_err_collect_show()
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','0')
               END IF
            END IF
         END IF
      END IF
      IF lc_state = 'O' AND g_indc_m.indcstus = 'S' THEN
         CALL s_transaction_begin()
         IF NOT cl_ask_confirm('sub-00233') THEN
            RETURN
         ELSE
            CALL cl_err_collect_init()
            IF NOT s_aint340_unpost_chk(g_indc_m.indcdocno) THEN
               CALL cl_err_collect_show()
               CALL s_transaction_end('N','0')
               RETURN
            ELSE
               IF NOT s_aint340_unpost_upd(g_indc_m.indcdocno) THEN
                  CALL cl_err_collect_show()
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','0')
               END IF
            END IF
         END IF    
      END IF ]]>
  </point>
