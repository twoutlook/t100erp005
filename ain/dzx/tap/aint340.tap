   #add-point:完成複製段落後
   {<point name="reproduce.after_reproduce" />}
   #end add-point
   
   CALL aint340_idx_chk()
   
   #功能已完成,通報訊息中心
   CALL aint340_msgcentre_notify('reproduce')
 
END FUNCTION
