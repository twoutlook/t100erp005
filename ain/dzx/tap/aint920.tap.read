   #LET g_wc2_table1 = l_wc2_table1
   #LET  g_wc2_table2 = l_wc2_table2 
 
 
 
    
   WHILE TRUE
      LET g_inpjsite_t = g_inpj_m.inpjsite
      LET g_inpjdocno_t = g_inpj_m.inpjdocno
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_inpj_m.inpjmodid = g_user 
LET g_inpj_m.inpjmoddt = cl_get_current()
LET g_inpj_m.inpjmodid_desc = cl_get_username(g_inpj_m.inpjmodid)
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
