      {<point name="browser_fill.add_browse.other"/>}
      #end add-point   
      CALL g_browser.clear()
      LET g_cnt = 1
   ELSE
      LET l_wc  = g_add_browse
      LET l_wc2 = " 1=1" 
      LET g_cnt = g_current_idx
   END IF
 
   #依照t0.inpjsite,t0.inpjdocno,t0.inpjdocdt,t0.inpj002,t0.inpj003 Browser欄位定義(取代原本bs_sql功能)
