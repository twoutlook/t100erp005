               CALL aint920_inpk_t_mask_restore('restore_mask_o')
      
               UPDATE inpk_t SET (inpksite,inpkdocno,inpkseq,inpk001) = (g_inpj_m.inpjsite,g_inpj_m.inpjdocno, 
                   g_inpk_d[l_ac].inpkseq,g_inpk_d[l_ac].inpk001)
