               LET gs_keys_bak[2] = g_inpjdocno_t
               LET gs_keys[3] = g_inpk_d[g_detail_idx].inpkseq
               LET gs_keys_bak[3] = g_inpk_d_t.inpkseq
               CALL aint920_update_b('inpk_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
               END CASE
 
               #將遮罩欄位進行遮蔽
               CALL aint920_inpk_t_mask_restore('restore_mask_n')
               
               #判斷key是否有改變
               INITIALIZE gs_keys TO NULL
               IF NOT(g_inpk_d[g_detail_idx].inpkseq = g_inpk_d_t.inpkseq 
 
                  ) THEN
                  LET gs_keys[01] = g_inpj_m.inpjsite
                  LET gs_keys[gs_keys.getLength()+1] = g_inpj_m.inpjdocno
 
                  LET gs_keys[gs_keys.getLength()+1] = g_inpk_d_t.inpkseq
 
                  CALL aint920_key_update_b(gs_keys,'inpk_t')
               END IF
               
               #修改歷程記錄(單身修改)
               LET g_log1 = util.JSON.stringify(g_inpj_m),util.JSON.stringify(g_inpk_d_t)
               LET g_log2 = util.JSON.stringify(g_inpj_m),util.JSON.stringify(g_inpk_d[l_ac])
               IF NOT cl_log_modified_record_d(g_log1,g_log2) THEN 
                  CALL s_transaction_end('N','0')
               END IF
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            #add-point:單身after_row
            {<point name="input.body.after_row"/>}
            #end add-point
            CALL aint920_unlock_b("inpk_t","'1'")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock
            #add-point:單身after_row2
            {<point name="input.body.after_row2"/>}
            #end add-point
              
         AFTER INPUT
            #add-point:input段after input 
            {<point name="input.body.after_input"/>}
            #end add-point 
 
         ON ACTION controlo    
            CALL FGL_SET_ARR_CURR(g_inpk_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_inpk_d.getLength()+1
            
         #ON ACTION cancel
         #   LET INT_FLAG = 1
         #   LET g_detail_idx = 1
         #   EXIT DIALOG 
 
      END INPUT
      
      INPUT ARRAY g_inpk2_d FROM s_detail2.*
         ATTRIBUTE(COUNT = g_rec_b,WITHOUT DEFAULTS, #MAXCOUNT = g_max_rec,
                 INSERT ROW = l_allow_insert, #此頁面insert功能由產生器控制, 手動之設定無效! 

                 DELETE ROW = l_allow_delete,
                 APPEND ROW = l_allow_insert)
                 
         #自訂ACTION(detail_input,page_2)
         
         
         BEFORE INPUT
            #add-point:資料輸入前
            {<point name="input.body2.before_input2"/>}
            #end add-point
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_inpk2_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL aint920_b_fill()
            #如果一直都在單身1則控制筆數位置
            IF g_loc = 'd' AND g_rec_b != 0 THEN
               CALL FGL_SET_ARR_CURR(g_detail_idx)
            END IF
            LET g_loc = 'd'
            LET g_rec_b = g_inpk2_d.getLength()
            #add-point:資料輸入前
            {<point name="input.body2.before_input"/>}
            #end add-point
            
         BEFORE INSERT
            
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_inpk2_d[l_ac].* TO NULL 
            INITIALIZE g_inpk2_d_t.* TO NULL 
            INITIALIZE g_inpk2_d_o.* TO NULL 
            #公用欄位給值(單身2)
            
            #自定義預設值(單身2)
            
            #add-point:modify段before備份
            {<point name="input.body2.insert.before_bak"/>}
            #end add-point
            LET g_inpk2_d_t.* = g_inpk2_d[l_ac].*     #新輸入資料
            LET g_inpk2_d_o.* = g_inpk2_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL aint920_set_entry_b(l_cmd)
            #add-point:modify段after_set_entry_b
            {<point name="input.body2.insert.after_set_entry_b"/>}
            #end add-point
            CALL aint920_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_inpk2_d[li_reproduce_target].* = g_inpk2_d[li_reproduce].*
 
               LET g_inpk2_d[li_reproduce_target].inpnseq = NULL
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body2.before_insert"/>}
            #end add-point  
            
         BEFORE ROW     
            #add-point:modify段before row2
            {<point name="input.body2.before_row2"/>}
            #end add-point  
            LET l_insert = FALSE
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET g_detail_idx = l_ac
            LET g_current_page = 2
              
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
         
            CALL s_transaction_begin()
            OPEN aint920_cl USING g_enterprise,g_inpj_m.inpjsite,g_inpj_m.inpjdocno
            IF STATUS THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "OPEN aint920_cl:" 
               LET g_errparam.code   = STATUS 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CLOSE aint920_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            
            LET g_rec_b = g_inpk2_d.getLength()
            
            IF g_rec_b >= l_ac 
               AND g_inpk2_d[l_ac].inpnseq IS NOT NULL
            THEN 
               LET l_cmd='u'
               LET g_inpk2_d_t.* = g_inpk2_d[l_ac].*  #BACKUP
               LET g_inpk2_d_o.* = g_inpk2_d[l_ac].*  #BACKUP
               CALL aint920_set_entry_b(l_cmd)
               #add-point:modify段after_set_entry_b
               {<point name="input.body2.after_set_entry_b"/>}
               #end add-point  
               CALL aint920_set_no_entry_b(l_cmd)
               IF NOT aint920_lock_b("inpn_t","'2'") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aint920_bcl2 INTO g_inpk2_d[l_ac].inpnseq,g_inpk2_d[l_ac].inpn001
                  IF SQLCA.sqlcode THEN
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = '' 
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     LET l_lock_sw = "Y"
                  END IF
                  
                  #遮罩相關處理
                  LET g_inpk2_d_mask_o[l_ac].* =  g_inpk2_d[l_ac].*
                  CALL aint920_inpn_t_mask()
                  LET g_inpk2_d_mask_n[l_ac].* =  g_inpk2_d[l_ac].*
                  
                  LET g_bfill = "N"
                  CALL aint920_show()
                  LET g_bfill = "Y"
                  
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body2.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
            
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' THEN
               LET l_cmd='d'
               #add-point:單身AFTER DELETE (=d)
               {<point name="input.body2.after_delete_d"/>}
               #end add-point
            ELSE
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "" 
                  LET g_errparam.code   = -263 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
                  CANCEL DELETE
               END IF
               
               #add-point:單身2刪除前
               {<point name="input.body2.b_delete" mark="Y"/>}
               #end add-point    
                  
               #取得該筆資料key值
               INITIALIZE gs_keys TO NULL
               LET gs_keys[01] = g_inpj_m.inpjsite
               LET gs_keys[gs_keys.getLength()+1] = g_inpj_m.inpjdocno
               LET gs_keys[gs_keys.getLength()+1] = g_inpk2_d_t.inpnseq
            
               #刪除同層單身
               IF NOT aint920_delete_b('inpn_t',gs_keys,"'2'") THEN
                  CALL s_transaction_end('N','0')
                  CLOSE aint920_bcl
                  CANCEL DELETE
               END IF
    
               #刪除下層單身
               IF NOT aint920_key_delete_b(gs_keys,'inpn_t') THEN
                  CALL s_transaction_end('N','0')
                  CLOSE aint920_bcl
                  CANCEL DELETE
               END IF
               
               #刪除多語言
               
               
               #add-point:單身2刪除中
               {<point name="input.body2.m_delete"/>}
               #end add-point    
               
               CALL s_transaction_end('Y','0')
               CLOSE aint920_bcl
               LET g_rec_b = g_rec_b-1
               
               #add-point:單身2刪除後
               {<point name="input.body2.a_delete"/>}
               #end add-point
               LET l_count = g_inpk_d.getLength()
               
               #add-point:單身刪除後(<>d)
               {<point name="input.body2.after_delete"/>}
               #end add-point
            END IF 
         AFTER DELETE
            #如果是最後一筆
            IF l_ac = (g_inpk2_d.getLength() + 1) THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
            END IF
 
         AFTER INSERT    
            LET l_insert = FALSE
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            #add-point:單身2新增前
            {<point name="input.body2.b_a_insert"/>}
            #end add-point
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM inpn_t 
             WHERE inpnent = g_enterprise AND inpnsite = g_inpj_m.inpjsite
               AND inpndocno = g_inpj_m.inpjdocno
               AND inpnseq = g_inpk2_d[l_ac].inpnseq
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身2新增前
               {<point name="input.body2.b_insert"/>}
               #end add-point
            
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_inpj_m.inpjsite
               LET gs_keys[2] = g_inpj_m.inpjdocno
               LET gs_keys[3] = g_inpk2_d[g_detail_idx].inpnseq
               CALL aint920_insert_b('inpn_t',gs_keys,"'2'")
                           
               #add-point:單身新增後2
               {<point name="input.body2.a_insert"/>}
               #end add-point
            ELSE    
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = 'INSERT' 
               LET g_errparam.code   = "std-00006" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               INITIALIZE g_inpk_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "inpn_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL aint920_b_fill()
               #資料多語言用-增/改
               
               #add-point:單身新增後
               {<point name="input.body2.after_insert"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               #ERROR 'INSERT O.K'
               LET g_rec_b = g_rec_b + 1
            END IF
            
         ON ROW CHANGE 
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               LET g_inpk2_d[l_ac].* = g_inpk2_d_t.*
               CLOSE aint920_bcl2
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
            
            IF l_lock_sw = 'Y' THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = -263 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               LET g_inpk2_d[l_ac].* = g_inpk2_d_t.*
            ELSE
               #add-point:單身page2修改前
               {<point name="input.body2.b_update" mark="Y"/>}
               #end add-point
               
               #寫入修改者/修改日期資訊(單身2)
               
               
               #將遮罩欄位還原
               CALL aint920_inpn_t_mask_restore('restore_mask_o')
                              
               UPDATE inpn_t SET (inpnsite,inpndocno,inpnseq,inpn001) = (g_inpj_m.inpjsite,g_inpj_m.inpjdocno, 
                   g_inpk2_d[l_ac].inpnseq,g_inpk2_d[l_ac].inpn001) #自訂欄位頁簽
                WHERE inpnent = g_enterprise AND inpnsite = g_inpj_m.inpjsite
                  AND inpndocno = g_inpj_m.inpjdocno
                  AND inpnseq = g_inpk2_d_t.inpnseq #項次 
                  
               #add-point:單身page2修改中
               {<point name="input.body2.m_update"/>}
               #end add-point
                  
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "inpn_t" 
                     LET g_errparam.code   = "std-00009" 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     CALL s_transaction_end('N','0')
                     LET g_inpk2_d[l_ac].* = g_inpk2_d_t.*
                  WHEN SQLCA.sqlcode #其他錯誤
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "inpn_t" 
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     CALL s_transaction_end('N','0')
                     LET g_inpk2_d[l_ac].* = g_inpk2_d_t.*
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_inpj_m.inpjsite
               LET gs_keys_bak[1] = g_inpjsite_t
               LET gs_keys[2] = g_inpj_m.inpjdocno
               LET gs_keys_bak[2] = g_inpjdocno_t
               LET gs_keys[3] = g_inpk2_d[g_detail_idx].inpnseq
               LET gs_keys_bak[3] = g_inpk2_d_t.inpnseq
               CALL aint920_update_b('inpn_t',gs_keys,gs_keys_bak,"'2'")
                     #資料多語言用-增/改
                     
               END CASE
               
               #將遮罩欄位進行遮蔽
               CALL aint920_inpn_t_mask_restore('restore_mask_n')
               
               #判斷key是否有改變
               INITIALIZE gs_keys TO NULL
               IF NOT (g_inpk2_d[g_detail_idx].inpnseq = g_inpk2_d_t.inpnseq 
                  ) THEN
                  LET gs_keys[01] = g_inpj_m.inpjsite
                  LET gs_keys[gs_keys.getLength()+1] = g_inpj_m.inpjdocno
                  LET gs_keys[gs_keys.getLength()+1] = g_inpk2_d_t.inpnseq
                  CALL aint920_key_update_b(gs_keys,'inpn_t')
               END IF
               
               #修改歷程記錄(單身修改)
               LET g_log1 = util.JSON.stringify(g_inpj_m),util.JSON.stringify(g_inpk2_d_t)
               LET g_log2 = util.JSON.stringify(g_inpj_m),util.JSON.stringify(g_inpk2_d[l_ac])
               IF NOT cl_log_modified_record_d(g_log1,g_log2) THEN 
                  CALL s_transaction_end('N','0')
               END IF
               
               #add-point:單身page2修改後
               {<point name="input.body2.a_update"/>}
               #end add-point
            END IF
         
                  #應用 a02 樣板自動產生(Version:1)
         AFTER FIELD inpnseq
            #應用 a15 樣板自動產生(Version:2)
            #確認欄位值在特定區間內
            IF NOT cl_ap_chk_range(g_inpk2_d[l_ac].inpnseq,"0","0","","","azz-00079",1) THEN
               NEXT FIELD inpnseq
            END IF 
 
 
            #add-point:AFTER FIELD inpnseq
            {<point name="input.a.page2.inpnseq" />}
            #END add-point
            
 
         #應用 a01 樣板自動產生(Version:1)
         BEFORE FIELD inpnseq
            #add-point:BEFORE FIELD inpnseq
            {<point name="input.b.page2.inpnseq" />}
            #END add-point
 
         #應用 a04 樣板自動產生(Version:2)
         ON CHANGE inpnseq
            #add-point:ON CHANGE inpnseq
            {<point name="input.g.page2.inpnseq" />}
            #END add-point 
 
         #應用 a02 樣板自動產生(Version:1)
         AFTER FIELD inpn001
            
            #add-point:AFTER FIELD inpn001
            {<point name="input.a.page2.inpn001" />}
            #END add-point
            
 
         #應用 a01 樣板自動產生(Version:1)
         BEFORE FIELD inpn001
            #add-point:BEFORE FIELD inpn001
            {<point name="input.b.page2.inpn001" />}
            #END add-point
 
         #應用 a04 樣板自動產生(Version:2)
         ON CHANGE inpn001
            #add-point:ON CHANGE inpn001
            {<point name="input.g.page2.inpn001" />}
            #END add-point 
 
 
                  #Ctrlp:input.c.page2.inpnseq
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD inpnseq
            #add-point:ON ACTION controlp INFIELD inpnseq
            {<point name="input.c.page2.inpnseq" />}
            #END add-point
 
         #Ctrlp:input.c.page2.inpn001
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD inpn001
            #add-point:ON ACTION controlp INFIELD inpn001
            {<point name="input.c.page2.inpn001" />}
            #END add-point
 
 
 
         AFTER ROW
            #add-point:單身page2 after_row
            {<point name="input.body2.after_row"/>}
            #end add-point
            LET l_ac = ARR_CURR()
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               IF l_cmd = 'u' THEN
                  LET g_inpk2_d[l_ac].* = g_inpk2_d_t.*
               END IF
               CLOSE aint920_bcl2
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
            
            #其他table進行unlock
            
            CALL aint920_unlock_b("inpn_t","'2'")
            CALL s_transaction_end('Y','0')
            #add-point:單身page2 after_row2
            {<point name="input.body2.after_row2"/>}
            #end add-point
 
         AFTER INPUT
            #add-point:input段after input 
            {<point name="input.body2.after_input"/>}
            #end add-point   
    
         ON ACTION controlo
            CALL FGL_SET_ARR_CURR(g_inpk2_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_inpk2_d.getLength()+1
 
      END INPUT
 
      
 
 
 
]]>
