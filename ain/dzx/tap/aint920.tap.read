  <point name="statechange.b_update" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[      CALL s_transaction_begin()
      IF lc_state = 'Y' THEN
         CALL s_aint920_conf_chk(g_inpj_m.inpjdocno) RETURNING r_success
         IF NOT r_success THEN
            RETURN
         ELSE
            IF NOT cl_ask_confirm('aim-00108') THEN
               RETURN
            ELSE
               CALL s_aint920_conf_upd(g_inpj_m.inpjdocno) RETURNING r_success
               IF NOT r_success THEN
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','0')
               END IF
            END IF
         END IF
         CALL aint920_b_fill()
      END IF
      IF lc_state = 'N' THEN
         CALL s_aint920_unconf_chk(g_inpj_m.inpjdocno) RETURNING r_success
         IF NOT r_success THEN
            RETURN
         ELSE
            IF NOT cl_ask_confirm('ain-00267') THEN
               RETURN
            ELSE
               CALL s_aint920_unconf_upd(g_inpj_m.inpjdocno) RETURNING r_success
               IF NOT r_success THEN
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','0')
               END IF
            END IF
         END IF
         CALL aint920_b_fill()
      END IF
      IF lc_state = 'X' THEN
         CALL s_aint920_void_chk(g_inpj_m.inpjdocno) RETURNING r_success
         IF NOT r_success THEN
            RETURN
         ELSE
            IF NOT cl_ask_confirm('aim-00109') THEN
               RETURN
            ELSE
               CALL s_aint920_void_upd(g_inpj_m.inpjdocno) RETURNING r_success
               IF NOT r_success THEN
                  CALL s_transaction_end('N','0')
                  RETURN
               ELSE
                  CALL s_transaction_end('Y','0')
               END IF
            END IF
         END IF
      END IF]]>
  </point>
