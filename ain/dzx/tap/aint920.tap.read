            {<point name="input.g.page1.inpk001" />}
            #END add-point 
 
 
                  #Ctrlp:input.c.page1.inpkseq
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD inpkseq
            #add-point:ON ACTION controlp INFIELD inpkseq
            {<point name="input.c.page1.inpkseq" />}
            #END add-point
 
         #Ctrlp:input.c.page1.inpk001
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD inpk001
            #add-point:ON ACTION controlp INFIELD inpk001
            {<point name="input.c.page1.inpk001" />}
            #END add-point
 
 
 
         ON ROW CHANGE
