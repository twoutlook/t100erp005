      LET g_update = FALSE
      CALL aint920_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE inpj_t SET (inpjmodid,inpjmoddt) = (g_inpj_m.inpjmodid,g_inpj_m.inpjmoddt)
          WHERE inpjent = g_enterprise AND inpjsite = g_inpjsite_t
            AND inpjdocno = g_inpjdocno_t
 
      END IF
    
      IF INT_FLAG THEN
