       g_inpj_m.inpj029,g_inpj_m.inpj030,g_inpj_m.inpj031,g_inpj_m.inpj032,g_inpj_m.inpj033,g_inpj_m.inpj034, 
       g_inpj_m.inpj035,g_inpj_m.inpj036,g_inpj_m.inpj037,g_inpj_m.inpj038,g_inpj_m.inpj039,g_inpj_m.inpj040, 
       g_inpj_m.inpj041,g_inpj_m.inpj042,g_inpj_m.inpj043,g_inpj_m.inpj044,g_inpj_m.inpj045,g_inpj_m.inpj046, 
       g_inpj_m.inpj047,g_inpj_m.inpj048,g_inpj_m.inpj049,g_inpj_m.inpj050,g_inpj_m.inpj051,g_inpj_m.inpj052, 
       g_inpj_m.inpj053,g_inpj_m.inpj054,g_inpj_m.inpj055,g_inpj_m.inpj056,g_inpj_m.inpj057,g_inpj_m.inpj058, 
       g_inpj_m.inpj059,g_inpj_m.inpj060,g_inpj_m.inpj061,g_inpj_m.inpj062,g_inpj_m.inpj063,g_inpj_m.inpj064, 
       g_inpj_m.inpj065,g_inpj_m.inpj066,g_inpj_m.inpj067,g_inpj_m.inpj068,g_inpj_m.inpj069,g_inpj_m.inpj070, 
