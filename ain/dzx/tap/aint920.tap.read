       g_inpj_m.inpj077,g_inpj_m.inpj078,g_inpj_m.inpj001,g_inpj_m.inpj006,g_inpj_m.inpjownid,g_inpj_m.inpjowndp, 
       g_inpj_m.inpjcrtid,g_inpj_m.inpjcrtdp,g_inpj_m.inpjcrtdt,g_inpj_m.inpjmodid,g_inpj_m.inpjmoddt, 
       g_inpj_m.inpjcnfid,g_inpj_m.inpjcnfdt,g_inpj_m.inpj002_desc,g_inpj_m.inpj003_desc,g_inpj_m.inpj012_desc, 
       g_inpj_m.inpjownid_desc,g_inpj_m.inpjowndp_desc,g_inpj_m.inpjcrtid_desc,g_inpj_m.inpjcrtdp_desc, 
       g_inpj_m.inpjmodid_desc,g_inpj_m.inpjcnfid_desc
   
   #檢查是否允許此動作
   IF NOT aint920_action_chk() THEN
      RETURN
   END IF
   
