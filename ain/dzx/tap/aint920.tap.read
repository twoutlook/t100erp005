  <point name="function.aint920_inpj016_chk" order="3" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 各選項都僅能選取其中一個，不可重複選擇，除了'7.無' 選項之外
# Memo...........:
# Usage..........: CALL aint920_inpj016_chk(p_order)
#                  RETURNING r_success
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By lixh
# Modify.........:
################################################################################
PRIVATE FUNCTION aint920_inpj016_chk(p_order)
DEFINE  p_order    LIKE inpa_t.inpa025
DEFINE  l_cnt      LIKE type_t.num5

   IF p_order = '7' THEN
      RETURN TRUE
   END IF
   SELECT COUNT(*) INTO l_cnt FROM inpj_t
    WHERE inpjent = g_enterprise
      AND inpjdocno = g_inpj_m.inpjdocno
      AND (inpj016 = p_order OR inpj017 = p_order
        OR inpj018 = p_order OR inpj019 = p_order 
        OR inpj020 = p_order OR inpj021 = p_order)
   IF cl_null(l_cnt) THEN LET l_cnt = 0 END IF
   IF l_cnt > 0 THEN 
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'ain-00250'
      LET g_errparam.extend = p_order
      LET g_errparam.popup = TRUE
      CALL cl_err()

      RETURN FALSE
   END IF
   RETURN TRUE
END FUNCTION]]>
  </point>
  <point name="function.aint920_inpj016_chk_a" order="4" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 重複性檢查
# Memo...........:
# Usage..........: CALL aint920_inpj016_chk_a(p_type,p_inpj_num)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By lixh
# Modify.........:
################################################################################
PRIVATE FUNCTION aint920_inpj016_chk_a(p_type,p_inpj_num)
DEFINE p_type       LIKE type_t.num5
DEFINE p_inpj_num   LIKE inpj_t.inpj025
DEFINE l_i          LIKE type_t.num5
DEFINE r_success    LIKE type_t.num5

   LET r_success = TRUE
   IF p_inpj_num = '7' THEN
      RETURN r_success
   END IF
   FOR l_i = 1 TO 6
      IF l_i = p_type THEN CONTINUE FOR END IF
      CASE l_i
         WHEN '1'
            IF p_inpj_num = g_inpj_m.inpj016 THEN LET r_success = FALSE END IF
         WHEN '2'
            IF p_inpj_num = g_inpj_m.inpj017 THEN LET r_success = FALSE END IF
         WHEN '3'
            IF p_inpj_num = g_inpj_m.inpj018 THEN LET r_success = FALSE END IF
         WHEN '4'
            IF p_inpj_num = g_inpj_m.inpj019 THEN LET r_success = FALSE END IF
         WHEN '5'
            IF p_inpj_num = g_inpj_m.inpj020 THEN LET r_success = FALSE END IF
         WHEN '6'
            IF p_inpj_num = g_inpj_m.inpj021 THEN LET r_success = FALSE END IF
      END CASE
      IF r_success = FALSE THEN EXIT FOR END IF      
   END FOR
   RETURN r_success
END FUNCTION]]>
  </point>
