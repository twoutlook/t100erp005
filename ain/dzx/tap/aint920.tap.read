                      " FROM inpj_t ",
                      " ",
                      " LEFT JOIN inpk_t ON inpkent = inpjent AND inpjsite = inpksite AND inpjdocno = inpkdocno ", "  ",
                      #add-point:browser_fill段sql(inpk_t1)
                      {<point name="browser_fill.cnt.join.}"/>}
                      #end add-point
                      " LEFT JOIN inpn_t ON inpnent = inpjent AND inpjsite = inpnsite AND inpjdocno = inpndocno", "  ",
                      #add-point:browser_fill段sql(inpn_t1)
                      {<point name="browser_fill.cnt.join.inpn_t1"/>}
                      #end add-point
 
 
 
                      " ", 
                      " ", 
                      " WHERE inpjent = '" ||g_enterprise|| "' AND inpkent = '" ||g_enterprise|| "' AND ",l_wc, " AND ", l_wc2, cl_sql_add_filter("inpj_t")
