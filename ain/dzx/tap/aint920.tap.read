         BEFORE CONSTRUCT
               DISPLAY aint920_filter_parser('inpjsite') TO s_browse[1].b_inpjsite
            DISPLAY aint920_filter_parser('inpjdocno') TO s_browse[1].b_inpjdocno
            DISPLAY aint920_filter_parser('inpjdocdt') TO s_browse[1].b_inpjdocdt
            DISPLAY aint920_filter_parser('inpj002') TO s_browse[1].b_inpj002
