   LET g_sql = " SELECT DISTINCT t0.inpjstus,t0.inpjsite,t0.inpjdocno,t0.inpjdocdt,t0.inpj002,t0.inpj003, 
       t1.ooag011 ,t2.ooefl003 ",
               " FROM inpj_t t0",
               "  ",
               "  LEFT JOIN inpk_t ON inpkent = inpjent AND inpjsite = inpksite AND inpjdocno = inpkdocno ", "  ", 
