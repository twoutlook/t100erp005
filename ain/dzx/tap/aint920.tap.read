   EXECUTE aint920_master_referesh USING g_inpj_m.inpjsite,g_inpj_m.inpjdocno INTO g_inpj_m.inpjdocno, 
       g_inpj_m.inpjdocdt,g_inpj_m.inpj002,g_inpj_m.inpjsite,g_inpj_m.inpj003,g_inpj_m.inpjstus,g_inpj_m.inpj011, 
