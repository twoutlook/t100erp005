  <section id="aint911_01.ui_dialog" ver="17" status="" src="s" readonly="">
    <![CDATA[#+ 功能選單 
PRIVATE FUNCTION aint911_01_ui_dialog()
   DEFINE li_idx   LIKE type_t.num10
   DEFINE la_param  RECORD #串查用
             prog   STRING,
             param  DYNAMIC ARRAY OF STRING
                    END RECORD
   DEFINE ls_js     STRING
