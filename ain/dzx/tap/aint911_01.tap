         CALL aint911_01_init()
      END IF
   
      CALL aint911_01_b_fill(g_wc2)
   
      DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
         DISPLAY ARRAY g_inbc_d TO s_detail1.* ATTRIBUTE(COUNT=g_detail_cnt) 
      
