  <section id="aini012.input" ver="3" status="" src="s">
<![CDATA[#+ 資料輸入
PRIVATE FUNCTION aini012_input(p_cmd)
   DEFINE  p_cmd                 LIKE type_t.chr1
   DEFINE  l_cmd_t               LIKE type_t.chr1
   DEFINE  l_cmd                 LIKE type_t.chr1
   DEFINE  l_ac_t                LIKE type_t.num5                #未取消的ARRAY CNT 
   DEFINE  l_n                   LIKE type_t.num5                #檢查重複用  
   DEFINE  l_cnt                 LIKE type_t.num5                #檢查重複用  
   DEFINE  l_lock_sw             LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert        LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete        LIKE type_t.num5                #可刪除否  
   DEFINE  l_count               LIKE type_t.num5
   DEFINE  l_i                   LIKE type_t.num5
   DEFINE  l_insert              BOOLEAN
   DEFINE  ls_return             STRING
   DEFINE  l_var_keys            DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys          DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                DYNAMIC ARRAY OF STRING
   DEFINE  l_fields              DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak        DYNAMIC ARRAY OF STRING
   DEFINE  lb_reproduce          BOOLEAN
   DEFINE  li_reproduce          LIKE type_t.num5
   DEFINE  li_reproduce_target   LIKE type_t.num5
   #add-point:input段define
   {<point name="input.define"/>}
   #end add-point    
   
   #先做狀態判定
   IF p_cmd = 'r' THEN
      LET l_cmd_t = 'r'
      LET p_cmd   = 'a'
   ELSE
      LET l_cmd_t = p_cmd
   END IF   
   
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF  
 
   CALL cl_set_head_visible("","YES")  
 
   LET l_insert = FALSE
   LET g_action_choice = ""
 
   #add-point:input段define_sql
   {<point name="input.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT inafstus,inaf003,inaf004,inaf005,inaf006,inaf007,inaf008,inaf009,inaf010, 
       inaf003,inafownid,inafowndp,inafcrtid,inafcrtdp,inafcrtdt,inafmodid,inafmoddt FROM inaf_t WHERE  
       inafent=? AND inafsite=? AND inaf001=? AND inaf002=? AND inaf003=? AND inaf004=? FOR UPDATE"
   #add-point:input段define_sql
   {<point name="input.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE aini012_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR
 
 
   
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   LET g_qryparam.state = 'i'
   
   #控制key欄位可否輸入
   CALL aini012_set_entry(p_cmd)
   #add-point:set_entry後
   {<point name="input.after_set_entry"/>}
   #end add-point
   CALL aini012_set_no_entry(p_cmd)
   #add-point:set_no_entry後
   {<point name="input.after_set_no_entry"/>}
   #end add-point
 
   DISPLAY BY NAME g_inaf_m.inafsite,g_inaf_m.inaf001,g_inaf_m.inaf002
   
   LET lb_reproduce = FALSE
   
   #add-point:進入修改段前
   {<point name="input.before_input"/>}
   #end add-point
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
]]>
</section>
  <section id="aini012.input.body" ver="6" status="" src="s">
<![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_inaf_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂單身ACTION
         
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_inaf_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL aini012_b_fill(g_wc2) 
            IF g_rec_b != 0 THEN
               CALL fgl_set_arr_curr(l_ac)
            END IF
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN aini012_cl USING g_enterprise,
                                               g_inaf_m.inafsite
                                               ,g_inaf_m.inaf001
                                               ,g_inaf_m.inaf002
 
                                               
               IF STATUS THEN
                  CALL cl_err("OPEN aini012_cl:", STATUS, 1)
                  CLOSE aini012_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_inaf_d[l_ac].inaf003 IS NOT NULL
               AND g_inaf_d[l_ac].inaf004 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_inaf_d_t.* = g_inaf_d[l_ac].*  #BACKUP
               LET g_inaf_d_o.* = g_inaf_d[l_ac].*  #BACKUP
               CALL aini012_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL aini012_set_no_entry_b(l_cmd)
               OPEN aini012_bcl USING g_enterprise,g_inaf_m.inafsite,
                                                g_inaf_m.inaf001,
                                                g_inaf_m.inaf002,
 
                                                g_inaf_d_t.inaf003
                                                ,g_inaf_d_t.inaf004
 
               IF STATUS THEN
                  CALL cl_err("OPEN aini012_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aini012_bcl INTO g_inaf_d[l_ac].inafstus,g_inaf_d[l_ac].inaf003,g_inaf_d[l_ac].inaf004, 
                      g_inaf_d[l_ac].inaf005,g_inaf_d[l_ac].inaf006,g_inaf_d[l_ac].inaf007,g_inaf_d[l_ac].inaf008, 
                      g_inaf_d[l_ac].inaf009,g_inaf_d[l_ac].inaf010,g_inaf2_d[l_ac].inaf003,g_inaf2_d[l_ac].inafownid, 
                      g_inaf2_d[l_ac].inafowndp,g_inaf2_d[l_ac].inafcrtid,g_inaf2_d[l_ac].inafcrtdp, 
                      g_inaf2_d[l_ac].inafcrtdt,g_inaf2_d[l_ac].inafmodid,g_inaf2_d[l_ac].inafmoddt
                  IF SQLCA.sqlcode THEN
                      CALL cl_err(g_inaf_d_t.inaf003,SQLCA.sqlcode,1)
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL aini012_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
        
         BEFORE INSERT
            
            INITIALIZE g_inaf_d_t.* TO NULL
            INITIALIZE g_inaf_d_o.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_inaf_d[l_ac].* TO NULL
            
            #公用欄位預設值
            #此段落由子樣板a14產生    
      LET g_inaf2_d[l_ac].inafownid = g_user
      LET g_inaf2_d[l_ac].inafowndp = g_dept
      LET g_inaf2_d[l_ac].inafcrtid = g_user
      LET g_inaf2_d[l_ac].inafcrtdp = g_dept 
      LET g_inaf2_d[l_ac].inafcrtdt = cl_get_current()
      LET g_inaf2_d[l_ac].inafmodid = ""
      LET g_inaf2_d[l_ac].inafmoddt = ""
      LET g_inaf_d[l_ac].inafstus = ""
 
  
            
            #一般欄位預設值
                  LET g_inaf_d[l_ac].inafstus = "Y"
      LET g_inaf_d[l_ac].inaf006 = "0"
      LET g_inaf_d[l_ac].inaf007 = "0"
      LET g_inaf_d[l_ac].inaf008 = "0"
      LET g_inaf_d[l_ac].inaf009 = "0"
      LET g_inaf_d[l_ac].inaf010 = "0"
 
            
            
            LET g_inaf_d_t.* = g_inaf_d[l_ac].*     #新輸入資料
            LET g_inaf_d_o.* = g_inaf_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL aini012_set_entry_b(l_cmd)
            #add-point:set_entry_b後
            {<point name="input.body.before_insert.set_entry_b"/>}
            #end add-point
            CALL aini012_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_inaf_d[li_reproduce_target].* = g_inaf_d[li_reproduce].*
               LET g_inaf2_d[li_reproduce_target].* = g_inaf2_d[li_reproduce].*
 
               LET g_inaf_d[g_inaf_d.getLength()].inaf003 = NULL
               LET g_inaf_d[g_inaf_d.getLength()].inaf004 = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
 
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM inaf_t 
             WHERE inafent = g_enterprise AND inafsite = g_inaf_m.inafsite
               AND inaf001 = g_inaf_m.inaf001
               AND inaf002 = g_inaf_m.inaf002
 
               AND inaf003 = g_inaf_d[l_ac].inaf003
               AND inaf004 = g_inaf_d[l_ac].inaf004
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               CALL s_transaction_begin()
               #add-point:單身新增前
               {<point name="input.body.b_insert" mark="Y"/>}
               #end add-point
               INSERT INTO inaf_t
                           (inafent,
                            inafsite,inaf001,inaf002,
                            inaf003,inaf004
                            ,inafstus,inaf005,inaf006,inaf007,inaf008,inaf009,inaf010,inafownid,inafowndp,inafcrtid,inafcrtdp,inafcrtdt,inafmodid,inafmoddt) 
                     VALUES(g_enterprise,
                            g_inaf_m.inafsite,g_inaf_m.inaf001,g_inaf_m.inaf002,
                            g_inaf_d[l_ac].inaf003,g_inaf_d[l_ac].inaf004
                            ,g_inaf_d[l_ac].inafstus,g_inaf_d[l_ac].inaf005,g_inaf_d[l_ac].inaf006,g_inaf_d[l_ac].inaf007, 
                                g_inaf_d[l_ac].inaf008,g_inaf_d[l_ac].inaf009,g_inaf_d[l_ac].inaf010, 
                                g_inaf2_d[l_ac].inafownid,g_inaf2_d[l_ac].inafowndp,g_inaf2_d[l_ac].inafcrtid, 
                                g_inaf2_d[l_ac].inafcrtdp,g_inaf2_d[l_ac].inafcrtdt,g_inaf2_d[l_ac].inafmodid, 
                                g_inaf2_d[l_ac].inafmoddt)
               #add-point:單身新增中
               {<point name="input.body.m_insert"/>}
               #end add-point
               LET p_cmd = 'u'
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_inaf_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("inaf_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR "INSERT O.K"
               LET g_rec_b=g_rec_b+1
               DISPLAY g_rec_b TO FORMONLY.cnt
            END IF
            
            #add-point:單身新增後
            {<point name="input.body.after_insert"/>}
            #end add-point
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_inaf_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_inaf_d.deleteElement(l_ac)
               NEXT FIELD inaf003
            END IF
            IF g_inaf_d_t.inaf003 IS NOT NULL
               AND g_inaf_d_t.inaf004 IS NOT NULL
 
               THEN
               
               #add-point:單身刪除前
