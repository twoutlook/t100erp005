  <section id="aini012.delete" ver="3" status="" src="s">
<![CDATA[#+ 資料刪除
PRIVATE FUNCTION aini012_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_inaf_m.inafsite IS NULL
   OR g_inaf_m.inaf001 IS NULL
   OR g_inaf_m.inaf002 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE aini012_master_referesh USING g_inaf_m.inafsite,g_inaf_m.inaf001,g_inaf_m.inaf002 INTO g_inaf_m.inafsite, 
       g_inaf_m.inaf001,g_inaf_m.inaf002
   
   CALL aini012_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN aini012_cl USING g_enterprise,g_inaf_m.inafsite
                                                       ,g_inaf_m.inaf001
                                                       ,g_inaf_m.inaf002
 
   IF STATUS THEN
      CALL cl_err("OPEN aini012_cl:", STATUS, 1)
      CLOSE aini012_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aini012_cl INTO g_inaf_m.inafsite,g_inaf_m.inaf001,g_inaf_m.inaf002,g_inaf_m.inaa002,g_inaf_m.inab003 

   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_inaf_m.inafsite,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL aini012_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM inaf_t WHERE inafent = g_enterprise AND inafsite = g_inaf_m.inafsite
                                                               AND inaf001 = g_inaf_m.inaf001
                                                               AND inaf002 = g_inaf_m.inaf002
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("inaf_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_inaf_d.clear() 
      CALL g_inaf2_d.clear()       
 
     
      CALL aini012_ui_browser_refresh()  
      CALL aini012_ui_headershow()  
      CALL aini012_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL aini012_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL aini012_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE aini012_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_inaf_m.inafsite,'D')
    
END FUNCTION
]]>
