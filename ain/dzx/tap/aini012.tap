<![CDATA[################################################################################
# Date & Author..: 2014/03/16 By chenjing
# Modify.........:
################################################################################
PRIVATE FUNCTION aini012_inaf003_ref()
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_inaf_d[l_ac].inaf003
   CALL ap_ref_array2(g_ref_fields,"SELECT imaal003,imaal004 FROM imaal_t WHERE imaalent='"||g_enterprise||"' AND imaal001=? AND imaal002='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_inaf_d[l_ac].inaf003_desc = '', g_rtn_fields[1] , ''
   LET g_inaf_d[l_ac].inaf003_desc_desc = '', g_rtn_fields[2] , ''
   DISPLAY BY NAME g_inaf_d[l_ac].inaf003_desc
   DISPLAY BY NAME g_inaf_d[l_ac].inaf003_desc_desc
   IF NOT cl_null(g_inaf_d[l_ac].inaf003) AND (g_inaf_d_t.inaf003 IS NULL OR g_inaf_d[l_ac].inaf003 != g_inaf_d_t.inaf003) THEN
      SELECT imaf053 INTO g_inaf_d[l_ac].inaf005 FROM imaf_t
       WHERE imafent = g_enterprise
         AND imafsite = g_site
         AND imaf001 = g_inaf_d[l_ac].inaf003
       CALL aini012_inaf005_ref()  
   END IF         
END FUNCTION]]>
</point>
