          g_inaf_d[l_ac].inaf010,g_inaf2_d[l_ac].inaf003,g_inaf2_d[l_ac].inafownid,g_inaf2_d[l_ac].inafowndp, 
          g_inaf2_d[l_ac].inafcrtid,g_inaf2_d[l_ac].inafcrtdp,g_inaf2_d[l_ac].inafcrtdt,g_inaf2_d[l_ac].inafmodid, 
          g_inaf2_d[l_ac].inafmoddt,g_inaf_d[l_ac].inaf003_desc,g_inaf_d[l_ac].inaf003_desc_desc,g_inaf_d[l_ac].inaf005_desc, 
          g_inaf2_d[l_ac].inafownid_desc,g_inaf2_d[l_ac].inafowndp_desc,g_inaf2_d[l_ac].inafcrtid_desc, 
          g_inaf2_d[l_ac].inafcrtdp_desc,g_inaf2_d[l_ac].inafmodid_desc
                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_inaf2_d[l_ac].inafownid_desc = cl_get_username(g_inaf2_d[l_ac].inafownid)
      #LET g_inaf2_d[l_ac].inafowndp_desc = cl_get_deptname(g_inaf2_d[l_ac].inafowndp)
      #LET g_inaf2_d[l_ac].inafcrtid_desc = cl_get_username(g_inaf2_d[l_ac].inafcrtid)
      #LET g_inaf2_d[l_ac].inafcrtdp_desc = cl_get_deptname(g_inaf2_d[l_ac].inafcrtdp)
      #LET g_inaf2_d[l_ac].inafmodid_desc = cl_get_username(g_inaf2_d[l_ac].inafmodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_inaf_d.deleteElement(g_inaf_d.getLength())
      CALL g_inaf2_d.deleteElement(g_inaf2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
