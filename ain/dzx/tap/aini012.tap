  <section id="aini012.detail_reproduce" ver="3" status="" src="s">
<![CDATA[#+ 單身自動複製
PRIVATE FUNCTION aini012_detail_reproduce()
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE inaf_t.*
 
 
   #add-point:delete段define
   {<point name="detail_reproduce.define"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE aini012_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE aini012_detail AS ",
                "SELECT * FROM inaf_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO aini012_detail SELECT * FROM inaf_t 
                                         WHERE inafent = g_enterprise AND inafsite = g_inafsite_t
                                         AND inaf001 = g_inaf001_t
                                         AND inaf002 = g_inaf002_t
 
   
   #將key修正為調整後   
   UPDATE aini012_detail 
      #更新key欄位
      SET inafsite = g_inaf_m.inafsite
          , inaf001 = g_inaf_m.inaf001
          , inaf002 = g_inaf_m.inaf002
 
      #更新共用欄位
      #此段落由子樣板a13產生
       , inafownid = g_user
       , inafowndp = g_dept
       , inafcrtid = g_user
       , inafcrtdp = g_dept 
       , inafcrtdt = ld_date
       , inafmodid = "" 
       , inafmoddt = "" 
      #, inafstus = "Y"
 
 
                                       
  
   #將資料塞回原table   
   INSERT INTO inaf_t SELECT * FROM aini012_detail
   
   IF SQLCA.sqlcode THEN
      CALL cl_err("reproduce",SQLCA.sqlcode,1)
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE aini012_detail
   
   #add-point:單身複製後1
   {<point name="detail_reproduce.body.table1.a_insert"/>}
   #end add-point
 
 
   
 
   
   #多語言複製段落
   
   
   CALL s_transaction_end('Y','0')
   
   #已新增完, 調整資料內容(修改時使用)
   LET g_inafsite_t = g_inaf_m.inafsite
   LET g_inaf001_t = g_inaf_m.inaf001
   LET g_inaf002_t = g_inaf_m.inaf002
 
   
   DROP TABLE aini012_detail
   
END FUNCTION
]]>
</section>
  <section id="aini012.fetch" ver="5" status="" src="s">
<![CDATA[#+ 指定PK後抓取單頭其他資料
PRIVATE FUNCTION aini012_fetch(p_flag)
   DEFINE p_flag     LIKE type_t.chr1
   DEFINE ls_msg     STRING
   #add-point:fetch段define
   {<point name="fetch.define"/>}
   #end add-point    
   
   #add-point:fetch段動作開始前
   {<point name="fetch.before_fetch"/>}
   #end add-point    
 
   CASE p_flag
      WHEN 'F' LET g_current_idx = 1
      WHEN 'L' LET g_current_idx = g_header_cnt        
      WHEN 'P'
         IF g_current_idx > 1 THEN               
            LET g_current_idx = g_current_idx - 1
         END IF 
      WHEN 'N'
         IF g_current_idx < g_header_cnt THEN
            LET g_current_idx =  g_current_idx + 1
         END IF        
      WHEN '/'
         IF (NOT g_no_ask) THEN    
            CALL cl_set_act_visible("accept,cancel", TRUE)    
            CALL cl_getmsg('fetch',g_lang) RETURNING ls_msg
            LET INT_FLAG = 0
 
            PROMPT ls_msg CLIPPED,': ' FOR g_jump
               #交談指令共用ACTION
               &include "common_action.4gl" 
            END PROMPT
 
            CALL cl_set_act_visible("accept,cancel", FALSE)    
            IF INT_FLAG THEN
               LET INT_FLAG = 0
               EXIT CASE  
            END IF
            
         END IF
         
         IF g_jump > 0 AND g_jump <= g_browser.getLength() THEN
            LET g_current_idx = g_jump
         END IF
 
         LET g_no_ask = FALSE  
   END CASE    
   
   #若無資料則離開
   IF g_current_idx = 0 THEN
      RETURN
   END IF
   
   
   CALL g_curr_diag.setCurrentRow("s_browse", g_current_idx) #設定browse 索引
   LET g_detail_cnt = g_header_cnt                  
   
   #單身筆數顯示
   DISPLAY g_detail_cnt TO FORMONLY.cnt                      #設定page 總筆數 
   #LET g_detail_idx = 1
   IF g_detail_cnt > 0 THEN
      #LET g_detail_idx = 1
      DISPLAY g_detail_idx TO FORMONLY.idx  
   ELSE
      LET g_detail_idx = 0
      DISPLAY ' ' TO FORMONLY.idx    
   END IF
   
   #瀏覽頁筆數顯示
   LET g_browser_idx = g_pagestart+g_current_idx-1
   DISPLAY g_browser_idx TO FORMONLY.b_index   #當下筆數
   DISPLAY g_browser_cnt TO FORMONLY.b_count   #總筆數
   DISPLAY g_browser_idx TO FORMONLY.h_index   #當下筆數
   DISPLAY g_browser_cnt TO FORMONLY.h_count   #總筆數
   
   CALL cl_navigator_setting( g_current_idx, g_detail_cnt )
   
   #代表沒有資料
   IF g_current_idx = 0 OR g_browser.getLength() = 0 THEN
      RETURN
   END IF
   
   #超出範圍
   IF g_current_idx > g_browser.getLength() THEN
      LET g_current_idx = g_browser.getLength()
   END IF
   
   LET g_inaf_m.inafsite = g_browser[g_current_idx].b_inafsite
   LET g_inaf_m.inaf001 = g_browser[g_current_idx].b_inaf001
   LET g_inaf_m.inaf002 = g_browser[g_current_idx].b_inaf002
 
   
   #重讀DB,因TEMP有不被更新特性
   EXECUTE aini012_master_referesh USING g_inaf_m.inafsite,g_inaf_m.inaf001,g_inaf_m.inaf002 INTO g_inaf_m.inafsite, 
       g_inaf_m.inaf001,g_inaf_m.inaf002
   IF SQLCA.sqlcode THEN
      CALL cl_err("inaf_t",SQLCA.sqlcode,1)
      INITIALIZE g_inaf_m.* TO NULL
      RETURN
   END IF
   
   #LET g_data_owner =       
   #LET g_data_group =   
   
   #保存單頭舊值
   LET g_inaf_m_t.* = g_inaf_m.*
   LET g_inaf_m_o.* = g_inaf_m.*
   
   #重新顯示   
   CALL aini012_show()
 
   
 
END FUNCTION
]]>
</section>
