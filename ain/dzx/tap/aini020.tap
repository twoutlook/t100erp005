      END FOREACH  
   END IF
   IF g_current_page = 2 THEN 
      LET l_ac = 1     
      LET g_sql = "SELECT DISTINCT inag004,inaa002,inag005,inab003,inag006,inag003,inag007,inag017,inag016,",
                  "              inag015,inag014,inag026,inag012,inag011,inag010,inaa015,inag019,inag020 ",
