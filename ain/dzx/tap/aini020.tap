<![CDATA[#此段落由子樣板a06產生
PRIVATE FUNCTION aini020_inag_t(ps_type)
 

   DEFINE li_cnt    LIKE type_t.num10
   DEFINE ps_type   STRING  
   DEFINE ls_sql    STRING  

   IF ps_type = "s" THEN 
            LET ls_sql = 'SELECT  FROM inag_t WHERE inagent= ? AND inagsite= ? AND inagsite=? AND inag001=? AND inag002=? AND inag003=? AND inag004=? AND inag005=? AND inag006=? AND inag007=? ' 
      DECLARE inag_t_cl CURSOR FROM ls_sql 
      OPEN inag_t_cl USING g_enterprise,g_site,.inae001,.inaesite,.inae002,.inae003,.inae004
      FETCH inag_t_cl INTO . 

      IF SQLCA.sqlcode THEN
                  LET .  = NULL 

      END IF 
      RETURN 
   END IF 

      SELECT COUNT(*) INTO li_cnt FROM inag_t WHERE inagent = g_enterprise AND inagsite = g_site AND inagsite = g_inae001_t AND inag001 = g_inaesite_t AND inag002 = g_inae002_t AND inag003 = g_inae003_t AND inag004 = g_inae004_t AND inag005 = g__t AND inag006 = g__t AND inag007 = g__t
   
   IF li_cnt = 0 AND ps_type = "u" THEN 
            INSERT INTO inag_t 
      (inagent,inagsite,inagsite,inag001,inag002,inag003,inag004,inag005,inag006,inag007, )
      VALUES (g_enterprise,g_site,.inae001,.inaesite,.inae002,.inae003,.inae004,. )
   END IF 

   IF li_cnt > 0 AND ps_type = "u" THEN 
            UPDATE inag_t SET 
      (inagsite,inag001,inag002,inag003,inag004,inag005,inag006,inag007, ) = 
      (.inae001,.inaesite,.inae002,.inae003,.inae004,. ) 
      WHERE inagent = g_enterprise AND inagsite = g_site AND inagsite = g_inae001_t AND inag001 = g_inaesite_t AND inag002 = g_inae002_t AND inag003 = g_inae003_t AND inag004 = g_inae004_t AND inag005 = g__t AND inag006 = g__t AND inag007 = g__t

   END IF 

   IF li_cnt > 0 AND ps_type = "d" THEN 
            DELETE FROM inag_t
      WHERE inagent = g_enterprise AND inagsite = g_site AND inagsite = g_inae001_t AND inag001 = g_inaesite_t AND inag002 = g_inae002_t AND inag003 = g_inae003_t AND inag004 = g_inae004_t AND inag005 = g__t AND inag006 = g__t AND inag007 = g__t
   END IF 

   IF SQLCA.sqlcode THEN
      CALL cl_err("",SQLCA.sqlcode,0)
   END IF 

END FUNCTION]]>
</point>
