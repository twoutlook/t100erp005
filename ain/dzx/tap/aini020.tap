  <point name="function.aini020_b_fill" cite_std="N" status="d" ver="1" src="s" new="Y" order="4" mark_hard="N">
<![CDATA[
PRIVATE FUNCTION aini020_b_fill()
   {<Local define>}
   DEFINE ls_wc           STRING
   {</Local define>}
   #add-point:b_fill段define

   #end add-point

   #add-point:b_fill段sql_before

   #end add-point

   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF

   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter

   LET g_sql = "SELECT  UNIQUE '','','','','' FROM inad_t",

               " LEFT JOIN inae_t ON inaeent = inadent AND inaesite = inadsite AND inad001 = inae001 AND inad002 = inae002 AND inad003 = inae003 AND  = inae004",

               " LEFT JOIN inag_t ON inagent = inadent AND inagsite = inadsite AND inad001 = inag001 AND inad002 = inag002 AND inad003 = inag003 AND  = inag004 AND  = inag005 AND  = inag006 AND  = inag007",

               " LEFT JOIN inaj_t ON inajent = inadent AND inajsite = inadsite AND inad001 = inaj001 AND inad002 = inaj002 AND inad003 = inaj003 AND  = inaj004",


               "",
               " WHERE inadent= ? AND inadsite= ? AND 1=1 AND ", ls_wc

   LET g_sql = g_sql, " ORDER BY inad_t.inad001,inad_t.inad002,inad_t.inad003"

   #add-point:b_fill段sql_after

   #end add-point

   PREPARE aini020_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aini020_pb

   OPEN b_fill_curs USING g_enterprise, g_site

   CALL g_inad_d.clear()

   #add-point:陣列清空

   #end add-point

   LET g_cnt = l_ac
   LET l_ac = 1
#  ERROR "Searching!"

   FOREACH b_fill_curs INTO g_inad_d[l_ac].inad001,g_inad_d[l_ac].inad002,g_inad_d[l_ac].inad003,g_inad_d[l_ac].inad011,
       g_inad_d[l_ac].inad012
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF

      LET g_inad_d[l_ac].sel = "N"
      #LET g_inad_d[l_ac].statepic = cl_get_actipic(g_inad_d[l_ac].statepic)



      #add-point:b_fill段資料填充

      #end add-point

      CALL aini020_detail_show("'1'")

      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            CALL cl_err( "", 9035, 1 )
         END IF
         EXIT FOREACH
      END IF

   END FOREACH
   LET g_error_show = 0

   CALL g_inad_d.deleteElement(g_inad_d.getLength())
   #add-point:陣列長度調整

   #end add-point

   #add-point:b_fill段資料填充(其他單身)

   #end add-point

   LET g_detail_cnt = l_ac - 1
   DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET g_detail_idx = 1
   DISPLAY g_detail_idx TO FORMONLY.h_index
   LET l_ac = g_cnt
   LET g_cnt = 0

   CLOSE b_fill_curs
   FREE aini020_pb

   LET l_ac = 1
   CALL aini020_fetch()



END FUNCTION]]>
</point>
  <point name="function.aini020_fetch" cite_std="N" status="d" ver="1" src="s" new="Y" order="5" mark_hard="N">
<![CDATA[
PRIVATE FUNCTION aini020_fetch()
   {<Local define>}
   DEFINE li_ac           LIKE type_t.num5
   {</Local define>}
   #add-point:fetch段define

   #end add-point

   CALL g_inad2_d.clear()

   CALL g_inad3_d.clear()

   CALL g_inad4_d.clear()


   #add-point:陣列清空

   #end add-point

   LET li_ac = l_ac

   LET g_sql = "SELECT  UNIQUE '','','','','','' FROM inae_t",
               "",
               " WHERE"

   IF NOT cl_null(g_wc2_table2) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
   END IF

   LET g_sql = g_sql, " ORDER BY inae_t.inae001,inae_t.inae002,inae_t.inae003,inae_t.inae004"

   #add-point:單身填充前

   #end add-point

   PREPARE aini020_pb2 FROM g_sql
   DECLARE b_fill_curs2 CURSOR FOR aini020_pb2

   OPEN b_fill_curs2 USING g_enterprise, g_site,g_inad_d[g_detail_idx].inad001
                                  ,g_inad_d[g_detail_idx].inad002

                                  ,g_inad_d[g_detail_idx].inad003


   LET l_ac = 1
   FOREACH b_fill_curs2 INTO g_inad2_d[l_ac].inae001,g_inad2_d[l_ac].inae002,g_inad2_d[l_ac].inae003,
       g_inad2_d[l_ac].inae004,g_inad2_d[l_ac].inae011,g_inad2_d[l_ac].inae030
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF



      #add-point:b_fill段資料填充

      #end add-point

      CALL aini020_detail_show("'2'")

      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         CALL cl_err( "", 9035, 1 )
         EXIT FOREACH
      END IF

   END FOREACH

   LET g_sql = "SELECT  UNIQUE '','','','','','','','','','','','','','','','' FROM inag_t",
               "",
               " WHERE"

   IF NOT cl_null(g_wc2_table3) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table3 CLIPPED
   END IF

   LET g_sql = g_sql, " ORDER BY inag_t.inag001,inag_t.inag002,inag_t.inag003,inag_t.inag004,inag_t.inag005,inag_t.inag006,inag_t.inag007"

   #add-point:單身填充前

   #end add-point

   PREPARE aini020_pb3 FROM g_sql
   DECLARE b_fill_curs3 CURSOR FOR aini020_pb3

   OPEN b_fill_curs3 USING g_enterprise, g_site,g_inad_d[g_detail_idx].inad001
                                  ,g_inad_d[g_detail_idx].inad002

                                  ,g_inad_d[g_detail_idx].inad003


   LET l_ac = 1
   FOREACH b_fill_curs3 INTO g_inad3_d[l_ac].inag004,g_inad3_d[l_ac].inag005,g_inad3_d[l_ac].inag006,
       g_inad3_d[l_ac].inag003,g_inad3_d[l_ac].inag007,g_inad3_d[l_ac].inag009,g_inad3_d[l_ac].inag017,
       g_inad3_d[l_ac].inag016,g_inad3_d[l_ac].inag015,g_inad3_d[l_ac].inag014,g_inad3_d[l_ac].inag026,
       g_inad3_d[l_ac].inag012,g_inad3_d[l_ac].inag011,g_inad3_d[l_ac].inag010,g_inad3_d[l_ac].inag019,
       g_inad3_d[l_ac].inag020
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF



      #add-point:b_fill段資料填充

      #end add-point

      CALL aini020_detail_show("'3'")

      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         CALL cl_err( "", 9035, 1 )
         EXIT FOREACH
      END IF

   END FOREACH

   LET g_sql = "SELECT  UNIQUE '','','','','','','','','','','' FROM inaj_t",
               "",
               " WHERE"

   IF NOT cl_null(g_wc2_table4) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table4 CLIPPED
   END IF

   LET g_sql = g_sql, " ORDER BY inaj_t.inaj001,inaj_t.inaj002,inaj_t.inaj003,inaj_t.inaj004"

   #add-point:單身填充前

   #end add-point

   PREPARE aini020_pb4 FROM g_sql
   DECLARE b_fill_curs4 CURSOR FOR aini020_pb4

   OPEN b_fill_curs4 USING g_enterprise, g_site,g_inad_d[g_detail_idx].inad001
                                  ,g_inad_d[g_detail_idx].inad002

                                  ,g_inad_d[g_detail_idx].inad003


   LET l_ac = 1
   FOREACH b_fill_curs4 INTO g_inad4_d[l_ac].inaj035,g_inad4_d[l_ac].inaj004,g_inad4_d[l_ac].inaj001,
       g_inad4_d[l_ac].inaj022,g_inad4_d[l_ac].inaj018,g_inad4_d[l_ac].inaj017,g_inad4_d[l_ac].inaj002,
       g_inad4_d[l_ac].inaj003,g_inad4_d[l_ac].inaj012,g_inad4_d[l_ac].inaj011,g_inad4_d[l_ac].inaj016

      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF



      #add-point:b_fill段資料填充

      #end add-point

      CALL aini020_detail_show("'4'")

      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         CALL cl_err( "", 9035, 1 )
         EXIT FOREACH
      END IF

   END FOREACH



   #add-point:單身填充後

   #end add-point

   CALL g_inad2_d.deleteElement(g_inad2_d.getLength())
   #單身總筆數顯示
   LET g_detail_cnt2 = g_inad2_d.getLength()
