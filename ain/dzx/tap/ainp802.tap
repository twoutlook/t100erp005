  <section id="ainp802.modify" ver="1" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION ainp802_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point
   
   IF g_inea_m.ineadocno IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF 
   
    SELECT UNIQUE ineaunit,ineasite,ineadocno,inea002
 INTO g_inea_m.ineaunit,g_inea_m.ineasite,g_inea_m.ineadocno,g_inea_m.inea002
 FROM inea_t
 WHERE ineaent = g_enterprise AND ineadocno = g_inea_m.ineadocno
 
   ERROR ""
  
   LET g_ineadocno_t = g_inea_m.ineadocno
 
   
   CALL s_transaction_begin()
   
   OPEN ainp802_cl USING g_enterprise,g_inea_m.ineadocno
   IF STATUS THEN
      CALL cl_err("OPEN ainp802_cl:", STATUS, 1)
      CLOSE ainp802_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH ainp802_cl INTO g_inea_m.ineaunit,g_inea_m.ineasite,g_inea_m.ineadocno,g_inea_m.inea002,g_inea_m.checkbox1, 
       g_inea_m.checkbox2
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err("inea_t",SQLCA.sqlcode,0)
      CLOSE ainp802_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
 
   CALL ainp802_show()
   
   WHILE TRUE
      LET g_inea_m.ineadocno = g_ineadocno_t
 
      
      #寫入修改者/修改日期資訊
      
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
 
      CALL ainp802_input("u")     #欄位更改
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_inea_m.* = g_inea_m_t.*
         CALL ainp802_show()
         CALL cl_err("",9001,0)
         EXIT WHILE
      END IF
 
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_inea_m.ineadocno,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE ainp802_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_inea_m.ineadocno,"U")
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
