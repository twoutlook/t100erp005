  <point name="function.aint700_inag_t" order="1" ver="1" cite_std="N" new="Y" status="d" src="s" readonly="" mark_hard="N">
    <![CDATA[#應用 a06 樣板自動產生(Version:2)
#次要單頭table相關處理
PRIVATE FUNCTION aint700_inag_t(ps_type)
 
   DEFINE li_cnt    LIKE type_t.num10
   DEFINE ps_type   STRING  
   DEFINE ls_sql    STRING  
 
   IF ps_type = "s" THEN 
            LET ls_sql = 'SELECT  FROM inag_t WHERE inagent= ? AND inagsite= ? AND inagsite=? AND inag001=? AND inag002=? AND inag003=? AND inag004=? AND inag005=? AND inag006=? AND inag007=? ' 
      LET ls_sql = cl_sql_add_mask(ls_sql) 
      DECLARE inag_t_cl CURSOR FROM ls_sql 
      OPEN inag_t_cl USING g_enterprise,g_site,g_indi_m.indidocno
      FETCH inag_t_cl INTO g_indi_m. 

      IF SQLCA.sqlcode THEN
                  LET g_indi_m.  = NULL 

      END IF 
      RETURN 
   END IF 
 
      SELECT COUNT(*) INTO li_cnt FROM inag_t WHERE inagent = g_enterprise AND inagsite = g_site AND inagsite = g_indidocno_t AND inag001 = g__t AND inag002 = g__t AND inag003 = g__t AND inag004 = g__t AND inag005 = g__t AND inag006 = g__t AND inag007 = g__t
   
   IF li_cnt = 0 AND ps_type = "u" THEN 
            INSERT INTO inag_t 
      (inagent,inagsite,inagsite,inag001,inag002,inag003,inag004,inag005,inag006,inag007, )
      VALUES (g_enterprise,g_site,g_indi_m.indidocno,g_indi_m. )
   END IF 
 
   IF li_cnt > 0 AND ps_type = "u" THEN 
            UPDATE inag_t SET 
      (inagsite,inag001,inag002,inag003,inag004,inag005,inag006,inag007, ) = 
      (g_indi_m.indidocno,g_indi_m. ) 
      WHERE inagent = g_enterprise AND inagsite = g_site AND inagsite = g_indidocno_t AND inag001 = g__t AND inag002 = g__t AND inag003 = g__t AND inag004 = g__t AND inag005 = g__t AND inag006 = g__t AND inag007 = g__t

   END IF 
 
   IF li_cnt > 0 AND ps_type = "d" THEN 
            DELETE FROM inag_t
      WHERE inagent = g_enterprise AND inagsite = g_site AND inagsite = g_indidocno_t AND inag001 = g__t AND inag002 = g__t AND inag003 = g__t AND inag004 = g__t AND inag005 = g__t AND inag006 = g__t AND inag007 = g__t
   END IF 
 
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "aint700" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   END IF 
 
END FUNCTION]]>
  </point>
  <point name="function.aint700_carry_pmdb" order="1" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#帶出要貨單資訊
PUBLIC FUNCTION aint700_carry_pmdb()
DEFINE l_bgjob      LIKE type_t.chr1
DEFINE l_inaa010    LIKE inaa_t.inaa010

   SELECT pmdbsite,pmdb004,pmdb005,pmdb201,
          pmdb212, pmdb007,pmdb006,pmdb204,
          pmdb037, pmdb038,pmdb039
     INTO g_indj_d[l_ac].indj003, g_indj_d[l_ac].indj004,g_indj_d[l_ac].indj005,g_indj_d[l_ac].indj006,
          g_indj_d[l_ac].indj007, g_indj_d[l_ac].indj008,g_indj_d[l_ac].indj009,g_indj_d[l_ac].indj012,
          g_indj_d[l_ac].indjunit,g_indj_d[l_ac].indj015,g_indj_d[l_ac].indj016 
     FROM pmdb_t
    WHERE pmdbent = g_enterprise
      AND pmdbdocno = g_indj_d[l_ac].indj001
      AND pmdbseq = g_indj_d[l_ac].indj002
      
   LET g_errshow = 0
          

   IF NOT cl_null(g_indj_d[l_ac].indj012) THEN
      INITIALIZE g_chkparam.* TO NULL
      LET g_chkparam.arg1 = g_indj_d[l_ac].indjsite
      LET g_chkparam.arg2 = g_indj_d[l_ac].indj012
      IF NOT cl_chk_exist("v_inaa001_18") THEN
         LET g_indj_d[l_ac].indj012 = ''
      END IF
   END IF
   
   IF cl_null(g_indj_d[l_ac].indj012) THEN
      IF cl_null(g_indj_d[l_ac].indj015) THEN      
         SELECT ooef124 INTO g_indj_d[l_ac].indj012 FROM ooef_t
          WHERE ooefent = g_enterprise
            AND ooef001 = g_indj_d[l_ac].indjsite
      ELSE
         #依收貨庫位為成本或非成本 來決定要預帶的倉庫為何
         LET l_inaa010 = ''
         SELECT inaa010 INTO l_inaa010 FROM inaa_t
          WHERE inaaent = g_enterprise
            AND inaasite = g_indj_d[l_ac].indjunit
            AND inaa001 = g_indj_d[l_ac].indj015
         IF l_inaa010 = 'Y' THEN
            SELECT ooef124 INTO g_indj_d[l_ac].indj012 FROM ooef_t
             WHERE ooefent = g_enterprise
               AND ooef001 = g_indj_d[l_ac].indjsite
         ELSE
            SELECT ooef127 INTO g_indj_d[l_ac].indj012 FROM ooef_t
             WHERE ooefent = g_enterprise
               AND ooef001 = g_indj_d[l_ac].indjsite
         END IF       
      END IF
      
      IF NOT cl_null(g_indj_d[l_ac].indj012) THEN
         INITIALIZE g_chkparam.* TO NULL
         LET g_chkparam.arg1 = g_indj_d[l_ac].indjsite
         LET g_chkparam.arg2 = g_indj_d[l_ac].indj012
         IF NOT cl_chk_exist("v_inaa001_18") THEN
            LET g_indj_d[l_ac].indj012 = ''
         END IF
      END IF
   END IF   
   
   IF NOT cl_null(g_indj_d[l_ac].indj015) THEN
      IF g_indj_d[l_ac].indj016 IS NULL THEN
         INITIALIZE g_chkparam.* TO NULL
         LET g_chkparam.arg1 = g_indj_d[l_ac].indjunit
         LET g_chkparam.arg2 = g_indj_d[l_ac].indj015
         IF NOT cl_chk_exist("v_inaa001_18") THEN
            LET g_indj_d[l_ac].indj015 = ''
         END IF
      ELSE
         INITIALIZE g_chkparam.* TO NULL
         LET g_chkparam.arg1 = g_indj_d[l_ac].indjunit
         LET g_chkparam.arg2 = g_indj_d[l_ac].indj015
         LET g_chkparam.arg2 = g_indj_d[l_ac].indj016
         IF NOT cl_chk_exist("v_inab002_4") THEN
            LET g_indj_d[l_ac].indj015 = ''
            LET g_indj_d[l_ac].indj016 = ''
         END IF
      END IF
   END IF
   LET l_bgjob = g_bgjob
   LET g_bgjob = 'Y' 
   IF NOT aint700_01_chk_stock( g_indj_d[l_ac].indjsite,g_indj_d[l_ac].indj012,
                                g_indj_d[l_ac].indjunit,g_indj_d[l_ac].indj015) THEN
      LET g_indj_d[l_ac].indj012 = ''                      
   END IF
   
   LET g_indj_d[l_ac].indj010 = g_indj_d[l_ac].indj009
   LET g_indj_d[l_ac].indj013 = ''
   LET g_indj_d[l_ac].indj014 = ''  
   LET g_indj_d[l_ac].indj019 = ''   
   LET g_errshow = 1
   LET g_bgjob = l_bgjob
   
   CALL s_desc_get_department_desc(g_indj_d[l_ac].indj003)
    RETURNING g_indj_d[l_ac].indj003_desc

   CALL s_desc_get_item_desc(g_indj_d[l_ac].indj004)
    RETURNING g_indj_d[l_ac].indj004_desc,g_indj_d[l_ac].indj004_desc_desc

   CALL s_desc_get_unit_desc(g_indj_d[l_ac].indj006)
    RETURNING g_indj_d[l_ac].indj006_desc

   CALL s_desc_get_unit_desc(g_indj_d[l_ac].indj008)
    RETURNING g_indj_d[l_ac].indj008_desc

   CALL s_desc_get_department_desc(g_indj_d[l_ac].indjsite)
    RETURNING g_indj_d[l_ac].indjsite_desc
   
   CALL s_desc_get_stock_desc(g_indj_d[l_ac].indjsite,g_indj_d[l_ac].indj012)
    RETURNING g_indj_d[l_ac].indj012_desc

   CALL s_desc_get_department_desc(g_indj_d[l_ac].indjunit)
    RETURNING g_indj_d[l_ac].indjunit_desc

   CALL s_desc_get_stock_desc(g_indj_d[l_ac].indjunit,g_indj_d[l_ac].indj015)
    RETURNING g_indj_d[l_ac].indj015_desc

   CALL s_desc_get_locator_desc(g_indj_d[l_ac].indjunit,g_indj_d[l_ac].indj015,g_indj_d[l_ac].indj016)
    RETURNING g_indj_d[l_ac].indj016_desc
   
END FUNCTION]]>
