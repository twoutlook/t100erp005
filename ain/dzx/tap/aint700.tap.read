  <section id="aint700.update_b" ver="7" status="" src="s" readonly="">
    <![CDATA[#+ 修改單身後其他table連動
PRIVATE FUNCTION aint700_update_b(ps_table,ps_keys,ps_keys_bak,ps_page)
   DEFINE ps_table         STRING
   DEFINE ps_page          STRING
   DEFINE ps_keys          DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ps_keys_bak      DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ls_group         STRING
   DEFINE li_idx           LIKE type_t.num10 
   DEFINE lb_chk           BOOLEAN
   DEFINE l_new_key        DYNAMIC ARRAY OF STRING
   DEFINE l_old_key        DYNAMIC ARRAY OF STRING
   DEFINE l_field_key      DYNAMIC ARRAY OF STRING
   #add-point:update_b段define
   {<point name="update_b.define" edit="s"/>}
   #end add-point   
   #add-point:update_b段define(客製用)
   {<point name="update_b.define_customerization" edit="c"/>}
   #end add-point   
   
   LET g_update = TRUE   
   
   #判斷key是否有改變
   LET lb_chk = TRUE
   FOR li_idx = 1 TO ps_keys.getLength()
      IF ps_keys[li_idx] <> ps_keys_bak[li_idx] THEN
         LET lb_chk = FALSE
         EXIT FOR
      END IF
   END FOR
   
   #不需要做處理
   IF lb_chk THEN
      RETURN
   END IF
   
   #判斷是否是同一群組的table
   LET ls_group = "'1',"
   IF ls_group.getIndexOf(ps_page,1) > 0 AND ps_table <> "indj_t" THEN
      #add-point:update_b段修改前
      {<point name="update_b.before_update" mark="Y"/>}
      #end add-point 
      
      #將遮罩欄位還原
      CALL aint700_indj_t_mask_restore('restore_mask_o')
               
      UPDATE indj_t 
         SET (indjdocno,
              indjseq
              ,indj001,indj002,indj003,indj004,indj005,indj006,indj007,indj008,indj009,indj010,indj011,indj018,indjsite,indj012,indj013,indj014,indj019,indjunit,indj015,indj016,indj017) 
              = 
             (ps_keys[1],ps_keys[2]
              ,g_indj_d[g_detail_idx].indj001,g_indj_d[g_detail_idx].indj002,g_indj_d[g_detail_idx].indj003, 
                  g_indj_d[g_detail_idx].indj004,g_indj_d[g_detail_idx].indj005,g_indj_d[g_detail_idx].indj006, 
                  g_indj_d[g_detail_idx].indj007,g_indj_d[g_detail_idx].indj008,g_indj_d[g_detail_idx].indj009, 
                  g_indj_d[g_detail_idx].indj010,g_indj_d[g_detail_idx].indj011,g_indj_d[g_detail_idx].indj018, 
                  g_indj_d[g_detail_idx].indjsite,g_indj_d[g_detail_idx].indj012,g_indj_d[g_detail_idx].indj013, 
                  g_indj_d[g_detail_idx].indj014,g_indj_d[g_detail_idx].indj019,g_indj_d[g_detail_idx].indjunit, 
                  g_indj_d[g_detail_idx].indj015,g_indj_d[g_detail_idx].indj016,g_indj_d[g_detail_idx].indj017)  

         WHERE indjent = g_enterprise AND indjdocno = ps_keys_bak[1] AND indjseq = ps_keys_bak[2]
      #add-point:update_b段修改中
      {<point name="update_b.m_update"/>}
      #end add-point   
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "indj_t" 
            LET g_errparam.code   = "std-00009" 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            CALL s_transaction_end('N','0')
         WHEN SQLCA.sqlcode #其他錯誤
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "indj_t" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            CALL s_transaction_end('N','0')
         OTHERWISE
            
      END CASE
      
      #將遮罩欄位進行遮蔽
      CALL aint700_indj_t_mask_restore('restore_mask_n')
               
      #add-point:update_b段修改後
      {<point name="update_b.after_update"/>}
      #end add-point  
   END IF
   
   LET ls_group = "'2',"
   #判斷是否是同一群組的table
   IF ls_group.getIndexOf(ps_page,1) > 0 AND ps_table <> "indk_t" THEN
      #add-point:update_b段修改前
      {<point name="update_b.before_update2" mark="Y"/>}
      #end add-point  
      
      #將遮罩欄位還原
      CALL aint700_indk_t_mask_restore('restore_mask_o')
               
      UPDATE indk_t 
         SET (indkdocno,
              indkseq,indkseq1
              ,indk001,indk002,indk003,indk004,indk005,indk006,indk007,indk008,indk009,indk010,indk015,indk011,indk012,indk013,indk014,indksite,indkunit) 
              = 
             (ps_keys[1],ps_keys[2],ps_keys[3]
              ,g_indj2_d[g_detail_idx].indk001,g_indj2_d[g_detail_idx].indk002,g_indj2_d[g_detail_idx].indk003, 
                  g_indj2_d[g_detail_idx].indk004,g_indj2_d[g_detail_idx].indk005,g_indj2_d[g_detail_idx].indk006, 
                  g_indj2_d[g_detail_idx].indk007,g_indj2_d[g_detail_idx].indk008,g_indj2_d[g_detail_idx].indk009, 
                  g_indj2_d[g_detail_idx].indk010,g_indj2_d[g_detail_idx].indk015,g_indj2_d[g_detail_idx].indk011, 
                  g_indj2_d[g_detail_idx].indk012,g_indj2_d[g_detail_idx].indk013,g_indj2_d[g_detail_idx].indk014, 
                  g_indj2_d[g_detail_idx].indksite,g_indj2_d[g_detail_idx].indkunit) 
         WHERE indkent = g_enterprise AND indkdocno = ps_keys_bak[1] AND indkseq = ps_keys_bak[2] AND indkseq1 = ps_keys_bak[3]
      #add-point:update_b段修改中
      {<point name="update_b.m_update2"/>}
      #end add-point  
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "indk_t" 
            LET g_errparam.code   = "std-00009" 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            CALL s_transaction_end('N','0')
         WHEN SQLCA.sqlcode #其他錯誤
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "indk_t" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            CALL s_transaction_end('N','0')
         OTHERWISE
            
      END CASE
      
      #將遮罩欄位進行遮蔽
      CALL aint700_indk_t_mask_restore('restore_mask_n')
 
      #add-point:update_b段修改後
      {<point name="update_b.after_update2"/>}
      #end add-point  
   END IF
 
 
   
 
   
   #add-point:update_b段other
   {<point name="update_b.other"/>}
   #end add-point  
   
END FUNCTION
]]>
