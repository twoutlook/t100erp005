                     LET g_errparam.extend = g_indj_d_t.indjseq 
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     LET l_lock_sw = "Y"
                  END IF
                  
                  #遮罩相關處理
                  LET g_indj_d_mask_o[l_ac].* =  g_indj_d[l_ac].*
                  CALL aint700_indj_t_mask()
                  LET g_indj_d_mask_n[l_ac].* =  g_indj_d[l_ac].*
                  
                  LET g_bfill = "N"
                  CALL aint700_show()
                  LET g_bfill = "Y"
                  
