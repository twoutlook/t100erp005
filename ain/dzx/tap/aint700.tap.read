                   ,g_indj_d[g_detail_idx].indj001,g_indj_d[g_detail_idx].indj002,g_indj_d[g_detail_idx].indj003, 
                       g_indj_d[g_detail_idx].indj004,g_indj_d[g_detail_idx].indj005,g_indj_d[g_detail_idx].indj006, 
                       g_indj_d[g_detail_idx].indj007,g_indj_d[g_detail_idx].indj008,g_indj_d[g_detail_idx].indj009, 
                       g_indj_d[g_detail_idx].indj010,g_indj_d[g_detail_idx].indj011,g_indj_d[g_detail_idx].indj018, 
                       g_indj_d[g_detail_idx].indjsite,g_indj_d[g_detail_idx].indj012,g_indj_d[g_detail_idx].indj013, 
                       g_indj_d[g_detail_idx].indj014,g_indj_d[g_detail_idx].indj019,g_indj_d[g_detail_idx].indjunit, 
                       g_indj_d[g_detail_idx].indj015,g_indj_d[g_detail_idx].indj016,g_indj_d[g_detail_idx].indj017) 

      #add-point:insert_b段資料新增中
      {<point name="insert_b.m_insert"/>}
      #end add-point 
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "indj_t" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
      END IF
      
      LET li_idx = g_detail_idx
      IF ps_page <> "'1'" THEN 
         CALL g_indj_d.insertElement(li_idx) 
      END IF 
 
      #add-point:insert_b段資料新增後
      {<point name="insert_b.after_insert"/>}
      #end add-point 
   END IF
   
   LET ls_group = "'2',"
   #判斷是否是同一群組的table
   IF ls_group.getIndexOf(ps_page,1) > 0 THEN
      #add-point:insert_b段資料新增前
