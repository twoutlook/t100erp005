  <section id="aint700.insert" ver="8" status="" src="s" readonly="">
    <![CDATA[#+ 資料新增
PRIVATE FUNCTION aint700_insert()
   #add-point:insert段define
   {<point name="insert.define" edit="s"/>}
   #end add-point    
   #add-point:insert段define(客製用)
   {<point name="insert.define_customerization" edit="c"/>}
   #end add-point    
   
   #清畫面欄位內容
   CLEAR FORM                    
   CALL g_indj_d.clear()   
   CALL g_indj2_d.clear()  
 
 
   INITIALIZE g_indi_m.* LIKE indi_t.*             #DEFAULT 設定
   
   LET g_indidocno_t = NULL
 
   
   LET g_master_insert = FALSE
   
   #add-point:insert段before
   {<point name="insert.before"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   WHILE TRUE
      #公用欄位給值(單頭)
      #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_indi_m.indiownid = g_user
      LET g_indi_m.indiowndp = g_dept
      LET g_indi_m.indicrtid = g_user
      LET g_indi_m.indicrtdp = g_dept 
      LET g_indi_m.indicrtdt = cl_get_current()
      LET g_indi_m.indimodid = g_user
      LET g_indi_m.indimoddt = cl_get_current()
      LET g_indi_m.indistus = 'N'
 
 
 
      #append欄位給值
      
     
      #一般欄位給值
      
 
      #add-point:單頭預設值
      {<point name="insert.default"/>}
      #end add-point 
 
      #保存單頭舊值(用於資料輸入錯誤還原預設值時使用)
      LET g_indi_m_t.* = g_indi_m.*
      LET g_indi_m_o.* = g_indi_m.*
      
      #顯示狀態(stus)圖片
            #應用 a21 樣板自動產生(Version:2)
	  #根據當下狀態碼顯示圖片
      CASE g_indi_m.indistus 
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "A"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/approved.png")
         WHEN "D"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/withdraw.png")
         WHEN "R"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/rejection.png")
         WHEN "W"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/signing.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
 
 
    
      CALL aint700_input("a")
      
      #add-point:單頭輸入後
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG AND NOT g_master_insert THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
         DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
         INITIALIZE g_indi_m.* TO NULL
         INITIALIZE g_indj_d TO NULL
         INITIALIZE g_indj2_d TO NULL
 
         #add-point:取消新增後
         {<point name="insert.cancel"/>}
         #end add-point 
         CALL aint700_show()
         RETURN
      END IF
      
      LET INT_FLAG = 0
      #CALL g_indj_d.clear()
      #CALL g_indj2_d.clear()
 
 
      LET g_rec_b = 0
      CALL s_transaction_end('Y','0')
      EXIT WHILE
        
   END WHILE
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL aint700_set_act_visible()   
   CALL aint700_set_act_no_visible()
   
   #將新增的資料併入搜尋條件中
   LET g_indidocno_t = g_indi_m.indidocno
 
   
   #組合新增資料的條件
   LET g_add_browse = " indient = '" ||g_enterprise|| "' AND",
                      " indidocno = '", g_indi_m.indidocno, "' "
 
                      
   #add-point:組合新增資料的條件後
   {<point name="insert.after.add_browse"/>}
   #end add-point
      
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL aint700_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   CLOSE aint700_cl
   
   CALL aint700_idx_chk()
   
   #撈取異動後的資料(主要是帶出reference)
   EXECUTE aint700_master_referesh USING g_indi_m.indidocno INTO g_indi_m.indisite,g_indi_m.indidocdt, 
       g_indi_m.indidocno,g_indi_m.indiunit,g_indi_m.indi001,g_indi_m.indi002,g_indi_m.indi003,g_indi_m.indi004, 
       g_indi_m.indi005,g_indi_m.indistus,g_indi_m.indiownid,g_indi_m.indiowndp,g_indi_m.indicrtid,g_indi_m.indicrtdp, 
       g_indi_m.indicrtdt,g_indi_m.indimodid,g_indi_m.indimoddt,g_indi_m.indicnfid,g_indi_m.indicnfdt, 
       g_indi_m.indisite_desc,g_indi_m.indiunit_desc,g_indi_m.indi002_desc,g_indi_m.indi003_desc,g_indi_m.indi004_desc, 
       g_indi_m.indiownid_desc,g_indi_m.indiowndp_desc,g_indi_m.indicrtid_desc,g_indi_m.indicrtdp_desc, 
       g_indi_m.indimodid_desc,g_indi_m.indicnfid_desc
   
   #遮罩相關處理
   LET g_indi_m_mask_o.* =  g_indi_m.*
   CALL aint700_indi_t_mask()
   LET g_indi_m_mask_n.* =  g_indi_m.*
   
   #將資料顯示到畫面上
   DISPLAY BY NAME g_indi_m.indisite,g_indi_m.indisite_desc,g_indi_m.indidocdt,g_indi_m.indidocno,g_indi_m.indiunit, 
       g_indi_m.indiunit_desc,g_indi_m.indi001,g_indi_m.indi002,g_indi_m.indi002_desc,g_indi_m.indi003, 
       g_indi_m.indi003_desc,g_indi_m.indi004,g_indi_m.indi004_desc,g_indi_m.indi005,g_indi_m.indistus, 
       g_indi_m.indiownid,g_indi_m.indiownid_desc,g_indi_m.indiowndp,g_indi_m.indiowndp_desc,g_indi_m.indicrtid, 
       g_indi_m.indicrtid_desc,g_indi_m.indicrtdp,g_indi_m.indicrtdp_desc,g_indi_m.indicrtdt,g_indi_m.indimodid, 
       g_indi_m.indimodid_desc,g_indi_m.indimoddt,g_indi_m.indicnfid,g_indi_m.indicnfid_desc,g_indi_m.indicnfdt 

   
   #add-point:新增結束後
   {<point name="insert.after"/>}
   #end add-point 
   
   #功能已完成,通報訊息中心
   CALL aint700_msgcentre_notify('')
   
END FUNCTION
]]>
  </section>
