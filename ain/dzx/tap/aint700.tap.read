                       g_indj2_d[g_detail_idx].indk004,g_indj2_d[g_detail_idx].indk005,g_indj2_d[g_detail_idx].indk006, 
                       g_indj2_d[g_detail_idx].indk007,g_indj2_d[g_detail_idx].indk008,g_indj2_d[g_detail_idx].indk009, 
                       g_indj2_d[g_detail_idx].indk010,g_indj2_d[g_detail_idx].indk015,g_indj2_d[g_detail_idx].indk011, 
                       g_indj2_d[g_detail_idx].indk012,g_indj2_d[g_detail_idx].indk013,g_indj2_d[g_detail_idx].indk014, 
                       g_indj2_d[g_detail_idx].indksite,g_indj2_d[g_detail_idx].indkunit)
      #add-point:insert_b段資料新增中
      {<point name="insert_b.m_insert2"/>}
