  <point name="statechange.b_update" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   CALL s_transaction_begin()

   OPEN aint700_cl USING g_enterprise,g_indi_m.indidocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.extend = "OPEN aint700_cl:"
      LET g_errparam.code   = STATUS
      LET g_errparam.popup  = TRUE
      CALL cl_err()
      CLOSE aint700_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF

   CALL cl_err_collect_init()
   IF lc_state = 'Y' THEN
      CALL s_aint700_conf_chk(g_indi_m.indidocno,g_indi_m.indistus) RETURNING l_success
      IF NOT l_success THEN
         CALL cl_err_collect_show()
         CALL s_transaction_end('N','0')
         RETURN
      ELSE
         IF NOT cl_ask_confirm('aim-00108') THEN
            CALL cl_err_collect_show()
            CALL s_transaction_end('N','0')
            RETURN
         ELSE
            CALL s_aint700_conf_upd(g_indi_m.indidocno) RETURNING l_success
            IF NOT l_success THEN
               CALL s_transaction_end('N','0')
               CALL cl_err_collect_show()
               RETURN
            ELSE
               #是否一併產生調撥單
               IF NOT cl_ask_confirm('ain-00378') THEN
                  CALL s_transaction_end('Y','0')
                  CALL cl_err_collect_show()
                  CALL aint700_b_fill()
               ELSE
                  CALL s_aint510_gen('1',g_indi_m.indidocno) RETURNING l_success
                  IF NOT l_success THEN
                     CALL s_transaction_end('N','0')
                     CALL cl_err_collect_show()
                     RETURN
                  ELSE
                     CALL s_transaction_end('Y','0')
                     CALL cl_err_collect_show()
                     CALL aint700_b_fill()
                  END IF
               END IF 
            END IF
         END IF
      END IF
   END IF

   IF lc_state = 'N' THEN
      CALL s_aint700_unconf_chk(g_indi_m.indidocno,g_indi_m.indistus) RETURNING l_success
      IF NOT l_success THEN
         CALL cl_err_collect_show()
         CALL s_transaction_end('N','0')
         RETURN
      ELSE
         IF NOT cl_ask_confirm('aim-00110') THEN
            CALL cl_err_collect_show()
            CALL s_transaction_end('N','0')
            RETURN
         ELSE
            CALL s_aint700_unconf_upd(g_indi_m.indidocno) RETURNING l_success
            IF NOT l_success THEN
               CALL s_transaction_end('N','0')
               CALL cl_err_collect_show()
               RETURN
            ELSE
               CALL s_transaction_end('Y','0')
               CALL cl_err_collect_show()
               CALL aint700_b_fill()
            END IF
         END IF
