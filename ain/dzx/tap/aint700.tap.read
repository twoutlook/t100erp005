  <point name="function.aint700_indk_b_fill" order="3" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#針對多庫儲批頁籤的內容進行顯示
PUBLIC FUNCTION aint700_indk_b_fill()
   
   CALL g_indj2_d.clear()
   
   LET g_sql = "SELECT  UNIQUE indkseq,indkseq1,indk001,indk002,indk003,indk004,indk005,indk006, 
       indk007,indk008,indk009,indk010,indk015,indk011,indk012,indk013,indk014,indksite,indkunit ,t12.imaal003 , 
       t13.imaal004 ,t14.oocal003 ,t15.oocal003 ,t16.ooefl003 ,t17.inayl003 ,t18.inab003 ,t19.ooefl003 , 
       t20.inayl003 ,t21.inab003 ,t22.ooefl003 ,t23.ooefl003 FROM indk_t",   
               " INNER JOIN indi_t ON indidocno = indkdocno ",               
         " LEFT JOIN imaal_t t12 ON t12.imaalent='"||g_enterprise||"' AND t12.imaal001=indk001 AND t12.imaal002='"||g_dlang||"' ",
         " LEFT JOIN imaal_t t13 ON t13.imaalent='"||g_enterprise||"' AND t13.imaal001=indk001 AND t13.imaal002='"||g_dlang||"' ",
         " LEFT JOIN oocal_t t14 ON t14.oocalent='"||g_enterprise||"' AND t14.oocal001=indk003 AND t14.oocal002='"||g_dlang||"' ",
         " LEFT JOIN oocal_t t15 ON t15.oocalent='"||g_enterprise||"' AND t15.oocal001=indk005 AND t15.oocal002='"||g_dlang||"' ",
         " LEFT JOIN ooefl_t t16 ON t16.ooeflent='"||g_enterprise||"' AND t16.ooefl001=indk007 AND t16.ooefl002='"||g_dlang||"' ",
         " LEFT JOIN inayl_t t17 ON t17.inaylent='"||g_enterprise||"' AND t17.inayl001=indk008 AND t17.inayl002='"||g_dlang||"' ",
         " LEFT JOIN inab_t t18 ON t18.inabent='"||g_enterprise||"' AND t18.inabsite = indk007 AND t18.inab001=indk008 AND t18.inab002=indk009  ",
         " LEFT JOIN ooefl_t t19 ON t19.ooeflent='"||g_enterprise||"' AND t19.ooefl001=indk011 AND t19.ooefl002='"||g_dlang||"' ",
         " LEFT JOIN inayl_t t20 ON t20.inaylent='"||g_enterprise||"' AND t20.inayl001=indk012 AND t20.inayl002='"||g_dlang||"' ",
         " LEFT JOIN inab_t t21 ON t21.inabent='"||g_enterprise||"' AND t21.inabsite = indk011 AND t21.inab001=indk012 AND t21.inab002=indk013  ",
         " LEFT JOIN ooefl_t t22 ON t22.ooeflent='"||g_enterprise||"' AND t22.ooefl001=indksite AND t22.ooefl002='"||g_dlang||"' ",
         " LEFT JOIN ooefl_t t23 ON t23.ooeflent='"||g_enterprise||"' AND t23.ooefl001=indkunit AND t23.ooefl002='"||g_dlang||"' ",
          " WHERE indkent=? AND indkdocno=?"   
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   IF NOT cl_null(g_wc2_table2) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
   END IF
   
   LET g_sql = g_sql, " ORDER BY indk_t.indkseq,indk_t.indkseq1"
   
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE aint700_pb2_u FROM g_sql
   DECLARE b_fill_cs2_u CURSOR FOR aint700_pb2_u
   
   LET g_cnt = l_ac
   LET l_ac = 1
   
   OPEN b_fill_cs2_u USING g_enterprise,g_indi_m.indidocno
                                            
   FOREACH b_fill_cs2_u INTO g_indj2_d[l_ac].indkseq,g_indj2_d[l_ac].indkseq1,g_indj2_d[l_ac].indk001, 
       g_indj2_d[l_ac].indk002,g_indj2_d[l_ac].indk003,g_indj2_d[l_ac].indk004,g_indj2_d[l_ac].indk005, 
       g_indj2_d[l_ac].indk006,g_indj2_d[l_ac].indk007,g_indj2_d[l_ac].indk008,g_indj2_d[l_ac].indk009, 
       g_indj2_d[l_ac].indk010,g_indj2_d[l_ac].indk015,g_indj2_d[l_ac].indk011,g_indj2_d[l_ac].indk012,g_indj2_d[l_ac].indk013, 
       g_indj2_d[l_ac].indk014,g_indj2_d[l_ac].indksite,g_indj2_d[l_ac].indkunit,g_indj2_d[l_ac].indk001_desc, 
       g_indj2_d[l_ac].indk001_desc_desc,g_indj2_d[l_ac].indk003_desc,g_indj2_d[l_ac].indk005_desc, 
       g_indj2_d[l_ac].indk007_desc,g_indj2_d[l_ac].indk008_desc,g_indj2_d[l_ac].indk009_desc,g_indj2_d[l_ac].indk011_desc, 
       g_indj2_d[l_ac].indk012_desc,g_indj2_d[l_ac].indk013_desc,g_indj2_d[l_ac].indksite_desc,g_indj2_d[l_ac].indkunit_desc 

      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
     
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = l_ac
         LET g_errparam.code   = 9035 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
      
   END FOREACH
   LET l_ac = g_cnt
   LET g_cnt = 0  
   CALL g_indj2_d.deleteElement(g_indj2_d.getLength())
END FUNCTION]]>
