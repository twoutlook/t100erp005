          g_indj_d[l_ac].indj004,g_indj_d[l_ac].indj005,g_indj_d[l_ac].indj006,g_indj_d[l_ac].indj007, 
          g_indj_d[l_ac].indj008,g_indj_d[l_ac].indj009,g_indj_d[l_ac].indj010,g_indj_d[l_ac].indj011, 
