  <section id="aint700.modify" ver="8" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aint700_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   DEFINE l_wc2_table1          STRING
   DEFINE l_wc2_table2   STRING
 
 
 
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point    
   #add-point:modify段define(客製用)
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point    
   
   #保存單頭舊值
   LET g_indi_m_t.* = g_indi_m.*
   LET g_indi_m_o.* = g_indi_m.*
   
   IF g_indi_m.indidocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_indidocno_t = g_indi_m.indidocno
 
   CALL s_transaction_begin()
   
   OPEN aint700_cl USING g_enterprise,g_indi_m.indidocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aint700_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE aint700_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aint700_master_referesh USING g_indi_m.indidocno INTO g_indi_m.indisite,g_indi_m.indidocdt, 
       g_indi_m.indidocno,g_indi_m.indiunit,g_indi_m.indi001,g_indi_m.indi002,g_indi_m.indi003,g_indi_m.indi004, 
       g_indi_m.indi005,g_indi_m.indistus,g_indi_m.indiownid,g_indi_m.indiowndp,g_indi_m.indicrtid,g_indi_m.indicrtdp, 
       g_indi_m.indicrtdt,g_indi_m.indimodid,g_indi_m.indimoddt,g_indi_m.indicnfid,g_indi_m.indicnfdt, 
       g_indi_m.indisite_desc,g_indi_m.indiunit_desc,g_indi_m.indi002_desc,g_indi_m.indi003_desc,g_indi_m.indi004_desc, 
       g_indi_m.indiownid_desc,g_indi_m.indiowndp_desc,g_indi_m.indicrtid_desc,g_indi_m.indicrtdp_desc, 
       g_indi_m.indimodid_desc,g_indi_m.indicnfid_desc
   
   #檢查是否允許此動作
   IF NOT aint700_action_chk() THEN
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_indi_m_mask_o.* =  g_indi_m.*
   CALL aint700_indi_t_mask()
   LET g_indi_m_mask_n.* =  g_indi_m.*
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
   #LET l_wc2_table2 = g_wc2_table2
   #LET l_wc2_table2 = " 1=1"
 
 
 
   
   CALL aint700_show()
   #add-point:modify段show之後
   {<point name="modify.after_show"/>}
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
   #LET  g_wc2_table2 = l_wc2_table2 
 
 
 
    
   WHILE TRUE
      LET g_indidocno_t = g_indi_m.indidocno
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_indi_m.indimodid = g_user 
LET g_indi_m.indimoddt = cl_get_current()
LET g_indi_m.indimodid_desc = cl_get_username(g_indi_m.indimodid)
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      CALL aint700_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE indi_t SET (indimodid,indimoddt) = (g_indi_m.indimodid,g_indi_m.indimoddt)
          WHERE indient = g_enterprise AND indidocno = g_indidocno_t
 
      END IF
    
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_indi_m.* = g_indi_m_t.*
         CALL aint700_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_indi_m.indidocno != g_indidocno_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE indj_t SET indjdocno = g_indi_m.indidocno
 
          WHERE indjent = g_enterprise AND indjdocno = g_indidocno_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "indj_t" 
               LET g_errparam.code   = "std-00009" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "indj_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
         #更新單身key值
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update2" mark="Y"/>}
         #end add-point
         
         UPDATE indk_t
            SET indkdocno = g_indi_m.indidocno
 
          WHERE indkent = g_enterprise AND
                indkdocno = g_indidocno_t
 
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update2"/>}
         #end add-point
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "indk_t" 
               LET g_errparam.code   = "std-00009" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "indk_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update2"/>}
         #end add-point
 
 
         
 
         
         #UPDATE 多語言table key值
         
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL aint700_set_act_visible()   
   CALL aint700_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " indient = '" ||g_enterprise|| "' AND",
                      " indidocno = '", g_indi_m.indidocno, "' "
 
   #填到對應位置
   CALL aint700_browser_fill("")
 
   CLOSE aint700_cl
   
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aint700_msgcentre_notify('')
 
END FUNCTION   
]]>
