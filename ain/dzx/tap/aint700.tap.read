  <section id="aint700.state_change" ver="9" status="" src="s" readonly="">
    <![CDATA[   #應用 a09 樣板自動產生(Version:11)
#+ 確認碼變更 
PRIVATE FUNCTION aint700_statechange()
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define
   {<point name="statechange.define" edit="s"/>}
   #end add-point  
   #add-point:statechange段define(客製用)
   {<point name="statechange.define_customerization" edit="c"/>}
   #end add-point  
   
   #add-point:statechange段開始前
   {<point name="statechange.before"/>}
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_indi_m.indidocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN aint700_cl USING g_enterprise,g_indi_m.indidocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aint700_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE aint700_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE aint700_master_referesh USING g_indi_m.indidocno INTO g_indi_m.indisite,g_indi_m.indidocdt, 
       g_indi_m.indidocno,g_indi_m.indiunit,g_indi_m.indi001,g_indi_m.indi002,g_indi_m.indi003,g_indi_m.indi004, 
       g_indi_m.indi005,g_indi_m.indistus,g_indi_m.indiownid,g_indi_m.indiowndp,g_indi_m.indicrtid,g_indi_m.indicrtdp, 
       g_indi_m.indicrtdt,g_indi_m.indimodid,g_indi_m.indimoddt,g_indi_m.indicnfid,g_indi_m.indicnfdt, 
       g_indi_m.indisite_desc,g_indi_m.indiunit_desc,g_indi_m.indi002_desc,g_indi_m.indi003_desc,g_indi_m.indi004_desc, 
       g_indi_m.indiownid_desc,g_indi_m.indiowndp_desc,g_indi_m.indicrtid_desc,g_indi_m.indicrtdp_desc, 
       g_indi_m.indimodid_desc,g_indi_m.indicnfid_desc
 
   #檢查是否允許此動作
   IF NOT aint700_action_chk() THEN
      CLOSE aint700_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_indi_m.indisite,g_indi_m.indisite_desc,g_indi_m.indidocdt,g_indi_m.indidocno,g_indi_m.indiunit, 
       g_indi_m.indiunit_desc,g_indi_m.indi001,g_indi_m.indi002,g_indi_m.indi002_desc,g_indi_m.indi003, 
       g_indi_m.indi003_desc,g_indi_m.indi004,g_indi_m.indi004_desc,g_indi_m.indi005,g_indi_m.indistus, 
       g_indi_m.indiownid,g_indi_m.indiownid_desc,g_indi_m.indiowndp,g_indi_m.indiowndp_desc,g_indi_m.indicrtid, 
       g_indi_m.indicrtid_desc,g_indi_m.indicrtdp,g_indi_m.indicrtdp_desc,g_indi_m.indicrtdt,g_indi_m.indimodid, 
       g_indi_m.indimodid_desc,g_indi_m.indimoddt,g_indi_m.indicnfid,g_indi_m.indicnfid_desc,g_indi_m.indicnfdt 

 
   CASE g_indi_m.indistus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
      WHEN "A"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/approved.png")
      WHEN "D"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/withdraw.png")
      WHEN "R"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/rejection.png")
      WHEN "W"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/signing.png")
      WHEN "X"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
      
   END CASE
 
   #add-point:資料刷新後
   {<point name="statechange.after_refresh"/>}
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_indi_m.indistus
            
            WHEN "N"
               HIDE OPTION "unconfirmed"
            WHEN "Y"
               HIDE OPTION "confirmed"
            WHEN "A"
               HIDE OPTION "approved"
            WHEN "D"
               HIDE OPTION "withdraw"
            WHEN "R"
               HIDE OPTION "rejection"
            WHEN "W"
               HIDE OPTION "signing"
            WHEN "X"
               HIDE OPTION "invalid"
         END CASE
     
      #add-point:menu前
      {<point name="statechange.before_menu"/>}
      #end add-point
      
      #應用 a36 樣板自動產生(Version:4)
      #提交
      ON ACTION signing
         IF cl_auth_chk_act("signing") THEN
            IF NOT aint700_send() THEN
               CALL s_transaction_end('N','0')
            ELSE
               CALL s_transaction_end('Y','0')
            END IF
            #因應簽核行為, 該動作完成後不再進行後續處理
            #於此處直接返回
            CLOSE aint700_cl
            RETURN
         END IF
    
      #抽單
      ON ACTION withdraw
         IF cl_auth_chk_act("withdraw") THEN
            IF NOT aint700_draw_out() THEN
               CALL s_transaction_end('N','0')
            ELSE
               CALL s_transaction_end('Y','0')
            END IF
            #因應簽核行為, 該動作完成後不再進行後續處理
            #於此處直接返回
            CLOSE aint700_cl
            RETURN
         END IF
 
 
	  
      ON ACTION unconfirmed
         IF cl_auth_chk_act("unconfirmed") THEN
            LET lc_state = "N"
            #add-point:action控制
            {<point name="statechange.unconfirmed"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION confirmed
         IF cl_auth_chk_act("confirmed") THEN
            LET lc_state = "Y"
            #add-point:action控制
            {<point name="statechange.confirmed"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION approved
         IF cl_auth_chk_act("approved") THEN
            LET lc_state = "A"
            #add-point:action控制
            {<point name="statechange.approved"/>}
            #end add-point
         END IF
         EXIT MENU
      #ON ACTION withdraw
      #   IF cl_auth_chk_act("withdraw") THEN
      #      LET lc_state = "D"
      #      #add-point:action控制
      #      {<point name="statechange.withdraw"/>}
      #      #end add-point
      #   END IF
      #   EXIT MENU
      ON ACTION rejection
         IF cl_auth_chk_act("rejection") THEN
            LET lc_state = "R"
            #add-point:action控制
            {<point name="statechange.rejection"/>}
            #end add-point
         END IF
         EXIT MENU
      #ON ACTION signing
      #   IF cl_auth_chk_act("signing") THEN
      #      LET lc_state = "W"
      #      #add-point:action控制
      #      {<point name="statechange.signing"/>}
      #      #end add-point
      #   END IF
      #   EXIT MENU
      ON ACTION invalid
         IF cl_auth_chk_act("invalid") THEN
            LET lc_state = "X"
            #add-point:action控制
            {<point name="statechange.invalid"/>}
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制
      {<point name="statechange.more_control"/>}
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      AND lc_state <> "A"
      AND lc_state <> "D"
      AND lc_state <> "R"
      AND lc_state <> "W"
      AND lc_state <> "X"
      ) OR 
      g_indi_m.indistus = lc_state OR cl_null(lc_state) THEN
      CLOSE aint700_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前
   {<point name="statechange.b_update"/>}
   #end add-point
   
   LET g_indi_m.indimodid = g_user
   LET g_indi_m.indimoddt = cl_get_current()
   LET g_indi_m.indistus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE indi_t 
      SET (indistus,indimodid,indimoddt) 
        = (g_indi_m.indistus,g_indi_m.indimodid,g_indi_m.indimoddt)     
    WHERE indient = g_enterprise AND indidocno = g_indi_m.indidocno
 
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "A"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/approved.png")
         WHEN "D"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/withdraw.png")
         WHEN "R"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/rejection.png")
         WHEN "W"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/signing.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE aint700_master_referesh USING g_indi_m.indidocno INTO g_indi_m.indisite,g_indi_m.indidocdt, 
          g_indi_m.indidocno,g_indi_m.indiunit,g_indi_m.indi001,g_indi_m.indi002,g_indi_m.indi003,g_indi_m.indi004, 
          g_indi_m.indi005,g_indi_m.indistus,g_indi_m.indiownid,g_indi_m.indiowndp,g_indi_m.indicrtid, 
          g_indi_m.indicrtdp,g_indi_m.indicrtdt,g_indi_m.indimodid,g_indi_m.indimoddt,g_indi_m.indicnfid, 
          g_indi_m.indicnfdt,g_indi_m.indisite_desc,g_indi_m.indiunit_desc,g_indi_m.indi002_desc,g_indi_m.indi003_desc, 
          g_indi_m.indi004_desc,g_indi_m.indiownid_desc,g_indi_m.indiowndp_desc,g_indi_m.indicrtid_desc, 
          g_indi_m.indicrtdp_desc,g_indi_m.indimodid_desc,g_indi_m.indicnfid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_indi_m.indisite,g_indi_m.indisite_desc,g_indi_m.indidocdt,g_indi_m.indidocno, 
          g_indi_m.indiunit,g_indi_m.indiunit_desc,g_indi_m.indi001,g_indi_m.indi002,g_indi_m.indi002_desc, 
          g_indi_m.indi003,g_indi_m.indi003_desc,g_indi_m.indi004,g_indi_m.indi004_desc,g_indi_m.indi005, 
          g_indi_m.indistus,g_indi_m.indiownid,g_indi_m.indiownid_desc,g_indi_m.indiowndp,g_indi_m.indiowndp_desc, 
          g_indi_m.indicrtid,g_indi_m.indicrtid_desc,g_indi_m.indicrtdp,g_indi_m.indicrtdp_desc,g_indi_m.indicrtdt, 
          g_indi_m.indimodid,g_indi_m.indimodid_desc,g_indi_m.indimoddt,g_indi_m.indicnfid,g_indi_m.indicnfid_desc, 
          g_indi_m.indicnfdt
   END IF
 
   #add-point:stus修改後
   {<point name="statechange.a_update"/>}
   #end add-point
 
   #add-point:statechange段結束前
   {<point name="statechange.after"/>}
   #end add-point  
 
   CLOSE aint700_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aint700_msgcentre_notify(lc_state)
   
END FUNCTION
 
 
]]>
