               #add-point:modify段after_set_entry_b
               {<point name="input.body.after_set_entry_b"/>}
               #end add-point  
               CALL aint700_set_no_entry_b(l_cmd)
               IF NOT aint700_lock_b("indj_t","'1'") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aint700_bcl INTO g_indj_d[l_ac].indjseq,g_indj_d[l_ac].indj001,g_indj_d[l_ac].indj002, 
