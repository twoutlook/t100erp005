      CLOSE aint911_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aint911_master_referesh USING g_inba_m.inbadocno INTO g_inba_m.inba001,g_inba_m.inbasite, 
