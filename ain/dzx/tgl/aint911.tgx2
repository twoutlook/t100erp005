       g_inba_m.inbacrtdp,g_inba_m.inbacrtdt,g_inba_m.inbamodid,g_inba_m.inbamoddt,g_inba_m.inbacnfid, 
       g_inba_m.inbacnfdt,g_inba_m.inbapstid,g_inba_m.inbapstdt,g_inba_m.inba003_desc,g_inba_m.inba004_desc, 
