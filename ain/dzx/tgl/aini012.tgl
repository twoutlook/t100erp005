      END IF
      CALL s_transaction_end('Y','0')
   ELSE
      CALL s_transaction_end('N','0')   
   END IF
 
   CLOSE aini012_cl
 
   #功能已完成,通報訊息中心
   CALL aini012_msgcentre_notify('delete')
    
