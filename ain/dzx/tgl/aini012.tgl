{<section id="aini012.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION aini012_b_fill(p_wc2)
   #add-point:b_fill段define name="b_fill.define_customerization"
   {<point name="b_fill.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE p_wc2      STRING
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   {<point name="b_fill.define"/>}
   #end add-point     
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   {<point name="b_fill.pre_function"/>}
   #end add-point
   
   #先清空單身變數內容
   CALL g_inaf_d.clear()
   CALL g_inaf2_d.clear()
 
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  DISTINCT inafstus,inaf003,inaf004,inaf005,inaf006,inaf007,inaf008,inaf009,inaf010, 
       inaf011,inaf003,inafownid,inafowndp,inafcrtid,inafcrtdp,inafcrtdt,inafmodid,inafmoddt,t1.imaal003 , 
       t2.imaal004 ,t3.oocal003 ,t4.ooag011 ,t5.ooefl003 ,t6.ooag011 ,t7.ooefl003 ,t8.ooag011 FROM inaf_t", 
          
               "",
               
                              " LEFT JOIN imaal_t t1 ON t1.imaalent="||g_enterprise||" AND t1.imaal001=inaf003 AND t1.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t2 ON t2.imaalent="||g_enterprise||" AND t2.imaal001=inaf003 AND t2.imaal002='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t3 ON t3.oocalent="||g_enterprise||" AND t3.oocal001=inaf005 AND t3.oocal002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t4 ON t4.ooagent="||g_enterprise||" AND t4.ooag001=inafownid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent="||g_enterprise||" AND t5.ooefl001=inafowndp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent="||g_enterprise||" AND t6.ooag001=inafcrtid  ",
               " LEFT JOIN ooefl_t t7 ON t7.ooeflent="||g_enterprise||" AND t7.ooefl001=inafcrtdp AND t7.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t8 ON t8.ooagent="||g_enterprise||" AND t8.ooag001=inafmodid  ",
 
               " WHERE inafent= ? AND inafsite=? AND inaf001=? AND inaf002=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("inaf_t")
   END IF
   
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF aini012_fill_chk(1) THEN
      IF (g_action_choice = "query" OR cl_null(g_action_choice))
         #add-point:b_fill段long_sql_if name="b_fill.long_sql_if"
         {<point name="b_fill.long_sql_if"/>}
         #end add-point
      THEN
         LET g_sql = g_sql, " ORDER BY inaf_t.inaf003,inaf_t.inaf004"
         #add-point:b_fill段fill_before name="b_fill.fill_before"
         {<point name="b_fill.fill_before"/>}
         #end add-point
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE aini012_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR aini012_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
   #  OPEN b_fill_cs USING g_enterprise,g_inaf_m.inafsite,g_inaf_m.inaf001,g_inaf_m.inaf002   #(ver:49)
                                               
      FOREACH b_fill_cs USING g_enterprise,g_inaf_m.inafsite,g_inaf_m.inaf001,g_inaf_m.inaf002 INTO g_inaf_d[l_ac].inafstus, 
          g_inaf_d[l_ac].inaf003,g_inaf_d[l_ac].inaf004,g_inaf_d[l_ac].inaf005,g_inaf_d[l_ac].inaf006, 
          g_inaf_d[l_ac].inaf007,g_inaf_d[l_ac].inaf008,g_inaf_d[l_ac].inaf009,g_inaf_d[l_ac].inaf010, 
          g_inaf_d[l_ac].inaf011,g_inaf2_d[l_ac].inaf003,g_inaf2_d[l_ac].inafownid,g_inaf2_d[l_ac].inafowndp, 
          g_inaf2_d[l_ac].inafcrtid,g_inaf2_d[l_ac].inafcrtdp,g_inaf2_d[l_ac].inafcrtdt,g_inaf2_d[l_ac].inafmodid, 
          g_inaf2_d[l_ac].inafmoddt,g_inaf_d[l_ac].inaf003_desc,g_inaf_d[l_ac].inaf003_desc_desc,g_inaf_d[l_ac].inaf005_desc, 
          g_inaf2_d[l_ac].inafownid_desc,g_inaf2_d[l_ac].inafowndp_desc,g_inaf2_d[l_ac].inafcrtid_desc, 
          g_inaf2_d[l_ac].inafcrtdp_desc,g_inaf2_d[l_ac].inafmodid_desc   #(ver:49)
                             
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充 name="b_fill.fill"
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #應用 a12 樣板自動產生(Version:4)
 
 
 
 
        
         #add-point:單身資料抓取 name="bfill.foreach"
         {<point name="bfill.foreach"/>}
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF 
            EXIT FOREACH
         END IF
      
         LET l_ac = l_ac + 1
         
      END FOREACH
 
            CALL g_inaf_d.deleteElement(g_inaf_d.getLength())
      CALL g_inaf2_d.deleteElement(g_inaf2_d.getLength())
 
      
   END IF
   
   #add-point:b_fill段more name="b_fill.more"
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_inaf_d.getLength()
      LET g_inaf_d_mask_o[l_ac].* =  g_inaf_d[l_ac].*
      CALL aini012_inaf_t_mask()
      LET g_inaf_d_mask_n[l_ac].* =  g_inaf_d[l_ac].*
   END FOR
   
   LET g_inaf2_d_mask_o.* =  g_inaf2_d.*
   FOR l_ac = 1 TO g_inaf2_d.getLength()
      LET g_inaf2_d_mask_o[l_ac].* =  g_inaf2_d[l_ac].*
      CALL aini012_inaf_t_mask()
      LET g_inaf2_d_mask_n[l_ac].* =  g_inaf2_d[l_ac].*
   END FOR
 
 
   FREE aini012_pb   
   
END FUNCTION

