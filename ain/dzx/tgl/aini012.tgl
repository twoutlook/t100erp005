{<section id="aini012.reproduce" >}
#+ 資料複製
PRIVATE FUNCTION aini012_reproduce()
   #add-point:reproduce段define name="reproduce.define_customerization"
   {<point name="reproduce.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE l_newno     LIKE inaf_t.inafsite 
   DEFINE l_oldno     LIKE inaf_t.inafsite 
   DEFINE l_newno02     LIKE inaf_t.inaf001 
   DEFINE l_oldno02     LIKE inaf_t.inaf001 
   DEFINE l_newno03     LIKE inaf_t.inaf002 
   DEFINE l_oldno03     LIKE inaf_t.inaf002 
 
   DEFINE l_master    RECORD LIKE inaf_t.* #此變數樣板目前無使用
   DEFINE l_detail    RECORD LIKE inaf_t.* #此變數樣板目前無使用
 
   DEFINE l_cnt       LIKE type_t.num10
   #add-point:reproduce段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="reproduce.define"
   {<point name="reproduce.define"/>}
   #end add-point
   
   #add-point:Function前置處理  name="reproduce.pre_function"
   {<point name="reproduce.pre_function"/>}
   #end add-point
   
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF     
 
   IF g_inaf_m.inafsite IS NULL
      OR g_inaf_m.inaf001 IS NULL
      OR g_inaf_m.inaf002 IS NULL
 
      THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
   
   LET g_inafsite_t = g_inaf_m.inafsite
   LET g_inaf001_t = g_inaf_m.inaf001
   LET g_inaf002_t = g_inaf_m.inaf002
 
   
   LET g_inaf_m.inafsite = ""
   LET g_inaf_m.inaf001 = ""
   LET g_inaf_m.inaf002 = ""
 
   LET g_master_insert = FALSE
   CALL aini012_set_entry('a')
   CALL aini012_set_no_entry('a')
   
   CALL cl_set_head_visible("","YES")
   CALL s_transaction_begin()
   
   #add-point:複製輸入前 name="reproduce.head.b_input"
   {<point name="reproduce.head.b_input"/>}
   #end add-point
   
   #清空key欄位的desc
   
   
   CALL aini012_input("r")
    
   IF INT_FLAG AND NOT g_master_insert THEN
      DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
      DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
      INITIALIZE g_inaf_m.* TO NULL
      INITIALIZE g_inaf_d TO NULL
      INITIALIZE g_inaf2_d TO NULL
 
      CALL aini012_show()
      LET INT_FLAG = 0
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = '' 
      LET g_errparam.code   = 9001 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN 
   END IF
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("modify,modify_detail,delete,reproduce", TRUE)
   CALL aini012_set_act_visible()
   CALL aini012_set_act_no_visible()
 
   #將新增的資料併入搜尋條件中
   LET g_state = "insert"
   
   LET g_inafsite_t = g_inaf_m.inafsite
   LET g_inaf001_t = g_inaf_m.inaf001
   LET g_inaf002_t = g_inaf_m.inaf002
 
   
   #組合新增資料的條件
   LET g_add_browse = " inafent = " ||g_enterprise|| " AND",
                      " inafsite = '", g_inaf_m.inafsite, "' "
                      ," AND inaf001 = '", g_inaf_m.inaf001, "' "
                      ," AND inaf002 = '", g_inaf_m.inaf002, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL aini012_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   LET g_curr_diag = ui.DIALOG.getCurrent()
   CALL aini012_idx_chk()
   
   #add-point:完成複製段落後 name="reproduce.after_reproduce"
   {<point name="reproduce.after_reproduce" />}
   #end add-point
   
   #功能已完成,通報訊息中心
   CALL aini012_msgcentre_notify('reproduce')
   
END FUNCTION

{</section>}

{<section id="aini012.detail_reproduce" >}
#+ 單身自動複製
PRIVATE FUNCTION aini012_detail_reproduce()
   #add-point:delete段define name="detail_reproduce.define_customerization"
   {<point name="detail_reproduce.define_customerization" edit="c" mark="Y"/>}
   #end add-point 
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE inaf_t.* #此變數樣板目前無使用
 
 
   #add-point:delete段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="detail_reproduce.define"
   {<point name="detail_reproduce.define"/>}
   #end add-point    
   
   #add-point:Function前置處理  name="detail_reproduce.pre_function"
   {<point name="detail_reproduce.pre_function"/>}
   #end add-point
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE aini012_detail
   
   #add-point:單身複製前1 name="detail_reproduce.body.table1.b_insert"
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   SELECT * FROM inaf_t
    WHERE inafent = g_enterprise AND inafsite = g_inafsite_t
    AND inaf001 = g_inaf001_t
    AND inaf002 = g_inaf002_t
 
       INTO TEMP aini012_detail
   
   #將key修正為調整後   
   UPDATE aini012_detail 
      #更新key欄位
      SET inafsite = g_inaf_m.inafsite
          , inaf001 = g_inaf_m.inaf001
          , inaf002 = g_inaf_m.inaf002
 
      #更新共用欄位
      #應用 a13 樣板自動產生(Version:4)
       , inafownid = g_user 
       , inafowndp = g_dept
       , inafcrtid = g_user
       , inafcrtdp = g_dept 
       , inafcrtdt = ld_date
       , inafmodid = g_user
       , inafmoddt = ld_date
      #, inafstus = "Y" 
 
 
 
                                       
   #將資料塞回原table   
   INSERT INTO inaf_t SELECT * FROM aini012_detail
   
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "reproduce:",SQLERRMESSAGE 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #add-point:單身複製中1 name="detail_reproduce.body.table1.m_insert"
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE aini012_detail
   
   #add-point:單身複製後1 name="detail_reproduce.body.table1.a_insert"
   {<point name="detail_reproduce.body.table1.a_insert"/>}
   #end add-point
 
 
   
 
   
   #多語言複製段落
   
   
   CALL s_transaction_end('Y','0')
   
   #已新增完, 調整資料內容(修改時使用)
   LET g_inafsite_t = g_inaf_m.inafsite
   LET g_inaf001_t = g_inaf_m.inaf001
   LET g_inaf002_t = g_inaf_m.inaf002
 
   
   DROP TABLE aini012_detail
   
END FUNCTION

