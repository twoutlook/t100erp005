                  CALL aint836_inpg_t_mask()
                  LET g_inpg_d_mask_n[l_ac].* =  g_inpg_d[l_ac].*
                  
                  LET g_bfill = "N"
                  CALL aint836_show()
                  LET g_bfill = "Y"
                  
                  CALL cl_show_fld_cont()
