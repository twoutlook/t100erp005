               {<point name="input.body.after_set_entry_b"/>}
               #end add-point  
               CALL aint836_set_no_entry_b(l_cmd)
               IF NOT aint836_lock_b("inpg_t","'1'") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aint836_bcl INTO g_inpg_d[l_ac].inpgseq1,g_inpg_d[l_ac].inpgseq2,g_inpg_d[l_ac].inpg001, 
