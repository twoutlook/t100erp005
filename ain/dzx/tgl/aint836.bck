   #add-point:delete段define(客製用) name="delete.define_customerization"
   {<point name="delete.define_customerization" edit="c" mark="Y"/>}
   #end add-point     
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="delete.define"
   {<point name="delete.define"/>} 
   #end add-point     
   
   #add-point:Function前置處理  name="delete.pre_function"
   {<point name="delete.pre_function"/>}
   #end add-point
   
   IF g_inpf_m.inpfsite IS NULL
   OR g_inpf_m.inpfdocno IS NULL
   OR g_inpf_m.inpfseq IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code = "std-00003" 
      LET g_errparam.popup = FALSE 
      CALL cl_err()
      RETURN
   END IF
   
   
   
   CALL s_transaction_begin()
 
   OPEN aint836_cl USING g_enterprise,g_inpf_m.inpfsite,g_inpf_m.inpfdocno,g_inpf_m.inpfseq
   IF SQLCA.SQLCODE THEN   #(ver:78)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aint836_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE   #(ver:78)
      LET g_errparam.popup = TRUE 
      CLOSE aint836_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aint836_master_referesh USING g_inpf_m.inpfsite,g_inpf_m.inpfdocno,g_inpf_m.inpfseq INTO g_inpf_m.inpfdocno, 
       g_inpf_m.inpf004,g_inpf_m.inpf022,g_inpf_m.inpf005,g_inpf_m.inpf023,g_inpf_m.inpfseq,g_inpf_m.inpfsite, 
       g_inpf_m.inpfstus,g_inpf_m.inpf001,g_inpf_m.inpf003,g_inpf_m.inpf006,g_inpf_m.inpf007,g_inpf_m.inpf002, 
       g_inpf_m.inpf009,g_inpf_m.inpfownid,g_inpf_m.inpfowndp,g_inpf_m.inpfcrtid,g_inpf_m.inpfcrtdp, 
       g_inpf_m.inpfcrtdt,g_inpf_m.inpfmodid,g_inpf_m.inpfmoddt,g_inpf_m.inpfcnfid,g_inpf_m.inpfcnfdt, 
       g_inpf_m.inpfpstid,g_inpf_m.inpfpstdt,g_inpf_m.inpf022_desc,g_inpf_m.inpf006_desc,g_inpf_m.inpf002_desc, 
       g_inpf_m.inpf009_desc,g_inpf_m.inpfownid_desc,g_inpf_m.inpfowndp_desc,g_inpf_m.inpfcrtid_desc, 
       g_inpf_m.inpfcrtdp_desc,g_inpf_m.inpfmodid_desc,g_inpf_m.inpfcnfid_desc,g_inpf_m.inpfpstid_desc 

   
   
   #檢查是否允許此動作
   IF NOT aint836_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_inpf_m_mask_o.* =  g_inpf_m.*
   CALL aint836_inpf_t_mask()
   LET g_inpf_m_mask_n.* =  g_inpf_m.*
   
   CALL aint836_show()
   
   #add-point:delete段before ask name="delete.before_ask"
   {<point name="delete.before_ask"/>}
   #end add-point 
 
   IF cl_ask_del_master() THEN              #確認一下
   
      #add-point:單頭刪除前 name="delete.head.b_delete"
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point   
      
      #應用 a47 樣板自動產生(Version:4)
