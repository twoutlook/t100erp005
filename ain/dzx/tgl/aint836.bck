PRIVATE FUNCTION aint836_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   {<point name="modify.define_customerization" edit="c" mark="Y"/>}
   #end add-point    
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   DEFINE l_wc2_table1          STRING
 
 
   #add-point:modify段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   {<point name="modify.define"/>} 
   #end add-point    
   
   #add-point:Function前置處理  name="modify.pre_function"
   {<point name="modify.pre_function"/>}
   #end add-point
   
   #保存單頭舊值
   LET g_inpf_m_t.* = g_inpf_m.*
   LET g_inpf_m_o.* = g_inpf_m.*
   
   IF g_inpf_m.inpfsite IS NULL
   OR g_inpf_m.inpfdocno IS NULL
   OR g_inpf_m.inpfseq IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code = "std-00003" 
      LET g_errparam.popup = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_inpfsite_t = g_inpf_m.inpfsite
   LET g_inpfdocno_t = g_inpf_m.inpfdocno
   LET g_inpfseq_t = g_inpf_m.inpfseq
 
   CALL s_transaction_begin()
   
   OPEN aint836_cl USING g_enterprise,g_inpf_m.inpfsite,g_inpf_m.inpfdocno,g_inpf_m.inpfseq
   IF SQLCA.SQLCODE THEN   #(ver:78)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aint836_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE   #(ver:78)
      LET g_errparam.popup = TRUE 
      CLOSE aint836_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aint836_master_referesh USING g_inpf_m.inpfsite,g_inpf_m.inpfdocno,g_inpf_m.inpfseq INTO g_inpf_m.inpfdocno, 
       g_inpf_m.inpf004,g_inpf_m.inpf022,g_inpf_m.inpf005,g_inpf_m.inpf023,g_inpf_m.inpfseq,g_inpf_m.inpfsite, 
       g_inpf_m.inpfstus,g_inpf_m.inpf001,g_inpf_m.inpf003,g_inpf_m.inpf006,g_inpf_m.inpf007,g_inpf_m.inpf002, 
       g_inpf_m.inpf009,g_inpf_m.inpfownid,g_inpf_m.inpfowndp,g_inpf_m.inpfcrtid,g_inpf_m.inpfcrtdp, 
       g_inpf_m.inpfcrtdt,g_inpf_m.inpfmodid,g_inpf_m.inpfmoddt,g_inpf_m.inpfcnfid,g_inpf_m.inpfcnfdt, 
       g_inpf_m.inpfpstid,g_inpf_m.inpfpstdt,g_inpf_m.inpf022_desc,g_inpf_m.inpf006_desc,g_inpf_m.inpf002_desc, 
       g_inpf_m.inpf009_desc,g_inpf_m.inpfownid_desc,g_inpf_m.inpfowndp_desc,g_inpf_m.inpfcrtid_desc, 
       g_inpf_m.inpfcrtdp_desc,g_inpf_m.inpfmodid_desc,g_inpf_m.inpfcnfid_desc,g_inpf_m.inpfpstid_desc 

   
   #檢查是否允許此動作
   IF NOT aint836_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_inpf_m_mask_o.* =  g_inpf_m.*
   CALL aint836_inpf_t_mask()
   LET g_inpf_m_mask_n.* =  g_inpf_m.*
   
   
   
   #add-point:modify段show之前 name="modify.before_show"
   {<point name="modify.before_show"/>}
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
 
   
   CALL aint836_show()
   #add-point:modify段show之後 name="modify.after_show"
   {<point name="modify.after_show"/>}
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
 
 
    
   WHILE TRUE
      LET g_inpfsite_t = g_inpf_m.inpfsite
      LET g_inpfdocno_t = g_inpf_m.inpfdocno
      LET g_inpfseq_t = g_inpf_m.inpfseq
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_inpf_m.inpfmodid = g_user 
LET g_inpf_m.inpfmoddt = cl_get_current()
LET g_inpf_m.inpfmodid_desc = cl_get_username(g_inpf_m.inpfmodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      LET g_master_commit = "N"
      CALL aint836_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後 name="modify.after_input"
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE inpf_t SET (inpfmodid,inpfmoddt) = (g_inpf_m.inpfmodid,g_inpf_m.inpfmoddt)
          WHERE inpfent = g_enterprise AND inpfsite = g_inpfsite_t
            AND inpfdocno = g_inpfdocno_t
            AND inpfseq = g_inpfseq_t
 
      END IF
    
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         #若單頭無commit則還原
         IF g_master_commit = "N" THEN
            LET g_inpf_m.* = g_inpf_m_t.*
            CALL aint836_show()
         END IF
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code = 9001 
         LET g_errparam.popup = FALSE 
         CALL cl_err()
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_inpf_m.inpfsite != g_inpf_m_t.inpfsite
      OR g_inpf_m.inpfdocno != g_inpf_m_t.inpfdocno
      OR g_inpf_m.inpfseq != g_inpf_m_t.inpfseq
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前 name="modify.body.b_fk_update"
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE inpg_t SET inpgsite = g_inpf_m.inpfsite
                                       ,inpgdocno = g_inpf_m.inpfdocno
                                       ,inpgseq = g_inpf_m.inpfseq
 
          WHERE inpgent = g_enterprise AND inpgsite = g_inpf_m_t.inpfsite
            AND inpgdocno = g_inpf_m_t.inpfdocno
            AND inpgseq = g_inpf_m_t.inpfseq
 
            
         #add-point:單身fk修改中 name="modify.body.m_fk_update"
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            #   INITIALIZE g_errparam TO NULL 
            #   LET g_errparam.extend = "inpg_t" 
            #   LET g_errparam.code = "std-00009" 
            #   LET g_errparam.popup = TRUE 
            #   CALL cl_err()
            #   CALL s_transaction_end('N','0')
            #   CONTINUE WHILE
            WHEN SQLCA.SQLCODE #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "inpg_t:",SQLERRMESSAGE 
               LET g_errparam.code = SQLCA.SQLCODE 
               LET g_errparam.popup = TRUE 
               CALL s_transaction_end('N','0')
               CALL cl_err()
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後 name="modify.body.a_fk_update"
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL aint836_set_act_visible()   
   CALL aint836_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " inpfent = " ||g_enterprise|| " AND",
                      " inpfsite = '", g_inpf_m.inpfsite, "' "
                      ," AND inpfdocno = '", g_inpf_m.inpfdocno, "' "
                      ," AND inpfseq = '", g_inpf_m.inpfseq, "' "
 
   #填到對應位置
   CALL aint836_browser_fill("")
 
   CLOSE aint836_cl
   
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aint836_msgcentre_notify('modify')
 
END FUNCTION 

