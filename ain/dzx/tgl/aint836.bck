            CALL s_transaction_begin()
            OPEN aint836_cl USING g_enterprise,g_inpf_m.inpfsite,g_inpf_m.inpfdocno,g_inpf_m.inpfseq
            IF SQLCA.SQLCODE THEN   #(ver:78)
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "OPEN aint836_cl:",SQLERRMESSAGE 
               LET g_errparam.code = SQLCA.SQLCODE   #(ver:78)
               LET g_errparam.popup = TRUE 
               CLOSE aint836_cl
               CALL s_transaction_end('N','0')
               CALL cl_err()
               RETURN
            END IF
            
            LET g_rec_b = g_inpg_d.getLength()
            
            IF g_rec_b >= l_ac 
               AND g_inpg_d[l_ac].inpgseq1 IS NOT NULL
               AND g_inpg_d[l_ac].inpgseq2 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_inpg_d_t.* = g_inpg_d[l_ac].*  #BACKUP
               LET g_inpg_d_o.* = g_inpg_d[l_ac].*  #BACKUP
               CALL aint836_set_entry_b(l_cmd)
