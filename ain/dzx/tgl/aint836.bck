   {<point name="main.define"/>}
   #end add-point   
   
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("ain","")
 
   #add-point:作業初始化 name="main.init"
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define name="main.define_sql"
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT inpfdocno,inpf004,inpf022,'','',inpf005,inpf023,inpfseq,inpfsite,inpfstus, 
       inpf001,inpf003,'','',inpf006,'',inpf007,inpf002,'','',inpf009,'',inpfownid,'',inpfowndp,'',inpfcrtid, 
       '',inpfcrtdp,'',inpfcrtdt,inpfmodid,'',inpfmoddt,inpfcnfid,'',inpfcnfdt,inpfpstid,'',inpfpstdt", 
        
                      " FROM inpf_t",
                      " WHERE inpfent= ? AND inpfsite=? AND inpfdocno=? AND inpfseq=? FOR UPDATE"
   #add-point:SQL_define name="main.after_define_sql"
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE aint836_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.inpfdocno,t0.inpf004,t0.inpf022,t0.inpf005,t0.inpf023,t0.inpfseq, 
       t0.inpfsite,t0.inpfstus,t0.inpf001,t0.inpf003,t0.inpf006,t0.inpf007,t0.inpf002,t0.inpf009,t0.inpfownid, 
       t0.inpfowndp,t0.inpfcrtid,t0.inpfcrtdp,t0.inpfcrtdt,t0.inpfmodid,t0.inpfmoddt,t0.inpfcnfid,t0.inpfcnfdt, 
       t0.inpfpstid,t0.inpfpstdt,t1.ooag011 ,t2.pmaal003 ,t3.oocql004 ,t4.ecba003 ,t5.ooag011 ,t6.ooefl003 , 
       t7.ooag011 ,t8.ooefl003 ,t9.ooag011 ,t10.ooag011 ,t11.ooag011",
               " FROM inpf_t t0",
                              " LEFT JOIN ooag_t t1 ON t1.ooagent="||g_enterprise||" AND t1.ooag001=t0.inpf022  ",
               " LEFT JOIN pmaal_t t2 ON t2.pmaalent="||g_enterprise||" AND t2.pmaal001=t0.inpf006 AND t2.pmaal002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t3 ON t3.oocqlent="||g_enterprise||" AND t3.oocql001='221' AND t3.oocql002=t0.inpf002 AND t3.oocql003='"||g_dlang||"' ",
               " LEFT JOIN ecba_t t4 ON t4.ecbaent="||g_enterprise||" AND t4.ecbasite=t0.inpfsite AND t4.ecba001=t0.inpf003 AND t4.ecba002=t0.inpf009  ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=t0.inpfownid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent="||g_enterprise||" AND t6.ooefl001=t0.inpfowndp AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent="||g_enterprise||" AND t7.ooag001=t0.inpfcrtid  ",
               " LEFT JOIN ooefl_t t8 ON t8.ooeflent="||g_enterprise||" AND t8.ooefl001=t0.inpfcrtdp AND t8.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t9 ON t9.ooagent="||g_enterprise||" AND t9.ooag001=t0.inpfmodid  ",
               " LEFT JOIN ooag_t t10 ON t10.ooagent="||g_enterprise||" AND t10.ooag001=t0.inpfcnfid  ",
               " LEFT JOIN ooag_t t11 ON t11.ooagent="||g_enterprise||" AND t11.ooag001=t0.inpfpstid  ",
 
               " WHERE t0.inpfent = " ||g_enterprise|| " AND t0.inpfsite = ? AND t0.inpfdocno = ? AND t0.inpfseq = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define name="main.after_refresh_sql"
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE aint836_master_referesh FROM g_sql
 
    
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call name="main.servicecall"
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_aint836 WITH FORM cl_ap_formpath("ain",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL aint836_init()   
 
      #進入選單 Menu (="N")
