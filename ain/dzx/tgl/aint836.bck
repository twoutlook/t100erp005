            END IF
               
            #add-point:單身新增 name="input.body.b_a_insert"
            {<point name="input.body.b_a_insert"/>}
            #end add-point
               
            LET l_count = 1  
            SELECT COUNT(1) INTO l_count FROM inpg_t 
             WHERE inpgent = g_enterprise AND inpgsite = g_inpf_m.inpfsite
               AND inpgdocno = g_inpf_m.inpfdocno
               AND inpgseq = g_inpf_m.inpfseq
 
               AND inpgseq1 = g_inpg_d[l_ac].inpgseq1
               AND inpgseq2 = g_inpg_d[l_ac].inpgseq2
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前 name="input.body.b_insert"
               {<point name="input.body.b_insert"/>}
               #end add-point
            
               #同步新增到同層的table
                              INITIALIZE gs_keys TO NULL 
