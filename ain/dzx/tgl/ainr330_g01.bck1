      LET g_errparam.code   = STATUS
      LET g_errparam.popup  = TRUE
      CALL cl_err()   
      LET g_rep_success = 'N'    
   END IF
   DECLARE ainr330_g01_curs CURSOR FOR ainr330_g01_prepare
 
END FUNCTION

