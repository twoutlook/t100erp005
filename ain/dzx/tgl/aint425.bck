            DISPLAY aint425_filter_parser('infiunit') TO s_browse[1].b_infiunit
            DISPLAY aint425_filter_parser('infidocdt') TO s_browse[1].b_infidocdt
            DISPLAY aint425_filter_parser('infidocno') TO s_browse[1].b_infidocno
