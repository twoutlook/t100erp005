                  WHEN SQLCA.SQLCODE #其他錯誤
                     LET g_infj_d[l_ac].* = g_infj_d_t.*  
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "infj_t:",SQLERRMESSAGE 
                     LET g_errparam.code = SQLCA.SQLCODE 
                     LET g_errparam.popup = TRUE 
                     CALL s_transaction_end('N','0')
                     CALL cl_err()                   
                     
                  OTHERWISE
                     #資料多語言用-增/改
                     
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_infi_m.infidocno
               LET gs_keys_bak[1] = g_infidocno_t
               LET gs_keys[2] = g_infj_d[g_detail_idx].infjseq
               LET gs_keys_bak[2] = g_infj_d_t.infjseq
