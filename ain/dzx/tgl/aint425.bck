               #將遮罩欄位還原
               CALL aint425_infj_t_mask_restore('restore_mask_o')
      
               UPDATE infj_t SET (infjdocno,infjseq,infj002,infj001,infj003,infj004,infj005,infj006, 
                   infj007,infj008,infjsite,infjunit) = (g_infi_m.infidocno,g_infj_d[l_ac].infjseq,g_infj_d[l_ac].infj002, 
                   g_infj_d[l_ac].infj001,g_infj_d[l_ac].infj003,g_infj_d[l_ac].infj004,g_infj_d[l_ac].infj005, 
                   g_infj_d[l_ac].infj006,g_infj_d[l_ac].infj007,g_infj_d[l_ac].infj008,g_infj_d[l_ac].infjsite, 
                   g_infj_d[l_ac].infjunit)
                WHERE infjent = g_enterprise AND infjdocno = g_infi_m.infidocno 
 
                  AND infjseq = g_infj_d_t.infjseq #項次   
 
                  
               #add-point:單身修改中 name="input.body.m_update"
               {<point name="input.body.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     LET g_infj_d[l_ac].* = g_infj_d_t.*
