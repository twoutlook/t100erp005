PRIVATE FUNCTION aint425_fetch(p_flag)
   #add-point:fetch段define(客製用) name="fetch.define_customerization"
   {<point name="fetch.define_customerization" edit="c" mark="Y"/>}
   #end add-point    
   DEFINE p_flag     LIKE type_t.chr1
   DEFINE ls_msg     STRING
   #add-point:fetch段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="fetch.define"
   {<point name="fetch.define"/>} 
   #end add-point    
   
   #add-point:Function前置處理  name="fetch.pre_function"
   {<point name="fetch.pre_function"/>}
   #end add-point
   
   IF g_browser_cnt = 0 THEN
      RETURN
   END IF
 
   #清空第二階單身
 
   
   CALL cl_ap_performance_next_start()
   CASE p_flag
      WHEN 'F' 
         LET g_current_idx = 1
      WHEN 'L'  
         LET g_current_idx = g_browser.getLength()              
      WHEN 'P'
         IF g_current_idx > 1 THEN               
            LET g_current_idx = g_current_idx - 1
         END IF 
      WHEN 'N'
         IF g_current_idx < g_header_cnt THEN
            LET g_current_idx =  g_current_idx + 1
         END IF        
      WHEN '/'
         IF (NOT g_no_ask) THEN    
            CALL cl_set_act_visible("accept,cancel", TRUE)    
            CALL cl_getmsg('fetch',g_lang) RETURNING ls_msg
            LET INT_FLAG = 0
 
            PROMPT ls_msg CLIPPED,':' FOR g_jump
               #交談指令共用ACTION
               &include "common_action.4gl" 
            END PROMPT
 
            CALL cl_set_act_visible("accept,cancel", FALSE)    
            IF INT_FLAG THEN
                LET INT_FLAG = 0
                EXIT CASE  
            END IF           
         END IF
         
         IF g_jump > 0 AND g_jump <= g_browser.getLength() THEN
             LET g_current_idx = g_jump
         END IF
         LET g_no_ask = FALSE  
   END CASE 
 
   CALL g_curr_diag.setCurrentRow("s_browse", g_current_idx) #設定browse 索引
   
   LET g_current_row = g_current_idx
   LET g_detail_cnt = g_header_cnt                  
   
   #單身總筆數顯示
   IF g_detail_cnt > 0 THEN
      #若單身有資料時, idx至少為1
      IF g_detail_idx <= 0 THEN
         LET g_detail_idx = 1
      END IF
      DISPLAY g_detail_idx TO FORMONLY.idx  
   ELSE
      LET g_detail_idx = 0
      DISPLAY '' TO FORMONLY.idx    
   END IF
   
   #瀏覽頁筆數顯示
   LET g_browser_idx = g_pagestart+g_current_idx-1
   DISPLAY g_browser_idx TO FORMONLY.b_index   #當下筆數
   DISPLAY g_browser_idx TO FORMONLY.h_index   #當下筆數
   
   CALL cl_navigator_setting( g_current_idx, g_browser_cnt )
 
   #代表沒有資料
   IF g_current_idx = 0 OR g_browser.getLength() = 0 THEN
      RETURN
   END IF
   
   #避免超出browser資料筆數上限
   IF g_current_idx > g_browser.getLength() THEN
      LET g_browser_idx = g_browser.getLength()
      LET g_current_idx = g_browser.getLength()
   END IF
   
   LET g_infi_m.infidocno = g_browser[g_current_idx].b_infidocno
 
   
   #重讀DB,因TEMP有不被更新特性
   EXECUTE aint425_master_referesh USING g_infi_m.infidocno INTO g_infi_m.infisite,g_infi_m.infidocdt, 
       g_infi_m.infidocno,g_infi_m.infi001,g_infi_m.infi002,g_infi_m.infi003,g_infi_m.infi004,g_infi_m.infiunit, 
       g_infi_m.infistus,g_infi_m.infiownid,g_infi_m.infiowndp,g_infi_m.inficrtid,g_infi_m.inficrtdp, 
       g_infi_m.inficrtdt,g_infi_m.infimodid,g_infi_m.infimoddt,g_infi_m.inficnfid,g_infi_m.inficnfdt, 
       g_infi_m.infisite_desc,g_infi_m.infi002_desc,g_infi_m.infi003_desc,g_infi_m.infi004_desc,g_infi_m.infiownid_desc, 
       g_infi_m.infiowndp_desc,g_infi_m.inficrtid_desc,g_infi_m.inficrtdp_desc,g_infi_m.infimodid_desc, 
       g_infi_m.inficnfid_desc
   
   #遮罩相關處理
   LET g_infi_m_mask_o.* =  g_infi_m.*
   CALL aint425_infi_t_mask()
   LET g_infi_m_mask_n.* =  g_infi_m.*
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL aint425_set_act_visible()   
   CALL aint425_set_act_no_visible()
   
   #add-point:fetch段action控制 name="fetch.action_control"
   {<point name="fetch.action_control"/>}
   #end add-point  
   
   
   
   #add-point:fetch結束前 name="fetch.after"
   {<point name="fetch.after" />}
   #end add-point
   
   #保存單頭舊值
   LET g_infi_m_t.* = g_infi_m.*
   LET g_infi_m_o.* = g_infi_m.*
   
   LET g_data_owner = g_infi_m.infiownid      
   LET g_data_dept  = g_infi_m.infiowndp
   
   #重新顯示   
   CALL aint425_show()
 
   
 
END FUNCTION

{</section>}

{<section id="aint425.insert" >}
#+ 資料新增
PRIVATE FUNCTION aint425_insert()
   #add-point:insert段define(客製用) name="insert.define_customerization"
   {<point name="insert.define_customerization" edit="c" mark="Y"/>}
   #end add-point    
   #add-point:insert段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="insert.define"
   {<point name="insert.define"/>} 
   #end add-point    
   
   #add-point:Function前置處理  name="insert.pre_function"
   {<point name="insert.pre_function"/>}
   #end add-point
   
   #清畫面欄位內容
   CLEAR FORM                    
   CALL g_infj_d.clear()   
 
 
   INITIALIZE g_infi_m.* TO NULL             #DEFAULT 設定
   
   LET g_infidocno_t = NULL
 
   
   LET g_master_insert = FALSE
   
   #add-point:insert段before name="insert.before"
   {<point name="insert.before"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   WHILE TRUE
      #公用欄位給值(單頭)
      #應用 a14 樣板自動產生(Version:5)    
      #公用欄位新增給值  
      LET g_infi_m.infiownid = g_user
      LET g_infi_m.infiowndp = g_dept
      LET g_infi_m.inficrtid = g_user
      LET g_infi_m.inficrtdp = g_dept 
      LET g_infi_m.inficrtdt = cl_get_current()
      LET g_infi_m.infimodid = g_user
      LET g_infi_m.infimoddt = cl_get_current()
      LET g_infi_m.infistus = 'N'
 
 
 
 
      #append欄位給值
      
     
      #一般欄位給值
      
  
      #add-point:單頭預設值 name="insert.default"
      {<point name="insert.default"/>}
      #end add-point 
      
      #保存單頭舊值(用於資料輸入錯誤還原預設值時使用)
      LET g_infi_m_t.* = g_infi_m.*
      LET g_infi_m_o.* = g_infi_m.*
      
      #顯示狀態(stus)圖片
            #應用 a21 樣板自動產生(Version:3)
	  #根據當下狀態碼顯示圖片
      CASE g_infi_m.infistus 
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
 
 
 
    
      CALL aint425_input("a")
      
      #add-point:單頭輸入後 name="insert.after_insert"
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code = 9001 
         LET g_errparam.popup = FALSE 
         CALL s_transaction_end('N','0')
         CALL cl_err()
      END IF
      
      IF NOT g_master_insert THEN
         DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
         DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
         INITIALIZE g_infi_m.* TO NULL
         INITIALIZE g_infj_d TO NULL
 
         #add-point:取消新增後 name="insert.cancel"
         {<point name="insert.cancel"/>}
         #end add-point 
         CALL aint425_show()
         RETURN
      END IF
      
      LET INT_FLAG = 0
      #CALL g_infj_d.clear()
 
 
      LET g_rec_b = 0
      CALL s_transaction_end('Y','0')
      EXIT WHILE
        
   END WHILE
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL aint425_set_act_visible()   
   CALL aint425_set_act_no_visible()
   
   #將新增的資料併入搜尋條件中
   LET g_infidocno_t = g_infi_m.infidocno
 
   
   #組合新增資料的條件
   LET g_add_browse = " infient = " ||g_enterprise|| " AND",
                      " infidocno = '", g_infi_m.infidocno, "' "
 
                      
   #add-point:組合新增資料的條件後 name="insert.after.add_browse"
   {<point name="insert.after.add_browse"/>}
   #end add-point
      
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL aint425_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   CLOSE aint425_cl
   
   CALL aint425_idx_chk()
   
   #撈取異動後的資料(主要是帶出reference)
   EXECUTE aint425_master_referesh USING g_infi_m.infidocno INTO g_infi_m.infisite,g_infi_m.infidocdt, 
       g_infi_m.infidocno,g_infi_m.infi001,g_infi_m.infi002,g_infi_m.infi003,g_infi_m.infi004,g_infi_m.infiunit, 
       g_infi_m.infistus,g_infi_m.infiownid,g_infi_m.infiowndp,g_infi_m.inficrtid,g_infi_m.inficrtdp, 
       g_infi_m.inficrtdt,g_infi_m.infimodid,g_infi_m.infimoddt,g_infi_m.inficnfid,g_infi_m.inficnfdt, 
       g_infi_m.infisite_desc,g_infi_m.infi002_desc,g_infi_m.infi003_desc,g_infi_m.infi004_desc,g_infi_m.infiownid_desc, 
       g_infi_m.infiowndp_desc,g_infi_m.inficrtid_desc,g_infi_m.inficrtdp_desc,g_infi_m.infimodid_desc, 
       g_infi_m.inficnfid_desc
   
   
   #遮罩相關處理
   LET g_infi_m_mask_o.* =  g_infi_m.*
   CALL aint425_infi_t_mask()
   LET g_infi_m_mask_n.* =  g_infi_m.*
   
   #將資料顯示到畫面上
   DISPLAY BY NAME g_infi_m.infisite,g_infi_m.infisite_desc,g_infi_m.infidocdt,g_infi_m.infidocno,g_infi_m.infi001, 
       g_infi_m.infi002,g_infi_m.infi002_desc,g_infi_m.infi003,g_infi_m.infi003_desc,g_infi_m.infi004, 
       g_infi_m.infi004_desc,g_infi_m.infiunit,g_infi_m.infistus,g_infi_m.infiownid,g_infi_m.infiownid_desc, 
       g_infi_m.infiowndp,g_infi_m.infiowndp_desc,g_infi_m.inficrtid,g_infi_m.inficrtid_desc,g_infi_m.inficrtdp, 
       g_infi_m.inficrtdp_desc,g_infi_m.inficrtdt,g_infi_m.infimodid,g_infi_m.infimodid_desc,g_infi_m.infimoddt, 
       g_infi_m.inficnfid,g_infi_m.inficnfid_desc,g_infi_m.inficnfdt
   
   #add-point:新增結束後 name="insert.after"
   {<point name="insert.after"/>}
   #end add-point 
   
   LET g_data_owner = g_infi_m.infiownid      
   LET g_data_dept  = g_infi_m.infiowndp
   
   #功能已完成,通報訊息中心
   CALL aint425_msgcentre_notify('insert')
   
END FUNCTION

