       g_inps_m.inpscnfid_desc,g_inps_m.inpspstid_desc
   
   #檢查是否允許此動作
   IF NOT aint930_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_inps_m_mask_o.* =  g_inps_m.*
   CALL aint930_inps_t_mask()
   LET g_inps_m_mask_n.* =  g_inps_m.*
   
   
   
