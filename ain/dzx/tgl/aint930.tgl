{<section id="aint930.b_fill2" >}
#+ 單身陣列填充2
PRIVATE FUNCTION aint930_b_fill2(pi_idx)
   #add-point:b_fill2段define(客製用) name="b_fill2.define_customerization"
   {<point name="b_fill2.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE pi_idx                 LIKE type_t.num10
   DEFINE li_ac                  LIKE type_t.num10
   DEFINE li_detail_idx_tmp      LIKE type_t.num10
   DEFINE ls_chk                 LIKE type_t.chr1
   #add-point:b_fill2段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill2.define"
   {<point name="b_fill2.define"/>} 
   #end add-point
   
   #add-point:Function前置處理  name="b_fill2.pre_function"
   {<point name="b_fill2.pre_function"/>}
   #end add-point
   
   LET li_ac = l_ac 
   
   IF g_detail_idx <= 0 THEN
      RETURN
   END IF
   
   LET li_detail_idx_tmp = g_detail_idx
   
   IF aint930_fill_chk(2) THEN
      IF (pi_idx = 2 OR pi_idx = 0 ) AND g_inpl_d.getLength() > 0 THEN
               CALL g_inpl2_d.clear()
 
         
         #取得該單身上階單身的idx
         LET g_detail_idx = g_detail_idx_list[1]
         
         LET g_sql = "SELECT  DISTINCT inpmseq1,inpm001,inpm002,inpm003,inpm004,inpm005,inpm006,inpm007, 
             inpm008,inpm009,inpm010,inpm011,inpm012,inpm030,inpm031,inpm032,inpm033,inpm034,inpm050, 
             inpm051,inpm052,inpm053,inpm054 ,t10.imaal003 ,t11.imaal004 ,t12.ooag011 ,t13.ooag011 , 
             t14.ooag011 ,t15.ooag011 FROM inpm_t",    
                     "",
                                    " LEFT JOIN imaal_t t10 ON t10.imaalent="||g_enterprise||" AND t10.imaal001=inpm001 AND t10.imaal002='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t11 ON t11.imaalent="||g_enterprise||" AND t11.imaal001=inpm001 AND t11.imaal002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t12 ON t12.ooagent="||g_enterprise||" AND t12.ooag001=inpm031  ",
               " LEFT JOIN ooag_t t13 ON t13.ooagent="||g_enterprise||" AND t13.ooag001=inpm033  ",
               " LEFT JOIN ooag_t t14 ON t14.ooagent="||g_enterprise||" AND t14.ooag001=inpm051  ",
               " LEFT JOIN ooag_t t15 ON t15.ooagent="||g_enterprise||" AND t15.ooag001=inpm053  ",
 
                     " WHERE inpment=? AND inpmsite=? AND inpmdocno=? AND inpmseq=?"
         
         IF NOT cl_null(g_wc2_table2) THEN
            LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
         END IF
         
         LET g_sql = g_sql, " ORDER BY  inpm_t.inpmseq1" 
                            
         #add-point:單身填充前 name="b_fill2.before_fill2"
         {<point name="b_fill2.before_fill2" />}
         #end add-point
         
         #先清空資料
               CALL g_inpl2_d.clear()
 
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE aint930_pb2 FROM g_sql
         DECLARE b_fill_curs2 CURSOR FOR aint930_pb2
         
      #  OPEN b_fill_curs2 USING g_enterprise,g_inps_m.inpssite,g_inps_m.inpsdocno,g_inpl_d[g_detail_idx].inplseq  
      #      #(ver:78)
         LET l_ac = 1
         FOREACH b_fill_curs2 USING g_enterprise,g_inps_m.inpssite,g_inps_m.inpsdocno,g_inpl_d[g_detail_idx].inplseq INTO g_inpl2_d[l_ac].inpmseq1, 
             g_inpl2_d[l_ac].inpm001,g_inpl2_d[l_ac].inpm002,g_inpl2_d[l_ac].inpm003,g_inpl2_d[l_ac].inpm004, 
             g_inpl2_d[l_ac].inpm005,g_inpl2_d[l_ac].inpm006,g_inpl2_d[l_ac].inpm007,g_inpl2_d[l_ac].inpm008, 
             g_inpl2_d[l_ac].inpm009,g_inpl2_d[l_ac].inpm010,g_inpl2_d[l_ac].inpm011,g_inpl2_d[l_ac].inpm012, 
             g_inpl2_d[l_ac].inpm030,g_inpl2_d[l_ac].inpm031,g_inpl2_d[l_ac].inpm032,g_inpl2_d[l_ac].inpm033, 
             g_inpl2_d[l_ac].inpm034,g_inpl2_d[l_ac].inpm050,g_inpl2_d[l_ac].inpm051,g_inpl2_d[l_ac].inpm052, 
             g_inpl2_d[l_ac].inpm053,g_inpl2_d[l_ac].inpm054,g_inpl2_d[l_ac].inpm001_desc,g_inpl2_d[l_ac].inpm001_desc_desc, 
             g_inpl2_d[l_ac].inpm031_desc,g_inpl2_d[l_ac].inpm033_desc,g_inpl2_d[l_ac].inpm051_desc, 
             g_inpl2_d[l_ac].inpm053_desc   #(ver:78)
            IF SQLCA.SQLCODE THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
               LET g_errparam.code = SQLCA.SQLCODE 
               LET g_errparam.popup = TRUE 
               CALL cl_err()
               EXIT FOREACH
            END IF
            
            #add-point:b_fill段資料填充 name="b_fill2.fill2"
            {<point name="b_fill2.fill2"/>}
            #end add-point
           
            IF l_ac > g_max_rec THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code = 9035 
               LET g_errparam.popup = TRUE 
               CALL cl_err()
               EXIT FOREACH
            END IF
            
            LET l_ac = l_ac + 1
            
         END FOREACH
               CALL g_inpl2_d.deleteElement(g_inpl2_d.getLength())
 
      END IF
   END IF
 
 
      
   LET g_inpl2_d_mask_o.* =  g_inpl2_d.*
   FOR l_ac = 1 TO g_inpl2_d.getLength()
      LET g_inpl2_d_mask_o[l_ac].* =  g_inpl2_d[l_ac].*
      CALL aint930_inpm_t_mask()
      LET g_inpl2_d_mask_n[l_ac].* =  g_inpl2_d[l_ac].*
   END FOR
 
      
   #add-point:單身填充後 name="b_fill2.after_fill"
   {<point name="b_fill2.after_fill" />}
   #end add-point
    
   LET l_ac = li_ac
   
   CALL aint930_detail_show()
   
   LET g_detail_idx = li_detail_idx_tmp
   
END FUNCTION

