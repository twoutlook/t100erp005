{<section id="aint930.fetch" >}
#+ 指定PK後抓取單頭其他資料
PRIVATE FUNCTION aint930_fetch(p_flag)
   #add-point:fetch段define(客製用) name="fetch.define_customerization"
   {<point name="fetch.define_customerization" edit="c" mark="Y"/>}
   #end add-point    
   DEFINE p_flag     LIKE type_t.chr1
   DEFINE ls_msg     STRING
   #add-point:fetch段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="fetch.define"
   {<point name="fetch.define"/>} 
   #end add-point    
   
   #add-point:Function前置處理  name="fetch.pre_function"
   {<point name="fetch.pre_function"/>}
   #end add-point
   
   IF g_browser_cnt = 0 THEN
      RETURN
   END IF
 
   #清空第二階單身
   CALL g_inpl2_d.clear()
 
   
   CALL cl_ap_performance_next_start()
   CASE p_flag
      WHEN 'F' 
         LET g_current_idx = 1
      WHEN 'L'  
         LET g_current_idx = g_browser.getLength()              
      WHEN 'P'
         IF g_current_idx > 1 THEN               
            LET g_current_idx = g_current_idx - 1
         END IF 
      WHEN 'N'
         IF g_current_idx < g_header_cnt THEN
            LET g_current_idx =  g_current_idx + 1
         END IF        
      WHEN '/'
         IF (NOT g_no_ask) THEN    
            CALL cl_set_act_visible("accept,cancel", TRUE)    
            CALL cl_getmsg('fetch',g_lang) RETURNING ls_msg
            LET INT_FLAG = 0
 
            PROMPT ls_msg CLIPPED,':' FOR g_jump
               #交談指令共用ACTION
               &include "common_action.4gl" 
            END PROMPT
 
            CALL cl_set_act_visible("accept,cancel", FALSE)    
            IF INT_FLAG THEN
                LET INT_FLAG = 0
                EXIT CASE  
            END IF           
         END IF
         
         IF g_jump > 0 AND g_jump <= g_browser.getLength() THEN
             LET g_current_idx = g_jump
         END IF
         LET g_no_ask = FALSE  
   END CASE 
 
   CALL g_curr_diag.setCurrentRow("s_browse", g_current_idx) #設定browse 索引
   
   LET g_current_row = g_current_idx
   LET g_detail_cnt = g_header_cnt                  
   
   #單身總筆數顯示
   IF g_detail_cnt > 0 THEN
      #若單身有資料時, idx至少為1
      IF g_detail_idx <= 0 THEN
         LET g_detail_idx = 1
      END IF
      DISPLAY g_detail_idx TO FORMONLY.idx  
   ELSE
      LET g_detail_idx = 0
      DISPLAY '' TO FORMONLY.idx    
   END IF
   
   #瀏覽頁筆數顯示
   LET g_browser_idx = g_pagestart+g_current_idx-1
   DISPLAY g_browser_idx TO FORMONLY.b_index   #當下筆數
   DISPLAY g_browser_idx TO FORMONLY.h_index   #當下筆數
   
   CALL cl_navigator_setting( g_current_idx, g_browser_cnt )
 
   #代表沒有資料
   IF g_current_idx = 0 OR g_browser.getLength() = 0 THEN
      RETURN
   END IF
   
   #避免超出browser資料筆數上限
   IF g_current_idx > g_browser.getLength() THEN
      LET g_browser_idx = g_browser.getLength()
      LET g_current_idx = g_browser.getLength()
   END IF
   
   LET g_inps_m.inpssite = g_browser[g_current_idx].b_inpssite
   LET g_inps_m.inpsdocno = g_browser[g_current_idx].b_inpsdocno
 
   
   #重讀DB,因TEMP有不被更新特性
   EXECUTE aint930_master_referesh USING g_inps_m.inpssite,g_inps_m.inpsdocno INTO g_inps_m.inpsdocno, 
       g_inps_m.inps007,g_inps_m.inps006,g_inps_m.inps001,g_inps_m.inps005,g_inps_m.inps008,g_inps_m.inpssite, 
       g_inps_m.inpsstus,g_inps_m.inpsownid,g_inps_m.inpsowndp,g_inps_m.inpscrtid,g_inps_m.inpscrtdp, 
       g_inps_m.inpscrtdt,g_inps_m.inpsmodid,g_inps_m.inpsmoddt,g_inps_m.inpscnfid,g_inps_m.inpscnfdt, 
       g_inps_m.inpspstid,g_inps_m.inpspstdt,g_inps_m.inps006_desc,g_inps_m.inps005_desc,g_inps_m.inpsownid_desc, 
       g_inps_m.inpsowndp_desc,g_inps_m.inpscrtid_desc,g_inps_m.inpscrtdp_desc,g_inps_m.inpsmodid_desc, 
       g_inps_m.inpscnfid_desc,g_inps_m.inpspstid_desc
   
   #遮罩相關處理
   LET g_inps_m_mask_o.* =  g_inps_m.*
   CALL aint930_inps_t_mask()
   LET g_inps_m_mask_n.* =  g_inps_m.*
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL aint930_set_act_visible()   
   CALL aint930_set_act_no_visible()
   
   #add-point:fetch段action控制 name="fetch.action_control"
   {<point name="fetch.action_control"/>}
   #end add-point  
   
   
   
   #add-point:fetch結束前 name="fetch.after"
   {<point name="fetch.after" />}
   #end add-point
   
   #保存單頭舊值
   LET g_inps_m_t.* = g_inps_m.*
   LET g_inps_m_o.* = g_inps_m.*
   
   LET g_data_owner = g_inps_m.inpsownid      
   LET g_data_dept  = g_inps_m.inpsowndp
   
   #重新顯示   
   CALL aint930_show()
 
   #應用 a56 樣板自動產生(Version:3)
   #檢查此單據是否需顯示BPM簽核狀況按鈕 
   IF cl_bpm_chk() THEN
      CALL cl_set_act_visible("bpm_status",TRUE)
   ELSE
      CALL cl_set_act_visible("bpm_status",FALSE)
   END IF
 
 
 
 
 
