              " OR (",
              " inegsite = '", g_ineg_m.inegsite CLIPPED, "' "
              ," AND inegdocno = '", g_ineg_m.inegdocno CLIPPED, "' "
 
              , ") "
   
   LET g_inegsite_t = g_ineg_m.inegsite
