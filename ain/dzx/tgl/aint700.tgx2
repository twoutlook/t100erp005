   LET g_indidocno_t = g_indi_m.indidocno
 
   CALL s_transaction_begin()
   
   OPEN aint700_cl USING g_enterprise,g_indi_m.indidocno
   IF SQLCA.SQLCODE THEN   #(ver:78)
      INITIALIZE g_errparam TO NULL 
