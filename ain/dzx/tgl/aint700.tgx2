         #若有modid跟moddt則進行update
         UPDATE indi_t SET (indimodid,indimoddt) = (g_indi_m.indimodid,g_indi_m.indimoddt)
          WHERE indient = g_enterprise AND indidocno = g_indidocno_t
 
      END IF
    
      IF INT_FLAG THEN
