   EXECUTE aint700_master_referesh USING g_indi_m.indidocno INTO g_indi_m.indisite,g_indi_m.indidocdt, 
       g_indi_m.indidocno,g_indi_m.indi006,g_indi_m.indi000,g_indi_m.indiunit,g_indi_m.indi001,g_indi_m.indi002, 
