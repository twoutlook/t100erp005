   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON indisite,indidocdt,indidocno,indiunit,indi001,indi002,indi003,indi004, 
          indi008
                          FROM s_browse[1].b_indisite,s_browse[1].b_indidocdt,s_browse[1].b_indidocno, 
                              s_browse[1].b_indiunit,s_browse[1].b_indi001,s_browse[1].b_indi002,s_browse[1].b_indi003, 
                              s_browse[1].b_indi004,s_browse[1].b_indi008
 
         BEFORE CONSTRUCT
               DISPLAY aint700_filter_parser('indisite') TO s_browse[1].b_indisite
            DISPLAY aint700_filter_parser('indidocdt') TO s_browse[1].b_indidocdt
            DISPLAY aint700_filter_parser('indidocno') TO s_browse[1].b_indidocno
            DISPLAY aint700_filter_parser('indiunit') TO s_browse[1].b_indiunit
            DISPLAY aint700_filter_parser('indi001') TO s_browse[1].b_indi001
            DISPLAY aint700_filter_parser('indi002') TO s_browse[1].b_indi002
            DISPLAY aint700_filter_parser('indi003') TO s_browse[1].b_indi003
            DISPLAY aint700_filter_parser('indi004') TO s_browse[1].b_indi004
            DISPLAY aint700_filter_parser('indi008') TO s_browse[1].b_indi008
      
