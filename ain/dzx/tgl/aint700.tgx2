   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define name="main.define_sql"
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT indisite,'',indidocdt,indidocno,indi006,'',indi000,indiunit,'',indi001, 
       indi002,'',indi007,indi003,'',indi004,'',indi008,indi005,indistus,indiownid,'',indiowndp,'',indicrtid, 
       '',indicrtdp,'',indicrtdt,indimodid,'',indimoddt,indicnfid,'',indicnfdt", 
                      " FROM indi_t",
                      " WHERE indient= ? AND indidocno=? FOR UPDATE"
   #add-point:SQL_define name="main.after_define_sql"
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE aint700_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.indisite,t0.indidocdt,t0.indidocno,t0.indi006,t0.indi000,t0.indiunit, 
       t0.indi001,t0.indi002,t0.indi007,t0.indi003,t0.indi004,t0.indi008,t0.indi005,t0.indistus,t0.indiownid, 
       t0.indiowndp,t0.indicrtid,t0.indicrtdp,t0.indicrtdt,t0.indimodid,t0.indimoddt,t0.indicnfid,t0.indicnfdt, 
       t1.ooefl003 ,t2.oocql004 ,t3.ooefl003 ,t4.dbabl003 ,t5.ooag011 ,t6.ooefl003 ,t7.ooag011 ,t8.ooefl003 , 
       t9.ooag011 ,t10.ooefl003 ,t11.ooag011 ,t12.ooag011",
               " FROM indi_t t0",
                              " LEFT JOIN ooefl_t t1 ON t1.ooeflent="||g_enterprise||" AND t1.ooefl001=t0.indisite AND t1.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t2 ON t2.oocqlent="||g_enterprise||" AND t2.oocql001='274' AND t2.oocql002=t0.indi006 AND t2.oocql003='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent="||g_enterprise||" AND t3.ooefl001=t0.indiunit AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN dbabl_t t4 ON t4.dbablent="||g_enterprise||" AND t4.dbabl001=t0.indi002 AND t4.dbabl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=t0.indi003  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent="||g_enterprise||" AND t6.ooefl001=t0.indi004 AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent="||g_enterprise||" AND t7.ooag001=t0.indiownid  ",
               " LEFT JOIN ooefl_t t8 ON t8.ooeflent="||g_enterprise||" AND t8.ooefl001=t0.indiowndp AND t8.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t9 ON t9.ooagent="||g_enterprise||" AND t9.ooag001=t0.indicrtid  ",
               " LEFT JOIN ooefl_t t10 ON t10.ooeflent="||g_enterprise||" AND t10.ooefl001=t0.indicrtdp AND t10.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t11 ON t11.ooagent="||g_enterprise||" AND t11.ooag001=t0.indimodid  ",
               " LEFT JOIN ooag_t t12 ON t12.ooagent="||g_enterprise||" AND t12.ooag001=t0.indicnfid  ",
 
               " WHERE t0.indient = " ||g_enterprise|| " AND t0.indidocno = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define name="main.after_refresh_sql"
   {<point name="main.after_refresh_sql"/>}
   #end add-point
