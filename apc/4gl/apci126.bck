   LET ls_wc = g_wc2
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      CONSTRUCT g_wc2 ON pcadstus,pcad001,pcadl003,pcad002,pcad003,pcad004,pcad005,pcad006,pcad007,pcad008, 
          pcadstamp,pcadpos,pcadunit,pcadownid,pcadowndp,pcadcrtid,pcadcrtdp,pcadcrtdt,pcadmodid,pcadmoddt  
 
 
         FROM s_detail1[1].pcadstus,s_detail1[1].pcad001,s_detail1[1].pcadl003,s_detail1[1].pcad002, 
             s_detail1[1].pcad003,s_detail1[1].pcad004,s_detail1[1].pcad005,s_detail1[1].pcad006,s_detail1[1].pcad007, 
             s_detail1[1].pcad008,s_detail1[1].pcadstamp,s_detail1[1].pcadpos,s_detail1[1].pcadunit, 
             s_detail2[1].pcadownid,s_detail2[1].pcadowndp,s_detail2[1].pcadcrtid,s_detail2[1].pcadcrtdp, 
             s_detail2[1].pcadcrtdt,s_detail2[1].pcadmodid,s_detail2[1].pcadmoddt 
      
         #應用 a11 樣板自動產生(Version:3)
         #共用欄位查詢處理  
         ##----<<pcadcrtdt>>----
         AFTER FIELD pcadcrtdt
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
 
         #----<<pcadmoddt>>----
         AFTER FIELD pcadmoddt
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
         
         #----<<pcadcnfdt>>----
         
         #----<<pcadpstdt>>----
 
 
 
      
                  #應用 a01 樣板自動產生(Version:2)
         BEFORE FIELD pcadstus
            #add-point:BEFORE FIELD pcadstus name="query.b.page1.pcadstus"
            
            #END add-point
 
 
         #應用 a02 樣板自動產生(Version:2)
         AFTER FIELD pcadstus
            
            #add-point:AFTER FIELD pcadstus name="query.a.page1.pcadstus"
            
            #END add-point
            
 
 
         #Ctrlp:query.c.page1.pcadstus
#         #應用 a03 樣板自動產生(Version:3)
         ON ACTION controlp INFIELD pcadstus
            #add-point:ON ACTION controlp INFIELD pcadstus name="query.c.page1.pcadstus"
            
            #END add-point
 
 
         #Ctrlp:construct.c.page1.pcad001
         #應用 a03 樣板自動產生(Version:3)
         ON ACTION controlp INFIELD pcad001
            #add-point:ON ACTION controlp INFIELD pcad001 name="construct.c.page1.pcad001"
            #此段落由子樣板a08產生
            #開窗c段
			INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c'
			LET g_qryparam.reqry = FALSE
            CALL q_pcad001()                           #呼叫開窗
