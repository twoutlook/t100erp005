PRIVATE FUNCTION apci126_b_fill(p_wc2)              #BODY FILL UP
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   
   #end add-point
   DEFINE p_wc2    STRING
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   
   #end add-point
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前 name="b_fill.sql_before"
   
   #end add-point
 
   LET g_sql = "SELECT  DISTINCT t0.pcadstus,t0.pcad001,t0.pcad002,t0.pcad003,t0.pcad004,t0.pcad005, 
       t0.pcad006,t0.pcad007,t0.pcad008,t0.pcadstamp,t0.pcadunit,t0.pcad001,t0.pcadownid,t0.pcadowndp, 
       t0.pcadcrtid,t0.pcadcrtdp,t0.pcadcrtdt,t0.pcadmodid,t0.pcadmoddt ,t1.pcadl003 ,t2.ooag011 ,t3.ooefl003 , 
       t4.ooag011 ,t5.ooefl003 ,t6.ooag011 FROM pcad_t t0",
               " LEFT JOIN pcadl_t ON pcadlent = "||g_enterprise||" AND pcad001 = pcadl001 AND pcadl002 = '",g_dlang,"'",
                              " LEFT JOIN pcadl_t t1 ON t1.pcadlent="||g_enterprise||" AND t1.pcadl001=t0.pcad003 AND t1.pcadl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t2 ON t2.ooagent="||g_enterprise||" AND t2.ooag001=t0.pcadownid  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent="||g_enterprise||" AND t3.ooefl001=t0.pcadowndp AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t4 ON t4.ooagent="||g_enterprise||" AND t4.ooag001=t0.pcadcrtid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent="||g_enterprise||" AND t5.ooefl001=t0.pcadcrtdp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent="||g_enterprise||" AND t6.ooag001=t0.pcadmodid  ",
 
               " WHERE t0.pcadent= ?  AND  1=1 AND (", p_wc2, ") " 
   #add-point:b_fill段sql wc name="b_fill.sql_wc"
   
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("pcad_t"),
                      " ORDER BY t0.pcad001"
   
   #add-point:b_fill段sql之後 name="b_fill.sql_after"
   
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"pcad_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE apci126_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR apci126_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_pcad_d.clear()
   CALL g_pcad2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_pcad_d[l_ac].pcadstus,g_pcad_d[l_ac].pcad001,g_pcad_d[l_ac].pcad002,g_pcad_d[l_ac].pcad003, 
       g_pcad_d[l_ac].pcad004,g_pcad_d[l_ac].pcad005,g_pcad_d[l_ac].pcad006,g_pcad_d[l_ac].pcad007,g_pcad_d[l_ac].pcad008, 
       g_pcad_d[l_ac].pcadstamp,g_pcad_d[l_ac].pcadunit,g_pcad2_d[l_ac].pcad001,g_pcad2_d[l_ac].pcadownid, 
       g_pcad2_d[l_ac].pcadowndp,g_pcad2_d[l_ac].pcadcrtid,g_pcad2_d[l_ac].pcadcrtdp,g_pcad2_d[l_ac].pcadcrtdt, 
       g_pcad2_d[l_ac].pcadmodid,g_pcad2_d[l_ac].pcadmoddt,g_pcad_d[l_ac].pcad003_desc,g_pcad2_d[l_ac].pcadownid_desc, 
       g_pcad2_d[l_ac].pcadowndp_desc,g_pcad2_d[l_ac].pcadcrtid_desc,g_pcad2_d[l_ac].pcadcrtdp_desc, 
       g_pcad2_d[l_ac].pcadmodid_desc
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充 name="b_fill.fill"
      LET g_pcad_d[l_ac].pcadpos = "N"
      #end add-point
      
      CALL apci126_detail_show()      
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
 
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   LET g_error_show = 0
   
 
   
   CALL g_pcad_d.deleteElement(g_pcad_d.getLength())   
   CALL g_pcad2_d.deleteElement(g_pcad2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_pcad_d.getLength()
      LET g_pcad2_d[l_ac].pcad001 = g_pcad_d[l_ac].pcad001 
 
      #add-point:b_fill段key值相關欄位 name="b_fill.keys.fill"
      
      #end add-point
   END FOR
   
   IF g_cnt > g_pcad_d.getLength() THEN
      LET l_ac = g_pcad_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_pcad_d.getLength()
      LET g_pcad_d_mask_o[l_ac].* =  g_pcad_d[l_ac].*
      CALL apci126_pcad_t_mask()
      LET g_pcad_d_mask_n[l_ac].* =  g_pcad_d[l_ac].*
   END FOR
   
   LET g_pcad2_d_mask_o.* =  g_pcad2_d.*
   FOR l_ac = 1 TO g_pcad2_d.getLength()
      LET g_pcad2_d_mask_o[l_ac].* =  g_pcad2_d[l_ac].*
      CALL apci126_pcad_t_mask()
      LET g_pcad2_d_mask_n[l_ac].* =  g_pcad2_d[l_ac].*
   END FOR
 
   
   LET l_ac = g_cnt
 
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_pcad_d.getLength()
   DISPLAY g_detail_idx TO FORMONLY.idx
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE apci126_pb
   
END FUNCTION
 
