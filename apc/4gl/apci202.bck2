   LET l_ac = g_detail_idx
   OPEN b_fill_curs2 USING g_enterprise,g_pcab_d[l_ac].pcab001
   
   LET l_ac = 1
   FOREACH b_fill_curs2 INTO g_pcab3_d[l_ac].pcacstus,g_pcab3_d[l_ac].pcac002,g_pcab3_d[l_ac].pcac003, 
       g_pcab3_d[l_ac].pcac004,g_pcab3_d[l_ac].pcacstamp,g_pcab3_d[l_ac].pcacunit,g_pcab3_d[l_ac].pcac002_desc, 
       g_pcab3_d[l_ac].pcac003_desc,g_pcab3_d[l_ac].pcac004_desc
      IF SQLCA.sqlcode THEN
