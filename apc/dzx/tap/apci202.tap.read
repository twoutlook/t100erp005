  </point>
  <point name="input.c.page3.pcac003" order="" ver="3" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[            IF cl_null(g_pcab3_d[l_ac].pcac002) THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "" 
               LET g_errparam.code   = 'apc-00022'
               LET g_errparam.popup  = TRUE 
               CALL cl_err()    

               NEXT FIELD pcac002
            END IF
            
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.reqry = FALSE
            IF l_cmd= 'u' THEN                                                                                                 #此段落由子樣板a07產生            
               LET g_qryparam.state = 'i'			      
               LET g_qryparam.default1 = g_pcab3_d[l_ac].pcac003        
               LET g_qryparam.arg1 = g_pcab3_d[l_ac].pcac002 
               CALL q_pcaa001_1()                                
               LET g_pcab3_d[l_ac].pcac003 = g_qryparam.return1  
               DISPLAY g_pcab3_d[l_ac].pcac003 TO pcac003        
               CALL apci202_pcac003_ref() 
                                  
               NEXT FIELD pcac003                
            ELSE
               LET g_qryparam.state = 'c'	
               LET g_qryparam.where = "pcaasite = '",g_pcab3_d[l_ac].pcac002,"' "
               CALL q_pcaa001_1() 
               LET l_return = g_qryparam.return1            
               IF NOT cl_null(l_return) THEN
                  LET l_count = 0
                  LET tok = base.StringTokenizer.create(l_return,"|")
                  LET l_detail_cnt = g_pcab3_d.getLength()
                  LET l_ac_t = l_ac
                  LET l_pcac002 = g_pcab3_d[l_ac].pcac002  #當前的組織先用變數紀錄
                  LET l_pcacstamp = cl_get_timestamp()     #150325-00001#2 150327 by lori522612 add
                  WHILE tok.hasMoreTokens()
                     LET l_pcac003  = tok.nextToken()
                     LET l_cnt = 0
                     #檢查資料有無重複
                     IF NOT apci202_chk_pcac(g_enterprise,g_pcab_d[g_detail_idx].pcab001,g_pcab3_d[l_ac].pcac002,l_pcac003,FALSE) THEN
                        CONTINUE WHILE
                     ELSE    
                        LET l_count = l_count + 1
                        
                        IF l_count > 1 THEN
                           LET l_ac = l_detail_cnt + l_count - 1                           
                           IF cl_null(g_pcab3_d[l_ac].pcacstus) THEN
                              LET g_pcab3_d[l_ac].pcacstus = 'Y'                           
                           END IF                                
                           LET g_pcab3_d[l_ac].pcac002 = l_pcac002   
                           LET g_pcab3_d[l_ac].pcac002_desc = s_desc_get_department_desc(g_pcab3_d[l_ac].pcac002)
                           LET g_pcab3_d[l_ac].pcac003 = l_pcac003
                           LET g_pcab3_d[l_ac].pcacstamp = l_pcacstamp   #150325-00001#2 150327 by lori522612 add
                           
                           INITIALIZE gs_keys TO NULL
                           LET gs_keys[1] = g_pcab_d[g_detail_idx].pcab001
                           LET gs_keys[2] = l_pcac002
                           LET gs_keys[3] = g_pcab3_d[l_ac].pcac003
                           CALL apci202_insert_b('pcac_t',gs_keys,"'1'")
                           IF SQLCA.sqlcode THEN
                              LET l_count = l_count - 1
                           ELSE                         
                              CALL apci202_pcac003_ref() 
                           END IF
                        ELSE
                           LET g_pcab3_d[l_ac].pcac002 = l_pcac002  
                           LET g_pcab3_d[l_ac].pcac002_desc = s_desc_get_department_desc(g_pcab3_d[l_ac].pcac002)
                           LET g_pcab3_d[l_ac].pcac003 = l_pcac003
                           LET g_pcab3_d[l_ac].pcacstamp = l_pcacstamp   #150325-00001#2 150327 by lori522612 add
                           
                           CALL apci202_pcac003_ref() 
                        END IF
                        
                        LET l_ac = l_ac_t
                     END IF 
                  END WHILE
                  IF l_count = 0 THEN
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "" 
                     LET g_errparam.code   = g_errno
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()  
                  END IF   
               END IF                   
            END IF]]>
  </point>
