         LET g_detail_idx = g_pcab_d.getLength()
      END IF
      IF g_detail_idx = 0 AND g_pcab_d.getLength() <> 0 THEN
         LET g_detail_idx = 1
      END IF
      LET li_idx = g_detail_idx
      LET li_cnt = g_pcab_d.getLength()
   END IF
   
   IF g_current_page = 2 THEN
