  <point name="function.apci202_chk_pcac" order="3" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 檢查組織+收銀機有無重複
# Memo...........:
# Usage..........: CALL apci202_chk_pcac(p_pcacent,p_pcac001,p_pcac002,p_pcac003,p_err_pop)
#                  RETURNING r_success
# Input parameter: p_pcacent   企業編號
#                : p_pcac001   收銀員編號
#                : p_pcac002   組織編號
#                : p_pcac003   收銀機編號
#                : p_err_pop   錯誤訊息是否彈窗 TRUE/FALSE
# Return code....: r_success   TRUE/FALSE
# Date & Author..: 2014/03/03 By Lori
# Modify.........:
################################################################################
PRIVATE FUNCTION apci202_chk_pcac(p_pcacent,p_pcac001,p_pcac002,p_pcac003,p_err_pop)
   DEFINE p_pcacent   LIKE pcac_t.pcacent,
          p_pcac001   LIKE pcac_t.pcac001,
          p_pcac002   LIKE pcac_t.pcac002,
          p_pcac003   LIKE pcac_t.pcac003,
          p_err_pop   LIKE type_t.num5
   DEFINE r_success   LIKE type_t.num5
   DEFINE l_cnt1      LIKE type_t.num5,
          l_cnt2      LIKE type_t.num5,
          l_cnt3      LIKE type_t.num5
   

   LET l_cnt1 = 0
   LET l_cnt2 = 0
   LET l_cnt3 = 0 
   LET r_success = TRUE
   
   #(組織編號&收銀機=' ') AND (組織編號&收銀機=xxx) 只可以有一筆資料
   #收銀員之下,不可同時有一筆"組織編號&收銀機=' '",另一筆"組織編號&收銀機=xxx" 
   SELECT COUNT(*) INTO l_cnt1
     FROM pcac_t
    WHERE pcacent = p_pcacent
      AND pcac001 = p_pcac001
      AND pcac002 = p_pcac002
      AND pcac003 = ' '
      AND pcacstus = 'Y'  

   IF cl_null(p_pcac003) THEN
      IF l_cnt1 > 0 THEN
         LET r_success = FALSE
      END IF   
   ELSE
      SELECT COUNT(*) INTO l_cnt2
        FROM pcac_t
       WHERE pcacent = p_pcacent
         AND pcac001 = p_pcac001
         AND pcac002 = p_pcac002
         AND pcac003 = p_pcac003
         AND pcacstus = 'Y'
      IF l_cnt2 > 0 THEN
         LET r_success = FALSE
      END IF         
   END IF
   #CASE
   #   WHEN p_pcac003 <> ' ' 
   #      SELECT COUNT(*) INTO l_cnt2
   #        FROM pcac_t
   #       WHERE pcacent = p_pcacent
   #         AND pcac001 = p_pcac001
   #         AND pcac002 = p_pcac002
   #         AND pcac003 = p_pcac003
   #         AND pcacstus = 'Y'
   #   WHEN p_pcac003 = ' ' 
   #      SELECT COUNT(*) INTO l_cnt2
   #        FROM pcac_t
   #       WHERE pcacent = p_pcacent
   #         AND pcac001 = p_pcac001
   #         AND pcac002 = p_pcac002
   #         AND pcac003 <> ' '
   #         AND pcacstus = 'Y'
   #END CASE

   IF NOT r_success AND p_err_pop THEN      
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = g_pcab3_d[l_ac].pcac002
      LET g_errparam.code   = 'apc-00019'
      LET g_errparam.popup  = p_err_pop
      CALL cl_err()
   END IF
   
   RETURN r_success
END FUNCTION]]>
  </point>
  <point name="function.apci202_set_pwd" order="4" ver="4" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 設定收銀人員的密碼
# Memo...........:
# Usage..........: CALL apci202_set_pwd(p_cmd)
# Input parameter: p_cmd   C:修改密碼/R:重設密碼
# Date & Author..: 2014/03/05 By Lori
# Modify.........:
################################################################################
PRIVATE FUNCTION apci202_set_pwd(p_cmd)
   DEFINE p_cmd   LIKE type_t.chr1
   DEFINE tm      RECORD                    #用g_pswd作為遮罩顯示在畫面上
          pcab008         LIKE pcab_t.pcab008,
          l_pcab008       LIKE pcab_t.pcab008,
          l_pcab008_new   LIKE pcab_t.pcab008,
          l_pcab008_chk   LIKE pcab_t.pcab008
                  END RECORD
   DEFINE tm_t    RECORD                    #紀錄真實密碼
          pcab008         LIKE pcab_t.pcab008,
          l_pcab008       LIKE pcab_t.pcab008,
          l_pcab008_new   LIKE pcab_t.pcab008,
          l_pcab008_chk   LIKE pcab_t.pcab008
                  END RECORD
   DEFINE l_pcabmoddt     DATETIME YEAR TO SECOND
   DEFINE l_str                  STRING 
   IF cl_null(g_pcab_d[g_detail_idx].pcab001) THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = 'apc-00026'
      LET g_errparam.popup  = TRUE 
      CALL cl_err()      
      RETURN
   END IF
   
   OPEN WINDOW w_apci202_s01 WITH FORM cl_ap_formpath("apc",'apci202_s01')
   INITIALIZE tm.* TO NULL
   INITIALIZE tm_t.* TO NULL
   
   IF p_cmd = 'R' THEN
      CALL cl_set_comp_visible('l_pcab008',FALSE)
   ELSE
      CALL cl_set_comp_visible('l_pcab008',TRUE)   
   END IF
   
   DISPLAY g_pcab_d[g_detail_idx].pcab001 TO l_pcab001
   DISPLAY g_pcab_d[g_detail_idx].pcab003 TO l_pcab001_desc
   
   WHILE TRUE
      INPUT BY NAME tm.l_pcab008,tm.l_pcab008_new,tm.l_pcab008_chk WITHOUT DEFAULTS
         BEFORE INPUT
            LET tm.pcab008 = g_pcab_d[g_detail_idx].pcab008
            IF cl_null(tm.pcab008) THEN
               CALL cl_set_comp_entry("l_pcab008",FALSE)
            ELSE
               CALL cl_set_comp_entry("l_pcab008",TRUE)         
            END IF
            #IF NOT cl_null(g_pcab_d[g_detail_idx].pcab008) THEN
            #   LET tm_t.l_pcab008 = tm.l_pcab008
            #   LET tm.l_pcab008 = g_pswd
            #   DISPLAY BY NAME tm.l_pcab008
            #END IF
         
         AFTER FIELD l_pcab008
            IF NOT cl_null(tm.l_pcab008) THEN
               INITIALIZE l_str TO NULL                            #add by geza 20150521  
               LET  l_str =  g_pcab_d[l_ac].pcab001,tm.l_pcab008   #add by geza 20150521
               LET  tm.l_pcab008 = cl_trust_genauthkey(l_str)      #add by geza 20150521
               IF tm.l_pcab008 <> tm.pcab008 AND tm.l_pcab008 <> g_pswd THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "" 
                  LET g_errparam.code   = 'apc-00027'
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()       
                  NEXT FIELD CURRENT
               ELSE
                  LET tm_t.l_pcab008 = tm.l_pcab008
                  LET tm.l_pcab008 = g_pswd
                  DISPLAY BY NAME tm.l_pcab008
               END IF
            END IF         
      
         AFTER FIELD l_pcab008_new    
            IF NOT cl_null(tm.l_pcab008_new) THEN
               LET tm_t.l_pcab008_new = tm.l_pcab008_new
               LET tm.l_pcab008_new = g_pswd
               DISPLAY BY NAME tm.l_pcab008_new
            END IF
      
         BEFORE FIELD l_pcab008_chk
         
         AFTER FIELD l_pcab008_chk    
            IF NOT cl_null(tm.l_pcab008_chk) THEN
               IF tm.l_pcab008_chk <> tm_t.l_pcab008_new AND tm.l_pcab008_chk <> g_pswd THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "" 
                  LET g_errparam.code   = 'apc-00028'
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()                   
                  
                  LET tm.l_pcab008_chk = ''
                  NEXT FIELD CURRENT
               ELSE
                  LET tm_t.l_pcab008_chk = tm.l_pcab008_chk
                  LET tm.l_pcab008_chk = g_pswd
                  DISPLAY BY NAME tm.l_pcab008_chk
               END IF
            END IF
            
         AFTER INPUT
            IF INT_FLAG THEN 
               CALL s_transaction_begin()
               
               IF tm_t.l_pcab008_new <> tm_t.l_pcab008_new THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "" 
                  LET g_errparam.code   = 'apc-00028'
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()                   
                  
                  LET tm.l_pcab008_chk = ''
                  NEXT FIELD l_pcab008_chk    
               END IF
               
               LET l_pcabmoddt = cl_get_current()
               
               
               INITIALIZE l_str TO NULL                            #add by geza 20150521  
               LET  l_str =  g_pcab_d[l_ac].pcab001,tm_t.l_pcab008_new   #add by geza 20150521
               LET  tm_t.l_pcab008_new = cl_trust_genauthkey(l_str)      #add by geza 20150521
               
               UPDATE pcab_t
                  SET pcab008 = tm_t.l_pcab008_new,
                      pcab009 = COALESCE(pcab009,0) + 1,
                      pcab010 = l_pcabmoddt,
                      pcabmodid = g_user,
                      pcabmoddt = l_pcabmoddt
                WHERE pcabent = g_enterprise
                  AND pcab001 = g_pcab_d[g_detail_idx].pcab001
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "pcab_t" 
                     LET g_errparam.code   = 'std-00009'
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()                
                     CALL s_transaction_end('N','0')               
                  WHEN SQLCA.sqlcode #其他錯誤
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "pcab_t" 
                     LET g_errparam.code   = SQLCA.sqlcode
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()                
                     CALL s_transaction_end('N','0')
               END CASE
               CALL s_transaction_end('Y','0')              
            
            ELSE
               CONTINUE INPUT
            END IF
         ON ACTION accept
            LET INT_FLAG = TRUE 
            ACCEPT INPUT
         
         ON ACTION cancel
            LET INT_FLAG = TRUE 
            EXIT INPUT 
            
      END INPUT
      
      IF INT_FLAG THEN
         EXIT WHILE
      ELSE
         CONTINUE WHILE
      END IF      
   END WHILE
   CLOSE WINDOW w_apci202_s01
   CALL apci202_b_fill(g_wc)
   
   LET l_ac = g_detail_idx
END FUNCTION]]>
  </point>
