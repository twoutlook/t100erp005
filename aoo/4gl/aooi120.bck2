   #add-point:delete段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="delete.define"
   
   #end add-point  
   
   #add-point:Function前置處理  name="delete.pre_function"
   
   #end add-point
   
   #先確定key值無遺漏
   IF g_ooei_m.ooei001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
    
   LET g_ooei001_t = g_ooei_m.ooei001
 
   
   LET g_master_multi_table_t.ooeil001 = g_ooei_m.ooei001
LET g_master_multi_table_t.ooeil003 = g_ooei_m.ooeil003
LET g_master_multi_table_t.ooeil004 = g_ooei_m.ooeil004
 
 
   OPEN aooi120_cl USING g_enterprise,g_ooei_m.ooei001
 
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aooi120_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CLOSE aooi120_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aooi120_master_referesh USING g_ooei_m.ooei001 INTO g_ooei_m.ooei001,g_ooei_m.ooei003,g_ooei_m.ooei017, 
       g_ooei_m.ooeistus,g_ooei_m.ooeiownid,g_ooei_m.ooeiowndp,g_ooei_m.ooeicrtid,g_ooei_m.ooeicrtdp, 
       g_ooei_m.ooeicrtdt,g_ooei_m.ooeimodid,g_ooei_m.ooeimoddt,g_ooei_m.ooei201,g_ooei_m.ooei202,g_ooei_m.ooei203, 
       g_ooei_m.ooei204,g_ooei_m.ooei205,g_ooei_m.ooei206,g_ooei_m.ooei207,g_ooei_m.ooei208,g_ooei_m.ooei209, 
       g_ooei_m.ooei210,g_ooei_m.ooei211,g_ooei_m.ooei212,g_ooei_m.ooei301,g_ooei_m.ooei302,g_ooei_m.ooei303, 
       g_ooei_m.ooei304,g_ooei_m.ooei305,g_ooei_m.ooei008,g_ooei_m.ooei009,g_ooei_m.ooei010,g_ooei_m.ooei004, 
       g_ooei_m.ooei011,g_ooei_m.ooei013,g_ooei_m.ooei023,g_ooei_m.ooei014,g_ooei_m.ooei016,g_ooei_m.ooei015, 
       g_ooei_m.ooei006,g_ooei_m.ooei018,g_ooei_m.ooei019,g_ooei_m.ooei017_desc,g_ooei_m.ooeiownid_desc, 
       g_ooei_m.ooeiowndp_desc,g_ooei_m.ooeicrtid_desc,g_ooei_m.ooeicrtdp_desc,g_ooei_m.ooeimodid_desc, 
       g_ooei_m.ooei008_desc,g_ooei_m.ooei004_desc,g_ooei_m.ooei014_desc,g_ooei_m.ooei016_desc,g_ooei_m.ooei015_desc, 
       g_ooei_m.ooei006_desc,g_ooei_m.ooei018_desc,g_ooei_m.ooei019_desc
   
   
   #檢查是否允許此動作
   IF NOT aooi120_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_ooei_m_mask_o.* =  g_ooei_m.*
   CALL aooi120_ooei_t_mask()
   LET g_ooei_m_mask_n.* =  g_ooei_m.*
   
   #將最新資料顯示到畫面上
   CALL aooi120_show()
   
   IF cl_ask_delete() THEN
 
      #add-point:單頭刪除前 name="delete.head.b_delete"
      
      #end add-point
 
      #應用 a47 樣板自動產生(Version:4)
      #刪除相關文件
      CALL aooi120_set_pk_array()
      #add-point:相關文件刪除前 name="delete.befroe.related_document_remove"
      
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
 
 
      DELETE FROM ooei_t 
       WHERE ooeient = g_enterprise AND ooei001 = g_ooei_m.ooei001 
 
 
      #add-point:單頭刪除中 name="delete.head.m_delete"
      
      #end add-point
         
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "ooei_t:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = FALSE 
         CALL s_transaction_end('N','0')
         CALL cl_err()
      END IF
  
      INITIALIZE l_var_keys_bak TO NULL 
   INITIALIZE l_field_keys   TO NULL 
   LET l_var_keys_bak[01] = g_enterprise
   LET l_field_keys[01] = 'ooeilent'
   LET l_var_keys_bak[02] = g_master_multi_table_t.ooeil001
   LET l_field_keys[02] = 'ooeil001'
   CALL cl_multitable_delete(l_field_keys,l_var_keys_bak,'ooeil_t')
 
      
      #add-point:單頭刪除後 name="delete.head.a_delete"
      
      #end add-point
      
       
 
      #修改歷程記錄(刪除)
      IF NOT cl_log_modified_record('','') THEN 
         CLOSE aooi120_cl
         CALL s_transaction_end('N','0')
         RETURN
      END IF
      
      CLEAR FORM
      CALL aooi120_ui_browser_refresh()
      
      #確保畫面上保有資料
      IF g_browser_cnt > 0 THEN
         #CALL aooi120_browser_fill(g_wc,"")
         CALL aooi120_fetch("P")
      ELSE
         CLEAR FORM
      END IF
      CALL s_transaction_end('Y','0')
   ELSE    
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE aooi120_cl
 
   #功能已完成,通報訊息中心
   CALL aooi120_msgcentre_notify('delete')
 
   #add-point:單頭刪除完成後 name="delete.a_delete"
   
   #end add-point
 
END FUNCTION
 
