{<section id="aooi120.state_change" >}
   #應用 a09 樣板自動產生(Version:17)
#+ 確認碼變更 
PRIVATE FUNCTION aooi120_statechange()
   #add-point:statechange段define(客製用) name="statechange.define_customerization"
   
   #end add-point  
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="statechange.define"
   
   #end add-point  
   
   #add-point:Function前置處理 name="statechange.before"
   
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_ooei_m.ooei001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN aooi120_cl USING g_enterprise,g_ooei_m.ooei001
   IF STATUS THEN
      CLOSE aooi120_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aooi120_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE aooi120_master_referesh USING g_ooei_m.ooei001 INTO g_ooei_m.ooei001,g_ooei_m.ooei003,g_ooei_m.ooei017, 
       g_ooei_m.ooeistus,g_ooei_m.ooeiownid,g_ooei_m.ooeiowndp,g_ooei_m.ooeicrtid,g_ooei_m.ooeicrtdp, 
       g_ooei_m.ooeicrtdt,g_ooei_m.ooeimodid,g_ooei_m.ooeimoddt,g_ooei_m.ooei201,g_ooei_m.ooei202,g_ooei_m.ooei203, 
       g_ooei_m.ooei204,g_ooei_m.ooei205,g_ooei_m.ooei206,g_ooei_m.ooei207,g_ooei_m.ooei208,g_ooei_m.ooei209, 
       g_ooei_m.ooei210,g_ooei_m.ooei211,g_ooei_m.ooei212,g_ooei_m.ooei301,g_ooei_m.ooei302,g_ooei_m.ooei303, 
       g_ooei_m.ooei304,g_ooei_m.ooei305,g_ooei_m.ooei008,g_ooei_m.ooei009,g_ooei_m.ooei010,g_ooei_m.ooei004, 
       g_ooei_m.ooei011,g_ooei_m.ooei013,g_ooei_m.ooei023,g_ooei_m.ooei014,g_ooei_m.ooei016,g_ooei_m.ooei015, 
       g_ooei_m.ooei006,g_ooei_m.ooei018,g_ooei_m.ooei019,g_ooei_m.ooei017_desc,g_ooei_m.ooeiownid_desc, 
       g_ooei_m.ooeiowndp_desc,g_ooei_m.ooeicrtid_desc,g_ooei_m.ooeicrtdp_desc,g_ooei_m.ooeimodid_desc, 
       g_ooei_m.ooei008_desc,g_ooei_m.ooei004_desc,g_ooei_m.ooei014_desc,g_ooei_m.ooei016_desc,g_ooei_m.ooei015_desc, 
       g_ooei_m.ooei006_desc,g_ooei_m.ooei018_desc,g_ooei_m.ooei019_desc
   
 
   #檢查是否允許此動作
   IF NOT aooi120_action_chk() THEN
      CLOSE aooi120_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_ooei_m.ooei001,g_ooei_m.ooeil003,g_ooei_m.ooeil004,g_ooei_m.ooei003,g_ooei_m.ooei017, 
       g_ooei_m.ooei017_desc,g_ooei_m.ooeistus,g_ooei_m.ooeiownid,g_ooei_m.ooeiownid_desc,g_ooei_m.ooeiowndp, 
       g_ooei_m.ooeiowndp_desc,g_ooei_m.ooeicrtid,g_ooei_m.ooeicrtid_desc,g_ooei_m.ooeicrtdp,g_ooei_m.ooeicrtdp_desc, 
       g_ooei_m.ooeicrtdt,g_ooei_m.ooeimodid,g_ooei_m.ooeimodid_desc,g_ooei_m.ooeimoddt,g_ooei_m.ooei201, 
       g_ooei_m.ooei202,g_ooei_m.ooei203,g_ooei_m.ooei204,g_ooei_m.ooei205,g_ooei_m.ooei206,g_ooei_m.ooei207, 
       g_ooei_m.ooei208,g_ooei_m.ooei209,g_ooei_m.ooei210,g_ooei_m.ooei211,g_ooei_m.ooei212,g_ooei_m.ooei301, 
       g_ooei_m.ooei302,g_ooei_m.ooei303,g_ooei_m.ooei304,g_ooei_m.ooei305,g_ooei_m.ooei008,g_ooei_m.ooei008_desc, 
       g_ooei_m.ooei009,g_ooei_m.ooei010,g_ooei_m.ooei004,g_ooei_m.ooei004_desc,g_ooei_m.ooei011,g_ooei_m.ooei013, 
       g_ooei_m.ooei023,g_ooei_m.ooei014,g_ooei_m.ooei014_desc,g_ooei_m.ooei016,g_ooei_m.ooei016_desc, 
       g_ooei_m.ooei015,g_ooei_m.ooei015_desc,g_ooei_m.ooei006,g_ooei_m.ooei006_desc,g_ooei_m.ooei018, 
       g_ooei_m.ooei018_desc,g_ooei_m.ooei019,g_ooei_m.ooei019_desc
 
   CASE g_ooei_m.ooeistus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_ooei_m.ooeistus
            
            WHEN "N"
               HIDE OPTION "inactive"
            WHEN "Y"
               HIDE OPTION "active"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      
      #end add-point
      
      
	  
      ON ACTION inactive
         IF cl_auth_chk_act("inactive") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.inactive"
            
            #end add-point
         END IF
         EXIT MENU
      ON ACTION active
         IF cl_auth_chk_act("active") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.active"
            
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      ) OR 
      g_ooei_m.ooeistus = lc_state OR cl_null(lc_state) THEN
      CLOSE aooi120_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   
   #end add-point
   
   LET g_ooei_m.ooeimodid = g_user
   LET g_ooei_m.ooeimoddt = cl_get_current()
   LET g_ooei_m.ooeistus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE ooei_t 
      SET (ooeistus,ooeimodid,ooeimoddt) 
        = (g_ooei_m.ooeistus,g_ooei_m.ooeimodid,g_ooei_m.ooeimoddt)     
    WHERE ooeient = g_enterprise AND ooei001 = g_ooei_m.ooei001
 
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE aooi120_master_referesh USING g_ooei_m.ooei001 INTO g_ooei_m.ooei001,g_ooei_m.ooei003, 
          g_ooei_m.ooei017,g_ooei_m.ooeistus,g_ooei_m.ooeiownid,g_ooei_m.ooeiowndp,g_ooei_m.ooeicrtid, 
          g_ooei_m.ooeicrtdp,g_ooei_m.ooeicrtdt,g_ooei_m.ooeimodid,g_ooei_m.ooeimoddt,g_ooei_m.ooei201, 
          g_ooei_m.ooei202,g_ooei_m.ooei203,g_ooei_m.ooei204,g_ooei_m.ooei205,g_ooei_m.ooei206,g_ooei_m.ooei207, 
          g_ooei_m.ooei208,g_ooei_m.ooei209,g_ooei_m.ooei210,g_ooei_m.ooei211,g_ooei_m.ooei212,g_ooei_m.ooei301, 
          g_ooei_m.ooei302,g_ooei_m.ooei303,g_ooei_m.ooei304,g_ooei_m.ooei305,g_ooei_m.ooei008,g_ooei_m.ooei009, 
          g_ooei_m.ooei010,g_ooei_m.ooei004,g_ooei_m.ooei011,g_ooei_m.ooei013,g_ooei_m.ooei023,g_ooei_m.ooei014, 
          g_ooei_m.ooei016,g_ooei_m.ooei015,g_ooei_m.ooei006,g_ooei_m.ooei018,g_ooei_m.ooei019,g_ooei_m.ooei017_desc, 
          g_ooei_m.ooeiownid_desc,g_ooei_m.ooeiowndp_desc,g_ooei_m.ooeicrtid_desc,g_ooei_m.ooeicrtdp_desc, 
          g_ooei_m.ooeimodid_desc,g_ooei_m.ooei008_desc,g_ooei_m.ooei004_desc,g_ooei_m.ooei014_desc, 
          g_ooei_m.ooei016_desc,g_ooei_m.ooei015_desc,g_ooei_m.ooei006_desc,g_ooei_m.ooei018_desc,g_ooei_m.ooei019_desc 
 
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_ooei_m.ooei001,g_ooei_m.ooeil003,g_ooei_m.ooeil004,g_ooei_m.ooei003,g_ooei_m.ooei017, 
          g_ooei_m.ooei017_desc,g_ooei_m.ooeistus,g_ooei_m.ooeiownid,g_ooei_m.ooeiownid_desc,g_ooei_m.ooeiowndp, 
          g_ooei_m.ooeiowndp_desc,g_ooei_m.ooeicrtid,g_ooei_m.ooeicrtid_desc,g_ooei_m.ooeicrtdp,g_ooei_m.ooeicrtdp_desc, 
          g_ooei_m.ooeicrtdt,g_ooei_m.ooeimodid,g_ooei_m.ooeimodid_desc,g_ooei_m.ooeimoddt,g_ooei_m.ooei201, 
          g_ooei_m.ooei202,g_ooei_m.ooei203,g_ooei_m.ooei204,g_ooei_m.ooei205,g_ooei_m.ooei206,g_ooei_m.ooei207, 
          g_ooei_m.ooei208,g_ooei_m.ooei209,g_ooei_m.ooei210,g_ooei_m.ooei211,g_ooei_m.ooei212,g_ooei_m.ooei301, 
          g_ooei_m.ooei302,g_ooei_m.ooei303,g_ooei_m.ooei304,g_ooei_m.ooei305,g_ooei_m.ooei008,g_ooei_m.ooei008_desc, 
          g_ooei_m.ooei009,g_ooei_m.ooei010,g_ooei_m.ooei004,g_ooei_m.ooei004_desc,g_ooei_m.ooei011, 
          g_ooei_m.ooei013,g_ooei_m.ooei023,g_ooei_m.ooei014,g_ooei_m.ooei014_desc,g_ooei_m.ooei016, 
          g_ooei_m.ooei016_desc,g_ooei_m.ooei015,g_ooei_m.ooei015_desc,g_ooei_m.ooei006,g_ooei_m.ooei006_desc, 
          g_ooei_m.ooei018,g_ooei_m.ooei018_desc,g_ooei_m.ooei019,g_ooei_m.ooei019_desc
   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
   CALL cl_set_act_visible("delete",TRUE)
   IF g_ooei_m.ooeistus = 'N' THEN
      CALL cl_set_act_visible("delete",FALSE)
   END IF
   #end add-point
 
   #add-point:statechange段結束前 name="statechange.after"
   
   #end add-point  
 
   CLOSE aooi120_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aooi120_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION
 
 
 
 
