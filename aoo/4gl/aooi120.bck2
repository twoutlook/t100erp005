       g_ooei_m.ooeistus,g_ooei_m.ooeiownid,g_ooei_m.ooeiowndp,g_ooei_m.ooeicrtid,g_ooei_m.ooeicrtdp, 
       g_ooei_m.ooeicrtdt,g_ooei_m.ooeimodid,g_ooei_m.ooeimoddt,g_ooei_m.ooei201,g_ooei_m.ooei202,g_ooei_m.ooei203, 
       g_ooei_m.ooei204,g_ooei_m.ooei205,g_ooei_m.ooei206,g_ooei_m.ooei207,g_ooei_m.ooei208,g_ooei_m.ooei209, 
       g_ooei_m.ooei210,g_ooei_m.ooei211,g_ooei_m.ooei212,g_ooei_m.ooei301,g_ooei_m.ooei302,g_ooei_m.ooei303, 
       g_ooei_m.ooei304,g_ooei_m.ooei305,g_ooei_m.ooei008,g_ooei_m.ooei009,g_ooei_m.ooei010,g_ooei_m.ooei004, 
       g_ooei_m.ooei011,g_ooei_m.ooei013,g_ooei_m.ooei023,g_ooei_m.ooei014,g_ooei_m.ooei016,g_ooei_m.ooei015, 
       g_ooei_m.ooei006,g_ooei_m.ooei018,g_ooei_m.ooei019,g_ooei_m.ooei017_desc,g_ooei_m.ooeiownid_desc, 
       g_ooei_m.ooeiowndp_desc,g_ooei_m.ooeicrtid_desc,g_ooei_m.ooeicrtdp_desc,g_ooei_m.ooeimodid_desc, 
       g_ooei_m.ooei008_desc,g_ooei_m.ooei004_desc,g_ooei_m.ooei014_desc,g_ooei_m.ooei016_desc,g_ooei_m.ooei015_desc, 
       g_ooei_m.ooei006_desc,g_ooei_m.ooei018_desc,g_ooei_m.ooei019_desc
   
   #遮罩相關處理
   LET g_ooei_m_mask_o.* =  g_ooei_m.*
   CALL aooi120_ooei_t_mask()
   LET g_ooei_m_mask_n.* =  g_ooei_m.*
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL aooi120_set_act_visible()
   CALL aooi120_set_act_no_visible()
 
   #add-point:fetch段action控制 name="fetch.action_control"
   CALL cl_set_act_visible("delete",TRUE)
   IF g_ooei_m.ooeistus = 'N' THEN
      CALL cl_set_act_visible("delete",FALSE)
   END IF
   #end add-point  
   
   
   
   #保存單頭舊值
   LET g_ooei_m_t.* = g_ooei_m.*
