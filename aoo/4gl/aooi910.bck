################################################################################
# Descriptions...: 確認欄位輸入是否正確
# Memo...........:
# Usage..........: CALL aooi910_chk_field(p_type,p_value,p_value1,p_value2)
#                  RETURNING l_msg
# Input parameter: p_type     欲確認的欄位
#                : p_value    條件
#                : p_value1   條件
#                : p_value2   條件
# Return code....: l_msg      錯誤訊息碼
#                : 
# Date & Author..: 2014/12/29 By Candice
# Modify.........:
################################################################################
PRIVATE FUNCTION aooi910_chk_field(p_type,p_value,p_value1,p_value2)
   DEFINE  p_type      STRING,
           p_value     LIKE type_t.chr20,
           p_value1    LIKE type_t.chr20,
           p_value2    LIKE type_t.chr20
   DEFINE  l_msg       STRING
   DEFINE  l_cnt       LIKE type_t.num5
   
   LET l_msg =''
   LET l_cnt = 0
   
   CASE p_type
      WHEN 'oojo001'
         IF p_value IS NOT NULL THEN
            SELECT COUNT(gzzz001) INTO l_cnt 
              FROM gzzz_t
             WHERE gzzzstus ='Y' AND gzzz001 = p_value
         END IF
         IF l_cnt = 0 THEN
            LET l_msg = "aoo-00382"
         END IF
      
      WHEN 'oojo002'
         CASE p_value
            WHEN '2'
               IF p_value1 IS NOT NULL AND p_value2 IS NOT NULL THEN
                  SELECT COUNT(gzya001) INTO l_cnt 
                    FROM gzya_t
                   WHERE gzyastus ='Y' AND gzya001 = p_value1 AND gzyaent = p_value2
               END IF
               
            WHEN '3'
               IF p_value1 IS NOT NULL AND p_value2 IS NOT NULL THEN
                  SELECT COUNT(gzxa003) INTO l_cnt 
                    FROM gzxa_t
                   WHERE gzxastus ='Y' AND gzxa003 = p_value1 AND gzxaent = p_value2
               END IF
         END CASE
         
         IF l_cnt = 0 THEN
            LET l_msg = "aoo-00412"
         END IF
      
   END CASE
   RETURN l_msg
END FUNCTION

################################################################################
# Descriptions...: 取得欄位說明
# Memo...........:
# Usage..........: CALL aooi910_get_desc(p_type,p_value,p_value1)
#                  RETURNING l_desc
# Input parameter: p_type     欲取得說明的欄位
#                : p_value    條件
#                : p_value1   條件
# Return code....: l_desc     欄位說明
#                : 
# Date & Author..: 2014/12/30 By Candice
# Modify.........:
################################################################################
PRIVATE FUNCTION aooi910_get_desc(p_type,p_value,p_value1)
   DEFINE  p_type      STRING,
           p_value     LIKE type_t.chr20,
           p_value1    LIKE type_t.chr20
   DEFINE l_desc   LIKE type_t.chr500
   DEFINE l_sql            STRING
   
   LET l_desc = ""
   LET l_sql = ""
   
   CASE p_type
      WHEN 'oojo001'
         LET l_sql = "SELECT gzzal003 FROM gzzal_t WHERE gzzal001=? AND gzzal002='"||g_dlang||"'"
         INITIALIZE g_ref_fields TO NULL
         LET g_ref_fields[1] = p_value
         CALL ap_ref_array2(g_ref_fields,l_sql, "") RETURNING g_rtn_fields
         LET l_desc = '', g_rtn_fields[1] , ''
      
      WHEN 'oojo002'
         CASE p_value1
            WHEN '2'   #2:角色
               LET l_sql = "SELECT gzyal003 FROM gzya_t ",
                           " LEFT JOIN gzyal_t ON gzyaent = gzyalent AND gzya001 = gzyal001 AND gzyal002=?",
                           " WHERE gzya001 = ? and gzyaent = ?"
               INITIALIZE g_ref_fields TO NULL
               LET g_ref_fields[1] = g_dlang
               LET g_ref_fields[2] = p_value
               LET g_ref_fields[3] = g_enterprise 
               CALL ap_ref_array2(g_ref_fields,l_sql, "") RETURNING g_rtn_fields
               LET l_desc = '', g_rtn_fields[1] , ''
               
            WHEN '3'   #4:用戶
               LET l_sql = "SELECT ooag011 FROM gzxa_t ",
                           " LEFT JOIN ooag_t ON gzxaent = ooagent AND gzxa003 = ooag001 ",
                           " WHERE gzxa003 = ? and gzxaent = ?"
               INITIALIZE g_ref_fields TO NULL
               LET g_ref_fields[1] = p_value
               LET g_ref_fields[2] = g_enterprise
               CALL ap_ref_array2(g_ref_fields,l_sql, "") RETURNING g_rtn_fields
               LET l_desc = '', g_rtn_fields[1] , ''
         END CASE
   END CASE
   
   RETURN l_desc
END FUNCTION

