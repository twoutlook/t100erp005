   IF g_oojl_m.oojl001 = '3' THEN 
      SELECT dbbc002,dbbc003,dbbc004,dbbc005   #銷售組織編號/渠道編號/產品組編號/辦事處編號
        INTO g_oojm_d[l_ac].oojm401,g_oojm_d[l_ac].oojm402,g_oojm_d[l_ac].oojm403,g_oojm_d[l_ac].oojm404   
        FROM dbbc_t
       WHERE dbbcent = g_enterprise
         AND dbbc001 = g_oojm_d[l_ac].oojm001
   
      CALL aoot101_oojm401_ref()
      CALL aoot101_oojm402_ref()
      CALL aoot101_oojm403_ref()
      CALL aoot101_oojm404_ref()   
      #sakura---add---str
         CALL s_desc_get_stock_desc(g_oojm_d[l_ac].oojm001,g_oojm.oojm405) RETURNING g_oojm.oojm405_desc #預設收貨成本倉 
         CALL s_desc_get_stock_desc(g_oojm_d[l_ac].oojm001,g_oojm.oojm408) RETURNING g_oojm.oojm408_desc #預設收貨非成本倉
         CALL s_desc_get_stock_desc(g_oojm_d[l_ac].oojm001,g_oojm.oojm406) RETURNING g_oojm.oojm406_desc #預設出貨成本倉
         CALL s_desc_get_stock_desc(g_oojm_d[l_ac].oojm001,g_oojm.oojm409) RETURNING g_oojm.oojm409_desc #預設出貨非成本倉 
         CALL s_desc_get_stock_desc(g_oojm_d[l_ac].oojm001,g_oojm.oojm407) RETURNING g_oojm.oojm407_desc #預設在途成本倉
         CALL s_desc_get_stock_desc(g_oojm_d[l_ac].oojm001,g_oojm.oojm410) RETURNING g_oojm.oojm410_desc #預設在途非成本倉
         DISPLAY g_oojm.oojm405_desc,g_oojm.oojm408_desc,g_oojm.oojm406_desc,
                 g_oojm.oojm409_desc,g_oojm.oojm407_desc,g_oojm.oojm410_desc         
              TO l_oojm405_desc,l_oojm408_desc,l_oojm406_desc,
                 l_oojm409_desc,l_oojm407_desc,l_oojm410_desc
      #sakura---add---end
   ELSE
      CALL aoot101_oojm017_ref()   #所屬法人
   END IF
   
   LET g_oojm_d_t.* = g_oojm_d[l_ac].*  
END FUNCTION

