{<section id="aoot101.state_change" >}
   #應用 a09 樣板自動產生(Version:17)
#+ 確認碼變更 
PRIVATE FUNCTION aoot101_statechange()
   #add-point:statechange段define(客製用) name="statechange.define_customerization"
   
   #end add-point  
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="statechange.define"
   DEFINE l_success   LIKE type_t.num5
   DEFINE l_oojl002        LIKE oojl_t.oojl002
   DEFINE l_prog           STRING
   DEFINE ls_js            STRING
   DEFINE la_param         RECORD
             prog          STRING,
             param         DYNAMIC ARRAY OF STRING
                            END RECORD   
   #end add-point  
   
   #add-point:Function前置處理 name="statechange.before"
   
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_oojl_m.oojldocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN aoot101_cl USING g_enterprise,g_oojl_m.oojldocno
   IF STATUS THEN
      CLOSE aoot101_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aoot101_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE aoot101_master_referesh USING g_oojl_m.oojldocno INTO g_oojl_m.oojlsite,g_oojl_m.oojldocdt, 
       g_oojl_m.oojldocno,g_oojl_m.oojl000,g_oojl_m.oojl003,g_oojl_m.oojl004,g_oojl_m.oojl002,g_oojl_m.oojl005, 
       g_oojl_m.oojl001,g_oojl_m.oojlunit,g_oojl_m.oojlstus,g_oojl_m.oojlownid,g_oojl_m.oojlowndp,g_oojl_m.oojlcrtid, 
       g_oojl_m.oojlcrtdp,g_oojl_m.oojlcrtdt,g_oojl_m.oojlmodid,g_oojl_m.oojlmoddt,g_oojl_m.oojlcnfid, 
       g_oojl_m.oojlcnfdt,g_oojl_m.oojlsite_desc,g_oojl_m.oojl003_desc,g_oojl_m.oojl004_desc,g_oojl_m.oojlownid_desc, 
       g_oojl_m.oojlowndp_desc,g_oojl_m.oojlcrtid_desc,g_oojl_m.oojlcrtdp_desc,g_oojl_m.oojlmodid_desc, 
       g_oojl_m.oojlcnfid_desc
   
 
   #檢查是否允許此動作
   IF NOT aoot101_action_chk() THEN
      CLOSE aoot101_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_oojl_m.oojlsite,g_oojl_m.oojlsite_desc,g_oojl_m.oojldocdt,g_oojl_m.oojldocno,g_oojl_m.oojl000, 
       g_oojl_m.oojl003,g_oojl_m.oojl003_desc,g_oojl_m.oojl004,g_oojl_m.oojl004_desc,g_oojl_m.oojl002, 
       g_oojl_m.oojl005,g_oojl_m.oojl001,g_oojl_m.oojlunit,g_oojl_m.oojlstus,g_oojl_m.oojlownid,g_oojl_m.oojlownid_desc, 
       g_oojl_m.oojlowndp,g_oojl_m.oojlowndp_desc,g_oojl_m.oojlcrtid,g_oojl_m.oojlcrtid_desc,g_oojl_m.oojlcrtdp, 
       g_oojl_m.oojlcrtdp_desc,g_oojl_m.oojlcrtdt,g_oojl_m.oojlmodid,g_oojl_m.oojlmodid_desc,g_oojl_m.oojlmoddt, 
       g_oojl_m.oojlcnfid,g_oojl_m.oojlcnfid_desc,g_oojl_m.oojlcnfdt
 
   CASE g_oojl_m.oojlstus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
      WHEN "A"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/approved.png")
      WHEN "D"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/withdraw.png")
      WHEN "R"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/rejection.png")
      WHEN "W"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/signing.png")
      WHEN "X"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_oojl_m.oojlstus
            
            WHEN "N"
               HIDE OPTION "unconfirmed"
            WHEN "Y"
               HIDE OPTION "confirmed"
            WHEN "A"
               HIDE OPTION "approved"
            WHEN "D"
               HIDE OPTION "withdraw"
            WHEN "R"
               HIDE OPTION "rejection"
            WHEN "W"
               HIDE OPTION "signing"
            WHEN "X"
               HIDE OPTION "invalid"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
     CALL cl_set_act_visible("signing,withdraw",FALSE)  #150814-00001#1 s983961 BPM/審核 同時出現修正
     CASE g_oojl_m.oojlstus
         WHEN "N"
            CALL cl_set_act_visible("unconfirmed,hold",FALSE)
            #需提交至BPM時，則顯示「提交」功能並隱藏「確認」功能
            IF cl_bpm_chk() THEN
                CALL cl_set_act_visible("signing",TRUE)
                CALL cl_set_act_visible("confirmed",FALSE)
            END IF
         WHEN "R"   #保留修改的功能(如作廢)，隱藏其他應用功能(如: 確認、未確認、留置、過帳…)
            CALL cl_set_act_visible("conl_set_firmed,unconfirmed,hold",FALSE)

         WHEN "D"   #保留修改的功能(如作廢)，隱藏其他應用功能(如: 確認、未確認、留置、過帳…)
            CALL cl_set_act_visible("confirmed,unconfirmed,hold",FALSE)

         WHEN "X"
            CALL cl_set_act_visible("unconfirmed,invalid,confirmed,hold",FALSE)
            CALL s_transaction_end('N','0')   #160812-00017#3 Add By Ken 160815
            RETURN
         WHEN "Y"
            CALL cl_set_act_visible("invalid,confirmed,hold",FALSE)
            IF g_oojl_m.oojl005 = 'Y' THEN   #資料已更新者不可取消確認
               CALL cl_set_act_visible("unconfirmed",FALSE)  
            END IF
            CALL s_transaction_end('N','0')   #160812-00017#3 Add By Ken 160815
            RETURN #150814-00001#1 s983961 add 按下去會鎖畫面的問題修正
         WHEN "W"    #只能顯示抽單;其餘應用功能皆隱藏
             CALL cl_set_act_visible("withdraw",TRUE)
             CALL cl_set_act_visible("unconfirmed,invalid,confirmed,hold",FALSE)

         WHEN "A"     #只能顯示確認; 其餘應用功能皆隱藏
             CALL cl_set_act_visible("confirmed ",TRUE)
             CALL cl_set_act_visible("unconfirmed,invalid,hold",FALSE)

      END CASE
      #end add-point
      
      #應用 a36 樣板自動產生(Version:5)
      #提交
      ON ACTION signing
         IF cl_auth_chk_act("signing") THEN
            IF NOT aoot101_send() THEN
               CALL s_transaction_end('N','0')
            ELSE
               CALL s_transaction_end('Y','0')
            END IF
            #因應簽核行為, 該動作完成後不再進行後續處理
            #於此處直接返回
            CLOSE aoot101_cl
            RETURN
         END IF
    
      #抽單
      ON ACTION withdraw
         IF cl_auth_chk_act("withdraw") THEN
            IF NOT aoot101_draw_out() THEN
               CALL s_transaction_end('N','0')
            ELSE
               CALL s_transaction_end('Y','0')
            END IF
            #因應簽核行為, 該動作完成後不再進行後續處理
            #於此處直接返回
            CLOSE aoot101_cl
            RETURN
         END IF
 
 
 
	  
      ON ACTION unconfirmed
         IF cl_auth_chk_act("unconfirmed") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.unconfirmed"
            
            #end add-point
         END IF
         EXIT MENU
      ON ACTION confirmed
         IF cl_auth_chk_act("confirmed") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.confirmed"
            
            #end add-point
         END IF
         EXIT MENU
      ON ACTION approved
         IF cl_auth_chk_act("approved") THEN
            LET lc_state = "A"
            #add-point:action控制 name="statechange.approved"
            
            #end add-point
         END IF
         EXIT MENU
      #ON ACTION withdraw
      #   IF cl_auth_chk_act("withdraw") THEN
      #      LET lc_state = "D"
      #      #add-point:action控制 name="statechange.withdraw"
      #      
      #      #end add-point
      #   END IF
      #   EXIT MENU
      ON ACTION rejection
         IF cl_auth_chk_act("rejection") THEN
            LET lc_state = "R"
            #add-point:action控制 name="statechange.rejection"
            
            #end add-point
         END IF
         EXIT MENU
      #ON ACTION signing
      #   IF cl_auth_chk_act("signing") THEN
      #      LET lc_state = "W"
      #      #add-point:action控制 name="statechange.signing"
      #      
      #      #end add-point
      #   END IF
      #   EXIT MENU
      ON ACTION invalid
         IF cl_auth_chk_act("invalid") THEN
            LET lc_state = "X"
            #add-point:action控制 name="statechange.invalid"
            
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      AND lc_state <> "A"
      AND lc_state <> "D"
      AND lc_state <> "R"
      AND lc_state <> "W"
      AND lc_state <> "X"
      ) OR 
      g_oojl_m.oojlstus = lc_state OR cl_null(lc_state) THEN
      CLOSE aoot101_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   CALL s_transaction_begin()
   OPEN aoot101_cl USING g_enterprise,g_oojl_m.oojldocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code =  STATUS
      LET g_errparam.extend = "OPEN aoot101_cl:"
      LET g_errparam.popup = TRUE
      CALL cl_err()

      CLOSE aoot101_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   CALL cl_err_collect_init()
   
   #未確認改確認(N->Y)
   IF lc_state = 'Y' AND g_oojl_m.oojlstus = 'N' THEN
      CALL s_aoot101_conf_chk(g_oojl_m.oojldocno) RETURNING l_success
      IF NOT l_success THEN
         CALL s_transaction_end('N','0')
         CALL cl_err_collect_show()
         RETURN
      ELSE
         IF cl_ask_confirm('aim-00108') THEN
            CALL s_aoot101_conf_upd(g_oojl_m.oojldocno) RETURNING l_success
            IF NOT l_success THEN
               CALL s_transaction_end('N','0')
               CALL cl_err_collect_show()
               RETURN
            ELSE
               CALL s_transaction_end('Y','0')
               
               # 如果異動生效日=系統日,背景執行 aoop101;否則寫排程
               LET l_oojl002 = DATE(g_oojl_m.oojl002)
               LET l_prog  = "aoop101"
               LET la_param.prog = l_prog
               LET la_param.param[1] = g_oojl_m.oojldocno
               LET la_param.param[2] = ''
               LET la_param.param[3] = ''
               LET la_param.param[4] = ''
               LET la_param.param[5] = l_oojl002
               LET la_param.param[6] = 'Y'
               
               LET ls_js = util.JSON.stringify(la_param)
               #
               IF l_oojl002 = g_today THEN               
                  CALL cl_cmdrun_wait(ls_js)
                  SELECT oojl005 INTO g_oojl_m.oojl005
                    FROM oojl_t
                   WHERE oojlent = g_enterprise
                     AND oojldocno = g_oojl_m.oojldocno
                  DISPLAY BY NAME g_oojl_m.oojl005
               #ELSE
               #   LET g_schedule.gzpa003 = "2"                             #排程類型
               #   LET g_schedule.gzpa002 = "aoop101-",g_oojl_m.oojldocno   #排程說明
               #   LET g_schedule.gzpa006 = l_oojl002                       #指定執行日期
               #   LET g_schedule.gzpa007 = "00:00:00"                      #指定執行時間
               #
               #   IF g_chk_jobid THEN
               #      LET g_jobid = g_schedule.gzpa001
               #   ELSE
               #      LET g_jobid = cl_schedule_get_jobid(l_prog)
               #   END IF
               #   
               #   CALL cl_schedule_write_new_prog(l_prog)
               #   CALL cl_schedule_update_data(g_jobid,ls_js)
               #
               #   DISPLAY "Background Schedule Setting"
               END IF
            
               CALL cl_err_collect_show()
            END IF       
         ELSE
            CALL s_transaction_end('N','0')
            CALL cl_err_collect_show()
            RETURN
         END IF
      END IF
   END IF   
   #確認改未確認(Y->N)
   IF lc_state = 'N' AND g_oojl_m.oojlstus = 'Y' THEN
      CALL s_aoot101_unconf_chk(g_oojl_m.oojldocno) RETURNING l_success
      IF NOT l_success THEN
         CALL s_transaction_end('N','0')
         CALL cl_err_collect_show()
         RETURN
      ELSE
         IF cl_ask_confirm('aim-00110') THEN
            CALL s_aoot101_unconf_upd(g_oojl_m.oojldocno) RETURNING l_success
            IF NOT l_success THEN
               CALL s_transaction_end('N','0')
               CALL cl_err_collect_show()
               RETURN
            ELSE
               CALL s_transaction_end('Y','0')
               CALL cl_err_collect_show()
            END IF
         ELSE
            CALL s_transaction_end('N','0')
            CALL cl_err_collect_show()
            RETURN
         END IF
      END IF
   END IF   
   #未確認改作廢(N->X)
   IF lc_state = 'X' AND g_oojl_m.oojlstus = 'N' THEN
      CALL s_aoot101_invalid_chk(g_oojl_m.oojldocno) RETURNING l_success
      IF NOT l_success THEN
         CALL s_transaction_end('N','0')
         CALL cl_err_collect_show()
         RETURN
      ELSE
         IF cl_ask_confirm('aim-00109') THEN
            CALL s_aoot101_invalid_upd(g_oojl_m.oojldocno) RETURNING l_success
            IF NOT l_success THEN
               CALL s_transaction_end('N','0')
               CALL cl_err_collect_show()
               RETURN
            ELSE
               CALL s_transaction_end('Y','0')
               CALL cl_err_collect_show()
            END IF
         ELSE
            CALL s_transaction_end('N','0')
            CALL cl_err_collect_show()
            RETURN
         END IF
      END IF
   END IF   
   #end add-point
   
   LET g_oojl_m.oojlmodid = g_user
   LET g_oojl_m.oojlmoddt = cl_get_current()
   LET g_oojl_m.oojlstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE oojl_t 
      SET (oojlstus,oojlmodid,oojlmoddt) 
        = (g_oojl_m.oojlstus,g_oojl_m.oojlmodid,g_oojl_m.oojlmoddt)     
    WHERE oojlent = g_enterprise AND oojldocno = g_oojl_m.oojldocno
 
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "A"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/approved.png")
         WHEN "D"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/withdraw.png")
         WHEN "R"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/rejection.png")
         WHEN "W"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/signing.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE aoot101_master_referesh USING g_oojl_m.oojldocno INTO g_oojl_m.oojlsite,g_oojl_m.oojldocdt, 
          g_oojl_m.oojldocno,g_oojl_m.oojl000,g_oojl_m.oojl003,g_oojl_m.oojl004,g_oojl_m.oojl002,g_oojl_m.oojl005, 
          g_oojl_m.oojl001,g_oojl_m.oojlunit,g_oojl_m.oojlstus,g_oojl_m.oojlownid,g_oojl_m.oojlowndp, 
          g_oojl_m.oojlcrtid,g_oojl_m.oojlcrtdp,g_oojl_m.oojlcrtdt,g_oojl_m.oojlmodid,g_oojl_m.oojlmoddt, 
          g_oojl_m.oojlcnfid,g_oojl_m.oojlcnfdt,g_oojl_m.oojlsite_desc,g_oojl_m.oojl003_desc,g_oojl_m.oojl004_desc, 
          g_oojl_m.oojlownid_desc,g_oojl_m.oojlowndp_desc,g_oojl_m.oojlcrtid_desc,g_oojl_m.oojlcrtdp_desc, 
          g_oojl_m.oojlmodid_desc,g_oojl_m.oojlcnfid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_oojl_m.oojlsite,g_oojl_m.oojlsite_desc,g_oojl_m.oojldocdt,g_oojl_m.oojldocno, 
          g_oojl_m.oojl000,g_oojl_m.oojl003,g_oojl_m.oojl003_desc,g_oojl_m.oojl004,g_oojl_m.oojl004_desc, 
          g_oojl_m.oojl002,g_oojl_m.oojl005,g_oojl_m.oojl001,g_oojl_m.oojlunit,g_oojl_m.oojlstus,g_oojl_m.oojlownid, 
          g_oojl_m.oojlownid_desc,g_oojl_m.oojlowndp,g_oojl_m.oojlowndp_desc,g_oojl_m.oojlcrtid,g_oojl_m.oojlcrtid_desc, 
          g_oojl_m.oojlcrtdp,g_oojl_m.oojlcrtdp_desc,g_oojl_m.oojlcrtdt,g_oojl_m.oojlmodid,g_oojl_m.oojlmodid_desc, 
          g_oojl_m.oojlmoddt,g_oojl_m.oojlcnfid,g_oojl_m.oojlcnfid_desc,g_oojl_m.oojlcnfdt
   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
   
   #end add-point
 
   #add-point:statechange段結束前 name="statechange.after"
   
   #end add-point  
 
   CLOSE aoot101_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aoot101_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION
 
 
 
 
