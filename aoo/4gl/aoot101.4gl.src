{<section id="aoot101.browser_fill" >}
#+ 瀏覽頁簽資料填充
PRIVATE FUNCTION aoot101_browser_fill(ps_page_action)
   #add-point:browser_fill段define(客製用) name="browser_fill.define_customerization"
   
   #end add-point  
   DEFINE ps_page_action    STRING
   DEFINE l_wc              STRING
   DEFINE l_wc2             STRING
   DEFINE l_sql             STRING
   DEFINE l_sub_sql         STRING
   DEFINE l_sql_rank        STRING
   #add-point:browser_fill段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="browser_fill.define"
   DEFINE l_where           STRING
   #end add-point    
   
   #add-point:Function前置處理 name="browser_fill.before_browser_fill"
   LET l_where = s_aooi500_sql_where(g_prog,'oojlsite')
   IF cl_null(g_wc) THEN
      LET g_wc = l_where
   ELSE 
      LET g_wc = g_wc CLIPPED," AND ",l_where
   END IF
   #end add-point
   
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   LET l_wc  = g_wc.trim() 
   LET l_wc2 = g_wc2.trim()
 
   #add-point:browser_fill,foreach前 name="browser_fill.before_foreach"
   
   #end add-point
   
   IF g_wc2 <> " 1=1" THEN
      #單身有輸入搜尋條件                      
      LET l_sub_sql = " SELECT DISTINCT oojldocno ",
                      " FROM oojl_t ",
                      " ",
                      " LEFT JOIN oojm_t ON oojment = oojlent AND oojldocno = oojmdocno ", "  ",
                      #add-point:browser_fill段sql(oojm_t1) name="browser_fill.cnt.join.}"
                      
                      #end add-point
 
 
                      " ", 
                      " LEFT JOIN oojml_t ON oojmlent = "||g_enterprise||" AND oojldocno = oojmldocno AND oojm001 = oojml001 AND oojml002 = '",g_dlang,"' ", 
 
 
                      " WHERE oojlent = " ||g_enterprise|| " AND oojment = " ||g_enterprise|| " AND ",l_wc, " AND ", l_wc2, cl_sql_add_filter("oojl_t")
   ELSE
      #單身未輸入搜尋條件
      LET l_sub_sql = " SELECT DISTINCT oojldocno ",
                      " FROM oojl_t ", 
                      "  ",
                      "  ",
                      " WHERE oojlent = " ||g_enterprise|| " AND ",l_wc CLIPPED, cl_sql_add_filter("oojl_t")
   END IF
   
   #add-point:browser_fill,cnt wc name="browser_fill.cnt_sqlwc"
   IF g_wc2 <> " 1=1" THEN               
      LET l_sub_sql = " SELECT UNIQUE oojldocno ",
                      " FROM oojl_t ",
                      " LEFT JOIN oojm_t ON oojment = oojlent AND oojldocno = oojmdocno ",
                      " LEFT JOIN oojml_t ON oojmlent = '"||g_enterprise||"' AND oojldocno = oojmldocno AND oojm001 = oojml001 AND oojml002 = '",g_dlang,"' ",
                      " LEFT JOIN oofb_t ON oofbent = oojment AND oofb002 = oojm012",
                      " LEFT JOIN oofc_t ON oofcent = oojment AND oofc002 = oojm012",
                      " WHERE oojlent = " ||g_enterprise|| 
                      "   AND oojment = " ||g_enterprise||
                      "   AND oojl001 = '",g_argv[1],"'",
                      "   AND ",l_wc, 
                      "   AND ", l_wc2, cl_sql_add_filter("oojl_t")
   ELSE
      #單身未輸入搜尋條件
      LET l_sub_sql = " SELECT UNIQUE oojldocno ",
                      " FROM oojl_t ", 
                      " WHERE oojlent = " ||g_enterprise||
                      "   AND oojl001 = '",g_argv[1],"'",
                      "   AND ",l_wc CLIPPED, cl_sql_add_filter("oojl_t")
   END IF
   #end add-point
   
   LET g_sql = " SELECT COUNT(1) FROM (",l_sub_sql,")"
   
   #add-point:browser_fill,count前 name="browser_fill.before_count"
 
   #end add-point
   
   IF g_sql.getIndexOf(" 1=2",1) THEN
      DISPLAY "INFO: 1=2 jumped!"
   ELSE
      PREPARE header_cnt_pre FROM g_sql
      EXECUTE header_cnt_pre INTO g_browser_cnt   #總筆數
      FREE header_cnt_pre
   END IF
    
   IF g_browser_cnt > g_max_browse THEN
      IF g_error_show = 1 THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = g_browser_cnt
         LET g_errparam.code = 9035 
         LET g_errparam.popup = TRUE 
         CALL cl_err()
      END IF
      LET g_browser_cnt = g_max_browse
   END IF
   
   DISPLAY g_browser_cnt TO FORMONLY.b_count   #總筆數的顯示
   DISPLAY g_browser_cnt TO FORMONLY.h_count   #總筆數的顯示
 
   #根據行為確定資料填充位置及WC
   IF cl_null(g_add_browse) THEN
      #清除畫面
      CLEAR FORM                
      INITIALIZE g_oojl_m.* TO NULL
      CALL g_oojm_d.clear()        
 
      #add-point:browser_fill g_add_browse段額外處理 name="browser_fill.add_browse.other"
      
      #end add-point   
      CALL g_browser.clear()
      LET g_cnt = 1
   ELSE
      LET l_wc  = g_add_browse
      LET l_wc2 = " 1=1" 
      LET g_cnt = g_current_idx
   END IF
 
   #依照t0.oojlstus,t0.oojldocno,t0.oojl000,t0.oojl002,t0.oojl003,t0.oojl004,t0.oojl005,t0.oojldocdt Browser欄位定義(取代原本bs_sql功能)
   #考量到單身可能下條件, 所以此處需join單身所有table
   #DISTINCT是為了避免在join時出現重複的資料(如果不加DISTINCT則須在程式中過濾)
   IF g_wc2 <> " 1=1" THEN
      #單身有輸入搜尋條件   
      LET g_sql = " SELECT DISTINCT t0.oojlstus,t0.oojlstus,t0.oojldocno,t0.oojl000,t0.oojl002,t0.oojl003, 
          t0.oojl004,t0.oojl005,t0.oojldocdt,t1.ooag011 ,t2.ooefl003 ",
                  " FROM oojl_t t0",
                  "  ",
                  "  LEFT JOIN oojm_t ON oojment = oojlent AND oojldocno = oojmdocno ", "  ", 
                  #add-point:browser_fill段sql(oojm_t1) name="browser_fill.join.oojm_t1"
                  
                  #end add-point
 
 
                  " LEFT JOIN oojml_t ON oojmlent = "||g_enterprise||" AND oojldocno = oojmldocno AND oojm001 = oojml001 AND oojml002 = '",g_dlang,"' ", 
 
 
                                 " LEFT JOIN ooag_t t1 ON t1.ooagent="||g_enterprise||" AND t1.ooag001=t0.oojl003  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent="||g_enterprise||" AND t2.ooefl001=t0.oojl004 AND t2.ooefl002='"||g_dlang||"' ",
 
                  " WHERE t0.oojlent = " ||g_enterprise|| " AND ",l_wc," AND ",l_wc2, cl_sql_add_filter("oojl_t")
   ELSE
      #單身無輸入搜尋條件   
      LET g_sql = " SELECT DISTINCT t0.oojlstus,t0.oojlstus,t0.oojldocno,t0.oojl000,t0.oojl002,t0.oojl003, 
          t0.oojl004,t0.oojl005,t0.oojldocdt,t1.ooag011 ,t2.ooefl003 ",
                  " FROM oojl_t t0",
                  "  ",
                                 " LEFT JOIN ooag_t t1 ON t1.ooagent="||g_enterprise||" AND t1.ooag001=t0.oojl003  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent="||g_enterprise||" AND t2.ooefl001=t0.oojl004 AND t2.ooefl002='"||g_dlang||"' ",
 
                  " WHERE t0.oojlent = " ||g_enterprise|| " AND ",l_wc, cl_sql_add_filter("oojl_t")
   END IF
   #add-point:browser_fill,sql wc name="browser_fill.fill_sqlwc"
   LET g_sql = " SELECT DISTINCT t0.oojlstus,t0.oojlstus,t0.oojldocno,t0.oojl000,  t0.oojl002,",
               "                 t0.oojl003, t0.oojl004, t0.oojl005,  t0.oojldocdt,t1.ooag011 ,",
               "                 t2.ooefl003 ",
               "   FROM oojl_t t0",
               "   LEFT JOIN oojm_t ON oojment = oojlent AND oojldocno = oojmdocno ",
               "   LEFT JOIN oojml_t ON oojmlent = '"||g_enterprise||"' AND oojldocno = oojmldocno AND oojm001 = oojml001 AND oojml002 = '",g_dlang,"' ",
               "   LEFT JOIN ooag_t t1 ON t1.ooagent='"||g_enterprise||"' AND t1.ooag001=t0.oojl003  ",
               "   LEFT JOIN ooefl_t t2 ON t2.ooeflent='"||g_enterprise||"' AND t2.ooefl001=t0.oojl004 AND t2.ooefl002='"||g_lang||"' ",
               "   LEFT JOIN oofb_t ON oofbent = oojment AND oofb002 = oojm012",
               "   LEFT JOIN oofc_t ON oofcent = oojment AND oofc002 = oojm012",
               " WHERE t0.oojlent = '" ||g_enterprise|| "' AND oojl001 = '",g_argv[1], "' AND ",l_wc," AND ",l_wc2, cl_sql_add_filter("oojl_t")
   #end add-point
   LET g_sql = g_sql, " ORDER BY oojldocno ",g_order
 
   #add-point:browser_fill,before_prepare name="browser_fill.before_prepare"
   DISPLAY 'g_sql = ',g_sql
   #end add-point
        
   #LET g_sql = cl_sql_add_tabid(g_sql,"oojl_t") #WC重組
   LET g_sql = cl_sql_add_mask(g_sql) #遮蔽特定資料
   
   IF g_sql.getIndexOf(" 1=2",1) THEN
      DISPLAY "INFO: 1=2 jumped!"
   ELSE
      PREPARE browse_pre FROM g_sql
      DECLARE browse_cur CURSOR FOR browse_pre
      
      #add-point:browser_fill段open cursor name="browser_fill.open"
      
      #end add-point
      
      FOREACH browse_cur INTO g_browser[g_cnt].b_statepic,g_browser[g_cnt].b_oojlstus,g_browser[g_cnt].b_oojldocno, 
          g_browser[g_cnt].b_oojl000,g_browser[g_cnt].b_oojl002,g_browser[g_cnt].b_oojl003,g_browser[g_cnt].b_oojl004, 
          g_browser[g_cnt].b_oojl005,g_browser[g_cnt].b_oojldocdt,g_browser[g_cnt].b_oojl003_desc,g_browser[g_cnt].b_oojl004_desc 
 
         IF SQLCA.SQLCODE THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "Foreach:",SQLERRMESSAGE 
            LET g_errparam.code = SQLCA.SQLCODE 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
      
         #add-point:browser_fill段reference name="browser_fill.reference"
         
         #end add-point
      
         #遮罩相關處理
         CALL aoot101_browser_mask()
      
               #應用 a24 樣板自動產生(Version:3)
      #browser顯示圖片
      CASE g_browser[g_cnt].b_statepic
         WHEN "N"
            LET g_browser[g_cnt].b_statepic = "stus/16/unconfirmed.png"
         WHEN "Y"
            LET g_browser[g_cnt].b_statepic = "stus/16/confirmed.png"
         WHEN "A"
            LET g_browser[g_cnt].b_statepic = "stus/16/approved.png"
         WHEN "D"
            LET g_browser[g_cnt].b_statepic = "stus/16/withdraw.png"
         WHEN "R"
            LET g_browser[g_cnt].b_statepic = "stus/16/rejection.png"
         WHEN "W"
            LET g_browser[g_cnt].b_statepic = "stus/16/signing.png"
         WHEN "X"
            LET g_browser[g_cnt].b_statepic = "stus/16/invalid.png"
         
      END CASE
 
 
 
         LET g_cnt = g_cnt + 1
         IF g_cnt > g_max_browse THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      FREE browse_pre
   END IF
   
   #清空g_add_browse, 並指定指標位置
   IF NOT cl_null(g_add_browse) THEN
      LET g_add_browse = ""
      CALL g_curr_diag.setCurrentRow("s_browse",g_current_idx)
   END IF
   
   IF cl_null(g_browser[g_cnt].b_oojldocno) THEN
      CALL g_browser.deleteElement(g_cnt)
   END IF
   
   LET g_header_cnt  = g_browser.getLength()
   LET g_browser_cnt = g_browser.getLength()
   
   #筆數顯示
   IF g_browser_cnt > 0 THEN
      DISPLAY g_browser_idx TO FORMONLY.b_index #當下筆數
      DISPLAY g_browser_cnt TO FORMONLY.b_count #總筆數
      DISPLAY g_browser_idx TO FORMONLY.h_index #當下筆數
      DISPLAY g_browser_cnt TO FORMONLY.h_count #總筆數
      DISPLAY g_detail_idx  TO FORMONLY.idx     #單身當下筆數
      DISPLAY g_detail_cnt  TO FORMONLY.cnt     #單身總筆數
   ELSE
      DISPLAY '' TO FORMONLY.b_index #當下筆數
      DISPLAY '' TO FORMONLY.b_count #總筆數
      DISPLAY '' TO FORMONLY.h_index #當下筆數
      DISPLAY '' TO FORMONLY.h_count #總筆數
      DISPLAY '' TO FORMONLY.idx     #單身當下筆數
      DISPLAY '' TO FORMONLY.cnt     #單身總筆數
   END IF
 
   LET g_rec_b = g_cnt - 1
   LET g_detail_cnt = g_rec_b
   LET g_cnt = 0
 
   #若無資料則關閉相關功能
   IF g_browser_cnt = 0 THEN
      CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce,mainhidden", FALSE)
      CALL cl_navigator_setting(0,0)
   ELSE
      CALL cl_set_act_visible("mainhidden", TRUE)
   END IF
                  
   
   #add-point:browser_fill段結束前 name="browser_fill.after"
   
   #end add-point   
 
END FUNCTION
 
