################################################################################
# Descriptions...: 新增聯絡地址,通訊地址
# Memo...........: 異動狀態是修改,且組織編號已有聯絡地址/通訊地址時,需將原資料複製到新的聯絡識別碼之下
# Usage..........: CALL aoot101_ins_address(p_cmd)
#                  RETURNING r_success
# Input Parameter: p_cmd      操作狀態
# Return code....: r_success
# Date & Author..: 2014/07/18 By Lori
# Modify.........:
################################################################################
PRIVATE FUNCTION aoot101_ins_address(p_cmd)
   DEFINE p_cmd           LIKE type_t.chr1
   DEFINE r_success       LIKE type_t.num5
   DEFINE l_success       LIKE type_t.num5
   DEFINE l_ooef012       LIKE ooef_t.ooef012    #組織主檔(ooef_t)中紀錄的聯絡識別碼
   DEFINE l_oofa001_new   LIKE oofa_t.oofa001   #申請單產生的新聯絡識別碼
   DEFINE l_oofb001       LIKE oofb_t.oofb001
   DEFINE l_oofb001_new   LIKE oofb_t.oofb001
   DEFINE l_oofc001       LIKE oofc_t.oofc001
   DEFINE l_oofc001_new   LIKE oofc_t.oofc001
   DEFINE l_dt            DATETIME YEAR TO SECOND   
   DEFINE l_sql           STRING   
   DEFINE l_wc            STRING   
   
   LET r_success = TRUE   
   LET l_success = ''
   LET l_ooef012 = ''
   LET l_oofa001_new = ''
   LET l_dt = cl_get_current()   

   SELECT ooef012 INTO l_ooef012 
     FROM ooef_t
    WHERE ooefent = g_enterprise
      AND ooef001 = g_oojm_d[l_ac].oojm001
  
   IF p_cmd = 'a' OR (p_cmd = 'u' AND cl_null(g_oojm_d[l_ac].oojm012)) THEN   
      CALL aoot101_get_contact_id() RETURNING l_success,l_oofa001_new   
      IF NOT l_success THEN
         LET r_success = FALSE
         RETURN r_success
      END IF
   ELSE
      LET l_oofa001_new = g_oojm_d[l_ac].oojm012  
   END IF
   
   IF NOT cl_null(l_ooef012) AND l_ooef012 <> l_oofa001_new THEN
      DISPLAY "Copy Address From : ",l_ooef012   
      LET l_sql = "SELECT oofb001 FROM oofb_t ",
                  " WHERE oofbent = ",g_enterprise,
                  "   AND oofb002 = '",l_ooef012,"' "
      PREPARE aoot101_oofb_sel_pre FROM l_sql
      DECLARE aoot101_oofb_sel_cur CURSOR FOR aoot101_oofb_sel_pre      

      LET l_sql = "SELECT oofc001 FROM oofc_t ",
                  " WHERE oofcent = ",g_enterprise,
                  "   AND oofc002 = '",l_ooef012,"' "
      PREPARE aoot101_oofc_sel_pre FROM l_sql
      DECLARE aoot101_oofc_sel_cur CURSOR FOR aoot101_oofc_sel_pre

      FOREACH aoot101_oofb_sel_cur INTO l_oofb001      
         LET l_oofb001_new = ''
         LET l_wc = " oofbent = ",g_enterprise
         CALL s_aooi350_get_idno('oofb001','oofb_t',l_wc)
            RETURNING l_success,l_oofb001_new
         IF NOT l_success THEN
            LET r_success = FALSE
            EXIT FOREACH
         END IF       
         
         INSERT INTO oofb_t(oofbstus,oofbent,oofb001,oofb002,oofb003,
                            oofb004,oofb005,oofb006,oofb007,oofb008,
                            oofb009,oofb010,oofb011,oofb012,oofb013,
                            oofb014,oofb015,oofb016,oofb017,oofb018,
                            oofb019,oofb020,oofb021,oofb022,
                            oofbownid,oofbowndp,
                            oofbcrtid,oofbcrtdp,oofbcrtdt,
                            oofbmodid,oofbmoddt)
              SELECT 'Y',oofbent,l_oofb001_new,l_oofa001_new,oofb003,  
                     oofb004,oofb005,oofb006,oofb007,oofb008,
                     oofb009,oofb010,oofb011,oofb012,oofb013,
                     oofb014,oofb015,oofb016,oofb017,oofb018,
                     oofb019,oofb020,oofb021,oofb022,
                     g_user,g_dept,
                     g_user,g_dept,l_dt,
                     '',''
                FROM oofb_t
               WHERE oofbent = g_enterprise
                 AND oofb001 = l_oofb001
         IF NOT l_success THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "oofb_t"
            LET g_errparam.popup = TRUE
            LET r_success = FALSE
            CALL cl_err()
            EXIT FOREACH
         END IF
         DISPLAY "oofb001: ",l_oofb001_new," (Source: ",l_oofb001,")"   
      END FOREACH
      
      FOREACH aoot101_oofc_sel_cur INTO l_oofc001
         LET l_oofc001_new = ''
         LET l_wc = " oofcent = ",g_enterprise
         CALL s_aooi350_get_idno('oofc001','oofc_t',l_wc)
            RETURNING l_success,l_oofc001_new
         IF NOT l_success THEN
            LET r_success = FALSE
            EXIT FOREACH
         END IF 
         
         INSERT INTO oofc_t(oofcstus,oofcent,oofc001,oofc002,oofc003,
                            oofc004,oofc005,oofc006,oofc007,oofc008,
                            oofc009,oofc010,oofc011,oofc012,oofc013,
                            oofc014,
                            oofcownid,oofcowndp,
                            oofccrtid,oofccrtdp,oofccrtdt,
                            oofcmodid,oofcmoddt)
              SELECT 'Y',oofcent,l_oofc001_new,l_oofa001_new,oofc003,
                     oofc004,oofc005,oofc006,oofc007,oofc008,
                     oofc009,oofc010,oofc011,oofc012,oofc013,
                     oofc014,
                     g_user,g_dept,
                     g_user,g_dept,l_dt,
                     '',''
                FROM oofc_t
               WHERE oofcent = g_enterprise
                 AND oofc001 = l_oofc001
         IF NOT l_success THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = SQLCA.sqlcode
            LET g_errparam.extend = "oofc_t"
            LET g_errparam.popup = TRUE
            LET r_success = FALSE
            CALL cl_err()
            EXIT FOREACH
         END IF
         DISPLAY "oofc001: ",l_oofc001_new," (Source: ",l_oofc001,")"         
      END FOREACH  
   END IF
   
   IF r_success THEN
      LET g_oojm_d[l_ac].oojm012 = l_oofa001_new
   END IF
   
   RETURN r_success
END FUNCTION

