################################################################################
# Descriptions...: 銷售範圍資料重複性檢查
# Memo...........: 銷售組織+渠道編號+產品組編號+辦事處編號 應為唯一，不可重複
# Usage..........: CALL aoot101_sales_range_dup_chk()
#                  RETURNING r_success
# Return code....: r_success   檢查結果
# Date & Author..: 2014/07/15 By Lori
# Modify.........:
################################################################################
PRIVATE FUNCTION aoot101_sales_range_dup_chk()
   DEFINE r_success   LIKE type_t.num5
   DEFINE l_cnt       LIKE type_t.num5
   
   LET r_success = TRUE
   LET l_cnt = 0
   
   IF cl_null(g_oojm_d[l_ac].oojm401) OR cl_null(g_oojm_d[l_ac].oojm402)
      OR cl_null(g_oojm_d[l_ac].oojm403) OR cl_null(g_oojm_d[l_ac].oojm404) THEN
      RETURN r_success
   END IF
   
   SELECT COUNT(*) INTO l_cnt
     FROM dbbc_t
    WHERE dbbcent = g_enterprise
      AND dbbc002 = g_oojm_d[l_ac].oojm401
      AND dbbc003 = g_oojm_d[l_ac].oojm402
      AND dbbc004 = g_oojm_d[l_ac].oojm403
      AND dbbc005 = g_oojm_d[l_ac].oojm404     
   IF l_cnt > 0 THEN
      LET r_success = FALSE
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code =  "aoo-00335"
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      LET g_errparam.replace[1] =  g_oojm_d[l_ac].oojm401
      LET g_errparam.replace[2] = g_oojm_d[l_ac].oojm402
      LET g_errparam.replace[3] = g_oojm_d[l_ac].oojm403
      LET g_errparam.replace[4] = g_oojm_d[l_ac].oojm404
      CALL cl_err()

      RETURN r_success
   END IF

   SELECT COUNT(*) INTO l_cnt
     FROM oojm_t,oojl_t
    WHERE oojment = oojlent
      AND oojmdocno = oojldocno
      AND oojment = g_enterprise
      AND oojm401 = g_oojm_d[l_ac].oojm401
      AND oojm402 = g_oojm_d[l_ac].oojm402
      AND oojm403 = g_oojm_d[l_ac].oojm403
      AND oojm404 = g_oojm_d[l_ac].oojm404
      AND oojl000 = 'I'
      AND COALESCE(oojl005,'N') = 'N'      
   IF l_cnt > 0 THEN
      LET r_success = FALSE
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code =  "aoo-00336"
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      LET g_errparam.replace[1] =  g_oojm_d[l_ac].oojm401
      LET g_errparam.replace[2] = g_oojm_d[l_ac].oojm402
      LET g_errparam.replace[3] = g_oojm_d[l_ac].oojm403
      LET g_errparam.replace[4] = g_oojm_d[l_ac].oojm404
      CALL cl_err()

      RETURN r_success
   END IF
   
   RETURN r_success
END FUNCTION

