   END IF
   IF NOT s_aooi500_comp_entry(g_prog,'oojlunit') THEN
      CALL cl_set_comp_entry("oojlunit",FALSE)
   END IF   
   LET l_cnt = 0 
   SELECT COUNT(*) INTO l_cnt
     FROM oojm_t
    WHERE oojment = g_enterprise
      AND oojmdocno = g_oojl_m.oojldocno
   IF l_cnt > 0 THEN      
      CALL cl_set_comp_entry("oojl000",FALSE)
   END IF
   #end add-point 
 
