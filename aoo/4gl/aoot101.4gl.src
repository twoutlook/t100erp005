################################################################################
# Descriptions...: 組織編號校驗
# Memo...........:
# Usage..........: CALL aoot101_oojm001_chk()
#                  RETURNING r_success
# Return code....: r_success   校驗結果
# Date & Author..: 2014/07/15 By Lori
# Modify.........:
################################################################################
PRIVATE FUNCTION aoot101_oojm001_chk()
   DEFINE r_success   LIKE type_t.num5
   DEFINE l_exist     LIKE type_t.chr1
   DEFINE l_ooef209   LIKE ooef_t.ooef209   #銷售範圍
   DEFINE l_ooef305   LIKE ooef_t.ooef305   #辦事處
   DEFINE l_gzze003   LIKE gzze_t.gzze003
   DEFINE l_cnt       LIKE type_t.num5
   
   LET r_success = TRUE 
   LET l_exist   = NULL 
   LET l_ooef209 = NULL 
   LET l_ooef305 = NULL 
   LET l_gzze003 = NULL
   LET l_cnt = 0 
   
   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt
     FROM oojm_t,oojl_t
    WHERE oojment = oojlent
      AND oojmdocno = oojldocno
      AND oojment = g_enterprise
      AND oojm001 = g_oojm_d[l_ac].oojm001
      AND oojmdocno <> g_oojl_m.oojldocno
      AND oojlstus ='N'
   IF l_cnt > 0 THEN
      LET r_success = FALSE
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code =  "aoo-00394"
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()
      RETURN r_success
   END IF   
   
   CASE g_oojl_m.oojl000 
      WHEN 'I'
         SELECT 'Y',ooef209,ooef305
           INTO l_exist,l_ooef209,l_ooef305
           FROM ooef_t
          WHERE ooef001 = g_oojm_d[l_ac].oojm001
            AND ooefent = g_enterprise #160905-00007#3 add
          
         CASE 
            WHEN l_ooef209 = 'Y' 
               LET l_gzze003 = cl_getmsg('aoo-00330',g_dlang)
            WHEN l_ooef209 = 'Y'
               LET l_gzze003 = cl_getmsg('aoo-00330',g_dlang)
            OTHERWISE
               LET l_gzze003 = cl_getmsg('aoo-00328',g_dlang)            
         END CASE
         
         IF l_exist = 'Y' THEN
            LET r_success = FALSE
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code =  "aoo-00331"
            LET g_errparam.extend = ''
            LET g_errparam.popup = TRUE
            LET g_errparam.replace[1] =  l_gzze003
            CALL cl_err()

            RETURN r_success
         END IF
         
         SELECT COUNT(*) INTO l_cnt
           FROM oojm_t,oojl_t
          WHERE oojment = oojlent
            AND oojmdocno = oojldocno
            AND oojment = g_enterprise
            AND oojm001 = g_oojm_d[l_ac].oojm001
            AND oojl000 = 'I'
            AND COALESCE(oojl005,'N') = 'N'
         IF l_cnt > 0 THEN
            LET r_success = FALSE
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code =  "aoo-00334"
            LET g_errparam.extend = ''
            LET g_errparam.popup = TRUE
            LET g_errparam.replace[1] =  g_oojm001_tit
            CALL cl_err()

            RETURN r_success
         END IF         
      WHEN 'U'
         INITIALIZE g_chkparam.* TO NULL
         LET g_chkparam.arg1 = g_oojm_d[l_ac].oojm001
         IF NOT cl_chk_exist("v_ooef001_19") THEN
            LET r_success = FALSE
            RETURN r_success
         END IF                        
   END CASE  
   
   RETURN r_success
END FUNCTION

