PRIVATE FUNCTION aooi900_01_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define

   #end add-point     
 
   #先清空單身變數內容
   CALL g_oojj_d.clear()
   CALL g_oojj2_d.clear()
 
 
   #add-point:b_fill段sql_before

   #end add-point
   
   LET g_sql = "SELECT  UNIQUE oojjstus,oojj006,oojj003,oojj004,oojj001,oojj002,oojj005,oojj007,oojjownid,oojjowndp, 
       oojjcrtid,oojjcrtdp,oojjcrtdt,oojjmodid,oojjmoddt,oojj001,oojj002,t1.gzzol003 FROM oojj_t",   
        
               "",
               
                              " LEFT JOIN gzzol_t t1 ON t1.gzzol001=oojj003 AND gzzol002 ='"||g_lang||"' ",
 
               " WHERE oojjent= ? AND oojjsite=?" 
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("oojj_t")
   END IF
   
   #add-point:b_fill段sql_after

   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF aooi900_01_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY oojj_t.oojj001,oojj_t.oojj002"
      
      #add-point:b_fill段fill_before

      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE aooi900_01_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR aooi900_01_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_oojj_m.oojjsite
                                               
      FOREACH b_fill_cs INTO g_oojj_d[l_ac].oojjstus,g_oojj_d[l_ac].oojj006,g_oojj_d[l_ac].oojj003,g_oojj_d[l_ac].oojj004, 
          g_oojj_d[l_ac].oojj001,g_oojj_d[l_ac].oojj002,g_oojj_d[l_ac].oojj005,g_oojj_d[l_ac].oojj007,g_oojj2_d[l_ac].oojjownid,
          g_oojj2_d[l_ac].oojjowndp,g_oojj2_d[l_ac].oojjcrtid,g_oojj2_d[l_ac].oojjcrtdp,g_oojj2_d[l_ac].oojjcrtdt, 
          g_oojj2_d[l_ac].oojjmodid,g_oojj2_d[l_ac].oojjmoddt,g_oojj2_d[l_ac].oojj001,g_oojj2_d[l_ac].oojj002, 
          g_oojj_d[l_ac].oojj003_desc
                             
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
#         INITIALIZE g_ref_fields TO NULL
#         LET g_ref_fields[1] = g_oojj_d[l_ac].oojj003
#         CALL ap_ref_array2(g_ref_fields,"SELECT gzzol003 FROM gzzol_t WHERE gzzol001=? AND gzzol002='"||g_lang||"'", "") RETURNING g_rtn_fields
#         LET g_oojj_d[l_ac].oojj003_desc = '', g_rtn_fields[1] , ''
         CALL s_azzi930_get_module_name(g_oojj_d[l_ac].oojj003) RETURNING g_oojj_d[l_ac].oojj003_desc   #15/06/25 Sarah add
     
         INITIALIZE g_ref_fields TO NULL
         LET g_ref_fields[1] = g_oojj_d[l_ac].oojj001
         CALL ap_ref_array2(g_ref_fields,"SELECT gzdel003 FROM gzdel_t WHERE gzdel001=? AND gzdel002='"||g_lang||"'", "") RETURNING g_rtn_fields
         LET g_oojj_d[l_ac].oojj001_desc = '', g_rtn_fields[1] , ''

       
         INITIALIZE g_ref_fields TO NULL
         LET g_ref_fields[1] = g_oojj_d[l_ac].oojj002
         CALL ap_ref_array2(g_ref_fields,"SELECT oobxl003 FROM oobxl_t WHERE oobxl001=? AND oobxl002='"||g_lang||"' AND oobxlent ='"||g_enterprise||"'", "") RETURNING g_rtn_fields
         LET g_oojj_d[l_ac].oojj002_desc = '', g_rtn_fields[1] , ''
        
       
         INITIALIZE g_ref_fields TO NULL
         LET g_ref_fields[1] = g_oojj_d[l_ac].oojj004
         CALL ap_ref_array2(g_ref_fields,"SELECT gzcbl004 FROM gzcbl_t WHERE gzcbl001='24' AND gzcbl002 =? AND gzcbl003='"||g_lang||"'", "") RETURNING g_rtn_fields
         LET g_oojj_d[l_ac].oojj004_desc = '', g_rtn_fields[1] , ''
         LET g_oojj_d[l_ac].oojj005_desc = s_aooi900_get_oojk004_str(g_oojj_d[l_ac].oojj005,g_lang) 
         #end add-point
      
         #帶出公用欄位reference值
         
         
         
 
 
        
         #add-point:單身資料抓取

         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF 
            EXIT FOREACH
         END IF
      
         LET l_ac = l_ac + 1
         
      END FOREACH
 
            CALL g_oojj_d.deleteElement(g_oojj_d.getLength())
      CALL g_oojj2_d.deleteElement(g_oojj2_d.getLength())
 
      
   END IF
   
   #add-point:b_fill段more

   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
