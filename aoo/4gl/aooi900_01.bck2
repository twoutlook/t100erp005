   OPEN aooi900_01_cl USING g_enterprise,g_oojj_m.oojjsite
 
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aooi900_01_cl:" 
