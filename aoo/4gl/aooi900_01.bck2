PRIVATE FUNCTION aooi900_01_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define

   #end add-point     
   
   IF g_oojj_m.oojjsite IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
 
      RETURN
   END IF
 
   EXECUTE aooi900_01_master_referesh USING g_oojj_m.oojjsite INTO g_oojj_m.oojjent,g_oojj_m.oojjsite
   LET g_oojj_m.oojjsite_desc = s_aooi900_get_desc("oojjsite",g_oojj_m.oojjent,g_oojj_m.oojjsite,0)
   DISPLAY g_oojj_m.oojjsite_desc TO FORMONLY.oojjsite_desc     
   CALL aooi900_01_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN aooi900_01_cl USING g_enterprise,g_oojj_m.oojjsite
 
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aooi900_01_cl:" 
      LET g_errparam.code   =  STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
 
      CLOSE aooi900_01_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aooi900_01_cl INTO g_oojj_m.oojjent,g_oojj_m.oojjsite
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = g_oojj_m.oojjsite 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
 
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL aooi900_01_set_pk_array()
      #add-point:相關文件刪除前

      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前

      #end add-point
      
      DELETE FROM oojj_t WHERE oojjent = g_enterprise AND oojjsite = g_oojj_m.oojjsite
 
                                                               
      #add-point:單身刪除中

      #end add-point
      
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "oojj_t" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
 
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 

      #end add-point
      
 
      
      CLEAR FORM
      CALL g_oojj_d.clear() 
      CALL g_oojj2_d.clear()       
 
     
      CALL aooi900_01_ui_browser_refresh()  
      CALL aooi900_01_ui_headershow()  
      CALL aooi900_01_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL aooi900_01_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL aooi900_01_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE aooi900_01_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_oojj_m.oojjsite,'D')
END FUNCTION

