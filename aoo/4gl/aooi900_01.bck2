PRIVATE FUNCTION aooi900_01_modify()
   #add-point:modify段define

   #end add-point    
   
   IF g_oojj_m.oojjsite IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
 
      RETURN
   END IF
   
   EXECUTE aooi900_01_master_referesh USING g_oojj_m.oojjsite INTO g_oojj_m.oojjent,g_oojj_m.oojjsite
 
   ERROR ""
  
   LET g_oojjsite_t = g_oojj_m.oojjsite
   LET g_oojj_m.oojjsite_desc = s_aooi900_get_desc("oojjsite",g_oojj_m.oojjent,g_oojj_m.oojjsite,0)
