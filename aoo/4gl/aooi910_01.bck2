   IF g_oojo_m.oojosite IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
 
      RETURN
   END IF
   
   EXECUTE aooi910_01_master_referesh USING g_oojo_m.oojosite INTO g_oojo_m.oojoent,g_oojo_m.oojosite
 
   ERROR ""
  
   LET g_oojosite_t = g_oojo_m.oojosite
