PRIVATE FUNCTION aooi910_01_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define

   #end add-point     
   
   IF g_oojo_m.oojosite IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
 
      RETURN
   END IF
 
   EXECUTE aooi910_01_master_referesh USING g_oojo_m.oojosite INTO g_oojo_m.oojoent,g_oojo_m.oojosite
   LET g_oojo_m.oojosite_desc = aooi910_01_get_desc("oojosite",g_oojo_m.oojoent,g_oojo_m.oojosite)
   DISPLAY g_oojo_m.oojosite_desc TO FORMONLY.oojosite_desc     
   CALL aooi910_01_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN aooi910_01_cl USING g_enterprise,g_oojo_m.oojosite
 
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aooi910_01_cl:" 
      LET g_errparam.code   =  STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
 
      CLOSE aooi910_01_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aooi910_01_cl INTO g_oojo_m.oojoent,g_oojo_m.oojosite
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = g_oojo_m.oojosite 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
 
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL aooi910_01_set_pk_array()
      #add-point:相關文件刪除前

      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前

      #end add-point
      
      DELETE FROM oojo_t WHERE oojoent = g_enterprise AND oojosite = g_oojo_m.oojosite
 
                                                               
      #add-point:單身刪除中

      #end add-point
      
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "oojo_t" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
 
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 

      #end add-point
      
 
      
      CLEAR FORM
      CALL g_oojo_d.clear() 
      CALL g_oojo2_d.clear()       
 
     
      CALL aooi910_01_ui_browser_refresh()  
      CALL aooi910_01_ui_headershow()  
      CALL aooi910_01_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL aooi910_01_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL aooi910_01_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE aooi910_01_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_oojo_m.oojosite,'D')
END FUNCTION

