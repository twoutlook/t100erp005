      CALL cl_err()
 
      CLOSE aooi910_01_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aooi910_01_cl INTO g_oojo_m.oojoent,g_oojo_m.oojosite
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
