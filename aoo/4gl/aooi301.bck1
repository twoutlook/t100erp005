           CALL cl_err()
           CALL s_transaction_end('N','0')
           RETURN
        END IF
        #160914-00021#1 add(E) 
                
         #end add-point   
         
         DELETE FROM oocq_t
          WHERE oocqent = g_enterprise AND 
                oocq001 = g_oocq_d[li_idx].oocq001
                AND oocq002 = g_oocq_d[li_idx].oocq002
 
