                  LET l_var_keys_bak[01] = g_enterprise
                  LET l_field_keys[02] = 'oocql001'
                  LET l_var_keys_bak[02] = g_detail_multi_table_t.oocql001
                  LET l_field_keys[03] = 'oocql002'
                  LET l_var_keys_bak[03] = g_detail_multi_table_t.oocql002
