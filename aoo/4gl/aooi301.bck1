                      g_oocq_d[l_ac].oocq023,g_oocq_d[l_ac].oocq024,g_oocq_d[l_ac].oocq025,g_oocq_d[l_ac].oocq026, 
                      g_oocq_d[l_ac].oocq027,g_oocq_d[l_ac].oocq028,g_oocq_d[l_ac].oocq029,g_oocq_d[l_ac].oocq030, 
