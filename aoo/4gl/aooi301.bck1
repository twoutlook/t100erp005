                      g_oocq_d[l_ac].oocq011,g_oocq_d[l_ac].oocq012,g_oocq_d[l_ac].oocq013,g_oocq_d[l_ac].oocq014, 
                      g_oocq_d[l_ac].oocq015,g_oocq_d[l_ac].oocq016,g_oocq_d[l_ac].oocq017,g_oocq_d[l_ac].oocq018, 
