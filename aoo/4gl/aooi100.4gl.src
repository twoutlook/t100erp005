   IF g_ooef_m.ooefstus <> 'Y' THEN
      CALL cl_set_act_visible("delete", FALSE)
   END IF
   #end add-point  
   
   
   
   #保存單頭舊值
   LET g_ooef_m_t.* = g_ooef_m.*
   LET g_ooef_m_o.* = g_ooef_m.*
   
   LET g_data_owner = g_ooef_m.ooefownid      
   LET g_data_dept  = g_ooef_m.ooefowndp
   
   #重新顯示
   CALL aooi100_show()
   
 
