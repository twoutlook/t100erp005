   LET g_ooef_m.ooefstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE ooef_t 
      SET (ooefstus,ooefmodid,ooefmoddt) 
        = (g_ooef_m.ooefstus,g_ooef_m.ooefmodid,g_ooef_m.ooefmoddt)     
    WHERE ooefent = g_enterprise AND ooef001 = g_ooef_m.ooef001
 
    
   IF SQLCA.sqlcode THEN
