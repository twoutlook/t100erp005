       g_ooef_m.ooef017_desc,g_ooef_m.ooefownid_desc,g_ooef_m.ooefowndp_desc,g_ooef_m.ooefcrtid_desc, 
       g_ooef_m.ooefcrtdp_desc,g_ooef_m.ooefmodid_desc,g_ooef_m.ooef014_desc,g_ooef_m.ooef016_desc,g_ooef_m.ooef015_desc, 
       g_ooef_m.ooef006_desc,g_ooef_m.ooef018_desc,g_ooef_m.ooef019_desc,g_ooef_m.ooef025_desc,g_ooef_m.ooef008_desc, 
