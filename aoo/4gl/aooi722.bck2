   LET g_qryparam.state = "c"
   
   #wc備份
   LET ls_wc = g_wc2
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      CONSTRUCT g_wc2 ON ooahstus,ooah001,ooah002,ooah003,ooah004,ooah005,ooah006,ooahownid,ooahowndp, 
          ooahcrtid,ooahcrtdp,ooahcrtdt,ooahmodid,ooahmoddt 
 
         FROM s_detail1[1].ooahstus,s_detail1[1].ooah001,s_detail1[1].ooah002,s_detail1[1].ooah003,s_detail1[1].ooah004, 
             s_detail1[1].ooah005,s_detail1[1].ooah006,s_detail2[1].ooahownid,s_detail2[1].ooahowndp, 
             s_detail2[1].ooahcrtid,s_detail2[1].ooahcrtdp,s_detail2[1].ooahcrtdt,s_detail2[1].ooahmodid, 
             s_detail2[1].ooahmoddt 
      
         #應用 a11 樣板自動產生(Version:3)
         #共用欄位查詢處理  
         ##----<<ooahcrtdt>>----
         AFTER FIELD ooahcrtdt
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
 
         #----<<ooahmoddt>>----
         AFTER FIELD ooahmoddt
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
         
         #----<<ooahcnfdt>>----
         
         #----<<ooahpstdt>>----
 
 
 
      
                  #應用 a01 樣板自動產生(Version:2)
         BEFORE FIELD ooahstus
            #add-point:BEFORE FIELD ooahstus name="query.b.page1.ooahstus"
            
            #END add-point
 
 
         #應用 a02 樣板自動產生(Version:2)
         AFTER FIELD ooahstus
            
            #add-point:AFTER FIELD ooahstus name="query.a.page1.ooahstus"
            
            #END add-point
            
 
 
         #Ctrlp:query.c.page1.ooahstus
#         #應用 a03 樣板自動產生(Version:3)
         ON ACTION controlp INFIELD ooahstus
            #add-point:ON ACTION controlp INFIELD ooahstus name="query.c.page1.ooahstus"
            
            #END add-point
 
 
         #Ctrlp:construct.c.page1.ooah001
         #應用 a03 樣板自動產生(Version:3)
         ON ACTION controlp INFIELD ooah001
            #add-point:ON ACTION controlp INFIELD ooah001 name="construct.c.page1.ooah001"
            #應用 a08 樣板自動產生(Version:3)
            #開窗c段
            INITIALIZE g_qryparam.* TO NULL
            LET g_qryparam.state = 'c' 
            LET g_qryparam.reqry = FALSE
            LET g_qryparam.where = " ooagstus = 'Y' "
            CALL q_ooag001()                           #呼叫開窗
            DISPLAY g_qryparam.return1 TO ooah001  #顯示到畫面上
            NEXT FIELD ooah001                     #返回原欄位
    



            #END add-point
 
 
         #應用 a01 樣板自動產生(Version:2)
         BEFORE FIELD ooah001
            #add-point:BEFORE FIELD ooah001 name="query.b.page1.ooah001"
            
            #END add-point
 
 
