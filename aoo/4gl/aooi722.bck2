#+ 單身陣列填充
PRIVATE FUNCTION aooi722_b_fill(p_wc2)              #BODY FILL UP
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   
   #end add-point
   DEFINE p_wc2    STRING
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   
   #end add-point
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前 name="b_fill.sql_before"
   
   #end add-point
 
   LET g_sql = "SELECT  DISTINCT t0.ooahstus,t0.ooah001,t0.ooah002,t0.ooah003,t0.ooah004,t0.ooah005, 
       t0.ooah006,t0.ooah001,t0.ooah002,t0.ooahownid,t0.ooahowndp,t0.ooahcrtid,t0.ooahcrtdp,t0.ooahcrtdt, 
       t0.ooahmodid,t0.ooahmoddt ,t1.ooag011 ,t2.ooefl003 ,t3.oocql004 ,t4.oocql004 ,t5.ooag011 ,t6.ooag011 , 
       t7.ooag011 ,t8.ooefl003 ,t9.ooag011 ,t10.ooefl003 ,t11.ooag011 FROM ooah_t t0",
               "",
                              " LEFT JOIN ooag_t t1 ON t1.ooagent="||g_enterprise||" AND t1.ooag001=t0.ooah001  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent="||g_enterprise||" AND t2.ooefl001=t0.ooah002 AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t3 ON t3.oocqlent="||g_enterprise||" AND t3.oocql001='5' AND t3.oocql002=t0.ooah003 AND t3.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t4 ON t4.oocqlent="||g_enterprise||" AND t4.oocql001='16' AND t4.oocql002=t0.ooah004 AND t4.oocql003='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=t0.ooah005  ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent="||g_enterprise||" AND t6.ooag001=t0.ooah006  ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent="||g_enterprise||" AND t7.ooag001=t0.ooahownid  ",
               " LEFT JOIN ooefl_t t8 ON t8.ooeflent="||g_enterprise||" AND t8.ooefl001=t0.ooahowndp AND t8.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t9 ON t9.ooagent="||g_enterprise||" AND t9.ooag001=t0.ooahcrtid  ",
               " LEFT JOIN ooefl_t t10 ON t10.ooeflent="||g_enterprise||" AND t10.ooefl001=t0.ooahcrtdp AND t10.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t11 ON t11.ooagent="||g_enterprise||" AND t11.ooag001=t0.ooahmodid  ",
 
               " WHERE t0.ooahent= ?  AND  1=1 AND (", p_wc2, ") " 
   #add-point:b_fill段sql wc name="b_fill.sql_wc"
   
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("ooah_t"),
                      " ORDER BY t0.ooah001,t0.ooah002"
   
   #add-point:b_fill段sql之後 name="b_fill.sql_after"
   
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"ooah_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE aooi722_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aooi722_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_ooah_d.clear()
   CALL g_ooah2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_ooah_d[l_ac].ooahstus,g_ooah_d[l_ac].ooah001,g_ooah_d[l_ac].ooah002,g_ooah_d[l_ac].ooah003, 
       g_ooah_d[l_ac].ooah004,g_ooah_d[l_ac].ooah005,g_ooah_d[l_ac].ooah006,g_ooah2_d[l_ac].ooah001, 
       g_ooah2_d[l_ac].ooah002,g_ooah2_d[l_ac].ooahownid,g_ooah2_d[l_ac].ooahowndp,g_ooah2_d[l_ac].ooahcrtid, 
       g_ooah2_d[l_ac].ooahcrtdp,g_ooah2_d[l_ac].ooahcrtdt,g_ooah2_d[l_ac].ooahmodid,g_ooah2_d[l_ac].ooahmoddt, 
       g_ooah_d[l_ac].ooah001_desc,g_ooah_d[l_ac].ooah002_desc,g_ooah_d[l_ac].ooah003_desc,g_ooah_d[l_ac].ooah004_desc, 
       g_ooah_d[l_ac].ooah005_desc,g_ooah_d[l_ac].ooah006_desc,g_ooah2_d[l_ac].ooahownid_desc,g_ooah2_d[l_ac].ooahowndp_desc, 
       g_ooah2_d[l_ac].ooahcrtid_desc,g_ooah2_d[l_ac].ooahcrtdp_desc,g_ooah2_d[l_ac].ooahmodid_desc
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充 name="b_fill.fill"
      
      #end add-point
      
      CALL aooi722_detail_show()      
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
 
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   LET g_error_show = 0
   
 
   
   CALL g_ooah_d.deleteElement(g_ooah_d.getLength())   
   CALL g_ooah2_d.deleteElement(g_ooah2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_ooah_d.getLength()
      LET g_ooah2_d[l_ac].ooah001 = g_ooah_d[l_ac].ooah001 
      LET g_ooah2_d[l_ac].ooah002 = g_ooah_d[l_ac].ooah002 
 
      #add-point:b_fill段key值相關欄位 name="b_fill.keys.fill"
      
      #end add-point
   END FOR
   
   IF g_cnt > g_ooah_d.getLength() THEN
      LET l_ac = g_ooah_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_ooah_d.getLength()
      LET g_ooah_d_mask_o[l_ac].* =  g_ooah_d[l_ac].*
      CALL aooi722_ooah_t_mask()
      LET g_ooah_d_mask_n[l_ac].* =  g_ooah_d[l_ac].*
   END FOR
   
   LET g_ooah2_d_mask_o.* =  g_ooah2_d.*
   FOR l_ac = 1 TO g_ooah2_d.getLength()
      LET g_ooah2_d_mask_o[l_ac].* =  g_ooah2_d[l_ac].*
      CALL aooi722_ooah_t_mask()
      LET g_ooah2_d_mask_n[l_ac].* =  g_ooah2_d[l_ac].*
   END FOR
 
   
   LET l_ac = g_cnt
 
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_ooah_d.getLength()
   DISPLAY g_detail_idx TO FORMONLY.idx
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE aooi722_pb
   
END FUNCTION
 
