################################################################################
# Descriptions...: SQL驗證
# Memo...........:
# Usage..........: CALL aooi390_sql_verify()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2013/12/12 By xumm
# Modify.........: 151208-00005#1 2015/12/08 By ming 增加在SQL中加入自訂參數的功能
################################################################################
PRIVATE FUNCTION aooi390_sql_verify()
DEFINE p_dzca003          STRING
DEFINE l_dzca003          base.StringBuffer
DEFINE l_index            LIKE type_t.num5
DEFINE l_err              STRING
DEFINE r_value            BOOLEAN
   #151208-00005#1 20151208 add by ming -----(S) 
   DEFINE l_index_start   LIKE type_t.num5
   DEFINE l_index_end     LIKE type_t.num5
   DEFINE l_replace       STRING            #要被取代的文字 
   #151208-00005#1 20151208 add by ming -----(E) 

   LET p_dzca003 = g_oofg_m.oofg013
   #SQL指令必須是由SELECT 語法開始
   LET p_dzca003 = p_dzca003.trim()
   IF NOT p_dzca003 MATCHES "SELECT *" AND NOT p_dzca003 MATCHES "select *"  THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code =  "std-00008"  #adz-00022  #160318-00005#31  By 07900
      LET g_errparam.extend = NULL
      LET g_errparam.popup = TRUE
      LET g_errparam.replace[1] =  " SQL must be 「SELECT xxxxxx」 ! "
      CALL cl_err()

      RETURN FALSE
   END IF

   TRY
      LET l_dzca003 = base.StringBuffer.create()
      CALL l_dzca003.append(p_dzca003)
      LET  r_value = FALSE 
      
      #151208-00005#1 20151208 add -----(S) 
      #檢查SQL中是否有user自定義的符號 
      #LET p_dzca003 = "SELECT * FROM imaa_t WHERE imaaent = :XMDCENT AND imaa001 = :XMDC001" 
      #LET p_dzca003 = "SELECT * FROM imaa_t WHERE imaaent = '99' AND imaa001 = 'ming001'" 

      WHILE TRUE
         #找到「:」所在的位置 
         #因為每一次找到後都會清掉，所以都從第1個字開始找 
         LET l_index_start = l_dzca003.getIndexOf(":",1)
         IF l_index_start <= 0 THEN
            #如果完全找不到就可以離開迴圈 
            EXIT WHILE
         END IF

         LET l_index_end = l_dzca003.getIndexOf(" ",l_index_start)
         IF l_index_end <= 0 THEN
            #如果開頭有抓到:，而最後又取不到空白，則直接取到最後一個字 
            LET l_index_end = l_dzca003.getLength()
         END IF
         #取得應該被替換掉的字段 
         LET l_replace = l_dzca003.subString(l_index_start,l_index_end)

         CALL l_dzca003.replace(l_replace.trim(),"'1'",1)
         #運算結束後的結果 
         # SELECT * FROM imaa_t WHERE imaaent = '1' AND imaa001 = '1' 

      END WHILE
      #151208-00005#1 20151208 add -----(E) 

      #替換掉預設的外部變數名稱，避免影響SQL指令的執行
      CALL l_dzca003.replace("arg1", "1", 0)
      CALL l_dzca003.replace("arg2", "1", 0)
      CALL l_dzca003.replace("arg3", "1", 0)
      CALL l_dzca003.replace("arg4", "1", 0)
      CALL l_dzca003.replace("arg5", "1", 0)
      CALL l_dzca003.replace("arg6", "1", 0)
      CALL l_dzca003.replace("arg7", "1", 0)
      CALL l_dzca003.replace("arg8", "1", 0)
      CALL l_dzca003.replace("arg9", "1", 0)

      #SQL允許使用的公用變數代碼
      CALL l_dzca003.replace(":DLANG", "'1'", 0)
      CALL l_dzca003.replace(":LANG", "'1'", 0)
      CALL l_dzca003.replace(":LEGAL", "'1'", 0)
      CALL l_dzca003.replace(":SITE", "'1'", 0)
      CALL l_dzca003.replace(":USER", "'1'", 0)
      CALL l_dzca003.replace(":DEPT", "'1'", 0)
      CALL l_dzca003.replace(":ENT", "'1'", 0)
      CALL l_dzca003.replace(":TODAY", "'2013/11/20'", 0)

      #另外怕使用者輸入";"會影響SQL執行,因此也先行剔除
      CALL l_dzca003.replace(";", "", 1)

      #因為只是為了要驗證SQL是否可以執行正常而已,所以只SELECT第一筆資料
      #這行以後應該要為了支援多種DB TOOL,要進行不同DB TOOL的語法改寫(CASE...WHEN)
      #LET p_dzca003 = "SELECT * FROM (", l_dzca003.toString(), ") WHERE ROWNUM = 1"
      #LET p_dzca003 = "SELECT COUNT(1) FROM (", l_dzca003.toString(), ") "

      LET p_dzca003 = l_dzca003.toString()

      DISPLAY "sql_verify:", p_dzca003

      PREPARE chk_sql_pre FROM  p_dzca003
      DECLARE chk_sql_cs CURSOR FOR chk_sql_pre

      OPEN chk_sql_cs

      MESSAGE 'Verify OK!'           
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'aoo-00263'
      LET g_errparam.extend = p_dzca003
      LET g_errparam.popup = TRUE
      CALL cl_err()

      LET r_value = TRUE
   CATCH
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code =  "adz-00024"
         LET g_errparam.extend = NULL
         LET g_errparam.popup = TRUE
         LET g_errparam.replace[1] = p_dzca003
         CALL cl_err()

      ELSE
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code =  "std-00008"  #adz-00023  #160318-00005#31  By 07900
         LET g_errparam.extend = NULL
         LET g_errparam.popup = TRUE
         LET g_errparam.replace[1] =  "Please notify administrator." 
         CALL cl_err()

      END IF
      DISPLAY "sql for chk = ",p_dzca003
      LET r_value = FALSE
   END TRY

   FREE chk_sql_cs
   FREE chk_sql_pre

   RETURN r_value
   
END FUNCTION
