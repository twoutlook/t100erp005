{<section id="aooi390.set_no_entry" >}
#+ 單頭欄位關閉設定
PRIVATE FUNCTION aooi390_set_no_entry(p_cmd)
   #add-point:set_no_entry段define name="set_no_entry.define_customerization"
   
   #end add-point     
   DEFINE p_cmd   LIKE type_t.chr1   
   #add-point:set_no_entry段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="set_no_entry.define"
   DEFINE l_cnt   LIKE type_t.num5
   #end add-point     
 
   #add-point:Function前置處理  name="set_no_entry.pre_function"
   
   #end add-point
      
   IF p_cmd = 'u' AND g_chkey = 'N' THEN
      CALL cl_set_comp_entry("oofg001,oofg005",FALSE)
      #根據azzi850使用者身分開關特定欄位
      IF NOT cl_null(g_no_entry) THEN
         CALL cl_set_comp_entry(g_no_entry,FALSE)
      END IF
      #add-point:set_no_entry段欄位控制 name="set_no_entry.field_control"
      
      #end add-point 
   END IF
   
   #add-point:set_no_entry段欄位控制後 name="set_no_entry.after_control"
   LET l_cnt = 0
   SELECT COUNT(1) INTO l_cnt
     FROM oofg_t
    WHERE oofgent = g_enterprise
      AND oofg001 = g_oofg_m.oofg001
   IF l_cnt > 0 THEN
      CALL cl_set_comp_entry("oofg002,oofg003",FALSE)
#      CALL cl_set_comp_entry("oofgl004,oofgl005",FALSE)   #mark--2015/05/05 By shiun
   END IF 
   
   IF g_current_row > 0 AND p_cmd = 'a' THEN
      IF g_browser[g_current_row].b_pid <> '0' THEN
         CALL cl_set_comp_entry("oofg001,oofg002,oofg003",FALSE)
#         CALL cl_set_comp_entry("oofgl004,oofgl005",FALSE)   #mark--2015/05/05 By shiun
      ELSE
         CALL cl_set_comp_entry("oofg006",FALSE)
      END IF 
   ELSE
      CALL cl_set_comp_entry("oofg006",FALSE)
   END IF
   IF g_oofg_m.oofg008 <> '0' THEN
      LET g_oofg_m.oofg010 = ''
      CALL cl_set_comp_entry("oofg010",FALSE)
   END IF
   IF g_oofg_m.oofg008 <> '1' THEN
      LET g_oofg_m.oofg011 = ''
      LET g_oofg_m.oofg012 = ''
      CALL cl_set_comp_entry("oofg011,oofg012",FALSE)
   END IF
   IF g_oofg_m.oofg008 <> '2' THEN
      LET g_oofg_m.oofg013 = ''
      LET g_oofg_m.oofg014 = ''
      LET g_oofg_m.oofg015 = ''
      LET g_oofg_m.column = ''
      LET g_oofg_m.table = ''
      LET g_oofg_m.condition = ''
      CALL cl_set_comp_entry("oofg013,oofg014,oofg015",FALSE)
      CALL cl_set_comp_entry("column,table,condition",FALSE)
   END IF
   IF g_oofg_m.oofg016 <> 'Y' THEN
      LET g_oofg_m.oofg017 = ''
      CALL cl_set_comp_entry("oofg017",FALSE)
   END IF
   IF g_oofg_m.oofg018 <> 'Y' THEN
      LET g_oofg_m.oofg019 = ''
      LET g_oofg_m.oofg020 = ''
      CALL cl_set_comp_entry("oofg019,oofg020",FALSE)
   END IF
   IF g_oofg_m.oofg021 <> 'Y' THEN
      LET g_oofg_m.oofg022 = ''
      LET g_oofg_m.oofg023 = ''
      CALL cl_set_comp_entry("oofg022,oofg023",FALSE)
   END IF
   IF g_oofg_m.oofg008 = '10' THEN
      LET g_oofg_m.oofg009 = ''
      CALL cl_set_comp_entry("oofg009",FALSE)
   END IF
   #20150901-dorislai-add----(S)
   #節點型態選 11 OR 12 OR 13，長度欄位不可輸
   IF g_oofg_m.oofg008 > 10 AND g_oofg_m.oofg008 <16 THEN
      CALL cl_set_comp_entry("oofg009",FALSE)
   END IF
   #20150901-dorislai-add----(E)
   #add by geza 20160608 #160512-00003#28 (S)
   IF g_oofg_m.oofg008 <> '16' THEN
      CALL cl_set_comp_entry("oofg024,oofg025,oofg026",FALSE)
   END IF
   #add by geza 20160608 #160512-00003#28 (E)
   #end add-point 
 
END FUNCTION
 
