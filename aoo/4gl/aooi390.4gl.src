               " WHERE  oofg006 = '", l_keyvalue,
               "' AND   oofg005 <> oofg006",
               " AND  oofg001 = '", l_typevalue,"'", 
               " AND oofgent = ",g_enterprise,              #160614-00024#1
               " ORDER BY oofg005"
   
   LET l_sql = cl_sql_add_mask(l_sql)              #遮蔽特定資料
   PREPARE tree_expand2 FROM l_sql
   DECLARE tree_ex_cur2 CURSOR FOR tree_expand2
  
   LET l_id = p_id + 1
   LET g_cnt = l_id
   CALL g_browser.insertElement(l_id)
   LET l_cnt = 1
   FOREACH tree_ex_cur2 INTO g_browser[l_id].* 
      #pid=父節點id
      LET g_browser[l_id].b_pid  = g_browser[p_id].b_id
      #id=本身節點id(採流水號遞增)
      LET g_browser[l_id].b_id   = g_browser[p_id].b_id||"."||l_cnt
      #hasC=確認該節點是否有子孫
      #LET g_browser[l_id].b_oofg005 = g_browser[l_id].b_oofg005 CLIPPED
      CALL aooi390_desc_show(l_id)
      LET g_browser[l_id].b_hasC = aooi390_chk_hasC(l_id)
      LET l_id = l_id + 1
      LET g_cnt = l_id
      CALL g_browser.insertElement(l_id)
      LET l_cnt = l_cnt + 1
      
      LET l_return = TRUE
   END FOREACH
   
   #刪除空資料
   CALL g_browser.deleteElement(l_id)
   #LET g_browser_cnt = g_browser.getLength() - g_browser_root.getLength()
   LET g_browser_cnt = g_browser.getLength()
   FREE tree_expand2
   RETURN
   #end add-point
 
   #add-point:Function前置處理  name="browser_expand.pre_function"
   
   #end add-point
      
   #若已經展開
   IF g_browser[p_id].b_isExp = 1 THEN
      RETURN
   END IF
   
   LET l_return = FALSE
 
   LET l_keyvalue = g_browser[p_id].b_oofg005
   LET l_typevalue = g_browser[p_id].b_oofg001
   
   CASE g_browser[p_id].b_expcode
      WHEN -1
         CALL g_browser.deleteElement(p_id)
      WHEN 0
         RETURN
      WHEN 1
         LET ls_source = "aooi390_tmp"
         LET ls_exp_code = "exp_code"
      WHEN 2
         LET ls_source = "oofg_t"
         LET ls_exp_code = "'2'"
   END CASE
   
   LET l_sql = " SELECT DISTINCT '','','','FALSE','','',",ls_exp_code,",oofg001,oofg005,oofg006",
               " FROM   ",ls_source,
               
               " WHERE  oofg006 = '", l_keyvalue,
               "' AND   oofg005 <> oofg006",
               " AND  oofg001 = '", l_typevalue,"'", 
               " ORDER BY oofg005"
   
   LET l_sql = cl_sql_add_mask(l_sql)              #遮蔽特定資料
   PREPARE tree_expand FROM l_sql
   DECLARE tree_ex_cur CURSOR FOR tree_expand
  
   LET l_id = p_id + 1
   LET g_cnt = l_id
   CALL g_browser.insertElement(l_id)
   LET l_cnt = 1
   FOREACH tree_ex_cur INTO g_browser[l_id].* 
      #pid=父節點id
      LET g_browser[l_id].b_pid  = g_browser[p_id].b_id
      #id=本身節點id(採流水號遞增)
      LET g_browser[l_id].b_id   = g_browser[p_id].b_id||"."||l_cnt
      #hasC=確認該節點是否有子孫
      #LET g_browser[l_id].b_oofg005 = g_browser[l_id].b_oofg005 CLIPPED
      CALL aooi390_desc_show(l_id)
      LET g_browser[l_id].b_hasC = aooi390_chk_hasC(l_id)
      LET l_id = l_id + 1
      LET g_cnt = l_id
      CALL g_browser.insertElement(l_id)
      LET l_cnt = l_cnt + 1
      
      LET l_return = TRUE
   END FOREACH
   
   #刪除空資料
   CALL g_browser.deleteElement(l_id)
   
   FREE tree_expand
 
END FUNCTION
 
