PRIVATE FUNCTION aooi390_modify()
   #add-point:modify段define name="modify.define_customerization"
   
   #end add-point    
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   
   #end add-point    
 
   #add-point:Function前置處理  name="modify.pre_function"
   
   #end add-point
      
   IF g_oofg_m.oofg001 IS NULL
   OR g_oofg_m.oofg005 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
 
      RETURN
   END IF
   
   EXECUTE aooi390_master_referesh USING g_oofg_m.oofg001,g_oofg_m.oofg005 INTO g_oofg_m.oofg001,g_oofg_m.oofg002, 
       g_oofg_m.oofg003,g_oofg_m.oofg007,g_oofg_m.oofgstus,g_oofg_m.oofg006,g_oofg_m.oofg005,g_oofg_m.oofg008, 
       g_oofg_m.oofg009,g_oofg_m.oofg010,g_oofg_m.oofg011,g_oofg_m.oofg012,g_oofg_m.oofg024,g_oofg_m.oofg025, 
       g_oofg_m.oofg026,g_oofg_m.oofg016,g_oofg_m.oofg017,g_oofg_m.oofg018,g_oofg_m.oofg019,g_oofg_m.oofg020, 
       g_oofg_m.oofg021,g_oofg_m.oofg022,g_oofg_m.oofg023,g_oofg_m.oofgownid,g_oofg_m.oofgowndp,g_oofg_m.oofgcrtid, 
       g_oofg_m.oofgcrtdp,g_oofg_m.oofgcrtdt,g_oofg_m.oofgmodid,g_oofg_m.oofgmoddt,g_oofg_m.oofg013, 
       g_oofg_m.oofg014,g_oofg_m.oofg015,g_oofg_m.oofg004,g_oofg_m.oofg006_desc,g_oofg_m.oofg024_desc, 
       g_oofg_m.oofg025_desc,g_oofg_m.oofg026_desc,g_oofg_m.oofg017_desc,g_oofg_m.oofg022_desc,g_oofg_m.oofg023_desc, 
       g_oofg_m.oofgownid_desc,g_oofg_m.oofgowndp_desc,g_oofg_m.oofgcrtid_desc,g_oofg_m.oofgcrtdp_desc, 
       g_oofg_m.oofgmodid_desc
 
   #檢查是否允許此動作
   IF NOT aooi390_action_chk() THEN
      RETURN
   END IF
  
   LET g_oofg001_t = g_oofg_m.oofg001
   LET g_oofg005_t = g_oofg_m.oofg005
 
   CALL s_transaction_begin()
   
   OPEN aooi390_cl USING g_enterprise,g_oofg_m.oofg001
                                                       ,g_oofg_m.oofg005
 
   IF SQLCA.SQLCODE THEN   #(ver:32)
      CLOSE aooi390_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aooi390_cl:" 
      LET g_errparam.code   = SQLCA.SQLCODE   #(ver:32)
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
 
