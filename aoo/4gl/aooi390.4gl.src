{<section id="aooi390.browser_create" >}
PRIVATE FUNCTION aooi390_browser_create(p_type)
   #add-point:browser_create name="browser_create.define_customerization"
   
   #end add-point
   DEFINE p_type   LIKE type_t.chr50
   DEFINE l_pid    LIKE type_t.chr50
   #add-point:browser_create(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="browser_create.define"
   DEFINE i        LIKE type_t.num5
   DEFINE l_oofg001 LIKE oofg_t.oofg001
   
   DEFINE l_oofg001a  LIKE oofg_t.oofg001,
          l_oofg005  LIKE oofg_t.oofg005
   DEFINE l_sql STRING  
   DEFINE li_idx     LIKE type_t.num5
   
   CALL g_browser.clear()
   
   IF cl_null(g_wc2) OR g_wc2.trim() = "1=1" THEN
      LET g_sql = "SELECT UNIQUE t0.oofg001 ",
                  " FROM oofg_t t0",
                  " WHERE oofgent = '" ||g_enterprise|| "' AND ",g_wc
   ELSE
      LET g_sql = "SELECT UNIQUE t0.oofg001 ",
                  " FROM oofg_t t0",
                  " INNER JOIN oofh_t ",
                  " ON oofh001 = oofg001 AND oofhent = oofgent",     #160614-00024#1
                  " INNER JOIN oofhl_t ",
                  " ON oofh001 = oofhl001 AND oofh002 = oofhl002 AND oofh003 = oofhl003 AND oofhl004 = '",g_dlang,"' AND oofhent = oofhlent ",     #160614-00024#1
                  " WHERE oofgent = '" ||g_enterprise|| "' AND ", g_wc, " AND ", g_wc2
   END IF
   LET g_sql = g_sql ," ORDER BY oofg001"   
   
   #先找出所有的帳別資料   
   #LET g_sql = " SELECT UNIQUE oofg001 FROM oofg_t ORDER BY oofg001"
   PREPARE master_type1 FROM g_sql
   DECLARE master_typecur1 CURSOR FOR master_type1
   
   INITIALIZE g_browser_root TO NULL
   
   LET l_ac = 1
   FOREACH master_typecur1 INTO g_browser[l_ac].b_oofg001
      IF l_ac = 1 THEN 
         LET l_oofg001 = g_browser[l_ac].b_oofg001
      END IF
      #確定root節點所在
      LET g_browser_root[g_browser_root.getLength()+1] = l_ac
      #此處為帳別部分(LV-1)
      LET g_browser[l_ac].b_oofg005  = g_browser[l_ac].b_oofg001
      LET g_browser[l_ac].b_oofg001 = g_browser[l_ac].b_oofg001
      LET g_browser[l_ac].b_pid = '0' CLIPPED
      LET g_browser[l_ac].b_id = l_ac USING "<<<"
      LET g_browser[l_ac].b_exp = TRUE
      LET g_browser[l_ac].b_hasC = TRUE
      LET l_pid = g_browser[l_ac].b_id CLIPPED
      LET l_ac = l_ac + 1
      
      #抓出LV2的所有資料
      LET g_sql = " SELECT UNIQUE oofg001,oofg005,oofg006 FROM oofg_t a ",
                  " WHERE ",
                  "a.oofg001 = ? AND a.oofgent =",g_enterprise,                  #160614-00024#1
                  " AND ",
                  " (( SELECT COUNT(1) FROM oofg_t b WHERE a.oofg006 = b.oofg005 ", 
                  " AND a.oofg001 = b.oofg001 AND a.oofg001 = b.oofg001 ",       #160614-00024#1
                  ") = 0 OR ", 
                  " a.oofg005 = a.oofg006 )", 
                  " ORDER BY a.oofg005"
      PREPARE master_getLV21 FROM g_sql
      DECLARE master_getLV2cur1 CURSOR FOR master_getLV21
      
      #以下為一般資料root(LV-2)
      OPEN master_getLV2cur1 USING g_browser[l_ac-1].b_oofg001
      
      LET g_cnt = l_ac
      
      FOREACH master_getLV2cur1 INTO g_browser[g_cnt].b_oofg001,g_browser[g_cnt].b_oofg005,g_browser[g_cnt].b_oofg006   #,g_browser[g_cnt].b_expcode
         #去除多餘空白
         #LET g_browser[g_cnt].b_oofg005 = g_browser[g_cnt].b_oofg005 CLIPPED
         LET g_browser[g_cnt].b_pid = l_pid
         LET g_browser[g_cnt].b_id = l_pid,".",g_cnt USING "<<<"
         LET g_browser[g_cnt].b_exp = FALSE
         LET g_browser[g_cnt].b_hasC = aooi390_chk_hasC(g_cnt)
#         CASE g_browser[g_cnt].b_expcode
#            WHEN 2
#               LET g_browser[g_cnt].b_hasC = aooi390_chk_hasC(g_cnt)
#            WHEN 1
#               LET g_browser[g_cnt].b_hasC = aooi390_chk_hasC(g_cnt)
#            WHEN 0
#               LET g_browser[g_cnt].b_hasC = FALSE
#            WHEN -1
#               #向下查找到展開值不等於-1得節點(temp table中查找)
#               LET g_cnt = aooi390_find_node(g_cnt)
#         END CASE
 
         LET g_cnt = g_cnt + 1   
         
         IF g_cnt > g_max_rec AND g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code = 9035
            LET g_errparam.extend = ''
            LET g_errparam.popup = FALSE
            CALL cl_err()

            EXIT FOREACH
         END IF
      END FOREACH
      LET l_ac = g_browser.getLength()
      
      IF g_cnt > g_max_rec AND g_error_show = 1 THEN
         EXIT FOREACH
      END IF
   END FOREACH
   
   CALL g_browser.deleteElement(l_ac)
   
   LET g_error_show = 0
   
   #組合描述欄位&刪除多於資料
   FOR l_ac = 1 TO g_browser.getLength()
      IF cl_null(g_browser[l_ac].b_oofg005) THEN
         CALL g_browser.deleteElement(l_ac)
         LET l_ac = l_ac - 1
      ELSE
         CALL aooi390_desc_show(l_ac)
      END IF
   END FOR
   
      #直接把樹不展開
   FOR i = 1 TO g_browser.getLength()
      IF ((g_browser[i].b_oofg001 = g_oofg_m.oofg001 OR g_browser[i].b_oofg001 = g_oofg001_t) AND g_browser[i].b_oofg001<> g_browser[i].b_oofg005) OR (l_oofg001 = g_browser[i].b_oofg001 AND g_browser[i].b_oofg001<> g_browser[i].b_oofg005)  THEN
         #LET g_browser[i].b_expcode = 2
         CALL aooi390_browser_expand(i)
         LET g_browser[i].b_exp = TRUE     #是否展開   TRUE:展開 FALSE:不展開
         LET g_browser[i].b_isExp = 1      #是否已展開 1:展開    2:不展開
      END IF
   END FOR
    
   #LET g_browser_cnt = g_browser.getLength() - g_browser_root.getLength()
   LET g_browser_cnt = g_browser.getLength()
   FREE master_getLV21
 
   RETURN
   #end add-point
 
   #add-point:Function前置處理  name="browser_create.pre_function"
   
   #end add-point
      
   #先找出所有的帳別資料
   LET g_sql = " SELECT DISTINCT oofg001 FROM aooi390_tmp ORDER BY oofg001"
   PREPARE master_type FROM g_sql
   DECLARE master_typecur CURSOR FOR master_type
   
   INITIALIZE g_browser_root TO NULL
   
   LET l_ac = 1
   FOREACH master_typecur INTO g_browser[l_ac].b_oofg001
      #確定root節點所在
      LET g_browser_root[g_browser_root.getLength()+1] = l_ac
      #此處為帳別部分(LV-1)
      LET g_browser[l_ac].b_oofg005  = g_browser[l_ac].b_oofg001
      LET g_browser[l_ac].b_oofg001 = g_browser[l_ac].b_oofg001
      LET g_browser[l_ac].b_pid = '0' CLIPPED
      LET g_browser[l_ac].b_id = l_ac USING "<<<"
      LET g_browser[l_ac].b_exp = TRUE
      LET g_browser[l_ac].b_hasC = TRUE
      LET l_pid = g_browser[l_ac].b_id CLIPPED
      LET l_ac = l_ac + 1
      
      #抓出LV2的所有資料
      LET g_sql = " SELECT DISTINCT t0.oofg001,t0.oofg005,t0.oofg006,exp_code FROM aooi390_tmp a ",
                  
                  " WHERE ",
                  "a.oofg001 = ? ",
                  " AND ",
                  " (( SELECT COUNT(1) FROM aooi390_tmp b WHERE a.oofg006 = b.oofg005 ", 
                  " AND a.oofg001 = b.oofg001",
                  ") = 0 OR ", 
                  " a.oofg005 = a.oofg006 )", 
                  " ORDER BY a.oofg005"
 
      PREPARE master_getLV2 FROM g_sql
      DECLARE master_getLV2cur CURSOR FOR master_getLV2
      
      #以下為一般資料root(LV-2)
      #OPEN master_getLV2cur USING g_browser[l_ac-1].b_oofg001   #(ver:32)
      
      LET g_cnt = l_ac
      
      FOREACH master_getLV2cur   #(ver:32)
         USING g_browser[l_ac-1].b_oofg001   #(ver:32)
         INTO g_browser[g_cnt].b_oofg001,g_browser[g_cnt].b_oofg005,g_browser[g_cnt].b_oofg006,g_browser[g_cnt].b_expcode  
               #(ver:32)
 
         #(ver:32) ---start---
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code   = SQLCA.SQLCODE
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
  
            EXIT FOREACH
         END IF
         #(ver:32) --- end ---
 
         #去除多餘空白
         #LET g_browser[g_cnt].b_oofg005 = g_browser[g_cnt].b_oofg005 CLIPPED
         LET g_browser[g_cnt].b_pid = l_pid
         LET g_browser[g_cnt].b_id = l_pid,".",g_cnt USING "<<<"
         LET g_browser[g_cnt].b_exp = FALSE
         #CASE g_browser[g_cnt].b_expcode
         #   WHEN 2
         #      LET g_browser[g_cnt].b_hasC = aooi390_chk_hasC(g_cnt)
         #   WHEN 1
         #      LET g_browser[g_cnt].b_hasC = aooi390_chk_hasC(g_cnt)
         #   WHEN 0
         #      LET g_browser[g_cnt].b_hasC = FALSE
         #   WHEN -1
         #      #向下查找到展開值不等於-1得節點(temp table中查找)
         #      LET g_cnt = aooi390_find_node(g_cnt)
         #END CASE
         LET g_browser[g_cnt].b_hasC = TRUE
 
         LET g_cnt = g_cnt + 1   
         
         IF g_cnt > g_max_rec AND g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = g_cnt
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
            EXIT FOREACH
         END IF
      END FOREACH
      LET l_ac = g_browser.getLength()
      
      IF g_cnt > g_max_rec AND g_error_show = 1 THEN
         EXIT FOREACH
      END IF
   END FOREACH
   
   LET g_error_show = 0
   
   #組合描述欄位&刪除多於資料
   FOR l_ac = 1 TO g_browser.getLength()
      IF cl_null(g_browser[l_ac].b_oofg005) THEN
         CALL g_browser.deleteElement(l_ac)
         LET l_ac = l_ac - 1
      ELSE
         CALL aooi390_desc_show(l_ac)
      END IF
   END FOR
   CALL g_browser.deleteElement(l_ac)
   
   LET g_browser_cnt = g_browser.getLength() - g_browser_root.getLength()
   
   FREE master_getLV2
   
END FUNCTION
 
