PRIVATE FUNCTION aooi390_chk_oofg006()
DEFINE l_n2      LIKE type_t.num5
DEFINE l_count   LIKE type_t.num5   #add--2015/05/11 By shiun

   LET g_errno = ""
   LET l_n2 = 0
   SELECT COUNT(1) INTO l_n2
     FROM oofg_t
    WHERE oofgent = g_enterprise
      AND oofg001 = g_oofg_m.oofg001
      AND oofg005 = g_oofg_m.oofg006
      AND oofgstus = 'Y'
  #IF l_n2 = 0 AND g_oofg_m.oofg006 <> g_oofg_m.oofg001 THEN      #160614-00024#2 mark
   IF l_n2 = 0 THEN      #160614-00024#2
      IF NOT cl_null(g_oofg_m.oofg005) AND g_oofg_m.oofg005 <> g_oofg_m.oofg006 THEN      #160614-00024#2
         LET g_errno = 'sub-01302'  #aoo-00249  #160318-00005#31   By 07900--mod 
         RETURN   #add--2015/05/11 By shiun
      END IF         #160614-00024#2
   END IF
   #add--2015/05/11 By shiun--(S)
   IF g_oofg_m.oofg002 = '30' THEN
      SELECT COUNT(1) INTO l_n2
        FROM oofg_t
       WHERE oofgent = g_enterprise
         AND oofg001 = g_oofg_m.oofg001
         AND oofg006 = g_oofg_m.oofg006
         AND oofgstus = 'Y'
      SELECT COUNT(1) INTO l_count
        FROM oofg_t
       WHERE oofgent = g_enterprise
         AND oofg001 = g_oofg_m.oofg001
         AND oofg005 = g_oofg_m.oofg006
         AND oofg006 = g_oofg_m.oofg006
         AND oofgstus = 'Y'
      IF cl_null(l_count) THEN
         LET l_count = 0
      END IF
      LET l_n2 = l_n2 - l_count
      IF l_n2 > 0 THEN
         LET g_errno = 'aoo-00638'
      END IF
   END IF               
   #add--2015/05/11 By shiun--(E)
   
END FUNCTION
