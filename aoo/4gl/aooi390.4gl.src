{<section id="aooi390.chk_hasC" >}
#+ QBE資料查詢
PRIVATE FUNCTION aooi390_chk_hasC(pi_id)
   #add-point:chk_hasC段define name="chk_hasC.define_customerization"
   
   #end add-point
   DEFINE pi_id    INTEGER
   DEFINE li_cnt   INTEGER
   #add-point:chk_hasC段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="chk_hasC.define"

 
   LET g_sql = "SELECT COUNT(1) FROM oofg_t ",
               " WHERE oofgent = '" ||g_enterprise|| "' AND ",
               "oofg005 <> oofg006 AND ",
               "oofg006 = ? "
                ," AND ",
               "oofg001 = ?",
               cl_sql_add_filter("oofg_t")     
   
   PREPARE aooi390_master_chk2 FROM g_sql

   EXECUTE aooi390_master_chk2 
     USING g_browser[pi_id].b_oofg005
           ,g_browser[pi_id].b_oofg001
      INTO li_cnt
   FREE aooi390_master_chk2

   
   IF li_cnt > 0 THEN
      RETURN TRUE
   ELSE
      RETURN FALSE
   END IF
   #end add-point
 
   #add-point:Function前置處理  name="chk_hasC.pre_function"
   
   #end add-point
      
   LET g_sql = "SELECT COUNT(oofg006) FROM aooi390_tmp ",
               " WHERE ",
                "oofg006 = ? AND ",
                "exp_code <> '-1' AND oofg005 <> oofg006 "
                 ," AND ",
                "oofg001 = ?"
 
   PREPARE aooi390_temp_chk FROM g_sql
 
   LET g_sql = "SELECT COUNT(1) FROM oofg_t ",
               " WHERE oofgent = " ||g_enterprise|| " AND ",
               "oofg005 <> oofg006 AND ",
               "oofg006 = ? "
                ," AND ",
               "oofg001 = ?",
               cl_sql_add_filter("oofg_t")     
   
   PREPARE aooi390_master_chk FROM g_sql
   
   CASE g_browser[pi_id].b_expcode 
      WHEN -1
         RETURN FALSE
      WHEN 0
         RETURN FALSE
      WHEN 1
         EXECUTE aooi390_temp_chk 
           USING g_browser[pi_id].b_oofg005
                 ,g_browser[pi_id].b_oofg001
            INTO li_cnt
         FREE aooi390_temp_chk
      WHEN 2 
         EXECUTE aooi390_master_chk 
           USING g_browser[pi_id].b_oofg005
                 ,g_browser[pi_id].b_oofg001
            INTO li_cnt
         FREE aooi390_master_chk
   END CASE
   
   IF li_cnt > 0 THEN
      RETURN TRUE
   ELSE
      RETURN FALSE
   END IF
 
END FUNCTION
 
