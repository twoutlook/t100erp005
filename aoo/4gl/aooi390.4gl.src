{<section id="aooi390.browser_fill" >}
#+ 瀏覽頁簽where條件組成
PRIVATE FUNCTION aooi390_browser_fill()
   #add-point:browser_fill段define name="browser_fill.define_customerization"
   
   #end add-point
   DEFINE l_cnt      LIKE type_t.num10
   DEFINE l_cnt2     LIKE type_t.num10
   #add-point:browser_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="browser_fill.define"
   DEFINE li_idx     LIKE type_t.num5  
   DEFINE l_oofg001  LIKE oofg_t.oofg001,
          l_oofg005  LIKE oofg_t.oofg005
   DEFINE l_sql STRING       

   CALL g_browser.clear()
   CLEAR FORM   
   
   CALL aooi390_browser_create(g_searchtype)
   #判斷前一個動作是否為新增, 若是的話切換到新增的筆數
   IF g_state = "Y" THEN
      FOR li_idx = 1 TO g_browser.getLength()
         IF g_browser[li_idx].b_oofg001 = g_oofg001_t
           AND g_browser[li_idx].b_oofg005 = g_oofg005_t
 
            THEN
            LET g_current_row = li_idx
            LET g_current_idx = li_idx
            EXIT FOR
         END IF
      END FOR
      LET g_state = "" 
   ELSE
      IF NOT (cl_null(g_wc2) OR g_wc2.trim() = "1=1") THEN
         LET l_sql = " SELECT oofg001,oofg005 ",
                     "   FROM oofg_t,oofh_t,oofhl_t",
                     "  WHERE oofgent = oofhent AND oofg001 = oofh001 AND oofg005 = oofh002 ",
                     "    AND oofhent = oofhlent AND oofh001=oofhl001 AND oofh002=oofhl002 ",
                     "    AND oofgent = '",g_enterprise,"' AND ", g_wc, " AND ", g_wc2,
                     " ORDER BY oofg001"                     
         PREPARE get_index_pre FROM l_sql
         DECLARE get_index_cur SCROLL CURSOR FOR get_index_pre
         OPEN get_index_cur
         FETCH FIRST get_index_cur INTO l_oofg001,l_oofg005        
         FOR li_idx = 1 TO g_browser.getLength()
            IF g_browser[li_idx].b_oofg001 = l_oofg001
              AND g_browser[li_idx].b_oofg005 = l_oofg005
        
               THEN
               LET g_current_row = li_idx
               LET g_current_idx = li_idx
               EXIT FOR
            END IF
         END FOR                  
      END IF                  
   END IF    
   RETURN
   #end add-point
 
   #add-point:Function前置處理  name="browser_fill.pre_function"
   
   #end add-point
      
   CALL g_browser.clear()
   CLEAR FORM
   
   DROP TABLE aooi390_tmp
   
   #Create temp table
   CREATE TEMP TABLE aooi390_tmp
   (
         oofg001 VARCHAR(500),
   oofg005 VARCHAR(500),
   oofg006 VARCHAR(500),
      #僅含browser的欄位
      exp_code  VARCHAR(5)
   );
 
   LET g_root_search = FALSE
   IF (g_wc.trim() = "1=1"  OR cl_null(g_wc))  AND 
      (g_wc2.trim() = "1=1" OR cl_null(g_wc2)) THEN
      #未輸入條件時則只查找root節點
      LET g_wc = " oofg005 = oofg006 "
      LET g_root_search = TRUE
   END IF
 
   IF cl_null(g_wc2) OR g_wc2.trim() = "1=1" THEN
      LET g_sql = "SELECT t0.oofg001,t0.oofg005,t0.oofg006 ",
                  " FROM oofg_t t0",
                  " WHERE oofgent = " ||g_enterprise|| " AND ",g_wc
   ELSE
      LET g_sql = "SELECT t0.oofg001,t0.oofg005,t0.oofg006 ",
                  " FROM oofg_t t0",
                  " INNER JOIN oofh_t ",
                  " ON oofh001 = oofg001 ",
 
                  " WHERE oofgent = " ||g_enterprise|| " AND ", g_wc, " AND ", g_wc2
   END IF
   LET g_sql = g_sql ,cl_sql_add_filter("oofg_t")
  
   #LET g_sql = cl_sql_add_tabid(g_sql,"oofg_t")             #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料   
   PREPARE master_ext FROM g_sql
   DECLARE master_extcur CURSOR FOR master_ext
 
   #搜尋建構樹所需的節點
   CASE g_searchtype
      WHEN "1" #上推
         CALL aooi390_match_node(g_wc,g_searchtype) 
      WHEN "2" #下展
         #CALL aooi390_find_speed_tbl(g_wc,g_searchtype) 
         CALL aooi390_match_node(g_wc,g_searchtype) 
      WHEN "3" #全部
         CALL aooi390_match_node(g_wc,g_searchtype) 
   END CASE
 
   CALL aooi390_browser_create(g_searchtype)
   
   FREE master_ext
     
   #若無資料則關閉相關功能
   IF g_browser_cnt = 0 THEN
      CALL cl_set_act_visible("statechange,modify,delete,reproduce", FALSE)
   ELSE
      CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   END IF
     
   #add-point:browser_fill段結束前 name="browser_fill.after"
   
   #end add-point   
     
END FUNCTION
 
