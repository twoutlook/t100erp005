################################################################################
# Descriptions...: 長度欄位帶值
# Memo...........:
# Usage..........: CALL aooi390_oofg_def()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2013/11/28 By xumm
# Modify.........:
################################################################################
PRIVATE FUNCTION aooi390_oofg_def()
DEFINE l_oofg009      LIKE oofg_t.oofg009
DEFINE l_oofg005      LIKE oofg_t.oofg005
DEFINE l_oofg006      LIKE oofg_t.oofg006
DEFINE l_sum          LIKE type_t.num5
DEFINE l_sum1         LIKE type_t.num5
DEFINE l_sum2         LIKE type_t.num5
DEFINE l_level        LIKE type_t.num10
DEFINE l_sql          STRING

   LET g_oofg_m.oofg009_1 = g_oofg_m.oofg009
   DISPLAY BY NAME g_oofg_m.oofg009_1
   LET l_sum = 0
   LET l_oofg009 = 0
   IF NOT cl_null(g_oofg_m.oofg001) AND NOT cl_null(g_oofg_m.oofg007) AND NOT cl_null(g_oofg_m.oofg009) THEN
      LET l_sql = "  SELECT LEVEL,oofg005,oofg006,oofg009 FROM  (SELECT oofg001,oofg005,oofg006,oofg009 ",
                  "                                FROM oofg_t ",
                  "                                WHERE oofgent = '",g_enterprise,"' ",
                  "                                  AND oofg001 = '",g_oofg_m.oofg001,"' ",
                  "                                  AND oofg005 <> '",g_oofg_m.oofg001,"' ",
                  "                                  AND oofg005 <> oofg006 ) ",
                  "   START WITH oofg005 = '",g_oofg_m.oofg006,"' ",
                  " CONNECT BY PRIOR oofg006 = oofg005 ",
                  " ORDER BY LEVEL "
      PREPARE aooi390_sel_oofg009_pr FROM l_sql
      DECLARE aooi390_sel_oofg009_cs CURSOR FOR aooi390_sel_oofg009_pr
      FOREACH aooi390_sel_oofg009_cs INTO l_level,l_oofg005,l_oofg006,l_oofg009
         IF cl_null(l_oofg009) THEN
            LET l_oofg009 = 0
         END IF
         LET l_sum = l_sum +l_oofg009
      END FOREACH
      IF cl_null(l_oofg006) AND g_oofg_m.oofg005 <> g_oofg_m.oofg006 THEN
         LET l_sum1 = 0
         SELECT oofg009 INTO l_sum1
           FROM oofg_t
          WHERE oofgent = g_enterprise
            AND oofg001 = g_oofg_m.oofg001
            AND oofg005 = g_oofg_m.oofg006
         IF cl_null(l_sum1) THEN
            LET l_sum1 = 0
         END IF
      END IF
      IF l_oofg005 <> l_oofg006 THEN
         LET l_sum2 = 0
         SELECT oofg009 INTO l_sum2
           FROM oofg_t
          WHERE oofgent = g_enterprise
            AND oofg001 = g_oofg_m.oofg001
            AND oofg005 = l_oofg006
         IF cl_null(l_sum2) THEN
            LET l_sum2 = 0
         END IF
      END IF
      LET g_oofg_m.oofg009_sum = l_sum + l_sum1 +l_sum2
      DISPLAY BY NAME g_oofg_m.oofg009_sum
      IF NOT cl_null(g_oofg_m.oofg003) THEN
         LET g_oofg_m.oofg009_after = g_oofg_m.oofg003 - g_oofg_m.oofg009_sum - g_oofg_m.oofg009
         DISPLAY BY NAME g_oofg_m.oofg009_after
      END IF   
   END IF
END FUNCTION
