{<section id="aooi390.state_change" >}
   #應用 a09 樣板自動產生(Version:17)
#+ 確認碼變更 
PRIVATE FUNCTION aooi390_statechange()
   #add-point:statechange段define(客製用) name="statechange.define_customerization"
   
   #end add-point  
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="statechange.define"
   
   #end add-point  
   
   #add-point:Function前置處理 name="statechange.before"
   
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_oofg_m.oofg001 IS NULL
      OR g_oofg_m.oofg005 IS NULL
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN aooi390_cl USING g_enterprise,g_oofg_m.oofg001,g_oofg_m.oofg005
   IF STATUS THEN
      CLOSE aooi390_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aooi390_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE aooi390_master_referesh USING g_oofg_m.oofg001,g_oofg_m.oofg005 INTO g_oofg_m.oofg001,g_oofg_m.oofg002, 
       g_oofg_m.oofg003,g_oofg_m.oofg007,g_oofg_m.oofgstus,g_oofg_m.oofg006,g_oofg_m.oofg005,g_oofg_m.oofg008, 
       g_oofg_m.oofg009,g_oofg_m.oofg010,g_oofg_m.oofg011,g_oofg_m.oofg012,g_oofg_m.oofg024,g_oofg_m.oofg025, 
       g_oofg_m.oofg026,g_oofg_m.oofg016,g_oofg_m.oofg017,g_oofg_m.oofg018,g_oofg_m.oofg019,g_oofg_m.oofg020, 
       g_oofg_m.oofg021,g_oofg_m.oofg022,g_oofg_m.oofg023,g_oofg_m.oofgownid,g_oofg_m.oofgowndp,g_oofg_m.oofgcrtid, 
       g_oofg_m.oofgcrtdp,g_oofg_m.oofgcrtdt,g_oofg_m.oofgmodid,g_oofg_m.oofgmoddt,g_oofg_m.oofg013, 
       g_oofg_m.oofg014,g_oofg_m.oofg015,g_oofg_m.oofg004,g_oofg_m.oofg006_desc,g_oofg_m.oofg024_desc, 
       g_oofg_m.oofg025_desc,g_oofg_m.oofg026_desc,g_oofg_m.oofg017_desc,g_oofg_m.oofg022_desc,g_oofg_m.oofg023_desc, 
       g_oofg_m.oofgownid_desc,g_oofg_m.oofgowndp_desc,g_oofg_m.oofgcrtid_desc,g_oofg_m.oofgcrtdp_desc, 
       g_oofg_m.oofgmodid_desc
   
 
   #檢查是否允許此動作
   IF NOT aooi390_action_chk() THEN
      CLOSE aooi390_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_oofg_m.oofg001,g_oofg_m.oofgl004,g_oofg_m.oofgl005,g_oofg_m.oofg002,g_oofg_m.oofg003, 
       g_oofg_m.oofg007,g_oofg_m.oofgstus,g_oofg_m.oofg006,g_oofg_m.oofg006_desc,g_oofg_m.oofg005,g_oofg_m.oofgl004_1, 
       g_oofg_m.oofg008,g_oofg_m.oofg009,g_oofg_m.oofg010,g_oofg_m.oofg011,g_oofg_m.oofg012,g_oofg_m.oofg024, 
       g_oofg_m.oofg024_desc,g_oofg_m.oofg025,g_oofg_m.oofg025_desc,g_oofg_m.oofg026,g_oofg_m.oofg026_desc, 
       g_oofg_m.oofg016,g_oofg_m.oofg017,g_oofg_m.oofg017_desc,g_oofg_m.oofg018,g_oofg_m.oofg019,g_oofg_m.oofg020, 
       g_oofg_m.oofg021,g_oofg_m.oofg022,g_oofg_m.oofg022_desc,g_oofg_m.oofg023,g_oofg_m.oofg023_desc, 
       g_oofg_m.oofgownid,g_oofg_m.oofgownid_desc,g_oofg_m.oofgowndp,g_oofg_m.oofgowndp_desc,g_oofg_m.oofgcrtid, 
       g_oofg_m.oofgcrtid_desc,g_oofg_m.oofgcrtdp,g_oofg_m.oofgcrtdp_desc,g_oofg_m.oofgcrtdt,g_oofg_m.oofgmodid, 
       g_oofg_m.oofgmodid_desc,g_oofg_m.oofgmoddt,g_oofg_m.column,g_oofg_m.table,g_oofg_m.condition, 
       g_oofg_m.oofg013,g_oofg_m.oofg014,g_oofg_m.oofg015,g_oofg_m.oofg009_sum,g_oofg_m.oofg009_1,g_oofg_m.oofg009_after, 
       g_oofg_m.oofg004
 
   CASE g_oofg_m.oofgstus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_oofg_m.oofgstus
            
            WHEN "N"
               HIDE OPTION "inactive"
            WHEN "Y"
               HIDE OPTION "active"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      
      #end add-point
      
      
	  
      ON ACTION inactive
         IF cl_auth_chk_act("inactive") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.inactive"
            
            #end add-point
         END IF
         EXIT MENU
      ON ACTION active
         IF cl_auth_chk_act("active") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.active"
            
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      ) OR 
      g_oofg_m.oofgstus = lc_state OR cl_null(lc_state) THEN
      CLOSE aooi390_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   IF NOT cl_ask_confirm('aim-00193') THEN
      CALL s_transaction_end('N','0')   #160812-00017#3 Add By Ken 160815
      RETURN
   ELSE
      UPDATE oofg_t SET oofgstus = lc_state 
       WHERE oofgent = g_enterprise
         AND oofg001 = g_oofg_m.oofg001
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = ""
         LET g_errparam.popup = FALSE
         CALL cl_err()
         CALL s_transaction_end('N','0')   #160812-00017#3 Add By Ken 160815
         RETURN
      END IF
   END IF
   #end add-point
   
   LET g_oofg_m.oofgmodid = g_user
   LET g_oofg_m.oofgmoddt = cl_get_current()
   LET g_oofg_m.oofgstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE oofg_t 
      SET (oofgstus,oofgmodid,oofgmoddt) 
        = (g_oofg_m.oofgstus,g_oofg_m.oofgmodid,g_oofg_m.oofgmoddt)     
    WHERE oofgent = g_enterprise AND oofg001 = g_oofg_m.oofg001
      AND oofg005 = g_oofg_m.oofg005
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE aooi390_master_referesh USING g_oofg_m.oofg001,g_oofg_m.oofg005 INTO g_oofg_m.oofg001, 
          g_oofg_m.oofg002,g_oofg_m.oofg003,g_oofg_m.oofg007,g_oofg_m.oofgstus,g_oofg_m.oofg006,g_oofg_m.oofg005, 
          g_oofg_m.oofg008,g_oofg_m.oofg009,g_oofg_m.oofg010,g_oofg_m.oofg011,g_oofg_m.oofg012,g_oofg_m.oofg024, 
          g_oofg_m.oofg025,g_oofg_m.oofg026,g_oofg_m.oofg016,g_oofg_m.oofg017,g_oofg_m.oofg018,g_oofg_m.oofg019, 
          g_oofg_m.oofg020,g_oofg_m.oofg021,g_oofg_m.oofg022,g_oofg_m.oofg023,g_oofg_m.oofgownid,g_oofg_m.oofgowndp, 
          g_oofg_m.oofgcrtid,g_oofg_m.oofgcrtdp,g_oofg_m.oofgcrtdt,g_oofg_m.oofgmodid,g_oofg_m.oofgmoddt, 
          g_oofg_m.oofg013,g_oofg_m.oofg014,g_oofg_m.oofg015,g_oofg_m.oofg004,g_oofg_m.oofg006_desc, 
          g_oofg_m.oofg024_desc,g_oofg_m.oofg025_desc,g_oofg_m.oofg026_desc,g_oofg_m.oofg017_desc,g_oofg_m.oofg022_desc, 
          g_oofg_m.oofg023_desc,g_oofg_m.oofgownid_desc,g_oofg_m.oofgowndp_desc,g_oofg_m.oofgcrtid_desc, 
          g_oofg_m.oofgcrtdp_desc,g_oofg_m.oofgmodid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_oofg_m.oofg001,g_oofg_m.oofgl004,g_oofg_m.oofgl005,g_oofg_m.oofg002,g_oofg_m.oofg003, 
          g_oofg_m.oofg007,g_oofg_m.oofgstus,g_oofg_m.oofg006,g_oofg_m.oofg006_desc,g_oofg_m.oofg005, 
          g_oofg_m.oofgl004_1,g_oofg_m.oofg008,g_oofg_m.oofg009,g_oofg_m.oofg010,g_oofg_m.oofg011,g_oofg_m.oofg012, 
          g_oofg_m.oofg024,g_oofg_m.oofg024_desc,g_oofg_m.oofg025,g_oofg_m.oofg025_desc,g_oofg_m.oofg026, 
          g_oofg_m.oofg026_desc,g_oofg_m.oofg016,g_oofg_m.oofg017,g_oofg_m.oofg017_desc,g_oofg_m.oofg018, 
          g_oofg_m.oofg019,g_oofg_m.oofg020,g_oofg_m.oofg021,g_oofg_m.oofg022,g_oofg_m.oofg022_desc, 
          g_oofg_m.oofg023,g_oofg_m.oofg023_desc,g_oofg_m.oofgownid,g_oofg_m.oofgownid_desc,g_oofg_m.oofgowndp, 
          g_oofg_m.oofgowndp_desc,g_oofg_m.oofgcrtid,g_oofg_m.oofgcrtid_desc,g_oofg_m.oofgcrtdp,g_oofg_m.oofgcrtdp_desc, 
          g_oofg_m.oofgcrtdt,g_oofg_m.oofgmodid,g_oofg_m.oofgmodid_desc,g_oofg_m.oofgmoddt,g_oofg_m.column, 
          g_oofg_m.table,g_oofg_m.condition,g_oofg_m.oofg013,g_oofg_m.oofg014,g_oofg_m.oofg015,g_oofg_m.oofg009_sum, 
          g_oofg_m.oofg009_1,g_oofg_m.oofg009_after,g_oofg_m.oofg004
   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
   
   #end add-point
 
   #add-point:statechange段結束前 name="statechange.after"
   
   #end add-point  
 
   CLOSE aooi390_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aooi390_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION
 
 
 
 
