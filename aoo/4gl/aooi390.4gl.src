{<section id="aooi390.insert" >}
#+ 資料新增
PRIVATE FUNCTION aooi390_insert()
   #add-point:insert段define name="insert.define_customerization"
   
   #end add-point    
   #add-point:insert段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="insert.define"
   
   #end add-point    
 
   #add-point:Function前置處理  name="insert.pre_function"
   
   #end add-point
      
   CLEAR FORM                                #清畫面欄位內容
   CALL g_oofh_d.clear()          #清除陣列
 
   
   INITIALIZE g_oofg_m.* LIKE oofg_t.*             #DEFAULT 設定
   
   LET g_oofg001_t = NULL
   LET g_oofg005_t = NULL
 
   LET g_oofg006_t = NULL
   
   CALL s_transaction_begin()
   WHILE TRUE
      #公用欄位給值(單頭)
      #應用 a14 樣板自動產生(Version:5)    
      #公用欄位新增給值  
      LET g_oofg_m.oofgownid = g_user
      LET g_oofg_m.oofgowndp = g_dept
      LET g_oofg_m.oofgcrtid = g_user
      LET g_oofg_m.oofgcrtdp = g_dept 
      LET g_oofg_m.oofgcrtdt = cl_get_current()
      LET g_oofg_m.oofgmodid = g_user
      LET g_oofg_m.oofgmoddt = cl_get_current()
      LET g_oofg_m.oofgstus = 'Y'
 
 
 
 
      #append欄位給值
      
     
      #一般欄位給值
            LET g_oofg_m.oofg016 = "N"
      LET g_oofg_m.oofg018 = "N"
      LET g_oofg_m.oofg021 = "N"
 
  
      #add-point:單頭預設值 name="insert.default"
      DISPLAY ARRAY g_browser TO s_browse.* ATTRIBUTES(COUNT=g_header_cnt)
         BEFORE DISPLAY
            EXIT DISPLAY
      END DISPLAY      
      INITIALIZE g_oofg_m_t.* TO NULL 
      LET g_oofg_m.oofgstus = "Y"
      IF g_current_row > 0 THEN
         IF g_browser[g_current_row].b_pid = '0' THEN
            LET g_oofg_m.oofg007 = 1
         ELSE
            LET g_oofg_m.oofg001 = g_browser[g_current_row].b_oofg001
            SELECT oofg002,oofg003 INTO g_oofg_m.oofg002,g_oofg_m.oofg003
              FROM oofg_t
             WHERE oofgent = g_enterprise
               AND oofg001 = g_browser[g_current_row].b_oofg001
               AND oofg005 = g_browser[g_current_row].b_oofg005
            SELECT oofgl004,oofgl005 INTO g_oofg_m.oofgl004,g_oofg_m.oofgl005
              FROM oofgl_t
             WHERE oofglent = g_enterprise
               AND oofgl001 = g_browser[g_current_row].b_oofg001
               AND oofgl002 = ' '               
            SELECT oofg005,oofg007+1 INTO g_oofg_m.oofg006,g_oofg_m.oofg007
              FROM oofg_t
             WHERE oofgent = g_enterprise
               AND oofg001 = g_browser[g_current_row].b_oofg001
               AND oofg005 = g_browser[g_current_row].b_oofg005
            IF cl_null(g_oofg_m.oofg007) THEN
               LET g_oofg_m.oofg007 = 1
            END IF
         END IF 
      ELSE
         LET g_oofg_m.oofg007 = 1
      END IF
      CALL aooi390_desc()
      CALL aooi390_set_entry('a')
      CALL aooi390_set_no_entry('a')
      #end add-point 
     
      CALL aooi390_input("a")
      
      #add-point:單頭輸入後 name="insert.after_insert"
      CALL aooi390_update_oofg004()
      IF NOT INT_FLAG THEN 
         LET g_state = "Y"
      END IF
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_oofg_m.* = g_oofg_m_t.*
         CALL aooi390_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
 
         EXIT WHILE
      END IF
      
      CALL g_oofh_d.clear()
 
 
      LET g_rec_b = 0
      EXIT WHILE
        
   END WHILE
   
   #功能已完成,通報訊息中心
   CALL aooi390_msgcentre_notify('insert')
   
END FUNCTION
 
