  <field cite_std="N" name="oogd_t.oogd006" ver="1" src="s" column="oogd006" table="oogd_t" attribute="N403" type="number(15,3)" req="N" i_zoom="" c_zoom="" chk_ref="" items="" default="" max="" min="" can_edit="Y" can_query="Y" widget="Edit" status="">
