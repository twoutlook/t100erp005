  <section id="aooi120.main" ver="1" status="" src="s">
<![CDATA[#+ 此段落由子樣板a26產生
#OPTIONS SHORT CIRCUIT
#+ 作業開始 
MAIN
   #add-point:main段define
   {<point name="main.define"/>}
   #end add-point   
 
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("aoo","")
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = "SELECT ooei001,'','',ooei003,ooei017,'',ooeistus,ooeiownid,'',ooeiowndp,'',ooeicrtid, 
       '',ooeicrtdp,'',ooeicrtdt,ooeimodid,'',ooeimoddt,ooei201,ooei202,ooei203,ooei204,ooei205,ooei206, 
       ooei207,ooei208,ooei209,ooei210,ooei211,ooei212,ooei301,ooei302,ooei303,ooei304,ooei305,ooei008, 
       '',ooei009,ooei010,ooei004,'',ooei011,ooei013,ooei023,ooei014,'',ooei016,'',ooei015,'',ooei006, 
       '',ooei018,'',ooei019,'' FROM ooei_t WHERE ooeient= ? AND ooei001=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   DECLARE aooi120_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE ooei001,ooei003,ooei017,ooeistus,ooeiownid,ooeiowndp,ooeicrtid,ooeicrtdp, 
       ooeicrtdt,ooeimodid,ooeimoddt,ooei201,ooei202,ooei203,ooei204,ooei205,ooei206,ooei207,ooei208, 
       ooei209,ooei210,ooei211,ooei212,ooei301,ooei302,ooei303,ooei304,ooei305,ooei008,ooei009,ooei010, 
       ooei004,ooei011,ooei013,ooei023,ooei014,ooei016,ooei015,ooei006,ooei018,ooei019",
               " FROM ooei_t",
               " WHERE ooeient = '" ||g_enterprise|| "' AND ooei001 = ?"
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE aooi120_master_referesh FROM g_sql
 
 
 
 
   IF g_bgjob = "Y" THEN
 
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
 
   ELSE
      
      #畫面開啟 (identifier)
      OPEN WINDOW w_aooi120 WITH FORM cl_ap_formpath("aoo",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL aooi120_init()   
 
      #進入選單 Menu (="N")
      CALL aooi120_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_aooi120
      
   END IF 
   
   CLOSE aooi120_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
   
END MAIN
 
 
]]>
