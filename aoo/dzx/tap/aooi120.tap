   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err("ooei_t",SQLCA.sqlcode,0)
      CLOSE aooi120_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
 
   CALL aooi120_show()
   
   WHILE TRUE
      LET g_ooei_m.ooei001 = g_ooei001_t
 
      
      #寫入修改者/修改日期資訊
      LET g_ooei_m.ooeimodid = g_user 
LET g_ooei_m.ooeimoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
 
      CALL aooi120_input("u")     #欄位更改
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_ooei_m.* = g_ooei_m_t.*
         CALL aooi120_show()
         CALL cl_err("",9001,0)
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE ooei_t SET (ooeimodid,ooeimoddt) = (g_ooei_m.ooeimodid,g_ooei_m.ooeimoddt)
       WHERE ooeient = g_enterprise AND ooei001 = g_ooei001_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_ooei_m.ooei001,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE aooi120_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_ooei_m.ooei001,"U")
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
]]>
</section>
