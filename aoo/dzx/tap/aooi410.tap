  <section id="aooi410.input.body" ver="8" status="" src="s">
    <![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_oogb_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂單身ACTION
         
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_oogb_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL aooi410_b_fill(g_wc2) 
            IF g_rec_b != 0 THEN
               CALL fgl_set_arr_curr(l_ac)
            END IF
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN aooi410_cl USING g_enterprise,
                                               g_oogb_m.oogb001
                                               ,g_oogb_m.oogb010
 
                                               
               IF STATUS THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "OPEN aooi410_cl:" 
                  LET g_errparam.code   =  STATUS 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
 
                  CLOSE aooi410_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_oogb_d[l_ac].oogb002 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_oogb_d_t.* = g_oogb_d[l_ac].*  #BACKUP
               LET g_oogb_d_o.* = g_oogb_d[l_ac].*  #BACKUP
               CALL aooi410_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL aooi410_set_no_entry_b(l_cmd)
               OPEN aooi410_bcl USING g_enterprise,g_oogb_m.oogb001,
                                                g_oogb_m.oogb010,
 
                                                g_oogb_d_t.oogb002
 
               IF STATUS THEN
                  INITIALIZE g_errparam TO NULL 
                  LET g_errparam.extend = "OPEN aooi410_bcl:" 
                  LET g_errparam.code   =  STATUS 
                  LET g_errparam.popup  = TRUE 
                  CALL cl_err()
 
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aooi410_bcl INTO g_oogb_d[l_ac].oogbstus,g_oogb_d[l_ac].oogb002,g_oogb_d[l_ac].oogb003, 
                      g_oogb_d[l_ac].oogb004,g_oogb_d[l_ac].oogb005,g_oogb_d[l_ac].oogb006,g_oogb_d[l_ac].oogb007, 
                      g_oogb_d[l_ac].oogb008,g_oogb_d[l_ac].oogb009,g_oogb1_info_d[l_ac].oogb002,g_oogb1_info_d[l_ac].oogbmodid, 
                      g_oogb1_info_d[l_ac].oogbmoddt,g_oogb1_info_d[l_ac].oogbownid,g_oogb1_info_d[l_ac].oogbowndp, 
                      g_oogb1_info_d[l_ac].oogbcrtid,g_oogb1_info_d[l_ac].oogbcrtdp,g_oogb1_info_d[l_ac].oogbcrtdt 

                  IF SQLCA.sqlcode THEN
                      INITIALIZE g_errparam TO NULL 
                      LET g_errparam.extend = g_oogb_d_t.oogb002 
                      LET g_errparam.code   = SQLCA.sqlcode 
                      LET g_errparam.popup  = TRUE 
                      CALL cl_err()
 
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL aooi410_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
        
         BEFORE INSERT
            
            INITIALIZE g_oogb_d_t.* TO NULL
            INITIALIZE g_oogb_d_o.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_oogb_d[l_ac].* TO NULL
            
            #公用欄位預設值
            #此段落由子樣板a14產生    
      #公用欄位新增給值
      LET g_oogb1_info_d[l_ac].oogbownid = g_user
      LET g_oogb1_info_d[l_ac].oogbowndp = g_dept
      LET g_oogb1_info_d[l_ac].oogbcrtid = g_user
      LET g_oogb1_info_d[l_ac].oogbcrtdp = g_dept 
      LET g_oogb1_info_d[l_ac].oogbcrtdt = cl_get_current()
      LET g_oogb1_info_d[l_ac].oogbmodid = ""
      LET g_oogb1_info_d[l_ac].oogbmoddt = ""
      LET g_oogb_d[l_ac].oogbstus = ""
 
  
            
            #一般欄位預設值
            
            
            
            #add-point:modify段before備份
            {<point name="input.body.before_insert.before_bak"/>}
            #end add-point
            LET g_oogb_d_t.* = g_oogb_d[l_ac].*     #新輸入資料
            LET g_oogb_d_o.* = g_oogb_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL aooi410_set_entry_b(l_cmd)
            #add-point:set_entry_b後
            {<point name="input.body.before_insert.set_entry_b"/>}
