  <section id="aooi410.construct" ver="6" status="" src="s">
    <![CDATA[#+ QBE資料查詢
PRIVATE FUNCTION aooi410_construct()
   DEFINE ls_return   STRING
   DEFINE ls_result   STRING 
   DEFINE ls_wc       STRING 
   #add-point:cs段define
   {<point name="cs.define"/>}
   #end add-point    
 
   #清除畫面上相關資料
   CLEAR FORM                 
   INITIALIZE g_oogb_m.* TO NULL
   CALL g_oogb_d.clear()
   CALL g_oogb1_info_d.clear()
 
   INITIALIZE g_wc TO NULL
   INITIALIZE g_wc2 TO NULL
   LET g_action_choice = ""
    
   LET g_qryparam.state = 'c'
   
   #add-point:cs段construct外
   {<point name="cs.head.before"/>}
   #end add-point 
   
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
      
      #單頭
      CONSTRUCT BY NAME g_wc ON oogb001,oogb010
 
         BEFORE CONSTRUCT
            #add-point:cs段more_construct
            {<point name="cs.head.before_construct"/>}
            #end add-point 
            
                 #Ctrlp:construct.c.oogb001
         ON ACTION controlp INFIELD oogb001
            #add-point:ON ACTION controlp INFIELD oogb001
            {<point name="construct.c.oogb001" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD oogb001
            #add-point:BEFORE FIELD oogb001
            {<point name="construct.b.oogb001" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD oogb001
            
            #add-point:AFTER FIELD oogb001
