  <point name="function.aooi410_oogb002_chk" order="5" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[# oogb002欄位控管
PRIVATE FUNCTION aooi410_oogb002_chk()
DEFINE l_n  LIKE type_t.num5
               IF NOT ap_chk_notDup(g_oogb_d[l_ac].oogb002,"SELECT COUNT(*) FROM oogb_t WHERE "||"oogbent = '" ||g_enterprise|| "' AND "||"oogb001 = '" ||g_oogb_m.oogb001|| "' AND "||"oogb002 = '"||g_oogb_d[l_ac].oogb002 ||"' AND oogb010="||g_oogb_m.oogb010,'std-00004',0) THEN 
                  RETURN FALSE
               END IF 
               LET l_n = 0 
               SELECT COUNT(*) INTO l_n FROM ooga_t
                WHERE ooga001 = g_oogb_d[l_ac].oogb002
               IF l_n = 0 THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = 'aoo-00085'
                  LET g_errparam.extend = g_oogb_d[l_ac].oogb002
                  LET g_errparam.popup = TRUE
                  CALL cl_err()

                  RETURN FALSE
               END IF 
               LET l_n = 0
               SELECT COUNT(*) INTO l_n FROM ooga_t
                WHERE ooga001 = g_oogb_d[l_ac].oogb002
                  AND oogastus= 'Y'
               IF l_n = 0 THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = 'aoo-00080'
                  LET g_errparam.extend = g_oogb_d[l_ac].oogb002
                  LET g_errparam.popup = TRUE
                  CALL cl_err()

                  RETURN FALSE
               END IF 
               
               IF YEAR(g_oogb_d[l_ac].oogb002) != g_oogb_m.oogb010 THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = 'aoo-00164'
                  LET g_errparam.extend = g_oogb_d[l_ac].oogb002
                  LET g_errparam.popup = TRUE
                  CALL cl_err()

                  RETURN FALSE
               END IF
               RETURN TRUE
END FUNCTION]]>
  </point>
