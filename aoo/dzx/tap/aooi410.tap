  <section id="aooi410.detail_reproduce" ver="4" status="" src="s">
    <![CDATA[#+ 單身自動複製
PRIVATE FUNCTION aooi410_detail_reproduce()
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE oogb_t.*
 
 
   #add-point:delete段define
   {<point name="detail_reproduce.define"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE aooi410_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE aooi410_detail AS ",
                "SELECT * FROM oogb_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO aooi410_detail SELECT * FROM oogb_t 
                                         WHERE oogbent = g_enterprise AND oogb001 = g_oogb001_t
                                         AND oogb010 = g_oogb010_t
 
   
   #將key修正為調整後   
   UPDATE aooi410_detail 
      #更新key欄位
      SET oogb001 = g_oogb_m.oogb001
          , oogb010 = g_oogb_m.oogb010
 
      #更新共用欄位
      #此段落由子樣板a13產生
       , oogbownid = g_user
       , oogbowndp = g_dept
       , oogbcrtid = g_user
       , oogbcrtdp = g_dept 
       , oogbcrtdt = ld_date
       , oogbmodid = "" 
       , oogbmoddt = "" 
      #, oogbstus = "Y"
 
 
                                       
  
   #將資料塞回原table   
   INSERT INTO oogb_t SELECT * FROM aooi410_detail
   
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "reproduce" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
 
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE aooi410_detail
   
   #add-point:單身複製後1
   {<point name="detail_reproduce.body.table1.a_insert"/>}
   #end add-point
 
 
   
 
   
   #多語言複製段落
   
   
   CALL s_transaction_end('Y','0')
   
   #已新增完, 調整資料內容(修改時使用)
   LET g_oogb001_t = g_oogb_m.oogb001
   LET g_oogb010_t = g_oogb_m.oogb010
 
   
   DROP TABLE aooi410_detail
   
END FUNCTION
