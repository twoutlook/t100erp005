  <point name="function.aooi410_oogb001_chk" order="2" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[# oogb001欄位控管
PRIVATE FUNCTION aooi410_oogb001_chk()
DEFINE l_n LIKE type_t.num5

        LET l_n = 0 
       SELECT COUNT(*) INTO l_n FROM ooal_t
        WHERE ooalent = g_enterprise
          AND ooal002 = g_oogb_m.oogb001
          AND ooal001 = 4
       IF l_n = 0 THEN
          IF cl_ask_confirm('aoo-00076') THEN
              IF NOT s_aooi070_ins(4,g_oogb_m.oogb001) THEN
                 RETURN FALSE
              END IF
           ELSE
               RETURN FALSE
          END IF
       END IF
       
       LET l_n = 0 
       SELECT COUNT(*) INTO l_n FROM ooal_t
        WHERE ooalent = g_enterprise
          AND ooal002 = g_oogb_m.oogb001
          AND ooal001 = 4
          AND ooalstus='Y'
       IF l_n = 0 THEN
          INITIALIZE g_errparam TO NULL
          LET g_errparam.code = 'aoo-00124'
          LET g_errparam.extend = g_oogb_m.oogb001
          LET g_errparam.popup = TRUE
          CALL cl_err()

          RETURN FALSE
       END IF
       RETURN TRUE
END FUNCTION]]>
  </point>
