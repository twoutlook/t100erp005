  <section id="aooi410.b_fill" ver="7" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aooi410_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_oogb_d.clear()
   CALL g_oogb1_info_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE oogbstus,oogb002,oogb003,oogb004,oogb005,oogb006,oogb007,oogb008,oogb009, 
       oogb002,oogbmodid,oogbmoddt,oogbownid,oogbowndp,oogbcrtid,oogbcrtdp,oogbcrtdt,t1.oocql004 ,t2.oocql004 , 
       t3.oocql004 ,t4.oocql004 ,t5.oocql004 ,t6.oocql004 ,t7.ooag011 ,t8.ooag011 ,t9.ooefl003 ,t10.ooag011 , 
       t11.ooefl003 FROM oogb_t",   
               "",
               
                              " LEFT JOIN oocql_t t1 ON t1.oocqlent='"||g_enterprise||"' AND t1.oocql001='6' AND t1.oocql002=oogb003 AND t1.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t2 ON t2.oocqlent='"||g_enterprise||"' AND t2.oocql001='7' AND t2.oocql002=oogb005 AND t2.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t3 ON t3.oocqlent='"||g_enterprise||"' AND t3.oocql001='8' AND t3.oocql002=oogb006 AND t3.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t4 ON t4.oocqlent='"||g_enterprise||"' AND t4.oocql001='9' AND t4.oocql002=oogb007 AND t4.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t5 ON t5.oocqlent='"||g_enterprise||"' AND t5.oocql001='10' AND t5.oocql002=oogb008 AND t5.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t6 ON t6.oocqlent='"||g_enterprise||"' AND t6.oocql001='11' AND t6.oocql002=oogb008 AND t6.oocql003='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent='"||g_enterprise||"' AND t7.ooag001=oogbmodid  ",
               " LEFT JOIN ooag_t t8 ON t8.ooagent='"||g_enterprise||"' AND t8.ooag001=oogbownid  ",
               " LEFT JOIN ooefl_t t9 ON t9.ooeflent='"||g_enterprise||"' AND t9.ooefl001=oogbowndp AND t9.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t10 ON t10.ooagent='"||g_enterprise||"' AND t10.ooag001=oogbcrtid  ",
               " LEFT JOIN ooefl_t t11 ON t11.ooeflent='"||g_enterprise||"' AND t11.ooefl001=oogbcrtdp AND t11.ooefl002='"||g_dlang||"' ",
 
               " WHERE oogbent= ? AND oogb001=? AND oogb010=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("oogb_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF aooi410_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY oogb_t.oogb002"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE aooi410_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR aooi410_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_oogb_m.oogb001,g_oogb_m.oogb010
                                               
      FOREACH b_fill_cs INTO g_oogb_d[l_ac].oogbstus,g_oogb_d[l_ac].oogb002,g_oogb_d[l_ac].oogb003,g_oogb_d[l_ac].oogb004, 
          g_oogb_d[l_ac].oogb005,g_oogb_d[l_ac].oogb006,g_oogb_d[l_ac].oogb007,g_oogb_d[l_ac].oogb008, 
          g_oogb_d[l_ac].oogb009,g_oogb1_info_d[l_ac].oogb002,g_oogb1_info_d[l_ac].oogbmodid,g_oogb1_info_d[l_ac].oogbmoddt, 
          g_oogb1_info_d[l_ac].oogbownid,g_oogb1_info_d[l_ac].oogbowndp,g_oogb1_info_d[l_ac].oogbcrtid, 
          g_oogb1_info_d[l_ac].oogbcrtdp,g_oogb1_info_d[l_ac].oogbcrtdt,g_oogb_d[l_ac].oogb003_desc, 
          g_oogb_d[l_ac].oogb005_desc,g_oogb_d[l_ac].oogb006_desc,g_oogb_d[l_ac].oogb007_desc,g_oogb_d[l_ac].oogb008_desc, 
          g_oogb_d[l_ac].oogb009_desc,g_oogb1_info_d[l_ac].oogbmodid_desc,g_oogb1_info_d[l_ac].oogbownid_desc, 
          g_oogb1_info_d[l_ac].oogbowndp_desc,g_oogb1_info_d[l_ac].oogbcrtid_desc,g_oogb1_info_d[l_ac].oogbcrtdp_desc 

                             
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_oogb_d.deleteElement(g_oogb_d.getLength())
      CALL g_oogb1_info_d.deleteElement(g_oogb1_info_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE aooi410_pb   
   
END FUNCTION
]]>
