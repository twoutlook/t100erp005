               UPDATE oogb_t SET (oogb001,oogb010) = (g_oogb_m.oogb001,g_oogb_m.oogb010)
                WHERE oogbent = g_enterprise AND oogb001 = g_oogb001_t
                  AND oogb010 = g_oogb010_t
 
               #add-point:單頭修改中
               {<point name="input.head.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "oogb_t" 
                     LET g_errparam.code   = "std-00009" 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
 
                     CALL s_transaction_end('N','0')
                  WHEN SQLCA.sqlcode #其他錯誤
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "oogb_t" 
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
 
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                     #資料多語言用-增/改
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_oogb_m.oogb001
               LET gs_keys_bak[1] = g_oogb001_t
               LET gs_keys[2] = g_oogb_m.oogb010
               LET gs_keys_bak[2] = g_oogb010_t
               LET gs_keys[3] = g_oogb_d[g_detail_idx].oogb002
               LET gs_keys_bak[3] = g_oogb_d_t.oogb002
               CALL aooi410_update_b('oogb_t',gs_keys,gs_keys_bak,"'1'")
                     
                     LET g_oogb001_t = g_oogb_m.oogb001
                     LET g_oogb010_t = g_oogb_m.oogb010
 
                     #add-point:單頭修改後
                     {<point name="input.head.a_update"/>}
                     #end add-point
                     
                     LET g_log1 = util.JSON.stringify(g_oogb_m_t)
                     LET g_log2 = util.JSON.stringify(g_oogb_m)
                     IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                        CALL s_transaction_end('N','0')
                     ELSE
                        CALL s_transaction_end('Y','0')
                     END IF
               END CASE
            
            ELSE    
               #add-point:單頭新增
               {<point name="input.head.a_insert"/>}
               #end add-point
                                 
               IF l_cmd_t = 'r' AND p_cmd = 'a' THEN
                  CALL aooi410_detail_reproduce()
               END IF
            END IF
           #controlp
                     
           LET g_oogb001_t = g_oogb_m.oogb001
           LET g_oogb010_t = g_oogb_m.oogb010
 
           
           #若單身還沒有輸入資料, 強制切換至單身
           #IF cl_null(g_oogb_d[1].oogb002) THEN
           #   CALL g_oogb_d.deleteElement(1)
           #   NEXT FIELD oogb002
           #END IF
           
           IF g_oogb_d.getLength() = 0 THEN
              NEXT FIELD oogb002
           END IF
 
      END INPUT
]]>
