  <point name="function.aooi160_ooal002_desc" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#+ 汇率/币别参照表说明
PRIVATE FUNCTION aooi160_ooal002_desc(p_ooal001,p_ooal002)
   DEFINE p_ooal001     LIKE ooal_t.ooal001
   DEFINE p_ooal002     LIKE ooal_t.ooal002
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = p_ooal001
   LET g_ref_fields[2] = p_ooal002
   CALL ap_ref_array2(g_ref_fields,"SELECT ooall004 FROM ooall_t WHERE ooallent='"||g_enterprise||"' AND ooall001=? AND ooall002=? AND ooall003='"||g_dlang||"'","") RETURNING g_rtn_fields
   RETURN g_rtn_fields[1]
END FUNCTION]]>
  </point>
  <point name="function.aooi160_ooan002_chk" order="2" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#+ 币别检查
PRIVATE FUNCTION aooi160_ooan002_chk(p_ooan002)
   DEFINE p_ooan002    LIKE ooan_t.ooan002
   DEFINE l_ooaistus   LIKE ooaj_t.ooajstus
   
   LET g_errno = ''
   SELECT ooaistus INTO l_ooaistus FROM ooai_t
    WHERE ooaient = g_enterprise
      AND ooai001 = p_ooan002

   IF SQLCA.SQLCODE = 100  THEN
      LET g_errno = 'aoo-00175'
   END IF
END FUNCTION]]>
  </point>
  <point name="function.aooi160_ooal002_chk" order="3" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#+ 汇率/币别参照表号检查
PRIVATE FUNCTION aooi160_ooal002_chk(p_ooal001,p_ooal002)
   DEFINE l_ooalstus    like ooal_t.ooalstus
   DEFINE p_ooal001     LIKE ooal_t.ooal001
   DEFINE p_ooal002     LIKE ooal_t.ooal002
   
   LET g_errno = ''
   SELECT ooalstus  INTO l_ooalstus
     FROM ooal_t 
    WHERE ooalent = g_enterprise
      AND ooal002 = p_ooal002
      AND ooal001 = p_ooal001
    CASE
       WHEN sqlca.sqlcode = 100   LET g_errno = 'aoo-00076'
       WHEN l_ooalstus ='N'  
        IF p_ooal001 = '1' THEN       
           LET g_errno = 'aoo-00128'
        ELSE
           LET g_errno = 'aoo-00169'
        END IF   
     END CASE
END FUNCTION]]>
  </point>
  <point name="function.aooi160_ooan002_desc" order="4" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#+币别说明
PRIVATE FUNCTION aooi160_ooan002_desc(p_ooan002)
   DEFINE p_ooan002    LIKE ooan_t.ooan002
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = p_ooan002
   CALL ap_ref_array2(g_ref_fields,"SELECT ooail003 FROM ooail_t WHERE ooailent='"||g_enterprise||"' AND ooail001=? AND ooail002='"||g_dlang||"'","") RETURNING g_rtn_fields
   RETURN g_rtn_fields[1]
   
END FUNCTION]]>
  </point>
  <point name="function.aooi160_ooam_key_chk" order="5" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION aooi160_ooam_key_chk(p_cmd)
DEFINE p_cmd   LIKE type_t.chr1   
   IF  NOT cl_null(g_ooam_m.ooam001) AND NOT cl_null(g_ooam_m.ooam003) AND NOT cl_null(g_ooam_m.ooam004) THEN 
       IF p_cmd = 'a' OR ( p_cmd = 'u' AND ( p_cmd = 'u' AND (g_ooam_m.ooam001 != g_ooam001_t  OR g_ooam_m.ooam003 != g_ooam003_t  OR g_ooam_m.ooam004 != g_ooam004_t ))) THEN 
          IF NOT ap_chk_notDup(g_ooam_m.ooam001||" + "||g_ooam_m.ooam003||" + "||g_ooam_m.ooam004,"SELECT COUNT(*) FROM ooam_t WHERE "||"ooament = '" ||g_enterprise|| "' AND "||"ooam001 = '"||g_ooam_m.ooam001 ||"' AND "|| "ooam003 = '"||g_ooam_m.ooam003 ||"' AND "|| "ooam004 = '"||g_ooam_m.ooam004 ||"'",'std-00004',1) THEN 
             RETURN FALSE
          END IF
       END IF
    END IF
    RETURN TRUE
END FUNCTION]]>
  </point>
  <point name="function.aooi160_idx_chk2" order="6" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#单身笔数变更
PRIVATE FUNCTION aooi160_idx_chk2()
    IF g_current_page = 2 THEN
      LET g_detail_idx = g_curr_diag.getCurrentRow("s_detail2")
      IF g_detail_idx > g_ooan2_d.getLength() THEN
         LET g_detail_idx = g_ooan2_d.getLength()
      END IF
      IF g_detail_idx = 0 AND g_ooan2_d.getLength() <> 0 THEN
         LET g_detail_idx = 1
      END IF
      DISPLAY g_detail_idx TO FORMONLY.idx
      DISPLAY g_ooan2_d.getLength() TO FORMONLY.cnt
   END IF
END FUNCTION]]>
  </point>
  <point name="function.aooi160_ooam007_default" order="7" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION aooi160_ooam007_default()
   SELECT ooam007 INTO g_ooam_m.ooam007 FROM ooam_t
    WHERE ooament = g_enterprise AND ooam001 = g_ooam_m.ooam001
      AND ooam003 = g_ooam_m.ooam003 AND ROWNUM = 1
    ORDER BY ooam004 DESC
   IF cl_null(g_ooam_m.ooam007) THEN
      LET g_ooam_m.ooam007 = 'N'
   END IF
END FUNCTION]]>
