  <point name="input.body.a_insert2" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[                              #更新时间
               LET g_ooan_d[l_ac].ooan010 = cl_get_current()
               UPDATE ooan_t SET ooan010 = g_ooan_d[l_ac].ooan010
                           WHERE ooanent = g_enterprise
                             AND ooan001 = g_ooam_m.ooam001
                             AND ooan003 = g_ooam_m.ooam003
                             AND ooan004 = g_ooam_m.ooam004
                             AND ooan002 = g_ooan_d[l_ac].ooan002
               DISPLAY BY NAME g_ooan_d[l_ac].ooan010              
                                      
               #1.單身新增修改時，產生日期=9999/12/31的最新匯率值
               CALL s_aooi160_gen_ref_exrate(g_ooam_m.ooam001,g_ooan_d[l_ac].ooan002,g_ooam_m.ooam003) RETURNING l_success
               IF l_success = FALSE THEN
                  CALL s_transaction_end('N','0')
                  CANCEL INSERT                  
               END IF
               #2.單身新增修改時，若單頭的自動預設交叉匯率有勾選(ooam007='Y')，自動新增一筆反向資料
               #2.1呼叫s_aooi160_cross_rate
               #2.2呼叫s_aooi160_gen_ref_exrate，產生日期=9999/12/31的最新匯率值   
               IF g_ooam_m.ooam007 = 'Y' THEN
                  CALL s_aooi160_cross_rate(g_ooam_m.ooam001,g_ooan_d[l_ac].ooan002,g_ooam_m.ooam003,g_ooam_m.ooam004) RETURNING l_success
                  IF l_success = FALSE THEN
                     CALL s_transaction_end('N','0')
                     CANCEL INSERT                  
                  END IF 
                 
               END IF 
               
               CALL aooi160_b_fill()]]>
  </point>
