  <section id="aooi160.modify" ver="7" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aooi160_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_ooam_m.ooam001 IS NULL
   OR g_ooam_m.ooam003 IS NULL
   OR g_ooam_m.ooam004 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE aooi160_master_referesh USING g_ooam_m.ooam001,g_ooam_m.ooam003,g_ooam_m.ooam004 INTO g_ooam_m.ooam001, 
       g_ooam_m.ooam003,g_ooam_m.ooam005,g_ooam_m.ooam004,g_ooam_m.ooam007,g_ooam_m.ooamstus,g_ooam_m.ooamownid, 
       g_ooam_m.ooamowndp,g_ooam_m.ooamcrtid,g_ooam_m.ooamcrtdp,g_ooam_m.ooamcrtdt,g_ooam_m.ooammodid, 
       g_ooam_m.ooammoddt,g_ooam_m.ooam001_desc,g_ooam_m.ooam003_desc,g_ooam_m.ooamownid_desc,g_ooam_m.ooamowndp_desc, 
       g_ooam_m.ooamcrtid_desc,g_ooam_m.ooamcrtdp_desc,g_ooam_m.ooammodid_desc
 
   ERROR ""
  
   LET g_ooam001_t = g_ooam_m.ooam001
   LET g_ooam003_t = g_ooam_m.ooam003
   LET g_ooam004_t = g_ooam_m.ooam004
 
   CALL s_transaction_begin()
   
   OPEN aooi160_cl USING g_enterprise,g_ooam_m.ooam001,g_ooam_m.ooam003,g_ooam_m.ooam004
   IF STATUS THEN
      CALL cl_err("OPEN aooi160_cl:", STATUS, 1)
      CLOSE aooi160_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aooi160_master_referesh USING g_ooam_m.ooam001,g_ooam_m.ooam003,g_ooam_m.ooam004 INTO g_ooam_m.ooam001, 
       g_ooam_m.ooam003,g_ooam_m.ooam005,g_ooam_m.ooam004,g_ooam_m.ooam007,g_ooam_m.ooamstus,g_ooam_m.ooamownid, 
       g_ooam_m.ooamowndp,g_ooam_m.ooamcrtid,g_ooam_m.ooamcrtdp,g_ooam_m.ooamcrtdt,g_ooam_m.ooammodid, 
       g_ooam_m.ooammoddt,g_ooam_m.ooam001_desc,g_ooam_m.ooam003_desc,g_ooam_m.ooamownid_desc,g_ooam_m.ooamowndp_desc, 
       g_ooam_m.ooamcrtid_desc,g_ooam_m.ooamcrtdp_desc,g_ooam_m.ooammodid_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_ooam_m.ooam001,SQLCA.sqlcode,0)
      CLOSE aooi160_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   CALL aooi160_show()
   WHILE TRUE
      LET g_ooam001_t = g_ooam_m.ooam001
      LET g_ooam003_t = g_ooam_m.ooam003
      LET g_ooam004_t = g_ooam_m.ooam004
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_ooam_m.ooammodid = g_user 
LET g_ooam_m.ooammoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      CALL aooi160_input("u")
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_ooam_m.* = g_ooam_m_t.*
         CALL aooi160_show()
         CALL cl_err('',9001,0)
         CALL s_transaction_end('N','0')
         EXIT WHILE
      END IF 
      
      #若有modid跟moddt則進行update
      UPDATE ooam_t SET (ooammodid,ooammoddt) = (g_ooam_m.ooammodid,g_ooam_m.ooammoddt)
       WHERE ooament = g_enterprise AND ooam001 = g_ooam001_t
         AND ooam003 = g_ooam003_t
         AND ooam004 = g_ooam004_t
 
                  
      #若單頭key欄位有變更
      IF g_ooam_m.ooam001 != g_ooam001_t 
      OR g_ooam_m.ooam003 != g_ooam003_t 
      OR g_ooam_m.ooam004 != g_ooam004_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE ooan_t SET ooan001 = g_ooam_m.ooam001
                                       ,ooan003 = g_ooam_m.ooam003
                                       ,ooan004 = g_ooam_m.ooam004
 
          WHERE ooanent = g_enterprise AND ooan001 = g_ooam001_t
            AND ooan003 = g_ooam003_t
            AND ooan004 = g_ooam004_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("ooan_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("ooan_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_ooam_m.ooam001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE aooi160_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_ooam_m.ooam001,'U')
 
END FUNCTION   
