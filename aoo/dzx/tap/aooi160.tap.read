  <section id="aooi160.b_fill" ver="5" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aooi160_b_fill()
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_ooan_d.clear()    #g_ooan_d 單頭及單身 
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF aooi160_fill_chk(1) THEN
   
      LET g_sql = "SELECT  UNIQUE ooan002,ooan012,ooan013,ooan005,ooan006,ooan007,ooan008,ooan009,ooan010, 
          ooan011 ,t1.ooail003 FROM ooan_t",   
                  " INNER JOIN ooam_t ON ooam001 = ooan001 ",
                  " AND ooam003 = ooan003 ",
                  " AND ooam004 = ooan004 ",
 
                  #"",
                  
                  "",
                                 " LEFT JOIN ooail_t t1 ON t1.ooailent='"||g_enterprise||"' AND t1.ooail001=ooan002 AND t1.ooail002='"||g_dlang||"' ",
 
                  " WHERE ooanent=? AND ooan001=? AND ooan003=? AND ooan004=?"
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      #add-point:b_fill段sql_before
      {<point name="b_fill.body.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table1) THEN
         LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
      END IF
      
      #子單身的WC
      
      
      LET g_sql = g_sql, " ORDER BY ooan_t.ooan002"
      
