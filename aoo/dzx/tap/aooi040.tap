  <point name="function.aooi040_oock002_ck" cite_std="N" status="" order="4" ver="1" new="N" src="s" mark_hard="N">
<![CDATA[# 檢查州省的存在及有效性
PRIVATE FUNCTION aooi040_oock002_ck(p_oock001,p_oock002)
DEFINE l_ooci002     LIKE ooci_t.ooci002
   DEFINE l_oocistus    LIKE ooci_t.oocistus
   DEFINE p_oock001     LIKE oock_t.oock001
   DEFINE p_oock002     LIKE oock_t.oock002
   
   LET g_errno = ''
   SELECT ooci002,oocistus into l_ooci002,l_oocistus 
     FROM ooci_t
    WHERE oocient=g_enterprise 
      AND ooci001=p_oock001
      AND ooci002 = p_oock002
      
  CASE 
     WHEN sqlca.sqlcode=100 LET g_errno='aoo-00014'
     WHEN l_oocistus='N'    LET g_errno='aoo-00143'
  END CASE
END FUNCTION]]>
