  <point name="function.aooi040_oock001_ck" cite_std="N" status="" order="1" ver="2" new="N" src="s" mark_hard="N">
<![CDATA[# 檢查國家值的存在及有效性
PRIVATE FUNCTION aooi040_oock001_ck(p_oock001)
DEFINE l_oocg001     LIKE oocg_t.oocg001
   DEFINE l_oocgstus    LIKE oocg_t.oocgstus
   DEFINE p_oock001     LIKE oock_t.oock001
    
   LET g_errno = ''
   SELECT oocg001,oocgstus into l_oocg001,l_oocgstus 
     FROM oocg_t
   WHERE oocgent=g_enterprise AND oocg001=p_oock001
      
  CASE 
     WHEN sqlca.sqlcode=100 LET g_errno='aoo-00013'
     WHEN l_oocgstus='N'    LET g_errno='aoo-00035'
  END CASE
END FUNCTION]]>
</point>
  <point name="function.aooi040_oock001_desc" cite_std="N" status="" order="2" ver="1" new="N" src="s" mark_hard="N">
<![CDATA[#+ 国家地区说明
PRIVATE FUNCTION aooi040_oock001_desc(p_oock001)
   DEFINE p_oock001     LIKE oock_t.oock001
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = p_oock001
   CALL ap_ref_array2(g_ref_fields,"SELECT oocgl003 FROM oocgl_t WHERE oocglent='"||g_enterprise||"' AND oocgl001=? AND oocgl002='"||g_dlang||"'","") RETURNING g_rtn_fields
   RETURN g_rtn_fields[1]
          
END FUNCTION]]>
</point>
