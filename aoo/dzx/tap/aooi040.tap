  <section id="aooi040.delete" ver="1" status="" src="s">
<![CDATA[#+ 資料刪除
PRIVATE FUNCTION aooi040_delete()
   {<Local define>}
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   {</Local define>}
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_oock_m.oock001 IS NULL
   OR g_oock_m.oock002 IS NULL
 
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
    SELECT UNIQUE oock001,oock002
 INTO g_oock_m.oock001,g_oock_m.oock002
 FROM oock_t
 WHERE oockent = g_enterprise AND oock001 = g_oock_m.oock001 AND oock002 = g_oock_m.oock002
   
   CALL aooi040_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN aooi040_cl USING g_enterprise,g_oock_m.oock001
                                                       ,g_oock_m.oock002
 
 
   IF STATUS THEN
      CALL cl_err("OPEN aooi040_cl:", STATUS, 1)
      CLOSE aooi040_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aooi040_cl INTO g_oock_m.oock001,g_oock_m.oock001_desc,g_oock_m.oock002,g_oock_m.oock002_desc 

   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_oock_m.oock001,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
