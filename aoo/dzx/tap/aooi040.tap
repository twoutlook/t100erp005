  <section id="aooi040.input" ver="1" status="" src="s">
<![CDATA[#+ 資料輸入
PRIVATE FUNCTION aooi040_input(p_cmd)
   {<Local define>}
   DEFINE  p_cmd                 LIKE type_t.chr1
   DEFINE  l_cmd_t               LIKE type_t.chr1
   DEFINE  l_cmd                 LIKE type_t.chr1
   DEFINE  l_ac_t                LIKE type_t.num5                #未取消的ARRAY CNT 
   DEFINE  l_n                   LIKE type_t.num5                #檢查重複用  
   DEFINE  l_cnt                 LIKE type_t.num5                #檢查重複用  
   DEFINE  l_lock_sw             LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert        LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete        LIKE type_t.num5                #可刪除否  
   DEFINE  l_count               LIKE type_t.num5
   DEFINE  l_i                   LIKE type_t.num5
   DEFINE  l_insert              BOOLEAN
   DEFINE  ls_return             STRING
   DEFINE  l_var_keys            DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys          DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                DYNAMIC ARRAY OF STRING
   DEFINE  l_fields              DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak        DYNAMIC ARRAY OF STRING
   DEFINE  lb_reproduce          BOOLEAN
   DEFINE  li_reproduce          LIKE type_t.num5
   DEFINE  li_reproduce_target   LIKE type_t.num5
   {</Local define>}
   #add-point:input段define
   {<point name="input.define"/>}
   #end add-point    
   
   #先做狀態判定
   IF p_cmd = 'r' THEN
      LET l_cmd_t = 'r'
      LET p_cmd   = 'a'
   ELSE
      LET l_cmd_t = p_cmd
   END IF   
   
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF  
 
   CALL cl_set_head_visible("","YES")  
 
   LET l_insert = FALSE
   LET g_action_choice = ""
 
   #add-point:input段define_sql
   {<point name="input.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT oockstus,oock003,'','','',oockmodid,'',oockmoddt,oockownid,'',oockowndp, 
       '',oockcrtid,'',oockcrtdp,'',oockcrtdt FROM oock_t WHERE oockent=? AND oock001=? AND oock002=?  
       AND oock003=? FOR UPDATE"
   #add-point:input段define_sql
   {<point name="input.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   DECLARE aooi040_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR
 
 
   
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   LET g_qryparam.state = 'i'
   
   #控制key欄位可否輸入
   CALL aooi040_set_entry(p_cmd)
   #add-point:set_entry後
   {<point name="input.after_set_entry"/>}
   #end add-point
   CALL aooi040_set_no_entry(p_cmd)
   #add-point:set_no_entry後
   {<point name="input.after_set_no_entry"/>}
   #end add-point
 
   DISPLAY BY NAME g_oock_m.oock001,g_oock_m.oock002
   
   LET lb_reproduce = FALSE
   
   #add-point:進入修改段前
   {<point name="input.before_input"/>}
   #end add-point
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
]]>
</section>
  <section id="aooi040.input.body" ver="1" status="" src="s">
<![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_oock_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂單身ACTION
         
 
 
         ON ACTION update_item
            #add-point:ON ACTION update_item
            {<point name="input.detail_input.page1.update_item" />}
            #END add-point
 
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_oock_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL aooi040_b_fill(g_wc2) 
            IF g_rec_b != 0 THEN
               CALL fgl_set_arr_curr(l_ac)
            END IF
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN aooi040_cl USING g_enterprise,
                                               g_oock_m.oock001
                                               ,g_oock_m.oock002
 
 
                                               
               IF STATUS THEN
                  CALL cl_err("OPEN aooi040_cl:", STATUS, 1)
                  CLOSE aooi040_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_oock_d[l_ac].oock003 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_oock_d_t.* = g_oock_d[l_ac].*  #BACKUP
               CALL aooi040_set_entry_b(l_cmd)
               CALL aooi040_set_no_entry_b(l_cmd)
               OPEN aooi040_bcl USING g_enterprise,g_oock_m.oock001,
                                                g_oock_m.oock002,
 
 
                                                g_oock_d_t.oock003
 
               IF STATUS THEN
                  CALL cl_err("OPEN aooi040_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aooi040_bcl INTO g_oock_d[l_ac].oockstus,g_oock_d[l_ac].oock003,g_oock_d[l_ac].oockl005, 
                      g_oock_d[l_ac].oockl006,g_oock1_info_d[l_ac].oock003,g_oock1_info_d[l_ac].oockmodid, 
                      g_oock1_info_d[l_ac].oockmodid_desc,g_oock1_info_d[l_ac].oockmoddt,g_oock1_info_d[l_ac].oockownid, 
                      g_oock1_info_d[l_ac].oockownid_desc,g_oock1_info_d[l_ac].oockowndp,g_oock1_info_d[l_ac].oockowndp_desc, 
                      g_oock1_info_d[l_ac].oockcrtid,g_oock1_info_d[l_ac].oockcrtid_desc,g_oock1_info_d[l_ac].oockcrtdp, 
                      g_oock1_info_d[l_ac].oockcrtdp_desc,g_oock1_info_d[l_ac].oockcrtdt
                  IF SQLCA.sqlcode THEN
                      CALL cl_err(g_oock_d_t.oock003,SQLCA.sqlcode,1)
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL aooi040_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            LET g_detail_multi_table_t.oockl001 = g_oock_m.oock001
LET g_detail_multi_table_t.oockl002 = g_oock_m.oock002
LET g_detail_multi_table_t.oockl003 = g_oock_d[l_ac].oock003
LET g_detail_multi_table_t.oockl004 = g_dlang
LET g_detail_multi_table_t.oockl005 = g_oock_d[l_ac].oockl005
LET g_detail_multi_table_t.oockl006 = g_oock_d[l_ac].oockl006
 
        
         BEFORE INSERT
            
            INITIALIZE g_oock_d_t.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_oock_d[l_ac].* TO NULL
            
            #公用欄位預設值
            #此段落由子樣板a14產生    
      LET g_oock1_info_d[l_ac].oockownid = g_user
      LET g_oock1_info_d[l_ac].oockowndp = g_dept
      LET g_oock1_info_d[l_ac].oockcrtid = g_user
      LET g_oock1_info_d[l_ac].oockcrtdp = g_dept 
      LET g_oock1_info_d[l_ac].oockcrtdt = cl_get_current()
      LET g_oock1_info_d[l_ac].oockmodid = ""
      LET g_oock1_info_d[l_ac].oockmoddt = ""
      #LET g_oock_d[l_ac].oockstus = ""
 
  
            
            #一般欄位預設值
            
            
            
            LET g_oock_d_t.* = g_oock_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL aooi040_set_entry_b(l_cmd)
            CALL aooi040_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_oock_d[li_reproduce_target].* = g_oock_d[li_reproduce].*
               LET g_oock1_info_d[li_reproduce_target].* = g_oock1_info_d[li_reproduce].*
 
               LET g_oock_d[g_oock_d.getLength()].oock003 = NULL
 
            END IF
            LET g_detail_multi_table_t.oockl001 = g_oock_m.oock001
LET g_detail_multi_table_t.oockl002 = g_oock_m.oock002
LET g_detail_multi_table_t.oockl003 = g_oock_d[l_ac].oock003
LET g_detail_multi_table_t.oockl004 = g_dlang
LET g_detail_multi_table_t.oockl005 = g_oock_d[l_ac].oockl005
LET g_detail_multi_table_t.oockl006 = g_oock_d[l_ac].oockl006
 
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
 
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM oock_t 
             WHERE oockent = g_enterprise AND oock001 = g_oock_m.oock001
               AND oock002 = g_oock_m.oock002
 
 
               AND oock003 = g_oock_d[l_ac].oock003
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               CALL s_transaction_begin()
               #add-point:單身新增前
               {<point name="input.body.b_insert" mark="Y"/>}
               #end add-point
               INSERT INTO oock_t
                           (oockent,
                            oock001,oock002,
                            oock003
                            ,oockstus,oockmodid,oockmoddt,oockownid,oockowndp,oockcrtid,oockcrtdp,oockcrtdt) 
                     VALUES(g_enterprise,
                            g_oock_m.oock001,g_oock_m.oock002,
                            g_oock_d[l_ac].oock003
                            ,g_oock_d[l_ac].oockstus,g_oock1_info_d[l_ac].oockmodid,g_oock1_info_d[l_ac].oockmoddt, 
                                g_oock1_info_d[l_ac].oockownid,g_oock1_info_d[l_ac].oockowndp,g_oock1_info_d[l_ac].oockcrtid, 
                                g_oock1_info_d[l_ac].oockcrtdp,g_oock1_info_d[l_ac].oockcrtdt)
               #add-point:單身新增中
               {<point name="input.body.m_insert"/>}
               #end add-point
               LET p_cmd = 'u'
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_oock_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("oock_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #資料多語言用-增/改
                        INITIALIZE l_var_keys TO NULL 
         INITIALIZE l_field_keys TO NULL 
         INITIALIZE l_vars TO NULL 
         INITIALIZE l_fields TO NULL 
         INITIALIZE l_var_keys_bak TO NULL 
         IF g_oock_m.oock001 = g_detail_multi_table_t.oockl001 AND
         g_oock_m.oock002 = g_detail_multi_table_t.oockl002 AND
         g_oock_d[l_ac].oock003 = g_detail_multi_table_t.oockl003 AND
         g_oock_d[l_ac].oockl005 = g_detail_multi_table_t.oockl005 AND
         g_oock_d[l_ac].oockl006 = g_detail_multi_table_t.oockl006 THEN
         ELSE 
            LET l_var_keys[01] = g_oock_m.oock001
            LET l_field_keys[01] = 'oockl001'
            LET l_var_keys[02] = g_oock_m.oock002
            LET l_field_keys[02] = 'oockl002'
            LET l_var_keys[03] = g_oock_d[l_ac].oock003
            LET l_field_keys[03] = 'oockl003'
            LET l_var_keys[04] = g_dlang
            LET l_field_keys[04] = 'oockl004'
            LET l_vars[01] = g_oock_d[l_ac].oockl005
            LET l_fields[01] = 'oockl005'
            LET l_vars[02] = g_oock_d[l_ac].oockl006
            LET l_fields[02] = 'oockl006'
            LET l_vars[03] = g_enterprise 
            LET l_fields[03] = 'oocklent'
            LET l_var_keys_bak[01] = g_detail_multi_table_t.oockl001
            LET l_var_keys_bak[02] = g_detail_multi_table_t.oockl002
            LET l_var_keys_bak[03] = g_detail_multi_table_t.oockl003
            LET l_var_keys_bak[04] = g_detail_multi_table_t.oockl004
            CALL cl_multitable(l_var_keys,l_field_keys,l_vars,l_fields,l_var_keys_bak,'oockl_t')
         END IF 
 
               #add-point:input段-after_insert
               {<point name="input.body.a_insert"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR "INSERT O.K"
               LET g_rec_b=g_rec_b+1
               DISPLAY g_rec_b TO FORMONLY.cnt
            END IF
            
            #add-point:單身新增後
            {<point name="input.body.after_insert"/>}
            #end add-point
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_oock_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_oock_d.deleteElement(l_ac)
               NEXT FIELD oock003
            END IF
            IF g_oock_d_t.oock003 IS NOT NULL
 
               THEN
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete"/>}
               #end add-point
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               IF aooi040_before_delete() THEN 
                  CALL s_transaction_end('Y','0')
               ELSE 
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               END IF 
               CLOSE aooi040_bcl
               LET l_count = g_oock_d.getLength()
            END IF 
            
            #add-point:單身刪除後
            {<point name="input.body.a_delete"/>}
            #end add-point
              
         AFTER DELETE 
            #add-point:單身AFTER DELETE 
            {<point name="input.body.after_delete"/>}
            #end add-point
 
         #---------------------<  Detail: page1  >---------------------
         #----<<oock003>>----
         #此段落由子樣板a01產生
         BEFORE FIELD oock003
            #add-point:BEFORE FIELD oock003
            {<point name="input.b.page1.oock003" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD oock003
            
            #add-point:AFTER FIELD oock003
            {<point name="input.a.page1.oock003" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE oock003
            #add-point:ON CHANGE oock003
            {<point name="input.g.page1.oock003" />}
            #END add-point
 
         #----<<oockl005>>----
         #此段落由子樣板a01產生
         BEFORE FIELD oockl005
            #add-point:BEFORE FIELD oockl005
            {<point name="input.b.page1.oockl005" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD oockl005
            
            #add-point:AFTER FIELD oockl005
            {<point name="input.a.page1.oockl005" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE oockl005
            #add-point:ON CHANGE oockl005
            {<point name="input.g.page1.oockl005" />}
            #END add-point
 
         #----<<oockl006>>----
         #此段落由子樣板a01產生
         BEFORE FIELD oockl006
            #add-point:BEFORE FIELD oockl006
            {<point name="input.b.page1.oockl006" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD oockl006
            
            #add-point:AFTER FIELD oockl006
            {<point name="input.a.page1.oockl006" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE oockl006
            #add-point:ON CHANGE oockl006
            {<point name="input.g.page1.oockl006" />}
            #END add-point
 
 
         #---------------------<  Detail: page1  >---------------------
         #----<<oock003>>----
         #Ctrlp:input.c.page1.oock003
         ON ACTION controlp INFIELD oock003
            #add-point:ON ACTION controlp INFIELD oock003
            {<point name="input.c.page1.oock003" />}
            #END add-point
 
         #----<<oockl005>>----
         #Ctrlp:input.c.page1.oockl005
         ON ACTION controlp INFIELD oockl005
            #add-point:ON ACTION controlp INFIELD oockl005
            {<point name="input.c.page1.oockl005" />}
            #END add-point
 
         #----<<oockl006>>----
         #Ctrlp:input.c.page1.oockl006
         ON ACTION controlp INFIELD oockl006
            #add-point:ON ACTION controlp INFIELD oockl006
            {<point name="input.c.page1.oockl006" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_oock_d[l_ac].* = g_oock_d_t.*
               CLOSE aooi040_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_oock_d[l_ac].oock003,-263,1)
               LET g_oock_d[l_ac].* = g_oock_d_t.*
            ELSE
               #寫入修改者/修改日期資訊
               LET g_oock1_info_d[l_ac].oockmodid = g_user 
LET g_oock1_info_d[l_ac].oockmoddt = cl_get_current()
 
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
         
               UPDATE oock_t SET (oock001,oock002,oockstus,oock003,oockmodid,oockmoddt,oockownid,oockowndp, 
                   oockcrtid,oockcrtdp,oockcrtdt) = (g_oock_m.oock001,g_oock_m.oock002,g_oock_d[l_ac].oockstus, 
                   g_oock_d[l_ac].oock003,g_oock1_info_d[l_ac].oockmodid,g_oock1_info_d[l_ac].oockmoddt, 
                   g_oock1_info_d[l_ac].oockownid,g_oock1_info_d[l_ac].oockowndp,g_oock1_info_d[l_ac].oockcrtid, 
                   g_oock1_info_d[l_ac].oockcrtdp,g_oock1_info_d[l_ac].oockcrtdt)
                WHERE oockent = g_enterprise AND oock001 = g_oock_m.oock001 
                 AND oock002 = g_oock_m.oock002 
 
 
                 AND oock003 = g_oock_d_t.oock003 #項次   
 
                 
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
               
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("oock_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                  #WHEN SQLCA.sqlcode #其他錯誤
                  #   CALL cl_err("oock_t",SQLCA.sqlcode,1)  
                  #   CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_oock_m.oock001
               LET gs_keys_bak[1] = g_oock001_t
               LET gs_keys[2] = g_oock_m.oock002
               LET gs_keys_bak[2] = g_oock002_t
               LET gs_keys[3] = g_oock_d[g_detail_idx].oock003
               LET gs_keys_bak[3] = g_oock_d_t.oock003
               CALL aooi040_update_b('oock_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                              INITIALIZE l_var_keys TO NULL 
         INITIALIZE l_field_keys TO NULL 
         INITIALIZE l_vars TO NULL 
         INITIALIZE l_fields TO NULL 
         INITIALIZE l_var_keys_bak TO NULL 
         IF g_oock_m.oock001 = g_detail_multi_table_t.oockl001 AND
         g_oock_m.oock002 = g_detail_multi_table_t.oockl002 AND
         g_oock_d[l_ac].oock003 = g_detail_multi_table_t.oockl003 AND
         g_oock_d[l_ac].oockl005 = g_detail_multi_table_t.oockl005 AND
         g_oock_d[l_ac].oockl006 = g_detail_multi_table_t.oockl006 THEN
         ELSE 
            LET l_var_keys[01] = g_oock_m.oock001
            LET l_field_keys[01] = 'oockl001'
            LET l_var_keys[02] = g_oock_m.oock002
            LET l_field_keys[02] = 'oockl002'
            LET l_var_keys[03] = g_oock_d[l_ac].oock003
            LET l_field_keys[03] = 'oockl003'
            LET l_var_keys[04] = g_dlang
            LET l_field_keys[04] = 'oockl004'
            LET l_vars[01] = g_oock_d[l_ac].oockl005
            LET l_fields[01] = 'oockl005'
            LET l_vars[02] = g_oock_d[l_ac].oockl006
            LET l_fields[02] = 'oockl006'
            LET l_vars[03] = g_enterprise 
            LET l_fields[03] = 'oocklent'
            LET l_var_keys_bak[01] = g_detail_multi_table_t.oockl001
            LET l_var_keys_bak[02] = g_detail_multi_table_t.oockl002
            LET l_var_keys_bak[03] = g_detail_multi_table_t.oockl003
            LET l_var_keys_bak[04] = g_detail_multi_table_t.oockl004
            CALL cl_multitable(l_var_keys,l_field_keys,l_vars,l_fields,l_var_keys_bak,'oockl_t')
         END IF 
 
               END CASE
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
            END IF
 
         AFTER INPUT
            #若單身還沒有輸入資料, 強制切換至單身
            IF cl_null(g_oock_d[1].oock003) THEN
               CALL g_oock_d.deleteElement(1)
               NEXT FIELD oock003
            END IF
            #add-point:input段after input 
            {<point name="input.body.after_input"/>}
            #end add-point    
            
         ON ACTION controlo   
            CALL FGL_SET_ARR_CURR(g_oock_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_oock_d.getLength()+1
            
      END INPUT
 
 
      
      DISPLAY ARRAY g_oock1_info_d TO s_detail1_info.* ATTRIBUTES(COUNT=g_rec_b)  
      
         BEFORE ROW
            CALL aooi040_b_fill(g_wc2) 
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1_info")
            LET l_ac = g_detail_idx
            DISPLAY g_detail_idx TO FORMONLY.idx
            CALL aooi040_ui_detailshow()
        
         BEFORE DISPLAY 
            CALL FGL_SET_ARR_CURR(g_detail_idx)      
         
         #add-point:page2自定義行為
         {<point name="input.body1_info.action"/>}
         #end add-point
         
      END DISPLAY
 
 
      
 
      
      #add-point:input段more_input
      {<point name="input.more_inputarray"/>}
      #end add-point    
      
      BEFORE DIALOG
         #add-point:input段before_dialog
         {<point name="input.before_dialog"/>}
         #end add-point 
         #新增時強制從單頭開始填
         IF p_cmd = 'a' THEN
            NEXT FIELD oock001
	     ELSE
            CASE g_aw
               WHEN "s_detail1"
                  NEXT FIELD oockstus
               WHEN "s_detail1_info"
                  NEXT FIELD oock003_2
 
 
            END CASE
         END IF
   
      ON ACTION controlf
         CALL cl_set_focus_form(ui.Interface.getRootNode()) RETURNING g_fld_name,g_frm_name
         CALL cl_fldhelp(g_frm_name,g_fld_name,g_lang)
 
      ON ACTION controlr
         CALL cl_show_req_fields()
 
      ON ACTION controls
         CALL cl_set_head_visible("","AUTO")
 
      ON ACTION accept
         ACCEPT DIALOG
        
      ON ACTION cancel      #在dialog button (放棄)
         LET g_action_choice=""
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION close       #在dialog 右上角 (X)
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION exit        #toolbar 離開
         LET INT_FLAG = TRUE 
