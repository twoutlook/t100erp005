  <section id="aooi040.b_fill" ver="1" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aooi040_b_fill(p_wc2)
   {<Local define>}
   DEFINE p_wc2      STRING
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_oock_d.clear()
   CALL g_oock1_info_d.clear()
 
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
      LET g_sql = "SELECT  UNIQUE oockstus,oock003,'','','',oockmodid,'',oockmoddt,oockownid,'',oockowndp, 
          '',oockcrtid,'',oockcrtdp,'',oockcrtdt FROM oock_t",   
                  " LEFT JOIN oockl_t ON oock001 = oockl001 AND oock002 = oockl002 AND oock003 = oockl003 AND oockl004 = '",g_lang,"'",
                  
                  " WHERE oockent= ? AND oock001=? AND oock002=?"  
                  
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF aooi040_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY oock_t.oock003"
      
      PREPARE aooi040_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR aooi040_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_oock_m.oock001
                                               ,g_oock_m.oock002
 
 
                                               
      FOREACH b_fill_cs INTO g_oock_d[l_ac].oockstus,g_oock_d[l_ac].oock003,g_oock_d[l_ac].oockl005, 
          g_oock_d[l_ac].oockl006,g_oock1_info_d[l_ac].oock003,g_oock1_info_d[l_ac].oockmodid,g_oock1_info_d[l_ac].oockmodid_desc, 
          g_oock1_info_d[l_ac].oockmoddt,g_oock1_info_d[l_ac].oockownid,g_oock1_info_d[l_ac].oockownid_desc, 
          g_oock1_info_d[l_ac].oockowndp,g_oock1_info_d[l_ac].oockowndp_desc,g_oock1_info_d[l_ac].oockcrtid, 
          g_oock1_info_d[l_ac].oockcrtid_desc,g_oock1_info_d[l_ac].oockcrtdp,g_oock1_info_d[l_ac].oockcrtdp_desc, 
          g_oock1_info_d[l_ac].oockcrtdt
                             ,g_oock_d[l_ac].oockstus,g_oock_d[l_ac].oock003,g_oock_d[l_ac].oockl005, 
                                 g_oock_d[l_ac].oockl006,g_oock1_info_d[l_ac].oock003,g_oock1_info_d[l_ac].oockmodid, 
                                 g_oock1_info_d[l_ac].oockmodid_desc,g_oock1_info_d[l_ac].oockmoddt, 
                                 g_oock1_info_d[l_ac].oockownid,g_oock1_info_d[l_ac].oockownid_desc, 
                                 g_oock1_info_d[l_ac].oockowndp,g_oock1_info_d[l_ac].oockowndp_desc, 
                                 g_oock1_info_d[l_ac].oockcrtid,g_oock1_info_d[l_ac].oockcrtid_desc, 
                                 g_oock1_info_d[l_ac].oockcrtdp,g_oock1_info_d[l_ac].oockcrtdp_desc, 
                                 g_oock1_info_d[l_ac].oockcrtdt
 
 
                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_oock1_info_d[l_ac].oockownid_desc = cl_get_username(g_oock1_info_d[l_ac].oockownid)
      #LET g_oock1_info_d[l_ac].oockowndp_desc = cl_get_deptname(g_oock1_info_d[l_ac].oockowndp)
      #LET g_oock1_info_d[l_ac].oockcrtid_desc = cl_get_username(g_oock1_info_d[l_ac].oockcrtid)
      #LET g_oock1_info_d[l_ac].oockcrtdp_desc = cl_get_deptname(g_oock1_info_d[l_ac].oockcrtdp)
      #LET g_oock1_info_d[l_ac].oockmodid_desc = cl_get_username(g_oock1_info_d[l_ac].oockmodid)
      ##LET g_oock_d[l_ac].oockcnfid_desc = cl_get_deptname(g_oock_d[l_ac].oockcnfid)
      ##LET g_oock_d[l_ac].oockpstid_desc = cl_get_deptname(g_oock_d[l_ac].oockpstid)
      
 
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_oock_d.deleteElement(g_oock_d.getLength())
      CALL g_oock1_info_d.deleteElement(g_oock1_info_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE aooi040_pb   
   
END FUNCTION
]]>
</section>
