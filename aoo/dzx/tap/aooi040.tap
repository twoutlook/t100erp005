  <section id="aooi040.modify" ver="1" status="" src="s">
<![CDATA[#+ 資料修改
PRIVATE FUNCTION aooi040_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_oock_m.oock001 IS NULL
   OR g_oock_m.oock002 IS NULL
 
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
    SELECT UNIQUE oock001,oock002
 INTO g_oock_m.oock001,g_oock_m.oock002
 FROM oock_t
 WHERE oockent = g_enterprise AND oock001 = g_oock_m.oock001 AND oock002 = g_oock_m.oock002
 
   ERROR ""
  
   LET g_oock001_t = g_oock_m.oock001
   LET g_oock002_t = g_oock_m.oock002
 
 
   CALL s_transaction_begin()
   
   OPEN aooi040_cl USING g_enterprise,g_oock_m.oock001
                                                       ,g_oock_m.oock002
 
 
   IF STATUS THEN
      CALL cl_err("OPEN aooi040_cl:", STATUS, 1)
      CLOSE aooi040_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aooi040_cl INTO g_oock_m.oock001,g_oock_m.oock001_desc,g_oock_m.oock002,g_oock_m.oock002_desc 

 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_oock_m.oock001,SQLCA.sqlcode,0)
      CLOSE aooi040_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL aooi040_show()
   WHILE TRUE
      LET g_oock001_t = g_oock_m.oock001
      LET g_oock002_t = g_oock_m.oock002
 
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL aooi040_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_oock_m.* = g_oock_m_t.*
         CALL aooi040_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_oock_m.oock001 != g_oock001_t 
      OR g_oock_m.oock002 != g_oock002_t 
 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE oock_t SET oock001 = g_oock_m.oock001
                                       , oock002 = g_oock_m.oock002
 
 
          WHERE oockent = g_enterprise AND oock001 = g_oock001_t
            AND oock002 = g_oock002_t
 
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("oock_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("oock_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_oock_m.oock001,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE aooi040_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_oock_m.oock001,'U')
 
   CALL aooi040_b_fill("1=1")
   
END FUNCTION   
]]>
</section>
