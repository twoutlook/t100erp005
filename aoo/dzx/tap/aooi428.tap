  <point name="input.c.page1.oogf002" cite_std="N" status="" ver="1" src="s" new="N" order="" cite_ver="" mark_hard="N">
<![CDATA[#此段落由子樣板a07產生            
            #開窗i段
          IF NOT cl_null(g_oogf_d[l_ac].oogf002)  THEN
             LET g_qryparam.reqry = FALSE
		    
