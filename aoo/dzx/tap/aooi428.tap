             #給予arg
             CALL q_ooag001_2()                                #呼叫開窗
		    
             LET g_oogf_d[l_ac].oogf002 = g_qryparam.return1              #將開窗取得的值回傳到變數
             CALL aooi428_oogf002_desc(g_oogf_d[l_ac].oogf002) RETURNING g_oogf_d[l_ac].oogf002_desc
             DISPLAY BY NAME g_oogf_d[l_ac].oogf002_desc
             DISPLAY g_oogf_d[l_ac].oogf002 TO oogf002              #顯示到畫面上
             NEXT FIELD oogf002             #返回原欄位
          ELSE
             LET g_qryparam.reqry = FALSE
             LET g_qryparam.state = 'c'
             LET g_qryparam.where  = "ooagstus = 'Y'"
             LET l_oogf002_str = ''
             CALL q_ooag001_2()             #呼叫開窗
             LET g_qryparam.where  =""
             LET l_oogf002_str = g_qryparam.return1 
          
             IF NOT cl_null(l_oogf002_str) THEN
                CALL aooi428_01()  RETURNING l_oogf003
                CALL s_transaction_begin()
                LET l_errno = 0
                LET l_errno2 = 0
                LET bst= base.StringTokenizer.create(l_oogf002_str,'|')
                WHILE bst.hasMoreTokens()
                   LET g_oogf_d[l_ac].oogf002=bst.nextToken()
                   INITIALIZE l_oogf.* TO NULL
                   LET l_oogf.oogf001 = g_oogf_m.oogf001
                   LET l_oogf.oogf002 = g_oogf_d[l_ac].oogf002
                   LET l_oogf.oogf003 = l_oogf003
                   LET l_oogf.oogfstus = 'Y'
                   LET l_oogf.oogfcrtdt = cl_get_current()
                   INSERT INTO oogf_t(oogfent,oogfsite,oogf001,oogf002,oogf003,oogfstus,oogfownid,oogfowndp,oogfcrtid,oogfcrtdp,oogfcrtdt)
                   VALUES(g_enterprise,g_site,l_oogf.oogf001,l_oogf.oogf002,l_oogf.oogf003,l_oogf.oogfstus,
                          g_user,g_dept,g_user,g_dept,l_oogf.oogfcrtdt)
                   IF SQLCA.sqlcode  THEN
                      CALL cl_err(l_oogf.oogf002,SQLCA.sqlcode,1)
                      LET l_errno = l_errno +1
                      EXIT WHILE	
                   ELSE
                      LET l_errno2 = l_errno2 +1
                      LET l_ac = l_ac + 1
                   END IF
                END WHILE
                IF l_errno >0 THEN
                   CALL s_transaction_end('N','0') 
                 ELSE
                    CALL s_transaction_end('Y','0') 
                 END IF
                 IF l_errno2 = 0 THEN
                    NEXT FIELD oogf002
                 ELSE
                    CALL aooi428_b_fill(" 1=1")                   
                    CALL aooi428_ref_show()
                 END IF
                 EXIT DIALOG                
             END IF    
          END IF  ]]>
</point>
