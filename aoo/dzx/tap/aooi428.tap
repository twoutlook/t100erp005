<![CDATA[#+组别说明
PRIVATE FUNCTION aooi428_oogf001_desc(p_oogf001)
   DEFINE p_oogf001    LIKE oogf_t.oogf001
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = p_oogf001
   CALL ap_ref_array2(g_ref_fields,"SELECT ooge002 FROM ooge_t WHERE oogeent='"||g_enterprise||"' AND oogesite='"||g_site||"' AND ooge001=? ","") RETURNING g_rtn_fields
   RETURN   g_rtn_fields[1]
END FUNCTION]]>
</point>
  <point name="function.aooi428_oogf002_desc" cite_std="N" status="" ver="1" src="s" new="Y" order="5" cite_ver="" mark_hard="N">
<![CDATA[# 抓取员工全名
PRIVATE FUNCTION aooi428_oogf002_desc(p_oogf002)
DEFINE l_oogf002_desc        LIKE oofa_t.oofa011
    DEFINE p_oogf002             LIKE oogf_t.oogf002
    
    LET  l_oogf002_desc = ''
    SELECT oofa011 INTO l_oogf002_desc FROM oofa_t
     WHERE oofaent = g_enterprise
       AND oofa001 IN (SELECT ooag002 FROM ooag_t
                        WHERE ooagent = g_enterprise
                          AND ooag001 = p_oogf002)
     RETURN l_oogf002_desc
END FUNCTION]]>
</point>
