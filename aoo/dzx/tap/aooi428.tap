  <point name="function.aooi428_oogf001_chk" cite_std="N" status="" ver="1" src="s" new="Y" order="2" cite_ver="" mark_hard="N">
<![CDATA[# 檢查資料資料是否組別當中存在且有效
PRIVATE FUNCTION aooi428_oogf001_chk(p_oogf001)
DEFINE  l_ooge001         LIKE ooge_t.ooge001
   DEFINE  l_oogestus        LIKE ooge_t.oogestus
   DEFINE  p_oogf001         LIKE oogf_t.oogf001
   
   LET g_errno='' 
   SELECT ooge001,oogestus INTO l_ooge001,l_oogestus FROM ooge_t
    WHERE oogeent = g_enterprise
      AND ooge001 = p_oogf001
      AND oogesite = g_site
      
   CASE
      WHEN SQLCA.SQLCODE=100   LET g_errno = 'aoo-00067'
      WHEN l_oogestus ='N'     LET g_errno = 'aoo-00068'   
   END CASE
END FUNCTION]]>
</point>
