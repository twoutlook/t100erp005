  <section id="aooi210.b_fill" ver="6" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aooi210_b_fill()
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_oobn_d.clear()    #g_oobn_d 單頭及單身 
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF aooi210_fill_chk(1) THEN
   
      LET g_sql = "SELECT  UNIQUE oobn002,oobn003 ,t1.oobx003 ,t2.oobx003 FROM oobn_t",   
                  " INNER JOIN oobm_t ON oobm001 = oobn001 ",
 
                  #"",
                  
                  "",
                                 " LEFT JOIN oobx_t t1 ON t1.oobxent='"||g_enterprise||"' AND t1.oobx001=oobn002  ",
               " LEFT JOIN oobx_t t2 ON t2.oobxent='"||g_enterprise||"' AND t2.oobx001=oobn003  ",
 
                  " WHERE oobnent=? AND oobn001=?"
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      #add-point:b_fill段sql_before
      {<point name="b_fill.body.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table1) THEN
         LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
      END IF
      
      #子單身的WC
      
      
      LET g_sql = g_sql, " ORDER BY oobn_t.oobn002,oobn_t.oobn003"
      
      #add-point:單身填充控制
      {<point name="b_fill.sql"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE aooi210_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR aooi210_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_oobm_m.oobm001
                                               
      FOREACH b_fill_cs INTO g_oobn_d[l_ac].oobn002,g_oobn_d[l_ac].oobn003,g_oobn_d[l_ac].oobn002_desc_desc, 
          g_oobn_d[l_ac].oobn003_desc_desc
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               CALL cl_err( '', 9035, 1)
            END IF
            EXIT FOREACH
         END IF
         
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
 
   
   #add-point:browser_fill段其他table處理
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_oobn_d.deleteElement(g_oobn_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE aooi210_pb
 
   
END FUNCTION
]]>
  </section>
