  <section id="aooi210.modify" ver="9" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aooi210_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_oobm_m.oobm001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE aooi210_master_referesh USING g_oobm_m.oobm001 INTO g_oobm_m.oobm001,g_oobm_m.oobm002,g_oobm_m.oobm003, 
       g_oobm_m.oobmstus,g_oobm_m.oobmownid,g_oobm_m.oobmowndp,g_oobm_m.oobmcrtid,g_oobm_m.oobmcrtdp, 
       g_oobm_m.oobmcrtdt,g_oobm_m.oobmmodid,g_oobm_m.oobmmoddt,g_oobm_m.oobm002_desc,g_oobm_m.oobm003_desc, 
       g_oobm_m.oobmownid_desc,g_oobm_m.oobmowndp_desc,g_oobm_m.oobmcrtid_desc,g_oobm_m.oobmcrtdp_desc, 
       g_oobm_m.oobmmodid_desc
 
   ERROR ""
  
   LET g_oobm001_t = g_oobm_m.oobm001
 
   CALL s_transaction_begin()
   
   OPEN aooi210_cl USING g_enterprise,g_oobm_m.oobm001
   IF STATUS THEN
      CALL cl_err("OPEN aooi210_cl:", STATUS, 1)
      CLOSE aooi210_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aooi210_master_referesh USING g_oobm_m.oobm001 INTO g_oobm_m.oobm001,g_oobm_m.oobm002,g_oobm_m.oobm003, 
       g_oobm_m.oobmstus,g_oobm_m.oobmownid,g_oobm_m.oobmowndp,g_oobm_m.oobmcrtid,g_oobm_m.oobmcrtdp, 
       g_oobm_m.oobmcrtdt,g_oobm_m.oobmmodid,g_oobm_m.oobmmoddt,g_oobm_m.oobm002_desc,g_oobm_m.oobm003_desc, 
       g_oobm_m.oobmownid_desc,g_oobm_m.oobmowndp_desc,g_oobm_m.oobmcrtid_desc,g_oobm_m.oobmcrtdp_desc, 
       g_oobm_m.oobmmodid_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_oobm_m.oobm001,SQLCA.sqlcode,0)
      CLOSE aooi210_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   CALL aooi210_show()
   WHILE TRUE
      LET g_oobm001_t = g_oobm_m.oobm001
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_oobm_m.oobmmodid = g_user 
LET g_oobm_m.oobmmoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      CALL aooi210_input("u")
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_oobm_m.* = g_oobm_m_t.*
         CALL aooi210_show()
         CALL cl_err('',9001,0)
         CALL s_transaction_end('N','0')
         EXIT WHILE
      END IF 
      
      #若有modid跟moddt則進行update
      UPDATE oobm_t SET (oobmmodid,oobmmoddt) = (g_oobm_m.oobmmodid,g_oobm_m.oobmmoddt)
       WHERE oobment = g_enterprise AND oobm001 = g_oobm001_t
 
                  
      #若單頭key欄位有變更
      IF g_oobm_m.oobm001 != g_oobm001_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE oobn_t SET oobn001 = g_oobm_m.oobm001
 
          WHERE oobnent = g_enterprise AND oobn001 = g_oobm001_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("oobn_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("oobn_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_oobm_m.oobm001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE aooi210_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_oobm_m.oobm001,'U')
 
END FUNCTION   
]]>
