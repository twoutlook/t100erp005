  <section id="aooi210.input.body" ver="6" status="" src="s">
    <![CDATA[   
      #Page1 預設值產生於此處
      INPUT ARRAY g_oobn_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_oobn_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL aooi210_b_fill()
            LET g_rec_b = g_oobn_d.getLength()
            #add-point:資料輸入前
            {<point name="input.d.before_input"/>}
            #end add-point
         
         BEFORE ROW
            #add-point:modify段before row2
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET l_insert = FALSE
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET g_detail_idx = l_ac
            
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
         
            CALL s_transaction_begin()
            OPEN aooi210_cl USING g_enterprise,g_oobm_m.oobm001
            IF STATUS THEN
               CALL cl_err("OPEN aooi210_cl:", STATUS, 1)
               CLOSE aooi210_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            
            LET g_rec_b = g_oobn_d.getLength()
            
            IF g_rec_b >= l_ac 
               AND g_oobn_d[l_ac].oobn002 IS NOT NULL
               AND g_oobn_d[l_ac].oobn003 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_oobn_d_t.* = g_oobn_d[l_ac].*  #BACKUP
               CALL aooi210_set_entry_b(l_cmd)
               #add-point:modify段after_set_entry_b
               {<point name="input.body.after_set_entry_b"/>}
               #end add-point  
               CALL aooi210_set_no_entry_b(l_cmd)
               IF NOT aooi210_lock_b("oobn_t","'1'") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aooi210_bcl INTO g_oobn_d[l_ac].oobn002,g_oobn_d[l_ac].oobn003
                  IF SQLCA.sqlcode THEN
                     CALL cl_err(g_oobn_d_t.oobn002,SQLCA.sqlcode,1)
                     LET l_lock_sw = "Y"
                  END IF
                  
                  LET g_bfill = "N"
                  CALL aooi210_show()
                  LET g_bfill = "Y"
                  
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
        
         BEFORE INSERT
            
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_oobn_d[l_ac].* TO NULL 
            
            LET g_oobn_d_t.* = g_oobn_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL aooi210_set_entry_b(l_cmd)
            #add-point:modify段after_set_entry_b
            {<point name="input.body.insert.after_set_entry_b"/>}
            #end add-point
            CALL aooi210_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_oobn_d[li_reproduce_target].* = g_oobn_d[li_reproduce].*
 
               LET g_oobn_d[li_reproduce_target].oobn002 = NULL
               LET g_oobn_d[li_reproduce_target].oobn003 = NULL
 
            END IF
            #公用欄位給值(單身)
            
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            #add-point:單身新增
            {<point name="input.body.b_a_insert"/>}
            #end add-point
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM oobn_t 
             WHERE oobnent = g_enterprise AND oobn001 = g_oobm_m.oobm001
 
               AND oobn002 = g_oobn_d[l_ac].oobn002
               AND oobn003 = g_oobn_d[l_ac].oobn003
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前
               {<point name="input.body.b_insert"/>}
               #end add-point
            
               #同步新增到同層的table
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_oobm_m.oobm001
               LET gs_keys[2] = g_oobn_d[g_detail_idx].oobn002
               LET gs_keys[3] = g_oobn_d[g_detail_idx].oobn003
               CALL aooi210_insert_b('oobn_t',gs_keys,"'1'")
                           
               #add-point:單身新增後
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_oobn_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("oobn_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL aooi210_b_fill()
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert2"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR 'INSERT O.K'
               LET g_rec_b = g_rec_b + 1
            END IF
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_oobn_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_oobn_d.deleteElement(l_ac)
               NEXT FIELD oobn002
            END IF
         
            IF g_oobn_d[l_ac].oobn002 IS NOT NULL
               AND g_oobn_d_t.oobn003 IS NOT NULL
 
               THEN 
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete" mark="Y"/>}
               #end add-point 
               
               DELETE FROM oobn_t
                WHERE oobnent = g_enterprise AND oobn001 = g_oobm_m.oobm001 AND
 
                      oobn002 = g_oobn_d_t.oobn002
                  AND oobn003 = g_oobn_d_t.oobn003
 
                  
               #add-point:單身刪除中
               {<point name="input.body.m_delete"/>}
               #end add-point 
               
               IF SQLCA.sqlcode THEN
                  CALL cl_err("oobn_t",SQLCA.sqlcode,1)
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               ELSE
                  LET g_rec_b = g_rec_b-1
                  
                  #add-point:單身刪除後
                  {<point name="input.body.a_delete"/>}
                  #end add-point
                  CALL s_transaction_end('Y','0')
               END IF 
               CLOSE aooi210_bcl
               LET l_count = g_oobn_d.getLength()
            END IF 
            
                           INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_oobm_m.oobm001
               LET gs_keys[2] = g_oobn_d[g_detail_idx].oobn002
               LET gs_keys[3] = g_oobn_d[g_detail_idx].oobn003
 
              
         AFTER DELETE 
            #add-point:單身刪除後2
            {<point name="input.body.after_delete"/>}
            #end add-point
                           CALL aooi210_delete_b('oobn_t',gs_keys,"'1'")
 
                  #此段落由子樣板a02產生
         AFTER FIELD oobn002
            
            #add-point:AFTER FIELD oobn002
            {<point name="input.a.page1.oobn002" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD oobn002
            #add-point:BEFORE FIELD oobn002
            {<point name="input.b.page1.oobn002" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE oobn002
            #add-point:ON CHANGE oobn002
            {<point name="input.g.page1.oobn002" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD oobn003
            
            #add-point:AFTER FIELD oobn003
            {<point name="input.a.page1.oobn003" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD oobn003
            #add-point:BEFORE FIELD oobn003
            {<point name="input.b.page1.oobn003" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE oobn003
            #add-point:ON CHANGE oobn003
            {<point name="input.g.page1.oobn003" />}
            #END add-point
 
 
                  #Ctrlp:input.c.page1.oobn002
         ON ACTION controlp INFIELD oobn002
            #add-point:ON ACTION controlp INFIELD oobn002
            {<point name="input.c.page1.oobn002" />}
            #END add-point
 
         #Ctrlp:input.c.page1.oobn003
         ON ACTION controlp INFIELD oobn003
            #add-point:ON ACTION controlp INFIELD oobn003
            {<point name="input.c.page1.oobn003" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_oobn_d[l_ac].* = g_oobn_d_t.*
               CLOSE aooi210_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_oobn_d[l_ac].oobn002,-263,1)
               LET g_oobn_d[l_ac].* = g_oobn_d_t.*
            ELSE
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               #寫入修改者/修改日期資訊(單身)
               
      
               UPDATE oobn_t SET (oobn001,oobn002,oobn003) = (g_oobm_m.oobm001,g_oobn_d[l_ac].oobn002, 
                   g_oobn_d[l_ac].oobn003)
                WHERE oobnent = g_enterprise AND oobn001 = g_oobm_m.oobm001 
 
                  AND oobn002 = g_oobn_d_t.oobn002 #項次   
                  AND oobn003 = g_oobn_d_t.oobn003  
 
                  
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("oobn_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                     LET g_oobn_d[l_ac].* = g_oobn_d_t.*
                  WHEN SQLCA.sqlcode #其他錯誤
                     CALL cl_err("oobn_t",SQLCA.sqlcode,1)
                     LET g_oobn_d[l_ac].* = g_oobn_d_t.*                     
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_oobm_m.oobm001
               LET gs_keys_bak[1] = g_oobm001_t
               LET gs_keys[2] = g_oobn_d[g_detail_idx].oobn002
               LET gs_keys_bak[2] = g_oobn_d_t.oobn002
               LET gs_keys[3] = g_oobn_d[g_detail_idx].oobn003
               LET gs_keys_bak[3] = g_oobn_d_t.oobn003
               CALL aooi210_update_b('oobn_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
               END CASE
               
               LET g_log1 = util.JSON.stringify(g_oobm_m),util.JSON.stringify(g_oobn_d_t)
               LET g_log2 = util.JSON.stringify(g_oobm_m),util.JSON.stringify(g_oobn_d[l_ac])
               
               IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                  CALL s_transaction_end('N','0')
               END IF
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            #add-point:單身after_row
            {<point name="input.body.after_row"/>}
            #end add-point
            CALL aooi210_unlock_b("oobn_t","'1'")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock
            #add-point:單身after_row2
            {<point name="input.body.after_row2"/>}
            #end add-point
              
         AFTER INPUT
            #add-point:input段after input 
            {<point name="input.body.after_input"/>}
            #end add-point 
 
         ON ACTION controlo    
            CALL FGL_SET_ARR_CURR(g_oobn_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_oobn_d.getLength()+1
            
         #ON ACTION cancel
         #   LET INT_FLAG = 1
         #   LET g_detail_idx = 1
         #   EXIT DIALOG 
 
      END INPUT
      
 
      
 
      
 
      
 
      
]]>
