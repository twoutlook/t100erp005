      LET g_ooib_m.ooibownid = g_user
      LET g_ooib_m.ooibowndp = g_dept
      LET g_ooib_m.ooibcrtid = g_user
      LET g_ooib_m.ooibcrtdp = g_dept 
      LET g_ooib_m.ooibcrtdt = cl_get_current()
