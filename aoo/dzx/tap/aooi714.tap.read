    <![CDATA[   LET ls_sql = " SELECT UNIQUE ooib002,ooib001,ooib004,(ooib011||':'||a.gzcbl004||'/'||ooib012||'"||l_season||"'||ooib013||'"||l_month||"'||ooib014||'"||l_days||"'),(ooib021||':'||b.gzcbl004||'/'||ooib022||'"||l_season||"'||ooib023||'"||l_month||"'||ooib024||'"||l_days||"'),ooib006,ooibstus ",
