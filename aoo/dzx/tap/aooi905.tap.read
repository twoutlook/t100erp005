  <section id="aooi905.b_fill" ver="7" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aooi905_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define" edit="s"/>}
   #end add-point     
   #add-point:b_fill段define
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
   
   #先清空單身變數內容
   CALL g_ooif_d.clear()
   CALL g_ooif2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE ooifstus,ooif001,ooif002,ooif003,ooif004,ooif005,ooif006,ooif007,ooif008, 
       ooif009,ooif010,ooif011,ooif012,ooif013,ooif014,ooif015,ooif016,ooif017,ooif018,ooif019,ooif020, 
       ooif021,ooif022,ooif023,ooif024,ooif025,ooif026,ooif027,ooif028,ooif029,ooifpos,ooifstamp,ooif001, 
       ooifownid,ooifowndp,ooifcrtid,ooifcrtdp,ooifcrtdt,ooifmodid,ooifmoddt,t1.ooial003 ,t2.ooail003 , 
       t3.ooag011 ,t4.ooefl003 ,t5.ooag011 ,t6.ooefl003 ,t7.ooag011 FROM ooif_t",   
               "",
               
                              " LEFT JOIN ooial_t t1 ON t1.ooialent='"||g_enterprise||"' AND t1.ooial001=ooif001 AND t1.ooial002='"||g_dlang||"' ",
               " LEFT JOIN ooail_t t2 ON t2.ooailent='"||g_enterprise||"' AND t2.ooail001=ooif002 AND t2.ooail002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent='"||g_enterprise||"' AND t3.ooag001=ooifownid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=ooifowndp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent='"||g_enterprise||"' AND t5.ooag001=ooifcrtid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent='"||g_enterprise||"' AND t6.ooefl001=ooifcrtdp AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent='"||g_enterprise||"' AND t7.ooag001=ooifmodid  ",
 
               " WHERE ooifent= ? AND ooif000=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("ooif_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF aooi905_fill_chk(1) THEN
      IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
         LET g_sql = g_sql, " ORDER BY ooif_t.ooif001"
         #add-point:b_fill段fill_before
         {<point name="b_fill.fill_before"/>}
         #end add-point
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE aooi905_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR aooi905_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_ooif_m.ooif000
                                               
      FOREACH b_fill_cs INTO g_ooif_d[l_ac].ooifstus,g_ooif_d[l_ac].ooif001,g_ooif_d[l_ac].ooif002,g_ooif_d[l_ac].ooif003, 
          g_ooif_d[l_ac].ooif004,g_ooif_d[l_ac].ooif005,g_ooif_d[l_ac].ooif006,g_ooif_d[l_ac].ooif007, 
          g_ooif_d[l_ac].ooif008,g_ooif_d[l_ac].ooif009,g_ooif_d[l_ac].ooif010,g_ooif_d[l_ac].ooif011, 
          g_ooif_d[l_ac].ooif012,g_ooif_d[l_ac].ooif013,g_ooif_d[l_ac].ooif014,g_ooif_d[l_ac].ooif015, 
          g_ooif_d[l_ac].ooif016,g_ooif_d[l_ac].ooif017,g_ooif_d[l_ac].ooif018,g_ooif_d[l_ac].ooif019, 
          g_ooif_d[l_ac].ooif020,g_ooif_d[l_ac].ooif021,g_ooif_d[l_ac].ooif022,g_ooif_d[l_ac].ooif023, 
          g_ooif_d[l_ac].ooif024,g_ooif_d[l_ac].ooif025,g_ooif_d[l_ac].ooif026,g_ooif_d[l_ac].ooif027, 
          g_ooif_d[l_ac].ooif028,g_ooif_d[l_ac].ooif029,g_ooif_d[l_ac].ooifpos,g_ooif_d[l_ac].ooifstamp, 
          g_ooif2_d[l_ac].ooif001,g_ooif2_d[l_ac].ooifownid,g_ooif2_d[l_ac].ooifowndp,g_ooif2_d[l_ac].ooifcrtid, 
          g_ooif2_d[l_ac].ooifcrtdp,g_ooif2_d[l_ac].ooifcrtdt,g_ooif2_d[l_ac].ooifmodid,g_ooif2_d[l_ac].ooifmoddt, 
          g_ooif_d[l_ac].ooif001_desc,g_ooif_d[l_ac].ooif002_desc,g_ooif2_d[l_ac].ooifownid_desc,g_ooif2_d[l_ac].ooifowndp_desc, 
          g_ooif2_d[l_ac].ooifcrtid_desc,g_ooif2_d[l_ac].ooifcrtdp_desc,g_ooif2_d[l_ac].ooifmodid_desc 

                             
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #應用 a12 樣板自動產生(Version:3)
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF 
            EXIT FOREACH
         END IF
      
         LET l_ac = l_ac + 1
         
      END FOREACH
 
            CALL g_ooif_d.deleteElement(g_ooif_d.getLength())
      CALL g_ooif2_d.deleteElement(g_ooif2_d.getLength())
 
      
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_ooif_d.getLength()
      LET g_ooif_d_mask_o[l_ac].* =  g_ooif_d[l_ac].*
      CALL aooi905_ooif_t_mask()
      LET g_ooif_d_mask_n[l_ac].* =  g_ooif_d[l_ac].*
   END FOR
   
   LET g_ooif2_d_mask_o.* =  g_ooif2_d.*
   FOR l_ac = 1 TO g_ooif2_d.getLength()
      LET g_ooif2_d_mask_o[l_ac].* =  g_ooif2_d[l_ac].*
      CALL aooi905_ooif_t_mask()
      LET g_ooif2_d_mask_n[l_ac].* =  g_ooif2_d[l_ac].*
   END FOR
 
 
   FREE aooi905_pb   
   
END FUNCTION
]]>
  </section>
