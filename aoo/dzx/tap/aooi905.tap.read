  <point name="function.aooi905_ooif000_chk" order="1" ver="2" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 店群檢查
# Memo...........:
# Usage..........: CALL aooi905_ooif000_chk()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/08/31 By yangxf
# Modify.........:
################################################################################
PRIVATE FUNCTION aooi905_ooif000_chk()
DEFINE l_rtaastus  LIKE rtaa_t.rtaastus
DEFINE l_rtak003   LIKE rtak_t.rtak003
   
   LET g_errno = ''
   #huangrh add rtak_t -----20150603
   SELECT rtak003,rtaastus INTO l_rtak003,l_rtaastus
     FROM rtaa_t,rtak_t
    WHERE rtaa001 = g_ooif_m.ooif000
      AND rtakent=rtaaent
      AND rtak001=rtaa001
      AND rtak002='5'
   CASE WHEN SQLCA.SQLCODE = 100   LET g_errno = 'aoo-00373'
        WHEN l_rtak003 <> 'Y'      LET g_errno = 'aoo-00374'
        WHEN l_rtaastus <> 'Y'     LET g_errno = 'art-00067'
        OTHERWISE                  LET g_errno = SQLCA.SQLCODE USING '-------'
   END CASE
END FUNCTION]]>
  </point>
  <point name="function.aooi905_ooif001_chk" order="2" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 款別編號檢查
# Memo...........:
# Usage..........: CALL aooi905_ooif001_chk()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/08/31 By yangxf
# Modify.........:
################################################################################
PRIVATE FUNCTION aooi905_ooif001_chk()
   DEFINE l_ooiastus   LIKE ooia_t.ooiastus
   DEFINE l_ooia002    LIKE ooia_t.ooia002
   DEFINE l_ooia012    LIKE ooia_t.ooia012
   
   LET g_errno = ''
   SELECT ooiastus,ooia002,ooia012 INTO l_ooiastus,l_ooia002,l_ooia012
     FROM ooia_t
    WHERE ooiaent = g_enterprise 
      AND ooia001 = g_ooif_d[l_ac].ooif001
   CASE WHEN SQLCA.SQLCODE = 100    LET g_errno = 'aoo-00364'
        WHEN l_ooiastus <> 'Y'      LET g_errno = 'aoo-00363'
        OTHERWISE                   LET g_errno = SQLCA.SQLCODE USING '-------'
   END CASE
   IF cl_null(g_errno) THEN
      IF g_ooif_d[l_ac].ooif001 != g_ooif_d_t.ooif001 AND NOT cl_null(g_ooif_d_t.ooif001) THEN 
         LET g_ooif_d[l_ac].ooia002 = l_ooia002
         LET g_ooif_d[l_ac].ooif003 = l_ooia012
      END IF 
   END IF 
END FUNCTION]]>
  </point>
