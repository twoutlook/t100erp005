  <section id="aooi905.reproduce" ver="6" status="" src="s" readonly="">
    <![CDATA[#+ 資料複製
PRIVATE FUNCTION aooi905_reproduce()
   DEFINE l_newno     LIKE ooif_t.ooif000 
   DEFINE l_oldno     LIKE ooif_t.ooif000 
 
   DEFINE l_master    RECORD LIKE ooif_t.*
   DEFINE l_detail    RECORD LIKE ooif_t.*
 
   DEFINE l_cnt       LIKE type_t.num10
