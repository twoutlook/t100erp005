  <section id="aooi905.before_delete" ver="2" status="" src="s" readonly="">
    <![CDATA[#+ 單身db資料刪除
PRIVATE FUNCTION aooi905_before_delete()
   #add-point:before_delete段define
   {<point name="before_delete.define" edit="s"/>}
   #end add-point 
   #add-point:before_delete段define
   {<point name="before_delete.define_customerization" edit="c"/>}
   #end add-point 
   #add-point:單筆刪除前
   {<point name="delete.body.b_single_delete" mark="Y"/>}
   #end add-point
   
   DELETE FROM ooif_t
    WHERE ooifent = g_enterprise AND ooif000 = g_ooif_m.ooif000 AND
 
          ooif001 = g_ooif_d_t.ooif001
 
      
   #add-point:單筆刪除中
   {<point name="delete.body.m_single_delete"/>}
   #end add-point
   
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "ooif_t" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN FALSE 
   END IF
   
   #add-point:單筆刪除後
