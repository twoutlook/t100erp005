  <section id="aooi905.detail_reproduce" ver="2" status="" src="s" readonly="">
    <![CDATA[#+ 單身自動複製
PRIVATE FUNCTION aooi905_detail_reproduce()
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE ooif_t.*
 
 
   #add-point:delete段define
   {<point name="detail_reproduce.define" edit="s"/>}
   #end add-point    
   #add-point:delete段define
   {<point name="detail_reproduce.define_customerization" edit="c"/>}
   #end add-point 
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE aooi905_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE aooi905_detail AS ",
                "SELECT * FROM ooif_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO aooi905_detail SELECT * FROM ooif_t 
                                         WHERE ooifent = g_enterprise AND ooif000 = g_ooif000_t
 
   
   #將key修正為調整後   
   UPDATE aooi905_detail 
      #更新key欄位
      SET ooif000 = g_ooif_m.ooif000
 
      #更新共用欄位
      #應用 a13 樣板自動產生(Version:3)
       , ooifownid = g_user 
       , ooifowndp = g_dept
       , ooifcrtid = g_user
       , ooifcrtdp = g_dept 
       , ooifcrtdt = ld_date
       , ooifmodid = g_user
       , ooifmoddt = ld_date
      #, ooifstus = "Y" 
 
 
                                       
   #將資料塞回原table   
   INSERT INTO ooif_t SELECT * FROM aooi905_detail
   
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "reproduce" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE aooi905_detail
   
   #add-point:單身複製後1
   {<point name="detail_reproduce.body.table1.a_insert"/>}
   #end add-point
 
 
   
 
   
   #多語言複製段落
   
   
   CALL s_transaction_end('Y','0')
   
   #已新增完, 調整資料內容(修改時使用)
   LET g_ooif000_t = g_ooif_m.ooif000
 
   
   DROP TABLE aooi905_detail
   
END FUNCTION
