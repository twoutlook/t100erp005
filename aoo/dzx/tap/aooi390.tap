       g_oofg_m.oofg018,g_oofg_m.oofg019,g_oofg_m.oofg020,g_oofg_m.oofg021,g_oofg_m.oofg022,g_oofg_m.oofg023, 
       g_oofg_m.oofgownid,g_oofg_m.oofgowndp,g_oofg_m.oofgcrtid,g_oofg_m.oofgcrtdp,g_oofg_m.oofgcrtdt, 
       g_oofg_m.oofgmodid,g_oofg_m.oofgmoddt,g_oofg_m.oofg013,g_oofg_m.oofg014,g_oofg_m.oofg015,g_oofg_m.oofg004, 
       g_oofg_m.oofg006_desc,g_oofg_m.oofg017_desc,g_oofg_m.oofg022_desc,g_oofg_m.oofg023_desc,g_oofg_m.oofgownid_desc, 
       g_oofg_m.oofgowndp_desc,g_oofg_m.oofgcrtid_desc,g_oofg_m.oofgcrtdp_desc,g_oofg_m.oofgmodid_desc 

   
   CALL aooi390_show()
   
   CALL s_transaction_begin()
   
   LET g_master_multi_table_t.oofgl001 = g_oofg_m.oofg001
LET g_master_multi_table_t.oofgl002 = ' '
LET g_master_multi_table_t.oofgl004 = g_oofg_m.oofgl004
