   LET g_sql = " SELECT UNIQUE oofg001,oofg002,oofg003,oofg007,oofgstus,oofg006,oofg005,oofg008,oofg009, 
       oofg010,oofg011,oofg012,oofg016,oofg017,oofg018,oofg019,oofg020,oofg021,oofg022,oofg023,oofgownid, 
       oofgowndp,oofgcrtid,oofgcrtdp,oofgcrtdt,oofgmodid,oofgmoddt,oofg013,oofg014,oofg015,oofg004,t1.oofgl004 , 
       t2.oofjl003 ,t3.oocql004 ,t4.oocql004 ,t5.oofa011 ,t6.ooefl003 ,t7.oofa011 ,t8.ooefl003 ,t9.oofa011", 

               " FROM oofg_t",
                              " LEFT JOIN oofgl_t t1 ON t1.oofglent='"||g_enterprise||"' AND t1.oofgl001=oofg001 AND t1.oofgl002=oofg006 AND t1.oofgl003='"||g_dlang||"' ",
               " LEFT JOIN oofjl_t t2 ON t2.oofjlent='"||g_enterprise||"' AND t2.oofjl001=oofg017 AND t2.oofjl002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t3 ON t3.oocqlent='"||g_enterprise||"' AND t3.oocql001='211' AND t3.oocql002=oofg022 AND t3.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t4 ON t4.oocqlent='"||g_enterprise||"' AND t4.oocql001='273' AND t4.oocql002=oofg023 AND t4.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t5 ON t5.oofaent='"||g_enterprise||"' AND t5.oofa002='2' AND t5.oofa003=oofgownid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent='"||g_enterprise||"' AND t6.ooefl001=oofgowndp AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t7 ON t7.oofaent='"||g_enterprise||"' AND t7.oofa002='2' AND t7.oofa003=oofgcrtid  ",
               " LEFT JOIN ooefl_t t8 ON t8.ooeflent='"||g_enterprise||"' AND t8.ooefl001=oofgcrtdp AND t8.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t9 ON t9.oofaent='"||g_enterprise||"' AND t9.oofa002='2' AND t9.oofa003=oofgmodid  ",
 
               " WHERE oofgent = '" ||g_enterprise|| "' AND oofg001 = ? AND oofg005 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE aooi390_master_referesh FROM g_sql
 
 
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_aooi390 WITH FORM cl_ap_formpath("aoo",g_code)
   
      #瀏覽頁簽資料初始化
