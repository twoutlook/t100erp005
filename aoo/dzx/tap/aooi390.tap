]]>
</section>
  <section id="aooi390.input.body" ver="4" status="" src="s">
<![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_oofh_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION
         
 
         #+ 此段落由子樣板a43產生
         ON ACTION update_item
            LET g_action_choice="update_item"
            IF cl_auth_chk_act("update_item") THEN
               
               #add-point:ON ACTION update_item
               {<point name="input.detail_input.page1.update_item" />}
               #END add-point
            END IF
 
 
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_oofh_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL aooi390_b_fill()
            LET g_rec_b = g_oofh_d.getLength()
            #add-point:資料輸入前
            {<point name="input.d.before_input"/>}
            #end add-point
 
         BEFORE ROW
            LET l_insert = FALSE
            LET l_cmd = ''
            #CALL FGL_SET_ARR_CURR(g_detail_idx)
            LET l_ac = ARR_CURR()
            LET g_detail_idx = l_ac
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_n TO FORMONLY.idx
         
            CALL s_transaction_begin()
            OPEN aooi390_cl USING g_enterprise,g_oofg_m.oofg001
                                                                ,g_oofg_m.oofg005
 
            IF STATUS THEN
               CALL cl_err("OPEN aooi390_cl:", STATUS, 1)
               CLOSE aooi390_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
                   
            #FETCH aooi390_cl INTO g_oofg_m.oofg001,g_oofg_m.oofgl004,g_oofg_m.oofgl005,g_oofg_m.oofg002, 
            #    g_oofg_m.oofg003,g_oofg_m.oofg007,g_oofg_m.oofgstus,g_oofg_m.oofg006,g_oofg_m.oofg006_desc, 
            #    g_oofg_m.oofg005,g_oofg_m.oofgl004_1,g_oofg_m.oofg008,g_oofg_m.oofg009,g_oofg_m.oofg010, 
            #    g_oofg_m.oofg011,g_oofg_m.oofg012,g_oofg_m.oofg016,g_oofg_m.oofg017,g_oofg_m.oofg017_desc, 
            #    g_oofg_m.oofg018,g_oofg_m.oofg019,g_oofg_m.oofg020,g_oofg_m.oofg021,g_oofg_m.oofg022, 
            #    g_oofg_m.oofg022_desc,g_oofg_m.oofg023,g_oofg_m.oofg023_desc,g_oofg_m.oofgownid,g_oofg_m.oofgownid_desc, 
            #    g_oofg_m.oofgowndp,g_oofg_m.oofgowndp_desc,g_oofg_m.oofgcrtid,g_oofg_m.oofgcrtid_desc, 
            #    g_oofg_m.oofgcrtdp,g_oofg_m.oofgcrtdp_desc,g_oofg_m.oofgcrtdt,g_oofg_m.oofgmodid,g_oofg_m.oofgmodid_desc, 
            #    g_oofg_m.oofgmoddt,g_oofg_m.column,g_oofg_m.table,g_oofg_m.condition,g_oofg_m.oofg013, 
            #    g_oofg_m.oofg014,g_oofg_m.oofg015,g_oofg_m.oofg009_sum,g_oofg_m.oofg009_1,g_oofg_m.oofg009_after, 
            #    g_oofg_m.oofg004 #鎖住將被更改或取消的資料
            #IF SQLCA.sqlcode THEN
            #   CALL cl_err(g_oofg_m.oofg001,SQLCA.sqlcode,0)
            #   CLOSE aooi390_cl
            #   CALL s_transaction_end('N','0')
            #   RETURN
            #END IF
            
            LET g_rec_b = g_oofh_d.getLength()
            
            IF g_rec_b >= l_ac 
               AND g_oofh_d[l_ac].oofh003 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_oofh_d_t.* = g_oofh_d[l_ac].*  #BACKUP
               CALL aooi390_set_entry_b(l_cmd)
               CALL aooi390_set_no_entry_b(l_cmd)
               IF NOT aooi390_lock_b("oofh_t","1") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aooi390_bcl INTO g_oofh_d[l_ac].oofh003
                  IF SQLCA.sqlcode THEN
                     CALL cl_err(g_oofh_d_t.oofh003,SQLCA.sqlcode,1)
                     LET l_lock_sw = "Y"
                  END IF
                  
                  LET g_bfill = "N"
                  CALL aooi390_show()
                  LET g_bfill = "Y"
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
            #其他table資料備份(確定是否更改用)
            LET g_detail_multi_table_t.oofhl001 = g_oofg_m.oofg001
LET g_detail_multi_table_t.oofhl002 = g_oofg_m.oofg005
LET g_detail_multi_table_t.oofhl003 = g_oofh_d[l_ac].oofh003
LET g_detail_multi_table_t.oofhl004 = g_dlang
LET g_detail_multi_table_t.oofhl005 = g_oofh_d[l_ac].oofhl005
 
            
            #其他table進行lock
                        INITIALIZE l_var_keys TO NULL 
            INITIALIZE l_field_keys TO NULL 
            LET l_field_keys[01] = 'oofhl001'
            LET l_var_keys[01] = g_oofg_m.oofg001
            LET l_field_keys[02] = 'oofhl002'
            LET l_var_keys[02] = g_oofg_m.oofg005
            LET l_field_keys[03] = 'oofhl003'
            LET l_var_keys[03] = g_oofh_d[l_ac].oofh003
            LET l_field_keys[04] = 'oofhl004'
            LET l_var_keys[04] = g_dlang
            IF NOT cl_multitable_lock(l_var_keys,l_field_keys,'oofhl_t') THEN
               RETURN 
            END IF 
 
        
         BEFORE INSERT
            
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_oofh_d[l_ac].* TO NULL 
            
            LET g_oofh_d_t.* = g_oofh_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL aooi390_set_entry_b(l_cmd)
            CALL aooi390_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_oofh_d[li_reproduce_target].* = g_oofh_d[li_reproduce].*
 
               LET g_oofh_d[g_oofh_d.getLength()].oofh003 = NULL
 
            END IF
            #公用欄位給值(單身)
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM oofh_t 
             WHERE oofhent = g_enterprise AND oofh001 = g_oofg_m.oofg001
               AND oofh002 = g_oofg_m.oofg005
 
               AND g_oofh_d[l_ac].oofh003 = oofh003
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前
               {<point name="input.body.b_insert"/>}
               #end add-point
            
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_oofg_m.oofg001
               LET gs_keys[2] = g_oofg_m.oofg005
               LET gs_keys[3] = g_oofh_d[g_detail_idx].oofh003
               CALL aooi390_insert_b('oofh_t',gs_keys,"'1'")
                           
               #add-point:單身新增後
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_oofh_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("oofh_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL aooi390_b_fill()
               #資料多語言用-增/改
                        INITIALIZE l_var_keys TO NULL 
         INITIALIZE l_field_keys TO NULL 
         INITIALIZE l_vars TO NULL 
         INITIALIZE l_fields TO NULL 
         INITIALIZE l_var_keys_bak TO NULL 
         IF g_oofg_m.oofg001 = g_detail_multi_table_t.oofhl001 AND
         g_oofg_m.oofg005 = g_detail_multi_table_t.oofhl002 AND
         g_oofh_d[l_ac].oofh003 = g_detail_multi_table_t.oofhl003 AND
         g_oofh_d[l_ac].oofhl005 = g_detail_multi_table_t.oofhl005 THEN
         ELSE 
            LET l_var_keys[01] = g_oofg_m.oofg001
            LET l_field_keys[01] = 'oofhl001'
            LET l_var_keys[02] = g_oofg_m.oofg005
            LET l_field_keys[02] = 'oofhl002'
            LET l_var_keys[03] = g_oofh_d[l_ac].oofh003
            LET l_field_keys[03] = 'oofhl003'
            LET l_var_keys[04] = g_dlang
            LET l_field_keys[04] = 'oofhl004'
            LET l_vars[01] = g_oofh_d[l_ac].oofhl005
            LET l_fields[01] = 'oofhl005'
            LET l_vars[02] = g_enterprise 
            LET l_fields[02] = 'oofhlent'
            LET l_var_keys_bak[01] = g_detail_multi_table_t.oofhl001
            LET l_var_keys_bak[02] = g_detail_multi_table_t.oofhl002
            LET l_var_keys_bak[03] = g_detail_multi_table_t.oofhl003
            LET l_var_keys_bak[04] = g_detail_multi_table_t.oofhl004
            CALL cl_multitable(l_var_keys,l_field_keys,l_vars,l_fields,l_var_keys_bak,'oofhl_t')
         END IF 
 
               #add-point:input段-after_insert
               {<point name="input.body.a_insert2"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR 'INSERT O.K'
               LET g_rec_b = g_rec_b + 1
            END IF
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_oofh_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_oofh_d.deleteElement(l_ac)
               NEXT FIELD oofh003
            END IF
            IF g_oofh_d[l_ac].oofh003 IS NOT NULL
 
               THEN
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete" mark="Y"/>}
               #end add-point   
               
               DELETE FROM oofh_t
                WHERE oofhent = g_enterprise AND oofh001 = g_oofg_m.oofg001 AND
                                          oofh002 = g_oofg_m.oofg005 AND
 
                      oofh003 = g_oofh_d_t.oofh003
 
                  
               #add-point:單身刪除中
               {<point name="input.body.m_delete"/>}
               #end add-point  
                  
               IF SQLCA.sqlcode THEN
                  CALL cl_err("oofh_t",SQLCA.sqlcode,1)
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               ELSE
                  LET g_rec_b = g_rec_b-1
                  INITIALIZE l_var_keys_bak TO NULL 
                  INITIALIZE l_field_keys TO NULL 
                  LET l_field_keys[01] = 'oofhl001'
                  LET l_field_keys[02] = 'oofhl002'
                  LET l_field_keys[03] = 'oofhl003'
                  LET l_field_keys[04] = 'oofhl004'
                  LET l_var_keys_bak[01] = g_detail_multi_table_t.oofhl001
                  LET l_var_keys_bak[02] = g_detail_multi_table_t.oofhl002
                  LET l_var_keys_bak[03] = g_detail_multi_table_t.oofhl003
                  LET l_var_keys_bak[04] = g_detail_multi_table_t.oofhl004
                  CALL cl_multitable_delete(l_field_keys,l_var_keys_bak,'oofhl_t')
 
                  #add-point:單身刪除後
                  {<point name="input.body.a_delete"/>}
                  #end add-point
                  CALL s_transaction_end('Y','0')
               END IF 
               CLOSE aooi390_bcl
               LET l_count = g_oofh_d.getLength()
            END IF 
            
                           INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_oofg_m.oofg001
               LET gs_keys[2] = g_oofg_m.oofg005
               LET gs_keys[3] = g_oofh_d[g_detail_idx].oofh003
 
              
         AFTER DELETE 
            #add-point:單身刪除後2
            {<point name="input.body.after_delete"/>}
            #end add-point
                           CALL aooi390_delete_b('oofh_t',gs_keys,"'1'")
 
                  #此段落由子樣板a01產生
         BEFORE FIELD oofh003
            #add-point:BEFORE FIELD oofh003
            {<point name="input.b.page1.oofh003" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD oofh003
            
            #add-point:AFTER FIELD oofh003
            {<point name="input.a.page1.oofh003" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE oofh003
            #add-point:ON CHANGE oofh003
            {<point name="input.g.page1.oofh003" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD oofhl005
            #add-point:BEFORE FIELD oofhl005
            {<point name="input.b.page1.oofhl005" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD oofhl005
            
            #add-point:AFTER FIELD oofhl005
            {<point name="input.a.page1.oofhl005" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE oofhl005
            #add-point:ON CHANGE oofhl005
            {<point name="input.g.page1.oofhl005" />}
            #END add-point
 
 
                  #Ctrlp:input.c.page1.oofh003
         ON ACTION controlp INFIELD oofh003
            #add-point:ON ACTION controlp INFIELD oofh003
            {<point name="input.c.page1.oofh003" />}
            #END add-point
 
         #Ctrlp:input.c.page1.oofhl005
         ON ACTION controlp INFIELD oofhl005
            #add-point:ON ACTION controlp INFIELD oofhl005
            {<point name="input.c.page1.oofhl005" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_oofh_d[l_ac].* = g_oofh_d_t.*
               CLOSE aooi390_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_oofh_d[l_ac].oofh003,-263,1)
               LET g_oofh_d[l_ac].* = g_oofh_d_t.*
            ELSE
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               #寫入修改者/修改日期資訊(單身)
               
    
               UPDATE oofh_t SET (oofh001,oofh002,oofh003) = (g_oofg_m.oofg001,g_oofg_m.oofg005,g_oofh_d[l_ac].oofh003) 

                WHERE oofhent = g_enterprise AND oofh001 = g_oofg_m.oofg001 
                  AND oofh002 = g_oofg_m.oofg005 
 
                  AND oofh003 = g_oofh_d_t.oofh003 #項次   
 
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
               
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("oofh_t","std-00009",1)
                     LET g_oofh_d[l_ac].* = g_oofh_d_t.*
                     CALL s_transaction_end('N','0')
                  WHEN SQLCA.sqlcode #其他錯誤
                     CALL cl_err("oofh_t",SQLCA.sqlcode,1) 
                     LET g_oofh_d[l_ac].* = g_oofh_d_t.*                     
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_oofg_m.oofg001
               LET gs_keys_bak[1] = g_oofg001_t
               LET gs_keys[2] = g_oofg_m.oofg005
               LET gs_keys_bak[2] = g_oofg005_t
               LET gs_keys[3] = g_oofh_d[g_detail_idx].oofh003
               LET gs_keys_bak[3] = g_oofh_d_t.oofh003
               CALL aooi390_update_b('oofh_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                              INITIALIZE l_var_keys TO NULL 
         INITIALIZE l_field_keys TO NULL 
         INITIALIZE l_vars TO NULL 
         INITIALIZE l_fields TO NULL 
         INITIALIZE l_var_keys_bak TO NULL 
         IF g_oofg_m.oofg001 = g_detail_multi_table_t.oofhl001 AND
         g_oofg_m.oofg005 = g_detail_multi_table_t.oofhl002 AND
         g_oofh_d[l_ac].oofh003 = g_detail_multi_table_t.oofhl003 AND
         g_oofh_d[l_ac].oofhl005 = g_detail_multi_table_t.oofhl005 THEN
         ELSE 
            LET l_var_keys[01] = g_oofg_m.oofg001
            LET l_field_keys[01] = 'oofhl001'
            LET l_var_keys[02] = g_oofg_m.oofg005
            LET l_field_keys[02] = 'oofhl002'
            LET l_var_keys[03] = g_oofh_d[l_ac].oofh003
            LET l_field_keys[03] = 'oofhl003'
            LET l_var_keys[04] = g_dlang
            LET l_field_keys[04] = 'oofhl004'
            LET l_vars[01] = g_oofh_d[l_ac].oofhl005
            LET l_fields[01] = 'oofhl005'
            LET l_vars[02] = g_enterprise 
            LET l_fields[02] = 'oofhlent'
            LET l_var_keys_bak[01] = g_detail_multi_table_t.oofhl001
            LET l_var_keys_bak[02] = g_detail_multi_table_t.oofhl002
            LET l_var_keys_bak[03] = g_detail_multi_table_t.oofhl003
            LET l_var_keys_bak[04] = g_detail_multi_table_t.oofhl004
            CALL cl_multitable(l_var_keys,l_field_keys,l_vars,l_fields,l_var_keys_bak,'oofhl_t')
         END IF 
 
                     LET g_log1 = util.JSON.stringify(g_oofg_m),util.JSON.stringify(g_oofh_d_t)
                     LET g_log2 = util.JSON.stringify(g_oofg_m),util.JSON.stringify(g_oofh_d[l_ac])
                     IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                        CALL s_transaction_end('N','0')
                     END IF
               END CASE
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            CALL aooi390_unlock_b("oofh_t","1")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock
            CALL cl_multitable_unlock()
            
         AFTER INPUT
            #add-point:input段after input 
            {<point name="input.body.after_input"/>}
            #end add-point   
              
         #add-point:page自定義行為
         {<point name="input.page.action"/>}
         #end add-point
 
         ON ACTION controlo   
            CALL FGL_SET_ARR_CURR(g_oofh_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_oofh_d.getLength()+1
            
      END INPUT
      
 
   
 
      
      BEFORE DIALOG 
         #add-point:input段before dialog
         {<point name="input.before_dialog"/>}
         #end add-point    
         #新增時強制從單頭開始填
         IF p_cmd = 'a' THEN
            NEXT FIELD oofg001
         ELSE
            CASE g_aw
               WHEN "s_detail1"
                  NEXT FIELD oofh003
 
            END CASE
         END IF
   
      ON ACTION controlf
         CALL cl_set_focus_form(ui.Interface.getRootNode()) RETURNING g_fld_name,g_frm_name
         CALL cl_fldhelp(g_frm_name,g_fld_name,g_lang)
 
      ON ACTION controlr
         CALL cl_show_req_fields()
 
      ON ACTION controls
         IF g_header_hidden THEN
            CALL gfrm_curr.setElementHidden("vb_master",0)
            CALL gfrm_curr.setElementImage("controls","small/arr-u.png")
            LET g_header_hidden = 0     #visible
         ELSE
            CALL gfrm_curr.setElementHidden("vb_master",1)
            CALL gfrm_curr.setElementImage("controls","small/arr-d.png")
            LET g_header_hidden = 1     #hidden     
         END IF
 
      ON ACTION accept
         ACCEPT DIALOG
        
      ON ACTION cancel      #在dialog button (放棄)
         LET g_action_choice=""
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION close       #在dialog 右上角 (X)
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION exit        #toolbar 離開
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      #交談指令共用ACTION
      &include "common_action.4gl" 
         CONTINUE DIALOG 
         
   END DIALOG
    
   #add-point:input段after input 
   {<point name="input.after_input"/>}
   #end add-point    
 
END FUNCTION
]]>
</section>
  <section id="aooi390.input.head" ver="4" status="" src="s">
<![CDATA[      #單頭段
      INPUT BY NAME g_oofg_m.oofg001,g_oofg_m.oofgl004,g_oofg_m.oofgl005,g_oofg_m.oofg002,g_oofg_m.oofg003, 
          g_oofg_m.oofg007,g_oofg_m.oofgstus,g_oofg_m.oofg006,g_oofg_m.oofg005,g_oofg_m.oofgl004_1,g_oofg_m.oofg008, 
