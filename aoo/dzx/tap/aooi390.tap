         RETURN
      END IF       
 
      #add-point:單身刪除後
      {<point name="delete.body.a_delete"/>}
      #end add-point      
                                                               
 
 
      CLEAR FORM
      CALL g_oofh_d.clear() 
 
     
      CALL aooi390_ui_browser_refresh()  
      CALL aooi390_ui_headershow()  
      CALL aooi390_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL aooi390_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         CALL aooi390_browser_fill()
      END IF
       
   END IF
 
   CLOSE aooi390_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_oofg_m.oofg001,'D')
    
END FUNCTION
