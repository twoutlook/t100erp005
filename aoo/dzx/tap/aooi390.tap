      DISPLAY g_detail_idx TO FORMONLY.idx  
   ELSE
      LET g_detail_idx = 0
      DISPLAY ' ' TO FORMONLY.idx    
   END IF
   
   #瀏覽頁筆數顯示
   LET g_browser_idx = g_pagestart+g_current_idx-1
   DISPLAY g_browser_idx TO FORMONLY.b_index   #當下筆數
   
   CALL cl_navigator_setting(g_current_idx, g_detail_cnt)
 
   #代表沒有資料
   IF g_current_idx = 0 THEN
      RETURN
   END IF
   
   #超出範圍
   IF g_current_idx > g_browser.getLength() THEN
      LET g_current_idx = g_browser.getLength()
   END IF
   
   LET g_oofg_m.oofg001 = g_browser[g_current_idx].b_oofg001
   LET g_oofg_m.oofg005 = g_browser[g_current_idx].b_oofg005


   
   #重讀DB,因TEMP有不被更新特性
    SELECT UNIQUE oofg001,oofg002,oofg003,oofg007,oofgstus,oofg006,oofg005,oofg008,oofg009,oofg010,oofg011,oofg012,oofg013,oofg014,oofg015,oofg016,oofg017,oofg018,oofg019,oofg020,oofg021,oofg022,oofg023,oofgownid,oofgowndp,oofgcrtid,oofgcrtdp,oofgcrtdt,oofgmodid,oofgmoddt,oofg004
 INTO g_oofg_m.oofg001,g_oofg_m.oofg002,g_oofg_m.oofg003,g_oofg_m.oofg007,g_oofg_m.oofgstus,g_oofg_m.oofg006,g_oofg_m.oofg005,g_oofg_m.oofg008,g_oofg_m.oofg009,g_oofg_m.oofg010,g_oofg_m.oofg011,g_oofg_m.oofg012,g_oofg_m.oofg013,g_oofg_m.oofg014,g_oofg_m.oofg015,g_oofg_m.oofg016,g_oofg_m.oofg017,g_oofg_m.oofg018,g_oofg_m.oofg019,g_oofg_m.oofg020,g_oofg_m.oofg021,g_oofg_m.oofg022,g_oofg_m.oofg023,g_oofg_m.oofgownid,g_oofg_m.oofgowndp,g_oofg_m.oofgcrtid,g_oofg_m.oofgcrtdp,g_oofg_m.oofgcrtdt,g_oofg_m.oofgmodid,g_oofg_m.oofgmoddt,g_oofg_m.oofg004
