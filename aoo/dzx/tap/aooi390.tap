  <point name="reproduce.define" cite_std="N" status="" ver="1" src="s" new="N" mark_hard="N">
<![CDATA[   DEFINE l_sql       STRING

   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF
   IF g_oofg_m.oofg001 IS NULL OR g_oofg_m.oofg005 IS NULL THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF

   LET g_oofg001_t = g_oofg_m.oofg001
   LET g_oofg005_t = g_oofg_m.oofg005

   CALL cl_set_comp_entry("oofg001",TRUE)
   CALL cl_set_head_visible("","YES")


   INPUT l_newno FROM oofg001 ATTRIBUTE(FIELD ORDER FORM)

      BEFORE INPUT
      AFTER FIELD oofg001
         IF l_newno IS NULL THEN
            NEXT FIELD CURRENT
         END IF
         IF NOT cl_null(l_newno) THEN
            IF NOT ap_chk_notDup(l_newno,"SELECT COUNT(*) FROM oofg_t WHERE "||"oofgent = '" ||g_enterprise|| "' AND "||"oofg001 = '"||l_newno ||"' AND "|| "oofg005 = '"||g_oofg_m.oofg005 ||"'",'std-00004',0) THEN
               LET l_newno = ''
               NEXT FIELD CURRENT
            END IF
         END IF

      AFTER INPUT
         #若取消則直接結束
         IF INT_FLAG = 1 THEN
            LET INT_FLAG = 0
            RETURN
         END IF

      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE INPUT
   END INPUT
   IF INT_FLAG OR l_newno IS NULL THEN
      LET INT_FLAG = 0
      RETURN
   END IF
   CALL s_transaction_begin()
   LET l_sql = " SELECT * FROM oofg_t ",
                " WHERE oofgent = '",g_enterprise,"'",
                "   AND oofg001 = '",g_oofg_m.oofg001,"'"
   PREPARE aooi390_sel_oofg_pr FROM l_sql
   DECLARE aooi390_sel_oofg_cs CURSOR FOR aooi390_sel_oofg_pr
   FOREACH aooi390_sel_oofg_cs INTO l_master.*

      LET l_master.oofg001 = l_newno
      #公用欄位給予預設值
      #此段落由子樣板a14產生    
      LET l_master.oofgownid = g_user
      LET l_master.oofgowndp = g_dept
      LET l_master.oofgcrtid = g_user
      LET l_master.oofgcrtdp = g_dept
      LET l_master.oofgcrtdt = cl_get_current()
      LET l_master.oofgmodid = ""
      LET l_master.oofgmoddt = ""
      LET l_master.oofgstus = 'Y'

      INSERT INTO oofg_t VALUES (l_master.*)
         IF SQLCA.sqlcode THEN
             CALL cl_err("oofg_t",SQLCA.sqlcode,1)
             CALL s_transaction_end('N','0')
             RETURN
         END IF
      INSERT INTO oofgl_t(oofglent,oofgl001,oofgl002,oofgl003,oofgl004,oofgl005)
      SELECT oofglent,l_newno,oofgl002,oofgl003,oofgl004,oofgl005
        FROM oofgl_t
       WHERE oofglent = g_enterprise
         AND oofgl001 = g_oofg_m.oofg001
         AND oofgl002 = l_master.oofg005
         IF SQLCA.sqlcode THEN
             CALL cl_err("oofgl_t",SQLCA.sqlcode,1)
             CALL s_transaction_end('N','0')
             RETURN
         END IF

      INSERT INTO oofh_t(oofhent,oofh001,oofh002,oofh003)
      SELECT oofhent,l_newno,oofh002,oofh003
        FROM oofh_t
       WHERE oofhent = g_enterprise
         AND oofh001 = g_oofg_m.oofg001
         AND oofh002 = l_master.oofg005
         IF SQLCA.sqlcode THEN
             CALL cl_err("oofh_t",SQLCA.sqlcode,1)
             CALL s_transaction_end('N','0')
             RETURN
         END IF
      INSERT INTO oofhl_t(oofhlent,oofhl001,oofhl002,oofhl003,oofhl004,oofhl005,oofhl006)
      SELECT oofhlent,l_newno,oofhl002,oofhl003,oofhl004,oofhl005,oofhl006
        FROM oofhl_t
       WHERE oofhlent = g_enterprise
         AND oofhl001 = g_oofg_m.oofg001
         AND oofhl002 = l_master.oofg005
         IF SQLCA.sqlcode THEN
             CALL cl_err("oofhl_t",SQLCA.sqlcode,1)
             CALL s_transaction_end('N','0')
             RETURN
         END IF
   END FOREACH
   INSERT INTO oofgl_t(oofglent,oofgl001,oofgl002,oofgl003,oofgl004,oofgl005)
   SELECT oofglent,l_newno,' ',oofgl003,oofgl004,oofgl005
     FROM oofgl_t
    WHERE oofglent = g_enterprise
      AND oofgl001 = g_oofg_m.oofg001
      AND oofgl002 = ' '
      IF SQLCA.sqlcode THEN
          CALL cl_err("oofgl_t",SQLCA.sqlcode,1)
          CALL s_transaction_end('N','0')
          RETURN
      END IF
   CALL s_transaction_end('Y','0')
   ERROR 'ROW(',l_newno,') O.K'

   CLOSE aooi390_sel_oofg_cs

   LET g_wc = g_wc," OR ( oofg001 = '", l_newno CLIPPED, "' ", ") "
   RETURN]]>
</point>
