   OPEN aooi390_cl USING g_enterprise,g_oofg_m.oofg001
                                                       ,g_oofg_m.oofg005
 
   IF STATUS THEN
      CALL cl_err("OPEN aooi390_cl:", STATUS, 1)
      CLOSE aooi390_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   FETCH aooi390_cl INTO g_oofg_m.oofg001,g_oofg_m.oofgl004,g_oofg_m.oofgl005,g_oofg_m.oofg002,g_oofg_m.oofg003, 
       g_oofg_m.oofg007,g_oofg_m.oofgstus,g_oofg_m.oofg006,g_oofg_m.oofg006_desc,g_oofg_m.oofg005,g_oofg_m.oofgl004_1, 
       g_oofg_m.oofg008,g_oofg_m.oofg009,g_oofg_m.oofg010,g_oofg_m.oofg011,g_oofg_m.oofg012,g_oofg_m.oofg016, 
       g_oofg_m.oofg017,g_oofg_m.oofg017_desc,g_oofg_m.oofg018,g_oofg_m.oofg019,g_oofg_m.oofg020,g_oofg_m.oofg021, 
       g_oofg_m.oofg022,g_oofg_m.oofg022_desc,g_oofg_m.oofg023,g_oofg_m.oofg023_desc,g_oofg_m.oofgownid, 
       g_oofg_m.oofgownid_desc,g_oofg_m.oofgowndp,g_oofg_m.oofgowndp_desc,g_oofg_m.oofgcrtid,g_oofg_m.oofgcrtid_desc, 
       g_oofg_m.oofgcrtdp,g_oofg_m.oofgcrtdp_desc,g_oofg_m.oofgcrtdt,g_oofg_m.oofgmodid,g_oofg_m.oofgmodid_desc, 
       g_oofg_m.oofgmoddt,g_oofg_m.column,g_oofg_m.table,g_oofg_m.condition,g_oofg_m.oofg013,g_oofg_m.oofg014, 
       g_oofg_m.oofg015,g_oofg_m.oofg009_sum,g_oofg_m.oofg009_1,g_oofg_m.oofg009_after,g_oofg_m.oofg004  
