      CALL cl_err(g_oofg_m.oofg001,SQLCA.sqlcode,0)          #資料被他人LOCK
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL aooi390_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
 
  
      #資料備份
      LET g_oofg001_t = g_oofg_m.oofg001
      LET g_oofg005_t = g_oofg_m.oofg005
 
      
      #add-point:單頭刪除前
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM oofg_t
       WHERE oofgent = g_enterprise AND oofg001 = g_oofg_m.oofg001
         AND oofg005 = g_oofg_m.oofg005
 
      
      #add-point:單頭刪除中
      {<point name="delete.head.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err(g_oofg_m.oofg001,SQLCA.sqlcode,0) 
         CALL s_transaction_end('N','0')
         RETURN
      END IF
      
      #add-point:單頭刪除後
      {<point name="delete.head.a_delete"/>}
      #end add-point
  
      
      INITIALIZE l_var_keys_bak TO NULL 
   INITIALIZE l_field_keys   TO NULL 
   LET l_var_keys_bak[01] = g_master_multi_table_t.oofgl001_idx2
   LET l_field_keys[01] = 'oofgl001'
   LET l_var_keys_bak[02] = g_master_multi_table_t.oofgl002_idx2
   LET l_field_keys[02] = 'oofgl002'
   LET l_var_keys_bak[03] = g_dlang
   LET l_field_keys[03] = 'oofgl003'
   CALL cl_multitable_delete(l_field_keys,l_var_keys_bak,'oofgl_t')
 
      
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM oofh_t
       WHERE oofhent = g_enterprise AND oofh001 = g_oofg_m.oofg001
         AND oofh002 = g_oofg_m.oofg005
 
         
      #add-point:單身刪除中
