      #更新key欄位
      SET oofh001 = g_oofg_m.oofg001
          , oofh002 = g_oofg_m.oofg005
 
      #更新共用欄位
      
                                       
  
   #將資料塞回原table   
   INSERT INTO oofh_t SELECT * FROM aooi390_detail
   
   IF SQLCA.sqlcode THEN
      CALL cl_err("reproduce",SQLCA.sqlcode,1)
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
