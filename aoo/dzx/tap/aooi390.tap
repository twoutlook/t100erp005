            LET g_oofg_m.oofg016 = "N"
      LET g_oofg_m.oofg018 = "N"
      LET g_oofg_m.oofg021 = "N"
 
  
      #add-point:單頭預設值
      {<point name="insert.default"/>}
      #end add-point 
     
      CALL aooi390_input("a")
      
      #add-point:單頭輸入後
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_oofg_m.* = g_oofg_m_t.*
