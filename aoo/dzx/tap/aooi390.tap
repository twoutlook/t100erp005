         FOR l_n = 1 TO l_oofg.oofg009
             LET l_oofg009 = l_oofg009,'M'
         END FOR
         LET l_oofg004 = l_oofg004,l_oofg009
      END IF
      IF l_oofg.oofg008 = '6' THEN
         LET l_oofg009 = ""
         FOR l_n = 1 TO l_oofg.oofg009
             LET l_oofg009 = l_oofg009,'P'
         END FOR
         LET l_oofg004 = l_oofg004,l_oofg009
      END IF
      IF l_oofg.oofg008 = '7' THEN
         LET l_oofg009 = ""
         FOR l_n = 1 TO l_oofg.oofg009
             LET l_oofg009 = l_oofg009,'W'
         END FOR
         LET l_oofg004 = l_oofg004,l_oofg009
      END IF
      IF l_oofg.oofg008 = '8' THEN
         LET l_oofg009 = ""
         FOR l_n = 1 TO l_oofg.oofg009
             LET l_oofg009 = l_oofg009,'D'
         END FOR
         LET l_oofg004 = l_oofg004,l_oofg009
      END IF
      IF l_oofg.oofg008 = '9' THEN
         LET l_oofg009 = ""
         FOR l_n = 1 TO l_oofg.oofg009
             LET l_oofg009 = l_oofg009,'*'
         END FOR
         LET l_oofg004 = l_oofg004,l_oofg009
      END IF
      LET l_oofg.oofg004 = l_oofg004
      UPDATE oofg_t SET oofg004 = l_oofg.oofg004
       WHERE oofgent = g_enterprise
         AND oofg001 = g_oofg_m.oofg001
         AND oofg005 = l_oofg.oofg005
      IF SQLCA.sqlcode THEN
         CALL cl_err('Update error!',SQLCA.sqlcode,1)
         CALL s_transaction_end('N','0')
         RETURN
      END IF
   END FOREACH
   CALL s_transaction_end('Y','0')

END FUNCTION]]>
