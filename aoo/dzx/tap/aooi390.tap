  <point name="function.aooi390_chk_legth" cite_std="N" status="" ver="1" src="s" new="Y" order="18" mark_hard="N">
<![CDATA[################################################################################
# Descriptions...: 總編碼長度檢查
# Memo...........:
# Usage..........: CALL aooi390_chk_legth()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2013/12/17 By xumm
# Modify.........:
################################################################################
PRIVATE FUNCTION aooi390_chk_legth()
DEFINE l_oofg009        LIKE oofg_t.oofg009
DEFINE l_oofg005        LIKE oofg_t.oofg005
DEFINE l_level          LIKE type_t.num10
DEFINE l_sum            LIKE type_t.num5
DEFINE l_sql            STRING

   LET g_errno = ""
   IF NOT cl_null(g_oofg_m.oofg003) AND NOT cl_null(g_oofg_m.oofg009) THEN 
      LET l_oofg005 = ''
      LET l_oofg009 = 0
      LET l_sum = 0    
      LET l_sql = " WITH TMP AS (SELECT LEVEL,oofg005,oofg006,oofg009 FROM (SELECT oofg001,oofg005,oofg006,oofg009 ",
                  "                                FROM oofg_t ",
                  "                                WHERE oofgent = '",g_enterprise,"' ",
                  "                                  AND oofg001 = '",g_oofg_m.oofg001,"' ",
                  "                                  AND oofg005 <> '",g_oofg_m.oofg001,"' ",
                  "                                  AND oofg005 <> oofg006 ) ",
                  "   START WITH oofg006= '",g_oofg_m.oofg005,"' ",
                  " CONNECT BY NOCYCLE PRIOR oofg005 = oofg006 )",
                  "  SELECT b.oofg005,(SELECT sum(a.oofg009) FROM TMP a START WITH a.oofg005 = b.oofg005 CONNECT BY NOCYCLE PRIOR a.oofg006 = a.oofg005),CONNECT_BY_ISLEAF ",
                  "    FROM TMP b ",
                  "   WHERE CONNECT_BY_ISLEAF = 1 ",
                  "   START WITH b.oofg006 = '",g_oofg_m.oofg005,"'",
                  " CONNECT BY NOCYCLE PRIOR oofg005 = oofg006 "
      PREPARE aooi390_sel_oofg005oofg009_pr FROM l_sql
      DECLARE aooi390_sel_oofg005oofg009_cs CURSOR FOR aooi390_sel_oofg005oofg009_pr
      FOREACH aooi390_sel_oofg005oofg009_cs INTO l_oofg005,l_oofg009,l_level
         LET l_sum = l_oofg009 + g_oofg_m.oofg009_sum + g_oofg_m.oofg009
         IF l_sum > g_oofg_m.oofg003 THEN
            LET g_errno = 'aoo-00264'
            RETURN
         END IF
      END FOREACH
      LET l_sum = g_oofg_m.oofg009_sum + g_oofg_m.oofg009
      IF l_sum > g_oofg_m.oofg003 THEN
         LET g_errno = 'aoo-00264'
         RETURN
      END IF
#      CALL aooi390_chk_legth_for(1,l_i,l_lev)
#      FOR i = 1 TO l_i - 1 
#         LET l_sum = l_sum + l_lev[l_i].oofg009
#         IF i <> 1 AND l_lev[l_i].level = 1 THEN
#            LET l_sum = g_oofg_m.oofg009_sum + l_sum + g_oofg_m.oofg009
#            IF l_sum > g_oofg_m.oofg003 THEN
#               LET g_errno = 'aoo-00264'
#               RETURN
#            END IF
#            CALL 
#         END IF
#      END FOR
#      LET l_sum = g_oofg_m.oofg009_sum + l_sum + g_oofg_m.oofg009
#      IF l_sum > g_oofg_m.oofg003 THEN
#         LET g_errno = 'aoo-00264'
#      END IF
   END IF
END FUNCTION]]>
