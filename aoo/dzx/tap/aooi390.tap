  <section id="aooi390.match_node" ver="4" status="" src="s">
<![CDATA[PRIVATE FUNCTION aooi390_match_node(p_wc,p_type)
   DEFINE p_wc         LIKE type_t.chr200
   DEFINE p_type       LIKE type_t.chr10
   DEFINE ls_code      LIKE type_t.chr50
   DEFINE ls_code2     LIKE type_t.chr50
   DEFINE l_bstmp      RECORD    #body欄位   
             oofg001 VARCHAR(500),
   oofg005 VARCHAR(500),
   oofg006 VARCHAR(500)
          #僅含單身table的欄位
   END RECORD 
   DEFINE l_child_list DYNAMIC ARRAY OF RECORD    #body欄位   
             oofg001 VARCHAR(500),
   oofg005 VARCHAR(500),
   oofg006 VARCHAR(500)
          #僅含單身table的欄位
   END RECORD 
   #add-point:match_node段define
   {<point name="match_node.define"/>}
   #end add-point
   
   #先找出符合條件的節點並給予展開值
   CASE p_type
      WHEN 1
         LET ls_code = "0"
      WHEN 2
         LET ls_code = "2"
      WHEN 3
         LET ls_code = "2"
   END CASE
   
   IF cl_null('oofg006') THEN
      LET ls_code = '0'
   END IF 
   
   CALL s_transaction_begin()
 
   LET g_sql = " INSERT INTO aooi390_tmp (oofg001,oofg005,oofg006,exp_code) VALUES (?,?,?,?)"
   PREPARE master_tmp FROM g_sql
   
   IF g_root_search THEN
      #DECLARE master_tmp_cur CURSOR FOR master_tmp
      #OPEN master_tmp_cur
      FOREACH master_extcur INTO l_bstmp.*
         EXECUTE master_tmp USING l_bstmp.*,ls_code
         #PUT master_tmp_cur FROM l_bstmp.*,ls_code
      END FOREACH
      #FLUSH master_tmp_cur
      CALL s_transaction_end('Y','0')
      RETURN
   END IF
 
   FOREACH master_extcur INTO l_bstmp.*
      EXECUTE master_tmp USING l_bstmp.*,ls_code
      LET l_child_list[l_child_list.getLength()+1].* = l_bstmp.*
       
      #找出符合條件的節點的所有祖先並給予展開值
      CASE p_type
         WHEN 1
            LET ls_code2 = "1"
         WHEN 2
            LET ls_code2 = "-1"
         WHEN 3
            LET ls_code2 = "1"
      END CASE
      
      #若pid欄位存在才進行後續處理
      #擷取該節點的父節點到temp table中
      LET g_sql = " SELECT oofg001,oofg005,oofg006 ",
                  " FROM oofg_t ",
                  " WHERE oofgent = '" ||g_enterprise|| "' AND oofg005 = ? ",
                  " AND oofg001 = ? ",
                  cl_sql_add_filter("oofg_t")
      PREPARE master_getparent_up FROM g_sql
      
      #擷取該節點的所有父節點
      WHILE TRUE
         IF cl_null(l_child_list[1].oofg005) THEN
            IF l_child_list.getLength() = 1 THEN
               EXIT WHILE
            ELSE
               CALL l_child_list.deleteElement(1)
               CONTINUE WHILE
            END IF
         END IF
      
         EXECUTE master_getparent_up USING l_child_list[1].oofg006
                                           ,l_child_list[1].oofg001
                                           INTO l_bstmp.*
         FREE master_getparent_up
      
      #確定該節點是否存在於temp table中
         
         IF STATUS = 0 AND aooi390_tmp_tbl_chk(l_bstmp.oofg005,ls_code2
                     ,l_bstmp.oofg001
                     ) THEN
            EXECUTE master_tmp USING l_bstmp.*,ls_code2
            LET l_child_list[l_child_list.getLength()+1].* = l_bstmp.*
         END IF
         CALL l_child_list.deleteElement(1)
      
      END WHILE
   
   END FOREACH  
   
   FREE master_tmp
   
   CALL s_transaction_end('Y','0')
 
END FUNCTION
]]>
</section>
  <section id="aooi390.modify" ver="5" status="" src="s">
<![CDATA[#+ 資料修改
PRIVATE FUNCTION aooi390_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_oofg_m.oofg001 IS NULL
   OR g_oofg_m.oofg005 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE aooi390_master_referesh USING g_oofg_m.oofg001,g_oofg_m.oofg005 INTO g_oofg_m.oofg001,g_oofg_m.oofg002, 
       g_oofg_m.oofg003,g_oofg_m.oofg007,g_oofg_m.oofgstus,g_oofg_m.oofg006,g_oofg_m.oofg005,g_oofg_m.oofg008, 
       g_oofg_m.oofg009,g_oofg_m.oofg010,g_oofg_m.oofg011,g_oofg_m.oofg012,g_oofg_m.oofg016,g_oofg_m.oofg017, 
       g_oofg_m.oofg018,g_oofg_m.oofg019,g_oofg_m.oofg020,g_oofg_m.oofg021,g_oofg_m.oofg022,g_oofg_m.oofg023, 
       g_oofg_m.oofgownid,g_oofg_m.oofgowndp,g_oofg_m.oofgcrtid,g_oofg_m.oofgcrtdp,g_oofg_m.oofgcrtdt, 
       g_oofg_m.oofgmodid,g_oofg_m.oofgmoddt,g_oofg_m.oofg013,g_oofg_m.oofg014,g_oofg_m.oofg015,g_oofg_m.oofg004, 
       g_oofg_m.oofg006_desc,g_oofg_m.oofg017_desc,g_oofg_m.oofg022_desc,g_oofg_m.oofg023_desc,g_oofg_m.oofgownid_desc, 
       g_oofg_m.oofgowndp_desc,g_oofg_m.oofgcrtid_desc,g_oofg_m.oofgcrtdp_desc,g_oofg_m.oofgmodid_desc 

 
   ERROR ""
  
   LET g_oofg001_t = g_oofg_m.oofg001
   LET g_oofg005_t = g_oofg_m.oofg005
 
   CALL s_transaction_begin()
   
   OPEN aooi390_cl USING g_enterprise,g_oofg_m.oofg001
                                                       ,g_oofg_m.oofg005
 
   IF STATUS THEN
      CALL cl_err("OPEN aooi390_cl:", STATUS, 1)
      CLOSE aooi390_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aooi390_cl INTO g_oofg_m.oofg001,g_oofg_m.oofgl004,g_oofg_m.oofgl005,g_oofg_m.oofg002,g_oofg_m.oofg003, 
       g_oofg_m.oofg007,g_oofg_m.oofgstus,g_oofg_m.oofg006,g_oofg_m.oofg006_desc,g_oofg_m.oofg005,g_oofg_m.oofgl004_1, 
       g_oofg_m.oofg008,g_oofg_m.oofg009,g_oofg_m.oofg010,g_oofg_m.oofg011,g_oofg_m.oofg012,g_oofg_m.oofg016, 
       g_oofg_m.oofg017,g_oofg_m.oofg017_desc,g_oofg_m.oofg018,g_oofg_m.oofg019,g_oofg_m.oofg020,g_oofg_m.oofg021, 
       g_oofg_m.oofg022,g_oofg_m.oofg022_desc,g_oofg_m.oofg023,g_oofg_m.oofg023_desc,g_oofg_m.oofgownid, 
       g_oofg_m.oofgownid_desc,g_oofg_m.oofgowndp,g_oofg_m.oofgowndp_desc,g_oofg_m.oofgcrtid,g_oofg_m.oofgcrtid_desc, 
       g_oofg_m.oofgcrtdp,g_oofg_m.oofgcrtdp_desc,g_oofg_m.oofgcrtdt,g_oofg_m.oofgmodid,g_oofg_m.oofgmodid_desc, 
       g_oofg_m.oofgmoddt,g_oofg_m.column,g_oofg_m.table,g_oofg_m.condition,g_oofg_m.oofg013,g_oofg_m.oofg014, 
       g_oofg_m.oofg015,g_oofg_m.oofg009_sum,g_oofg_m.oofg009_1,g_oofg_m.oofg009_after,g_oofg_m.oofg004 

 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_oofg_m.oofg001,SQLCA.sqlcode,0)
      CLOSE aooi390_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
 
   CALL aooi390_show()
   WHILE TRUE
      LET g_oofg001_t = g_oofg_m.oofg001
      LET g_oofg005_t = g_oofg_m.oofg005
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_oofg_m.oofgmodid = g_user 
LET g_oofg_m.oofgmoddt = cl_get_current()
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL aooi390_input("u")     #欄位更改
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_oofg_m.* = g_oofg_m_t.*
         CALL aooi390_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若有modid跟moddt則進行update
      UPDATE oofg_t SET (oofgmodid,oofgmoddt) = (g_oofg_m.oofgmodid,g_oofg_m.oofgmoddt)
       WHERE oofgent = g_enterprise AND oofg001 = g_oofg001_t
         AND oofg005 = g_oofg005_t
 
      
      #若單頭key欄位有變更
      IF g_oofg_m.oofg001 != g_oofg001_t 
      OR g_oofg_m.oofg005 != g_oofg005_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE oofh_t SET oofh001 = g_oofg_m.oofg001
                                      ,oofh002 = g_oofg_m.oofg005
 
          WHERE oofhent = g_enterprise AND oofh001 = g_oofg001_t
            AND oofh002 = g_oofg005_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("oofh_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("oofh_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         IF SQLCA.sqlcode THEN
             CALL cl_err("",SQLCA.sqlcode,1) 
             CALL s_transaction_end('N','0')
             CONTINUE WHILE
         END IF
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
      
      EXIT WHILE
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_oofg_m.oofg001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE aooi390_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_oofg_m.oofg001,'U')
 
END FUNCTION   
]]>
</section>
