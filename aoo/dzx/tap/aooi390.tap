   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE aooi390_detail_lang AS ",
                "SELECT * FROM oofhl_t "
   PREPARE repro_oofhl_t FROM ls_sql
   EXECUTE repro_oofhl_t
   FREE repro_oofhl_t
                
   #將符合條件的資料丟入TEMP TABLE
