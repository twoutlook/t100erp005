  <section id="aooi390.b_fill" ver="4" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aooi390_b_fill()
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_oofh_d.clear()    #g_oofh_d 單頭及單身 
 
 
   #add-point:b_fill段define
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE oofh003  FROM oofh_t",   
               " INNER JOIN oofg_t ON oofg001 = oofh001 ",
               " AND oofg005 = oofh002 ",
 
               " LEFT JOIN oofhl_t ON oofg001 = oofhl001 AND oofg005 = oofhl002 AND oofh003 = oofhl003 AND oofhl004 = '",g_lang,"'",
               
               " WHERE oofhent=? AND oofh001=? AND oofh002=?"
 
   IF NOT cl_null(g_wc_table1) THEN
      LET g_sql = g_sql CLIPPED, " AND ", g_wc_table1 CLIPPED
   END IF
 
   LET g_sql = g_sql, " ORDER BY oofh003"
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料   
   PREPARE aooi390_pb FROM g_sql
   DECLARE b_fill_cs CURSOR FOR aooi390_pb
 
   LET g_cnt = l_ac
   LET l_ac = 1
 
   OPEN b_fill_cs USING g_enterprise,g_oofg_m.oofg001,g_oofg_m.oofg005
                                            
   FOREACH b_fill_cs INTO g_oofh_d[l_ac].oofh003
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
     
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
 
   
   CALL g_oofh_d.deleteElement(g_oofh_d.getLength())
 
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE aooi390_pb
 
   
   
END FUNCTION
]]>
</section>
  <section id="aooi390.browser_create" ver="3" status="" src="s">
<![CDATA[PRIVATE FUNCTION aooi390_browser_create(p_type)
   DEFINE p_type   LIKE type_t.chr50
   DEFINE l_pid    LIKE type_t.chr50
   #add-point:browser_create
   {<point name="browser_create.define"/>}
   #end add-point
   
   #先找出所有的帳別資料
   LET g_sql = " SELECT UNIQUE oofg001 FROM aooi390_tmp ORDER BY oofg001"
   PREPARE master_type FROM g_sql
   DECLARE master_typecur CURSOR FOR master_type
   
   INITIALIZE g_browser_root TO NULL
   
   LET l_ac = 1
   FOREACH master_typecur INTO g_browser[l_ac].b_oofg001
      #確定root節點所在
      LET g_browser_root[g_browser_root.getLength()+1] = l_ac
      #此處為帳別部分(LV-1)
      LET g_browser[l_ac].b_oofg005  = g_browser[l_ac].b_oofg001
      LET g_browser[l_ac].b_oofg001 = g_browser[l_ac].b_oofg001
      LET g_browser[l_ac].b_pid = '0' CLIPPED
      LET g_browser[l_ac].b_id = l_ac USING "<<<"
      LET g_browser[l_ac].b_exp = TRUE
      LET g_browser[l_ac].b_hasC = TRUE
      LET l_pid = g_browser[l_ac].b_id CLIPPED
      LET l_ac = l_ac + 1
      
      #抓出LV2的所有資料
      LET g_sql = " SELECT UNIQUE oofg001,oofg005,oofg006,exp_code FROM aooi390_tmp a ",
                  
                  " WHERE ",
                  "a.oofg001 = ? ",
                  " AND ",
                  " (( SELECT COUNT(*) FROM aooi390_tmp b WHERE a.oofg006 = b.oofg005 ", 
                  " AND a.oofg001 = b.oofg001",
                  ") = 0 OR ", 
                  " a.oofg005 = a.oofg006 )", 
                  " ORDER BY a.oofg005"
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料   
      PREPARE master_getLV2 FROM g_sql
      DECLARE master_getLV2cur CURSOR FOR master_getLV2
      
      #以下為一般資料root(LV-2)
      OPEN master_getLV2cur USING g_browser[l_ac-1].b_oofg001
      
      LET g_cnt = l_ac
      
      FOREACH master_getLV2cur INTO g_browser[g_cnt].b_oofg001,g_browser[g_cnt].b_oofg005,g_browser[g_cnt].b_oofg006, 
          g_browser[g_cnt].b_expcode 
         #去除多餘空白
         #LET g_browser[g_cnt].b_oofg005 = g_browser[g_cnt].b_oofg005 CLIPPED
         LET g_browser[g_cnt].b_pid = l_pid
         LET g_browser[g_cnt].b_id = l_pid,".",g_cnt USING "<<<"
         LET g_browser[g_cnt].b_exp = FALSE
         #CASE g_browser[g_cnt].b_expcode
         #   WHEN 2
         #      LET g_browser[g_cnt].b_hasC = aooi390_chk_hasC(g_cnt)
         #   WHEN 1
         #      LET g_browser[g_cnt].b_hasC = aooi390_chk_hasC(g_cnt)
         #   WHEN 0
         #      LET g_browser[g_cnt].b_hasC = FALSE
         #   WHEN -1
         #      #向下查找到展開值不等於-1得節點(temp table中查找)
         #      LET g_cnt = aooi390_find_node(g_cnt)
         #END CASE
         LET g_browser[g_cnt].b_hasC = TRUE
 
         LET g_cnt = g_cnt + 1   
         
         IF g_cnt > g_max_rec AND g_error_show = 1 THEN
            CALL cl_err('',9035,0)
            EXIT FOREACH
         END IF
      END FOREACH
      LET l_ac = g_browser.getLength()
      
      IF g_cnt > g_max_rec AND g_error_show = 1 THEN
         EXIT FOREACH
      END IF
   END FOREACH
   
   LET g_error_show = 0
   
   #組合描述欄位&刪除多於資料
   FOR l_ac = 1 TO g_browser.getLength()
      IF cl_null(g_browser[l_ac].b_oofg005) THEN
         CALL g_browser.deleteElement(l_ac)
         LET l_ac = l_ac - 1
      ELSE
         CALL aooi390_desc_show(l_ac)
      END IF
   END FOR
   CALL g_browser.deleteElement(l_ac)
   
   LET g_browser_cnt = g_browser.getLength() - g_browser_root.getLength()
   
   FREE master_getLV2
   
END FUNCTION
]]>
</section>
  <section id="aooi390.browser_expand" ver="3" status="" src="s">
<![CDATA[#+ Tree子節點展開
PRIVATE FUNCTION aooi390_browser_expand(p_id)
   DEFINE p_id          LIKE type_t.num10
   DEFINE l_id          LIKE type_t.num10
   DEFINE l_cnt         LIKE type_t.num10
   DEFINE l_keyvalue    LIKE type_t.chr50
   DEFINE l_typevalue   LIKE type_t.chr50
   DEFINE l_type        LIKE type_t.chr50
   DEFINE l_sql         LIKE type_t.chr500
   DEFINE ls_source     LIKE type_t.chr500
   DEFINE ls_exp_code   LIKE type_t.chr500
   DEFINE l_return      LIKE type_t.num5
   #add-point:browser_expand段define
   {<point name="browser_expand.define"/>}
   #end add-point
   
   #若已經展開
   IF g_browser[p_id].b_isExp = 1 THEN
      RETURN
   END IF
   
   LET l_return = FALSE
 
   LET l_keyvalue = g_browser[p_id].b_oofg005
   LET l_typevalue = g_browser[p_id].b_oofg001
   
   CASE g_browser[p_id].b_expcode
      WHEN -1
         CALL g_browser.deleteElement(p_id)
      WHEN 0
         RETURN
      WHEN 1
         LET ls_source = "aooi390_tmp"
         LET ls_exp_code = "exp_code"
      WHEN 2
         LET ls_source = "oofg_t"
         LET ls_exp_code = "'2'"
   END CASE
   
   LET l_sql = " SELECT UNIQUE '','','','FALSE','','',",ls_exp_code,",oofg001,oofg005,oofg006",
               " FROM   ",ls_source,
               
               " WHERE  oofg006 = '", l_keyvalue,
               "' AND   oofg005 <> oofg006",
               " AND  oofg001 = '", l_typevalue,"'", 
               " ORDER BY oofg005"
 
   PREPARE tree_expand FROM l_sql
   DECLARE tree_ex_cur CURSOR FOR tree_expand
  
   LET l_id = p_id + 1
   LET g_cnt = l_id
   CALL g_browser.insertElement(l_id)
   LET l_cnt = 1
   FOREACH tree_ex_cur INTO g_browser[l_id].* 
      #pid=父節點id
      LET g_browser[l_id].b_pid  = g_browser[p_id].b_id
      #id=本身節點id(採流水號遞增)
      LET g_browser[l_id].b_id   = g_browser[p_id].b_id||"."||l_cnt
      #hasC=確認該節點是否有子孫
      #LET g_browser[l_id].b_oofg005 = g_browser[l_id].b_oofg005 CLIPPED
      CALL aooi390_desc_show(l_id)
      LET g_browser[l_id].b_hasC = aooi390_chk_hasC(l_id)
      LET l_id = l_id + 1
      LET g_cnt = l_id
      CALL g_browser.insertElement(l_id)
      LET l_cnt = l_cnt + 1
      
      LET l_return = TRUE
   END FOREACH
   
   #刪除空資料
   CALL g_browser.deleteElement(l_id)
   
   FREE tree_expand
 
END FUNCTION
]]>
</section>
  <section id="aooi390.browser_fill" ver="4" status="" src="s">
<![CDATA[#+ 瀏覽頁簽where條件組成
PRIVATE FUNCTION aooi390_browser_fill()
   DEFINE l_cnt      LIKE type_t.num10
   DEFINE l_cnt2     LIKE type_t.num10
   #add-point:browser_fill段define
   {<point name="browser_fill.define"/>}
   #end add-point
 
   CALL g_browser.clear()
   CLEAR FORM
   
   DROP TABLE aooi390_tmp
   
   #Create temp table
   CREATE TEMP TABLE aooi390_tmp
   (
         oofg001 VARCHAR(500),
   oofg005 VARCHAR(500),
   oofg006 VARCHAR(500),
      #僅含browser的欄位
      exp_code  VARCHAR(5)
   );
 
   LET g_root_search = FALSE
   IF (g_wc.trim() = "1=1"  OR cl_null(g_wc))  AND 
      (g_wc2.trim() = "1=1" OR cl_null(g_wc2)) THEN
      #未輸入條件時則只查找root節點
      LET g_wc = " oofg005 = oofg006 "
      LET g_root_search = TRUE
   END IF
 
   IF cl_null(g_wc2) OR g_wc2.trim() = "1=1" THEN
      LET g_sql = "SELECT oofg001,oofg005,oofg006 ",
                  " FROM oofg_t ",
                  " WHERE oofgent = '" ||g_enterprise|| "' AND ",g_wc
   ELSE
      LET g_sql = "SELECT oofg001,oofg005,oofg006 ",
                  " FROM oofg_t ",
                  " INNER JOIN oofh_t ",
                  " ON oofh001 = oofg001 ",
 
                  " WHERE oofgent = '" ||g_enterprise|| "' AND ", g_wc, " AND ", g_wc2
   END IF
   LET g_sql = g_sql ,cl_sql_add_filter("oofg_t")
  
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料   
   PREPARE master_ext FROM g_sql
   DECLARE master_extcur CURSOR FOR master_ext
 
   #搜尋建構樹所需的節點
   CASE g_searchtype
      WHEN "1" #上推
         CALL aooi390_match_node(g_wc,g_searchtype) 
      WHEN "2" #下展
         #CALL aooi390_find_speed_tbl(g_wc,g_searchtype) 
         CALL aooi390_match_node(g_wc,g_searchtype) 
      WHEN "3" #全部
         CALL aooi390_match_node(g_wc,g_searchtype) 
   END CASE
 
   CALL aooi390_browser_create(g_searchtype)
   
   FREE master_ext
     
   #若無資料則關閉相關功能
   IF g_browser_cnt = 0 THEN
      CALL cl_set_act_visible("statechange,modify,delete,reproduce", FALSE)
   ELSE
      CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   END IF
     
END FUNCTION
]]>
</section>
  <section id="aooi390.browser_search" ver="3" status="" src="s">
<![CDATA[#+ 瀏覽頁簽資料搜尋
PRIVATE FUNCTION aooi390_browser_search(p_type)
   DEFINE p_type LIKE type_t.chr10
   #add-point:browser_search段define
   {<point name="browser_search.define"/>}
   #end add-point    
   
   #若無輸入關鍵字則查找出所有資料
   IF NOT cl_null(g_searchstr) AND g_searchcol='0' THEN
      CALL cl_err("searchcol","std-00005",0)
      RETURN FALSE 
   END IF 
   
   IF NOT cl_null(g_searchstr) THEN
      LET g_wc = " lower(", g_searchcol, ") LIKE '%", g_searchstr, "%'"
      LET g_wc = g_wc.toLowerCase()
   ELSE
      LET g_wc = " 1=1 "
   END IF         
   
   #若為排序搜尋則添加以下條件
   IF cl_null(g_searchcol) OR g_searchcol = "0" THEN
      LET g_wc = g_wc, " ORDER BY oofg001"
   ELSE
      LET g_wc = g_wc, " ORDER BY ", g_searchcol
   END IF 
 
   CALL aooi390_browser_fill()
   RETURN TRUE
 
END FUNCTION
]]>
</section>
  <section id="aooi390.chk_hasC" ver="4" status="" src="s">
<![CDATA[#+ QBE資料查詢
PRIVATE FUNCTION aooi390_chk_hasC(pi_id)
   DEFINE pi_id    INTEGER
   DEFINE li_cnt   INTEGER
   #add-point:chk_hasC段define
   {<point name="chk_hasC.define"/>}
   #end add-point
   
   LET g_sql = "SELECT COUNT(oofg006) FROM aooi390_tmp ",
               " WHERE ",
                "oofg006 = ? AND ",
                "exp_code <> '-1' AND oofg005 <> oofg006 "
                 ," AND ",
                "oofg001 = ?"
 
   PREPARE aooi390_temp_chk FROM g_sql
 
   LET g_sql = "SELECT COUNT(*) FROM oofg_t ",
               " WHERE oofgent = '" ||g_enterprise|| "' AND ",
               "oofg005 <> oofg006 AND ",
               "oofg006 = ? "
                ," AND ",
               "oofg001 = ?",
               cl_sql_add_filter("oofg_t")     
   
   PREPARE aooi390_master_chk FROM g_sql
   
   CASE g_browser[pi_id].b_expcode 
      WHEN -1
         RETURN FALSE
      WHEN 0
         RETURN FALSE
      WHEN 1
         EXECUTE aooi390_temp_chk 
           USING g_browser[pi_id].b_oofg005
                 ,g_browser[pi_id].b_oofg001
            INTO li_cnt
         FREE aooi390_temp_chk
      WHEN 2 
         EXECUTE aooi390_master_chk 
           USING g_browser[pi_id].b_oofg005
                 ,g_browser[pi_id].b_oofg001
            INTO li_cnt
         FREE aooi390_master_chk
   END CASE
   
   IF li_cnt > 0 THEN
      RETURN TRUE
   ELSE
      RETURN FALSE
   END IF
 
END FUNCTION
]]>
</section>
