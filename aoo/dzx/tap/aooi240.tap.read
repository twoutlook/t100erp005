  <section id="aooi240.b_fill" ver="7" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aooi240_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2    STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE ooctstus,ooct002,ooct001,ooct004,ooct003,ooct001,ooct003,ooctmodid,ooctmoddt, 
       ooctownid,ooctowndp,ooctcrtid,ooctcrtdp,ooctcrtdt ,t1.oocal003 ,t2.oocal003 ,t3.oofa011 ,t4.oofa011 , 
       t5.ooefl003 ,t6.oofa011 ,t7.ooefl003 FROM ooct_t",
               "",
                              " LEFT JOIN oocal_t t1 ON t1.oocalent='"||g_enterprise||"' AND t1.oocal001=ooct001 AND t1.oocal002='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t2 ON t2.oocalent='"||g_enterprise||"' AND t2.oocal001=ooct003 AND t2.oocal002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t3 ON t3.oofaent='"||g_enterprise||"' AND t3.oofa002='2' AND t3.oofa003=ooctmodid  ",
               " LEFT JOIN oofa_t t4 ON t4.oofaent='"||g_enterprise||"' AND t4.oofa002='2' AND t4.oofa003=ooctownid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=ooctowndp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t6 ON t6.oofaent='"||g_enterprise||"' AND t6.oofa002='2' AND t6.oofa003=ooctcrtid  ",
               " LEFT JOIN ooefl_t t7 ON t7.ooeflent='"||g_enterprise||"' AND t7.ooefl001=ooctcrtdp AND t7.ooefl002='"||g_dlang||"' ",
 
               " WHERE ooctent= ? AND 1=1 AND ", p_wc2 
    
   LET g_sql = g_sql, cl_sql_add_filter("ooct_t"),
                      " ORDER BY ooct_t.ooct001,ooct_t.ooct003"
   
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE aooi240_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aooi240_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_ooct_d.clear()
   CALL g_ooct1_info_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_ooct_d[l_ac].ooctstus,g_ooct_d[l_ac].ooct002,g_ooct_d[l_ac].ooct001,g_ooct_d[l_ac].ooct004, 
       g_ooct_d[l_ac].ooct003,g_ooct1_info_d[l_ac].ooct001,g_ooct1_info_d[l_ac].ooct003,g_ooct1_info_d[l_ac].ooctmodid, 
       g_ooct1_info_d[l_ac].ooctmoddt,g_ooct1_info_d[l_ac].ooctownid,g_ooct1_info_d[l_ac].ooctowndp, 
       g_ooct1_info_d[l_ac].ooctcrtid,g_ooct1_info_d[l_ac].ooctcrtdp,g_ooct1_info_d[l_ac].ooctcrtdt, 
       g_ooct_d[l_ac].ooct001_desc,g_ooct_d[l_ac].ooct003_desc,g_ooct1_info_d[l_ac].ooctmodid_desc,g_ooct1_info_d[l_ac].ooctownid_desc, 
       g_ooct1_info_d[l_ac].ooctowndp_desc,g_ooct1_info_d[l_ac].ooctcrtid_desc,g_ooct1_info_d[l_ac].ooctcrtdp_desc 

      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL aooi240_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec THEN
      IF g_error_show = 1 THEN
         CALL cl_err("ooct_t",9035,1)
      END IF
   END IF 
   LET g_error_show = 0
   
 
   
   CALL g_ooct_d.deleteElement(g_ooct_d.getLength())   
   CALL g_ooct1_info_d.deleteElement(g_ooct1_info_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_ooct_d.getLength()
      LET g_ooct1_info_d[l_ac].ooct001 = g_ooct_d[l_ac].ooct001 
      LET g_ooct1_info_d[l_ac].ooct003 = g_ooct_d[l_ac].ooct003 
 
   END FOR
   
   IF g_cnt > g_ooct_d.getLength() THEN
      LET l_ac = g_ooct_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_ooct_d.getLength()
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE aooi240_pb
   
END FUNCTION
]]>
  </section>
