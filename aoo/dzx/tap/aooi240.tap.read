            CALL ap_ref_array2(g_ref_fields,"SELECT oocsl003 FROM oocsl_t WHERE oocslent='"||g_enterprise||"' AND oocsl001=? AND oocsl002='"||g_dlang||"'","") RETURNING g_rtn_fields
            LET g_ooct_d[l_ac].ooct001_desc = g_rtn_fields[1]
            DISPLAY BY NAME g_ooct_d[l_ac].ooct001_desc
]]>
