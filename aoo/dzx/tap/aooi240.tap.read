  <point name="input.a.page1.ooct001" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[            #此段落由子樣板a05產生
            IF  NOT cl_null(g_ooct_d[l_ac].ooct001) AND NOT cl_null(g_ooct_d[l_ac].ooct003) THEN 
               IF l_cmd = 'a' OR ( l_cmd = 'u' AND ( l_cmd = 'u' AND (g_ooct_d[l_ac].ooct001 != g_ooct_d_t.ooct001 OR g_ooct_d[l_ac].ooct003 != g_ooct_d_t.ooct003))) THEN 
                  IF NOT ap_chk_notDup("","SELECT COUNT(*) FROM ooct_t WHERE "||"ooctent = '" ||g_enterprise|| "' AND "||"ooct001 = '"||g_ooct_d[l_ac].ooct001 ||"' AND "|| "ooct003 = '"||g_ooct_d[l_ac].ooct003 ||"'",'std-00004',1) THEN 
                     LET g_ooct_d[l_ac].ooct001=g_ooct_d_t.ooct001
                     LET g_ooct_d[l_ac].ooct001_desc =g_ooct_d_t.ooct001_desc                     
                     NEXT FIELD CURRENT
                  END IF
               END IF
