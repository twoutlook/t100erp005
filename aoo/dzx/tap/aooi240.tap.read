   #判斷是否是同一群組的table
   LET ls_group = "ooct_t,"
   IF ls_group.getIndexOf(ps_table,1) > 0 AND ps_table <> 'ooct_t' THEN
   
      #add-point:delete_b段刪除前
      {<point name="delete_b.b_delete" mark="Y"/>}
