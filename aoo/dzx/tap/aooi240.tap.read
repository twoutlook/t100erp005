         ON ACTION controlp INFIELD ooct003
            #add-point:ON ACTION controlp INFIELD ooct003
            {<point name="input.c.page1.ooct003" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_ooct_d[l_ac].* = g_ooct_d_t.*
               CLOSE aooi240_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_ooct_d[l_ac].ooct001,-263,1)
               LET g_ooct_d[l_ac].* = g_ooct_d_t.*
            ELSE
               
               #寫入修改者/修改日期資訊(單身)
               LET g_ooct1_info_d[l_ac].ooctmodid = g_user 
LET g_ooct1_info_d[l_ac].ooctmoddt = cl_get_current()
 
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               UPDATE ooct_t SET (ooctstus,ooct002,ooct001,ooct004,ooct003,ooctmodid,ooctmoddt,ooctownid, 
                   ooctowndp,ooctcrtid,ooctcrtdp,ooctcrtdt) = (g_ooct_d[l_ac].ooctstus,g_ooct_d[l_ac].ooct002, 
                   g_ooct_d[l_ac].ooct001,g_ooct_d[l_ac].ooct004,g_ooct_d[l_ac].ooct003,g_ooct1_info_d[l_ac].ooctmodid, 
                   g_ooct1_info_d[l_ac].ooctmoddt,g_ooct1_info_d[l_ac].ooctownid,g_ooct1_info_d[l_ac].ooctowndp, 
                   g_ooct1_info_d[l_ac].ooctcrtid,g_ooct1_info_d[l_ac].ooctcrtdp,g_ooct1_info_d[l_ac].ooctcrtdt) 

                WHERE ooctent = g_enterprise AND
                  ooct001 = g_ooct_d_t.ooct001 #項次   
                  AND ooct003 = g_ooct_d_t.ooct003  
 
                  
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
                  
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("ooct_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                    WHEN SQLCA.sqlcode #其他錯誤
                     CALL cl_err("ooct_t",SQLCA.sqlcode,1)  
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_ooct_d[g_detail_idx].ooct001
               LET gs_keys_bak[1] = g_ooct_d_t.ooct001
               LET gs_keys[2] = g_ooct_d[g_detail_idx].ooct003
               LET gs_keys_bak[2] = g_ooct_d_t.ooct003
               CALL aooi240_update_b('ooct_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
                     LET g_log1 = util.JSON.stringify(g_ooct_d_t)
                     LET g_log2 = util.JSON.stringify(g_ooct_d[l_ac])
                     IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                        CALL s_transaction_end('N','0')
                     END IF
               END CASE
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            CALL aooi240_unlock_b("ooct_t")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock
            
             #add-point:單身after row
            {<point name="input.body.a_row"/>}
            #end add-point
            
         AFTER INPUT
            #add-point:單身input後
            {<point name="input.body.a_input"/>}
            #end add-point
            
         ON ACTION controlo   
            CALL FGL_SET_ARR_CURR(g_ooct_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_ooct_d.getLength()+1
            
      END INPUT
      
 
      
      DISPLAY ARRAY g_ooct1_info_d TO s_detail1_info.*
         ATTRIBUTES(COUNT=g_detail_cnt)  
      
         BEFORE DISPLAY 
            CALL aooi240_b_fill(g_wc2)
            CALL FGL_SET_ARR_CURR(g_detail_idx)
      
         BEFORE ROW
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1_info")
            LET l_ac = g_detail_idx
            LET g_temp_idx = l_ac
            DISPLAY g_detail_idx TO FORMONLY.idx
            CALL cl_show_fld_cont() 
            
         #add-point:page2自定義行為
         {<point name="input.body1_info.action"/>}
         #end add-point
            
      END DISPLAY
 
      
      #add-point:before_more_input
      {<point name="input.more_input"/>}
      #end add-point
      
      BEFORE DIALOG 
         IF g_temp_idx > 0 THEN
            LET l_ac = g_temp_idx
            CALL DIALOG.setCurrentRow("s_detail1",l_ac)
            LET g_temp_idx = 1
         END IF
         LET g_curr_diag = ui.DIALOG.getCurrent()
         #add-point:before_dialog
         {<point name="input.before_dialog"/>}
         #end add-point
         CASE g_aw
            WHEN "s_detail1"
               NEXT FIELD ooctstus
            WHEN "s_detail1_info"
               NEXT FIELD ooct001_2
 
         END CASE
   
      ON ACTION accept
         ACCEPT DIALOG
      
      ON ACTION cancel
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION controlr
         CALL cl_show_req_fields()
 
      ON ACTION controlf
         CALL cl_set_focus_form(ui.Interface.getRootNode()) 
              RETURNING g_fld_name,g_frm_name 
         CALL cl_fldhelp(g_frm_name,g_fld_name,g_lang) 
           
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE DIALOG
   
   END DIALOG 
   
   #add-point:modify段修改後
   {<point name="modify.after_input"/>}
   #end add-point
 
   CLOSE aooi240_bcl
   CALL s_transaction_end('Y','0')
   
END FUNCTION
]]>
  </section>
