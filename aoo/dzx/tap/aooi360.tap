  <point name="function.aooi360_chk_ooff_copy" cite_std="N" status="u" order="1" ver="1" cite_ver="" new="Y" src="s" mark_hard="N">
<![CDATA[# 複製KEY值檢查
PRIVATE FUNCTION aooi360_chk_ooff_copy(p_ooff)
DEFINE p_ooff  RECORD LIKE ooff_t.*

   IF NOT cl_null(p_ooff.ooff001) AND NOT cl_null(p_ooff.ooff002) AND NOT cl_null(p_ooff.ooff003) 
      AND NOT cl_null(p_ooff.ooff004) AND NOT cl_null(p_ooff.ooff005) 
      AND NOT cl_null(p_ooff.ooff006) AND NOT cl_null(p_ooff.ooff007) 
      AND NOT cl_null(p_ooff.ooff008) AND NOT cl_null(p_ooff.ooff009) 
      AND NOT cl_null(p_ooff.ooff010) AND NOT cl_null(p_ooff.ooff011) AND NOT cl_null(p_ooff.ooff012) THEN 
      IF NOT ap_chk_notDup(p_ooff.ooff001||"~"||p_ooff.ooff012,"SELECT COUNT(*) FROM ooff_t WHERE "||"ooffent = '" ||g_enterprise|| "' AND "||"ooff001 = '"||p_ooff.ooff001 ||"' AND "|| "ooff002 = '"||p_ooff.ooff002 ||
                              "' AND "|| "ooff003 = '"||p_ooff.ooff003 ||"' AND "|| "ooff004 = '"|| p_ooff.ooff004 ||"' AND "|| 
                              "ooff005 = '"||p_ooff.ooff005 ||"' AND "|| "ooff006 = '"||p_ooff.ooff006 ||
                              "' AND "|| "ooff007 = '"||p_ooff.ooff007 ||"' AND "|| "ooff008 = '"||p_ooff.ooff008 ||"' AND "|| 
                              "ooff009 = '"||p_ooff.ooff009 ||"' AND "|| "ooff010 = '"||p_ooff.ooff010 ||"' AND "|| 
                              "ooff011 = '"||p_ooff.ooff011 ||"' AND "|| "ooff012 = '"||p_ooff.ooff012 ||"'",'std-00004',0) THEN 
         RETURN FALSE
      END IF
   END IF
   RETURN TRUE
END FUNCTION]]>
</point>
