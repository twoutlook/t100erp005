  <section id="aooi360.modify" ver="1" status="" src="s">
<![CDATA[#+ 資料修改
PRIVATE FUNCTION aooi360_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point
   
   IF g_ooff_m.ooff001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF 
   
    SELECT UNIQUE 
 INTO 
 FROM ooff_t
 WHERE ooffent = g_enterprise AND ooff001 = g_ooff_m.ooff001 AND ooff002 = g_ooff_m.ooff002 AND ooff003 = g_ooff_m.ooff003 AND ooff004 = g_ooff_m.ooff004 AND ooff005 = g_ooff_m.ooff005 AND ooff006 = g_ooff_m.ooff006 AND ooff007 = g_ooff_m.ooff007 AND ooff008 = g_ooff_m.ooff008 AND ooff009 = g_ooff_m.ooff009 AND ooff010 = g_ooff_m.ooff010 AND ooff011 = g_ooff_m.ooff011 AND ooff012 = g_ooff_m.ooff012
 
   ERROR ""
  
   LET g_ooff001_t = g_ooff_m.ooff001
   LET g_ooff002_t = g_ooff_m.ooff002
 
   LET g_ooff003_t = g_ooff_m.ooff003
 
   LET g_ooff004_t = g_ooff_m.ooff004
 
   LET g_ooff005_t = g_ooff_m.ooff005
 
   LET g_ooff006_t = g_ooff_m.ooff006
 
   LET g_ooff007_t = g_ooff_m.ooff007
 
   LET g_ooff008_t = g_ooff_m.ooff008
 
   LET g_ooff009_t = g_ooff_m.ooff009
 
   LET g_ooff010_t = g_ooff_m.ooff010
 
   LET g_ooff011_t = g_ooff_m.ooff011
 
   LET g_ooff012_t = g_ooff_m.ooff012
 
 
   
   CALL s_transaction_begin()
   
   OPEN aooi360_cl USING g_enterprise,g_ooff_m.ooff001,g_ooff_m.ooff002,g_ooff_m.ooff003,g_ooff_m.ooff004,g_ooff_m.ooff005,g_ooff_m.ooff006,g_ooff_m.ooff007,g_ooff_m.ooff008,g_ooff_m.ooff009,g_ooff_m.ooff010,g_ooff_m.ooff011,g_ooff_m.ooff012
   IF STATUS THEN
      CALL cl_err("OPEN aooi360_cl:", STATUS, 1)
      CLOSE aooi360_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aooi360_cl INTO 
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err("ooff_t",SQLCA.sqlcode,0)
      CLOSE aooi360_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
 
   CALL aooi360_show()
   
   WHILE TRUE
      LET g_ooff_m.ooff001 = g_ooff001_t
      LET g_ooff_m.ooff002 = g_ooff002_t
 
      LET g_ooff_m.ooff003 = g_ooff003_t
 
      LET g_ooff_m.ooff004 = g_ooff004_t
 
      LET g_ooff_m.ooff005 = g_ooff005_t
 
      LET g_ooff_m.ooff006 = g_ooff006_t
 
      LET g_ooff_m.ooff007 = g_ooff007_t
 
      LET g_ooff_m.ooff008 = g_ooff008_t
 
      LET g_ooff_m.ooff009 = g_ooff009_t
 
      LET g_ooff_m.ooff010 = g_ooff010_t
 
      LET g_ooff_m.ooff011 = g_ooff011_t
 
      LET g_ooff_m.ooff012 = g_ooff012_t
 
 
      
      #寫入修改者/修改日期資訊
      
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
 
      CALL aooi360_input("u")     #欄位更改
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_ooff_m.* = g_ooff_m_t.*
         CALL aooi360_show()
         CALL cl_err("",9001,0)
         EXIT WHILE
      END IF
 
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_ooff_m.ooff001,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE aooi360_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_ooff_m.ooff001,"U")
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
]]>
