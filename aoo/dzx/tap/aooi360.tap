   {</Local define>}
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point  
   
   IF g_ooff_m.ooff001 IS NULL
   OR g_ooff_m.ooff002 IS NULL
 
   OR g_ooff_m.ooff003 IS NULL
 
   OR g_ooff_m.ooff004 IS NULL
 
   OR g_ooff_m.ooff005 IS NULL
 
   OR g_ooff_m.ooff006 IS NULL
 
   OR g_ooff_m.ooff007 IS NULL
 
   OR g_ooff_m.ooff008 IS NULL
 
   OR g_ooff_m.ooff009 IS NULL
 
   OR g_ooff_m.ooff010 IS NULL
 
   OR g_ooff_m.ooff011 IS NULL
 
   OR g_ooff_m.ooff012 IS NULL
 
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   CALL aooi360_show()
   
   CALL s_transaction_begin()
    
   LET g_ooff001_t = g_ooff_m.ooff001
   LET g_ooff002_t = g_ooff_m.ooff002
 
   LET g_ooff003_t = g_ooff_m.ooff003
 
   LET g_ooff004_t = g_ooff_m.ooff004
 
   LET g_ooff005_t = g_ooff_m.ooff005
 
   LET g_ooff006_t = g_ooff_m.ooff006
 
   LET g_ooff007_t = g_ooff_m.ooff007
 
   LET g_ooff008_t = g_ooff_m.ooff008
 
   LET g_ooff009_t = g_ooff_m.ooff009
 
   LET g_ooff010_t = g_ooff_m.ooff010
 
   LET g_ooff011_t = g_ooff_m.ooff011
 
   LET g_ooff012_t = g_ooff_m.ooff012
 
 
   
   
 
   OPEN aooi360_cl USING g_enterprise,
                           g_ooff_m.ooff001
                           ,g_ooff_m.ooff002
 
                           ,g_ooff_m.ooff003
 
                           ,g_ooff_m.ooff004
 
                           ,g_ooff_m.ooff005
 
                           ,g_ooff_m.ooff006
 
                           ,g_ooff_m.ooff007
 
                           ,g_ooff_m.ooff008
 
                           ,g_ooff_m.ooff009
 
                           ,g_ooff_m.ooff010
 
                           ,g_ooff_m.ooff011
 
                           ,g_ooff_m.ooff012
 
 
   IF STATUS THEN
      CALL cl_err("OPEN aooi360_cl:", STATUS, 0)
      CLOSE aooi360_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   FETCH aooi360_cl INTO 
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_ooff_m.ooff001,SQLCA.sqlcode,0)
      RETURN
   END IF
   
   IF cl_ask_delete() THEN
      INITIALIZE g_doc.* TO NULL
      LET g_doc.column1 = "ooff001"
      LET g_doc.value1 = g_ooff_m.ooff001
      CALL cl_doc_remove()
 
      #add-point:單頭刪除前
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point
 
      DELETE FROM ooff_t 
       WHERE ooffent = g_enterprise AND ooff001 = g_ooff_m.ooff001 
         AND ooff002 = g_ooff_m.ooff002 
 
         AND ooff003 = g_ooff_m.ooff003 
 
         AND ooff004 = g_ooff_m.ooff004 
 
         AND ooff005 = g_ooff_m.ooff005 
 
         AND ooff006 = g_ooff_m.ooff006 
 
         AND ooff007 = g_ooff_m.ooff007 
 
         AND ooff008 = g_ooff_m.ooff008 
 
         AND ooff009 = g_ooff_m.ooff009 
 
         AND ooff010 = g_ooff_m.ooff010 
 
         AND ooff011 = g_ooff_m.ooff011 
 
         AND ooff012 = g_ooff_m.ooff012 
 
 
 
      #add-point:單頭刪除中
      {<point name="delete.head.m_delete"/>}
      #end add-point
         
      IF SQLCA.sqlcode THEN
         CALL cl_err("ooff_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
  
      
      
      #add-point:單頭刪除後
      {<point name="delete.head.a_delete"/>}
      #end add-point
      
          
      
      CLEAR FORM
      CALL aooi360_ui_browser_refresh()
      IF g_browser_cnt > 0 THEN
         CALL aooi360_fetch("P")
      ELSE
         CALL aooi360_browser_fill(" 1=1 ","F")
      END IF
      
   END IF
 
   CLOSE aooi360_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_ooff_m.ooff001,"D")
 
END FUNCTION
]]>
