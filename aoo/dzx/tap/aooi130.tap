  <point name="function.aooi130_ooagmodid_ref" cite_std="N" status="u" ver="" src="s" new="Y" order="5">
<![CDATA[
PRIVATE FUNCTION aooi130_ooagmodid_ref(p_ooagmodid)
DEFINE p_ooagmodid   LIKE ooag_t.ooagmodid
DEFINE r_ooag011     LIKE ooag_t.ooag011

      LET r_ooag011 = ''
      SELECT ooag011 INTO r_ooag011 FROM ooag_t WHERE ooagent = g_enterprise AND ooag001 = p_ooagmodid
      RETURN r_ooag011
      
END FUNCTION]]>
</point>
  <section id="aooi130.b_fill" ver="9" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aooi130_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2    STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE ooagstus,ooag001,ooag008,ooag009,ooag010,ooag011,ooag012,ooag013,ooag014, 
       ooag003,ooag004,ooag005,ooag001,ooagmodid,ooagmoddt,ooagownid,ooagowndp,ooagcrtid,ooagcrtdp,ooagcrtdt , 
       t1.ooefl003 ,t2.ooefl003 ,t3.oocql004 ,t4.ooefl003 ,t5.ooefl003 FROM ooag_t",
               "",
                              " LEFT JOIN ooefl_t t1 ON t1.ooeflent='"||g_enterprise||"' AND t1.ooefl001=ooag003 AND t1.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent='"||g_enterprise||"' AND t2.ooefl001=ooag004 AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t3 ON t3.oocqlent='"||g_enterprise||"' AND t3.oocql001='5' AND t3.oocql002=ooag005 AND t3.oocql003='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=ooagowndp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=ooagcrtdp AND t5.ooefl002='"||g_dlang||"' ",
 
               " WHERE ooagent= ? AND 1=1 AND ", p_wc2 
    
   LET g_sql = g_sql, cl_sql_add_filter("ooag_t"),
                      " ORDER BY ooag_t.ooag001"
   
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE aooi130_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aooi130_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_ooag_d.clear()
   CALL g_ooag1_info_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_ooag_d[l_ac].ooagstus,g_ooag_d[l_ac].ooag001,g_ooag_d[l_ac].ooag008,g_ooag_d[l_ac].ooag009, 
       g_ooag_d[l_ac].ooag010,g_ooag_d[l_ac].ooag011,g_ooag_d[l_ac].ooag012,g_ooag_d[l_ac].ooag013,g_ooag_d[l_ac].ooag014, 
       g_ooag_d[l_ac].ooag003,g_ooag_d[l_ac].ooag004,g_ooag_d[l_ac].ooag005,g_ooag1_info_d[l_ac].ooag001, 
       g_ooag1_info_d[l_ac].ooagmodid,g_ooag1_info_d[l_ac].ooagmoddt,g_ooag1_info_d[l_ac].ooagownid, 
       g_ooag1_info_d[l_ac].ooagowndp,g_ooag1_info_d[l_ac].ooagcrtid,g_ooag1_info_d[l_ac].ooagcrtdp, 
       g_ooag1_info_d[l_ac].ooagcrtdt,g_ooag_d[l_ac].ooag003_desc,g_ooag_d[l_ac].ooag004_desc,g_ooag_d[l_ac].ooag005_desc, 
       g_ooag1_info_d[l_ac].ooagowndp_desc,g_ooag1_info_d[l_ac].ooagcrtdp_desc
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL aooi130_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec THEN
      IF g_error_show = 1 THEN
         CALL cl_err("ooag_t",9035,1)
      END IF
   END IF 
   LET g_error_show = 0
   
 
   
   CALL g_ooag_d.deleteElement(g_ooag_d.getLength())   
   CALL g_ooag1_info_d.deleteElement(g_ooag1_info_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_ooag_d.getLength()
      LET g_ooag1_info_d[l_ac].ooag001 = g_ooag_d[l_ac].ooag001 
 
   END FOR
   
   IF g_cnt > g_ooag_d.getLength() THEN
      LET l_ac = g_ooag_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_ooag_d.getLength()
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE aooi130_pb
   
END FUNCTION
]]>
