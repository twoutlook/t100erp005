  <point name="function.aooi130_ooag004" cite_std="N" status="" ver="1" src="s" new="Y" order="2" mark_hard="N">
<![CDATA[# 營運據點欄位控管
PRIVATE FUNCTION aooi130_ooag004(p_ooag004)
DEFINE p_ooag004       LIKE ooag_t.ooag004
 
 IF NOT cl_null(p_ooag004) THEN
    IF NOT ap_chk_isExist(p_ooag004,"SELECT COUNT(*) FROM ooef_t WHERE ooefent = '" ||g_enterprise||"' AND ooef001 = ? ","aoo-00090",0 ) THEN
       RETURN FALSE
    END IF
    IF NOT ap_chk_isExist(p_ooag004,"SELECT COUNT(*) FROM ooef_t WHERE ooefent = '" ||g_enterprise||"' AND ooefstus = 'Y' AND ooef001 = ? ","aoo-00091",0 ) THEN
       RETURN FALSE
    END IF
 END IF   
RETURN TRUE
END FUNCTION]]>
</point>
