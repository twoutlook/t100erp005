<![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_oocm_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂單身ACTION
         
 
         #+ 此段落由子樣板a43產生
         ON ACTION update_item
            LET g_action_choice="update_item"
            IF cl_auth_chk_act("update_item") THEN
               
               #add-point:ON ACTION update_item
               {<point name="input.detail_input.page1.update_item" />}
               #END add-point
            END IF
 
 
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_oocm_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL aooi050_b_fill(g_wc2) 
            IF g_rec_b != 0 THEN
               CALL fgl_set_arr_curr(l_ac)
            END IF
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN aooi050_cl USING g_enterprise,
                                               g_oocm_m.oocm001
                                               ,g_oocm_m.oocm002
                                               ,g_oocm_m.oocm003
 
                                               
               IF STATUS THEN
                  CALL cl_err("OPEN aooi050_cl:", STATUS, 1)
                  CLOSE aooi050_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_oocm_d[l_ac].oocm004 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_oocm_d_t.* = g_oocm_d[l_ac].*  #BACKUP
               CALL aooi050_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL aooi050_set_no_entry_b(l_cmd)
               OPEN aooi050_bcl USING g_enterprise,g_oocm_m.oocm001,
                                                g_oocm_m.oocm002,
                                                g_oocm_m.oocm003,
 
                                                g_oocm_d_t.oocm004
 
               IF STATUS THEN
                  CALL cl_err("OPEN aooi050_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aooi050_bcl INTO g_oocm_d[l_ac].oocmstus,g_oocm_d[l_ac].oocm004
                  IF SQLCA.sqlcode THEN
                      CALL cl_err(g_oocm_d_t.oocm004,SQLCA.sqlcode,1)
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL aooi050_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            LET g_detail_multi_table_t.oocml001 = g_oocm_m.oocm001
LET g_detail_multi_table_t.oocml002 = g_oocm_m.oocm002
LET g_detail_multi_table_t.oocml003 = g_oocm_m.oocm003
LET g_detail_multi_table_t.oocml004 = g_oocm_d[l_ac].oocm004
LET g_detail_multi_table_t.oocml005 = g_dlang
LET g_detail_multi_table_t.oocml006 = g_oocm_d[l_ac].oocml006
LET g_detail_multi_table_t.oocml007 = g_oocm_d[l_ac].oocml007
 
        
         BEFORE INSERT
            
            INITIALIZE g_oocm_d_t.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_oocm_d[l_ac].* TO NULL
            
            #公用欄位預設值
            #此段落由子樣板a14產生    
      LET g_oocm1_info_d[l_ac].oocmownid = g_user
      LET g_oocm1_info_d[l_ac].oocmowndp = g_dept
      LET g_oocm1_info_d[l_ac].oocmcrtid = g_user
      LET g_oocm1_info_d[l_ac].oocmcrtdp = g_dept 
      LET g_oocm1_info_d[l_ac].oocmcrtdt = cl_get_current()
      LET g_oocm1_info_d[l_ac].oocmmodid = ""
      LET g_oocm1_info_d[l_ac].oocmmoddt = ""
      LET g_oocm_d[l_ac].oocmstus = ""
 
  
            
            #一般欄位預設值
            
            
            
            LET g_oocm_d_t.* = g_oocm_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL aooi050_set_entry_b(l_cmd)
            #add-point:set_entry_b後
            {<point name="input.body.before_insert.set_entry_b"/>}
            #end add-point
            CALL aooi050_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_oocm_d[li_reproduce_target].* = g_oocm_d[li_reproduce].*
               LET g_oocm1_info_d[li_reproduce_target].* = g_oocm1_info_d[li_reproduce].*
 
               LET g_oocm_d[g_oocm_d.getLength()].oocm004 = NULL
 
            END IF
            LET g_detail_multi_table_t.oocml001 = g_oocm_m.oocm001
LET g_detail_multi_table_t.oocml002 = g_oocm_m.oocm002
LET g_detail_multi_table_t.oocml003 = g_oocm_m.oocm003
LET g_detail_multi_table_t.oocml004 = g_oocm_d[l_ac].oocm004
LET g_detail_multi_table_t.oocml005 = g_dlang
LET g_detail_multi_table_t.oocml006 = g_oocm_d[l_ac].oocml006
LET g_detail_multi_table_t.oocml007 = g_oocm_d[l_ac].oocml007
 
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
 
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM oocm_t 
             WHERE oocment = g_enterprise AND oocm001 = g_oocm_m.oocm001
               AND oocm002 = g_oocm_m.oocm002
               AND oocm003 = g_oocm_m.oocm003
 
               AND oocm004 = g_oocm_d[l_ac].oocm004
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               CALL s_transaction_begin()
               #add-point:單身新增前
               {<point name="input.body.b_insert" mark="Y"/>}
               #end add-point
               INSERT INTO oocm_t
                           (oocment,
                            oocm001,oocm002,oocm003,
                            oocm004
                            ,oocmstus,oocmmodid,oocmmoddt,oocmownid,oocmowndp,oocmcrtid,oocmcrtdp,oocmcrtdt) 
                     VALUES(g_enterprise,
                            g_oocm_m.oocm001,g_oocm_m.oocm002,g_oocm_m.oocm003,
                            g_oocm_d[l_ac].oocm004
                            ,g_oocm_d[l_ac].oocmstus,g_oocm1_info_d[l_ac].oocmmodid,g_oocm1_info_d[l_ac].oocmmoddt, 
                                g_oocm1_info_d[l_ac].oocmownid,g_oocm1_info_d[l_ac].oocmowndp,g_oocm1_info_d[l_ac].oocmcrtid, 
                                g_oocm1_info_d[l_ac].oocmcrtdp,g_oocm1_info_d[l_ac].oocmcrtdt)
               #add-point:單身新增中
               {<point name="input.body.m_insert"/>}
               #end add-point
               LET p_cmd = 'u'
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_oocm_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("oocm_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #資料多語言用-增/改
                        INITIALIZE l_var_keys TO NULL 
         INITIALIZE l_field_keys TO NULL 
         INITIALIZE l_vars TO NULL 
         INITIALIZE l_fields TO NULL 
         INITIALIZE l_var_keys_bak TO NULL 
         IF g_oocm_m.oocm001 = g_detail_multi_table_t.oocml001 AND
         g_oocm_m.oocm002 = g_detail_multi_table_t.oocml002 AND
         g_oocm_m.oocm003 = g_detail_multi_table_t.oocml003 AND
         g_oocm_d[l_ac].oocm004 = g_detail_multi_table_t.oocml004 AND
         g_oocm_d[l_ac].oocml006 = g_detail_multi_table_t.oocml006 AND
         g_oocm_d[l_ac].oocml007 = g_detail_multi_table_t.oocml007 THEN
         ELSE 
            LET l_var_keys[01] = g_oocm_m.oocm001
            LET l_field_keys[01] = 'oocml001'
            LET l_var_keys[02] = g_oocm_m.oocm002
            LET l_field_keys[02] = 'oocml002'
            LET l_var_keys[03] = g_oocm_m.oocm003
            LET l_field_keys[03] = 'oocml003'
            LET l_var_keys[04] = g_oocm_d[l_ac].oocm004
            LET l_field_keys[04] = 'oocml004'
            LET l_var_keys[05] = g_dlang
            LET l_field_keys[05] = 'oocml005'
            LET l_vars[01] = g_oocm_d[l_ac].oocml006
            LET l_fields[01] = 'oocml006'
            LET l_vars[02] = g_oocm_d[l_ac].oocml007
            LET l_fields[02] = 'oocml007'
            LET l_vars[03] = g_enterprise 
            LET l_fields[03] = 'oocmlent'
            LET l_var_keys_bak[01] = g_detail_multi_table_t.oocml001
            LET l_var_keys_bak[02] = g_detail_multi_table_t.oocml002
            LET l_var_keys_bak[03] = g_detail_multi_table_t.oocml003
            LET l_var_keys_bak[04] = g_detail_multi_table_t.oocml004
            LET l_var_keys_bak[05] = g_detail_multi_table_t.oocml005
            CALL cl_multitable(l_var_keys,l_field_keys,l_vars,l_fields,l_var_keys_bak,'oocml_t')
         END IF 
 
               #add-point:input段-after_insert
               {<point name="input.body.a_insert"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR "INSERT O.K"
               LET g_rec_b=g_rec_b+1
               DISPLAY g_rec_b TO FORMONLY.cnt
            END IF
            
            #add-point:單身新增後
            {<point name="input.body.after_insert"/>}
            #end add-point
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_oocm_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_oocm_d.deleteElement(l_ac)
               NEXT FIELD oocm004
            END IF
            IF g_oocm_d_t.oocm004 IS NOT NULL
 
               THEN
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete"/>}
               #end add-point
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               IF aooi050_before_delete() THEN 
                  CALL s_transaction_end('Y','0')
               ELSE 
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               END IF 
               CLOSE aooi050_bcl
               LET l_count = g_oocm_d.getLength()
            END IF 
            
            #add-point:單身刪除後
            {<point name="input.body.a_delete"/>}
            #end add-point
              
         AFTER DELETE 
            #add-point:單身AFTER DELETE 
            {<point name="input.body.after_delete"/>}
            #end add-point
 
                  #此段落由子樣板a01產生
         BEFORE FIELD oocm004
            #add-point:BEFORE FIELD oocm004
            {<point name="input.b.page1.oocm004" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD oocm004
            
            #add-point:AFTER FIELD oocm004
            {<point name="input.a.page1.oocm004" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE oocm004
            #add-point:ON CHANGE oocm004
            {<point name="input.g.page1.oocm004" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD oocml006
            #add-point:BEFORE FIELD oocml006
            {<point name="input.b.page1.oocml006" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD oocml006
            
            #add-point:AFTER FIELD oocml006
            {<point name="input.a.page1.oocml006" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE oocml006
            #add-point:ON CHANGE oocml006
            {<point name="input.g.page1.oocml006" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD oocml007
            #add-point:BEFORE FIELD oocml007
            {<point name="input.b.page1.oocml007" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD oocml007
            
            #add-point:AFTER FIELD oocml007
            {<point name="input.a.page1.oocml007" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE oocml007
            #add-point:ON CHANGE oocml007
            {<point name="input.g.page1.oocml007" />}
            #END add-point
 
 
                  #Ctrlp:input.c.page1.oocm004
         ON ACTION controlp INFIELD oocm004
            #add-point:ON ACTION controlp INFIELD oocm004
            {<point name="input.c.page1.oocm004" />}
            #END add-point
 
         #Ctrlp:input.c.page1.oocml006
         ON ACTION controlp INFIELD oocml006
            #add-point:ON ACTION controlp INFIELD oocml006
            {<point name="input.c.page1.oocml006" />}
            #END add-point
 
         #Ctrlp:input.c.page1.oocml007
         ON ACTION controlp INFIELD oocml007
            #add-point:ON ACTION controlp INFIELD oocml007
            {<point name="input.c.page1.oocml007" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_oocm_d[l_ac].* = g_oocm_d_t.*
               CLOSE aooi050_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_oocm_d[l_ac].oocm004,-263,1)
               LET g_oocm_d[l_ac].* = g_oocm_d_t.*
            ELSE
               #寫入修改者/修改日期資訊
               LET g_oocm1_info_d[l_ac].oocmmodid = g_user 
LET g_oocm1_info_d[l_ac].oocmmoddt = cl_get_current()
 
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
         
               UPDATE oocm_t SET (oocm001,oocm002,oocm003,oocmstus,oocm004,oocmmodid,oocmmoddt,oocmownid, 
                   oocmowndp,oocmcrtid,oocmcrtdp,oocmcrtdt) = (g_oocm_m.oocm001,g_oocm_m.oocm002,g_oocm_m.oocm003, 
                   g_oocm_d[l_ac].oocmstus,g_oocm_d[l_ac].oocm004,g_oocm1_info_d[l_ac].oocmmodid,g_oocm1_info_d[l_ac].oocmmoddt, 
                   g_oocm1_info_d[l_ac].oocmownid,g_oocm1_info_d[l_ac].oocmowndp,g_oocm1_info_d[l_ac].oocmcrtid, 
                   g_oocm1_info_d[l_ac].oocmcrtdp,g_oocm1_info_d[l_ac].oocmcrtdt)
                WHERE oocment = g_enterprise AND oocm001 = g_oocm_m.oocm001 
                 AND oocm002 = g_oocm_m.oocm002 
                 AND oocm003 = g_oocm_m.oocm003 
 
                 AND oocm004 = g_oocm_d_t.oocm004 #項次   
 
                 
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
               
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("oocm_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                  #WHEN SQLCA.sqlcode #其他錯誤
                  #   CALL cl_err("oocm_t",SQLCA.sqlcode,1)  
                  #   CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_oocm_m.oocm001
               LET gs_keys_bak[1] = g_oocm001_t
               LET gs_keys[2] = g_oocm_m.oocm002
               LET gs_keys_bak[2] = g_oocm002_t
               LET gs_keys[3] = g_oocm_m.oocm003
               LET gs_keys_bak[3] = g_oocm003_t
               LET gs_keys[4] = g_oocm_d[g_detail_idx].oocm004
               LET gs_keys_bak[4] = g_oocm_d_t.oocm004
               CALL aooi050_update_b('oocm_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                              INITIALIZE l_var_keys TO NULL 
         INITIALIZE l_field_keys TO NULL 
         INITIALIZE l_vars TO NULL 
         INITIALIZE l_fields TO NULL 
         INITIALIZE l_var_keys_bak TO NULL 
         IF g_oocm_m.oocm001 = g_detail_multi_table_t.oocml001 AND
         g_oocm_m.oocm002 = g_detail_multi_table_t.oocml002 AND
         g_oocm_m.oocm003 = g_detail_multi_table_t.oocml003 AND
         g_oocm_d[l_ac].oocm004 = g_detail_multi_table_t.oocml004 AND
         g_oocm_d[l_ac].oocml006 = g_detail_multi_table_t.oocml006 AND
         g_oocm_d[l_ac].oocml007 = g_detail_multi_table_t.oocml007 THEN
         ELSE 
            LET l_var_keys[01] = g_oocm_m.oocm001
            LET l_field_keys[01] = 'oocml001'
            LET l_var_keys[02] = g_oocm_m.oocm002
            LET l_field_keys[02] = 'oocml002'
            LET l_var_keys[03] = g_oocm_m.oocm003
            LET l_field_keys[03] = 'oocml003'
            LET l_var_keys[04] = g_oocm_d[l_ac].oocm004
            LET l_field_keys[04] = 'oocml004'
            LET l_var_keys[05] = g_dlang
            LET l_field_keys[05] = 'oocml005'
            LET l_vars[01] = g_oocm_d[l_ac].oocml006
            LET l_fields[01] = 'oocml006'
            LET l_vars[02] = g_oocm_d[l_ac].oocml007
            LET l_fields[02] = 'oocml007'
            LET l_vars[03] = g_enterprise 
            LET l_fields[03] = 'oocmlent'
            LET l_var_keys_bak[01] = g_detail_multi_table_t.oocml001
            LET l_var_keys_bak[02] = g_detail_multi_table_t.oocml002
            LET l_var_keys_bak[03] = g_detail_multi_table_t.oocml003
            LET l_var_keys_bak[04] = g_detail_multi_table_t.oocml004
            LET l_var_keys_bak[05] = g_detail_multi_table_t.oocml005
            CALL cl_multitable(l_var_keys,l_field_keys,l_vars,l_fields,l_var_keys_bak,'oocml_t')
         END IF 
 
                     LET g_log1 = util.JSON.stringify(g_oocm_m),util.JSON.stringify(g_oocm_d_t)
                     LET g_log2 = util.JSON.stringify(g_oocm_m),util.JSON.stringify(g_oocm_d[l_ac])
                     IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                        CALL s_transaction_end('N','0')
                     END IF
               END CASE
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
            END IF
 
         AFTER INPUT
            #若單身還沒有輸入資料, 強制切換至單身
            IF g_oocm_d.getLength() = 0 THEN
               NEXT FIELD oocm004
            END IF
            #add-point:input段after input 
            {<point name="input.body.after_input"/>}
            #end add-point    
            
         ON ACTION controlo   
            CALL FGL_SET_ARR_CURR(g_oocm_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_oocm_d.getLength()+1
            
      END INPUT
 
 
      
      DISPLAY ARRAY g_oocm1_info_d TO s_detail1_info.* ATTRIBUTES(COUNT=g_rec_b)  
      
         BEFORE ROW
            CALL aooi050_b_fill(g_wc2) 
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1_info")
            LET l_ac = g_detail_idx
            DISPLAY g_detail_idx TO FORMONLY.idx
            CALL aooi050_ui_detailshow()
        
         BEFORE DISPLAY 
            CALL FGL_SET_ARR_CURR(g_detail_idx)      
         
         #add-point:page2自定義行為
         {<point name="input.body1_info.action"/>}
         #end add-point
         
      END DISPLAY
 
      
 
      
      #add-point:input段more_input
      {<point name="input.more_inputarray"/>}
      #end add-point    
      
      BEFORE DIALOG
         #add-point:input段before_dialog
