   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE aooi050_detail_lang AS ",
                "SELECT * FROM oocml_t "
   PREPARE repro_oocml_t FROM ls_sql
   EXECUTE repro_oocml_t
   FREE repro_oocml_t
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO aooi050_detail_lang SELECT * FROM oocml_t 
                                             WHERE oocmlent = g_enterprise AND oocml001 = g_oocm001_t
                                             AND oocml002 = g_oocm002_t                                             AND oocml003 = g_oocm003_t
   
   #將key修正為調整後   
   UPDATE aooi050_detail_lang 
