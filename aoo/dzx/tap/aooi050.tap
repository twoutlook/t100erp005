  <section id="aooi050.delete" ver="6" status="" src="s">
<![CDATA[#+ 資料刪除
PRIVATE FUNCTION aooi050_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_oocm_m.oocm001 IS NULL
   OR g_oocm_m.oocm002 IS NULL
   OR g_oocm_m.oocm003 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE aooi050_master_referesh USING g_oocm_m.oocm001,g_oocm_m.oocm002,g_oocm_m.oocm003 INTO g_oocm_m.oocm001, 
       g_oocm_m.oocm002,g_oocm_m.oocm003,g_oocm_m.oocm001_desc,g_oocm_m.oocm002_desc,g_oocm_m.oocm003_desc 

   
   CALL aooi050_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN aooi050_cl USING g_enterprise,g_oocm_m.oocm001
                                                       ,g_oocm_m.oocm002
                                                       ,g_oocm_m.oocm003
 
   IF STATUS THEN
      CALL cl_err("OPEN aooi050_cl:", STATUS, 1)
      CLOSE aooi050_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aooi050_cl INTO g_oocm_m.oocm001,g_oocm_m.oocm001_desc,g_oocm_m.oocm002,g_oocm_m.oocm002_desc, 
       g_oocm_m.oocm003,g_oocm_m.oocm003_desc
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_oocm_m.oocm001,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL aooi050_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM oocm_t WHERE oocment = g_enterprise AND oocm001 = g_oocm_m.oocm001
                                                               AND oocm002 = g_oocm_m.oocm002
                                                               AND oocm003 = g_oocm_m.oocm003
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("oocm_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_oocm_d.clear() 
      CALL g_oocm1_info_d.clear()       
 
     
      CALL aooi050_ui_browser_refresh()  
      CALL aooi050_ui_headershow()  
      CALL aooi050_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL aooi050_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL aooi050_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE aooi050_cl
