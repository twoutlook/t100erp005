       , oocmcrtdp = g_dept 
       , oocmcrtdt = ld_date
       , oocmmodid = "" 
       , oocmmoddt = "" 
      #, oocmstus = "Y"
 
 
                                       
  
   #將資料塞回原table   
   INSERT INTO oocm_t SELECT * FROM aooi050_detail
   
   IF SQLCA.sqlcode THEN
      CALL cl_err("reproduce",SQLCA.sqlcode,1)
