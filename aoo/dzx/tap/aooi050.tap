  <section id="aooi050.main" ver="5" status="" src="s">
<![CDATA[#+ 此段落由子樣板a26產生
#OPTIONS SHORT CIRCUIT
#+ 作業開始 
MAIN
   #add-point:main段define
   {<point name="main.define"/>}
   #end add-point   
 
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("aoo","")
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT oocm001,'',oocm002,'',oocm003,''", 
                      " FROM oocm_t",
                      " WHERE oocment= ? AND oocm001=? AND oocm002=? AND oocm003=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE aooi050_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE oocm001,oocm002,oocm003,t1.oocgl003 ,t2.oocil004 ,t3.oockl005",
               " FROM oocm_t",
                              " LEFT JOIN oocgl_t t1 ON t1.oocglent='"||g_enterprise||"' AND t1.oocgl001=oocm001 AND t1.oocgl002='"||g_dlang||"' ",
               " LEFT JOIN oocil_t t2 ON t2.oocilent='"||g_enterprise||"' AND t2.oocil001=oocm001 AND t2.oocil002=oocm002 AND t2.oocil003='"||g_dlang||"' ",
               " LEFT JOIN oockl_t t3 ON t3.oocklent='"||g_enterprise||"' AND t3.oockl001=oocm001 AND t3.oockl002=oocm002 AND t3.oockl003=oocm003 AND t3.oockl004='"||g_dlang||"' ",
 
               " WHERE oocment = '" ||g_enterprise|| "' AND oocm001 = ? AND oocm002 = ? AND oocm003 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE aooi050_master_referesh FROM g_sql
 
 
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_aooi050 WITH FORM cl_ap_formpath("aoo",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL aooi050_init()   
 
      #進入選單 Menu (="N")
      CALL aooi050_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_aooi050
      
   END IF 
   
   CLOSE aooi050_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
   
END MAIN
 
 
]]>
</section>
  <section id="aooi050.modify" ver="4" status="" src="s">
<![CDATA[#+ 資料修改
PRIVATE FUNCTION aooi050_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_oocm_m.oocm001 IS NULL
   OR g_oocm_m.oocm002 IS NULL
   OR g_oocm_m.oocm003 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE aooi050_master_referesh USING g_oocm_m.oocm001,g_oocm_m.oocm002,g_oocm_m.oocm003 INTO g_oocm_m.oocm001, 
       g_oocm_m.oocm002,g_oocm_m.oocm003,g_oocm_m.oocm001_desc,g_oocm_m.oocm002_desc,g_oocm_m.oocm003_desc 

 
   ERROR ""
  
   LET g_oocm001_t = g_oocm_m.oocm001
   LET g_oocm002_t = g_oocm_m.oocm002
   LET g_oocm003_t = g_oocm_m.oocm003
 
   CALL s_transaction_begin()
   
   OPEN aooi050_cl USING g_enterprise,g_oocm_m.oocm001
                                                       ,g_oocm_m.oocm002
                                                       ,g_oocm_m.oocm003
 
   IF STATUS THEN
      CALL cl_err("OPEN aooi050_cl:", STATUS, 1)
      CLOSE aooi050_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aooi050_cl INTO g_oocm_m.oocm001,g_oocm_m.oocm001_desc,g_oocm_m.oocm002,g_oocm_m.oocm002_desc, 
       g_oocm_m.oocm003,g_oocm_m.oocm003_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_oocm_m.oocm001,SQLCA.sqlcode,0)
      CLOSE aooi050_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL aooi050_show()
   WHILE TRUE
      LET g_oocm001_t = g_oocm_m.oocm001
      LET g_oocm002_t = g_oocm_m.oocm002
      LET g_oocm003_t = g_oocm_m.oocm003
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL aooi050_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_oocm_m.* = g_oocm_m_t.*
         CALL aooi050_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_oocm_m.oocm001 != g_oocm001_t 
      OR g_oocm_m.oocm002 != g_oocm002_t 
      OR g_oocm_m.oocm003 != g_oocm003_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE oocm_t SET oocm001 = g_oocm_m.oocm001
                                       , oocm002 = g_oocm_m.oocm002
                                       , oocm003 = g_oocm_m.oocm003
 
          WHERE oocment = g_enterprise AND oocm001 = g_oocm001_t
            AND oocm002 = g_oocm002_t
            AND oocm003 = g_oocm003_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("oocm_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("oocm_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_oocm_m.oocm001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE aooi050_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_oocm_m.oocm001,'U')
 
   CALL aooi050_b_fill("1=1")
   
END FUNCTION   
]]>
</section>
