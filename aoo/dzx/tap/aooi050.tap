  <section id="aooi050.b_fill" ver="6" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aooi050_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_oocm_d.clear()
   CALL g_oocm1_info_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE oocmstus,oocm004,oocm004,oocmmodid,oocmmoddt,oocmownid,oocmowndp,oocmcrtid, 
       oocmcrtdp,oocmcrtdt,t1.oofa011 ,t2.oofa011 ,t3.ooefl003 ,t4.oofa011 ,t5.ooefl003 FROM oocm_t", 
          
               " LEFT JOIN oocml_t ON oocm001 = oocml001 AND oocm002 = oocml002 AND oocm003 = oocml003 AND oocm004 = oocml004 AND oocml005 = '",g_lang,"'",
               
                              " LEFT JOIN oofa_t t1 ON t1.oofaent='"||g_enterprise||"' AND t1.oofa002='2' AND t1.oofa003=oocmmodid  ",
               " LEFT JOIN oofa_t t2 ON t2.oofaent='"||g_enterprise||"' AND t2.oofa002='2' AND t2.oofa003=oocmownid  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent='"||g_enterprise||"' AND t3.ooefl001=oocmowndp AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t4 ON t4.oofaent='"||g_enterprise||"' AND t4.oofa002='2' AND t4.oofa003=oocmcrtid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=oocmcrtdp AND t5.ooefl002='"||g_dlang||"' ",
 
               " WHERE oocment= ? AND oocm001=? AND oocm002=? AND oocm003=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("oocm_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF aooi050_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY oocm_t.oocm004"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE aooi050_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR aooi050_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_oocm_m.oocm001,g_oocm_m.oocm002,g_oocm_m.oocm003
                                               
      FOREACH b_fill_cs INTO g_oocm_d[l_ac].oocmstus,g_oocm_d[l_ac].oocm004,g_oocm1_info_d[l_ac].oocm004, 
          g_oocm1_info_d[l_ac].oocmmodid,g_oocm1_info_d[l_ac].oocmmoddt,g_oocm1_info_d[l_ac].oocmownid, 
          g_oocm1_info_d[l_ac].oocmowndp,g_oocm1_info_d[l_ac].oocmcrtid,g_oocm1_info_d[l_ac].oocmcrtdp, 
          g_oocm1_info_d[l_ac].oocmcrtdt,g_oocm1_info_d[l_ac].oocmmodid_desc,g_oocm1_info_d[l_ac].oocmownid_desc, 
          g_oocm1_info_d[l_ac].oocmowndp_desc,g_oocm1_info_d[l_ac].oocmcrtid_desc,g_oocm1_info_d[l_ac].oocmcrtdp_desc 

                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_oocm1_info_d[l_ac].oocmownid_desc = cl_get_username(g_oocm1_info_d[l_ac].oocmownid)
      #LET g_oocm1_info_d[l_ac].oocmowndp_desc = cl_get_deptname(g_oocm1_info_d[l_ac].oocmowndp)
      #LET g_oocm1_info_d[l_ac].oocmcrtid_desc = cl_get_username(g_oocm1_info_d[l_ac].oocmcrtid)
      #LET g_oocm1_info_d[l_ac].oocmcrtdp_desc = cl_get_deptname(g_oocm1_info_d[l_ac].oocmcrtdp)
      #LET g_oocm1_info_d[l_ac].oocmmodid_desc = cl_get_username(g_oocm1_info_d[l_ac].oocmmodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_oocm_d.deleteElement(g_oocm_d.getLength())
      CALL g_oocm1_info_d.deleteElement(g_oocm1_info_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE aooi050_pb   
   
END FUNCTION
]]>
</section>
