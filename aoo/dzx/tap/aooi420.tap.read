  <point name="function.aooi420_oogc001_chk" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[# oogc001欄位控管
PRIVATE FUNCTION aooi420_oogc001_chk(p_oogc001)
DEFINE l_n LIKE type_t.num5
DEFINE p_oogc001 LIKE oogc_t.oogc001
       LET l_n = 0 
       SELECT COUNT(*) INTO l_n FROM ooal_t
        WHERE ooalent = g_enterprise
          AND ooal002 = p_oogc001
          AND ooal001 = 4
       IF l_n = 0 THEN
          IF cl_ask_confirm('aoo-00076') THEN
              IF NOT s_aooi070_ins(4,p_oogc001) THEN
                 RETURN FALSE
              END IF
           ELSE
               RETURN FALSE
          END IF
       END IF
       
       LET l_n = 0 
       SELECT COUNT(*) INTO l_n FROM ooal_t
        WHERE ooalent = g_enterprise
          AND ooal002 = p_oogc001
          AND ooal001 = 4
          AND ooalstus='Y'
       IF l_n = 0 THEN
          CALL cl_err(p_oogc001,'aoo-00124',1)
          RETURN FALSE
       END IF
       RETURN TRUE
END FUNCTION]]>
  </point>
