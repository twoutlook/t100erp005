  <point name="function.aooi420_oogc003_chk" order="4" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[# oogc003欄位控管
PRIVATE FUNCTION aooi420_oogc003_chk()
DEFINE l_n  LIKE type_t.num5
               IF NOT ap_chk_notDup("","SELECT COUNT(*) FROM oogc_t WHERE "||"oogcent = '" ||g_enterprise|| "' AND "||"oogc001 = '"||g_oogc_m.oogc001 ||"' AND "|| "oogc002 = '"||g_oogc_m.oogc002 ||"' AND "|| "oogc003 = '"||g_oogc_d[l_ac].oogc003 ||"'",'std-00004',0) THEN 
                  RETURN FALSE
               END IF
               LET l_n = 0 
               SELECT COUNT(*) INTO l_n FROM ooga_t
                WHERE ooga001 = g_oogc_d[l_ac].oogc003
               IF l_n = 0 THEN
                  CALL cl_err(g_oogc_d[l_ac].oogc003,'aoo-00085',1)
                  RETURN FALSE
               END IF 
               LET l_n = 0
               SELECT COUNT(*) INTO l_n FROM ooga_t
                WHERE ooga001 = g_oogc_d[l_ac].oogc003
                  AND oogastus= 'Y'
               IF l_n = 0 THEN
                  CALL cl_err(g_oogc_d[l_ac].oogc003,'aoo-00080',1)
                  RETURN FALSE
               END IF 
               IF g_oogc_m.oogc002 != '4' AND NOT cl_null(g_oogc_m.oogc015) THEN
                  IF YEAR(g_oogc_d[l_ac].oogc003) != g_oogc_m.oogc015 THEN
                     CALL cl_err(g_oogc_d[l_ac].oogc003,'aoo-00164',1)
                     RETURN FALSE
                  END IF
               END IF
               RETURN TRUE
END FUNCTION]]>
  </point>
  <point name="function.aooi420_oogc002_chk" order="5" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[# oogc002 欄位控管
PRIVATE FUNCTION aooi420_oogc002_chk()
DEFINE l_n   LIKE type_t.num5
     LET l_n = 0
     SELECT COUNT(*) INTO l_n FROM  gzcb_t
      WHERE gzcb002 = g_oogc_m.oogc002 
        AND gzcb001 = 25
      IF l_n = 0 THEN
         CALL cl_err(g_oogc_m.oogc002,'azz-00023',1)
         RETURN FALSE
      END IF
      
     LET l_n = 0
     SELECT COUNT(*) INTO l_n FROM  gzca_t,gzcb_t
      WHERE gzcb002 = g_oogc_m.oogc002 
        AND gzcb001 = 25
        AND gzcastus = 'Y'
        AND gzca001 = gzcb001
      IF l_n = 0 THEN
         CALL cl_err(g_oogc_m.oogc002,'azz-00024',1)
         RETURN FALSE
      END IF
      RETURN TRUE
END FUNCTION]]>
  </point>
