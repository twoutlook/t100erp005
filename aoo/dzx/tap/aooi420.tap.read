  <section id="aooi420.main" ver="6" status="" src="s">
    <![CDATA[#+ 此段落由子樣板a26產生
#OPTIONS SHORT CIRCUIT
#+ 作業開始 
MAIN
   #add-point:main段define
   {<point name="main.define"/>}
   #end add-point   
 
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("aoo","")
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT oogc001,'',oogc015,oogc002", 
                      " FROM oogc_t",
                      " WHERE oogcent= ? AND oogc001=? AND oogc002=? AND oogc015=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE aooi420_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE oogc001,oogc015,oogc002,t1.ooall004",
               " FROM oogc_t",
                              " LEFT JOIN ooall_t t1 ON t1.ooallent='"||g_enterprise||"' AND t1.ooall001='4' AND t1.ooall002=oogc001 AND t1.ooall003='"||g_dlang||"' ",
 
               " WHERE oogcent = '" ||g_enterprise|| "' AND oogc001 = ? AND oogc002 = ? AND oogc015 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE aooi420_master_referesh FROM g_sql
 
 
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_aooi420 WITH FORM cl_ap_formpath("aoo",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL aooi420_init()   
 
      #進入選單 Menu (="N")
      CALL aooi420_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_aooi420
      
   END IF 
   
   CLOSE aooi420_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
   
END MAIN
 
 
]]>
  </section>
  <section id="aooi420.modify" ver="4" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aooi420_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_oogc_m.oogc001 IS NULL
   OR g_oogc_m.oogc002 IS NULL
   OR g_oogc_m.oogc015 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE aooi420_master_referesh USING g_oogc_m.oogc001,g_oogc_m.oogc002,g_oogc_m.oogc015 INTO g_oogc_m.oogc001, 
       g_oogc_m.oogc015,g_oogc_m.oogc002,g_oogc_m.oogc001_desc
 
   ERROR ""
  
   LET g_oogc001_t = g_oogc_m.oogc001
   LET g_oogc002_t = g_oogc_m.oogc002
   LET g_oogc015_t = g_oogc_m.oogc015
 
   CALL s_transaction_begin()
   
   OPEN aooi420_cl USING g_enterprise,g_oogc_m.oogc001
                                                       ,g_oogc_m.oogc002
                                                       ,g_oogc_m.oogc015
 
   IF STATUS THEN
      CALL cl_err("OPEN aooi420_cl:", STATUS, 1)
      CLOSE aooi420_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aooi420_cl INTO g_oogc_m.oogc001,g_oogc_m.oogc001_desc,g_oogc_m.oogc015,g_oogc_m.oogc002
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_oogc_m.oogc001,SQLCA.sqlcode,0)
      CLOSE aooi420_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL aooi420_show()
   WHILE TRUE
      LET g_oogc001_t = g_oogc_m.oogc001
      LET g_oogc002_t = g_oogc_m.oogc002
      LET g_oogc015_t = g_oogc_m.oogc015
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL aooi420_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_oogc_m.* = g_oogc_m_t.*
         CALL aooi420_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_oogc_m.oogc001 != g_oogc001_t 
      OR g_oogc_m.oogc002 != g_oogc002_t 
      OR g_oogc_m.oogc015 != g_oogc015_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE oogc_t SET oogc001 = g_oogc_m.oogc001
                                       , oogc002 = g_oogc_m.oogc002
                                       , oogc015 = g_oogc_m.oogc015
 
          WHERE oogcent = g_enterprise AND oogc001 = g_oogc001_t
            AND oogc002 = g_oogc002_t
            AND oogc015 = g_oogc015_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("oogc_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("oogc_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_oogc_m.oogc001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE aooi420_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_oogc_m.oogc001,'U')
 
   CALL aooi420_b_fill("1=1")
   
END FUNCTION   
]]>
  </section>
