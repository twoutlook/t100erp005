   LET g_sql = "SELECT  UNIQUE oogcstus,oogc003,oogc004,oogc005,oogc008,oogc006,oogc007,oogc016,oogc017, 
       oogc009,oogc010,oogc011,oogc012,oogc013,oogc014,oogc003,oogcmodid,oogcmoddt,oogcownid,oogcowndp, 
       oogccrtid,oogccrtdp,oogccrtdt,t1.oocql004 ,t2.oocql004 ,t3.oocql004 ,t4.oocql004 ,t5.oocql004 , 
       t6.oocql004 ,t7.oofa011 ,t8.oofa011 ,t9.ooefl003 ,t10.oofa011 ,t11.ooefl003 FROM oogc_t",   
               "",
               
                              " LEFT JOIN oocql_t t1 ON t1.oocqlent='"||g_enterprise||"' AND t1.oocql001='6' AND t1.oocql002=oogc004 AND t1.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t2 ON t2.oocqlent='"||g_enterprise||"' AND t2.oocql001='7' AND t2.oocql002=oogc010 AND t2.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t3 ON t3.oocqlent='"||g_enterprise||"' AND t3.oocql001='8' AND t3.oocql002=oogc011 AND t3.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t4 ON t4.oocqlent='"||g_enterprise||"' AND t4.oocql001='9' AND t4.oocql002=oogc012 AND t4.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t5 ON t5.oocqlent='"||g_enterprise||"' AND t5.oocql001='10' AND t5.oocql002=oogc013 AND t5.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t6 ON t6.oocqlent='"||g_enterprise||"' AND t6.oocql001='11' AND t6.oocql002=oogc014 AND t6.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t7 ON t7.oofaent='"||g_enterprise||"' AND t7.oofa002='2' AND t7.oofa003=oogcmodid  ",
               " LEFT JOIN oofa_t t8 ON t8.oofaent='"||g_enterprise||"' AND t8.oofa002='2' AND t8.oofa003=oogcownid  ",
               " LEFT JOIN ooefl_t t9 ON t9.ooeflent='"||g_enterprise||"' AND t9.ooefl001=oogcowndp AND t9.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t10 ON t10.oofaent='"||g_enterprise||"' AND t10.oofa002='2' AND t10.oofa003=oogccrtid  ",
               " LEFT JOIN ooefl_t t11 ON t11.ooeflent='"||g_enterprise||"' AND t11.ooefl001=oogccrtdp AND t11.ooefl002='"||g_dlang||"' ",
 
               " WHERE oogcent= ? AND oogc001=? AND oogc002=? AND oogc015=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("oogc_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF aooi420_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY oogc_t.oogc003"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE aooi420_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR aooi420_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_oogc_m.oogc001,g_oogc_m.oogc002,g_oogc_m.oogc015
                                               
      FOREACH b_fill_cs INTO g_oogc_d[l_ac].oogcstus,g_oogc_d[l_ac].oogc003,g_oogc_d[l_ac].oogc004,g_oogc_d[l_ac].oogc005, 
          g_oogc_d[l_ac].oogc008,g_oogc_d[l_ac].oogc006,g_oogc_d[l_ac].oogc007,g_oogc_d[l_ac].oogc016, 
          g_oogc_d[l_ac].oogc017,g_oogc_d[l_ac].oogc009,g_oogc_d[l_ac].oogc010,g_oogc_d[l_ac].oogc011, 
          g_oogc_d[l_ac].oogc012,g_oogc_d[l_ac].oogc013,g_oogc_d[l_ac].oogc014,g_oogc1_info_d[l_ac].oogc003, 
          g_oogc1_info_d[l_ac].oogcmodid,g_oogc1_info_d[l_ac].oogcmoddt,g_oogc1_info_d[l_ac].oogcownid, 
          g_oogc1_info_d[l_ac].oogcowndp,g_oogc1_info_d[l_ac].oogccrtid,g_oogc1_info_d[l_ac].oogccrtdp, 
          g_oogc1_info_d[l_ac].oogccrtdt,g_oogc_d[l_ac].oogc004_desc,g_oogc_d[l_ac].oogc010_desc,g_oogc_d[l_ac].oogc011_desc, 
          g_oogc_d[l_ac].oogc012_desc,g_oogc_d[l_ac].oogc013_desc,g_oogc_d[l_ac].oogc014_desc,g_oogc1_info_d[l_ac].oogcmodid_desc, 
          g_oogc1_info_d[l_ac].oogcownid_desc,g_oogc1_info_d[l_ac].oogcowndp_desc,g_oogc1_info_d[l_ac].oogccrtid_desc, 
          g_oogc1_info_d[l_ac].oogccrtdp_desc
                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_oogc1_info_d[l_ac].oogcownid_desc = cl_get_username(g_oogc1_info_d[l_ac].oogcownid)
      #LET g_oogc1_info_d[l_ac].oogcowndp_desc = cl_get_deptname(g_oogc1_info_d[l_ac].oogcowndp)
      #LET g_oogc1_info_d[l_ac].oogccrtid_desc = cl_get_username(g_oogc1_info_d[l_ac].oogccrtid)
      #LET g_oogc1_info_d[l_ac].oogccrtdp_desc = cl_get_deptname(g_oogc1_info_d[l_ac].oogccrtdp)
      #LET g_oogc1_info_d[l_ac].oogcmodid_desc = cl_get_username(g_oogc1_info_d[l_ac].oogcmodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_oogc_d.deleteElement(g_oogc_d.getLength())
      CALL g_oogc1_info_d.deleteElement(g_oogc1_info_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE aooi420_pb   
   
END FUNCTION
