   OR g_oogc_m.oogc002 IS NULL
   OR g_oogc_m.oogc015 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE aooi420_master_referesh USING g_oogc_m.oogc001,g_oogc_m.oogc002,g_oogc_m.oogc015 INTO g_oogc_m.oogc001, 
       g_oogc_m.oogc015,g_oogc_m.oogc002,g_oogc_m.oogc001_desc
   
   CALL aooi420_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN aooi420_cl USING g_enterprise,g_oogc_m.oogc001
                                                       ,g_oogc_m.oogc002
                                                       ,g_oogc_m.oogc015
 
   IF STATUS THEN
      CALL cl_err("OPEN aooi420_cl:", STATUS, 1)
      CLOSE aooi420_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aooi420_cl INTO g_oogc_m.oogc001,g_oogc_m.oogc001_desc,g_oogc_m.oogc015,g_oogc_m.oogc002
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_oogc_m.oogc001,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL aooi420_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM oogc_t WHERE oogcent = g_enterprise AND oogc001 = g_oogc_m.oogc001
                                                               AND oogc002 = g_oogc_m.oogc002
                                                               AND oogc015 = g_oogc_m.oogc015
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("oogc_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_oogc_d.clear() 
      CALL g_oogc1_info_d.clear()       
 
     
      CALL aooi420_ui_browser_refresh()  
      CALL aooi420_ui_headershow()  
      CALL aooi420_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL aooi420_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL aooi420_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE aooi420_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_oogc_m.oogc001,'D')
    
END FUNCTION
