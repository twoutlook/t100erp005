  <section id="aooi420.detail_reproduce" ver="3" status="" src="s">
    <![CDATA[#+ 單身自動複製
PRIVATE FUNCTION aooi420_detail_reproduce()
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE oogc_t.*
 
 
   #add-point:delete段define
   {<point name="detail_reproduce.define"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE aooi420_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE aooi420_detail AS ",
                "SELECT * FROM oogc_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO aooi420_detail SELECT * FROM oogc_t 
                                         WHERE oogcent = g_enterprise AND oogc001 = g_oogc001_t
                                         AND oogc002 = g_oogc002_t
                                         AND oogc015 = g_oogc015_t
 
   
   #將key修正為調整後   
   UPDATE aooi420_detail 
      #更新key欄位
      SET oogc001 = g_oogc_m.oogc001
          , oogc002 = g_oogc_m.oogc002
          , oogc015 = g_oogc_m.oogc015
 
      #更新共用欄位
      #此段落由子樣板a13產生
       , oogcownid = g_user
       , oogcowndp = g_dept
       , oogccrtid = g_user
       , oogccrtdp = g_dept 
       , oogccrtdt = ld_date
       , oogcmodid = "" 
       , oogcmoddt = "" 
      #, oogcstus = "Y"
 
 
                                       
  
   #將資料塞回原table   
   INSERT INTO oogc_t SELECT * FROM aooi420_detail
   
   IF SQLCA.sqlcode THEN
      CALL cl_err("reproduce",SQLCA.sqlcode,1)
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE aooi420_detail
   
   #add-point:單身複製後1
   {<point name="detail_reproduce.body.table1.a_insert"/>}
   #end add-point
 
 
   
 
   
   #多語言複製段落
   
   
   CALL s_transaction_end('Y','0')
   
   #已新增完, 調整資料內容(修改時使用)
   LET g_oogc001_t = g_oogc_m.oogc001
   LET g_oogc002_t = g_oogc_m.oogc002
   LET g_oogc015_t = g_oogc_m.oogc015
 
   
   DROP TABLE aooi420_detail
   
END FUNCTION
