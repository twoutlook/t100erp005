   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE aooi170_detail AS ",
                "SELECT * FROM ooao_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
