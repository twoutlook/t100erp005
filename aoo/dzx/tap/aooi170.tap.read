      CALL aooi170_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM ooao_t WHERE ooaoent = g_enterprise AND ooao001 = g_ooao_m.ooao001
                                                               AND ooao004 = g_ooao_m.ooao004
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("ooao_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_ooao_d.clear() 
      CALL g_ooao2_d.clear()       
 
     
      CALL aooi170_ui_browser_refresh()  
      CALL aooi170_ui_headershow()  
      CALL aooi170_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL aooi170_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL aooi170_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE aooi170_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_ooao_m.ooao001,'D')
    
