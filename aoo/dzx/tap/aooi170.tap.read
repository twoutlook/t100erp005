  <section id="aooi170.main" ver="2" status="" src="s">
    <![CDATA[#+ 此段落由子樣板a26產生
#OPTIONS SHORT CIRCUIT
#+ 作業開始 
MAIN
   #add-point:main段define
   {<point name="main.define"/>}
   #end add-point   
 
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("aoo","")
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT ooao001,'',ooao004", 
                      " FROM ooao_t",
                      " WHERE ooaoent= ? AND ooao001=? AND ooao004=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE aooi170_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE ooao001,ooao004,t1.ooall004",
               " FROM ooao_t",
                              " LEFT JOIN ooall_t t1 ON t1.ooallent='"||g_enterprise||"' AND t1.ooall001='1' AND t1.ooall002=ooao001 AND t1.ooall003='"||g_dlang||"' ",
 
               " WHERE ooaoent = '" ||g_enterprise|| "' AND ooao001 = ? AND ooao004 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE aooi170_master_referesh FROM g_sql
 
 
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_aooi170 WITH FORM cl_ap_formpath("aoo",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL aooi170_init()   
 
      #進入選單 Menu (="N")
      CALL aooi170_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_aooi170
      
   END IF 
   
   CLOSE aooi170_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
   
END MAIN
 
 
]]>
  </section>
  <section id="aooi170.modify" ver="2" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aooi170_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_ooao_m.ooao001 IS NULL
   OR g_ooao_m.ooao004 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE aooi170_master_referesh USING g_ooao_m.ooao001,g_ooao_m.ooao004 INTO g_ooao_m.ooao001,g_ooao_m.ooao004, 
       g_ooao_m.ooao001_desc
 
   ERROR ""
  
   LET g_ooao001_t = g_ooao_m.ooao001
   LET g_ooao004_t = g_ooao_m.ooao004
 
   CALL s_transaction_begin()
   
   OPEN aooi170_cl USING g_enterprise,g_ooao_m.ooao001
                                                       ,g_ooao_m.ooao004
 
   IF STATUS THEN
      CALL cl_err("OPEN aooi170_cl:", STATUS, 1)
      CLOSE aooi170_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aooi170_cl INTO g_ooao_m.ooao001,g_ooao_m.ooao001_desc,g_ooao_m.ooao004
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_ooao_m.ooao001,SQLCA.sqlcode,0)
      CLOSE aooi170_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL aooi170_show()
   WHILE TRUE
      LET g_ooao001_t = g_ooao_m.ooao001
      LET g_ooao004_t = g_ooao_m.ooao004
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL aooi170_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_ooao_m.* = g_ooao_m_t.*
         CALL aooi170_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_ooao_m.ooao001 != g_ooao001_t 
      OR g_ooao_m.ooao004 != g_ooao004_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE ooao_t SET ooao001 = g_ooao_m.ooao001
                                       , ooao004 = g_ooao_m.ooao004
 
          WHERE ooaoent = g_enterprise AND ooao001 = g_ooao001_t
            AND ooao004 = g_ooao004_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("ooao_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("ooao_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_ooao_m.ooao001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE aooi170_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_ooao_m.ooao001,'U')
 
   CALL aooi170_b_fill("1=1")
   
END FUNCTION   
]]>
  </section>
