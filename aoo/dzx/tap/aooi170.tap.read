  <point name="function.aooi170_ooao004_chk" order="4" ver="3" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION aooi170_ooao004_chk()
DEFINE l_ooao004   STRING
DEFINE l_length    LIKE type_t.num5
DEFINE l_i         LIKE type_t.num5     
DEFINE l_year      STRING
DEFINE l_mm        STRING
     LET l_ooao004 = g_ooao_m.ooao004
     LET l_length = l_ooao004.getLength()
     IF l_length != 6 THEN
        RETURN FALSE
     END IF
     
     FOR l_i = 1 TO 6 
        IF g_ooao_m.ooao004[l_i,l_i] NOT MATCHES '[0123456789]' THEN
           RETURN FALSE
        END IF 
     END FOR
     
     LET l_year = g_ooao_m.ooao004[1,4]
     LET l_mm = g_ooao_m.ooao004[5,6]
     IF l_year <= 0 THEN
        RETURN FALSE
     END IF
     
     IF l_mm > 12 OR l_mm <= 0 THEN
        RETURN FALSE
     END IF
     RETURN TRUE
END FUNCTION]]>
  </point>
