  <section id="aooi170.delete" ver="2" status="" src="s">
    <![CDATA[#+ 資料刪除
PRIVATE FUNCTION aooi170_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_ooao_m.ooao001 IS NULL
   OR g_ooao_m.ooao004 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE aooi170_master_referesh USING g_ooao_m.ooao001,g_ooao_m.ooao004 INTO g_ooao_m.ooao001,g_ooao_m.ooao004, 
       g_ooao_m.ooao001_desc
   
   CALL aooi170_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN aooi170_cl USING g_enterprise,g_ooao_m.ooao001
                                                       ,g_ooao_m.ooao004
 
   IF STATUS THEN
      CALL cl_err("OPEN aooi170_cl:", STATUS, 1)
      CLOSE aooi170_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aooi170_cl INTO g_ooao_m.ooao001,g_ooao_m.ooao001_desc,g_ooao_m.ooao004
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_ooao_m.ooao001,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
