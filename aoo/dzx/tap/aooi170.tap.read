  <section id="aooi170.b_fill" ver="3" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aooi170_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_ooao_d.clear()
   CALL g_ooao2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE ooao002,ooao014,ooao003,ooao015,ooao005,ooao006,ooao007,ooao008,ooao009, 
       ooao010,ooao011,ooao012,ooao013,ooaostus,ooao002,ooao003,ooaoownid,ooaoowndp,ooaocrtid,ooaocrtdp, 
       ooaocrtdt,ooaomodid,ooaomoddt,t1.ooail003 ,t2.ooail003 ,t3.ooag011 ,t4.ooefl003 ,t5.ooag011 , 
       t6.ooefl003 ,t7.ooag011 FROM ooao_t",   
               "",
               
                              " LEFT JOIN ooail_t t1 ON t1.ooailent='"||g_enterprise||"' AND t1.ooail001=ooao002 AND t1.ooail002='"||g_dlang||"' ",
               " LEFT JOIN ooail_t t2 ON t2.ooailent='"||g_enterprise||"' AND t2.ooail001=ooao003 AND t2.ooail002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent='"||g_enterprise||"' AND t3.ooag001=ooaoownid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=ooaoowndp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent='"||g_enterprise||"' AND t5.ooag001=ooaocrtid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent='"||g_enterprise||"' AND t6.ooefl001=ooaocrtdp AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent='"||g_enterprise||"' AND t7.ooag001=ooaomodid  ",
 
               " WHERE ooaoent= ? AND ooao001=? AND ooao004=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("ooao_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF aooi170_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY ooao_t.ooao002,ooao_t.ooao003"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE aooi170_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR aooi170_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_ooao_m.ooao001,g_ooao_m.ooao004
                                               
      FOREACH b_fill_cs INTO g_ooao_d[l_ac].ooao002,g_ooao_d[l_ac].ooao014,g_ooao_d[l_ac].ooao003,g_ooao_d[l_ac].ooao015, 
          g_ooao_d[l_ac].ooao005,g_ooao_d[l_ac].ooao006,g_ooao_d[l_ac].ooao007,g_ooao_d[l_ac].ooao008, 
          g_ooao_d[l_ac].ooao009,g_ooao_d[l_ac].ooao010,g_ooao_d[l_ac].ooao011,g_ooao_d[l_ac].ooao012, 
          g_ooao_d[l_ac].ooao013,g_ooao_d[l_ac].ooaostus,g_ooao2_d[l_ac].ooao002,g_ooao2_d[l_ac].ooao003, 
          g_ooao2_d[l_ac].ooaoownid,g_ooao2_d[l_ac].ooaoowndp,g_ooao2_d[l_ac].ooaocrtid,g_ooao2_d[l_ac].ooaocrtdp, 
          g_ooao2_d[l_ac].ooaocrtdt,g_ooao2_d[l_ac].ooaomodid,g_ooao2_d[l_ac].ooaomoddt,g_ooao_d[l_ac].ooao002_desc, 
          g_ooao_d[l_ac].ooao003_desc,g_ooao2_d[l_ac].ooaoownid_desc,g_ooao2_d[l_ac].ooaoowndp_desc, 
          g_ooao2_d[l_ac].ooaocrtid_desc,g_ooao2_d[l_ac].ooaocrtdp_desc,g_ooao2_d[l_ac].ooaomodid_desc 

                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_ooao2_d[l_ac].ooaoownid_desc = cl_get_username(g_ooao2_d[l_ac].ooaoownid)
      #LET g_ooao2_d[l_ac].ooaoowndp_desc = cl_get_deptname(g_ooao2_d[l_ac].ooaoowndp)
      #LET g_ooao2_d[l_ac].ooaocrtid_desc = cl_get_username(g_ooao2_d[l_ac].ooaocrtid)
      #LET g_ooao2_d[l_ac].ooaocrtdp_desc = cl_get_deptname(g_ooao2_d[l_ac].ooaocrtdp)
      #LET g_ooao2_d[l_ac].ooaomodid_desc = cl_get_username(g_ooao2_d[l_ac].ooaomodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_ooao_d.deleteElement(g_ooao_d.getLength())
      CALL g_ooao2_d.deleteElement(g_ooao2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE aooi170_pb   
   
END FUNCTION
]]>
  </section>
