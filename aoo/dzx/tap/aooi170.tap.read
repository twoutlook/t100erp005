  <section id="aooi170.input.body" ver="4" status="" src="s">
    <![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_ooao_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂單身ACTION
         
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_ooao_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL aooi170_b_fill(g_wc2) 
            IF g_rec_b != 0 THEN
               CALL fgl_set_arr_curr(l_ac)
            END IF
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN aooi170_cl USING g_enterprise,
                                               g_ooao_m.ooao001
                                               ,g_ooao_m.ooao004
 
                                               
               IF STATUS THEN
                  CALL cl_err("OPEN aooi170_cl:", STATUS, 1)
                  CLOSE aooi170_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_ooao_d[l_ac].ooao002 IS NOT NULL
               AND g_ooao_d[l_ac].ooao003 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_ooao_d_t.* = g_ooao_d[l_ac].*  #BACKUP
               LET g_ooao_d_o.* = g_ooao_d[l_ac].*  #BACKUP
               CALL aooi170_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL aooi170_set_no_entry_b(l_cmd)
               OPEN aooi170_bcl USING g_enterprise,g_ooao_m.ooao001,
                                                g_ooao_m.ooao004,
 
                                                g_ooao_d_t.ooao002
                                                ,g_ooao_d_t.ooao003
 
               IF STATUS THEN
                  CALL cl_err("OPEN aooi170_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aooi170_bcl INTO g_ooao_d[l_ac].ooao002,g_ooao_d[l_ac].ooao014,g_ooao_d[l_ac].ooao003, 
                      g_ooao_d[l_ac].ooao015,g_ooao_d[l_ac].ooao005,g_ooao_d[l_ac].ooao006,g_ooao_d[l_ac].ooao007, 
                      g_ooao_d[l_ac].ooao008,g_ooao_d[l_ac].ooao009,g_ooao_d[l_ac].ooao010,g_ooao_d[l_ac].ooao011, 
                      g_ooao_d[l_ac].ooao012,g_ooao_d[l_ac].ooao013,g_ooao_d[l_ac].ooaostus,g_ooao2_d[l_ac].ooao002, 
                      g_ooao2_d[l_ac].ooao003,g_ooao2_d[l_ac].ooaoownid,g_ooao2_d[l_ac].ooaoowndp,g_ooao2_d[l_ac].ooaocrtid, 
                      g_ooao2_d[l_ac].ooaocrtdp,g_ooao2_d[l_ac].ooaocrtdt,g_ooao2_d[l_ac].ooaomodid, 
                      g_ooao2_d[l_ac].ooaomoddt
                  IF SQLCA.sqlcode THEN
                      CALL cl_err(g_ooao_d_t.ooao002,SQLCA.sqlcode,1)
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL aooi170_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
        
         BEFORE INSERT
            
            INITIALIZE g_ooao_d_t.* TO NULL
            INITIALIZE g_ooao_d_o.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_ooao_d[l_ac].* TO NULL
            
            #公用欄位預設值
            #此段落由子樣板a14產生    
      LET g_ooao2_d[l_ac].ooaoownid = g_user
      LET g_ooao2_d[l_ac].ooaoowndp = g_dept
      LET g_ooao2_d[l_ac].ooaocrtid = g_user
      LET g_ooao2_d[l_ac].ooaocrtdp = g_dept 
      LET g_ooao2_d[l_ac].ooaocrtdt = cl_get_current()
      LET g_ooao2_d[l_ac].ooaomodid = ""
      LET g_ooao2_d[l_ac].ooaomoddt = ""
      LET g_ooao_d[l_ac].ooaostus = ""
 
  
            
            #一般欄位預設值
                  LET g_ooao_d[l_ac].ooao014 = "1"
      LET g_ooao_d[l_ac].ooao015 = "1"
      LET g_ooao_d[l_ac].ooao013 = "1"
 
            
            
            LET g_ooao_d_t.* = g_ooao_d[l_ac].*     #新輸入資料
            LET g_ooao_d_o.* = g_ooao_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL aooi170_set_entry_b(l_cmd)
            #add-point:set_entry_b後
            {<point name="input.body.before_insert.set_entry_b"/>}
            #end add-point
            CALL aooi170_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_ooao_d[li_reproduce_target].* = g_ooao_d[li_reproduce].*
               LET g_ooao2_d[li_reproduce_target].* = g_ooao2_d[li_reproduce].*
 
               LET g_ooao_d[g_ooao_d.getLength()].ooao002 = NULL
               LET g_ooao_d[g_ooao_d.getLength()].ooao003 = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
 
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM ooao_t 
             WHERE ooaoent = g_enterprise AND ooao001 = g_ooao_m.ooao001
               AND ooao004 = g_ooao_m.ooao004
 
               AND ooao002 = g_ooao_d[l_ac].ooao002
               AND ooao003 = g_ooao_d[l_ac].ooao003
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               CALL s_transaction_begin()
               #add-point:單身新增前
               {<point name="input.body.b_insert" mark="Y"/>}
               #end add-point
               INSERT INTO ooao_t
                           (ooaoent,
                            ooao001,ooao004,
                            ooao002,ooao003
                            ,ooao014,ooao015,ooao005,ooao006,ooao007,ooao008,ooao009,ooao010,ooao011,ooao012,ooao013,ooaostus,ooaoownid,ooaoowndp,ooaocrtid,ooaocrtdp,ooaocrtdt,ooaomodid,ooaomoddt) 
                     VALUES(g_enterprise,
                            g_ooao_m.ooao001,g_ooao_m.ooao004,
                            g_ooao_d[l_ac].ooao002,g_ooao_d[l_ac].ooao003
                            ,g_ooao_d[l_ac].ooao014,g_ooao_d[l_ac].ooao015,g_ooao_d[l_ac].ooao005,g_ooao_d[l_ac].ooao006, 
                                g_ooao_d[l_ac].ooao007,g_ooao_d[l_ac].ooao008,g_ooao_d[l_ac].ooao009, 
                                g_ooao_d[l_ac].ooao010,g_ooao_d[l_ac].ooao011,g_ooao_d[l_ac].ooao012, 
                                g_ooao_d[l_ac].ooao013,g_ooao_d[l_ac].ooaostus,g_ooao2_d[l_ac].ooaoownid, 
                                g_ooao2_d[l_ac].ooaoowndp,g_ooao2_d[l_ac].ooaocrtid,g_ooao2_d[l_ac].ooaocrtdp, 
                                g_ooao2_d[l_ac].ooaocrtdt,g_ooao2_d[l_ac].ooaomodid,g_ooao2_d[l_ac].ooaomoddt) 

               #add-point:單身新增中
               {<point name="input.body.m_insert"/>}
               #end add-point
               LET p_cmd = 'u'
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_ooao_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("ooao_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR "INSERT O.K"
               LET g_rec_b=g_rec_b+1
               DISPLAY g_rec_b TO FORMONLY.cnt
            END IF
            
            #add-point:單身新增後
            {<point name="input.body.after_insert"/>}
            #end add-point
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_ooao_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_ooao_d.deleteElement(l_ac)
