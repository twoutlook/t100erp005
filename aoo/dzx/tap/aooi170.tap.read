    <![CDATA[#+ 資料新增
PRIVATE FUNCTION aooi170_insert()
   #add-point:insert段define
   {<point name="insert.define"/>}
   #end add-point    
   
   #add-point:insert段before
   {<point name="insert.before"/>}
   #end add-point    
   
   #清除相關資料
   CLEAR FORM                    
   CALL g_ooao_d.clear()
   CALL g_ooao2_d.clear()
 
 
   INITIALIZE g_ooao_m.* LIKE ooao_t.*             #DEFAULT 設定
   LET g_ooao001_t = NULL
   LET g_ooao004_t = NULL
 
   CALL s_transaction_begin()
   WHILE TRUE
     
      #單頭預設值
      
     
      #add-point:單頭預設值
      {<point name="insert.default"/>}
      #end add-point 
 
      CALL aooi170_input("a")
      
      #add-point:單頭輸入後
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_ooao_m.* = g_ooao_m_t.*
         CALL aooi170_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      CALL g_ooao_d.clear()
      CALL g_ooao2_d.clear()
 
      
      #add-point:單頭輸入後2
      {<point name="insert.after_insert2"/>}
      #end add-point
 
      LET g_rec_b = 0
      EXIT WHILE
      
   END WHILE
   
   #將新增的資料併入搜尋條件中
