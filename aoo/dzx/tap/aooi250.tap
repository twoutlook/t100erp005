  <section id="aooi250.b_fill" ver="6" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aooi250_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2           STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE oocastus,ooca001,'','',ooca002,ooca003,ooca004,ooca005,ooca006,ooca007, 
       ooca001,oocamodid,'',oocamoddt,oocaownid,'',oocaowndp,'',oocacrtid,'',oocacrtdp,'',oocacrtdt FROM ooca_t", 

               " LEFT JOIN oocal_t ON ooca001 = oocal001 AND oocal002 = '",g_lang,"'",
               " WHERE oocaent= ? AND 1=1 AND ", p_wc2 
    
   LET g_sql = g_sql, #,cl_get_extra_cond('zzuser', 'zzgrup') 
                      " ORDER BY ooca_t.ooca001"
  
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
  
   PREPARE aooi250_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aooi250_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_ooca_d.clear()
   CALL g_ooca1_info_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_ooca_d[l_ac].oocastus,g_ooca_d[l_ac].ooca001,g_ooca_d[l_ac].oocal003,g_ooca_d[l_ac].oocal004, 
       g_ooca_d[l_ac].ooca002,g_ooca_d[l_ac].ooca003,g_ooca_d[l_ac].ooca004,g_ooca_d[l_ac].ooca005,g_ooca_d[l_ac].ooca006, 
       g_ooca_d[l_ac].ooca007,g_ooca1_info_d[l_ac].ooca001,g_ooca1_info_d[l_ac].oocamodid,g_ooca1_info_d[l_ac].oocamodid_desc, 
       g_ooca1_info_d[l_ac].oocamoddt,g_ooca1_info_d[l_ac].oocaownid,g_ooca1_info_d[l_ac].oocaownid_desc, 
       g_ooca1_info_d[l_ac].oocaowndp,g_ooca1_info_d[l_ac].oocaowndp_desc,g_ooca1_info_d[l_ac].oocacrtid, 
       g_ooca1_info_d[l_ac].oocacrtid_desc,g_ooca1_info_d[l_ac].oocacrtdp,g_ooca1_info_d[l_ac].oocacrtdp_desc, 
       g_ooca1_info_d[l_ac].oocacrtdt
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL aooi250_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec THEN
      IF g_error_show = 1 THEN
         CALL cl_err("ooca_t",9035,1)
      END IF
   END IF 
   LET g_error_show = 0
   
 
  
   
   CALL g_ooca_d.deleteElement(g_ooca_d.getLength())   
   CALL g_ooca1_info_d.deleteElement(g_ooca1_info_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_ooca_d.getLength()
      LET g_ooca1_info_d[l_ac].ooca001 = g_ooca_d[l_ac].ooca001 
 
   END FOR
   
   IF g_cnt > g_ooca_d.getLength() THEN
      LET l_ac = g_ooca_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = g_ooca_d.getLength() 
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE aooi250_pb
   
END FUNCTION
