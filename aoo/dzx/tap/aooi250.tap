  <point name="init.define" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[   DEFINE ls_values       STRING             #ComboBox values
   DEFINE ls_items        STRING             #ComboBox items
   DEFINE l_gzcb002       LIKE gzcb_t.gzcb002
   DEFINE l_gzcbl004      LIKE gzcbl_t.gzcbl004]]>
  </point>
  <point name="init.init" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[   LET ls_values = NULL
   LET ls_items = NULL 
   LET l_gzcb002 = NULL
   LET l_gzcbl004 = NULL
   DECLARE gzcb_001_cs CURSOR FOR  
    SELECT gzcb002,gzcbl004 
      FROM gzcb_t,gzcbl_t
     WHERE gzcb001 = gzcbl001
       AND gzcb002 = gzcbl002
       AND gzcb001 = '5'
       AND gzcbl003 = g_dlang
     ORDER BY gzcb001
   FOREACH gzcb_001_cs INTO l_gzcb002,l_gzcbl004
      LET ls_values = ls_values CLIPPED ,",",l_gzcb002
      LET ls_items = ls_items CLIPPED ,",",l_gzcb002 CLIPPED,':',l_gzcbl004
   END FOREACH
   CALL cl_set_combo_items("ooca_t.ooca003",ls_values,ls_items)
   
   LET ls_values = NULL
   LET ls_items = NULL 
   LET l_gzcb002 = NULL
   LET l_gzcbl004 = NULL
   DECLARE gzcb_002_cs CURSOR FOR  
    SELECT gzcb002,gzcbl004 
      FROM gzcb_t,gzcbl_t
     WHERE gzcb001 = gzcbl001
       AND gzcb002 = gzcbl002
       AND gzcb001 = '12'
       AND gzcbl003 = g_dlang
     ORDER BY gzcb001
   FOREACH gzcb_002_cs INTO l_gzcb002,l_gzcbl004
      LET ls_values = ls_values CLIPPED ,",",l_gzcb002
      LET ls_items = ls_items CLIPPED ,",",l_gzcb002 CLIPPED,':',l_gzcbl004
   END FOREACH

   CALL cl_set_combo_items("ooca_t.ooca004",ls_values,ls_items) ]]>
  </point>
