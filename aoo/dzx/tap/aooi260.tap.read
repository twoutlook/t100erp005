  <section id="aooi260.b_fill" ver="8" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aooi260_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2    STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE ooccstus,oocc002,oocc001,oocc004,oocc003,oocc001,oocc003,ooccmodid,ooccmoddt, 
       ooccownid,ooccowndp,oocccrtid,oocccrtdp,oocccrtdt ,t1.oocal003 ,t2.oocal003 ,t3.ooag011 ,t4.ooag011 , 
       t5.ooefl003 ,t6.ooag011 ,t7.ooefl003 FROM oocc_t",
               "",
                              " LEFT JOIN oocal_t t1 ON t1.oocalent='"||g_enterprise||"' AND t1.oocal001=oocc001 AND t1.oocal002='"||g_dlang||"' ",
               " LEFT JOIN oocal_t t2 ON t2.oocalent='"||g_enterprise||"' AND t2.oocal001=oocc003 AND t2.oocal002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent='"||g_enterprise||"' AND t3.ooag001=ooccmodid  ",
               " LEFT JOIN ooag_t t4 ON t4.ooagent='"||g_enterprise||"' AND t4.ooag001=ooccownid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=ooccowndp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent='"||g_enterprise||"' AND t6.ooag001=oocccrtid  ",
               " LEFT JOIN ooefl_t t7 ON t7.ooeflent='"||g_enterprise||"' AND t7.ooefl001=oocccrtdp AND t7.ooefl002='"||g_dlang||"' ",
 
               " WHERE ooccent= ? AND 1=1 AND ", p_wc2 
    
   LET g_sql = g_sql, cl_sql_add_filter("oocc_t"),
                      " ORDER BY oocc_t.oocc001,oocc_t.oocc003"
   
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE aooi260_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aooi260_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_oocc_d.clear()
   CALL g_oocc1_info_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_oocc_d[l_ac].ooccstus,g_oocc_d[l_ac].oocc002,g_oocc_d[l_ac].oocc001,g_oocc_d[l_ac].oocc004, 
       g_oocc_d[l_ac].oocc003,g_oocc1_info_d[l_ac].oocc001,g_oocc1_info_d[l_ac].oocc003,g_oocc1_info_d[l_ac].ooccmodid, 
       g_oocc1_info_d[l_ac].ooccmoddt,g_oocc1_info_d[l_ac].ooccownid,g_oocc1_info_d[l_ac].ooccowndp, 
       g_oocc1_info_d[l_ac].oocccrtid,g_oocc1_info_d[l_ac].oocccrtdp,g_oocc1_info_d[l_ac].oocccrtdt, 
       g_oocc_d[l_ac].oocc001_desc,g_oocc_d[l_ac].oocc003_desc,g_oocc1_info_d[l_ac].ooccmodid_desc,g_oocc1_info_d[l_ac].ooccownid_desc, 
       g_oocc1_info_d[l_ac].ooccowndp_desc,g_oocc1_info_d[l_ac].oocccrtid_desc,g_oocc1_info_d[l_ac].oocccrtdp_desc 

      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL aooi260_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec THEN
      IF g_error_show = 1 THEN
         CALL cl_err("oocc_t",9035,1)
      END IF
   END IF 
   LET g_error_show = 0
   
 
   
   CALL g_oocc_d.deleteElement(g_oocc_d.getLength())   
   CALL g_oocc1_info_d.deleteElement(g_oocc1_info_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_oocc_d.getLength()
      LET g_oocc1_info_d[l_ac].oocc001 = g_oocc_d[l_ac].oocc001 
      LET g_oocc1_info_d[l_ac].oocc003 = g_oocc_d[l_ac].oocc003 
 
   END FOR
   
   IF g_cnt > g_oocc_d.getLength() THEN
      LET l_ac = g_oocc_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_oocc_d.getLength()
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE aooi260_pb
   
END FUNCTION
]]>
  </section>
