  <section id="aooi260.modify" ver="9" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aooi260_modify()
   DEFINE  l_cmd                  LIKE type_t.chr1
   DEFINE  l_ac_t                 LIKE type_t.num5                #未取消的ARRAY CNT 
   DEFINE  l_n                    LIKE type_t.num5                #檢查重複用  
   DEFINE  l_cnt                  LIKE type_t.num5                #檢查重複用  
   DEFINE  l_lock_sw              LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert         LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete         LIKE type_t.num5                #可刪除否  
   DEFINE  l_count                LIKE type_t.num5
   DEFINE  l_i                    LIKE type_t.num5
   DEFINE  ls_return              STRING
   DEFINE  l_var_keys             DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys           DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                 DYNAMIC ARRAY OF STRING
   DEFINE  l_fields               DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak         DYNAMIC ARRAY OF STRING
   DEFINE  li_reproduce           LIKE type_t.num5
   DEFINE  li_reproduce_target    LIKE type_t.num5
   DEFINE  lb_reproduce           BOOLEAN
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point 
   
   LET g_action_choice = ""
   
   LET g_qryparam.state = "i"
 
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   
   #add-point:modify開始前
   {<point name="modify.define_sql" mark="Y"/>}
   #end add-point
   
   LET INT_FLAG = FALSE
   LET lb_reproduce = FALSE
 
   #add-point:modify段修改前
   {<point name="modify.before_input"/>}
   #end add-point
 
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #Page1 預設值產生於此處
      INPUT ARRAY g_oocc_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_detail_cnt,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_oocc_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL aooi260_b_fill(g_wc2)
            LET g_detail_cnt = g_oocc_d.getLength()
         
         BEFORE ROW
            #add-point:modify段before row
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_cmd = ''
            LET l_ac = g_detail_idx
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            DISPLAY g_oocc_d.getLength() TO FORMONLY.cnt
         
            CALL s_transaction_begin()
            LET g_detail_cnt = g_oocc_d.getLength()
            
            IF g_detail_cnt >= l_ac 
               AND g_oocc_d[l_ac].oocc001 IS NOT NULL
               AND g_oocc_d[l_ac].oocc003 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_oocc_d_t.* = g_oocc_d[l_ac].*  #BACKUP
               LET g_oocc_d_o.* = g_oocc_d[l_ac].*  #BACKUP
               IF NOT aooi260_lock_b("oocc_t") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aooi260_bcl INTO g_oocc_d[l_ac].ooccstus,g_oocc_d[l_ac].oocc002,g_oocc_d[l_ac].oocc001, 
                      g_oocc_d[l_ac].oocc004,g_oocc_d[l_ac].oocc003,g_oocc1_info_d[l_ac].oocc001,g_oocc1_info_d[l_ac].oocc003, 
                      g_oocc1_info_d[l_ac].ooccmodid,g_oocc1_info_d[l_ac].ooccmoddt,g_oocc1_info_d[l_ac].ooccownid, 
                      g_oocc1_info_d[l_ac].ooccowndp,g_oocc1_info_d[l_ac].oocccrtid,g_oocc1_info_d[l_ac].oocccrtdp, 
                      g_oocc1_info_d[l_ac].oocccrtdt
                  IF SQLCA.sqlcode THEN
                     CALL cl_err(g_oocc_d_t.oocc001,SQLCA.sqlcode,1)
                     LET l_lock_sw = "Y"
                  END IF
                  
                  CALL aooi260_detail_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
        
         BEFORE INSERT
            
            CALL s_transaction_begin()
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_oocc_d_t.* TO NULL
            INITIALIZE g_oocc_d_o.* TO NULL
            INITIALIZE g_oocc_d[l_ac].* TO NULL 
            #公用欄位給值(單身)
            #此段落由子樣板a14產生    
      LET g_oocc1_info_d[l_ac].ooccownid = g_user
      LET g_oocc1_info_d[l_ac].ooccowndp = g_dept
      LET g_oocc1_info_d[l_ac].oocccrtid = g_user
      LET g_oocc1_info_d[l_ac].oocccrtdp = g_dept 
      LET g_oocc1_info_d[l_ac].oocccrtdt = cl_get_current()
      LET g_oocc1_info_d[l_ac].ooccmodid = ""
      LET g_oocc1_info_d[l_ac].ooccmoddt = ""
      LET g_oocc_d[l_ac].ooccstus = ""
 
 
            
            LET g_oocc_d_t.* = g_oocc_d[l_ac].*     #新輸入資料
            LET g_oocc_d_o.* = g_oocc_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL aooi260_set_entry_b("a")
            CALL aooi260_set_no_entry_b("a")
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_oocc_d[li_reproduce_target].* = g_oocc_d[li_reproduce].*
               LET g_oocc1_info_d[li_reproduce_target].* = g_oocc1_info_d[li_reproduce].*
 
               LET g_oocc_d[g_oocc_d.getLength()].oocc001 = NULL
               LET g_oocc_d[g_oocc_d.getLength()].oocc003 = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM oocc_t 
             WHERE ooccent = g_enterprise AND oocc001 = g_oocc_d[l_ac].oocc001
                                       AND oocc003 = g_oocc_d[l_ac].oocc003
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前
               {<point name="input.body.b_insert"/>}
               #end add-point
            
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_oocc_d[g_detail_idx].oocc001
               LET gs_keys[2] = g_oocc_d[g_detail_idx].oocc003
               CALL aooi260_insert_b('oocc_t',gs_keys,"'1'")
                           
               #add-point:單身新增後
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_oocc_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("oocc_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL aooi260_b_fill(g_wc2)
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert2"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR 'INSERT O.K'
               LET g_detail_cnt = g_detail_cnt + 1
               
               LET g_wc2 = g_wc2, " OR (oocc001 = '", g_oocc_d[l_ac].oocc001, "' "
                                  ," AND oocc003 = '", g_oocc_d[l_ac].oocc003, "' "
 
                                  ,")"
            END IF                
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_oocc_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_oocc_d.deleteElement(l_ac)
               NEXT FIELD oocc001
            END IF
            IF g_oocc_d[l_ac].oocc001 IS NOT NULL
               AND g_oocc_d_t.oocc003 IS NOT NULL
 
               THEN     
            
               #add-point:單身刪除ask前
               {<point name="input.body.b_delete_ask"/>}
               #end add-point   
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete" mark="Y"/>}
               #end add-point   
               
               DELETE FROM oocc_t
                WHERE ooccent = g_enterprise AND 
                      oocc001 = g_oocc_d_t.oocc001
                      AND oocc003 = g_oocc_d_t.oocc003
 
                      
               #add-point:單身刪除中
               {<point name="input.body.m_delete"/>}
               #end add-point  
                      
               IF SQLCA.sqlcode THEN
                  CALL cl_err("oocc_t",SQLCA.sqlcode,1)
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               ELSE
                  LET g_detail_cnt = g_detail_cnt-1
                  
                  #add-point:單身刪除後
                  {<point name="input.body.a_delete"/>}
                  #end add-point
                  CALL s_transaction_end('Y','0')
               END IF 
               CLOSE aooi260_bcl
               LET l_count = g_oocc_d.getLength()
            END IF 
                           INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_oocc_d[g_detail_idx].oocc001
               LET gs_keys[2] = g_oocc_d[g_detail_idx].oocc003
 
              
         AFTER DELETE 
            #add-point:單身刪除後2
            {<point name="input.body.after_delete"/>}
            #end add-point
                           CALL aooi260_delete_b('oocc_t',gs_keys,"'1'")
 
                  #此段落由子樣板a01產生
         BEFORE FIELD oocc002
            #add-point:BEFORE FIELD oocc002
            {<point name="input.b.page1.oocc002" />}
            #END add-point
 
