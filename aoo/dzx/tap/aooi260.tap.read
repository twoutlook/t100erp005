            {<point name="input.b.page1.oocc003" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE oocc003
            #add-point:ON CHANGE oocc003
            {<point name="input.g.page1.oocc003" />}
            #END add-point
 
 
                  #Ctrlp:input.c.page1.oocc002
         ON ACTION controlp INFIELD oocc002
            #add-point:ON ACTION controlp INFIELD oocc002
            {<point name="input.c.page1.oocc002" />}
            #END add-point
 
         #Ctrlp:input.c.page1.oocc001
         ON ACTION controlp INFIELD oocc001
            #add-point:ON ACTION controlp INFIELD oocc001
            {<point name="input.c.page1.oocc001" />}
            #END add-point
 
         #Ctrlp:input.c.page1.l_chr2
         ON ACTION controlp INFIELD l_chr2
            #add-point:ON ACTION controlp INFIELD l_chr2
            {<point name="input.c.page1.l_chr2" />}
            #END add-point
 
         #Ctrlp:input.c.page1.oocc004
         ON ACTION controlp INFIELD oocc004
            #add-point:ON ACTION controlp INFIELD oocc004
            {<point name="input.c.page1.oocc004" />}
            #END add-point
 
         #Ctrlp:input.c.page1.oocc003
         ON ACTION controlp INFIELD oocc003
            #add-point:ON ACTION controlp INFIELD oocc003
            {<point name="input.c.page1.oocc003" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_oocc_d[l_ac].* = g_oocc_d_t.*
               CLOSE aooi260_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_oocc_d[l_ac].oocc001,-263,1)
               LET g_oocc_d[l_ac].* = g_oocc_d_t.*
            ELSE
               
               #寫入修改者/修改日期資訊(單身)
               LET g_oocc1_info_d[l_ac].ooccmodid = g_user 
LET g_oocc1_info_d[l_ac].ooccmoddt = cl_get_current()
 
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               UPDATE oocc_t SET (ooccstus,oocc002,oocc001,oocc004,oocc003,ooccmodid,ooccmoddt,ooccownid, 
                   ooccowndp,oocccrtid,oocccrtdp,oocccrtdt) = (g_oocc_d[l_ac].ooccstus,g_oocc_d[l_ac].oocc002, 
                   g_oocc_d[l_ac].oocc001,g_oocc_d[l_ac].oocc004,g_oocc_d[l_ac].oocc003,g_oocc1_info_d[l_ac].ooccmodid, 
                   g_oocc1_info_d[l_ac].ooccmoddt,g_oocc1_info_d[l_ac].ooccownid,g_oocc1_info_d[l_ac].ooccowndp, 
                   g_oocc1_info_d[l_ac].oocccrtid,g_oocc1_info_d[l_ac].oocccrtdp,g_oocc1_info_d[l_ac].oocccrtdt) 

                WHERE ooccent = g_enterprise AND
                  oocc001 = g_oocc_d_t.oocc001 #項次   
                  AND oocc003 = g_oocc_d_t.oocc003  
 
                  
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
                  
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("oocc_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                    WHEN SQLCA.sqlcode #其他錯誤
                     CALL cl_err("oocc_t",SQLCA.sqlcode,1)  
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_oocc_d[g_detail_idx].oocc001
               LET gs_keys_bak[1] = g_oocc_d_t.oocc001
               LET gs_keys[2] = g_oocc_d[g_detail_idx].oocc003
               LET gs_keys_bak[2] = g_oocc_d_t.oocc003
               CALL aooi260_update_b('oocc_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
                     LET g_log1 = util.JSON.stringify(g_oocc_d_t)
                     LET g_log2 = util.JSON.stringify(g_oocc_d[l_ac])
                     IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                        CALL s_transaction_end('N','0')
                     END IF
               END CASE
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            CALL aooi260_unlock_b("oocc_t")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock
            
             #add-point:單身after row
            {<point name="input.body.a_row"/>}
            #end add-point
            
         AFTER INPUT
            #add-point:單身input後
            {<point name="input.body.a_input"/>}
            #end add-point
            
         ON ACTION controlo   
            CALL FGL_SET_ARR_CURR(g_oocc_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_oocc_d.getLength()+1
            
      END INPUT
      
 
      
      DISPLAY ARRAY g_oocc1_info_d TO s_detail1_info.*
         ATTRIBUTES(COUNT=g_detail_cnt)  
      
         BEFORE DISPLAY 
            CALL aooi260_b_fill(g_wc2)
            CALL FGL_SET_ARR_CURR(g_detail_idx)
      
         BEFORE ROW
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1_info")
            LET l_ac = g_detail_idx
            LET g_temp_idx = l_ac
            DISPLAY g_detail_idx TO FORMONLY.idx
            CALL cl_show_fld_cont() 
            
         #add-point:page2自定義行為
         {<point name="input.body1_info.action"/>}
         #end add-point
            
      END DISPLAY
 
      
      #add-point:before_more_input
      {<point name="input.more_input"/>}
      #end add-point
      
      BEFORE DIALOG 
         IF g_temp_idx > 0 THEN
            LET l_ac = g_temp_idx
            CALL DIALOG.setCurrentRow("s_detail1",l_ac)
            LET g_temp_idx = 1
         END IF
         LET g_curr_diag = ui.DIALOG.getCurrent()
         #add-point:before_dialog
         {<point name="input.before_dialog"/>}
         #end add-point
         CASE g_aw
            WHEN "s_detail1"
               NEXT FIELD ooccstus
            WHEN "s_detail1_info"
               NEXT FIELD oocc001_2
 
         END CASE
   
      ON ACTION accept
         ACCEPT DIALOG
      
      ON ACTION cancel
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION controlr
         CALL cl_show_req_fields()
 
      ON ACTION controlf
         CALL cl_set_focus_form(ui.Interface.getRootNode()) 
              RETURNING g_fld_name,g_frm_name 
         CALL cl_fldhelp(g_frm_name,g_fld_name,g_lang) 
           
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE DIALOG
   
   END DIALOG 
   
   #add-point:modify段修改後
   {<point name="modify.after_input"/>}
   #end add-point
 
   CLOSE aooi260_bcl
   CALL s_transaction_end('Y','0')
   
END FUNCTION
]]>
  </section>
