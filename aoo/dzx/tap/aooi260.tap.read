    <![CDATA[{<point name="global.memo" />}
 
IMPORT os
IMPORT util
#add-point:增加匯入項目
{<point name="global.import" />}
#end add-point
 
SCHEMA ds
 
GLOBALS "../../cfg/top_global.inc"
 
#add-point:增加匯入變數檔
{<point name="global.inc" />}
#end add-point
 
#單身 type 宣告
PRIVATE TYPE type_g_oocc_d RECORD
       ooccstus LIKE oocc_t.ooccstus, 
   oocc002 LIKE oocc_t.oocc002, 
   oocc001 LIKE oocc_t.oocc001, 
   oocc001_desc LIKE type_t.chr500, 
   l_chr2 LIKE type_t.chr80, 
   oocc004 LIKE oocc_t.oocc004, 
   oocc003 LIKE oocc_t.oocc003, 
   oocc003_desc LIKE type_t.chr500
       END RECORD
PRIVATE TYPE type_g_oocc1_info_d RECORD
       oocc001 LIKE oocc_t.oocc001, 
   oocc003 LIKE oocc_t.oocc003, 
   ooccmodid LIKE oocc_t.ooccmodid, 
   ooccmodid_desc LIKE type_t.chr500, 
   ooccmoddt DATETIME YEAR TO SECOND, 
   ooccownid LIKE oocc_t.ooccownid, 
   ooccownid_desc LIKE type_t.chr500, 
   ooccowndp LIKE oocc_t.ooccowndp, 
   ooccowndp_desc LIKE type_t.chr500, 
   oocccrtid LIKE oocc_t.oocccrtid, 
