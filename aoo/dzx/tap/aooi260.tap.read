               IF l_cmd = 'a' OR ( l_cmd = 'u' AND ( l_cmd = 'u' AND (g_oocc_d[l_ac].oocc001 != g_oocc_d_t.oocc001 OR g_oocc_d[l_ac].oocc003 != g_oocc_d_t.oocc003))) THEN 
