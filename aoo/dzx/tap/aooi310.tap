  <point name="function.aooi310_check_count" cite_std="N" status="" ver="1" src="s" new="Y" cite_ver="" order="1" mark_hard="N">
<![CDATA[# 檢查有效筆數
# 若小於256，回傳true
# 反之，回傳false
PUBLIC FUNCTION aooi310_check_count(p_oocq001)
DEFINE p_oocq001    LIKE oocq_t.oocq001
DEFINE l_cnt        LIKE type_t.num10
   
   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt FROM oocq_t
    WHERE oocqent = g_enterprise
      AND oocq001 = p_oocq001
      AND oocqstus = 'Y'
   IF NOT cl_null(l_cnt) AND l_cnt >= 256 THEN
      RETURN FALSE
   END IF
   
   RETURN TRUE
END FUNCTION]]>
</point>
  <point name="function.aooi310_get_oocq004" cite_std="N" status="" ver="1" src="s" new="Y" cite_ver="" order="2" mark_hard="N">
<![CDATA[# 1~256，且不存在其他有效的oocq_t(庫存標籤檔)的oocq004內，找最小的數值
PUBLIC FUNCTION aooi310_get_oocq004()
DEFINE i           LIKE type_t.num5
DEFINE l_cnt       LIKE type_t.num5
DEFINE l_oocq004   LIKE oocq_t.oocq004

   IF g_oocq_d[l_ac].oocqstus = 'N' THEN 
      SELECT oocq004 INTO l_oocq004 FROM oocq_t
       WHERE oocqent = g_enterprise
         AND oocq001 = g_oocq_d_t.oocq001
         AND oocq002 = g_oocq_d_t.oocq002
      RETURN l_oocq004
   END IF

   FOR i=1 TO 256
      LET l_cnt = 0
      SELECT COUNT(*) INTO l_cnt FROM oocq_t
       WHERE oocqent = g_enterprise
         AND oocq001 = g_oocq_d[l_ac].oocq001
         AND oocq004 = i
         AND oocqstus= 'Y'
      IF cl_null(l_cnt) OR l_cnt = 0 THEN
         LET l_oocq004 = i
         EXIT FOR
      END IF
   END FOR
   
   RETURN l_oocq004
END FUNCTION]]>
</point>
  <point name="function.aooi310_check_oocq002" cite_std="N" status="" ver="1" src="s" new="Y" cite_ver="" order="3" mark_hard="N">
<![CDATA[
PUBLIC FUNCTION aooi310_check_oocq002()
DEFINE l_cnt    LIKE type_t.num10

   #修改時，先判斷是否存在於inac_t，存在則不允許修改
   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt FROM inac_t
    WHERE inacent = g_enterprise
      AND inac003 = g_oocq_d_t.oocq002
   IF NOT cl_null(l_cnt) AND l_cnt > 0 THEN
      RETURN FALSE
   END IF
   
   RETURN TRUE
END FUNCTION]]>
</point>
  <point name="function.aooi310_check_oocq001" cite_std="N" status="" ver="1" src="s" new="Y" cite_ver="" order="4" mark_hard="N">
<![CDATA[#+ 檢查是否存在應用分類檔內，且有效
PRIVATE FUNCTION aooi310_check_oocq001(p_oocq001)
DEFINE p_oocq001    LIKE oocq_t.oocq001
DEFINE l_gzaastus   LIKE gzaa_t.gzaastus

   LET g_errno = ''
   SELECT gzaastus INTO l_gzaastus FROM gzaa_t
    WHERE gzaa001 = p_oocq001
    
   CASE
      WHEN SQLCA.sqlcode = 100  LET g_errno = 'aoo-00086'
      WHEN l_gzaastus = 'N'     LET g_errno = 'aoo-00087'
      OTHERWISE                 LET g_errno = SQLCA.sqlcode USING '-----'
   END CASE
   
END FUNCTION]]>
</point>
