  <section id="aooi400.b_fill" ver="8" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aooi400_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2    STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE oogastus,ooga001,ooga002,ooga003,ooga011,ooga009,ooga010,ooga012,ooga013, 
       ooga004,ooga005,ooga006,ooga007,ooga008,ooga001,oogamodid,oogamoddt,oogaownid,oogaowndp,oogacrtid, 
       oogacrtdp,oogacrtdt ,t1.oocql004 ,t2.oocql004 ,t3.oocql004 ,t4.oocql004 ,t5.oocql004 ,t6.oofa011 , 
       t7.oofa011 ,t8.ooefl003 ,t9.oofa011 ,t10.ooefl003 FROM ooga_t",
               "",
                              " LEFT JOIN oocql_t t1 ON t1.oocqlent='"||g_enterprise||"' AND t1.oocql001='7' AND t1.oocql002=ooga004 AND t1.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t2 ON t2.oocqlent='"||g_enterprise||"' AND t2.oocql001='8' AND t2.oocql002=ooga005 AND t2.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t3 ON t3.oocqlent='"||g_enterprise||"' AND t3.oocql001='9' AND t3.oocql002=ooga006 AND t3.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t4 ON t4.oocqlent='"||g_enterprise||"' AND t4.oocql001='10' AND t4.oocql002=ooga007 AND t4.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t5 ON t5.oocqlent='"||g_enterprise||"' AND t5.oocql001='11' AND t5.oocql002=ooga008 AND t5.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t6 ON t6.oofaent='"||g_enterprise||"' AND t6.oofa002='2' AND t6.oofa003=oogamodid  ",
               " LEFT JOIN oofa_t t7 ON t7.oofaent='"||g_enterprise||"' AND t7.oofa002='2' AND t7.oofa003=oogaownid  ",
               " LEFT JOIN ooefl_t t8 ON t8.ooeflent='"||g_enterprise||"' AND t8.ooefl001=oogaowndp AND t8.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t9 ON t9.oofaent='"||g_enterprise||"' AND t9.oofa002='2' AND t9.oofa003=oogacrtid  ",
               " LEFT JOIN ooefl_t t10 ON t10.ooeflent='"||g_enterprise||"' AND t10.ooefl001=oogacrtdp AND t10.ooefl002='"||g_dlang||"' ",
 
               " WHERE oogaent= ? AND 1=1 AND ", p_wc2 
    
   LET g_sql = g_sql, cl_sql_add_filter("ooga_t"),
                      " ORDER BY ooga_t.ooga001"
   
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE aooi400_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aooi400_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_ooga_d.clear()
   CALL g_ooga2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_ooga_d[l_ac].oogastus,g_ooga_d[l_ac].ooga001,g_ooga_d[l_ac].ooga002,g_ooga_d[l_ac].ooga003, 
       g_ooga_d[l_ac].ooga011,g_ooga_d[l_ac].ooga009,g_ooga_d[l_ac].ooga010,g_ooga_d[l_ac].ooga012,g_ooga_d[l_ac].ooga013, 
       g_ooga_d[l_ac].ooga004,g_ooga_d[l_ac].ooga005,g_ooga_d[l_ac].ooga006,g_ooga_d[l_ac].ooga007,g_ooga_d[l_ac].ooga008, 
       g_ooga2_d[l_ac].ooga001,g_ooga2_d[l_ac].oogamodid,g_ooga2_d[l_ac].oogamoddt,g_ooga2_d[l_ac].oogaownid, 
       g_ooga2_d[l_ac].oogaowndp,g_ooga2_d[l_ac].oogacrtid,g_ooga2_d[l_ac].oogacrtdp,g_ooga2_d[l_ac].oogacrtdt, 
       g_ooga_d[l_ac].ooga004_desc,g_ooga_d[l_ac].ooga005_desc,g_ooga_d[l_ac].ooga006_desc,g_ooga_d[l_ac].ooga007_desc, 
       g_ooga_d[l_ac].ooga008_desc,g_ooga2_d[l_ac].oogamodid_desc,g_ooga2_d[l_ac].oogaownid_desc,g_ooga2_d[l_ac].oogaowndp_desc, 
       g_ooga2_d[l_ac].oogacrtid_desc,g_ooga2_d[l_ac].oogacrtdp_desc
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL aooi400_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec THEN
      IF g_error_show = 1 THEN
         CALL cl_err("ooga_t",9035,1)
      END IF
   END IF 
   LET g_error_show = 0
   
 
   
   CALL g_ooga_d.deleteElement(g_ooga_d.getLength())   
   CALL g_ooga2_d.deleteElement(g_ooga2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_ooga_d.getLength()
      LET g_ooga2_d[l_ac].ooga001 = g_ooga_d[l_ac].ooga001 
 
   END FOR
   
   IF g_cnt > g_ooga_d.getLength() THEN
      LET l_ac = g_ooga_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_ooga_d.getLength()
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE aooi400_pb
   
END FUNCTION
]]>
</section>
