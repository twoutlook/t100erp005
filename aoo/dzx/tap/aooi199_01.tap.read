  <section id="aooi199_01.modify" ver="10" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aooi199_01_modify()
   DEFINE  l_cmd                  LIKE type_t.chr1
   DEFINE  l_ac_t                 LIKE type_t.num5                #未取消的ARRAY CNT 
   DEFINE  l_n                    LIKE type_t.num5                #檢查重複用  
   DEFINE  l_cnt                  LIKE type_t.num5                #檢查重複用  
   DEFINE  l_lock_sw              LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert         LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete         LIKE type_t.num5                #可刪除否  
   DEFINE  l_count                LIKE type_t.num5
   DEFINE  l_i                    LIKE type_t.num5
   DEFINE  ls_return              STRING
   DEFINE  l_var_keys             DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys           DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                 DYNAMIC ARRAY OF STRING
   DEFINE  l_fields               DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak         DYNAMIC ARRAY OF STRING
   DEFINE  li_reproduce           LIKE type_t.num5
   DEFINE  li_reproduce_target    LIKE type_t.num5
   DEFINE  lb_reproduce           BOOLEAN
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point 
   
   LET g_action_choice = ""
   
   LET g_qryparam.state = "i"
 
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   
   #add-point:modify開始前
   {<point name="modify.define_sql" mark="Y"/>}
   #end add-point
   
   LET INT_FLAG = FALSE
   LET lb_reproduce = FALSE
 
   #add-point:modify段修改前
   {<point name="modify.before_input"/>}
   #end add-point
 
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #Page1 預設值產生於此處
      INPUT ARRAY g_oobl_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_detail_cnt,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_oobl_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL aooi199_01_b_fill(g_wc2)
            LET g_detail_cnt = g_oobl_d.getLength()
         
         BEFORE ROW
            #add-point:modify段before row
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_cmd = ''
            LET l_ac = g_detail_idx
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            DISPLAY g_oobl_d.getLength() TO FORMONLY.cnt
         
            CALL s_transaction_begin()
            LET g_detail_cnt = g_oobl_d.getLength()
            
            IF g_detail_cnt >= l_ac 
               AND g_oobl_d[l_ac].oobl001 IS NOT NULL
               AND g_oobl_d[l_ac].oobl002 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_oobl_d_t.* = g_oobl_d[l_ac].*  #BACKUP
               LET g_oobl_d_o.* = g_oobl_d[l_ac].*  #BACKUP
               IF NOT aooi199_01_lock_b("oobl_t") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aooi199_01_bcl INTO g_oobl_d[l_ac].oobl001,g_oobl_d[l_ac].oobl002
                  IF SQLCA.sqlcode THEN
                     CALL cl_err(g_oobl_d_t.oobl001,SQLCA.sqlcode,1)
                     LET l_lock_sw = "Y"
                  END IF
                  
                  CALL aooi199_01_detail_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
        
         BEFORE INSERT
            
            CALL s_transaction_begin()
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_oobl_d_t.* TO NULL
            INITIALIZE g_oobl_d_o.* TO NULL
            INITIALIZE g_oobl_d[l_ac].* TO NULL 
            #公用欄位給值(單身)
            
            
            LET g_oobl_d_t.* = g_oobl_d[l_ac].*     #新輸入資料
            LET g_oobl_d_o.* = g_oobl_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL aooi199_01_set_entry_b("a")
            CALL aooi199_01_set_no_entry_b("a")
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_oobl_d[li_reproduce_target].* = g_oobl_d[li_reproduce].*
 
               LET g_oobl_d[g_oobl_d.getLength()].oobl001 = NULL
               LET g_oobl_d[g_oobl_d.getLength()].oobl002 = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM oobl_t 
             WHERE ooblent = g_enterprise AND oobl001 = g_oobl_d[l_ac].oobl001
                                       AND oobl002 = g_oobl_d[l_ac].oobl002
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前
               {<point name="input.body.b_insert"/>}
               #end add-point
            
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_oobl_d[g_detail_idx].oobl001
               LET gs_keys[2] = g_oobl_d[g_detail_idx].oobl002
               CALL aooi199_01_insert_b('oobl_t',gs_keys,"'1'")
                           
               #add-point:單身新增後
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_oobl_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("oobl_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL aooi199_01_b_fill(g_wc2)
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert2"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR 'INSERT O.K'
               LET g_detail_cnt = g_detail_cnt + 1
               
               LET g_wc2 = g_wc2, " OR (oobl001 = '", g_oobl_d[l_ac].oobl001, "' "
                                  ," AND oobl002 = '", g_oobl_d[l_ac].oobl002, "' "
 
                                  ,")"
            END IF                
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_oobl_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_oobl_d.deleteElement(l_ac)
               NEXT FIELD oobl001
            END IF
            IF g_oobl_d[l_ac].oobl001 IS NOT NULL
               AND g_oobl_d_t.oobl002 IS NOT NULL
 
               THEN     
            
               #add-point:單身刪除ask前
               {<point name="input.body.b_delete_ask"/>}
               #end add-point   
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete" mark="Y"/>}
               #end add-point   
               
               DELETE FROM oobl_t
                WHERE ooblent = g_enterprise AND 
                      oobl001 = g_oobl_d_t.oobl001
                      AND oobl002 = g_oobl_d_t.oobl002
 
                      
               #add-point:單身刪除中
               {<point name="input.body.m_delete"/>}
               #end add-point  
                      
               IF SQLCA.sqlcode THEN
                  CALL cl_err("oobl_t",SQLCA.sqlcode,1)
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               ELSE
                  LET g_detail_cnt = g_detail_cnt-1
                  
                  #add-point:單身刪除後
                  {<point name="input.body.a_delete"/>}
                  #end add-point
                  CALL s_transaction_end('Y','0')
               END IF 
               CLOSE aooi199_01_bcl
               LET l_count = g_oobl_d.getLength()
            END IF 
                           INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_oobl_d[g_detail_idx].oobl001
               LET gs_keys[2] = g_oobl_d[g_detail_idx].oobl002
 
              
         AFTER DELETE 
            #add-point:單身刪除後2
            {<point name="input.body.after_delete"/>}
            #end add-point
                           CALL aooi199_01_delete_b('oobl_t',gs_keys,"'1'")
 
                  #此段落由子樣板a01產生
         BEFORE FIELD oobl001
            #add-point:BEFORE FIELD oobl001
            {<point name="input.b.page1.oobl001" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD oobl001
            
            #add-point:AFTER FIELD oobl001
            {<point name="input.a.page1.oobl001" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE oobl001
            #add-point:ON CHANGE oobl001
            {<point name="input.g.page1.oobl001" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD oobl002
            
            #add-point:AFTER FIELD oobl002
            {<point name="input.a.page1.oobl002" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD oobl002
            #add-point:BEFORE FIELD oobl002
            {<point name="input.b.page1.oobl002" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE oobl002
            #add-point:ON CHANGE oobl002
            {<point name="input.g.page1.oobl002" />}
            #END add-point
 
 
                  #Ctrlp:input.c.page1.oobl001
         ON ACTION controlp INFIELD oobl001
            #add-point:ON ACTION controlp INFIELD oobl001
            {<point name="input.c.page1.oobl001" />}
            #END add-point
 
         #Ctrlp:input.c.page1.oobl002
         ON ACTION controlp INFIELD oobl002
            #add-point:ON ACTION controlp INFIELD oobl002
            {<point name="input.c.page1.oobl002" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_oobl_d[l_ac].* = g_oobl_d_t.*
               CLOSE aooi199_01_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_oobl_d[l_ac].oobl001,-263,1)
               LET g_oobl_d[l_ac].* = g_oobl_d_t.*
            ELSE
               
               #寫入修改者/修改日期資訊(單身)
               
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               UPDATE oobl_t SET (oobl001,oobl002) = (g_oobl_d[l_ac].oobl001,g_oobl_d[l_ac].oobl002)
                WHERE ooblent = g_enterprise AND
                  oobl001 = g_oobl_d_t.oobl001 #項次   
                  AND oobl002 = g_oobl_d_t.oobl002  
 
                  
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
                  
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("oobl_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                    WHEN SQLCA.sqlcode #其他錯誤
                     CALL cl_err("oobl_t",SQLCA.sqlcode,1)  
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_oobl_d[g_detail_idx].oobl001
               LET gs_keys_bak[1] = g_oobl_d_t.oobl001
               LET gs_keys[2] = g_oobl_d[g_detail_idx].oobl002
               LET gs_keys_bak[2] = g_oobl_d_t.oobl002
               CALL aooi199_01_update_b('oobl_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
                     LET g_log1 = util.JSON.stringify(g_oobl_d_t)
                     LET g_log2 = util.JSON.stringify(g_oobl_d[l_ac])
                     IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                        CALL s_transaction_end('N','0')
                     END IF
               END CASE
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            CALL aooi199_01_unlock_b("oobl_t")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock
            
             #add-point:單身after row
            {<point name="input.body.a_row"/>}
            #end add-point
            
         AFTER INPUT
            #add-point:單身input後
            {<point name="input.body.a_input"/>}
            #end add-point
            
         ON ACTION controlo   
            CALL FGL_SET_ARR_CURR(g_oobl_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_oobl_d.getLength()+1
            
      END INPUT
      
 
      
 
      
      #add-point:before_more_input
      {<point name="input.more_input"/>}
      #end add-point
      
      BEFORE DIALOG 
         IF g_temp_idx > 0 THEN
            LET l_ac = g_temp_idx
            CALL DIALOG.setCurrentRow("s_detail1",l_ac)
            LET g_temp_idx = 1
         END IF
         LET g_curr_diag = ui.DIALOG.getCurrent()
         #add-point:before_dialog
         {<point name="input.before_dialog"/>}
         #end add-point
         CASE g_aw
            WHEN "s_detail1"
               NEXT FIELD oobl001
 
         END CASE
   
      ON ACTION accept
         ACCEPT DIALOG
      
      ON ACTION cancel
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION controlr
         CALL cl_show_req_fields()
 
      ON ACTION controlf
         CALL cl_set_focus_form(ui.Interface.getRootNode()) 
              RETURNING g_fld_name,g_frm_name 
         CALL cl_fldhelp(g_frm_name,g_fld_name,g_lang) 
           
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE DIALOG
   
   END DIALOG 
   
   #add-point:modify段修改後
   {<point name="modify.after_input"/>}
   #end add-point
 
   CLOSE aooi199_01_bcl
   CALL s_transaction_end('Y','0')
   
END FUNCTION
]]>
  </section>
