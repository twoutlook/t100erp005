  <section id="aooi199_01.b_fill" ver="8" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aooi199_01_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2    STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE oobl001,oobl002 ,t1.gzzal003 FROM oobl_t",
               "",
                              " LEFT JOIN gzzal_t t1 ON t1.gzzal001=oobl002 AND t1.gzzal002='"||g_lang||"' ",
 
               " WHERE ooblent= ? AND 1=1 AND ", p_wc2 
    
   LET g_sql = g_sql, cl_sql_add_filter("oobl_t"),
                      " ORDER BY oobl_t.oobl001,oobl_t.oobl002"
   
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE aooi199_01_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aooi199_01_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_oobl_d.clear()
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_oobl_d[l_ac].oobl001,g_oobl_d[l_ac].oobl002,g_oobl_d[l_ac].oobl002_desc 

      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL aooi199_01_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec THEN
      IF g_error_show = 1 THEN
         CALL cl_err("oobl_t",9035,1)
      END IF
   END IF 
   LET g_error_show = 0
   
 
   
   CALL g_oobl_d.deleteElement(g_oobl_d.getLength())   
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_oobl_d.getLength()
 
   END FOR
   
   IF g_cnt > g_oobl_d.getLength() THEN
      LET l_ac = g_oobl_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_oobl_d.getLength()
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE aooi199_01_pb
   
END FUNCTION
]]>
  </section>
