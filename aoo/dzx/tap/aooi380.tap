  <section id="aooi380.b_fill" ver="5" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aooi380_b_fill()
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_oohb_d.clear()    #g_oohb_d 單頭及單身 
   CALL g_oohb10_d.clear()
   CALL g_oohb11_d.clear()
   CALL g_oohb2_d.clear()
   CALL g_oohb3_d.clear()
   CALL g_oohb4_d.clear()
   CALL g_oohb5_d.clear()
   CALL g_oohb6_d.clear()
   CALL g_oohb7_d.clear()
   CALL g_oohb8_d.clear()
   CALL g_oohb9_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF aooi380_fill_chk(1) THEN
   
      LET g_sql = "SELECT  UNIQUE oohb002,oohb003,oohb004 ,t1.ooefl003 FROM oohb_t",   
                  " INNER JOIN ooha_t ON ooha001 = oohb001 ",
 
                  #"",
                  
                  "",
                                 " LEFT JOIN ooefl_t t1 ON t1.ooeflent='"||g_enterprise||"' AND t1.ooefl001=oohb002 AND t1.ooefl002='"||g_dlang||"' ",
 
                  " WHERE oohbent=? AND oohb001=?"
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      #add-point:b_fill段sql_before
      {<point name="b_fill.body.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table1) THEN
         LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
      END IF
      
      #子單身的WC
      
      
      LET g_sql = g_sql, " ORDER BY oohb_t.oohb002"
      
      #add-point:單身填充控制
      {<point name="b_fill.sql"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE aooi380_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR aooi380_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_ooha_m.ooha001
                                               
      FOREACH b_fill_cs INTO g_oohb_d[l_ac].oohb002,g_oohb_d[l_ac].oohb003,g_oohb_d[l_ac].oohb004,g_oohb_d[l_ac].oohb002_desc 

         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               CALL cl_err( '', 9035, 1)
            END IF
            EXIT FOREACH
         END IF
         
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
   #判斷是否填充
   IF aooi380_fill_chk(2) THEN
      LET g_sql = "SELECT  UNIQUE oohk002,oohk003,oohk004 ,t2.ooefl003 FROM oohk_t",   
                  " INNER JOIN ooha_t ON ooha001 = oohk001 ",
 
                  "",
                  
                                 " LEFT JOIN ooefl_t t2 ON t2.ooeflent='"||g_enterprise||"' AND t2.ooefl001=oohk002 AND t2.ooefl002='"||g_dlang||"' ",
 
                  " WHERE oohkent=? AND oohk001=?"   
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      #add-point:b_fill段sql_before
      {<point name="b_fill.body2.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table2) THEN
         LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
      END IF
      
      #子單身的WC
      
      
      LET g_sql = g_sql, " ORDER BY oohk_t.oohk002"
      
      #add-point:單身填充控制
      {<point name="b_fill.sql2"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE aooi380_pb2 FROM g_sql
      DECLARE b_fill_cs2 CURSOR FOR aooi380_pb2
      
      LET l_ac = 1
      
      OPEN b_fill_cs2 USING g_enterprise,g_ooha_m.ooha001
                                               
      FOREACH b_fill_cs2 INTO g_oohb10_d[l_ac].oohk002,g_oohb10_d[l_ac].oohk003,g_oohb10_d[l_ac].oohk004, 
          g_oohb10_d[l_ac].oohk002_desc
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill2.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            CALL cl_err( '', 9035, 0 )
            EXIT FOREACH
         END IF
         
      END FOREACH
   END IF
 
   #判斷是否填充
   IF aooi380_fill_chk(3) THEN
      LET g_sql = "SELECT  UNIQUE oohl002,oohl003,oohl004,oohl005 ,t3.ooefl003 ,t4.inaa002 FROM oohl_t", 
             
                  " INNER JOIN ooha_t ON ooha001 = oohl001 ",
 
                  "",
                  
                                 " LEFT JOIN ooefl_t t3 ON t3.ooeflent='"||g_enterprise||"' AND t3.ooefl001=oohl002 AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN inaa_t t4 ON t4.inaaent='"||g_enterprise||"' AND t4.inaa001=oohl003 AND t4.inaasite=oohl002  ",
 
                  " WHERE oohlent=? AND oohl001=?"   
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      #add-point:b_fill段sql_before
      {<point name="b_fill.body3.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table3) THEN
         LET g_sql = g_sql CLIPPED," AND ",g_wc2_table3 CLIPPED
      END IF
      
      #子單身的WC
      
      
      LET g_sql = g_sql, " ORDER BY oohl_t.oohl002,oohl_t.oohl003"
      
      #add-point:單身填充控制
      {<point name="b_fill.sql3"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE aooi380_pb3 FROM g_sql
      DECLARE b_fill_cs3 CURSOR FOR aooi380_pb3
      
      LET l_ac = 1
      
      OPEN b_fill_cs3 USING g_enterprise,g_ooha_m.ooha001
                                               
      FOREACH b_fill_cs3 INTO g_oohb11_d[l_ac].oohl002,g_oohb11_d[l_ac].oohl003,g_oohb11_d[l_ac].oohl004, 
