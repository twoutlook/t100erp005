  <section id="aooi140.b_fill" ver="6" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aooi140_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2           STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE ooaistus,ooai001,'','',ooai002,'',ooai003,ooai004,ooai005,ooai001,ooaimodid, 
       '',ooaimoddt,ooaiownid,'',ooaiowndp,'',ooaicrtid,'',ooaicrtdp,'',ooaicrtdt FROM ooai_t",
               " LEFT JOIN ooail_t ON ooai001 = ooail001 AND ooail002 = '",g_lang,"'",
               " WHERE ooaient= ? AND 1=1 AND ", p_wc2 
    
   LET g_sql = g_sql, #,cl_get_extra_cond('zzuser', 'zzgrup') 
                      " ORDER BY ooai_t.ooai001"
  
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
  
   PREPARE aooi140_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aooi140_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_ooai_d.clear()
   CALL g_ooai2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_ooai_d[l_ac].ooaistus,g_ooai_d[l_ac].ooai001,g_ooai_d[l_ac].ooail003,g_ooai_d[l_ac].ooail004, 
       g_ooai_d[l_ac].ooai002,g_ooai_d[l_ac].ooai002_desc,g_ooai_d[l_ac].ooai003,g_ooai_d[l_ac].ooai004, 
       g_ooai_d[l_ac].ooai005,g_ooai2_d[l_ac].ooai001,g_ooai2_d[l_ac].ooaimodid,g_ooai2_d[l_ac].ooaimodid_desc, 
       g_ooai2_d[l_ac].ooaimoddt,g_ooai2_d[l_ac].ooaiownid,g_ooai2_d[l_ac].ooaiownid_desc,g_ooai2_d[l_ac].ooaiowndp, 
       g_ooai2_d[l_ac].ooaiowndp_desc,g_ooai2_d[l_ac].ooaicrtid,g_ooai2_d[l_ac].ooaicrtid_desc,g_ooai2_d[l_ac].ooaicrtdp, 
       g_ooai2_d[l_ac].ooaicrtdp_desc,g_ooai2_d[l_ac].ooaicrtdt
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL aooi140_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec THEN
      IF g_error_show = 1 THEN
         CALL cl_err("ooai_t",9035,1)
      END IF
   END IF 
   LET g_error_show = 0
   
 
  
   
   CALL g_ooai_d.deleteElement(g_ooai_d.getLength())   
   CALL g_ooai2_d.deleteElement(g_ooai2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_ooai_d.getLength()
      LET g_ooai2_d[l_ac].ooai001 = g_ooai_d[l_ac].ooai001 
 
   END FOR
   
   IF g_cnt > g_ooai_d.getLength() THEN
      LET l_ac = g_ooai_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = g_ooai_d.getLength() 
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE aooi140_pb
   
END FUNCTION
]]>
  </section>
  <section id="aooi140.default_search" ver="3" status="" src="s">
    <![CDATA[#+ 外部參數搜尋
PRIVATE FUNCTION aooi140_default_search()
   DEFINE li_idx  LIKE type_t.num5
   DEFINE li_cnt  LIKE type_t.num5
   DEFINE ls_wc   STRING
   #add-point:default_search段define
   {<point name="default_search.define"/>}
   #end add-point  
   
   IF NOT cl_null(g_argv[1]) THEN
      LET ls_wc = ls_wc, " ooai001 = '", g_argv[1], "' AND "
   END IF
   
 
 
