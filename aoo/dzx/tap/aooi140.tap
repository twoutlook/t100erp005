  <point name="function.aooi140_ooai002_chk" order="2" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[#+ 国家地区检察
PRIVATE FUNCTION aooi140_ooai002_chk(p_ooai002)
    DEFINE p_ooai002        LIKE ooai_t.ooai002
    DEFINE l_oocgstus       LIKE oocg_t.oocgstus
    
   LET g_errno = ''
   SELECT oocgstus into l_oocgstus
     FROM oocg_t
   WHERE oocgent=g_enterprise 
     AND oocg001=p_ooai002

  CASE
     WHEN sqlca.sqlcode=100 LET g_errno='aoo-00013'
     WHEN l_oocgstus='N'    LET g_errno='aoo-00035'
  END CASE
END FUNCTION]]>
