  <section id="aooi230.b_fill" ver="2" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aooi230_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2    STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE oocsstus,oocs001,oocs002,oocs003,oocs004,oocs001,oocsownid,oocsowndp, 
       oocscrtid,oocscrtdp,oocscrtdt,oocsmodid,oocsmoddt ,t1.oofa011 ,t2.ooefl003 ,t3.oofa011 ,t4.ooefl003 , 
       t5.oofa011 FROM oocs_t",
               " LEFT JOIN oocsl_t ON oocs001 = oocsl001 AND oocsl002 = '",g_lang,"'",
                              " LEFT JOIN oofa_t t1 ON t1.oofaent='"||g_enterprise||"' AND t1.oofa002='2' AND t1.oofa003=oocsownid  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent='"||g_enterprise||"' AND t2.ooefl001=oocsowndp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t3 ON t3.oofaent='"||g_enterprise||"' AND t3.oofa002='2' AND t3.oofa003=oocscrtid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=oocscrtdp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t5 ON t5.oofaent='"||g_enterprise||"' AND t5.oofa002='2' AND t5.oofa003=oocsmodid  ",
 
               " WHERE oocsent= ? AND 1=1 AND ", p_wc2 
    
   LET g_sql = g_sql, cl_sql_add_filter("oocs_t"),
                      " ORDER BY oocs_t.oocs001"
   
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE aooi230_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aooi230_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_oocs_d.clear()
   CALL g_oocs2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_oocs_d[l_ac].oocsstus,g_oocs_d[l_ac].oocs001,g_oocs_d[l_ac].oocs002,g_oocs_d[l_ac].oocs003, 
       g_oocs_d[l_ac].oocs004,g_oocs2_d[l_ac].oocs001,g_oocs2_d[l_ac].oocsownid,g_oocs2_d[l_ac].oocsowndp, 
       g_oocs2_d[l_ac].oocscrtid,g_oocs2_d[l_ac].oocscrtdp,g_oocs2_d[l_ac].oocscrtdt,g_oocs2_d[l_ac].oocsmodid, 
       g_oocs2_d[l_ac].oocsmoddt,g_oocs2_d[l_ac].oocsownid_desc,g_oocs2_d[l_ac].oocsowndp_desc,g_oocs2_d[l_ac].oocscrtid_desc, 
       g_oocs2_d[l_ac].oocscrtdp_desc,g_oocs2_d[l_ac].oocsmodid_desc
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL aooi230_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec THEN
      IF g_error_show = 1 THEN
         CALL cl_err("oocs_t",9035,1)
      END IF
   END IF 
   LET g_error_show = 0
   
 
   
   CALL g_oocs_d.deleteElement(g_oocs_d.getLength())   
   CALL g_oocs2_d.deleteElement(g_oocs2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_oocs_d.getLength()
      LET g_oocs2_d[l_ac].oocs001 = g_oocs_d[l_ac].oocs001 
 
   END FOR
   
   IF g_cnt > g_oocs_d.getLength() THEN
      LET l_ac = g_oocs_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_oocs_d.getLength()
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE aooi230_pb
   
END FUNCTION
]]>
  </section>
