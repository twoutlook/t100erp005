         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN aooi150_cl USING g_enterprise,
                                               g_ooaj_m.ooaj001
 
                                               
               IF STATUS THEN
                  CALL cl_err("OPEN aooi150_cl:", STATUS, 1)
                  CLOSE aooi150_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_ooaj_d[l_ac].ooaj002 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_ooaj_d_t.* = g_ooaj_d[l_ac].*  #BACKUP
