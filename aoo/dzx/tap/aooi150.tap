  <point name="function.aooi150_ooaj001_chk" order="1" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[#+ 币别参照表号检查
PRIVATE FUNCTION aooi150_ooaj001_chk(p_ooaj001)
   DEFINE l_ooalstus    like ooal_t.ooalstus
   DEFINE p_ooaj001     LIKE ooaj_t.ooaj001
   
   LET g_errno = ''
   SELECT ooalstus  INTO l_ooalstus
     FROM ooal_t 
    WHERE ooalent = g_enterprise
      AND ooal002 = p_ooaj001
      AND ooal001 = '10'
    CASE
       WHEN sqlca.sqlcode = 100   LET g_errno = 'aoo-00076'
       WHEN l_ooalstus ='N'       LET g_errno = 'aoo-00169'
     END CASE
END FUNCTION]]>
  </point>
  <point name="function.aooi150_ooaj002_chk" order="2" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[#+币别检查
PRIVATE FUNCTION aooi150_ooaj002_chk(p_ooaj002)
  DEFINE p_ooaj002   LIKE ooaj_t.ooaj002
  DEFINE l_ooaistus    like ooai_t.ooaistus
  
   LET g_errno = ''
   SELECT ooaistus  INTO l_ooaistus
     FROM ooai_t 
    WHERE ooaient = g_enterprise
      AND ooai001 = p_ooaj002
    CASE
       WHEN sqlca.sqlcode = 100   LET g_errno = 'aoo-00028'
       WHEN l_ooaistus ='N'       LET g_errno = 'aoo-00011'
     END CASE
  
END FUNCTION]]>
  </point>
  <point name="function.aooi150_ooaj001_desc" order="3" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[#+ 币别参照表号说明
PRIVATE FUNCTION aooi150_ooaj001_desc(p_ooaj001)
   DEFINE p_ooaj001    LIKE ooaj_t.ooaj001 
     
