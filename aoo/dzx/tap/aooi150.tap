               #end add-point
               CALL aooi150_set_no_entry_b(l_cmd)
               OPEN aooi150_bcl USING g_enterprise,g_ooaj_m.ooaj001,
 
                                                g_ooaj_d_t.ooaj002
 
               IF STATUS THEN
                  CALL cl_err("OPEN aooi150_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aooi150_bcl INTO g_ooaj_d[l_ac].ooajstus,g_ooaj_d[l_ac].ooaj002,g_ooaj_d[l_ac].ooaj002_desc, 
                      g_ooaj_d[l_ac].ooaj003,g_ooaj_d[l_ac].ooaj004,g_ooaj_d[l_ac].ooaj005,g_ooaj2_d[l_ac].ooaj002, 
                      g_ooaj2_d[l_ac].ooajmodid,g_ooaj2_d[l_ac].ooajmodid_desc,g_ooaj2_d[l_ac].ooajmoddt, 
                      g_ooaj2_d[l_ac].ooajownid,g_ooaj2_d[l_ac].ooajownid_desc,g_ooaj2_d[l_ac].ooajowndp, 
                      g_ooaj2_d[l_ac].ooajowndp_desc,g_ooaj2_d[l_ac].ooajcrtid,g_ooaj2_d[l_ac].ooajcrtid_desc, 
                      g_ooaj2_d[l_ac].ooajcrtdp,g_ooaj2_d[l_ac].ooajcrtdp_desc,g_ooaj2_d[l_ac].ooajcrtdt 

                  IF SQLCA.sqlcode THEN
                      CALL cl_err(g_ooaj_d_t.ooaj002,SQLCA.sqlcode,1)
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL aooi150_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
        
         BEFORE INSERT
            
