   LET g_wc = cl_replace_str(g_wc, g_wc_filter_t, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON ooaj001
                          FROM s_browse[1].b_ooaj001
 
         BEFORE CONSTRUCT
               DISPLAY aooi150_filter_parser('ooaj001') TO s_browse[1].b_ooaj001
      
