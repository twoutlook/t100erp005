  <section id="aooi150.modify" ver="2" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aooi150_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_ooaj_m.ooaj001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE aooi150_master_referesh USING g_ooaj_m.ooaj001 INTO g_ooaj_m.ooaj001
 
   ERROR ""
  
   LET g_ooaj001_t = g_ooaj_m.ooaj001
 
   CALL s_transaction_begin()
   
   OPEN aooi150_cl USING g_enterprise,g_ooaj_m.ooaj001
 
   IF STATUS THEN
      CALL cl_err("OPEN aooi150_cl:", STATUS, 1)
      CLOSE aooi150_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aooi150_cl INTO g_ooaj_m.ooaj001,g_ooaj_m.ooaj001_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_ooaj_m.ooaj001,SQLCA.sqlcode,0)
      CLOSE aooi150_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL aooi150_show()
   WHILE TRUE
      LET g_ooaj001_t = g_ooaj_m.ooaj001
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL aooi150_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_ooaj_m.* = g_ooaj_m_t.*
         CALL aooi150_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_ooaj_m.ooaj001 != g_ooaj001_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE ooaj_t SET ooaj001 = g_ooaj_m.ooaj001
 
          WHERE ooajent = g_enterprise AND ooaj001 = g_ooaj001_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("ooaj_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("ooaj_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_ooaj_m.ooaj001,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE aooi150_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_ooaj_m.ooaj001,'U')
 
   CALL aooi150_b_fill("1=1")
   
END FUNCTION   
]]>
  </section>
