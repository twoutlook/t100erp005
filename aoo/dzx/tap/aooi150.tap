  <section id="aooi150.detail_reproduce" ver="2" status="" src="s">
    <![CDATA[#+ 單身自動複製
PRIVATE FUNCTION aooi150_detail_reproduce()
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE ooaj_t.*
 
 
   #add-point:delete段define
   {<point name="detail_reproduce.define"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE aooi150_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE aooi150_detail AS ",
                "SELECT * FROM ooaj_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO aooi150_detail SELECT * FROM ooaj_t 
                                         WHERE ooajent = g_enterprise AND ooaj001 = g_ooaj001_t
 
   
   #將key修正為調整後   
   UPDATE aooi150_detail 
      #更新key欄位
      SET ooaj001 = g_ooaj_m.ooaj001
 
      #更新共用欄位
      #此段落由子樣板a13產生
       , ooajownid = g_user
       , ooajowndp = g_dept
       , ooajcrtid = g_user
       , ooajcrtdp = g_dept 
       , ooajcrtdt = ld_date
       , ooajmodid = "" 
       , ooajmoddt = "" 
      #, ooajstus = "Y"
 
 
                                       
  
   #將資料塞回原table   
   INSERT INTO ooaj_t SELECT * FROM aooi150_detail
   
   IF SQLCA.sqlcode THEN
      CALL cl_err("reproduce",SQLCA.sqlcode,1)
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE aooi150_detail
   
   #add-point:單身複製後1
   {<point name="detail_reproduce.body.table1.a_insert"/>}
   #end add-point
 
 
   
 
   
   #多語言複製段落
   
   
   CALL s_transaction_end('Y','0')
   
   #已新增完, 調整資料內容(修改時使用)
   LET g_ooaj001_t = g_ooaj_m.ooaj001
 
   
   DROP TABLE aooi150_detail
   
END FUNCTION
