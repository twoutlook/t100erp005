       , oocncrtid = g_user
       , oocncrtdp = g_dept 
       , oocncrtdt = ld_date
       , oocnmodid = "" 
       , oocnmoddt = "" 
      #, oocnstus = "Y"
 
 
                                       
  
   #將資料塞回原table   
