    <![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_oocn_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂單身ACTION
         
 
         #+ 此段落由子樣板a43產生
         ON ACTION update_item
            LET g_action_choice="update_item"
            IF cl_auth_chk_act("update_item") THEN
               
               #add-point:ON ACTION update_item
               {<point name="input.detail_input.page1.update_item" />}
               #END add-point
            END IF
 
 
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_oocn_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL aooi060_b_fill(g_wc2) 
            IF g_rec_b != 0 THEN
               CALL fgl_set_arr_curr(l_ac)
            END IF
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN aooi060_cl USING g_enterprise,
                                               g_oocn_m.oocn001
 
                                               
               IF STATUS THEN
                  CALL cl_err("OPEN aooi060_cl:", STATUS, 1)
                  CLOSE aooi060_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_oocn_d[l_ac].oocn007 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_oocn_d_t.* = g_oocn_d[l_ac].*  #BACKUP
               CALL aooi060_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL aooi060_set_no_entry_b(l_cmd)
               OPEN aooi060_bcl USING g_enterprise,g_oocn_m.oocn001,
 
                                                g_oocn_d_t.oocn007
 
               IF STATUS THEN
                  CALL cl_err("OPEN aooi060_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aooi060_bcl INTO g_oocn_d[l_ac].oocnstus,g_oocn_d[l_ac].oocn007,g_oocn_d[l_ac].oocn002, 
                      g_oocn_d[l_ac].oocn003,g_oocn_d[l_ac].oocn004,g_oocn_d[l_ac].oocn005,g_oocn_d[l_ac].oocn006 

                  IF SQLCA.sqlcode THEN
                      CALL cl_err(g_oocn_d_t.oocn007,SQLCA.sqlcode,1)
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL aooi060_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            LET g_detail_multi_table_t.oocnl001 = g_oocn_m.oocn001
LET g_detail_multi_table_t.oocnl006 = g_oocn_d[l_ac].oocn007
LET g_detail_multi_table_t.oocnl003 = g_dlang
LET g_detail_multi_table_t.oocnl004 = g_oocn_d[l_ac].oocnl004
LET g_detail_multi_table_t.oocnl005 = g_oocn_d[l_ac].oocnl005
 
        
         BEFORE INSERT
            
            INITIALIZE g_oocn_d_t.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_oocn_d[l_ac].* TO NULL
            
            #公用欄位預設值
            #此段落由子樣板a14產生    
      LET g_oocn1_info_d[l_ac].oocnownid = g_user
      LET g_oocn1_info_d[l_ac].oocnowndp = g_dept
      LET g_oocn1_info_d[l_ac].oocncrtid = g_user
      LET g_oocn1_info_d[l_ac].oocncrtdp = g_dept 
      LET g_oocn1_info_d[l_ac].oocncrtdt = cl_get_current()
      LET g_oocn1_info_d[l_ac].oocnmodid = ""
      LET g_oocn1_info_d[l_ac].oocnmoddt = ""
      LET g_oocn_d[l_ac].oocnstus = ""
 
  
            
            #一般欄位預設值
                  LET g_oocn_d[l_ac].oocnstus = "Y"
 
            
            
            LET g_oocn_d_t.* = g_oocn_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL aooi060_set_entry_b(l_cmd)
            #add-point:set_entry_b後
            {<point name="input.body.before_insert.set_entry_b"/>}
            #end add-point
            CALL aooi060_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_oocn_d[li_reproduce_target].* = g_oocn_d[li_reproduce].*
               LET g_oocn1_info_d[li_reproduce_target].* = g_oocn1_info_d[li_reproduce].*
 
               LET g_oocn_d[g_oocn_d.getLength()].oocn007 = NULL
 
            END IF
            LET g_detail_multi_table_t.oocnl001 = g_oocn_m.oocn001
LET g_detail_multi_table_t.oocnl006 = g_oocn_d[l_ac].oocn007
LET g_detail_multi_table_t.oocnl003 = g_dlang
LET g_detail_multi_table_t.oocnl004 = g_oocn_d[l_ac].oocnl004
LET g_detail_multi_table_t.oocnl005 = g_oocn_d[l_ac].oocnl005
 
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
 
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM oocn_t 
             WHERE oocnent = g_enterprise AND oocn001 = g_oocn_m.oocn001
 
               AND oocn007 = g_oocn_d[l_ac].oocn007
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               CALL s_transaction_begin()
               #add-point:單身新增前
               {<point name="input.body.b_insert" mark="Y"/>}
               #end add-point
               INSERT INTO oocn_t
                           (oocnent,
                            oocn001,
                            oocn007
                            ,oocnstus,oocn002,oocn003,oocn004,oocn005,oocn006,oocnmodid,oocnmoddt,oocnownid,oocnowndp,oocncrtid,oocncrtdp,oocncrtdt) 
                     VALUES(g_enterprise,
                            g_oocn_m.oocn001,
                            g_oocn_d[l_ac].oocn007
                            ,g_oocn_d[l_ac].oocnstus,g_oocn_d[l_ac].oocn002,g_oocn_d[l_ac].oocn003,g_oocn_d[l_ac].oocn004, 
                                g_oocn_d[l_ac].oocn005,g_oocn_d[l_ac].oocn006,g_oocn1_info_d[l_ac].oocnmodid, 
                                g_oocn1_info_d[l_ac].oocnmoddt,g_oocn1_info_d[l_ac].oocnownid,g_oocn1_info_d[l_ac].oocnowndp, 
                                g_oocn1_info_d[l_ac].oocncrtid,g_oocn1_info_d[l_ac].oocncrtdp,g_oocn1_info_d[l_ac].oocncrtdt) 

               #add-point:單身新增中
               {<point name="input.body.m_insert"/>}
               #end add-point
               LET p_cmd = 'u'
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_oocn_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("oocn_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #資料多語言用-增/改
                        INITIALIZE l_var_keys TO NULL 
         INITIALIZE l_field_keys TO NULL 
         INITIALIZE l_vars TO NULL 
         INITIALIZE l_fields TO NULL 
         INITIALIZE l_var_keys_bak TO NULL 
         IF g_oocn_m.oocn001 = g_detail_multi_table_t.oocnl001 AND
         g_oocn_d[l_ac].oocn007 = g_detail_multi_table_t.oocnl006 AND
         g_oocn_d[l_ac].oocnl004 = g_detail_multi_table_t.oocnl004 AND
         g_oocn_d[l_ac].oocnl005 = g_detail_multi_table_t.oocnl005 THEN
         ELSE 
            LET l_var_keys[01] = g_oocn_m.oocn001
            LET l_field_keys[01] = 'oocnl001'
            LET l_var_keys[02] = g_oocn_d[l_ac].oocn007
            LET l_field_keys[02] = 'oocnl006'
            LET l_var_keys[03] = g_dlang
            LET l_field_keys[03] = 'oocnl003'
            LET l_vars[01] = g_oocn_d[l_ac].oocnl004
            LET l_fields[01] = 'oocnl004'
