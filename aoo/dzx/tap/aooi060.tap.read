  <point name="function.aooi060_show_ref" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 取得州省名稱
# Memo...........:
# Usage..........: CALL aooi060_oocn003_ref(p_oocn001,p_oocn003,p_oocn004,p_oocn005)
# Input parameter: p_oocn001       國家地區編號
#                : p_oocn003       州省編號
#                : p_oocn004       縣市編號
#                : p_oocn005       行政地區編號
# Return code....: 
# Date & Author..: 2014/03/06 By Sarah
# Modify.........:
################################################################################
PRIVATE FUNCTION aooi060_show_ref(p_oocn001,p_oocn003,p_oocn004,p_oocn005)
DEFINE p_oocn001       LIKE oocn_t.oocn001
DEFINE p_oocn003       LIKE oocn_t.oocn003
DEFINE p_oocn004       LIKE oocn_t.oocn004
DEFINE p_oocn005       LIKE oocn_t.oocn005
  
   IF cl_null(p_oocn004) THEN LET p_oocn004 = ' ' END IF
  
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = p_oocn001
   LET g_ref_fields[2] = p_oocn003
   CALL ap_ref_array2(g_ref_fields,"SELECT oocil004 FROM oocil_t WHERE oocilent='"||g_enterprise||"' AND oocil001=? AND oocil002=? AND oocil003='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_oocn_d[l_ac].oocn003_desc = g_rtn_fields[1]
   DISPLAY BY NAME g_oocn_d[l_ac].oocn003_desc

   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = p_oocn001
   LET g_ref_fields[2] = p_oocn003
   LET g_ref_fields[3] = p_oocn004   
   CALL ap_ref_array2(g_ref_fields,"SELECT oockl005 FROM oockl_t WHERE oocklent='"||g_enterprise||"' AND oockl001=? AND oockl002=? AND oockl003=? AND oockl004='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_oocn_d[l_ac].oocn004_desc = g_rtn_fields[1]
   DISPLAY BY NAME g_oocn_d[l_ac].oocn004_desc

   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = p_oocn001
   LET g_ref_fields[2] = p_oocn003
   LET g_ref_fields[3] = p_oocn004
   LET g_ref_fields[4] = p_oocn005
   CALL ap_ref_array2(g_ref_fields,"SELECT oocml006 FROM oocml_t WHERE oocmlent='"||g_enterprise||"' AND oocml001=? AND oocml002=? AND oocml003=? AND oocml004=? AND oocml005='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_oocn_d[l_ac].oocn005_desc = g_rtn_fields[1]
   DISPLAY BY NAME g_oocn_d[l_ac].oocn005_desc
   
END FUNCTION]]>
  </point>
  <point name="function.aooi060_oocn001_chk" order="2" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[# 郵政資料相關欄位檢查
PRIVATE FUNCTION aooi060_oocn001_chk(p_oocn001,p_oocn003,p_oocn004,p_oocn005)
DEFINE p_oocn001    LIKE oocn_t.oocn001
DEFINE p_oocn003    LIKE oocn_t.oocn003
DEFINE p_oocn004    LIKE oocn_t.oocn004
DEFINE p_oocn005    LIKE oocn_t.oocn005
DEFINE l_oocg001    LIKE oocg_t.oocg001
DEFINE l_oocgstus   LIKE oocg_t.oocgstus
DEFINE l_sql        STRING 


IF NOT cl_null(p_oocn001) THEN 
   SELECT oocg001,oocgstus INTO l_oocg001,l_oocgstus FROM oocg_t 
    WHERE oocg001 = p_oocn001 AND oocgent = g_enterprise
   IF cl_null(l_oocg001) THEN
      CALL cl_err(p_oocn001,'aoo-00013',0)
      RETURN FALSE
   END IF 
   IF l_oocgstus != 'Y' THEN 
      CALL cl_err(p_oocn001,'aoo-00035',0)
      RETURN FALSE
   END IF 
END IF 

IF NOT cl_null(p_oocn003) THEN 
   LET l_oocg001 = ''
   LET l_oocgstus = ''
   SELECT ooci002,oocistus INTO l_oocg001,l_oocgstus FROM ooci_t
    WHERE ooci001 = p_oocn001 AND ooci002 = p_oocn003 AND oocient = g_enterprise
   IF cl_null(l_oocg001) THEN
      CALL cl_err(p_oocn003,'aoo-00014',0)
      RETURN FALSE
   END IF 
   IF l_oocgstus != 'Y' THEN 
      CALL cl_err(p_oocn003,'aoo-00143',0)
      RETURN FALSE
   END IF  
END IF 

IF NOT cl_null(p_oocn004) THEN 
   LET l_oocg001 = ''
   LET l_oocgstus = ''
   LET l_sql = "SELECT oock003,oockstus FROM oock_t",
               " WHERE oock001 = '",p_oocn001,"' AND oock003 = '",p_oocn004,"' AND oockent = '",g_enterprise,"'"
   IF NOT cl_null(p_oocn003) THEN 
      LET l_sql = l_sql ," AND oock002='",p_oocn003,"'"
   END IF
   PREPARE pre_oocn004 FROM l_sql
   EXECUTE pre_oocn004 INTO l_oocg001,l_oocgstus
   IF cl_null(l_oocg001) THEN
      CALL cl_err(p_oocn004,'aoo-00015',0)
      RETURN FALSE
   END IF 
   IF l_oocgstus != 'Y' THEN 
      CALL cl_err(p_oocn004,'aoo-00036',0)
      RETURN FALSE
   END IF  
END IF 

IF NOT cl_null(p_oocn005) THEN 
   LET l_oocg001 = ''
   LET l_oocgstus = ''
   LET l_sql = "SELECT oocm004,oocmstus FROM oocm_t",
               " WHERE oocm001 = '",p_oocn001,"' AND oocm004 = '",p_oocn005,"' AND oocment = '",g_enterprise,"'"
   IF NOT cl_null(p_oocn003) THEN 
      LET l_sql = l_sql ," AND oocm002='",p_oocn003,"'"
   END IF
   IF NOT cl_null(p_oocn004) THEN
      LET l_sql = l_sql ," AND oocm003='",p_oocn004,"'"
   END IF   
   PREPARE pre_oocn005 FROM l_sql
   EXECUTE pre_oocn005 INTO l_oocg001,l_oocgstus
   IF cl_null(l_oocg001) THEN
      CALL cl_err(p_oocn005,'aoo-00022',0)
      RETURN FALSE
   END IF 
   IF l_oocgstus != 'Y' THEN 
      CALL cl_err(p_oocn005,'aoo-00144',0)
      RETURN FALSE
   END IF  
END IF 

RETURN TRUE
END FUNCTION]]>
