  <section id="aooi060.delete" ver="6" status="" src="s">
    <![CDATA[#+ 資料刪除
PRIVATE FUNCTION aooi060_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_oocn_m.oocn001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE aooi060_master_referesh USING g_oocn_m.oocn001 INTO g_oocn_m.oocn001,g_oocn_m.oocn001_desc 

   
   CALL aooi060_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN aooi060_cl USING g_enterprise,g_oocn_m.oocn001
 
   IF STATUS THEN
      CALL cl_err("OPEN aooi060_cl:", STATUS, 1)
      CLOSE aooi060_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aooi060_cl INTO g_oocn_m.oocn001,g_oocn_m.oocn001_desc
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_oocn_m.oocn001,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL aooi060_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM oocn_t WHERE oocnent = g_enterprise AND oocn001 = g_oocn_m.oocn001
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("oocn_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_oocn_d.clear() 
      CALL g_oocn1_info_d.clear()       
 
     
      CALL aooi060_ui_browser_refresh()  
      CALL aooi060_ui_headershow()  
      CALL aooi060_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL aooi060_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL aooi060_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE aooi060_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_oocn_m.oocn001,'D')
    
END FUNCTION
