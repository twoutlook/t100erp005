  <section id="aooi060.b_fill" ver="11" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aooi060_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_oocn_d.clear()
   CALL g_oocn1_info_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE oocnstus,oocn007,oocn002,oocn003,oocn004,oocn005,oocn006,oocn007,oocnmodid, 
       oocnmoddt,oocnownid,oocnowndp,oocncrtid,oocncrtdp,oocncrtdt,t1.oocil004 ,t2.oockl005 ,t3.oocml006 , 
       t4.oofa011 ,t5.oofa011 ,t6.ooefl003 ,t7.oofa011 ,t8.ooefl003 FROM oocn_t",   
               " LEFT JOIN oocnl_t ON oocn001 = oocnl001 AND oocn007 = oocnl006 AND oocnl003 = '",g_lang,"'",
               
                              " LEFT JOIN oocil_t t1 ON t1.oocilent='"||g_enterprise||"' AND t1.oocil001=oocn001 AND t1.oocil002=oocn003 AND t1.oocil003='"||g_dlang||"' ",
               " LEFT JOIN oockl_t t2 ON t2.oocklent='"||g_enterprise||"' AND t2.oockl001=oocn001 AND t2.oockl002=oocn003 AND t2.oockl003=oocn004 AND t2.oockl004='"||g_dlang||"' ",
               " LEFT JOIN oocml_t t3 ON t3.oocmlent='"||g_enterprise||"' AND t3.oocml001=oocn001 AND t3.oocml002=oocn003 AND t3.oocml003=oocn004 AND t3.oocml004=oocn005 AND t3.oocml005='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t4 ON t4.oofaent='"||g_enterprise||"' AND t4.oofa002='2' AND t4.oofa003=oocnmodid  ",
               " LEFT JOIN oofa_t t5 ON t5.oofaent='"||g_enterprise||"' AND t5.oofa002='2' AND t5.oofa003=oocnownid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent='"||g_enterprise||"' AND t6.ooefl001=oocnowndp AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t7 ON t7.oofaent='"||g_enterprise||"' AND t7.oofa002='2' AND t7.oofa003=oocncrtid  ",
               " LEFT JOIN ooefl_t t8 ON t8.ooeflent='"||g_enterprise||"' AND t8.ooefl001=oocncrtdp AND t8.ooefl002='"||g_dlang||"' ",
 
               " WHERE oocnent= ? AND oocn001=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("oocn_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF aooi060_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY oocn_t.oocn007"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE aooi060_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR aooi060_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_oocn_m.oocn001
                                               
      FOREACH b_fill_cs INTO g_oocn_d[l_ac].oocnstus,g_oocn_d[l_ac].oocn007,g_oocn_d[l_ac].oocn002,g_oocn_d[l_ac].oocn003, 
          g_oocn_d[l_ac].oocn004,g_oocn_d[l_ac].oocn005,g_oocn_d[l_ac].oocn006,g_oocn1_info_d[l_ac].oocn007, 
          g_oocn1_info_d[l_ac].oocnmodid,g_oocn1_info_d[l_ac].oocnmoddt,g_oocn1_info_d[l_ac].oocnownid, 
          g_oocn1_info_d[l_ac].oocnowndp,g_oocn1_info_d[l_ac].oocncrtid,g_oocn1_info_d[l_ac].oocncrtdp, 
          g_oocn1_info_d[l_ac].oocncrtdt,g_oocn_d[l_ac].oocn003_desc,g_oocn_d[l_ac].oocn004_desc,g_oocn_d[l_ac].oocn005_desc, 
          g_oocn1_info_d[l_ac].oocnmodid_desc,g_oocn1_info_d[l_ac].oocnownid_desc,g_oocn1_info_d[l_ac].oocnowndp_desc, 
          g_oocn1_info_d[l_ac].oocncrtid_desc,g_oocn1_info_d[l_ac].oocncrtdp_desc
                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_oocn1_info_d[l_ac].oocnownid_desc = cl_get_username(g_oocn1_info_d[l_ac].oocnownid)
      #LET g_oocn1_info_d[l_ac].oocnowndp_desc = cl_get_deptname(g_oocn1_info_d[l_ac].oocnowndp)
      #LET g_oocn1_info_d[l_ac].oocncrtid_desc = cl_get_username(g_oocn1_info_d[l_ac].oocncrtid)
      #LET g_oocn1_info_d[l_ac].oocncrtdp_desc = cl_get_deptname(g_oocn1_info_d[l_ac].oocncrtdp)
      #LET g_oocn1_info_d[l_ac].oocnmodid_desc = cl_get_username(g_oocn1_info_d[l_ac].oocnmodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_oocn_d.deleteElement(g_oocn_d.getLength())
      CALL g_oocn1_info_d.deleteElement(g_oocn1_info_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE aooi060_pb   
   
END FUNCTION
]]>
  </section>
  <section id="aooi060.b_fill2" ver="2" status="" src="s">
    <![CDATA[#+ 單身陣列填充2
PRIVATE FUNCTION aooi060_b_fill2(pi_idx)
   DEFINE pi_idx          LIKE type_t.num5
   DEFINE li_ac           LIKE type_t.num5
   #add-point:b_fill2段define
   {<point name="b_fill2.define"/>}
