   #end add-point    
   
   IF g_oogh_m.oogh001 IS NULL
   OR g_oogh_m.oogh003 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE aooi430_master_referesh USING g_oogh_m.oogh001,g_oogh_m.oogh003 INTO g_oogh_m.oogh001,g_oogh_m.oogh003, 
       g_oogh_m.oogh001_desc,g_oogh_m.oogh003_desc
 
   ERROR ""
  
   LET g_oogh001_t = g_oogh_m.oogh001
   LET g_oogh003_t = g_oogh_m.oogh003
 
   CALL s_transaction_begin()
   
   OPEN aooi430_cl USING g_enterprise, g_site,g_oogh_m.oogh001
                                                       ,g_oogh_m.oogh003
 
   IF STATUS THEN
      CALL cl_err("OPEN aooi430_cl:", STATUS, 1)
      CLOSE aooi430_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aooi430_cl INTO g_oogh_m.oogh001,g_oogh_m.oogh001_desc,g_oogh_m.oogh003,g_oogh_m.oogh003_desc 

 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_oogh_m.oogh001,SQLCA.sqlcode,0)
      CLOSE aooi430_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL aooi430_show()
   WHILE TRUE
      LET g_oogh001_t = g_oogh_m.oogh001
      LET g_oogh003_t = g_oogh_m.oogh003
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL aooi430_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_oogh_m.* = g_oogh_m_t.*
         CALL aooi430_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_oogh_m.oogh001 != g_oogh001_t 
      OR g_oogh_m.oogh003 != g_oogh003_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE oogh_t SET oogh001 = g_oogh_m.oogh001
                                       , oogh003 = g_oogh_m.oogh003
 
          WHERE ooghent = g_enterprise AND ooghsite = g_site AND oogh001 = g_oogh001_t
            AND oogh003 = g_oogh003_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("oogh_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("oogh_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_oogh_m.oogh001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE aooi430_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_oogh_m.oogh001,'U')
 
   CALL aooi430_b_fill("1=1")
   
END FUNCTION   
]]>
  </section>
