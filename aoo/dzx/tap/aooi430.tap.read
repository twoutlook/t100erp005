  <section id="aooi430.delete" ver="6" status="" src="s">
    <![CDATA[#+ 資料刪除
PRIVATE FUNCTION aooi430_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_oogh_m.oogh001 IS NULL
   OR g_oogh_m.oogh003 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE aooi430_master_referesh USING g_oogh_m.oogh001,g_oogh_m.oogh003 INTO g_oogh_m.oogh001,g_oogh_m.oogh003, 
       g_oogh_m.oogh001_desc,g_oogh_m.oogh003_desc
   
   CALL aooi430_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN aooi430_cl USING g_enterprise, g_site,g_oogh_m.oogh001
                                                       ,g_oogh_m.oogh003
 
   IF STATUS THEN
      CALL cl_err("OPEN aooi430_cl:", STATUS, 1)
      CLOSE aooi430_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aooi430_cl INTO g_oogh_m.oogh001,g_oogh_m.oogh001_desc,g_oogh_m.oogh003,g_oogh_m.oogh003_desc 

   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_oogh_m.oogh001,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL aooi430_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM oogh_t WHERE ooghent = g_enterprise AND ooghsite = g_site AND oogh001 = g_oogh_m.oogh001 

                                                               AND oogh003 = g_oogh_m.oogh003
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("oogh_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_oogh_d.clear() 
      CALL g_oogh2_d.clear()       
 
     
      CALL aooi430_ui_browser_refresh()  
      CALL aooi430_ui_headershow()  
      CALL aooi430_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL aooi430_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL aooi430_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE aooi430_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_oogh_m.oogh001,'D')
    
END FUNCTION
]]>
