  <section id="aooi430.detail_reproduce" ver="4" status="" src="s">
    <![CDATA[#+ 單身自動複製
PRIVATE FUNCTION aooi430_detail_reproduce()
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE oogh_t.*
 
 
   #add-point:delete段define
   {<point name="detail_reproduce.define"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE aooi430_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE aooi430_detail AS ",
                "SELECT * FROM oogh_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO aooi430_detail SELECT * FROM oogh_t 
                                         WHERE ooghent = g_enterprise AND ooghsite = g_site AND oogh001 = g_oogh001_t
                                         AND oogh003 = g_oogh003_t
 
   
   #將key修正為調整後   
   UPDATE aooi430_detail 
      #更新key欄位
      SET oogh001 = g_oogh_m.oogh001
          , oogh003 = g_oogh_m.oogh003
 
      #更新共用欄位
      #此段落由子樣板a13產生
       , ooghownid = g_user
       , ooghowndp = g_dept
       , ooghcrtid = g_user
       , ooghcrtdp = g_dept 
       , ooghcrtdt = ld_date
       , ooghmodid = "" 
       , ooghmoddt = "" 
 
 
                                       
  
   #將資料塞回原table   
   INSERT INTO oogh_t SELECT * FROM aooi430_detail
   
   IF SQLCA.sqlcode THEN
      CALL cl_err("reproduce",SQLCA.sqlcode,1)
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE aooi430_detail
   
   #add-point:單身複製後1
   {<point name="detail_reproduce.body.table1.a_insert"/>}
   #end add-point
 
 
   
 
   
   #多語言複製段落
   
   
   CALL s_transaction_end('Y','0')
   
   #已新增完, 調整資料內容(修改時使用)
   LET g_oogh001_t = g_oogh_m.oogh001
   LET g_oogh003_t = g_oogh_m.oogh003
 
   
   DROP TABLE aooi430_detail
   
END FUNCTION
]]>
