  <point name="function.aooi430_b_default" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#+ 單頭輸入完以後帶出單身的值
PRIVATE FUNCTION aooi430_b_default()
DEFINE p_wc2      STRING
   DEFINE l_ooef008  LIKE ooef_t.ooef008
   DEFINE l_ooef009  LIKE ooef_t.ooef009
   DECLARE b_default_cs CURSOR FOR
   SELECT oogf002 FROM oogf_t
    WHERE oogfent = g_enterprise
      AND oogfsite = g_site
      AND oogf001 = g_oogh_m.oogh001
      AND ((oogf003<= g_oogh_m.oogh003 AND oogf004 >=g_oogh_m.oogh003)OR (oogf003<=g_oogh_m.oogh003 AND oogf004 IS NULL))
   LET l_ac = 1
   FOREACH b_default_cs INTO g_oogh_d[l_ac].oogh002
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
      SELECT ooef008,ooef009 INTO l_ooef008,l_ooef009 FROM ooef_t WHERE ooefent = g_enterprise AND ooef001 = g_site
      SELECT oogc009 INTO g_oogh_d[l_ac].oogh004 FROM oogc_t
       WHERE oogcent = g_enterprise
         AND oogc001 = l_ooef008
         AND oogc002 = l_ooef009
         AND oogc003 = g_oogh_m.oogh003
     
      LET g_oogh2_d[l_ac].ooghownid = g_user
      LET g_oogh2_d[l_ac].ooghowndp = g_dept
      LET g_oogh2_d[l_ac].ooghcrtid = g_user
      LET g_oogh2_d[l_ac].ooghcrtdp = g_dept 
      LET g_oogh2_d[l_ac].ooghcrtdt = cl_get_current()
      LET g_oogh2_d[l_ac].ooghmodid = g_user
      LET g_oogh2_d[l_ac].ooghmoddt = cl_get_current()

      INSERT INTO oogh_t (ooghent,ooghsite,oogh001,oogh003,oogh002,oogh004,oogh005,oogh006,oogh008,ooghmodid,ooghmoddt,ooghownid,ooghowndp,ooghcrtid,ooghcrtdp,ooghcrtdt)
           VALUES (g_enterprise,g_site,g_oogh_m.oogh001,g_oogh_m.oogh003,g_oogh_d[l_ac].oogh002,g_oogh_d[l_ac].oogh004,0,0,0,g_oogh2_d[l_ac].ooghmodid,g_oogh2_d[l_ac].ooghmoddt,g_oogh2_d[l_ac].ooghownid,g_oogh2_d[l_ac].ooghowndp,g_oogh2_d[l_ac].ooghcrtid,g_oogh2_d[l_ac].ooghcrtdp,g_oogh2_d[l_ac].ooghcrtdt)
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         CALL cl_err( '', 9035, 0 )
         EXIT FOREACH
      END IF
   END FOREACH
    CALL aooi430_b_fill(g_wc2)
END FUNCTION]]>
  </point>
