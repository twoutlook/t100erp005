      #LET g_oogh2_d[l_ac].ooghowndp_desc = cl_get_deptname(g_oogh2_d[l_ac].ooghowndp)
      #LET g_oogh2_d[l_ac].ooghcrtid_desc = cl_get_username(g_oogh2_d[l_ac].ooghcrtid)
      #LET g_oogh2_d[l_ac].ooghcrtdp_desc = cl_get_deptname(g_oogh2_d[l_ac].ooghcrtdp)
      #LET g_oogh2_d[l_ac].ooghmodid_desc = cl_get_username(g_oogh2_d[l_ac].ooghmodid)
      
 
 
 
        
         #add-point:單身資料抓取
