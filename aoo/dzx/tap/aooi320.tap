  <point name="function.aooi320_multi_insert" cite_std="N" status="" ver="1" src="s" new="Y" cite_ver="" order="1" mark_hard="N">
<![CDATA[# 開窗後新增到oocr_t
PUBLIC FUNCTION aooi320_multi_insert(p_str)
DEFINE p_str     STRING
DEFINE l_token   base.StringTokenizer
DEFINE l_oocr    RECORD LIKE oocr_t.*
DEFINE l_now     DATETIME YEAR TO SECOND

   INITIALIZE l_oocr.* TO NULL
   LET l_oocr.oocrent = g_enterprise
   LET l_oocr.oocr001 = g_oocr001
   LET l_oocr.oocr002 = g_oocr_m.oocr002
   LET l_oocr.oocrownid = g_user
   LET l_oocr.oocrowndp = g_dept
   LET l_oocr.oocrcrtid = g_user
   LET l_oocr.oocrcrtdp = g_dept
   LET l_now = cl_get_current()
   LET l_oocr.oocrstus = "Y"
   
   LET l_token = base.StringTokenizer.create(p_str, "|")

   CALL s_transaction_begin()
   
   WHILE l_token.hasMoreTokens()
      LET l_oocr.oocr003 = l_token.nextToken()

      INSERT INTO oocr_t (oocrent,oocr001,oocr002,oocr003,oocrownid,oocrowndp,oocrcrtid,oocrcrtdp,oocrcrtdt,oocrstus)
             VALUES (l_oocr.oocrent,l_oocr.oocr001,l_oocr.oocr002,l_oocr.oocr003,l_oocr.oocrownid,l_oocr.oocrowndp,l_oocr.oocrcrtid,
                     l_oocr.oocrcrtdp,l_now,l_oocr.oocrstus)
      IF SQLCA.sqlcode OR SQLCA.sqlerrd[3] = 0 THEN
         CALL cl_err('INSERT',"std-00006",1)
         CALL s_transaction_end('N','0')
         EXIT WHILE
      END IF
   END WHILE
   
   CALL s_transaction_end('Y','0')
END FUNCTION]]>
</point>
  <point name="function.aooi320_check_oocr001" cite_std="N" status="" ver="1" src="s" new="Y" cite_ver="" order="2" mark_hard="N">
<![CDATA[#+ 檢查是否存在於應用分類檔內並有效
PRIVATE FUNCTION aooi320_check_oocr001(p_oocr001)
DEFINE p_oocr001     LIKE oocr_t.oocr001
DEFINE l_gzaastus   LIKE gzaa_t.gzaastus

   LET g_errno = ''
   SELECT gzaastus INTO l_gzaastus FROM gzaa_t
    WHERE gzaa001 = p_oocr001
    
   CASE
      WHEN SQLCA.sqlcode = 100  LET g_errno = 'aoo-00086'
      WHEN l_gzaastus = 'N'     LET g_errno = 'aoo-00087'
      OTHERWISE                 LET g_errno = SQLCA.sqlcode USING '-----'
   END CASE
   
END FUNCTION]]>
</point>
