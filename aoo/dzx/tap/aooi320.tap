  <section id="aooi320.input" ver="2" status="" src="s">
<![CDATA[#+ 資料輸入
PRIVATE FUNCTION aooi320_input(p_cmd)
   DEFINE  p_cmd                 LIKE type_t.chr1
   DEFINE  l_cmd_t               LIKE type_t.chr1
   DEFINE  l_cmd                 LIKE type_t.chr1
   DEFINE  l_ac_t                LIKE type_t.num5                #未取消的ARRAY CNT 
   DEFINE  l_n                   LIKE type_t.num5                #檢查重複用  
   DEFINE  l_cnt                 LIKE type_t.num5                #檢查重複用  
   DEFINE  l_lock_sw             LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert        LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete        LIKE type_t.num5                #可刪除否  
   DEFINE  l_count               LIKE type_t.num5
   DEFINE  l_i                   LIKE type_t.num5
   DEFINE  l_insert              BOOLEAN
   DEFINE  ls_return             STRING
   DEFINE  l_var_keys            DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys          DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                DYNAMIC ARRAY OF STRING
   DEFINE  l_fields              DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak        DYNAMIC ARRAY OF STRING
   DEFINE  lb_reproduce          BOOLEAN
   DEFINE  li_reproduce          LIKE type_t.num5
   DEFINE  li_reproduce_target   LIKE type_t.num5
   #add-point:input段define
   {<point name="input.define"/>}
   #end add-point    
   
   #先做狀態判定
   IF p_cmd = 'r' THEN
      LET l_cmd_t = 'r'
      LET p_cmd   = 'a'
   ELSE
      LET l_cmd_t = p_cmd
   END IF   
   
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF  
 
   CALL cl_set_head_visible("","YES")  
 
   LET l_insert = FALSE
   LET g_action_choice = ""
 
   #add-point:input段define_sql
   {<point name="input.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT oocrstus,oocr003,'','',oocrmodid,'',oocrmoddt,oocrownid,'',oocrowndp,'', 
       oocrcrtid,'',oocrcrtdp,'',oocrcrtdt FROM oocr_t WHERE oocrent=? AND oocr001=? AND oocr002=? AND  
       oocr003=? FOR UPDATE"
   #add-point:input段define_sql
   {<point name="input.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   DECLARE aooi320_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR
 
 
   
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
   LET g_qryparam.state = 'i'
   
   #控制key欄位可否輸入
   CALL aooi320_set_entry(p_cmd)
   #add-point:set_entry後
   {<point name="input.after_set_entry"/>}
   #end add-point
   CALL aooi320_set_no_entry(p_cmd)
   #add-point:set_no_entry後
   {<point name="input.after_set_no_entry"/>}
   #end add-point
 
   DISPLAY BY NAME g_oocr_m.oocr001,g_oocr_m.oocr002
   
   LET lb_reproduce = FALSE
   
   #add-point:進入修改段前
   {<point name="input.before_input"/>}
   #end add-point
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
]]>
</section>
  <section id="aooi320.input.body" ver="4" status="" src="s">
<![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_oocr_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂單身ACTION
         
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_oocr_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL aooi320_b_fill(g_wc2) 
            IF g_rec_b != 0 THEN
               CALL fgl_set_arr_curr(l_ac)
            END IF
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN aooi320_cl USING g_enterprise,
                                               g_oocr_m.oocr001
                                               ,g_oocr_m.oocr002
 
                                               
               IF STATUS THEN
                  CALL cl_err("OPEN aooi320_cl:", STATUS, 1)
                  CLOSE aooi320_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_oocr_d[l_ac].oocr003 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_oocr_d_t.* = g_oocr_d[l_ac].*  #BACKUP
               CALL aooi320_set_entry_b(l_cmd)
               CALL aooi320_set_no_entry_b(l_cmd)
               OPEN aooi320_bcl USING g_enterprise,g_oocr_m.oocr001,
                                                g_oocr_m.oocr002,
 
                                                g_oocr_d_t.oocr003
 
               IF STATUS THEN
                  CALL cl_err("OPEN aooi320_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aooi320_bcl INTO g_oocr_d[l_ac].oocrstus,g_oocr_d[l_ac].oocr003,g_oocr_d[l_ac].oocr003_desc, 
                      g_oocr1_info_d[l_ac].oocr003,g_oocr1_info_d[l_ac].oocrmodid,g_oocr1_info_d[l_ac].oocrmodid_desc, 
                      g_oocr1_info_d[l_ac].oocrmoddt,g_oocr1_info_d[l_ac].oocrownid,g_oocr1_info_d[l_ac].oocrownid_desc, 
                      g_oocr1_info_d[l_ac].oocrowndp,g_oocr1_info_d[l_ac].oocrowndp_desc,g_oocr1_info_d[l_ac].oocrcrtid, 
                      g_oocr1_info_d[l_ac].oocrcrtid_desc,g_oocr1_info_d[l_ac].oocrcrtdp,g_oocr1_info_d[l_ac].oocrcrtdp_desc, 
                      g_oocr1_info_d[l_ac].oocrcrtdt
                  IF SQLCA.sqlcode THEN
                      CALL cl_err(g_oocr_d_t.oocr003,SQLCA.sqlcode,1)
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL aooi320_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
        
         BEFORE INSERT
            
            INITIALIZE g_oocr_d_t.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_oocr_d[l_ac].* TO NULL
            
            #公用欄位預設值
            #此段落由子樣板a14產生    
      LET g_oocr1_info_d[l_ac].oocrownid = g_user
      LET g_oocr1_info_d[l_ac].oocrowndp = g_dept
      LET g_oocr1_info_d[l_ac].oocrcrtid = g_user
      LET g_oocr1_info_d[l_ac].oocrcrtdp = g_dept 
      LET g_oocr1_info_d[l_ac].oocrcrtdt = cl_get_current()
      LET g_oocr1_info_d[l_ac].oocrmodid = ""
      LET g_oocr1_info_d[l_ac].oocrmoddt = ""
      #LET g_oocr_d[l_ac].oocrstus = ""
 
  
            
            #一般欄位預設值
            
            
            
            LET g_oocr_d_t.* = g_oocr_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL aooi320_set_entry_b(l_cmd)
            CALL aooi320_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_oocr_d[li_reproduce_target].* = g_oocr_d[li_reproduce].*
               LET g_oocr1_info_d[li_reproduce_target].* = g_oocr1_info_d[li_reproduce].*
 
               LET g_oocr_d[g_oocr_d.getLength()].oocr003 = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
 
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM oocr_t 
             WHERE oocrent = g_enterprise AND oocr001 = g_oocr_m.oocr001
               AND oocr002 = g_oocr_m.oocr002
 
               AND oocr003 = g_oocr_d[l_ac].oocr003
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               CALL s_transaction_begin()
               #add-point:單身新增前
               {<point name="input.body.b_insert" mark="Y"/>}
               #end add-point
               INSERT INTO oocr_t
                           (oocrent,
                            oocr001,oocr002,
                            oocr003
                            ,oocrstus,oocrmodid,oocrmoddt,oocrownid,oocrowndp,oocrcrtid,oocrcrtdp,oocrcrtdt) 
                     VALUES(g_enterprise,
                            g_oocr_m.oocr001,g_oocr_m.oocr002,
                            g_oocr_d[l_ac].oocr003
                            ,g_oocr_d[l_ac].oocrstus,g_oocr1_info_d[l_ac].oocrmodid,g_oocr1_info_d[l_ac].oocrmoddt, 
                                g_oocr1_info_d[l_ac].oocrownid,g_oocr1_info_d[l_ac].oocrowndp,g_oocr1_info_d[l_ac].oocrcrtid, 
                                g_oocr1_info_d[l_ac].oocrcrtdp,g_oocr1_info_d[l_ac].oocrcrtdt)
               #add-point:單身新增中
               {<point name="input.body.m_insert"/>}
               #end add-point
               LET p_cmd = 'u'
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_oocr_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("oocr_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR "INSERT O.K"
               LET g_rec_b=g_rec_b+1
               DISPLAY g_rec_b TO FORMONLY.cnt
            END IF
            
            #add-point:單身新增後
            {<point name="input.body.after_insert"/>}
            #end add-point
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_oocr_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_oocr_d.deleteElement(l_ac)
               NEXT FIELD oocr003
            END IF
            IF g_oocr_d_t.oocr003 IS NOT NULL
 
               THEN
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete"/>}
               #end add-point
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               IF aooi320_before_delete() THEN 
                  CALL s_transaction_end('Y','0')
               ELSE 
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               END IF 
               CLOSE aooi320_bcl
               LET l_count = g_oocr_d.getLength()
            END IF 
            
            #add-point:單身刪除後
            {<point name="input.body.a_delete"/>}
            #end add-point
              
         AFTER DELETE 
            #add-point:單身AFTER DELETE 
            {<point name="input.body.after_delete"/>}
            #end add-point
 
         #---------------------<  Detail: page1  >---------------------
         #----<<oocrstus>>----
         #----<<oocr003>>----
         #此段落由子樣板a02產生
         AFTER FIELD oocr003
            
            #add-point:AFTER FIELD oocr003
            {<point name="input.a.page1.oocr003" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD oocr003
            #add-point:BEFORE FIELD oocr003
            {<point name="input.b.page1.oocr003" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE oocr003
            #add-point:ON CHANGE oocr003
            {<point name="input.g.page1.oocr003" />}
            #END add-point
 
         #----<<oocr003_desc>>----
 
         #---------------------<  Detail: page1  >---------------------
         #----<<oocrstus>>----
         #----<<oocr003>>----
         #Ctrlp:input.c.page1.oocr003
         ON ACTION controlp INFIELD oocr003
            #add-point:ON ACTION controlp INFIELD oocr003
            {<point name="input.c.page1.oocr003" />}
            #END add-point
 
         #----<<oocr003_desc>>----
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_oocr_d[l_ac].* = g_oocr_d_t.*
               CLOSE aooi320_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_oocr_d[l_ac].oocr003,-263,1)
               LET g_oocr_d[l_ac].* = g_oocr_d_t.*
            ELSE
               #寫入修改者/修改日期資訊
               LET g_oocr1_info_d[l_ac].oocrmodid = g_user 
LET g_oocr1_info_d[l_ac].oocrmoddt = cl_get_current()
 
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
         
               UPDATE oocr_t SET (oocr001,oocr002,oocrstus,oocr003,oocrmodid,oocrmoddt,oocrownid,oocrowndp, 
                   oocrcrtid,oocrcrtdp,oocrcrtdt) = (g_oocr_m.oocr001,g_oocr_m.oocr002,g_oocr_d[l_ac].oocrstus, 
                   g_oocr_d[l_ac].oocr003,g_oocr1_info_d[l_ac].oocrmodid,g_oocr1_info_d[l_ac].oocrmoddt, 
                   g_oocr1_info_d[l_ac].oocrownid,g_oocr1_info_d[l_ac].oocrowndp,g_oocr1_info_d[l_ac].oocrcrtid, 
                   g_oocr1_info_d[l_ac].oocrcrtdp,g_oocr1_info_d[l_ac].oocrcrtdt)
                WHERE oocrent = g_enterprise AND oocr001 = g_oocr_m.oocr001 
                 AND oocr002 = g_oocr_m.oocr002 
 
                 AND oocr003 = g_oocr_d_t.oocr003 #項次   
 
                 
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
               
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("oocr_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                  #WHEN SQLCA.sqlcode #其他錯誤
                  #   CALL cl_err("oocr_t",SQLCA.sqlcode,1)  
                  #   CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_oocr_m.oocr001
               LET gs_keys_bak[1] = g_oocr001_t
               LET gs_keys[2] = g_oocr_m.oocr002
               LET gs_keys_bak[2] = g_oocr002_t
               LET gs_keys[3] = g_oocr_d[g_detail_idx].oocr003
               LET gs_keys_bak[3] = g_oocr_d_t.oocr003
               CALL aooi320_update_b('oocr_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
               END CASE
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
            END IF
 
         AFTER INPUT
            #若單身還沒有輸入資料, 強制切換至單身
            IF cl_null(g_oocr_d[1].oocr003) THEN
               CALL g_oocr_d.deleteElement(1)
               NEXT FIELD oocr003
            END IF
            #add-point:input段after input 
            {<point name="input.body.after_input"/>}
            #end add-point    
            
         ON ACTION controlo   
            CALL FGL_SET_ARR_CURR(g_oocr_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_oocr_d.getLength()+1
            
      END INPUT
 
 
      
      DISPLAY ARRAY g_oocr1_info_d TO s_detail1_info.* ATTRIBUTES(COUNT=g_rec_b)  
      
         BEFORE ROW
            CALL aooi320_b_fill(g_wc2) 
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1_info")
            LET l_ac = g_detail_idx
            DISPLAY g_detail_idx TO FORMONLY.idx
            CALL aooi320_ui_detailshow()
        
         BEFORE DISPLAY 
            CALL FGL_SET_ARR_CURR(g_detail_idx)      
         
         #add-point:page2自定義行為
         {<point name="input.body1_info.action"/>}
         #end add-point
         
      END DISPLAY
 
      
 
      
      #add-point:input段more_input
      {<point name="input.more_inputarray"/>}
      #end add-point    
      
      BEFORE DIALOG
         #add-point:input段before_dialog
         {<point name="input.before_dialog"/>}
         #end add-point 
         #新增時強制從單頭開始填
         IF p_cmd = 'a' THEN
            NEXT FIELD oocr001
        ELSE
            CASE g_aw
               WHEN "s_detail1"
                  NEXT FIELD oocrstus
               WHEN "s_detail1_info"
                  NEXT FIELD oocr003_2
 
            END CASE
         END IF
   
      ON ACTION controlf
         CALL cl_set_focus_form(ui.Interface.getRootNode()) RETURNING g_fld_name,g_frm_name
         CALL cl_fldhelp(g_frm_name,g_fld_name,g_lang)
 
      ON ACTION controlr
         CALL cl_show_req_fields()
 
      ON ACTION controls
         IF g_header_hidden THEN
            CALL gfrm_curr.setElementHidden("vb_master",0)
            CALL gfrm_curr.setElementImage("controls","small/arr-u.png")
            LET g_header_hidden = 0     #visible
         ELSE
            CALL gfrm_curr.setElementHidden("vb_master",1)
            CALL gfrm_curr.setElementImage("controls","small/arr-d.png")
            LET g_header_hidden = 1     #hidden     
         END IF
 
      ON ACTION accept
         ACCEPT DIALOG
        
      ON ACTION cancel      #在dialog button (放棄)
         LET g_action_choice=""
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION close       #在dialog 右上角 (X)
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION exit        #toolbar 離開
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      #交談指令共用ACTION
      &include "common_action.4gl" 
         CONTINUE DIALOG 
   END DIALOG
   
   #add-point:input段after_input
   {<point name="input.after_input"/>}
   #end add-point  
   
END FUNCTION
]]>
</section>
  <section id="aooi320.input.head" ver="3" status="" src="s">
<![CDATA[   
      #單頭段
      INPUT BY NAME g_oocr_m.oocr001,g_oocr_m.oocr002 
         ATTRIBUTE(WITHOUT DEFAULTS)
         
         #自訂單頭ACTION
         
         
         BEFORE INPUT
            IF s_transaction_chk("N",0) THEN
               CALL s_transaction_begin()
            END IF
            
            IF l_cmd_t = 'r' THEN
               
            END IF
            #add-point:單頭input前
            {<point name="input.head.b_input"/>}
            #end add-point
          
         #---------------------------<  Master  >---------------------------
         #----<<oocr001>>----
         #此段落由子樣板a01產生
         BEFORE FIELD oocr001
            #add-point:BEFORE FIELD oocr001
            {<point name="input.b.oocr001" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD oocr001
            
            #add-point:AFTER FIELD oocr001
            {<point name="input.a.oocr001" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE oocr001
            #add-point:ON CHANGE oocr001
            {<point name="input.g.oocr001" />}
            #END add-point
 
         #----<<oocr002>>----
         #此段落由子樣板a02產生
         AFTER FIELD oocr002
            
            #add-point:AFTER FIELD oocr002
            {<point name="input.a.oocr002" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD oocr002
            #add-point:BEFORE FIELD oocr002
            {<point name="input.b.oocr002" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE oocr002
            #add-point:ON CHANGE oocr002
            {<point name="input.g.oocr002" />}
            #END add-point
 
         #----<<oocr002_desc>>----
 #欄位檢查
         #---------------------------<  Master  >---------------------------
         #----<<oocr001>>----
         #Ctrlp:input.c.oocr001
         ON ACTION controlp INFIELD oocr001
            #add-point:ON ACTION controlp INFIELD oocr001
            {<point name="input.c.oocr001" />}
            #END add-point
 
         #----<<oocr002>>----
         #Ctrlp:input.c.oocr002
         ON ACTION controlp INFIELD oocr002
            #add-point:ON ACTION controlp INFIELD oocr002
            {<point name="input.c.oocr002" />}
            #END add-point
 
         #----<<oocr002_desc>>----
 #欄位開窗
 
         AFTER INPUT
            IF INT_FLAG THEN
               EXIT DIALOG
            END IF
            
            IF s_transaction_chk("N",0) THEN
                CALL s_transaction_begin()
            END IF
 
            #多語言處理
            
                
            CALL cl_showmsg()
            DISPLAY BY NAME g_oocr_m.oocr001             
                            ,g_oocr_m.oocr002   
 
 
            IF p_cmd = 'u' THEN
               #add-point:單頭修改前
               {<point name="input.head.b_update" mark="Y"/>}
               #end add-point
            
               UPDATE oocr_t SET (oocr001,oocr002) = (g_oocr_m.oocr001,g_oocr_m.oocr002)
                WHERE oocrent = g_enterprise AND oocr001 = g_oocr001_t
                  AND oocr002 = g_oocr002_t
 
               #add-point:單頭修改中
               {<point name="input.head.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("oocr_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                  WHEN SQLCA.sqlcode #其他錯誤
                     CALL cl_err("oocr_t",SQLCA.sqlcode,1)  
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                     #資料多語言用-增/改
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_oocr_m.oocr001
               LET gs_keys_bak[1] = g_oocr001_t
               LET gs_keys[2] = g_oocr_m.oocr002
               LET gs_keys_bak[2] = g_oocr002_t
               LET gs_keys[3] = g_oocr_d[g_detail_idx].oocr003
               LET gs_keys_bak[3] = g_oocr_d_t.oocr003
               CALL aooi320_update_b('oocr_t',gs_keys,gs_keys_bak,"'1'")
                     
                     LET g_oocr001_t = g_oocr_m.oocr001
                     LET g_oocr002_t = g_oocr_m.oocr002
 
                     #add-point:單頭修改後
                     {<point name="input.head.a_update"/>}
                     #end add-point
                     CALL s_transaction_end('Y','0')
               END CASE
            
            ELSE    
               #add-point:單頭新增
               {<point name="input.head.a_insert"/>}
               #end add-point
                                 
               IF l_cmd_t = 'r' AND p_cmd = 'a' THEN
                  CALL aooi320_detail_reproduce()
               END IF
            END IF
           #controlp
                     
           LET g_oocr001_t = g_oocr_m.oocr001
           LET g_oocr002_t = g_oocr_m.oocr002
 
           
           #若單身還沒有輸入資料, 強制切換至單身
           IF cl_null(g_oocr_d[1].oocr003) THEN
              CALL g_oocr_d.deleteElement(1)
              NEXT FIELD oocr003
           END IF
 
      END INPUT
]]>
</section>
  <section id="aooi320.insert" ver="2" status="" src="s">
<![CDATA[#+ 資料新增
PRIVATE FUNCTION aooi320_insert()
   #add-point:insert段define
   {<point name="insert.define"/>}
   #end add-point    
   
   #add-point:insert段before
   {<point name="insert.before"/>}
   #end add-point    
   
   #清除相關資料
   CLEAR FORM                    
   CALL g_oocr_d.clear()
   CALL g_oocr1_info_d.clear()
 
 
   INITIALIZE g_oocr_m.* LIKE oocr_t.*             #DEFAULT 設定
   LET g_oocr001_t = NULL
   LET g_oocr002_t = NULL
 
   CALL s_transaction_begin()
   WHILE TRUE
     
      #單頭預設值
      
     
      #add-point:單頭預設值
      {<point name="insert.default"/>}
      #end add-point 
 
      CALL aooi320_input("a")
      
      #add-point:單頭輸入後
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_oocr_m.* = g_oocr_m_t.*
         CALL aooi320_show()
