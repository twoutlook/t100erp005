   END IF 
   
   CLOSE aooi320_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
   
END MAIN
 
 
]]>
</section>
  <section id="aooi320.modify" ver="2" status="" src="s">
<![CDATA[#+ 資料修改
PRIVATE FUNCTION aooi320_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_oocr_m.oocr001 IS NULL
   OR g_oocr_m.oocr002 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
    SELECT UNIQUE oocr001,oocr002
 INTO g_oocr_m.oocr001,g_oocr_m.oocr002
 FROM oocr_t
 WHERE oocrent = g_enterprise AND oocr001 = g_oocr_m.oocr001 AND oocr002 = g_oocr_m.oocr002
 
   ERROR ""
  
   LET g_oocr001_t = g_oocr_m.oocr001
   LET g_oocr002_t = g_oocr_m.oocr002
 
   CALL s_transaction_begin()
   
   OPEN aooi320_cl USING g_enterprise,g_oocr_m.oocr001
                                                       ,g_oocr_m.oocr002
 
   IF STATUS THEN
      CALL cl_err("OPEN aooi320_cl:", STATUS, 1)
      CLOSE aooi320_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aooi320_cl INTO g_oocr_m.oocr001,g_oocr_m.oocr002,g_oocr_m.oocr002_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_oocr_m.oocr001,SQLCA.sqlcode,0)
      CLOSE aooi320_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL aooi320_show()
   WHILE TRUE
      LET g_oocr001_t = g_oocr_m.oocr001
      LET g_oocr002_t = g_oocr_m.oocr002
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL aooi320_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_oocr_m.* = g_oocr_m_t.*
         CALL aooi320_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_oocr_m.oocr001 != g_oocr001_t 
      OR g_oocr_m.oocr002 != g_oocr002_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE oocr_t SET oocr001 = g_oocr_m.oocr001
                                       , oocr002 = g_oocr_m.oocr002
 
          WHERE oocrent = g_enterprise AND oocr001 = g_oocr001_t
            AND oocr002 = g_oocr002_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("oocr_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("oocr_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_oocr_m.oocr001,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE aooi320_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_oocr_m.oocr001,'U')
 
   CALL aooi320_b_fill("1=1")
   
END FUNCTION   
]]>
</section>
