  <section id="aooi320.b_fill" ver="4" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aooi320_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_oocr_d.clear()
   CALL g_oocr1_info_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
      LET g_sql = "SELECT  UNIQUE oocrstus,oocr003,'',oocr003,oocrmodid,'',oocrmoddt,oocrownid,'',oocrowndp, 
          '',oocrcrtid,'',oocrcrtdp,'',oocrcrtdt FROM oocr_t",   
                  "",
                  
                  " WHERE oocrent= ? AND oocr001=? AND oocr002=?"  
                  
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF aooi320_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY oocr_t.oocr003"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      PREPARE aooi320_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR aooi320_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_oocr_m.oocr001
                                               ,g_oocr_m.oocr002
 
                                               
      FOREACH b_fill_cs INTO g_oocr_d[l_ac].oocrstus,g_oocr_d[l_ac].oocr003,g_oocr_d[l_ac].oocr003_desc, 
          g_oocr1_info_d[l_ac].oocr003,g_oocr1_info_d[l_ac].oocrmodid,g_oocr1_info_d[l_ac].oocrmodid_desc, 
          g_oocr1_info_d[l_ac].oocrmoddt,g_oocr1_info_d[l_ac].oocrownid,g_oocr1_info_d[l_ac].oocrownid_desc, 
          g_oocr1_info_d[l_ac].oocrowndp,g_oocr1_info_d[l_ac].oocrowndp_desc,g_oocr1_info_d[l_ac].oocrcrtid, 
          g_oocr1_info_d[l_ac].oocrcrtid_desc,g_oocr1_info_d[l_ac].oocrcrtdp,g_oocr1_info_d[l_ac].oocrcrtdp_desc, 
          g_oocr1_info_d[l_ac].oocrcrtdt
                             ,g_oocr_d[l_ac].oocrstus,g_oocr_d[l_ac].oocr003,g_oocr_d[l_ac].oocr003_desc, 
                                 g_oocr1_info_d[l_ac].oocr003,g_oocr1_info_d[l_ac].oocrmodid,g_oocr1_info_d[l_ac].oocrmodid_desc, 
                                 g_oocr1_info_d[l_ac].oocrmoddt,g_oocr1_info_d[l_ac].oocrownid,g_oocr1_info_d[l_ac].oocrownid_desc, 
                                 g_oocr1_info_d[l_ac].oocrowndp,g_oocr1_info_d[l_ac].oocrowndp_desc, 
                                 g_oocr1_info_d[l_ac].oocrcrtid,g_oocr1_info_d[l_ac].oocrcrtid_desc, 
                                 g_oocr1_info_d[l_ac].oocrcrtdp,g_oocr1_info_d[l_ac].oocrcrtdp_desc, 
                                 g_oocr1_info_d[l_ac].oocrcrtdt
 
                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_oocr1_info_d[l_ac].oocrownid_desc = cl_get_username(g_oocr1_info_d[l_ac].oocrownid)
      #LET g_oocr1_info_d[l_ac].oocrowndp_desc = cl_get_deptname(g_oocr1_info_d[l_ac].oocrowndp)
      #LET g_oocr1_info_d[l_ac].oocrcrtid_desc = cl_get_username(g_oocr1_info_d[l_ac].oocrcrtid)
      #LET g_oocr1_info_d[l_ac].oocrcrtdp_desc = cl_get_deptname(g_oocr1_info_d[l_ac].oocrcrtdp)
      #LET g_oocr1_info_d[l_ac].oocrmodid_desc = cl_get_username(g_oocr1_info_d[l_ac].oocrmodid)
      ##LET .oocrcnfid_desc = cl_get_deptname(.oocrcnfid)
      ##LET .oocrpstid_desc = cl_get_deptname(.oocrpstid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_oocr_d.deleteElement(g_oocr_d.getLength())
      CALL g_oocr1_info_d.deleteElement(g_oocr1_info_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE aooi320_pb   
   
END FUNCTION
]]>
</section>
