               IF NOT aooi723_lock_b("ooap_t") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aooi723_bcl INTO g_ooap_d[l_ac].ooapstus,g_ooap_d[l_ac].ooap001,g_ooap_d[l_ac].ooap002, 
