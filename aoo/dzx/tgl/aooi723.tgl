#+ 單身陣列填充
PRIVATE FUNCTION aooi723_b_fill(p_wc2)              #BODY FILL UP
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   {<point name="b_fill.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE p_wc2            STRING
   DEFINE ls_owndept_list  STRING  #(ver:35) add
   DEFINE ls_ownuser_list  STRING  #(ver:35) add
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   {<point name="b_fill.define"/>}
   #end add-point
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   {<point name="b_fill.pre_function"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前 name="b_fill.sql_before"
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  DISTINCT t0.ooapstus,t0.ooap001,t0.ooap002,t0.ooap001,t0.ooapownid,t0.ooapowndp, 
       t0.ooapcrtid,t0.ooapcrtdp,t0.ooapcrtdt,t0.ooapmodid,t0.ooapmoddt ,t1.ooag011 ,t2.ooag011 ,t3.ooefl003 , 
       t4.ooag011 ,t5.ooefl003 ,t6.ooag011 FROM ooap_t t0",
               "",
                              " LEFT JOIN ooag_t t1 ON t1.ooagent="||g_enterprise||" AND t1.ooag001=t0.ooap001  ",
               " LEFT JOIN ooag_t t2 ON t2.ooagent="||g_enterprise||" AND t2.ooag001=t0.ooapownid  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent="||g_enterprise||" AND t3.ooefl001=t0.ooapowndp AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t4 ON t4.ooagent="||g_enterprise||" AND t4.ooag001=t0.ooapcrtid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent="||g_enterprise||" AND t5.ooefl001=t0.ooapcrtdp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent="||g_enterprise||" AND t6.ooag001=t0.ooapmodid  ",
 
               " WHERE t0.ooapent= ?  AND  1=1 AND (", p_wc2, ") "
 
   #(ver:35) ---add start---
      #應用 a68 樣板自動產生(Version:1)
   #若是修改，須視權限加上條件
   IF g_action_choice = "modify" OR g_action_choice = "modify_detail" THEN
      LET ls_owndept_list = NULL
      LET ls_ownuser_list = NULL
 
      #若有設定部門權限
      CALL cl_get_owndept_list("ooap_t","modify") RETURNING ls_owndept_list
      IF NOT cl_null(ls_owndept_list) THEN
         LET g_sql = g_sql, " AND ooapowndp IN (",ls_owndept_list,")"
      END IF
 
      #若有設定個人權限
      CALL cl_get_ownuser_list("ooap_t","modify") RETURNING ls_ownuser_list
      IF NOT cl_null(ls_ownuser_list) THEN
         LET g_sql = g_sql," AND ooapownid IN (",ls_ownuser_list,")"
      END IF
   END IF
 
 
 
   #(ver:35) --- add end ---
 
   #add-point:b_fill段sql wc name="b_fill.sql_wc"
   {<point name="b_fill.sql_wc"/>}
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("ooap_t"),
                      " ORDER BY t0.ooap001"
   
   #add-point:b_fill段sql之後 name="b_fill.sql_after"
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"ooap_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE aooi723_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aooi723_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_ooap_d.clear()
   CALL g_ooap2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_ooap_d[l_ac].ooapstus,g_ooap_d[l_ac].ooap001,g_ooap_d[l_ac].ooap002,g_ooap2_d[l_ac].ooap001, 
       g_ooap2_d[l_ac].ooapownid,g_ooap2_d[l_ac].ooapowndp,g_ooap2_d[l_ac].ooapcrtid,g_ooap2_d[l_ac].ooapcrtdp, 
       g_ooap2_d[l_ac].ooapcrtdt,g_ooap2_d[l_ac].ooapmodid,g_ooap2_d[l_ac].ooapmoddt,g_ooap_d[l_ac].ooap001_desc, 
       g_ooap2_d[l_ac].ooapownid_desc,g_ooap2_d[l_ac].ooapowndp_desc,g_ooap2_d[l_ac].ooapcrtid_desc, 
       g_ooap2_d[l_ac].ooapcrtdp_desc,g_ooap2_d[l_ac].ooapmodid_desc
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH b_fill_curs:",SQLERRMESSAGE 
         LET g_errparam.code = SQLCA.SQLCODE
         LET g_errparam.popup = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充 name="b_fill.fill"
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL aooi723_detail_show()      
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
 
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   LET g_error_show = 0
   
 
   
   CALL g_ooap_d.deleteElement(g_ooap_d.getLength())   
   CALL g_ooap2_d.deleteElement(g_ooap2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_ooap_d.getLength()
      LET g_ooap2_d[l_ac].ooap001 = g_ooap_d[l_ac].ooap001 
 
      #add-point:b_fill段key值相關欄位 name="b_fill.keys.fill"
      {<point name="b_fill.keys.fill"/>}
      #end add-point
   END FOR
   
   IF g_cnt > g_ooap_d.getLength() THEN
      LET l_ac = g_ooap_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_ooap_d.getLength()
      LET g_ooap_d_mask_o[l_ac].* =  g_ooap_d[l_ac].*
      CALL aooi723_ooap_t_mask()
      LET g_ooap_d_mask_n[l_ac].* =  g_ooap_d[l_ac].*
   END FOR
   
   LET g_ooap2_d_mask_o.* =  g_ooap2_d.*
   FOR l_ac = 1 TO g_ooap2_d.getLength()
      LET g_ooap2_d_mask_o[l_ac].* =  g_ooap2_d[l_ac].*
      CALL aooi723_ooap_t_mask()
      LET g_ooap2_d_mask_n[l_ac].* =  g_ooap2_d[l_ac].*
   END FOR
 
   
   LET l_ac = g_cnt
 
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   {<point name="b_fill.others.fill"/>}
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_ooap_d.getLength()
   DISPLAY g_detail_idx TO FORMONLY.idx
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE aooi723_pb
   
END FUNCTION

