{<section id="aooi900.update_b" >}
#+ 修改單身後其他table連動
PRIVATE FUNCTION aooi900_update_b(ps_table,ps_keys,ps_keys_bak,ps_page)
   DEFINE ps_page          STRING
   DEFINE ps_table         STRING
   DEFINE ps_keys          DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ps_keys_bak      DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ls_group         STRING
   DEFINE li_idx           LIKE type_t.num5 
   DEFINE lb_chk           BOOLEAN
   DEFINE l_new_key        DYNAMIC ARRAY OF STRING
   DEFINE l_old_key        DYNAMIC ARRAY OF STRING
   DEFINE l_field_key      DYNAMIC ARRAY OF STRING
   #add-point:update_b段define
{<point name="update_b.define"/>}
   #end add-point     
   
   #比對新舊值, 判斷key是否有改變
   LET lb_chk = TRUE
   FOR li_idx = 1 TO ps_keys.getLength()
      IF ps_keys[li_idx] <> ps_keys_bak[li_idx] THEN
         LET lb_chk = FALSE
         EXIT FOR
      END IF
   END FOR
   
   #若key無變動, 不需要做處理
   IF lb_chk THEN
      RETURN
   END IF
    
   #若key有變動, 則連動其他table的資料   
   #判斷是否是同一群組的table
   LET ls_group = "oojj_t,"
   IF ls_group.getIndexOf(ps_table,1) > 0 AND ps_table <> "oojj_t" THEN
      #add-point:update_b段修改前
{<point name="update_b.b_update" mark="Y"/>}
      #end add-point     
      UPDATE oojj_t 
         SET (oojjsite,oojj001,oojj002
              ,oojjstus,oojj006,oojj003,oojj004,oojj005,oojj007,oojjownid,oojjowndp,oojjcrtid,oojjcrtdp,oojjcrtdt,oojjmodid,oojjmoddt) 
              = 
             (ps_keys[1],ps_keys[2],ps_keys[3]
              ,g_oojj_d[l_ac].oojjstus,g_oojj_d[l_ac].oojj006,g_oojj_d[l_ac].oojj003,g_oojj_d[l_ac].oojj004, 
                  g_oojj_d[l_ac].oojj005,g_oojj_d[l_ac].oojj007,g_oojj2_d[l_ac].oojjownid,g_oojj2_d[l_ac].oojjowndp,g_oojj2_d[l_ac].oojjcrtid,
                  g_oojj2_d[l_ac].oojjcrtdp,g_oojj2_d[l_ac].oojjcrtdt,g_oojj2_d[l_ac].oojjmodid,g_oojj2_d[l_ac].oojjmoddt)  

         WHERE oojjsite = ps_keys_bak[1] AND oojj001 = ps_keys_bak[2] AND oojj002 = ps_keys_bak[3]
      #add-point:update_b段修改中
{<point name="update_b.m_update"/>}
      #end add-point 
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "oojj_t" 
            LET g_errparam.code   = "std-00009" 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
            CALL s_transaction_end('N','0')
         WHEN SQLCA.sqlcode #其他錯誤
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "oojj_t" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
            CALL s_transaction_end('N','0')
         OTHERWISE
            
      END CASE
      #add-point:update_b段修改後
{<point name="update_b.a_update"/>}
      #end add-point 
      RETURN
   END IF
   
 
   
END FUNCTION

{</section>}

