{<section id="aooi900.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION aooi900_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2    STRING
   #add-point:b_fill段define
{<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
{<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE t0.oojjstus,t0.oojjsite,t0.oojj006,t0.oojj003,t0.oojj004,t0.oojj001,t0.oojj002, 
       t0.oojj005,t0.oojj007,t0.oojjownid,t0.oojjowndp,t0.oojjcrtid,t0.oojjcrtdp,t0.oojjcrtdt,t0.oojjmodid,t0.oojjmoddt,  
       t0.oojj001,t0.oojj002,t0.oojjsite ,t1.gzzol003 FROM oojj_t t0",
               "",
                              " LEFT JOIN gzzol_t t1 ON t1.gzzol001=t0.oojj003 AND t1.gzzol002='"||g_lang||"' ",
 
               #" WHERE t0.oojjent= ?  AND  1=1 AND (", p_wc2, ") "   #140825 mark
               " WHERE t0.oojjent= ?  AND t0.oojjsite ='default' AND  1=1 AND (", p_wc2, ") "    #140825 add 只撈site=default 
    
   LET g_sql = g_sql, cl_sql_add_filter("oojj_t"),
                      " ORDER BY t0.oojjsite,t0.oojj001,t0.oojj002"
   
   #add-point:b_fill段sql之後
{<point name="b_fill.sql_after"/>}
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"oojj_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE aooi900_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aooi900_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_oojj_d.clear()
   CALL g_oojj2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_oojj_d[l_ac].oojjstus,g_oojj_d[l_ac].oojjsite,g_oojj_d[l_ac].oojj006,g_oojj_d[l_ac].oojj003, 
       g_oojj_d[l_ac].oojj004,g_oojj_d[l_ac].oojj001,g_oojj_d[l_ac].oojj002,g_oojj_d[l_ac].oojj005,g_oojj_d[l_ac].oojj007,g_oojj2_d[l_ac].oojjownid,
       g_oojj2_d[l_ac].oojjowndp,g_oojj2_d[l_ac].oojjcrtid,g_oojj2_d[l_ac].oojjcrtdp,g_oojj2_d[l_ac].oojjcrtdt, 
       g_oojj2_d[l_ac].oojjmodid,g_oojj2_d[l_ac].oojjmoddt,g_oojj2_d[l_ac].oojj001,g_oojj2_d[l_ac].oojj002, 
       g_oojj2_d[l_ac].oojjsite,g_oojj_d[l_ac].oojj003_desc
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
{<point name="b_fill.fill"/>}
        
      #end add-point
      
      CALL aooi900_detail_show()      
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
 
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   LET g_error_show = 0
   
 
   
   CALL g_oojj_d.deleteElement(g_oojj_d.getLength())   
   CALL g_oojj2_d.deleteElement(g_oojj2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_oojj_d.getLength()
      LET g_oojj2_d[l_ac].oojjsite = g_oojj_d[l_ac].oojjsite 
      LET g_oojj2_d[l_ac].oojj001 = g_oojj_d[l_ac].oojj001 
      LET g_oojj2_d[l_ac].oojj002 = g_oojj_d[l_ac].oojj002 
 
   END FOR
   
   IF g_cnt > g_oojj_d.getLength() THEN
      LET l_ac = g_oojj_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #add-point:b_fill段資料填充(其他單身)
{<point name="b_fill.others.fill"/>}
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_oojj_d.getLength()
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE aooi900_pb
   
END FUNCTION

