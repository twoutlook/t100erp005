#+ 資料複製
PRIVATE FUNCTION aooi014_reproduce()
   {<Local define>}
   DEFINE l_newno     LIKE oock_t.oock001 
   DEFINE l_oldno     LIKE oock_t.oock001 
      #key2
   DEFINE l_newno02     LIKE oock_t.oock002 
   DEFINE l_oldno02     LIKE oock_t.oock002 


   DEFINE l_master    RECORD LIKE oock_t.*
   DEFINE l_detail    RECORD LIKE oock_t.*
   DEFINE l_cnt       LIKE type_t.num5
   {</Local define>}
   
   #add-point:reproduce段define
   {<point name="reproduce.define"/>}
   #end add-point   
 
   IF g_oock_m.oock001 IS NULL
      #key2
      OR g_oock_m.oock002 IS NULL


      THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
    
   LET g_before_input_done = FALSE
   CALL aooi014_set_entry('a')
   LET g_before_input_done = TRUE
 
   CALL cl_set_head_visible("","YES")
 
   INPUT l_newno #FROM 
      #key2
         ,l_newno02


    FROM oock001 
      #key2
         ,oock002 


      BEFORE INPUT
            
      AFTER INPUT
         IF INT_FLAG = 1 THEN
            EXIT INPUT
         END IF
         IF l_newno IS NULL 
      #key2
            OR l_newno02 IS NULL


            THEN
         CALL cl_err("Reproduce","std-00006",1) 
         NEXT FIELD oock001 
         END IF
         #確定該key值是否有重複定義
         LET l_cnt = 0
         SELECT COUNT(*) INTO l_cnt FROM oock_t 
          WHERE oockent = g_enterprise AND oock001 = l_newno
      #key2
            AND oock002 = l_newno02


         IF l_cnt > 0 THEN
            CALL cl_err("Reproduce","std-00006",1)
            #NEXT FIELD oock001 
         END IF
 
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE INPUT
   END INPUT
   
   IF INT_FLAG OR l_newno IS NULL THEN
      LET INT_FLAG = 0
      RETURN
   END IF
   
   BEGIN WORK
   
   LET g_sql = "SELECT * FROM oock_t WHERE oockent = '" ||g_enterprise|| "' AND ",
               " oock001 = '",g_oock_m.oock001,"'"
      #key2
              ," AND oock002 = '",g_oock_m.oock002,"'"


   DECLARE aooi014_reproduce CURSOR FROM g_sql
 
   FOREACH aooi014_reproduce INTO l_detail.*
   
      #add-point:單身複製前
      {<point name="reproduce.body.b_insert"/>}
      #end add-point
   
      LET l_detail.oock001 = l_newno
      #key2
      LET l_detail.oock002 = l_newno02


      LET l_detail.oockuser = g_user
      LET l_detail.oockdept = g_dept
      LET l_detail.oockbuid = cl_get_current()
      LET l_detail.oockoriu = g_user 
      LET l_detail.oockorid = g_dept 
      LET l_detail.oockmodu = g_user
      LET l_detail.oockdate = cl_get_current()
      #LET l_detail.oockstus = 'Y'
 
      INSERT INTO oock_t VALUES (l_detail.*) #複製單身
      IF SQLCA.sqlcode THEN
         CALL cl_err('Insert error!',SQLCA.sqlcode,1)
         ROLLBACK WORK
         RETURN
      END IF
  
      #add-point:單身複製後
      {<point name="reproduce.body.a_insert"/>}
      #end add-point
   END FOREACH
 
   COMMIT WORK
   ERROR 'ROW(',l_newno,') O.K'
   LET l_oldno = g_oock_m.oock001
      #key2
   LET l_oldno02 = g_oock_m.oock002


   
   SELECT oock001,oock002 INTO g_oock_m.oock001,g_oock_m.oock002 FROM oock_t 
    WHERE oockent = g_enterprise AND oock001 = l_newno
      #key2
      AND oock002 = l_newno02


      
   CALL aooi014_show()
   LET g_oock_m.oock001 = l_oldno
      #key2
   LET g_oock_m.oock002 = l_oldno02


   
   SELECT oock001,oock002 INTO g_oock_m.oock001,g_oock_m.oock002 FROM oock_t 
    WHERE oockent = g_enterprise AND oock001 = g_oock_m.oock001
      #key2
      AND oock002 = g_oock_m.oock002


   CALL aooi014_show()
 
   DISPLAY BY NAME g_oock_m.oock001
      #key2
   DISPLAY BY NAME g_oock_m.oock002


   
END FUNCTION
 
#+ 資料刪除
PRIVATE FUNCTION aooi014_delete()
   {<Local define>}
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   {</Local define>}
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_oock_m.oock001 IS NULL
      #key2
   OR g_oock_m.oock002 IS NULL


   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
    SELECT UNIQUE oock001,oock002
 INTO g_oock_m.oock001,g_oock_m.oock002
 FROM oock_t
 WHERE oockent = g_enterprise AND oock001 = g_oock_m.oock001 AND oock002 = g_oock_m.oock002
   BEGIN WORK
   
   
 
   OPEN aooi014_cl USING g_enterprise,g_oock_m.oock001
      #key2
                                                       ,g_oock_m.oock002


   IF STATUS THEN
      CALL cl_err("OPEN aooi014_cl:", STATUS, 1)
      CLOSE aooi014_cl
      ROLLBACK WORK
      RETURN
   END IF
 
   FETCH aooi014_cl INTO g_oock_m.oock001,g_oock_m.oock002,g_oock_m.oock002_desc,g_oock_m.oock001_desc              # 鎖住將被更改或取消的資料
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_oock_m.oock001,SQLCA.sqlcode,0)          #資料被他人LOCK
      ROLLBACK WORK
      RETURN
   END IF
 
   CALL aooi014_show()
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      INITIALIZE g_doc.* TO NULL         
      LET g_doc.column1 = "oock001"       
      LET g_doc.value1 = g_oock_m.oock001     
      CALL cl_del_doc()    
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete"/>}
      #end add-point
      
      DELETE FROM oock_t WHERE oockent = g_enterprise AND oock001 = g_oock_m.oock001
      #key2
                                                               AND oock002 = g_oock_m.oock002


      IF SQLCA.sqlcode THEN
         CALL cl_err("oock_t",SQLCA.sqlcode,0)
         ROLLBACK WORK
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
      CLEAR FORM
      CALL g_oock_d.clear() 

     
      CALL aooi014_ui_browser_refresh()  
      CALL aooi014_ui_headershow()  
      CALL aooi014_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL aooi014_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         CALL aooi014_browser_fill("first")
      END IF
       
   END IF
 
   CLOSE aooi014_cl
   COMMIT WORK
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_oock_m.oock001,'D')
    
END FUNCTION
 
 
#+ 單身陣列填充
PRIVATE FUNCTION aooi014_b_fill(p_wc2)
   {<Local define>}
   DEFINE p_wc2      STRING
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_oock_d.clear()    #g_oock_d 單頭及單身 

 
   LET g_sql = "SELECT oockstus,oock003,'','',oockmodu,'',oockdate,oockoriu,'',oockorid,'',oockuser,'',oockdept,'',oockbuid FROM oock_t WHERE oockent= ? AND oock001=? AND oock002=?"   
 
   IF NOT cl_null(p_wc2) THEN
      LET g_sql=g_sql CLIPPED," AND ",p_wc2 CLIPPED
   END IF
   
   LET g_sql = g_sql, " ORDER BY oock001,oock002,oock003"
 
   PREPARE aooi014_pb FROM g_sql
   DECLARE b_fill_cs CURSOR FOR aooi014_pb
 
   LET g_cnt = l_ac
   LET l_ac = 1
 
   OPEN b_fill_cs USING g_enterprise,g_oock_m.oock001
      #key2
                                            ,g_oock_m.oock002


                                            
   FOREACH b_fill_cs INTO g_oock_d[l_ac].oockstus,g_oock_d[l_ac].oock003,g_oock_d[l_ac].oockl005,g_oock_d[l_ac].oockl006,g_oock_d[l_ac].oockmodu,g_oock_d[l_ac].modu_desc,g_oock_d[l_ac].oockdate,g_oock_d[l_ac].oockoriu,g_oock_d[l_ac].oriu_desc,g_oock_d[l_ac].oockorid,g_oock_d[l_ac].orid_desc,g_oock_d[l_ac].oockuser,g_oock_d[l_ac].user_desc,g_oock_d[l_ac].oockdept,g_oock_d[l_ac].dept_desc,g_oock_d[l_ac].oockbuid

                          
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
                        
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      #帶出預設欄位之值
      LET g_oock_d[l_ac].modu_desc = cl_get_username(g_oock_d[l_ac].oockmodu)
      LET g_oock_d[l_ac].oriu_desc = cl_get_username(g_oock_d[l_ac].oockoriu)
      LET g_oock_d[l_ac].user_desc = cl_get_username(g_oock_d[l_ac].oockuser)
      LET g_oock_d[l_ac].dept_desc = cl_get_deptname(g_oock_d[l_ac].oockdept)
      LET g_oock_d[l_ac].orid_desc = cl_get_deptname(g_oock_d[l_ac].oockorid)
     

     
      #add-point:單身資料抓取
      {<point name="bfill.foreach"/>}
     #end add-point
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         CALL cl_err( '', 9035, 0 )
         EXIT FOREACH
      END IF
   END FOREACH
   CALL g_oock_d.deleteElement(l_ac)

 
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
END FUNCTION
 
 
