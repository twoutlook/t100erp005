#+ 資料修改
PRIVATE FUNCTION aooi014_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_oock_m.oock001 IS NULL
      #key2
   OR g_oock_m.oock002 IS NULL


   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
    SELECT UNIQUE oock001,oock002
 INTO g_oock_m.oock001,g_oock_m.oock002
 FROM oock_t
 WHERE oockent = g_enterprise AND oock001 = g_oock_m.oock001 AND oock002 = g_oock_m.oock002
 
   ERROR ""
  
   LET g_oock001_t = g_oock_m.oock001
      #key2
   LET g_oock002_t = g_oock_m.oock002


   BEGIN WORK
   
   OPEN aooi014_cl USING g_enterprise,g_oock_m.oock001
      #key2
                                                       ,g_oock_m.oock002


   IF STATUS THEN
      CALL cl_err("OPEN aooi014_cl:", STATUS, 1)
      CLOSE aooi014_cl
      ROLLBACK WORK
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aooi014_cl INTO g_oock_m.oock001,g_oock_m.oock002,g_oock_m.oock002_desc,g_oock_m.oock001_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_oock_m.oock001,SQLCA.sqlcode,0)
      CLOSE aooi014_cl
      ROLLBACK WORK
      RETURN
   END IF
 
   CALL aooi014_show()
   WHILE TRUE
      LET g_oock001_t = g_oock_m.oock001
      #key2
      LET g_oock002_t = g_oock_m.oock002


 
      CALL aooi014_input("u")     #欄位更改
 
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_oock_m.* = g_oock_m_t.*
         CALL aooi014_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_oock_m.oock001 != g_oock001_t 
      #key2
      OR g_oock_m.oock002 != g_oock002_t 


      THEN
         BEGIN WORK
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE oock_t SET oock001 = g_oock_m.oock001
      #key2
                                       , oock002 = g_oock_m.oock002


          WHERE oockent = g_enterprise AND oock001 = g_oock001_t
      #key2
            AND oock002 = g_oock002_t


          IF SQLCA.sqlcode THEN
             CALL cl_err("oock_t",SQLCA.sqlcode,1) 
             ROLLBACK WORK
             CONTINUE WHILE
         END IF
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_used_modified_record(g_oock_m.oock001,g_site) THEN 
      ROLLBACK WORK
   END IF
 
   CLOSE aooi014_cl
   COMMIT WORK
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_oock_m.oock001,'U')
 
   CALL aooi014_b_fill("1=1")
   
END FUNCTION   
 
 
