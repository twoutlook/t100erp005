          ooif029,ooifpos,ooifstamp,ooif032,ooif038,ooif033,ooif034,ooif035,ooifownid,ooifowndp,ooifcrtid, 
          ooifcrtdp,ooifcrtdt,ooifmodid,ooifmoddt
