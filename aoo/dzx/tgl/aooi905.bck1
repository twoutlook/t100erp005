{<section id="aooi905.modify" >}
#+ 資料修改
PRIVATE FUNCTION aooi905_modify()
   #add-point:modify段define name="modify.define_customerization"
   {<point name="modify.define_customerization" edit="c" mark="Y"/>}
   #end add-point    
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   {<point name="modify.define"/>}
   #end add-point  
   
   #add-point:Function前置處理  name="modify.pre_function"
   {<point name="modify.pre_function"/>}
   #end add-point
   
   IF g_ooif_m.ooif000 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL  
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
   LET g_ooif000_t = g_ooif_m.ooif000
 
   CALL s_transaction_begin()
   
   OPEN aooi905_cl USING g_enterprise,g_ooif_m.ooif000
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aooi905_cl:" 
      LET g_errparam.code   =  STATUS 
      LET g_errparam.popup  = TRUE 
      CLOSE aooi905_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aooi905_master_referesh USING g_ooif_m.ooif000 INTO g_ooif_m.ooif000,g_ooif_m.ooif000_desc 

   
   #遮罩相關處理
   LET g_ooif_m_mask_o.* =  g_ooif_m.*
   CALL aooi905_ooif_t_mask()
   LET g_ooif_m_mask_n.* =  g_ooif_m.*
   
   CALL s_transaction_end('Y','0')
 
   CALL aooi905_show()
   WHILE TRUE
      LET g_ooif000_t = g_ooif_m.ooif000
 
 
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL aooi905_input("u")     #欄位更改
      
      #add-point:modify段修改後 name="modify.after_input"
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         LET g_ooif_m.* = g_ooif_m_t.*
         CALL aooi905_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更(更新單身table的key欄位值)
      IF g_ooif_m.ooif000 != g_ooif000_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前 name="modify.b_key_update"
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #add-point:單頭(偽)修改中 name="modify.m_key_update"
         {<point name="modify.m_key_update"/>}
         #end add-point
         
 
         
         #add-point:單頭(偽)修改後 name="modify.a_key_update"
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("modify,modify_detail,delete,reproduce", TRUE)
   CALL aooi905_set_act_visible()
   CALL aooi905_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " ooifent = " ||g_enterprise|| " AND",
                      " ooif000 = '", g_ooif_m.ooif000, "' "
 
   #填到對應位置
   CALL aooi905_browser_fill("")
 
   CALL aooi905_idx_chk()
 
   CLOSE aooi905_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aooi905_msgcentre_notify('modify')
   
END FUNCTION   

