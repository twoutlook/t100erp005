   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
      
      #單頭
      CONSTRUCT BY NAME g_wc ON ooif000
 
         BEFORE CONSTRUCT
            #add-point:cs段more_construct name="cs.head.before_construct"
