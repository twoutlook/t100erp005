            #add-point:AFTER FIELD ooif000 name="construct.a.ooif000"
            {<point name="construct.a.ooif000" />}
            #END add-point
            
 
 
 
         
      END CONSTRUCT
 
      CONSTRUCT g_wc2_table1 ON ooifstus,ooif001,ooif002,ooif031,ooif036,ooif037,ooif039,ooif003,ooif004, 
          ooif005,ooif006,ooif007,ooif008,ooif009,ooif010,ooif011,ooif012,ooif013,ooif014,ooif015,ooif016, 
