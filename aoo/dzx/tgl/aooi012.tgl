#+ 單身陣列填充
PRIVATE FUNCTION aooi012_b_fill(p_wc2)              #BODY FILL UP
   {<Local define>}
   DEFINE p_wc2           STRING
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
 
   LET g_sql = "SELECT oocgstus,oocg001,'','',oocg002,'',oocg003,oocg004,oocg005,oocgmodu,'',oocgdate,oocgoriu,'',oocgorid,'',oocguser,'',oocgdept,'',oocgbuid FROM oocg_t WHERE oocgent= ? AND 1=1  AND ", p_wc2 CLIPPED,           #單身
               " ORDER BY oocg001" 
  
   PREPARE aooi012_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aooi012_pb
   
      OPEN b_fill_curs USING g_enterprise
 
   CALL g_oocg_d.clear()

 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
   FOREACH b_fill_curs INTO g_oocg_d[l_ac].oocgstus,g_oocg_d[l_ac].oocg001,g_oocg_d[l_ac].oocgl003,g_oocg_d[l_ac].oocgl004,g_oocg_d[l_ac].oocg002,g_oocg_d[l_ac].oocg002_desc,g_oocg_d[l_ac].oocg003,g_oocg_d[l_ac].oocg004,g_oocg_d[l_ac].oocg005,g_oocg_d[l_ac].oocgmodu,g_oocg_d[l_ac].modu_desc,g_oocg_d[l_ac].oocgdate,g_oocg_d[l_ac].oocgoriu,g_oocg_d[l_ac].oriu_desc,g_oocg_d[l_ac].oocgorid,g_oocg_d[l_ac].orid_desc,g_oocg_d[l_ac].oocguser,g_oocg_d[l_ac].user_desc,g_oocg_d[l_ac].oocgdept,g_oocg_d[l_ac].dept_desc,g_oocg_d[l_ac].oocgbuid

      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 

      CALL aooi012_detail_show()      
      
      #add-point:單身資料抓取
      {<point name="bfill.foreach"/>}
      #end add-point
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         CALL cl_err( "", 9035, 0 )
         EXIT FOREACH
      END IF
      
   END FOREACH
 
   CALL g_oocg_d.deleteElement(l_ac) 
   ERROR "" 

 
   LET g_detail_cnt = l_ac - 1
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   LET l_ac = g_cnt
   LET g_cnt = 0
   
END FUNCTION
 
