#+ 功能選單 
PRIVATE FUNCTION aooi012_ui_dialog()
   {<Local define>}
   DEFINE li_idx   LIKE type_t.num5
   {</Local define>}
   #add-point:ui_dialog段define
   {<point name="ui_dialog.define"/>}
   #end add-point 
 
   LET g_action_choice = " "          
 
   CALL cl_set_act_visible("accept,cancel", FALSE)
   
   WHILE TRUE
   
      DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
         DISPLAY ARRAY g_oocg_d TO s_detail1.* ATTRIBUTE(COUNT=g_detail_cnt) 
      
            BEFORE ROW
               LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
               LET l_ac = g_detail_idx
               LET g_temp_idx = l_ac
               IF g_oocg_d[l_ac].oocgstus = "Y" THEN
                  CALL cl_set_act_visible("void", TRUE)
                  CALL cl_set_act_visible("valid", FALSE)
               ELSE
                  CALL cl_set_act_visible("valid", TRUE)
                  CALL cl_set_act_visible("void", FALSE)
               END IF
               DISPLAY g_detail_idx TO FORMONLY.idx
               CALL cl_show_fld_cont() 
         END DISPLAY
      

      
         #add-point:ui_dialog段define
         {<point name="ui_dialog.more_displayarray"/>}
         #end add-point
         
         BEFORE DIALOG
            IF g_temp_idx > 0 THEN
               LET l_ac = g_temp_idx
               CALL DIALOG.setCurrentRow("s_detail1",l_ac)
               LET g_temp_idx = 1
            END IF
            LET g_curr_diag = ui.DIALOG.getCurrent()         
            CALL DIALOG.setSelectionMode("s_detail1", 1)
            NEXT FIELD CURRENT
      
         


         ON ACTION modify
            LET g_action_choice="modify"
            IF cl_chk_act_auth() THEN 
               CALL aooi012_modify()
            #add-point:ON ACTION modify
            {<point name="menu.modify" />}
            #END add-point

                EXIT DIALOG
            END IF



         ON ACTION query
            LET g_action_choice="query"
            IF cl_chk_act_auth() THEN 
               CALL aooi012_query()
            #add-point:ON ACTION query
            {<point name="menu.query" />}
            #END add-point

                EXIT DIALOG
            END IF

      
         # 全選修改狀態
         ON ACTION valid
            CALL aooi012_statechange("Y")
            #資料重讀
            CALL aooi012_b_fill(g_wc2)
            EXIT DIALOG
            
         ON ACTION void
            CALL aooi012_statechange("N")
            #資料重讀
            CALL aooi012_b_fill(g_wc2)
            EXIT DIALOG
      
         ON ACTION close
            LET INT_FLAG=FALSE         
            LET g_action_choice="exit"
            EXIT WHILE
      
         ON ACTION exit
            LET g_action_choice="exit"
            EXIT WHILE
      
         #主選單用ACTION
         &include "main_menu.4gl"
         
         #交談指令共用ACTION
         &include "common_action.4gl"
            CONTINUE DIALOG
      END DIALOG
	  
   END WHILE
 
   CALL cl_set_act_visible("accept,cancel", TRUE)
 
END FUNCTION
 
 
#+ QBE資料查詢
PRIVATE FUNCTION aooi012_query()
   {<Local define>}
   DEFINE ls_wc      LIKE type_t.chr500
   DEFINE ls_return  STRING
   DEFINE ls_result  STRING 
   {</Local define>}
   #add-point:query段define
   {<point name="query.define"/>}
   #end add-point 
   
   LET INT_FLAG = 0
   CLEAR FORM
   CALL g_oocg_d.clear()
   
   #wc備份
   LET ls_wc = g_wc2
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      CONSTRUCT g_wc2 ON oocgstus,oocg001,oocgl003,oocgl004,oocg002,oocg003,oocg004,oocg005,oocgmodu,oocgdate,oocgoriu,oocgorid,oocguser,oocgdept,oocgbuid                        
         FROM s_detail1[1].oocgstus,s_detail1[1].oocg001,s_detail1[1].oocgl003,s_detail1[1].oocgl004,s_detail1[1].oocg002,s_detail1[1].oocg003,s_detail1[1].oocg004,s_detail1[1].oocg005,s_detail1[1].oocgmodu,s_detail1[1].oocgdate,s_detail1[1].oocgoriu,s_detail1[1].oocgorid,s_detail1[1].oocguser,s_detail1[1].oocgdept,s_detail1[1].oocgbuid 
      
         #----<<oocgmodu>>----
         ON ACTION controlp INFIELD oocgmodu
            CALL q_common("oocg_t","oocgmodu",TRUE,FALSE,g_oocg_d[l_ac].oocgmodu) RETURNING ls_return
            DISPLAY ls_return TO oocgmodu
            NEXT FIELD oocgmodu
      
         #----<<oocgoriu>>----
         ON ACTION controlp INFIELD oocgoriu
            CALL q_common("oocg_t","oocgoriu",TRUE,FALSE,g_oocg_d[l_ac].oocgoriu) RETURNING ls_return
            DISPLAY ls_return TO oocgoriu
            NEXT FIELD oocgoriu
      
         #----<<oocguser>>----
         ON ACTION controlp INFIELD oocguser
            CALL q_common("oocg_t","oocguser",TRUE,FALSE,g_oocg_d[l_ac].oocguser) RETURNING ls_return
            DISPLAY ls_return TO oocguser
            NEXT FIELD oocguser
      
         #----<<oocgorid>>----
         ON ACTION controlp INFIELD oocgorid
            CALL q_common("oocg_t","oocgorid",TRUE,FALSE,g_oocg_d[l_ac].oocgorid) RETURNING ls_return
            DISPLAY ls_return TO oocgorid
            NEXT FIELD oocgorid
      
         #----<<oocgdept>>----
         ON ACTION controlp INFIELD oocgdept
            CALL q_common("oocg_t","oocgdept",TRUE,FALSE,g_oocg_d[l_ac].oocgdept) RETURNING ls_return
            DISPLAY ls_return TO oocgdept
            NEXT FIELD oocgdept
 
         #----<<oocgbuid>>----
         AFTER FIELD oocgbuid
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
         
         #----<<oocgdate>>----
         AFTER FIELD oocgdate
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
      
         #---------------------<  Detail: page1  >---------------------
         #----<<oocgstus>>----
         #Ctrlp:query.c.oocgstus
         ON ACTION controlp INFIELD oocgstus
            #add-point:ON ACTION controlp INFIELD oocgstus
            {<point name="query.c.page1.oocgstus" />}
            #END add-point

         #----<<oocg001>>----
         #Ctrlp:construct.c.page1.oocg001
         ON ACTION controlp INFIELD oocg001
            #add-point:ON ACTION controlp INFIELD oocg001
            {<point name="construct.c.page1.oocg001" />}
            #END add-point

         #----<<oocgl003>>----
         #Ctrlp:query.c.oocgl003
         ON ACTION controlp INFIELD oocgl003
            #add-point:ON ACTION controlp INFIELD oocgl003
            {<point name="query.c.page1.oocgl003" />}
            #END add-point

         #----<<oocgl004>>----
         #Ctrlp:query.c.oocgl004
         ON ACTION controlp INFIELD oocgl004
            #add-point:ON ACTION controlp INFIELD oocgl004
            {<point name="query.c.page1.oocgl004" />}
            #END add-point

         #----<<oocg002>>----
         #Ctrlp:construct.c.page1.oocg002
         ON ACTION controlp INFIELD oocg002
            #add-point:ON ACTION controlp INFIELD oocg002
            {<point name="construct.c.page1.oocg002" />}
            #END add-point

         #----<<oocg002_desc>>----
         #----<<oocg003>>----
         #Ctrlp:query.c.oocg003
         ON ACTION controlp INFIELD oocg003
            #add-point:ON ACTION controlp INFIELD oocg003
            {<point name="query.c.page1.oocg003" />}
            #END add-point

         #----<<oocg004>>----
         #Ctrlp:query.c.oocg004
         ON ACTION controlp INFIELD oocg004
            #add-point:ON ACTION controlp INFIELD oocg004
            {<point name="query.c.page1.oocg004" />}
            #END add-point

         #----<<oocg005>>----
         #Ctrlp:query.c.oocg005
         ON ACTION controlp INFIELD oocg005
            #add-point:ON ACTION controlp INFIELD oocg005
            {<point name="query.c.page1.oocg005" />}
            #END add-point

         #----<<oocgmodu>>----
         #----<<modu_desc>>----
         #----<<oocgdate>>----
         #----<<oocgoriu>>----
         #----<<oriu_desc>>----
         #----<<oocgorid>>----
         #----<<orid_desc>>----
         #----<<oocguser>>----
         #----<<user_desc>>----
         #----<<oocgdept>>----
         #----<<dept_desc>>----
         #----<<oocgbuid>>----
  
      
         BEFORE CONSTRUCT
            CALL cl_qbe_init()
      
         ON ACTION qbe_select
            CALL cl_qbe_select()
      
         ON ACTION qbe_save
            CALL cl_qbe_save()
      
      END CONSTRUCT
  
      #add-point:query段more_construct
      {<point name="query.more_construct"/>}
      #end add-point 
	  
	  ON ACTION accept
	     EXIT DIALOG
		 
	  ON ACTION cancel
	     LET INT_FLAG = 1
		 EXIT DIALOG
	  
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE DIALOG 
   END DIALOG
 
   LET g_wc2 = g_wc2 CLIPPED, cl_get_extra_cond("g_oocg_d[l_ac].oocguser", "")
 
   IF INT_FLAG THEN
      LET INT_FLAG = 0
      #還原
      #LET g_wc2 = ls_wc
	  LET g_wc2 = " 1=1"
   END IF
  
   CALL aooi012_b_fill(g_wc2)
   
   IF g_detail_cnt = 0 AND NOT INT_FLAG THEN
      CALL cl_err("",-100,1)
   END IF
   
   LET INT_FLAG = FALSE
   
END FUNCTION
 
 
#+ 資料修改
PRIVATE FUNCTION aooi012_modify()
   {<Local define>}
   DEFINE l_ac_t          LIKE type_t.num5     #未取消的ARRAY CNT      
   DEFINE l_n             LIKE type_t.num5     #檢查重複用             
   DEFINE l_lock_sw       LIKE type_t.chr1     #單身鎖住否            
   DEFINE p_cmd           LIKE type_t.chr1     #處理狀態              
   DEFINE l_allow_insert  LIKE type_t.chr1     #可新增否
   DEFINE l_allow_delete  LIKE type_t.chr1     
   DEFINE l_insert        LIKE type_t.num5     #輸入時判斷是否在inser狀態
   DEFINE ls_return       STRING
   DEFINE l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE l_vars          DYNAMIC ARRAY OF STRING
   DEFINE l_fields        DYNAMIC ARRAY OF STRING
   DEFINE l_var_keys_bak  DYNAMIC ARRAY OF STRING
   {</Local define>}
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point 
   
   LET l_insert = FALSE
   LET g_action_choice = ""
 
   LET l_allow_insert = cl_detail_input_auth("insert")
   LET l_allow_delete = cl_detail_input_auth("delete")
 
   LET g_forupd_sql = "SELECT oocgstus,oocg001,'','',oocg002,'',oocg003,oocg004,oocg005,oocgmodu,'',oocgdate,oocgoriu,'',oocgorid,'',oocguser,'',oocgdept,'',oocgbuid FROM oocg_t WHERE oocgent=? AND oocg001=? FOR UPDATE"
   LET g_forupd_sql = cl_forupd_sql(g_forupd_sql)
   DECLARE aooi012_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR
 
   INPUT ARRAY g_oocg_d FROM s_detail1.*
         ATTRIBUTE (WITHOUT DEFAULTS, UNBUFFERED, FIELD ORDER FORM,
                    COUNT = g_detail_cnt,MAXCOUNT = g_max_rec,
                    INSERT ROW = l_allow_insert, DELETE ROW = l_allow_delete,
                    APPEND ROW = l_allow_insert)
      #自訂ACTION
         ON ACTION update_item 


 
      BEFORE INPUT
         IF g_detail_cnt != 0 THEN
            CALL fgl_set_arr_curr(l_ac)
         END IF
 
      BEFORE ROW
         LET p_cmd="" 
         LET l_ac = ARR_CURR()
         LET l_lock_sw = "N"            #DEFAULT
         LET l_n  = ARR_COUNT()
 
         IF g_detail_cnt>=l_ac THEN
            BEGIN WORK
            LET p_cmd="u"
 
            LET g_before_input_done = FALSE                                      
            CALL aooi012_set_entry_b(p_cmd)                                           
            CALL aooi012_set_no_entry_b(p_cmd)                                        
            LET g_before_input_done = TRUE                                       
            
            LET g_oocg_d_t.* = g_oocg_d[l_ac].*  #BACKUP
 
            OPEN aooi012_bcl USING g_enterprise,g_oocg_d_t.oocg001

            IF STATUS THEN
               CALL cl_err("OPEN aooi012_bcl:", STATUS, 1)
               LET l_lock_sw = "Y"
            ELSE 
               FETCH aooi012_bcl INTO g_oocg_d[l_ac].oocgstus,g_oocg_d[l_ac].oocg001,g_oocg_d[l_ac].oocgl003,g_oocg_d[l_ac].oocgl004,g_oocg_d[l_ac].oocg002,g_oocg_d[l_ac].oocg002_desc,g_oocg_d[l_ac].oocg003,g_oocg_d[l_ac].oocg004,g_oocg_d[l_ac].oocg005,g_oocg_d[l_ac].oocgmodu,g_oocg_d[l_ac].modu_desc,g_oocg_d[l_ac].oocgdate,g_oocg_d[l_ac].oocgoriu,g_oocg_d[l_ac].oriu_desc,g_oocg_d[l_ac].oocgorid,g_oocg_d[l_ac].orid_desc,g_oocg_d[l_ac].oocguser,g_oocg_d[l_ac].user_desc,g_oocg_d[l_ac].oocgdept,g_oocg_d[l_ac].dept_desc,g_oocg_d[l_ac].oocgbuid 
               IF SQLCA.sqlcode THEN
                  CALL cl_err(g_oocg_d_t.oocg001,SQLCA.sqlcode,1)
                  LET l_lock_sw = "Y"
               END IF
            END IF
            
            CALL aooi012_detail_show()      
            CALL cl_show_fld_cont() 
 
            LET g_detail_multi_table_t.oocgl001 = g_oocg_d[l_ac].oocg001
LET g_detail_multi_table_t.oocgl002 = g_dlang
LET g_detail_multi_table_t.oocgl003 = g_oocg_d[l_ac].oocgl003
LET g_detail_multi_table_t.oocgl004 = g_oocg_d[l_ac].oocgl004

 
            #add-point:modify段before_row
            {<point name="modify.before_row"/>}
            #end add-point  
         END IF
  
      BEFORE INSERT
         LET l_insert = TRUE
         LET l_n = ARR_COUNT()
         LET p_cmd="a"   
                                                        
         LET g_before_input_done = FALSE                                        
         CALL aooi012_set_entry_b(p_cmd)                                             
         CALL aooi012_set_no_entry_b(p_cmd)                                          
         LET g_before_input_done = TRUE                                         
 
         INITIALIZE g_oocg_d[l_ac].* TO NULL      
 
         LET g_oocg_d_t.* = g_oocg_d[l_ac].*         #新輸入資料
         CALL cl_show_fld_cont()  
         LET g_oocg_d[l_ac].oocgstus = "Y"
         LET g_oocg_d[l_ac].oocgbuid = cl_get_current()
         LET g_oocg_d[l_ac].oocgdate = cl_get_current()
         LET g_oocg_d[l_ac].oocgmodu = g_user
         LET g_oocg_d[l_ac].oocguser = g_user
         LET g_oocg_d[l_ac].oocgoriu = g_user
         LET g_oocg_d[l_ac].oocgorid = g_dept
         LET g_oocg_d[l_ac].oocgdept = g_dept
         
        
                  
         CALL aooi012_detail_show()  
         #add-point:modify段before insert
         {<point name="modify.before_insert"/>}
         #end add-point  
         NEXT FIELD oocg001
 
      AFTER INSERT
         LET l_insert = FALSE
         IF INT_FLAG THEN
            CALL cl_err("",9001,0)
            LET INT_FLAG = 0
            CLOSE aooi012_bcl
            CANCEL INSERT
         END IF
      
         BEGIN WORK                    
 
         #add-point:單身新增前
         {<point name="modify.b_insert"/>}
         #end add-point
 
         INSERT INTO oocg_t
                (oocgent,oocgstus,oocg001,oocg002,oocg003,oocg004,oocg005,oocgmodu,oocgdate,oocgoriu,oocgorid,oocguser,oocgdept,oocgbuid)
         VALUES (g_enterprise,g_oocg_d[l_ac].oocgstus,g_oocg_d[l_ac].oocg001,g_oocg_d[l_ac].oocg002,g_oocg_d[l_ac].oocg003,g_oocg_d[l_ac].oocg004,g_oocg_d[l_ac].oocg005,g_oocg_d[l_ac].oocgmodu,g_oocg_d[l_ac].oocgdate,g_oocg_d[l_ac].oocgoriu,g_oocg_d[l_ac].oocgorid,g_oocg_d[l_ac].oocguser,g_oocg_d[l_ac].oocgdept,g_oocg_d[l_ac].oocgbuid)
                
         IF SQLCA.sqlcode THEN
            CALL cl_err("oocg_t",SQLCA.sqlcode,1)  
            ROLLBACK WORK              
            CANCEL INSERT
         ELSE
            #add-point:after_insert
            {<point name="modify.after_insert"/>}
            #end add-point
            #資料多語言用-增/改
                     INITIALIZE l_var_keys TO NULL 
         INITIALIZE l_field_keys TO NULL 
         INITIALIZE l_vars TO NULL 
         INITIALIZE l_fields TO NULL 
         INITIALIZE l_var_keys_bak TO NULL 
         IF g_oocg_d[l_ac].oocg001 = g_detail_multi_table_t.oocgl001 AND
         g_oocg_d[l_ac].oocgl003 = g_detail_multi_table_t.oocgl002 AND
         g_oocg_d[l_ac].oocgl004 = g_detail_multi_table_t.oocgl003 THEN
         ELSE 
            LET l_var_keys[01] = g_oocg_d[l_ac].oocg001
            LET l_field_keys[01] = 'oocgl001'
            LET l_var_keys[02] = g_dlang
            LET l_field_keys[02] = 'oocgl002'
            LET l_vars[01] = g_oocg_d[l_ac].oocgl003
            LET l_fields[01] = 'oocgl003'
            LET l_vars[02] = g_oocg_d[l_ac].oocgl004
            LET l_fields[02] = 'oocgl004'
            LET l_vars[03] = g_enterprise 
