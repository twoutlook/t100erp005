   {<point name="b_fill.pre_function"/>}
   #end add-point
      
   CALL g_oofh_d.clear()    #g_oofh_d 單頭及單身 
 
 
   #add-point:b_fill段define name="b_fill.sql_before"
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  DISTINCT oofh003  FROM oofh_t",   
               " INNER JOIN oofg_t ON oofg001 = oofh001 ",
               " AND oofg005 = oofh002 ",
 
               " LEFT JOIN oofhl_t ON oofhlent = "||g_enterprise||" AND oofg001 = oofhl001 AND oofg005 = oofhl002 AND oofh003 = oofhl003 AND oofhl004 = '",g_dlang,"'",
               
               " WHERE oofhent=? AND oofh001=? AND oofh002=?"
 
   IF NOT cl_null(g_wc_table1) THEN
      LET g_sql = g_sql CLIPPED, " AND ", g_wc_table1 CLIPPED
   END IF
 
   LET g_sql = g_sql, " ORDER BY oofh003"
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料   
   PREPARE aooi390_pb FROM g_sql
   DECLARE b_fill_cs CURSOR FOR aooi390_pb
 
   LET g_cnt = l_ac
   LET l_ac = 1
 
   OPEN b_fill_cs USING g_enterprise,g_oofg_m.oofg001,g_oofg_m.oofg005
                                            
   FOREACH b_fill_cs INTO g_oofh_d[l_ac].oofh003
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
     
      #add-point:b_fill段資料填充 name="b_fill.fill"
      {<point name="b_fill.fill"/>}
      #end add-point
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
 
   
   CALL g_oofh_d.deleteElement(g_oofh_d.getLength())
 
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE aooi390_pb
 
   
   
END FUNCTION

