LET g_master_multi_table_t.oofgl002 = ' '
LET g_master_multi_table_t.oofgl004 = g_oofg_m.oofgl004
LET g_master_multi_table_t.oofgl005 = g_oofg_m.oofgl005
LET g_master_multi_table_t.oofgl001_idx2 = g_oofg_m.oofg001
LET g_master_multi_table_t.oofgl002_idx2 = g_oofg_m.oofg005
LET g_master_multi_table_t.oofgl004_1_idx2 = g_oofg_m.oofgl004_1
 
 
