#+ 功能選單 
PRIVATE FUNCTION aooi015_ui_dialog()
   {<Local define>}
   DEFINE li_idx   LIKE type_t.num5
   {</Local define>}
   #add-point:ui_dialog段define
   {<point name="ui_dialog.define"/>}
   #end add-point 
 
   LET g_action_choice = " "          
 
   CALL cl_set_act_visible("accept,cancel", FALSE)
   
   WHILE TRUE
   
      DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
         DISPLAY ARRAY g_oocm_d TO s_detail1.* ATTRIBUTE(COUNT=g_detail_cnt) 
      
            BEFORE ROW
               LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
               LET l_ac = g_detail_idx
               LET g_temp_idx = l_ac
               IF g_oocm_d[l_ac].oocmstus = "Y" THEN
                  CALL cl_set_act_visible("void", TRUE)
                  CALL cl_set_act_visible("valid", FALSE)
               ELSE
                  CALL cl_set_act_visible("valid", TRUE)
                  CALL cl_set_act_visible("void", FALSE)
               END IF
               DISPLAY g_detail_idx TO FORMONLY.idx
               CALL cl_show_fld_cont() 
         END DISPLAY
      

      
         #add-point:ui_dialog段define
         {<point name="ui_dialog.more_displayarray"/>}
         #end add-point
         
         BEFORE DIALOG
            IF g_temp_idx > 0 THEN
               LET l_ac = g_temp_idx
               CALL DIALOG.setCurrentRow("s_detail1",l_ac)
               LET g_temp_idx = 1
            END IF
            LET g_curr_diag = ui.DIALOG.getCurrent()         
            CALL DIALOG.setSelectionMode("s_detail1", 1)
            NEXT FIELD CURRENT
      
         


         ON ACTION delete
            LET g_action_choice="delete"
            IF cl_chk_act_auth() THEN 
               CALL aooi015_delete()
            #add-point:ON ACTION delete
            {<point name="menu.delete" />}
            #END add-point

                EXIT DIALOG
            END IF



         ON ACTION insert
            LET g_action_choice="insert"
            IF cl_chk_act_auth() THEN 
               CALL aooi015_insert()
            #add-point:ON ACTION insert
            {<point name="menu.insert" />}
            #END add-point

                EXIT DIALOG
            END IF



         ON ACTION modify
            LET g_action_choice="modify"
            IF cl_chk_act_auth() THEN 
               CALL aooi015_modify()
            #add-point:ON ACTION modify
            {<point name="menu.modify" />}
            #END add-point

                EXIT DIALOG
            END IF



         ON ACTION query
            LET g_action_choice="query"
            IF cl_chk_act_auth() THEN 
               CALL aooi015_query()
            #add-point:ON ACTION query
            {<point name="menu.query" />}
            #END add-point

                EXIT DIALOG
            END IF



         ON ACTION reproduce
            LET g_action_choice="reproduce"
            IF cl_chk_act_auth() THEN 
               CALL aooi015_reproduce()
            #add-point:ON ACTION reproduce
            {<point name="menu.reproduce" />}
            #END add-point

                EXIT DIALOG
            END IF

      
         # 全選修改狀態
         ON ACTION valid
            CALL aooi015_statechange("Y")
            #資料重讀
            CALL aooi015_b_fill(g_wc2)
            EXIT DIALOG
            
         ON ACTION void
            CALL aooi015_statechange("N")
            #資料重讀
            CALL aooi015_b_fill(g_wc2)
            EXIT DIALOG
      
         ON ACTION close
            LET INT_FLAG=FALSE         
            LET g_action_choice="exit"
            EXIT WHILE
      
         ON ACTION exit
            LET g_action_choice="exit"
            EXIT WHILE
      
         #主選單用ACTION
         &include "main_menu.4gl"
         
         #交談指令共用ACTION
         &include "common_action.4gl"
            CONTINUE DIALOG
      END DIALOG
	  
   END WHILE
 
   CALL cl_set_act_visible("accept,cancel", TRUE)
 
END FUNCTION
 
 
#+ QBE資料查詢
PRIVATE FUNCTION aooi015_query()
   {<Local define>}
   DEFINE ls_wc      LIKE type_t.chr500
   DEFINE ls_return  STRING
   DEFINE ls_result  STRING 
   {</Local define>}
   #add-point:query段define
   {<point name="query.define"/>}
   #end add-point 
   
   LET INT_FLAG = 0
   CLEAR FORM
   CALL g_oocm_d.clear()
   
   #wc備份
   LET ls_wc = g_wc2
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      CONSTRUCT g_wc2 ON oocmstus,oocm004,oocn006,oocn007,oocmmodu,oocmdate,oocmoriu,oocmorid,oocmuser,oocmdept,oocmbuid                        
         FROM s_detail1[1].oocmstus,s_detail1[1].oocm004,s_detail1[1].oocn006,s_detail1[1].oocn007,s_detail1[1].oocmmodu,s_detail1[1].oocmdate,s_detail1[1].oocmoriu,s_detail1[1].oocmorid,s_detail1[1].oocmuser,s_detail1[1].oocmdept,s_detail1[1].oocmbuid 
      
         #----<<oocmmodu>>----
         ON ACTION controlp INFIELD oocmmodu
            CALL q_common("oocm_t","oocmmodu",TRUE,FALSE,g_oocm_d[l_ac].oocmmodu) RETURNING ls_return
            DISPLAY ls_return TO oocmmodu
            NEXT FIELD oocmmodu
      
         #----<<oocmoriu>>----
         ON ACTION controlp INFIELD oocmoriu
            CALL q_common("oocm_t","oocmoriu",TRUE,FALSE,g_oocm_d[l_ac].oocmoriu) RETURNING ls_return
            DISPLAY ls_return TO oocmoriu
            NEXT FIELD oocmoriu
      
         #----<<oocmuser>>----
         ON ACTION controlp INFIELD oocmuser
            CALL q_common("oocm_t","oocmuser",TRUE,FALSE,g_oocm_d[l_ac].oocmuser) RETURNING ls_return
            DISPLAY ls_return TO oocmuser
            NEXT FIELD oocmuser
      
         #----<<oocmorid>>----
         ON ACTION controlp INFIELD oocmorid
            CALL q_common("oocm_t","oocmorid",TRUE,FALSE,g_oocm_d[l_ac].oocmorid) RETURNING ls_return
            DISPLAY ls_return TO oocmorid
            NEXT FIELD oocmorid
      
         #----<<oocmdept>>----
         ON ACTION controlp INFIELD oocmdept
            CALL q_common("oocm_t","oocmdept",TRUE,FALSE,g_oocm_d[l_ac].oocmdept) RETURNING ls_return
            DISPLAY ls_return TO oocmdept
            NEXT FIELD oocmdept
 
         #----<<oocmbuid>>----
         AFTER FIELD oocmbuid
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
         
         #----<<oocmdate>>----
         AFTER FIELD oocmdate
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
      
         #---------------------<  Detail: page1  >---------------------
         #----<<oocmstus>>----
         #Ctrlp:query.c.oocmstus
         ON ACTION controlp INFIELD oocmstus
            #add-point:ON ACTION controlp INFIELD oocmstus
            {<point name="query.c.page1.oocmstus" />}
            #END add-point

         #----<<oocm004>>----
         #Ctrlp:construct.c.page1.oocm004
         ON ACTION controlp INFIELD oocm004
            #add-point:ON ACTION controlp INFIELD oocm004
            {<point name="construct.c.page1.oocm004" />}
            #END add-point

         #----<<oocn006>>----
         #Ctrlp:query.c.oocn006
         ON ACTION controlp INFIELD oocn006
            #add-point:ON ACTION controlp INFIELD oocn006
            {<point name="query.c.page1.oocn006" />}
            #END add-point

         #----<<oocn007>>----
         #Ctrlp:query.c.oocn007
         ON ACTION controlp INFIELD oocn007
            #add-point:ON ACTION controlp INFIELD oocn007
            {<point name="query.c.page1.oocn007" />}
            #END add-point

         #----<<oocmmodu>>----
         #----<<modu_desc>>----
         #----<<oocmdate>>----
         #----<<oocmoriu>>----
         #----<<oriu_desc>>----
         #----<<oocmorid>>----
         #----<<orid_desc>>----
         #----<<oocmuser>>----
         #----<<user_desc>>----
         #----<<oocmdept>>----
         #----<<dept_desc>>----
         #----<<oocmbuid>>----
  
      
         BEFORE CONSTRUCT
            CALL cl_qbe_init()
      
         ON ACTION qbe_select
            CALL cl_qbe_select()
      
         ON ACTION qbe_save
            CALL cl_qbe_save()
      
      END CONSTRUCT
  
      #add-point:query段more_construct
      {<point name="query.more_construct"/>}
      #end add-point 
	  
	  ON ACTION accept
	     EXIT DIALOG
		 
	  ON ACTION cancel
	     LET INT_FLAG = 1
		 EXIT DIALOG
	  
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE DIALOG 
   END DIALOG
 
   LET g_wc2 = g_wc2 CLIPPED, cl_get_extra_cond("g_oocm_d[l_ac].oocmuser", "")
 
   IF INT_FLAG THEN
      LET INT_FLAG = 0
      #還原
      #LET g_wc2 = ls_wc
	  LET g_wc2 = " 1=1"
   END IF
  
   CALL aooi015_b_fill(g_wc2)
   
   IF g_detail_cnt = 0 AND NOT INT_FLAG THEN
      CALL cl_err("",-100,1)
   END IF
   
   LET INT_FLAG = FALSE
   
END FUNCTION
 
 
#+ 資料修改
PRIVATE FUNCTION aooi015_modify()
   {<Local define>}
   DEFINE l_ac_t          LIKE type_t.num5     #未取消的ARRAY CNT      
   DEFINE l_n             LIKE type_t.num5     #檢查重複用             
   DEFINE l_lock_sw       LIKE type_t.chr1     #單身鎖住否            
   DEFINE p_cmd           LIKE type_t.chr1     #處理狀態              
   DEFINE l_allow_insert  LIKE type_t.chr1     #可新增否
   DEFINE l_allow_delete  LIKE type_t.chr1     
   DEFINE l_insert        LIKE type_t.num5     #輸入時判斷是否在inser狀態
   DEFINE ls_return       STRING
   DEFINE l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE l_vars          DYNAMIC ARRAY OF STRING
   DEFINE l_fields        DYNAMIC ARRAY OF STRING
   DEFINE l_var_keys_bak  DYNAMIC ARRAY OF STRING
   {</Local define>}
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point 
   
   LET l_insert = FALSE
   LET g_action_choice = ""
 
   LET l_allow_insert = cl_detail_input_auth("insert")
   LET l_allow_delete = cl_detail_input_auth("delete")
 
   LET g_forupd_sql = "SELECT oocmstus,oocm004,'','',oocmmodu,'',oocmdate,oocmoriu,'',oocmorid,'',oocmuser,'',oocmdept,'',oocmbuid FROM oocm_t WHERE oocment=? AND oocm001=? AND oocm002=? AND oocm003=? AND oocm004=? FOR UPDATE"
   LET g_forupd_sql = cl_forupd_sql(g_forupd_sql)
   DECLARE aooi015_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR
 
   INPUT ARRAY g_oocm_d FROM s_detail1.*
         ATTRIBUTE (WITHOUT DEFAULTS, UNBUFFERED, FIELD ORDER FORM,
                    COUNT = g_detail_cnt,MAXCOUNT = g_max_rec,
                    INSERT ROW = l_allow_insert, DELETE ROW = l_allow_delete,
                    APPEND ROW = l_allow_insert)
      #自訂ACTION
         ON ACTION update_item 


 
      BEFORE INPUT
         IF g_detail_cnt != 0 THEN
            CALL fgl_set_arr_curr(l_ac)
         END IF
 
      BEFORE ROW
         LET p_cmd="" 
         LET l_ac = ARR_CURR()
         LET l_lock_sw = "N"            #DEFAULT
         LET l_n  = ARR_COUNT()
 
         IF g_detail_cnt>=l_ac THEN
            BEGIN WORK
            LET p_cmd="u"
 
            LET g_before_input_done = FALSE                                      
            CALL aooi015_set_entry_b(p_cmd)                                           
            CALL aooi015_set_no_entry_b(p_cmd)                                        
            LET g_before_input_done = TRUE                                       
            
            LET g_oocm_d_t.* = g_oocm_d[l_ac].*  #BACKUP
 
            OPEN aooi015_bcl USING g_enterprise,g_oocm_d_t.oocm001
      #key2
                                    ,g_oocm_d_t.oocm002

      #key3
                                    ,g_oocm_d_t.oocm003

      #key4
                                    ,g_oocm_d_t.oocm004


            IF STATUS THEN
               CALL cl_err("OPEN aooi015_bcl:", STATUS, 1)
               LET l_lock_sw = "Y"
            ELSE 
               FETCH aooi015_bcl INTO g_oocm_d[l_ac].oocmstus,g_oocm_d[l_ac].oocm004,g_oocm_d[l_ac].oocn006,g_oocm_d[l_ac].oocn007,g_oocm_d[l_ac].oocmmodu,g_oocm_d[l_ac].modu_desc,g_oocm_d[l_ac].oocmdate,g_oocm_d[l_ac].oocmoriu,g_oocm_d[l_ac].oriu_desc,g_oocm_d[l_ac].oocmorid,g_oocm_d[l_ac].orid_desc,g_oocm_d[l_ac].oocmuser,g_oocm_d[l_ac].user_desc,g_oocm_d[l_ac].oocmdept,g_oocm_d[l_ac].dept_desc,g_oocm_d[l_ac].oocmbuid 
               IF SQLCA.sqlcode THEN
                  CALL cl_err(g_oocm_d_t.oocm001,SQLCA.sqlcode,1)
                  LET l_lock_sw = "Y"
               END IF
            END IF
            
            CALL aooi015_detail_show()      
            CALL cl_show_fld_cont() 
 
            LET g_detail_multi_table_t.oocn001 = g_oocm_d[l_ac].oocm001
LET g_detail_multi_table_t.oocn002 = g_oocm_d[l_ac].oocm002
LET g_detail_multi_table_t.oocn003 = g_oocm_d[l_ac].oocm003
LET g_detail_multi_table_t.oocn004 = g_oocm_d[l_ac].oocm004
LET g_detail_multi_table_t.oocn005 = g_dlang
LET g_detail_multi_table_t.oocn006 = g_oocm_d[l_ac].oocn006
LET g_detail_multi_table_t.oocn007 = g_oocm_d[l_ac].oocn007

 
            #add-point:modify段before_row
            {<point name="modify.before_row"/>}
            #end add-point  
         END IF
  
      BEFORE INSERT
         LET l_insert = TRUE
         LET l_n = ARR_COUNT()
         LET p_cmd="a"   
                                                        
         LET g_before_input_done = FALSE                                        
         CALL aooi015_set_entry_b(p_cmd)                                             
         CALL aooi015_set_no_entry_b(p_cmd)                                          
         LET g_before_input_done = TRUE                                         
 
         INITIALIZE g_oocm_d[l_ac].* TO NULL      
 
         LET g_oocm_d_t.* = g_oocm_d[l_ac].*         #新輸入資料
         CALL cl_show_fld_cont()  
         LET g_oocm_d[l_ac].oocmstus = "Y"
         LET g_oocm_d[l_ac].oocmbuid = cl_get_current()
         LET g_oocm_d[l_ac].oocmdate = cl_get_current()
         LET g_oocm_d[l_ac].oocmmodu = g_user
         LET g_oocm_d[l_ac].oocmuser = g_user
         LET g_oocm_d[l_ac].oocmoriu = g_user
         LET g_oocm_d[l_ac].oocmorid = g_dept
         LET g_oocm_d[l_ac].oocmdept = g_dept
         
        
                  
         CALL aooi015_detail_show()  
         #add-point:modify段before insert
         {<point name="modify.before_insert"/>}
         #end add-point  
         NEXT FIELD oocm001
 
      AFTER INSERT
         LET l_insert = FALSE
         IF INT_FLAG THEN
            CALL cl_err("",9001,0)
            LET INT_FLAG = 0
            CLOSE aooi015_bcl
            CANCEL INSERT
         END IF
      
         BEGIN WORK                    
 
         #add-point:單身新增前
         {<point name="modify.b_insert"/>}
         #end add-point
 
         INSERT INTO oocm_t
                (oocment,oocmstus,oocm004,oocmmodu,oocmdate,oocmoriu,oocmorid,oocmuser,oocmdept,oocmbuid)
         VALUES (g_enterprise,g_oocm_d[l_ac].oocmstus,g_oocm_d[l_ac].oocm004,g_oocm_d[l_ac].oocmmodu,g_oocm_d[l_ac].oocmdate,g_oocm_d[l_ac].oocmoriu,g_oocm_d[l_ac].oocmorid,g_oocm_d[l_ac].oocmuser,g_oocm_d[l_ac].oocmdept,g_oocm_d[l_ac].oocmbuid)
                
         IF SQLCA.sqlcode THEN
            CALL cl_err("oocm_t",SQLCA.sqlcode,1)  
            ROLLBACK WORK              
            CANCEL INSERT
         ELSE
            #add-point:after_insert
            {<point name="modify.after_insert"/>}
            #end add-point
            #資料多語言用-增/改
                     INITIALIZE l_var_keys TO NULL 
         INITIALIZE l_field_keys TO NULL 
         INITIALIZE l_vars TO NULL 
         INITIALIZE l_fields TO NULL 
         INITIALIZE l_var_keys_bak TO NULL 
         IF g_oocm_d[l_ac].oocm001 = g_detail_multi_table_t.oocn001 AND
         g_oocm_d[l_ac].oocm002 = g_detail_multi_table_t.oocn002 AND
         g_oocm_d[l_ac].oocm003 = g_detail_multi_table_t.oocn003 AND
         g_oocm_d[l_ac].oocm004 = g_detail_multi_table_t.oocn004 AND
         g_oocm_d[l_ac].oocn006 = g_detail_multi_table_t.oocn005 AND
         g_oocm_d[l_ac].oocn007 = g_detail_multi_table_t.oocn006 THEN
         ELSE 
            LET l_var_keys[01] = g_oocm_d[l_ac].oocm001
            LET l_field_keys[01] = 'oocn001'
            LET l_var_keys[02] = g_oocm_d[l_ac].oocm002
            LET l_field_keys[02] = 'oocn002'
            LET l_var_keys[03] = g_oocm_d[l_ac].oocm003
            LET l_field_keys[03] = 'oocn003'
            LET l_var_keys[04] = g_oocm_d[l_ac].oocm004
            LET l_field_keys[04] = 'oocn004'
            LET l_var_keys[05] = g_dlang
            LET l_field_keys[05] = 'oocn005'
            LET l_vars[01] = g_oocm_d[l_ac].oocn006
            LET l_fields[01] = 'oocn006'
