#+ 單身陣列填充
PRIVATE FUNCTION aooi015_b_fill(p_wc2)              #BODY FILL UP
   {<Local define>}
   DEFINE p_wc2           STRING
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
 
   LET g_sql = "SELECT oocmstus,oocm004,'','',oocmmodu,'',oocmdate,oocmoriu,'',oocmorid,'',oocmuser,'',oocmdept,'',oocmbuid FROM oocm_t WHERE oocment= ? AND 1=1  AND ", p_wc2 CLIPPED,           #單身
               " ORDER BY oocm001" 
  
   PREPARE aooi015_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aooi015_pb
   
      OPEN b_fill_curs USING g_enterprise
 
   CALL g_oocm_d.clear()

 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
   FOREACH b_fill_curs INTO g_oocm_d[l_ac].oocmstus,g_oocm_d[l_ac].oocm004,g_oocm_d[l_ac].oocn006,g_oocm_d[l_ac].oocn007,g_oocm_d[l_ac].oocmmodu,g_oocm_d[l_ac].modu_desc,g_oocm_d[l_ac].oocmdate,g_oocm_d[l_ac].oocmoriu,g_oocm_d[l_ac].oriu_desc,g_oocm_d[l_ac].oocmorid,g_oocm_d[l_ac].orid_desc,g_oocm_d[l_ac].oocmuser,g_oocm_d[l_ac].user_desc,g_oocm_d[l_ac].oocmdept,g_oocm_d[l_ac].dept_desc,g_oocm_d[l_ac].oocmbuid

      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 

      CALL aooi015_detail_show()      
      
      #add-point:單身資料抓取
      {<point name="bfill.foreach"/>}
      #end add-point
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         CALL cl_err( "", 9035, 0 )
         EXIT FOREACH
      END IF
      
   END FOREACH
 
   CALL g_oocm_d.deleteElement(l_ac) 
   ERROR "" 

 
   LET g_detail_cnt = l_ac - 1
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   LET l_ac = g_cnt
   LET g_cnt = 0
   
END FUNCTION
 
