   END IF


   
   IF NOT cl_null(ls_wc) THEN
      LET ls_wc = ls_wc.subString(1,ls_wc.getLength()-5)
      LET g_wc2 = ls_wc
      CALL aooi015_b_fill(g_wc2)
   ELSE
      LET g_wc2 = " 1=1"
      CALL aooi015_b_fill(g_wc2)
   END IF
 
END FUNCTION
 
#確認碼變更
PRIVATE FUNCTION aooi015_statechange(pc_state)
   {<Local define>}
   DEFINE pc_state  LIKE type_t.chr5
   DEFINE lc_state  LIKE type_t.chr5
   DEFINE ls_sql    STRING
   DEFINE li_idx    LIKE type_t.num5
   DEFINE li_cnt    LIKE type_t.num5
   DEFINE li_state  LIKE type_t.num5
   {</Local define>}
   #add-point:statechange段define
   {<point name="statechange.define"/>}
   #end add-point 
   
   #add-point:statechange段開始前
   {<point name="statechange.before"/>}
   #end add-point  
   
   ERROR ""
   
   LET li_idx = l_ac
   
   LET li_state = 1
   LET li_cnt = 0
   
   #add-point:stus修改前
   {<point name="statechange.b_update"/>}
   #end add-point   
   
   FOR l_ac = 1 TO g_oocm_d.getLength()
   
      IF NOT g_curr_diag.isRowSelected("s_detail1", l_ac) OR
         g_oocm_d[l_ac].oocmstus = pc_state THEN
         CONTINUE FOR
      END IF
      
      IF g_oocm_d[l_ac].oocm001 IS NULL
      #key2
      OR g_oocm_d[l_ac].oocm002 IS NULL

      #key3
      OR g_oocm_d[l_ac].oocm003 IS NULL

      #key4
      OR g_oocm_d[l_ac].oocm004 IS NULL


      THEN
         CALL cl_err("","std-00003",0)
         RETURN
      END IF
      
      BEGIN WORK
      
      LET lc_state = pc_state
      
      LET ls_sql = " SELECT oocmstus FROM oocm_t ",
                   " WHERE oocment = '" ||g_enterprise|| "' AND oocm001 = '",g_oocm_d[l_ac].oocm001,"'",
      #key2
                   " AND oocm002 = '",g_oocm_d[l_ac].oocm002,"'",

      #key3
                   " AND oocm003 = '",g_oocm_d[l_ac].oocm003,"'",

      #key4
                   " AND oocm004 = '",g_oocm_d[l_ac].oocm004,"'",


                   " FOR UPDATE NOWAIT"
                   
      PREPARE upd_state FROM ls_sql
      DECLARE upd_state_cur CURSOR FOR upd_state
      
      OPEN upd_state_cur
      IF SQLCA.sqlcode THEN
         CALL cl_err("",SQLCA.sqlcode,0)
         CLOSE upd_state_cur
         ROLLBACK WORK
         LET li_cnt = li_cnt + 1
         LET li_state = 2
         CONTINUE FOR
      END IF
      
      UPDATE oocm_t SET oocmstus = lc_state 
       WHERE oocment = g_enterprise AND oocm001 = g_oocm_d[l_ac].oocm001
      #key2
         AND oocm002 = g_oocm_d[l_ac].oocm002

      #key3
         AND oocm003 = g_oocm_d[l_ac].oocm003

      #key4
         AND oocm004 = g_oocm_d[l_ac].oocm004


      IF SQLCA.sqlcode THEN
         CALL cl_err("",SQLCA.sqlcode,0)
         CLOSE upd_state_cur
         ROLLBACK WORK
         LET li_cnt = li_cnt + 1
         LET li_state = 2
         CONTINUE FOR
      END IF
      
      COMMIT WORK
      
      CLOSE upd_state_cur
 
   END FOR
   
   #add-point:stus修改後
   {<point name="statechange.a_update"/>}
   #end add-point
   
   CASE li_state
      WHEN 1
      WHEN 2
         CALL cl_err(li_cnt,"std-00009",1)
   END CASE
   
   LET l_ac = li_idx
   
   #add-point:statechange段結束前
   {<point name="statechange.after"/>}
   #end add-point 
 
END FUNCTION
 
