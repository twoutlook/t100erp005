   OR g_ooff_m.ooff012 IS NULL

   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
    
   LET g_ooff001_t = g_ooff_m.ooff001
   LET g_ooff002_t = g_ooff_m.ooff002
   LET g_ooff003_t = g_ooff_m.ooff003
   LET g_ooff004_t = g_ooff_m.ooff004
   LET g_ooff005_t = g_ooff_m.ooff005
   LET g_ooff006_t = g_ooff_m.ooff006
   LET g_ooff007_t = g_ooff_m.ooff007
   LET g_ooff008_t = g_ooff_m.ooff008
   LET g_ooff009_t = g_ooff_m.ooff009
   LET g_ooff010_t = g_ooff_m.ooff010
   LET g_ooff011_t = g_ooff_m.ooff011
   LET g_ooff012_t = g_ooff_m.ooff012

   
   
 
   OPEN aooi360_cl USING g_enterprise,g_ooff_m.ooff001,g_ooff_m.ooff002,g_ooff_m.ooff003,g_ooff_m.ooff004,g_ooff_m.ooff005,g_ooff_m.ooff006,g_ooff_m.ooff007,g_ooff_m.ooff008,g_ooff_m.ooff009,g_ooff_m.ooff010,g_ooff_m.ooff011,g_ooff_m.ooff012
   IF SQLCA.SQLCODE THEN    #(ver:49)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aooi360_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE
      LET g_errparam.popup = TRUE 
      CLOSE aooi360_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aooi360_master_referesh USING g_ooff_m.ooff001,g_ooff_m.ooff002,g_ooff_m.ooff003,g_ooff_m.ooff004,g_ooff_m.ooff005,g_ooff_m.ooff006,g_ooff_m.ooff007,g_ooff_m.ooff008,g_ooff_m.ooff009,g_ooff_m.ooff010,g_ooff_m.ooff011,g_ooff_m.ooff012 INTO g_ooff_m.ooff001,g_ooff_m.ooff014,g_ooff_m.ooff012,g_ooff_m.ooff002,g_ooff_m.ooff003,g_ooff_m.ooff004,g_ooff_m.ooff005,g_ooff_m.ooff006,g_ooff_m.ooff007,g_ooff_m.ooff008,g_ooff_m.ooff009,g_ooff_m.ooff010,g_ooff_m.ooff011,g_ooff_m.ooff013,g_ooff_m.ooffstus,g_ooff_m.ooffownid,g_ooff_m.ooffowndp,g_ooff_m.ooffcrtid,g_ooff_m.ooffcrtdp,g_ooff_m.ooffcrtdt,g_ooff_m.ooffmodid,g_ooff_m.ooffmoddt,g_ooff_m.ooffownid_desc,g_ooff_m.ooffowndp_desc,g_ooff_m.ooffcrtid_desc,g_ooff_m.ooffcrtdp_desc,g_ooff_m.ooffmodid_desc
   
   
   #檢查是否允許此動作
   IF NOT aooi360_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_ooff_m_mask_o.* =  g_ooff_m.*
   CALL aooi360_ooff_t_mask()
   LET g_ooff_m_mask_n.* =  g_ooff_m.*
   
   #將最新資料顯示到畫面上
   CALL aooi360_show()
   
   IF cl_ask_delete() THEN
 
      #add-point:單頭刪除前 name="delete.head.b_delete"
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point
 
      #應用 a47 樣板自動產生(Version:4)
      #刪除相關文件
      CALL aooi360_set_pk_array()
      #add-point:相關文件刪除前 name="delete.befroe.related_document_remove"
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 


 
 
      DELETE FROM ooff_t 
       WHERE ooffent = g_enterprise AND ooff001 = g_ooff_m.ooff001 
         AND ooff002 = g_ooff_m.ooff002 
         AND ooff003 = g_ooff_m.ooff003 
