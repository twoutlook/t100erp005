{<section id="aooi360.state_change" >}
   #應用 a09 樣板自動產生(Version:17)
#+ 確認碼變更 
PRIVATE FUNCTION aooi360_statechange()
   #add-point:statechange段define(客製用) name="statechange.define_customerization"
   {<point name="statechange.define_customerization" edit="c" mark="Y"/>}
   #end add-point  
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="statechange.define"
   {<point name="statechange.define"/>}
   #end add-point  
   
   #add-point:Function前置處理 name="statechange.before"
   {<point name="statechange.before"/>}
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_ooff_m.ooff001 IS NULL
      OR g_ooff_m.ooff002 IS NULL      OR g_ooff_m.ooff003 IS NULL      OR g_ooff_m.ooff004 IS NULL      OR g_ooff_m.ooff005 IS NULL      OR g_ooff_m.ooff006 IS NULL      OR g_ooff_m.ooff007 IS NULL      OR g_ooff_m.ooff008 IS NULL      OR g_ooff_m.ooff009 IS NULL      OR g_ooff_m.ooff010 IS NULL      OR g_ooff_m.ooff011 IS NULL      OR g_ooff_m.ooff012 IS NULL
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN aooi360_cl USING g_enterprise,g_ooff_m.ooff001,g_ooff_m.ooff002,g_ooff_m.ooff003,g_ooff_m.ooff004,g_ooff_m.ooff005,g_ooff_m.ooff006,g_ooff_m.ooff007,g_ooff_m.ooff008,g_ooff_m.ooff009,g_ooff_m.ooff010,g_ooff_m.ooff011,g_ooff_m.ooff012
   IF STATUS THEN
      CLOSE aooi360_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aooi360_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE aooi360_master_referesh USING g_ooff_m.ooff001,g_ooff_m.ooff002,g_ooff_m.ooff003,g_ooff_m.ooff004,g_ooff_m.ooff005,g_ooff_m.ooff006,g_ooff_m.ooff007,g_ooff_m.ooff008,g_ooff_m.ooff009,g_ooff_m.ooff010,g_ooff_m.ooff011,g_ooff_m.ooff012 INTO g_ooff_m.ooff001,g_ooff_m.ooff014,g_ooff_m.ooff012,g_ooff_m.ooff002,g_ooff_m.ooff003,g_ooff_m.ooff004,g_ooff_m.ooff005,g_ooff_m.ooff006,g_ooff_m.ooff007,g_ooff_m.ooff008,g_ooff_m.ooff009,g_ooff_m.ooff010,g_ooff_m.ooff011,g_ooff_m.ooff013,g_ooff_m.ooffstus,g_ooff_m.ooffownid,g_ooff_m.ooffowndp,g_ooff_m.ooffcrtid,g_ooff_m.ooffcrtdp,g_ooff_m.ooffcrtdt,g_ooff_m.ooffmodid,g_ooff_m.ooffmoddt,g_ooff_m.ooffownid_desc,g_ooff_m.ooffowndp_desc,g_ooff_m.ooffcrtid_desc,g_ooff_m.ooffcrtdp_desc,g_ooff_m.ooffmodid_desc
   
 
   #檢查是否允許此動作
   IF NOT aooi360_action_chk() THEN
      CLOSE aooi360_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_ooff_m.ooff001,g_ooff_m.ooff014,g_ooff_m.ooff012,g_ooff_m.ooff002,g_ooff_m.ooff003,g_ooff_m.ooff004,g_ooff_m.ooff005,g_ooff_m.ooff006,g_ooff_m.ooff007,g_ooff_m.ooff008,g_ooff_m.ooff009,g_ooff_m.ooff010,g_ooff_m.ooff011,g_ooff_m.ooff013,g_ooff_m.ooffstus,g_ooff_m.ooffownid,g_ooff_m.ooffownid_desc,g_ooff_m.ooffowndp,g_ooff_m.ooffowndp_desc,g_ooff_m.ooffcrtid,g_ooff_m.ooffcrtid_desc,g_ooff_m.ooffcrtdp,g_ooff_m.ooffcrtdp_desc,g_ooff_m.ooffcrtdt,g_ooff_m.ooffmodid,g_ooff_m.ooffmodid_desc,g_ooff_m.ooffmoddt
 
   CASE g_ooff_m.ooffstus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   {<point name="statechange.after_refresh"/>}
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_ooff_m.ooffstus
            
            WHEN "N"
               HIDE OPTION "inactive"
            WHEN "Y"
               HIDE OPTION "active"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      {<point name="statechange.before_menu"/>}
      #end add-point
      
      
	  
      ON ACTION inactive
         IF cl_auth_chk_act("inactive") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.inactive"
            {<point name="statechange.inactive"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION active
         IF cl_auth_chk_act("active") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.active"
            {<point name="statechange.active"/>}
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      {<point name="statechange.more_control"/>}
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      ) OR 
      g_ooff_m.ooffstus = lc_state OR cl_null(lc_state) THEN
      CLOSE aooi360_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   {<point name="statechange.b_update"/>}
   #end add-point
   
   LET g_ooff_m.ooffmodid = g_user
   LET g_ooff_m.ooffmoddt = cl_get_current()
   LET g_ooff_m.ooffstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE ooff_t 
      SET (ooffstus,ooffmodid,ooffmoddt) 
        = (g_ooff_m.ooffstus,g_ooff_m.ooffmodid,g_ooff_m.ooffmoddt)     
    WHERE ooffent = g_enterprise AND ooff001 = g_ooff_m.ooff001
      AND ooff002 = g_ooff_m.ooff002      AND ooff003 = g_ooff_m.ooff003      AND ooff004 = g_ooff_m.ooff004      AND ooff005 = g_ooff_m.ooff005      AND ooff006 = g_ooff_m.ooff006      AND ooff007 = g_ooff_m.ooff007      AND ooff008 = g_ooff_m.ooff008      AND ooff009 = g_ooff_m.ooff009      AND ooff010 = g_ooff_m.ooff010      AND ooff011 = g_ooff_m.ooff011      AND ooff012 = g_ooff_m.ooff012
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE aooi360_master_referesh USING g_ooff_m.ooff001,g_ooff_m.ooff002,g_ooff_m.ooff003,g_ooff_m.ooff004,g_ooff_m.ooff005,g_ooff_m.ooff006,g_ooff_m.ooff007,g_ooff_m.ooff008,g_ooff_m.ooff009,g_ooff_m.ooff010,g_ooff_m.ooff011,g_ooff_m.ooff012 INTO g_ooff_m.ooff001,g_ooff_m.ooff014,g_ooff_m.ooff012,g_ooff_m.ooff002,g_ooff_m.ooff003,g_ooff_m.ooff004,g_ooff_m.ooff005,g_ooff_m.ooff006,g_ooff_m.ooff007,g_ooff_m.ooff008,g_ooff_m.ooff009,g_ooff_m.ooff010,g_ooff_m.ooff011,g_ooff_m.ooff013,g_ooff_m.ooffstus,g_ooff_m.ooffownid,g_ooff_m.ooffowndp,g_ooff_m.ooffcrtid,g_ooff_m.ooffcrtdp,g_ooff_m.ooffcrtdt,g_ooff_m.ooffmodid,g_ooff_m.ooffmoddt,g_ooff_m.ooffownid_desc,g_ooff_m.ooffowndp_desc,g_ooff_m.ooffcrtid_desc,g_ooff_m.ooffcrtdp_desc,g_ooff_m.ooffmodid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_ooff_m.ooff001,g_ooff_m.ooff014,g_ooff_m.ooff012,g_ooff_m.ooff002,g_ooff_m.ooff003,g_ooff_m.ooff004,g_ooff_m.ooff005,g_ooff_m.ooff006,g_ooff_m.ooff007,g_ooff_m.ooff008,g_ooff_m.ooff009,g_ooff_m.ooff010,g_ooff_m.ooff011,g_ooff_m.ooff013,g_ooff_m.ooffstus,g_ooff_m.ooffownid,g_ooff_m.ooffownid_desc,g_ooff_m.ooffowndp,g_ooff_m.ooffowndp_desc,g_ooff_m.ooffcrtid,g_ooff_m.ooffcrtid_desc,g_ooff_m.ooffcrtdp,g_ooff_m.ooffcrtdp_desc,g_ooff_m.ooffcrtdt,g_ooff_m.ooffmodid,g_ooff_m.ooffmodid_desc,g_ooff_m.ooffmoddt
   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
   {<point name="statechange.a_update"/>}
   #end add-point
 
   #add-point:statechange段結束前 name="statechange.after"
   {<point name="statechange.after"/>}
   #end add-point  
 
   CLOSE aooi360_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aooi360_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION



