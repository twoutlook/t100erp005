#+ 資料修改
PRIVATE FUNCTION aooi016_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_ooan_m.ooan001 IS NULL
      #key2
   OR g_ooan_m.ooan002 IS NULL

      #key3
   OR g_ooan_m.ooan003 IS NULL


   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
    SELECT UNIQUE ooan001,ooan002,ooan003
 INTO g_ooan_m.ooan001,g_ooan_m.ooan002,g_ooan_m.ooan003
 FROM ooan_t
 WHERE ooanent = g_enterprise AND ooan001 = g_ooan_m.ooan001 AND ooan002 = g_ooan_m.ooan002 AND ooan003 = g_ooan_m.ooan003
 
   ERROR ""
  
   LET g_ooan001_t = g_ooan_m.ooan001
      #key2
   LET g_ooan002_t = g_ooan_m.ooan002

      #key3
   LET g_ooan003_t = g_ooan_m.ooan003


   BEGIN WORK
   
   OPEN aooi016_cl USING g_enterprise,g_ooan_m.ooan001
      #key2
                                                       ,g_ooan_m.ooan002

      #key3
                                                       ,g_ooan_m.ooan003


   IF STATUS THEN
      CALL cl_err("OPEN aooi016_cl:", STATUS, 1)
      CLOSE aooi016_cl
      ROLLBACK WORK
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aooi016_cl INTO g_ooan_m.ooan001,g_ooan_m.ooan002,g_ooan_m.ooan003,g_ooan_m.ooan001_desc,g_ooan_m.ooan002_desc,g_ooan_m.ooan003_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_ooan_m.ooan001,SQLCA.sqlcode,0)
      CLOSE aooi016_cl
      ROLLBACK WORK
      RETURN
   END IF
 
   CALL aooi016_show()
   WHILE TRUE
      LET g_ooan001_t = g_ooan_m.ooan001
      #key2
      LET g_ooan002_t = g_ooan_m.ooan002

      #key3
      LET g_ooan003_t = g_ooan_m.ooan003


 
      CALL aooi016_input("u")     #欄位更改
 
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_ooan_m.* = g_ooan_m_t.*
         CALL aooi016_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_ooan_m.ooan001 != g_ooan001_t 
      #key2
      OR g_ooan_m.ooan002 != g_ooan002_t 

      #key3
      OR g_ooan_m.ooan003 != g_ooan003_t 


      THEN
         BEGIN WORK
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE ooan_t SET ooan001 = g_ooan_m.ooan001
      #key2
                                       , ooan002 = g_ooan_m.ooan002

      #key3
                                       , ooan003 = g_ooan_m.ooan003


          WHERE ooanent = g_enterprise AND ooan001 = g_ooan001_t
      #key2
            AND ooan002 = g_ooan002_t

      #key3
            AND ooan003 = g_ooan003_t


          IF SQLCA.sqlcode THEN
             CALL cl_err("ooan_t",SQLCA.sqlcode,1) 
             ROLLBACK WORK
             CONTINUE WHILE
         END IF
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_used_modified_record(g_ooan_m.ooan001,g_site) THEN 
      ROLLBACK WORK
   END IF
 
   CLOSE aooi016_cl
   COMMIT WORK
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_ooan_m.ooan001,'U')
 
   CALL aooi016_b_fill("1=1")
   
END FUNCTION   
 
 
#+ 資料輸入
PRIVATE FUNCTION aooi016_input(p_cmd)
   {<Local define>}
   DEFINE  p_cmd           LIKE type_t.chr1
   DEFINE  l_cmd           LIKE type_t.chr1
