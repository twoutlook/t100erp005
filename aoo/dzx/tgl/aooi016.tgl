#+ 資料複製
PRIVATE FUNCTION aooi016_reproduce()
   {<Local define>}
   DEFINE l_newno     LIKE ooan_t.ooan001 
   DEFINE l_oldno     LIKE ooan_t.ooan001 
      #key2
   DEFINE l_newno02     LIKE ooan_t.ooan002 
   DEFINE l_oldno02     LIKE ooan_t.ooan002 

      #key3
   DEFINE l_newno03     LIKE ooan_t.ooan003 
   DEFINE l_oldno03     LIKE ooan_t.ooan003 


   DEFINE l_master    RECORD LIKE ooan_t.*
   DEFINE l_detail    RECORD LIKE ooan_t.*
   DEFINE l_cnt       LIKE type_t.num5
   {</Local define>}
   
   #add-point:reproduce段define
   {<point name="reproduce.define"/>}
   #end add-point   
 
   IF g_ooan_m.ooan001 IS NULL
      #key2
      OR g_ooan_m.ooan002 IS NULL

      #key3
      OR g_ooan_m.ooan003 IS NULL


      THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
    
   LET g_before_input_done = FALSE
   CALL aooi016_set_entry('a')
   LET g_before_input_done = TRUE
 
   CALL cl_set_head_visible("","YES")
 
   INPUT l_newno #FROM 
      #key2
         ,l_newno02

      #key3
         ,l_newno03


    FROM ooan001 
      #key2
         ,ooan002 

      #key3
         ,ooan003 


      BEFORE INPUT
            
      AFTER INPUT
         IF INT_FLAG = 1 THEN
            EXIT INPUT
         END IF
         IF l_newno IS NULL 
      #key2
            OR l_newno02 IS NULL

      #key3
            OR l_newno03 IS NULL


            THEN
         CALL cl_err("Reproduce","std-00006",1) 
         NEXT FIELD ooan001 
         END IF
         #確定該key值是否有重複定義
         LET l_cnt = 0
         SELECT COUNT(*) INTO l_cnt FROM ooan_t 
          WHERE ooanent = g_enterprise AND ooan001 = l_newno
      #key2
            AND ooan002 = l_newno02

      #key3
            AND ooan003 = l_newno03


         IF l_cnt > 0 THEN
            CALL cl_err("Reproduce","std-00006",1)
            #NEXT FIELD ooan001 
         END IF
 
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE INPUT
   END INPUT
   
   IF INT_FLAG OR l_newno IS NULL THEN
      LET INT_FLAG = 0
      RETURN
   END IF
   
   BEGIN WORK
   
   LET g_sql = "SELECT * FROM ooan_t WHERE ooanent = '" ||g_enterprise|| "' AND ",
               " ooan001 = '",g_ooan_m.ooan001,"'"
      #key2
              ," AND ooan002 = '",g_ooan_m.ooan002,"'"

      #key3
              ," AND ooan003 = '",g_ooan_m.ooan003,"'"


   DECLARE aooi016_reproduce CURSOR FROM g_sql
 
   FOREACH aooi016_reproduce INTO l_detail.*
   
      #add-point:單身複製前
      {<point name="reproduce.body.b_insert"/>}
      #end add-point
   
      LET l_detail.ooan001 = l_newno
      #key2
      LET l_detail.ooan002 = l_newno02

      #key3
      LET l_detail.ooan003 = l_newno03


      LET l_detail.ooanuser = g_user
      LET l_detail.ooandept = g_dept
      LET l_detail.ooanbuid = cl_get_current()
      LET l_detail.ooanoriu = g_user 
      LET l_detail.ooanorid = g_dept 
      LET l_detail.ooanmodu = g_user
      LET l_detail.ooandate = cl_get_current()
      #LET l_detail.ooanstus = 'Y'
 
      INSERT INTO ooan_t VALUES (l_detail.*) #複製單身
      IF SQLCA.sqlcode THEN
         CALL cl_err('Insert error!',SQLCA.sqlcode,1)
         ROLLBACK WORK
         RETURN
      END IF
  
      #add-point:單身複製後
      {<point name="reproduce.body.a_insert"/>}
      #end add-point
   END FOREACH
 
   COMMIT WORK
   ERROR 'ROW(',l_newno,') O.K'
   LET l_oldno = g_ooan_m.ooan001
      #key2
   LET l_oldno02 = g_ooan_m.ooan002

      #key3
   LET l_oldno03 = g_ooan_m.ooan003


   
   SELECT ooan001,ooan002,ooan003 INTO g_ooan_m.ooan001,g_ooan_m.ooan002,g_ooan_m.ooan003 FROM ooan_t 
    WHERE ooanent = g_enterprise AND ooan001 = l_newno
      #key2
      AND ooan002 = l_newno02

      #key3
      AND ooan003 = l_newno03


      
   CALL aooi016_show()
   LET g_ooan_m.ooan001 = l_oldno
      #key2
   LET g_ooan_m.ooan002 = l_oldno02

      #key3
   LET g_ooan_m.ooan003 = l_oldno03


   
   SELECT ooan001,ooan002,ooan003 INTO g_ooan_m.ooan001,g_ooan_m.ooan002,g_ooan_m.ooan003 FROM ooan_t 
    WHERE ooanent = g_enterprise AND ooan001 = g_ooan_m.ooan001
      #key2
      AND ooan002 = g_ooan_m.ooan002

      #key3
      AND ooan003 = g_ooan_m.ooan003


   CALL aooi016_show()
 
   DISPLAY BY NAME g_ooan_m.ooan001
      #key2
   DISPLAY BY NAME g_ooan_m.ooan002

      #key3
   DISPLAY BY NAME g_ooan_m.ooan003


   
END FUNCTION
 
