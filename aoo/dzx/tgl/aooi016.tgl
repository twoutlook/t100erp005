   {</Local define>}
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_ooan_m.ooan001 IS NULL
      #key2
   OR g_ooan_m.ooan002 IS NULL

      #key3
   OR g_ooan_m.ooan003 IS NULL


   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
    SELECT UNIQUE ooan001,ooan002,ooan003
 INTO g_ooan_m.ooan001,g_ooan_m.ooan002,g_ooan_m.ooan003
 FROM ooan_t
 WHERE ooanent = g_enterprise AND ooan001 = g_ooan_m.ooan001 AND ooan002 = g_ooan_m.ooan002 AND ooan003 = g_ooan_m.ooan003
   BEGIN WORK
   
   
 
   OPEN aooi016_cl USING g_enterprise,g_ooan_m.ooan001
      #key2
                                                       ,g_ooan_m.ooan002

      #key3
                                                       ,g_ooan_m.ooan003


   IF STATUS THEN
      CALL cl_err("OPEN aooi016_cl:", STATUS, 1)
      CLOSE aooi016_cl
      ROLLBACK WORK
      RETURN
   END IF
 
   FETCH aooi016_cl INTO g_ooan_m.ooan001,g_ooan_m.ooan002,g_ooan_m.ooan003,g_ooan_m.ooan001_desc,g_ooan_m.ooan002_desc,g_ooan_m.ooan003_desc              # 鎖住將被更改或取消的資料
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_ooan_m.ooan001,SQLCA.sqlcode,0)          #資料被他人LOCK
      ROLLBACK WORK
      RETURN
   END IF
 
   CALL aooi016_show()
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      INITIALIZE g_doc.* TO NULL         
      LET g_doc.column1 = "ooan001"       
      LET g_doc.value1 = g_ooan_m.ooan001     
      CALL cl_del_doc()    
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete"/>}
      #end add-point
      
      DELETE FROM ooan_t WHERE ooanent = g_enterprise AND ooan001 = g_ooan_m.ooan001
      #key2
                                                               AND ooan002 = g_ooan_m.ooan002

      #key3
                                                               AND ooan003 = g_ooan_m.ooan003


      IF SQLCA.sqlcode THEN
         CALL cl_err("ooan_t",SQLCA.sqlcode,0)
         ROLLBACK WORK
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
      CLEAR FORM
      CALL g_ooan_d.clear() 

     
      CALL aooi016_ui_browser_refresh()  
      CALL aooi016_ui_headershow()  
      CALL aooi016_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL aooi016_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         CALL aooi016_browser_fill("first")
      END IF
       
   END IF
 
   CLOSE aooi016_cl
   COMMIT WORK
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_ooan_m.ooan001,'D')
    
END FUNCTION
 
 
#+ 單身陣列填充
PRIVATE FUNCTION aooi016_b_fill(p_wc2)
   {<Local define>}
   DEFINE p_wc2      STRING
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_ooan_d.clear()    #g_ooan_d 單頭及單身 

 
   LET g_sql = "SELECT ooanstus,ooan004,ooan005,ooan006,ooan007,ooan008,ooan009,ooanmodu,'',ooandate,ooanoriu,'',ooanorid,'',ooanuser,'',ooandept,'',ooanbuid FROM ooan_t WHERE ooanent= ? AND ooan001=? AND ooan002=? AND ooan003=?"   
 
   IF NOT cl_null(p_wc2) THEN
      LET g_sql=g_sql CLIPPED," AND ",p_wc2 CLIPPED
   END IF
   
   LET g_sql = g_sql, " ORDER BY ooan001,ooan002,ooan003,ooan004"
 
   PREPARE aooi016_pb FROM g_sql
   DECLARE b_fill_cs CURSOR FOR aooi016_pb
 
   LET g_cnt = l_ac
   LET l_ac = 1
 
   OPEN b_fill_cs USING g_enterprise,g_ooan_m.ooan001
      #key2
                                            ,g_ooan_m.ooan002

      #key3
                                            ,g_ooan_m.ooan003


                                            
   FOREACH b_fill_cs INTO g_ooan_d[l_ac].ooanstus,g_ooan_d[l_ac].ooan004,g_ooan_d[l_ac].ooan005,g_ooan_d[l_ac].ooan006,g_ooan_d[l_ac].ooan007,g_ooan_d[l_ac].ooan008,g_ooan_d[l_ac].ooan009,g_ooan_d[l_ac].ooanmodu,g_ooan_d[l_ac].modu_desc,g_ooan_d[l_ac].ooandate,g_ooan_d[l_ac].ooanoriu,g_ooan_d[l_ac].oriu_desc,g_ooan_d[l_ac].ooanorid,g_ooan_d[l_ac].orid_desc,g_ooan_d[l_ac].ooanuser,g_ooan_d[l_ac].user_desc,g_ooan_d[l_ac].ooandept,g_ooan_d[l_ac].dept_desc,g_ooan_d[l_ac].ooanbuid

                          
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
                        
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      #帶出預設欄位之值
      LET g_ooan_d[l_ac].modu_desc = cl_get_username(g_ooan_d[l_ac].ooanmodu)
      LET g_ooan_d[l_ac].oriu_desc = cl_get_username(g_ooan_d[l_ac].ooanoriu)
      LET g_ooan_d[l_ac].user_desc = cl_get_username(g_ooan_d[l_ac].ooanuser)
      LET g_ooan_d[l_ac].dept_desc = cl_get_deptname(g_ooan_d[l_ac].ooandept)
      LET g_ooan_d[l_ac].orid_desc = cl_get_deptname(g_ooan_d[l_ac].ooanorid)
     

     
      #add-point:單身資料抓取
      {<point name="bfill.foreach"/>}
     #end add-point
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         CALL cl_err( '', 9035, 0 )
         EXIT FOREACH
      END IF
   END FOREACH
   CALL g_ooan_d.deleteElement(l_ac)

 
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
END FUNCTION
 
 
