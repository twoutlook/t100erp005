  <section id="aqci005.b_fill" ver="1" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aqci005_b_fill(p_wc2)              #BODY FILL UP
   {<Local define>}
   DEFINE p_wc2           STRING
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE qcaestus,qcae001,qcae002,qcae003,qcae004,qcae005,qcae006,'','','',qcaemodid, 
       '',qcaemoddt,qcaeownid,'',qcaeowndp,'',qcaecrtid,'',qcaecrtdp,'',qcaecrtdt FROM qcae_t",
               "",
               " WHERE qcaeent= ? AND 1=1 AND ", p_wc2
    
   LET g_sql = g_sql, " ORDER BY qcae_t.qcae001,qcae_t.qcae002,qcae_t.qcae003"
  
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
  
   PREPARE aqci005_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aqci005_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_qcae_d.clear()
   CALL g_qcae1_info_d.clear()   
 
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_qcae_d[l_ac].qcaestus,g_qcae_d[l_ac].qcae001,g_qcae_d[l_ac].qcae002,g_qcae_d[l_ac].qcae003, 
       g_qcae_d[l_ac].qcae004,g_qcae_d[l_ac].qcae005,g_qcae_d[l_ac].qcae006,g_qcae1_info_d[l_ac].qcae001, 
       g_qcae1_info_d[l_ac].qcae002,g_qcae1_info_d[l_ac].qcae003,g_qcae1_info_d[l_ac].qcaemodid,g_qcae1_info_d[l_ac].qcaemodid_desc, 
       g_qcae1_info_d[l_ac].qcaemoddt,g_qcae1_info_d[l_ac].qcaeownid,g_qcae1_info_d[l_ac].qcaeownid_desc, 
       g_qcae1_info_d[l_ac].qcaeowndp,g_qcae1_info_d[l_ac].qcaeowndp_desc,g_qcae1_info_d[l_ac].qcaecrtid, 
       g_qcae1_info_d[l_ac].qcaecrtid_desc,g_qcae1_info_d[l_ac].qcaecrtdp,g_qcae1_info_d[l_ac].qcaecrtdp_desc, 
       g_qcae1_info_d[l_ac].qcaecrtdt
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL aqci005_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec AND g_error_show = 1 THEN
      CALL cl_err("qcae_t",9035,1)
   END IF 
   LET g_error_show = 0
   
 
  
   
   CALL g_qcae_d.deleteElement(g_qcae_d.getLength())   
   CALL g_qcae1_info_d.deleteElement(g_qcae1_info_d.getLength())
 
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_qcae_d.getLength()
      LET g_qcae1_info_d[l_ac].qcae001 = g_qcae_d[l_ac].qcae001 
      LET g_qcae1_info_d[l_ac].qcae002 = g_qcae_d[l_ac].qcae002 
 
      LET g_qcae1_info_d[l_ac].qcae003 = g_qcae_d[l_ac].qcae003 
 
 
 
   END FOR
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = l_ac - 1
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE aqci005_pb
   
END FUNCTION
]]>
  </section>
