  <section id="aqci005.modify" ver="1" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aqci005_modify()
   {<Local define>}
   DEFINE  l_cmd                  LIKE type_t.chr1
   DEFINE  l_ac_t                 LIKE type_t.num5                #未取消的ARRAY CNT 
   DEFINE  l_n                    LIKE type_t.num5                #檢查重複用  
   DEFINE  l_cnt                  LIKE type_t.num5                #檢查重複用  
   DEFINE  l_lock_sw              LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert         LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete         LIKE type_t.num5                #可刪除否  
   DEFINE  l_count                LIKE type_t.num5
   DEFINE  l_i                    LIKE type_t.num5
   DEFINE  ls_return              STRING
   DEFINE  l_var_keys             DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys           DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                 DYNAMIC ARRAY OF STRING
   DEFINE  l_fields               DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak         DYNAMIC ARRAY OF STRING
   DEFINE  li_reproduce           LIKE type_t.num5
   DEFINE  li_reproduce_target    LIKE type_t.num5
   DEFINE  lb_reproduce           BOOLEAN
   {</Local define>}
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point 
   
   LET g_action_choice = ""
   
   LET g_qryparam.state = "i"
 
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
 
   #add-point:modify段define_sql
   {<point name="modify.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT qcaestus,qcae001,qcae002,qcae003,qcae004,qcae005,qcae006,'','','',qcaemodid, 
       '',qcaemoddt,qcaeownid,'',qcaeowndp,'',qcaecrtid,'',qcaecrtdp,'',qcaecrtdt FROM qcae_t WHERE  
       qcaeent=? AND qcae001=? AND qcae002=? AND qcae003=? FOR UPDATE"
   #add-point:modify段define_sql
   {<point name="modify.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   DECLARE aqci005_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR
   
 
   
   LET INT_FLAG = FALSE
   LET lb_reproduce = FALSE
 
   #add-point:modify段修改前
   {<point name="modify.before_input"/>}
   #end add-point
 
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #Page1 預設值產生於此處
      INPUT ARRAY g_qcae_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_detail_cnt,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_qcae_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL aqci005_b_fill(g_wc2)
            LET g_detail_cnt = g_qcae_d.getLength()
         
         BEFORE ROW
            #add-point:modify段before row
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            DISPLAY g_qcae_d.getLength() TO FORMONLY.cnt
         
            CALL s_transaction_begin()
            LET g_detail_cnt = g_qcae_d.getLength()
            
            IF g_detail_cnt >= l_ac 
               AND g_qcae_d[l_ac].qcae001 IS NOT NULL
               AND g_qcae_d[l_ac].qcae002 IS NOT NULL
 
               AND g_qcae_d[l_ac].qcae003 IS NOT NULL
 
 
            THEN
               LET l_cmd='u'
               LET g_qcae_d_t.* = g_qcae_d[l_ac].*  #BACKUP
               IF NOT aqci005_lock_b("qcae_t") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aqci005_bcl INTO g_qcae_d[l_ac].qcaestus,g_qcae_d[l_ac].qcae001,g_qcae_d[l_ac].qcae002, 
                      g_qcae_d[l_ac].qcae003,g_qcae_d[l_ac].qcae004,g_qcae_d[l_ac].qcae005,g_qcae_d[l_ac].qcae006, 
                      g_qcae1_info_d[l_ac].qcae001,g_qcae1_info_d[l_ac].qcae002,g_qcae1_info_d[l_ac].qcae003, 
                      g_qcae1_info_d[l_ac].qcaemodid,g_qcae1_info_d[l_ac].qcaemodid_desc,g_qcae1_info_d[l_ac].qcaemoddt, 
                      g_qcae1_info_d[l_ac].qcaeownid,g_qcae1_info_d[l_ac].qcaeownid_desc,g_qcae1_info_d[l_ac].qcaeowndp, 
                      g_qcae1_info_d[l_ac].qcaeowndp_desc,g_qcae1_info_d[l_ac].qcaecrtid,g_qcae1_info_d[l_ac].qcaecrtid_desc, 
                      g_qcae1_info_d[l_ac].qcaecrtdp,g_qcae1_info_d[l_ac].qcaecrtdp_desc,g_qcae1_info_d[l_ac].qcaecrtdt 

                  IF SQLCA.sqlcode THEN
                     CALL cl_err(g_qcae_d_t.qcae001,SQLCA.sqlcode,1)
                     LET l_lock_sw = "Y"
                  END IF
                  
                  CALL aqci005_detail_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
        
         BEFORE INSERT
            
            CALL s_transaction_begin()
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_qcae_d_t.* TO NULL
            INITIALIZE g_qcae_d[l_ac].* TO NULL 
            
            LET g_qcae_d_t.* = g_qcae_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL aqci005_set_entry_b("a")
            CALL aqci005_set_no_entry_b("a")
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_qcae_d[li_reproduce_target].* = g_qcae_d[li_reproduce].*
               LET g_qcae1_info_d[li_reproduce_target].* = g_qcae1_info_d[li_reproduce].*
 
               LET g_qcae_d[g_qcae_d.getLength()].qcae001 = NULL
               LET g_qcae_d[g_qcae_d.getLength()].qcae002 = NULL
 
               LET g_qcae_d[g_qcae_d.getLength()].qcae003 = NULL
 
 
            END IF
            #公用欄位給值(單身)
            #此段落由子樣板a14產生    
      LET g_qcae1_info_d[l_ac].qcaeownid = g_user
      LET g_qcae1_info_d[l_ac].qcaeowndp = g_dept
      LET g_qcae1_info_d[l_ac].qcaecrtid = g_user
      LET g_qcae1_info_d[l_ac].qcaecrtdp = g_dept 
      LET g_qcae1_info_d[l_ac].qcaecrtdt = cl_get_current()
      LET g_qcae1_info_d[l_ac].qcaemodid = ""
      LET g_qcae1_info_d[l_ac].qcaemoddt = ""
      #LET g_qcae_d[l_ac].qcaestus = ""
 
 
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM qcae_t 
             WHERE qcaeent = g_enterprise AND qcae001 = g_qcae_d[l_ac].qcae001
                                       AND qcae002 = g_qcae_d[l_ac].qcae002
 
                                       AND qcae003 = g_qcae_d[l_ac].qcae003
 
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前
               {<point name="input.body.b_insert"/>}
               #end add-point
            
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_qcae_d[g_detail_idx].qcae001
               LET gs_keys[2] = g_qcae_d[g_detail_idx].qcae002
               LET gs_keys[3] = g_qcae_d[g_detail_idx].qcae003
               CALL aqci005_insert_b('qcae_t',gs_keys,"'1'")
                           
               #add-point:單身新增後
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_qcae_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("qcae_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL aqci005_b_fill(g_wc2)
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert2"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR 'INSERT O.K'
               LET g_detail_cnt = g_detail_cnt + 1
               
               LET g_wc2 = g_wc2, " OR (qcae001 = '", g_qcae_d[l_ac].qcae001, "' "
                                  ," AND qcae002 = '", g_qcae_d[l_ac].qcae002, "' "
 
                                  ," AND qcae003 = '", g_qcae_d[l_ac].qcae003, "' "
 
 
                                  ,")"
            END IF                
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_qcae_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_qcae_d.deleteElement(l_ac)
               NEXT FIELD qcae001
            END IF
            IF g_qcae_d[l_ac].qcae001 IS NOT NULL
               AND g_qcae_d_t.qcae002 IS NOT NULL
 
               AND g_qcae_d_t.qcae003 IS NOT NULL
 
 
               THEN     
			   
               #add-point:單身刪除ask前
               {<point name="input.body.b_delete_ask"/>}
               #end add-point   
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete" mark="Y"/>}
               #end add-point   
               
               DELETE FROM qcae_t
                WHERE qcaeent = g_enterprise AND 
                      qcae001 = g_qcae_d_t.qcae001
                      AND qcae002 = g_qcae_d_t.qcae002
 
                      AND qcae003 = g_qcae_d_t.qcae003
 
 
                      
               #add-point:單身刪除中
               {<point name="input.body.m_delete"/>}
               #end add-point  
                      
               IF SQLCA.sqlcode THEN
                  CALL cl_err("qcae_t",SQLCA.sqlcode,1)
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               ELSE
                  LET g_detail_cnt = g_detail_cnt-1
                  
                  #add-point:單身刪除後
                  {<point name="input.body.a_delete"/>}
                  #end add-point
                  CALL s_transaction_end('Y','0')
               END IF 
               CLOSE aqci005_bcl
               LET l_count = g_qcae_d.getLength()
            END IF 
                           INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_qcae_d[g_detail_idx].qcae001
               LET gs_keys[2] = g_qcae_d[g_detail_idx].qcae002
               LET gs_keys[3] = g_qcae_d[g_detail_idx].qcae003
 
              
         AFTER DELETE 
            #add-point:單身刪除後2
            {<point name="input.body.after_delete"/>}
            #end add-point
                           CALL aqci005_delete_b('qcae_t',gs_keys,"'1'")
 
         #---------------------<  Detail: page1  >---------------------
         #----<<qcaestus>>----
         #此段落由子樣板a01產生
         BEFORE FIELD qcaestus
            #add-point:BEFORE FIELD qcaestus
            {<point name="input.b.page1.qcaestus" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD qcaestus
            
            #add-point:AFTER FIELD qcaestus
            {<point name="input.a.page1.qcaestus" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE qcaestus
            #add-point:ON CHANGE qcaestus
            {<point name="input.g.page1.qcaestus" />}
            #END add-point
 
 
         #---------------------<  Detail: page1  >---------------------
         #----<<qcaestus>>----
         #Ctrlp:input.c.page1.qcaestus
         ON ACTION controlp INFIELD qcaestus
            #add-point:ON ACTION controlp INFIELD qcaestus
            {<point name="input.c.page1.qcaestus" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_qcae_d[l_ac].* = g_qcae_d_t.*
               CLOSE aqci005_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_qcae_d[l_ac].qcae001,-263,1)
               LET g_qcae_d[l_ac].* = g_qcae_d_t.*
            ELSE
               
               #寫入修改者/修改日期資訊(單身)
               LET g_qcae1_info_d[l_ac].qcaemodid = g_user 
LET g_qcae1_info_d[l_ac].qcaemoddt = cl_get_current()
 
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               UPDATE qcae_t SET (qcaestus,qcae001,qcae002,qcae003,qcae004,qcae005,qcae006,qcaemodid, 
                   qcaemoddt,qcaeownid,qcaeowndp,qcaecrtid,qcaecrtdp,qcaecrtdt) = (g_qcae_d[l_ac].qcaestus, 
                   g_qcae_d[l_ac].qcae001,g_qcae_d[l_ac].qcae002,g_qcae_d[l_ac].qcae003,g_qcae_d[l_ac].qcae004, 
                   g_qcae_d[l_ac].qcae005,g_qcae_d[l_ac].qcae006,g_qcae1_info_d[l_ac].qcaemodid,g_qcae1_info_d[l_ac].qcaemoddt, 
                   g_qcae1_info_d[l_ac].qcaeownid,g_qcae1_info_d[l_ac].qcaeowndp,g_qcae1_info_d[l_ac].qcaecrtid, 
                   g_qcae1_info_d[l_ac].qcaecrtdp,g_qcae1_info_d[l_ac].qcaecrtdt)
                WHERE qcaeent = g_enterprise AND
                  qcae001 = g_qcae_d_t.qcae001 #項次   
                  AND qcae002 = g_qcae_d_t.qcae002  
 
                  AND qcae003 = g_qcae_d_t.qcae003  
 
 
                  
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
                  
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("qcae_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                    WHEN SQLCA.sqlcode #其他錯誤
                     CALL cl_err("qcae_t",SQLCA.sqlcode,1)  
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_qcae_d[g_detail_idx].qcae001
               LET gs_keys_bak[1] = g_qcae_d_t.qcae001
               LET gs_keys[2] = g_qcae_d[g_detail_idx].qcae002
               LET gs_keys_bak[2] = g_qcae_d_t.qcae002
               LET gs_keys[3] = g_qcae_d[g_detail_idx].qcae003
               LET gs_keys_bak[3] = g_qcae_d_t.qcae003
               CALL aqci005_update_b('qcae_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
               END CASE
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            CALL aqci005_unlock_b("qcae_t")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock
            
            
         AFTER INPUT
            #add-point:單身input後
            {<point name="input.body.a_input"/>}
            #end add-point
            
         ON ACTION controlo   
            CALL FGL_SET_ARR_CURR(g_qcae_d.getLength()+1)
            LET lb_reproduce = TRUE
