  <point name="function.aqci050_qcap005_entry" cite_std="N" status="" ver="1" src="s" new="Y" order="4" mark_hard="N">
<![CDATA[
PRIVATE FUNCTION aqci050_qcap005_entry(p_qcap001)
DEFINE p_qcap001    LIKE qcap_t.qcap001   
DEFINE l_n          LIKE type_t.num5
   IF NOT cl_null(p_qcap001) THEN
      LET l_n = 0
      SELECT COUNT(*) INTO l_n FROM imaa_t WHERE imaa001 = p_qcap001
         AND imaaent = g_enterprise AND imaa005 IS NOT NULL
      IF l_n = 0 THEN
         RETURN FALSE
      ELSE
         RETURN TRUE
      END IF             
   END IF
   RETURN FALSE
END FUNCTION]]>
</point>
