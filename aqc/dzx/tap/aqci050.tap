               END IF
            END IF
           #controlp
                     
           LET g_qcap001_t = g_qcap_m.qcap001
 
           
           #若單身還沒有輸入資料, 強制切換至單身
           #IF cl_null(g_qcap_d[1].qcap002) THEN
           #   CALL g_qcap_d.deleteElement(1)
           #   NEXT FIELD qcap002
           #END IF
           
           IF g_qcap_d.getLength() = 0 THEN
              NEXT FIELD qcap002
           END IF
 
