  <section id="aqci050.b_fill" ver="6" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aqci050_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   #先清空單身變數內容
   CALL g_qcap_d.clear()
   CALL g_qcap2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE qcap002,qcap003,qcap004,qcap005,qcap006,qcap007,qcap008,qcap009,qcap002, 
       qcap003,qcap004,qcap005,qcapownid,qcapowndp,qcapcrtid,qcapcrtdp,qcapcrtdt,qcapmodid,qcapmoddt, 
       t1.pmaal004 ,t2.oocql004 ,t3.oofa011 ,t4.ooefl003 ,t5.oofa011 ,t6.ooefl003 ,t7.oofa011 FROM qcap_t", 
          
               "",
               
                              " LEFT JOIN pmaal_t t1 ON t1.pmaalent='"||g_enterprise||"' AND t1.pmaal001=qcap002 AND t1.pmaal002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t2 ON t2.oocqlent='"||g_enterprise||"' AND t2.oocql001='221' AND t2.oocql002=qcap003 AND t2.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t3 ON t3.oofaent='"||g_enterprise||"' AND t3.oofa002='2' AND t3.oofa003=qcapownid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=qcapowndp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t5 ON t5.oofaent='"||g_enterprise||"' AND t5.oofa002='2' AND t5.oofa003=qcapcrtid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent='"||g_enterprise||"' AND t6.ooefl001=qcapcrtdp AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t7 ON t7.oofaent='"||g_enterprise||"' AND t7.oofa002='2' AND t7.oofa003=qcapmodid  ",
 
               " WHERE qcapent= ? AND qcapsite= ? AND qcap001=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("qcap_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF aqci050_fill_chk(1) THEN
      LET g_sql = g_sql, " ORDER BY qcap_t.qcap002,qcap_t.qcap003,qcap_t.qcap004,qcap_t.qcap005"
      
      #add-point:b_fill段fill_before
      {<point name="b_fill.fill_before"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE aqci050_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR aqci050_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise, g_site,g_qcap_m.qcap001
                                               
      FOREACH b_fill_cs INTO g_qcap_d[l_ac].qcap002,g_qcap_d[l_ac].qcap003,g_qcap_d[l_ac].qcap004,g_qcap_d[l_ac].qcap005, 
          g_qcap_d[l_ac].qcap006,g_qcap_d[l_ac].qcap007,g_qcap_d[l_ac].qcap008,g_qcap_d[l_ac].qcap009, 
          g_qcap2_d[l_ac].qcap002,g_qcap2_d[l_ac].qcap003,g_qcap2_d[l_ac].qcap004,g_qcap2_d[l_ac].qcap005, 
          g_qcap2_d[l_ac].qcapownid,g_qcap2_d[l_ac].qcapowndp,g_qcap2_d[l_ac].qcapcrtid,g_qcap2_d[l_ac].qcapcrtdp, 
          g_qcap2_d[l_ac].qcapcrtdt,g_qcap2_d[l_ac].qcapmodid,g_qcap2_d[l_ac].qcapmoddt,g_qcap_d[l_ac].qcap002_desc, 
          g_qcap_d[l_ac].qcap003_desc,g_qcap2_d[l_ac].qcapownid_desc,g_qcap2_d[l_ac].qcapowndp_desc, 
          g_qcap2_d[l_ac].qcapcrtid_desc,g_qcap2_d[l_ac].qcapcrtdp_desc,g_qcap2_d[l_ac].qcapmodid_desc 

                             
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #此段落由子樣板a12產生
      #LET g_qcap2_d[l_ac].qcapownid_desc = cl_get_username(g_qcap2_d[l_ac].qcapownid)
      #LET g_qcap2_d[l_ac].qcapowndp_desc = cl_get_deptname(g_qcap2_d[l_ac].qcapowndp)
      #LET g_qcap2_d[l_ac].qcapcrtid_desc = cl_get_username(g_qcap2_d[l_ac].qcapcrtid)
      #LET g_qcap2_d[l_ac].qcapcrtdp_desc = cl_get_deptname(g_qcap2_d[l_ac].qcapcrtdp)
      #LET g_qcap2_d[l_ac].qcapmodid_desc = cl_get_username(g_qcap2_d[l_ac].qcapmodid)
      
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            EXIT FOREACH
         END IF
         
      END FOREACH
      
            CALL g_qcap_d.deleteElement(g_qcap_d.getLength())
      CALL g_qcap2_d.deleteElement(g_qcap2_d.getLength())
 
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   FREE aqci050_pb   
   
END FUNCTION
]]>
</section>
