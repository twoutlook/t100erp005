  <section id="aqci050.before_delete" ver="2" status="" src="s">
<![CDATA[#+ 單身db資料刪除
PRIVATE FUNCTION aqci050_before_delete()
   #add-point:before_delete段define
   {<point name="before_delete.define"/>}
   #end add-point 
   
   #add-point:單筆刪除前
   {<point name="delete.body.b_single_delete" mark="Y"/>}
   #end add-point
   
   DELETE FROM qcap_t
    WHERE qcapent = g_enterprise AND qcapsite = g_site AND qcap001 = g_qcap_m.qcap001 AND
 
          qcap002 = g_qcap_d_t.qcap002
      AND qcap003 = g_qcap_d_t.qcap003
      AND qcap004 = g_qcap_d_t.qcap004
      AND qcap005 = g_qcap_d_t.qcap005
 
      
   #add-point:單筆刪除中
   {<point name="delete.body.m_single_delete"/>}
   #end add-point
   
   IF SQLCA.sqlcode THEN
