                                       AND qcaa003 = g_qcaa_d[l_ac].qcaa003
 
                                       AND qcaa004 = g_qcaa_d[l_ac].qcaa004
 
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
