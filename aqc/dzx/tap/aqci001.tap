  <section id="aqci001.b_fill" ver="1" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aqci001_b_fill(p_wc2)              #BODY FILL UP
   {<Local define>}
   DEFINE p_wc2           STRING
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE qcaastus,qcaa001,qcaa002,qcaa003,qcaa004,qcaa005,qcaa006,qcaa007,qcaa001, 
       qcaa002,qcaa003,qcaa004,qcaamodid,'',qcaamoddt,qcaaownid,'',qcaaowndp,'',qcaacrtid,'',qcaacrtdp, 
       '',qcaacrtdt FROM qcaa_t",
               "",
               " WHERE qcaaent= ? AND 1=1 AND ", p_wc2
    
   LET g_sql = g_sql, " ORDER BY qcaa_t.qcaa001,qcaa_t.qcaa002,qcaa_t.qcaa003,qcaa_t.qcaa004"
  
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
  
   PREPARE aqci001_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aqci001_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_qcaa_d.clear()
   CALL g_qcaa1_info_d.clear()   
 
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_qcaa_d[l_ac].qcaastus,g_qcaa_d[l_ac].qcaa001,g_qcaa_d[l_ac].qcaa002,g_qcaa_d[l_ac].qcaa003, 
       g_qcaa_d[l_ac].qcaa004,g_qcaa_d[l_ac].qcaa005,g_qcaa_d[l_ac].qcaa006,g_qcaa_d[l_ac].qcaa007,g_qcaa1_info_d[l_ac].qcaa001, 
       g_qcaa1_info_d[l_ac].qcaa002,g_qcaa1_info_d[l_ac].qcaa003,g_qcaa1_info_d[l_ac].qcaa004,g_qcaa1_info_d[l_ac].qcaamodid, 
       g_qcaa1_info_d[l_ac].qcaamodid_desc,g_qcaa1_info_d[l_ac].qcaamoddt,g_qcaa1_info_d[l_ac].qcaaownid, 
       g_qcaa1_info_d[l_ac].qcaaownid_desc,g_qcaa1_info_d[l_ac].qcaaowndp,g_qcaa1_info_d[l_ac].qcaaowndp_desc, 
       g_qcaa1_info_d[l_ac].qcaacrtid,g_qcaa1_info_d[l_ac].qcaacrtid_desc,g_qcaa1_info_d[l_ac].qcaacrtdp, 
       g_qcaa1_info_d[l_ac].qcaacrtdp_desc,g_qcaa1_info_d[l_ac].qcaacrtdt
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL aqci001_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec AND g_error_show = 1 THEN
      CALL cl_err("qcaa_t",9035,1)
   END IF 
   LET g_error_show = 0
   
 
  
   
   CALL g_qcaa_d.deleteElement(g_qcaa_d.getLength())   
   CALL g_qcaa1_info_d.deleteElement(g_qcaa1_info_d.getLength())
 
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_qcaa_d.getLength()
      LET g_qcaa1_info_d[l_ac].qcaa001 = g_qcaa_d[l_ac].qcaa001 
      LET g_qcaa1_info_d[l_ac].qcaa002 = g_qcaa_d[l_ac].qcaa002 
 
      LET g_qcaa1_info_d[l_ac].qcaa003 = g_qcaa_d[l_ac].qcaa003 
 
      LET g_qcaa1_info_d[l_ac].qcaa004 = g_qcaa_d[l_ac].qcaa004 
 
 
 
   END FOR
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = l_ac - 1
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE aqci001_pb
   
END FUNCTION
]]>
  </section>
  <section id="aqci001.default_search" ver="1" status="" src="s">
