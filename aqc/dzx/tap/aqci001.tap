  <point name="function.aqci001_chk_0" order="1" ver="2" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[# 傳入參數：p_type
# 判斷是否不小於零  p_type =0
# 判斷是否大於零     p_type =1
#
PRIVATE FUNCTION aqci001_chk_0(p_chk_num,p_type)
DEFINE p_type LIKE type_t.chr1
   DEFINE p_chk_num LIKE qcaa_t.qcaa001
   DEFINE r_success LIKE type_t.num5
   LET r_success = TRUE
   IF NOT cl_null(p_chk_num) THEN 
      IF p_type = '0' THEN
         IF p_chk_num < 0 THEN
            LET r_success = FALSE
            CALL cl_err(p_chk_num,'aqc-00004',0)
         END IF 
      END IF 
      IF p_type = '1' THEN
         IF p_chk_num <= 0 THEN
            LET r_success = FALSE
            CALL cl_err(p_chk_num,'aqc-00011',0)
         END IF 
      END IF 
   END IF 
   RETURN r_success
END FUNCTION]]>
  </point>
  <point name="function.aqci001_chk_qcaa001" order="2" ver="2" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[# 對qcaa001進行檢查
PRIVATE FUNCTION aqci001_chk_qcaa001(p_cmd)
DEFINE p_cmd LIKE type_t.chr1
   DEFINE r_success LIKE type_t.num5
   LET r_success = TRUE
   #檢查key值重複性
   IF NOT aqci001_key_chk(p_cmd) THEN
      LET r_success = FALSE
   END IF 
   #檢查是否小於0
   IF r_success = TRUE AND NOT aqci001_chk_0(g_qcaa_d[l_ac].qcaa001,'0') THEN
      LET r_success = FALSE
   END IF 
   #檢查qcaa001不可大於qcaa002
   IF r_success = TRUE THEN
      IF NOT cl_null(g_qcaa_d[l_ac].qcaa001) AND NOT cl_null(g_qcaa_d[l_ac].qcaa002) THEN
         IF g_qcaa_d[l_ac].qcaa001 > g_qcaa_d[l_ac].qcaa002 THEN
            LET r_success = FALSE
            CALL cl_err(g_qcaa_d[l_ac].qcaa001||">"||g_qcaa_d[l_ac].qcaa002,'aqc-00005',0)
         END IF 
      END IF 
   END IF 
   #檢查是否存在重疊塊
   IF r_success = TRUE THEN
      IF NOT aqci001_qcaa(g_qcaa_d[l_ac].qcaa001,g_qcaa_d_t.qcaa001,g_qcaa_d[l_ac].qcaa002,g_qcaa_d_t.qcaa002,g_qcaa_d[l_ac].qcaa003,g_qcaa_d_t.qcaa003) THEN 
       　CALL cl_err(g_qcaa_d[l_ac].qcaa001||"~"||g_qcaa_d[l_ac].qcaa002,'aqc-00007',0)
         LET r_success = FALSE
      END IF
   END IF 
   RETURN r_success
END FUNCTION]]>
  </point>
