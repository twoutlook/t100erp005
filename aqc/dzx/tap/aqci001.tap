            #add-point:BEFORE FIELD qcaastus
            {<point name="input.b.page1.qcaastus" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD qcaastus
            
            #add-point:AFTER FIELD qcaastus
            {<point name="input.a.page1.qcaastus" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE qcaastus
            #add-point:ON CHANGE qcaastus
            {<point name="input.g.page1.qcaastus" />}
            #END add-point
 
 
         #---------------------<  Detail: page1  >---------------------
         #----<<qcaastus>>----
         #Ctrlp:input.c.page1.qcaastus
         ON ACTION controlp INFIELD qcaastus
            #add-point:ON ACTION controlp INFIELD qcaastus
            {<point name="input.c.page1.qcaastus" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_qcaa_d[l_ac].* = g_qcaa_d_t.*
               CLOSE aqci001_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_qcaa_d[l_ac].qcaa001,-263,1)
               LET g_qcaa_d[l_ac].* = g_qcaa_d_t.*
            ELSE
               
               #寫入修改者/修改日期資訊(單身)
               LET g_qcaa1_info_d[l_ac].qcaamodid = g_user 
LET g_qcaa1_info_d[l_ac].qcaamoddt = cl_get_current()
 
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               UPDATE qcaa_t SET (qcaastus,qcaa001,qcaa002,qcaa003,qcaa004,qcaa005,qcaa006,qcaa007,qcaamodid, 
                   qcaamoddt,qcaaownid,qcaaowndp,qcaacrtid,qcaacrtdp,qcaacrtdt) = (g_qcaa_d[l_ac].qcaastus, 
                   g_qcaa_d[l_ac].qcaa001,g_qcaa_d[l_ac].qcaa002,g_qcaa_d[l_ac].qcaa003,g_qcaa_d[l_ac].qcaa004, 
                   g_qcaa_d[l_ac].qcaa005,g_qcaa_d[l_ac].qcaa006,g_qcaa_d[l_ac].qcaa007,g_qcaa1_info_d[l_ac].qcaamodid, 
                   g_qcaa1_info_d[l_ac].qcaamoddt,g_qcaa1_info_d[l_ac].qcaaownid,g_qcaa1_info_d[l_ac].qcaaowndp, 
                   g_qcaa1_info_d[l_ac].qcaacrtid,g_qcaa1_info_d[l_ac].qcaacrtdp,g_qcaa1_info_d[l_ac].qcaacrtdt) 

                WHERE qcaaent = g_enterprise AND
                  qcaa001 = g_qcaa_d_t.qcaa001 #項次   
                  AND qcaa002 = g_qcaa_d_t.qcaa002  
 
                  AND qcaa003 = g_qcaa_d_t.qcaa003  
 
                  AND qcaa004 = g_qcaa_d_t.qcaa004  
 
 
                  
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
                  
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("qcaa_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                    WHEN SQLCA.sqlcode #其他錯誤
                     CALL cl_err("qcaa_t",SQLCA.sqlcode,1)  
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_qcaa_d[g_detail_idx].qcaa001
               LET gs_keys_bak[1] = g_qcaa_d_t.qcaa001
               LET gs_keys[2] = g_qcaa_d[g_detail_idx].qcaa002
               LET gs_keys_bak[2] = g_qcaa_d_t.qcaa002
               LET gs_keys[3] = g_qcaa_d[g_detail_idx].qcaa003
               LET gs_keys_bak[3] = g_qcaa_d_t.qcaa003
               LET gs_keys[4] = g_qcaa_d[g_detail_idx].qcaa004
               LET gs_keys_bak[4] = g_qcaa_d_t.qcaa004
               CALL aqci001_update_b('qcaa_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
               END CASE
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            CALL aqci001_unlock_b("qcaa_t")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock
            
             #add-point:單身after row
            {<point name="input.body.a_row"/>}
            #end add-point
            
         AFTER INPUT
