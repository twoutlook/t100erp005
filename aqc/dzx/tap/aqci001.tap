  <section id="aqci001.modify" ver="1" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aqci001_modify()
   {<Local define>}
   DEFINE  l_cmd                  LIKE type_t.chr1
   DEFINE  l_ac_t                 LIKE type_t.num5                #未取消的ARRAY CNT 
   DEFINE  l_n                    LIKE type_t.num5                #檢查重複用  
   DEFINE  l_cnt                  LIKE type_t.num5                #檢查重複用  
   DEFINE  l_lock_sw              LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert         LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete         LIKE type_t.num5                #可刪除否  
   DEFINE  l_count                LIKE type_t.num5
   DEFINE  l_i                    LIKE type_t.num5
   DEFINE  ls_return              STRING
   DEFINE  l_var_keys             DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys           DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                 DYNAMIC ARRAY OF STRING
   DEFINE  l_fields               DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak         DYNAMIC ARRAY OF STRING
   DEFINE  li_reproduce           LIKE type_t.num5
   DEFINE  li_reproduce_target    LIKE type_t.num5
   DEFINE  lb_reproduce           BOOLEAN
   {</Local define>}
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point 
   
   LET g_action_choice = ""
   
   LET g_qryparam.state = "i"
 
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
 
   #add-point:modify段define_sql
   {<point name="modify.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT qcaastus,qcaa001,qcaa002,qcaa003,qcaa004,qcaa005,qcaa006,qcaa007,'','', 
       '','',qcaamodid,'',qcaamoddt,qcaaownid,'',qcaaowndp,'',qcaacrtid,'',qcaacrtdp,'',qcaacrtdt FROM  
       qcaa_t WHERE qcaaent=? AND qcaa001=? AND qcaa002=? AND qcaa003=? AND qcaa004=? FOR UPDATE"
   #add-point:modify段define_sql
   {<point name="modify.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   DECLARE aqci001_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR
   
 
   
   LET INT_FLAG = FALSE
   LET lb_reproduce = FALSE
 
   #add-point:modify段修改前
   {<point name="modify.before_input"/>}
   #end add-point
 
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #Page1 預設值產生於此處
      INPUT ARRAY g_qcaa_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_detail_cnt,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_qcaa_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL aqci001_b_fill(g_wc2)
            LET g_detail_cnt = g_qcaa_d.getLength()
         
         BEFORE ROW
            #add-point:modify段before row
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            DISPLAY g_qcaa_d.getLength() TO FORMONLY.cnt
         
            CALL s_transaction_begin()
            LET g_detail_cnt = g_qcaa_d.getLength()
            
            IF g_detail_cnt >= l_ac 
               AND g_qcaa_d[l_ac].qcaa001 IS NOT NULL
               AND g_qcaa_d[l_ac].qcaa002 IS NOT NULL
 
               AND g_qcaa_d[l_ac].qcaa003 IS NOT NULL
 
               AND g_qcaa_d[l_ac].qcaa004 IS NOT NULL
 
 
            THEN
               LET l_cmd='u'
               LET g_qcaa_d_t.* = g_qcaa_d[l_ac].*  #BACKUP
               IF NOT aqci001_lock_b("qcaa_t") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aqci001_bcl INTO g_qcaa_d[l_ac].qcaastus,g_qcaa_d[l_ac].qcaa001,g_qcaa_d[l_ac].qcaa002, 
                      g_qcaa_d[l_ac].qcaa003,g_qcaa_d[l_ac].qcaa004,g_qcaa_d[l_ac].qcaa005,g_qcaa_d[l_ac].qcaa006, 
                      g_qcaa_d[l_ac].qcaa007,g_qcaa1_info_d[l_ac].qcaa001,g_qcaa1_info_d[l_ac].qcaa002, 
                      g_qcaa1_info_d[l_ac].qcaa003,g_qcaa1_info_d[l_ac].qcaa004,g_qcaa1_info_d[l_ac].qcaamodid, 
                      g_qcaa1_info_d[l_ac].qcaamodid_desc,g_qcaa1_info_d[l_ac].qcaamoddt,g_qcaa1_info_d[l_ac].qcaaownid, 
                      g_qcaa1_info_d[l_ac].qcaaownid_desc,g_qcaa1_info_d[l_ac].qcaaowndp,g_qcaa1_info_d[l_ac].qcaaowndp_desc, 
                      g_qcaa1_info_d[l_ac].qcaacrtid,g_qcaa1_info_d[l_ac].qcaacrtid_desc,g_qcaa1_info_d[l_ac].qcaacrtdp, 
                      g_qcaa1_info_d[l_ac].qcaacrtdp_desc,g_qcaa1_info_d[l_ac].qcaacrtdt
                  IF SQLCA.sqlcode THEN
                     CALL cl_err(g_qcaa_d_t.qcaa001,SQLCA.sqlcode,1)
                     LET l_lock_sw = "Y"
                  END IF
                  
                  CALL aqci001_detail_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
        
         BEFORE INSERT
            
            CALL s_transaction_begin()
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_qcaa_d_t.* TO NULL
            INITIALIZE g_qcaa_d[l_ac].* TO NULL 
            
            LET g_qcaa_d_t.* = g_qcaa_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL aqci001_set_entry_b("a")
            CALL aqci001_set_no_entry_b("a")
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_qcaa_d[li_reproduce_target].* = g_qcaa_d[li_reproduce].*
               LET g_qcaa1_info_d[li_reproduce_target].* = g_qcaa1_info_d[li_reproduce].*
 
               LET g_qcaa_d[g_qcaa_d.getLength()].qcaa001 = NULL
               LET g_qcaa_d[g_qcaa_d.getLength()].qcaa002 = NULL
 
               LET g_qcaa_d[g_qcaa_d.getLength()].qcaa003 = NULL
 
               LET g_qcaa_d[g_qcaa_d.getLength()].qcaa004 = NULL
 
 
            END IF
            #公用欄位給值(單身)
            #此段落由子樣板a14產生    
      LET g_qcaa1_info_d[l_ac].qcaaownid = g_user
      LET g_qcaa1_info_d[l_ac].qcaaowndp = g_dept
      LET g_qcaa1_info_d[l_ac].qcaacrtid = g_user
      LET g_qcaa1_info_d[l_ac].qcaacrtdp = g_dept 
      LET g_qcaa1_info_d[l_ac].qcaacrtdt = cl_get_current()
