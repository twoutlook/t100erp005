  <point name="function.aqci004_qcad" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#同一級數下，起始批量和截止批量不可有重疊區域
PRIVATE FUNCTION aqci004_qcad(p_qcad001,p_qcad001_t,p_qcad003,p_qcad003_t,p_qcad004,p_qcad004_t)
   DEFINE p_cmd        LIKE type_t.chr1
   DEFINE p_qcad001    LIKE qcad_t.qcad001
   DEFINE p_qcad001_t  LIKE qcad_t.qcad001
   DEFINE p_qcad003    LIKE qcad_t.qcad003
   DEFINE p_qcad003_t  LIKE qcad_t.qcad003
   DEFINE p_qcad004    LIKE qcad_t.qcad004
   DEFINE p_qcad004_t  LIKE qcad_t.qcad004
   DEFINE l_qcad003    LIKE qcad_t.qcad003
   DEFINE l_qcad004    LIKE qcad_t.qcad004
   DEFINE l_cnt        LIKE type_t.num5
   DEFINE l_sql        STRING

   IF cl_null(p_qcad001) OR cl_null(p_qcad003) OR cl_null(p_qcad004) THEN
      RETURN TRUE
   END IF
   IF p_qcad003_t IS NULL OR
      ( NOT cl_null(p_qcad003) AND
      p_qcad003_t >p_qcad003) THEN
      # check所輸入之數量是否有區間重疊
      LET l_cnt = 0
      SELECT COUNT(*) INTO l_cnt FROM qcad_t
       WHERE ((qcad003 BETWEEN p_qcad003 AND p_qcad004) OR (qcad004 BETWEEN p_qcad003 AND p_qcad004))
         AND qcad001 = p_qcad001
         AND (qcad003 != p_qcad003_t OR qcad004 != p_qcad004_t)
      IF l_cnt > 0 THEN
         RETURN FALSE
      END IF
   END IF
   IF p_qcad004_t IS NULL OR
      ( NOT cl_null(p_qcad004) AND
      p_qcad004_t <p_qcad004) THEN
      # check所輸入之數量是否有區間重疊
      SELECT COUNT(*) INTO l_cnt FROM qcad_t
       WHERE ((qcad003 BETWEEN p_qcad003 AND p_qcad004) OR (qcad004 BETWEEN p_qcad003 AND p_qcad004))
         AND qcad001 = p_qcad001
         AND (qcad003 != p_qcad003_t OR qcad004 != p_qcad004_t)
      IF l_cnt > 0 THEN
         RETURN FALSE
      END IF
   END IF
   IF p_qcad001_t IS NULL OR
      ( NOT cl_null(p_qcad001) AND
      p_qcad001_t <> p_qcad001) THEN
      # check所輸入之數量是否有區間重疊
      SELECT COUNT(*) INTO l_cnt FROM qcad_t
       WHERE ((qcad004 BETWEEN p_qcad003 AND p_qcad004) OR (qcad003 BETWEEN p_qcad003 AND p_qcad004))
         AND qcad001 = p_qcad001
      IF l_cnt > 0 THEN
         RETURN FALSE
      END IF
   END IF
   RETURN TRUE
END FUNCTION]]>
  </point>
  <point name="function.aqci004_qcad003" order="2" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[# default qcad003
PRIVATE FUNCTION aqci004_qcad003()
DEFINE l_cnt  LIKE type_t.num5
   SELECT COUNT(*) INTO l_cnt FROM qcad_t WHERE qcad001 = g_qcad_d[l_ac].qcad001
   IF l_cnt=0 THEN
      LET g_qcad_d[l_ac].qcad003=2
   ELSE
      SELECT MAX(qcad004)+1 INTO g_qcad_d[l_ac].qcad003 FROM qcad_t WHERE qcad001 = g_qcad_d[l_ac].qcad001
   END IF
   IF cl_null(g_qcad_d[l_ac].qcad003) THEN
      LET g_qcad_d[l_ac].qcad003=2
   END IF
   DISPLAY BY NAME g_qcad_d[l_ac].qcad003
END FUNCTION]]>
