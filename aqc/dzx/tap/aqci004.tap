  <section id="aqci004.b_fill" ver="1" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aqci004_b_fill(p_wc2)              #BODY FILL UP
   {<Local define>}
   DEFINE p_wc2           STRING
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE qcadstus,qcad001,qcad002,qcad003,qcad004,'','','',qcadmodid,'',qcadmoddt, 
       qcadownid,'',qcadowndp,'',qcadcrtid,'',qcadcrtdp,'',qcadcrtdt FROM qcad_t",
               "",
               " WHERE qcadent= ? AND 1=1 AND ", p_wc2
    
   LET g_sql = g_sql, " ORDER BY qcad_t.qcad001,qcad_t.qcad002,qcad_t.qcad003"
  
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
  
   PREPARE aqci004_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aqci004_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_qcad_d.clear()
   CALL g_qcad2_d.clear()   
 
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_qcad_d[l_ac].qcadstus,g_qcad_d[l_ac].qcad001,g_qcad_d[l_ac].qcad002,g_qcad_d[l_ac].qcad003, 
       g_qcad_d[l_ac].qcad004,g_qcad2_d[l_ac].qcad001,g_qcad2_d[l_ac].qcad002,g_qcad2_d[l_ac].qcad003, 
       g_qcad2_d[l_ac].qcadmodid,g_qcad2_d[l_ac].qcadmodid_desc,g_qcad2_d[l_ac].qcadmoddt,g_qcad2_d[l_ac].qcadownid, 
       g_qcad2_d[l_ac].qcadownid_desc,g_qcad2_d[l_ac].qcadowndp,g_qcad2_d[l_ac].qcadowndp_desc,g_qcad2_d[l_ac].qcadcrtid, 
       g_qcad2_d[l_ac].qcadcrtid_desc,g_qcad2_d[l_ac].qcadcrtdp,g_qcad2_d[l_ac].qcadcrtdp_desc,g_qcad2_d[l_ac].qcadcrtdt 

      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL aqci004_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec AND g_error_show = 1 THEN
      CALL cl_err("qcad_t",9035,1)
   END IF 
   LET g_error_show = 0
   
 
  
   
   CALL g_qcad_d.deleteElement(g_qcad_d.getLength())   
   CALL g_qcad2_d.deleteElement(g_qcad2_d.getLength())
 
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_qcad_d.getLength()
      LET g_qcad2_d[l_ac].qcad001 = g_qcad_d[l_ac].qcad001 
      LET g_qcad2_d[l_ac].qcad002 = g_qcad_d[l_ac].qcad002 
 
      LET g_qcad2_d[l_ac].qcad003 = g_qcad_d[l_ac].qcad003 
 
 
 
   END FOR
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = l_ac - 1
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE aqci004_pb
   
END FUNCTION
]]>
  </section>
