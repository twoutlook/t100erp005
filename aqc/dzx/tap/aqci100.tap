  <section id="aqci100.delete" ver="1" status="" src="s">
<![CDATA[#+ 資料刪除 
PRIVATE FUNCTION aqci100_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point  
   
   IF g_qcaz_m.qcaz001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   CALL aqci100_show()
   
   CALL s_transaction_begin()
    
   LET g_qcaz001_t = g_qcaz_m.qcaz001
 
   
   
 
   OPEN aqci100_cl USING g_enterprise,
                           g_qcaz_m.qcaz001
 
   IF STATUS THEN
      CALL cl_err("OPEN aqci100_cl:", STATUS, 0)
      CLOSE aqci100_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   FETCH aqci100_cl INTO g_qcaz_m.qcaz001,g_qcaz_m.qcaz001_desc,g_qcaz_m.qcazstus,g_qcaz_m.qcaz002,g_qcaz_m.qcaz004, 
       g_qcaz_m.qcaz006,g_qcaz_m.qcaz003,g_qcaz_m.qcaz005,g_qcaz_m.qcaz007,g_qcaz_m.qcaz008,g_qcaz_m.qcaz009, 
       g_qcaz_m.qcaz010,g_qcaz_m.qcaz011,g_qcaz_m.qcaz012,g_qcaz_m.qcaz013,g_qcaz_m.qcaz014,g_qcaz_m.qcaz015, 
       g_qcaz_m.qcaz016,g_qcaz_m.qcazownid,g_qcaz_m.qcazownid_desc,g_qcaz_m.qcazowndp,g_qcaz_m.qcazowndp_desc, 
       g_qcaz_m.qcazcrtid,g_qcaz_m.qcazcrtid_desc,g_qcaz_m.qcazcrtdp,g_qcaz_m.qcazcrtdp_desc,g_qcaz_m.qcazcrtdt, 
       g_qcaz_m.qcazmodid,g_qcaz_m.qcazmodid_desc,g_qcaz_m.qcazmoddt
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_qcaz_m.qcaz001,SQLCA.sqlcode,0)
      RETURN
   END IF
   
   IF cl_ask_delete() THEN
      INITIALIZE g_doc.* TO NULL
      LET g_doc.column1 = "qcaz001"
      LET g_doc.value1 = g_qcaz_m.qcaz001
      CALL cl_doc_remove()
 
      #add-point:單頭刪除前
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point
 
      DELETE FROM qcaz_t 
       WHERE qcazent = g_enterprise AND qcaz001 = g_qcaz_m.qcaz001 
 
 
      #add-point:單頭刪除中
      {<point name="delete.head.m_delete"/>}
      #end add-point
         
      IF SQLCA.sqlcode THEN
         CALL cl_err("qcaz_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
  
      
      
      #add-point:單頭刪除後
      {<point name="delete.head.a_delete"/>}
      #end add-point
      
          
      
      CLEAR FORM
      CALL aqci100_ui_browser_refresh()
      IF g_browser_cnt > 0 THEN
         CALL aqci100_fetch("P")
      ELSE
         CALL aqci100_browser_fill(" 1=1 ","F")
      END IF
      
   END IF
 
   CLOSE aqci100_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_qcaz_m.qcaz001,"D")
 
END FUNCTION
]]>
</section>
