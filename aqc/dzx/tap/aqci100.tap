         AFTER INPUT
            IF INT_FLAG THEN
               EXIT DIALOG
            END IF
                
            CALL cl_showmsg()   #錯誤訊息統整顯示
 
            IF p_cmd <> "u" THEN
               LET l_count = 1  
 
               SELECT COUNT(*) INTO l_count FROM qcaz_t
                WHERE qcazent = g_enterprise AND qcaz001 = g_qcaz_m.qcaz001
 
               IF l_count = 0 THEN
               
                  #add-point:單頭新增前
                  {<point name="input.head.b_insert" mark="Y"/>}
                  #end add-point
               
                  INSERT INTO qcaz_t (qcazent,qcaz001,qcazstus,qcaz002,qcaz004,qcaz006,qcaz003,qcaz005, 
                      qcaz007,qcaz008,qcaz009,qcaz010,qcaz011,qcaz012,qcaz013,qcaz014,qcaz015,qcaz016, 
                      qcazownid,qcazowndp,qcazcrtid,qcazcrtdp,qcazcrtdt,qcazmodid,qcazmoddt)
                  VALUES (g_enterprise,g_qcaz_m.qcaz001,g_qcaz_m.qcazstus,g_qcaz_m.qcaz002,g_qcaz_m.qcaz004, 
                      g_qcaz_m.qcaz006,g_qcaz_m.qcaz003,g_qcaz_m.qcaz005,g_qcaz_m.qcaz007,g_qcaz_m.qcaz008, 
                      g_qcaz_m.qcaz009,g_qcaz_m.qcaz010,g_qcaz_m.qcaz011,g_qcaz_m.qcaz012,g_qcaz_m.qcaz013, 
                      g_qcaz_m.qcaz014,g_qcaz_m.qcaz015,g_qcaz_m.qcaz016,g_qcaz_m.qcazownid,g_qcaz_m.qcazowndp, 
                      g_qcaz_m.qcazcrtid,g_qcaz_m.qcazcrtdp,g_qcaz_m.qcazcrtdt,g_qcaz_m.qcazmodid,g_qcaz_m.qcazmoddt)  

                  
                  #add-point:單頭新增中
                  {<point name="input.head.m_insert"/>}
                  #end add-point
                  
                  IF SQLCA.sqlcode THEN
                     CALL cl_err("qcaz_t",SQLCA.sqlcode,1)  
                     CONTINUE DIALOG
                  END IF
                  
                  
                  
                  #資料多語言用-增/改
                  
                  
                  #add-point:單頭新增後
                  {<point name="input.head.a_insert"/>}
                  #end add-point
                  
                  CALL s_transaction_end('Y','0')
               ELSE
                  CALL cl_err( "g_qcaz_m.qcaz001", "std-00006", 0 )
                  CALL s_transaction_end('N','0')
               END IF 
            ELSE
               #add-point:單頭修改前
               {<point name="input.head.b_update" mark="Y"/>}
               #end add-point
               UPDATE qcaz_t SET (qcaz001,qcazstus,qcaz002,qcaz004,qcaz006,qcaz003,qcaz005,qcaz007,qcaz008, 
                   qcaz009,qcaz010,qcaz011,qcaz012,qcaz013,qcaz014,qcaz015,qcaz016,qcazownid,qcazowndp, 
                   qcazcrtid,qcazcrtdp,qcazcrtdt,qcazmodid,qcazmoddt) = (g_qcaz_m.qcaz001,g_qcaz_m.qcazstus, 
                   g_qcaz_m.qcaz002,g_qcaz_m.qcaz004,g_qcaz_m.qcaz006,g_qcaz_m.qcaz003,g_qcaz_m.qcaz005, 
                   g_qcaz_m.qcaz007,g_qcaz_m.qcaz008,g_qcaz_m.qcaz009,g_qcaz_m.qcaz010,g_qcaz_m.qcaz011, 
                   g_qcaz_m.qcaz012,g_qcaz_m.qcaz013,g_qcaz_m.qcaz014,g_qcaz_m.qcaz015,g_qcaz_m.qcaz016, 
                   g_qcaz_m.qcazownid,g_qcaz_m.qcazowndp,g_qcaz_m.qcazcrtid,g_qcaz_m.qcazcrtdp,g_qcaz_m.qcazcrtdt, 
                   g_qcaz_m.qcazmodid,g_qcaz_m.qcazmoddt)
                WHERE qcazent = g_enterprise AND qcaz001 = g_qcaz001_t #
 
               #add-point:單頭修改中
               {<point name="input.head.m_update"/>}
               #end add-point
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("qcaz_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                  WHEN SQLCA.sqlcode #其他錯誤
                     CALL cl_err("qcaz_t",SQLCA.sqlcode,1)  
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                     
                     #資料多語言用-增/改
                     
                     #add-point:單頭修改後
                     {<point name="input.head.a_update"/>}
                     #end add-point
                     CALL s_transaction_end('Y','0')
               END CASE
            END IF
           #controlp
      END INPUT
          
      #add-point:input段more input 
      {<point name="input.more_input"/>}
      #end add-point
          
      ON ACTION controlf
         CALL cl_set_focus_form(ui.Interface.getRootNode()) RETURNING g_fld_name,g_frm_name
         CALL cl_fldhelp(g_frm_name, g_fld_name, g_lang)
 
      ON ACTION controlr
         CALL cl_show_req_fields()
 
      ON ACTION controls
         CALL cl_set_head_visible("","AUTO")
 
      ON ACTION accept
         ACCEPT DIALOG
        
      ON ACTION cancel      #在dialog button (放棄)
         LET g_action_choice=""
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION close       #在dialog 右上角 (X)
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION exit        #toolbar 離開
         LET INT_FLAG = TRUE 
         EXIT DIALOG
   
      #交談指令共用ACTION
      &include "common_action.4gl" 
         CONTINUE DIALOG 
   END DIALOG
    
   #add-point:input段after input 
   {<point name="input.after_input"/>}
   #end add-point    
 
END FUNCTION
]]>
</section>
