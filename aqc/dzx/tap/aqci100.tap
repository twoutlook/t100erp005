  <section id="aqci100.main" ver="1" status="" src="s">
<![CDATA[#+ 此段落由子樣板a26產生
#OPTIONS SHORT CIRCUIT
#+ 作業開始 
MAIN
   #add-point:main段define
   {<point name="main.define"/>}
   #end add-point   
 
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("aqc","")
 
   #add-point:作業初始化
   {<point name="main.init" />}
   #end add-point
   
   
 
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = "SELECT qcaz001,'',qcazstus,qcaz002,qcaz004,qcaz006,qcaz003,qcaz005,qcaz007,qcaz008, 
       qcaz009,qcaz010,qcaz011,qcaz012,qcaz013,qcaz014,qcaz015,qcaz016,qcazownid,'',qcazowndp,'',qcazcrtid, 
       '',qcazcrtdp,'',qcazcrtdt,qcazmodid,'',qcazmoddt FROM qcaz_t WHERE qcazent= ? AND qcaz001=? FOR  
       UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   DECLARE aqci100_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   IF g_bgjob = "Y" THEN
 
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
 
   ELSE
      
      #畫面開啟 (identifier)
      OPEN WINDOW w_aqci100 WITH FORM cl_ap_formpath("aqc",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL aqci100_init()   
 
      #進入選單 Menu (="N")
      CALL aqci100_ui_dialog() 
	  
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_aqci100
      
   END IF 
   
   CLOSE aqci100_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
   
END MAIN
 
 
]]>
</section>
  <section id="aqci100.modify" ver="1" status="" src="s">
<![CDATA[#+ 資料修改
PRIVATE FUNCTION aqci100_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point
   
   IF g_qcaz_m.qcaz001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF 
   
    SELECT UNIQUE qcaz001,qcazstus,qcaz002,qcaz004,qcaz006,qcaz003,qcaz005,qcaz007,qcaz008,qcaz009,qcaz010, 
        qcaz011,qcaz012,qcaz013,qcaz014,qcaz015,qcaz016,qcazownid,qcazowndp,qcazcrtid,qcazcrtdp,qcazcrtdt, 
        qcazmodid,qcazmoddt
 INTO g_qcaz_m.qcaz001,g_qcaz_m.qcazstus,g_qcaz_m.qcaz002,g_qcaz_m.qcaz004,g_qcaz_m.qcaz006,g_qcaz_m.qcaz003, 
     g_qcaz_m.qcaz005,g_qcaz_m.qcaz007,g_qcaz_m.qcaz008,g_qcaz_m.qcaz009,g_qcaz_m.qcaz010,g_qcaz_m.qcaz011, 
     g_qcaz_m.qcaz012,g_qcaz_m.qcaz013,g_qcaz_m.qcaz014,g_qcaz_m.qcaz015,g_qcaz_m.qcaz016,g_qcaz_m.qcazownid, 
     g_qcaz_m.qcazowndp,g_qcaz_m.qcazcrtid,g_qcaz_m.qcazcrtdp,g_qcaz_m.qcazcrtdt,g_qcaz_m.qcazmodid, 
     g_qcaz_m.qcazmoddt
 FROM qcaz_t
 WHERE qcazent = g_enterprise AND qcaz001 = g_qcaz_m.qcaz001
 
   ERROR ""
  
   LET g_qcaz001_t = g_qcaz_m.qcaz001
 
   
   CALL s_transaction_begin()
   
   OPEN aqci100_cl USING g_enterprise,g_qcaz_m.qcaz001
   IF STATUS THEN
      CALL cl_err("OPEN aqci100_cl:", STATUS, 1)
      CLOSE aqci100_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH aqci100_cl INTO g_qcaz_m.qcaz001,g_qcaz_m.qcaz001_desc,g_qcaz_m.qcazstus,g_qcaz_m.qcaz002,g_qcaz_m.qcaz004, 
       g_qcaz_m.qcaz006,g_qcaz_m.qcaz003,g_qcaz_m.qcaz005,g_qcaz_m.qcaz007,g_qcaz_m.qcaz008,g_qcaz_m.qcaz009, 
       g_qcaz_m.qcaz010,g_qcaz_m.qcaz011,g_qcaz_m.qcaz012,g_qcaz_m.qcaz013,g_qcaz_m.qcaz014,g_qcaz_m.qcaz015, 
       g_qcaz_m.qcaz016,g_qcaz_m.qcazownid,g_qcaz_m.qcazownid_desc,g_qcaz_m.qcazowndp,g_qcaz_m.qcazowndp_desc, 
       g_qcaz_m.qcazcrtid,g_qcaz_m.qcazcrtid_desc,g_qcaz_m.qcazcrtdp,g_qcaz_m.qcazcrtdp_desc,g_qcaz_m.qcazcrtdt, 
       g_qcaz_m.qcazmodid,g_qcaz_m.qcazmodid_desc,g_qcaz_m.qcazmoddt
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err("qcaz_t",SQLCA.sqlcode,0)
      CLOSE aqci100_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
 
   CALL aqci100_show()
   
   WHILE TRUE
      LET g_qcaz_m.qcaz001 = g_qcaz001_t
 
      
      #寫入修改者/修改日期資訊
      LET g_qcaz_m.qcazmodid = g_user 
LET g_qcaz_m.qcazmoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
 
      CALL aqci100_input("u")     #欄位更改
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_qcaz_m.* = g_qcaz_m_t.*
         CALL aqci100_show()
         CALL cl_err("",9001,0)
         EXIT WHILE
      END IF
 
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_qcaz_m.qcaz001,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE aqci100_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_qcaz_m.qcaz001,"U")
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
]]>
</section>
