   {<point name="b_fill.sql_before"/>}
   #end add-point

   LET g_sql = "SELECT  UNIQUE qcbdsite,qcbddocno,qcbdseq,qcbd001,'',qcbd002,qcbd009,qcbd014,qcbd013,qcbd015,'','','','' FROM qcbd_t",
               "",
               " WHERE qcbdent= ? AND 1=1 AND ", p_wc2

   LET g_sql = g_sql, " ORDER BY qcbd_t.qcbdsite,qcbd_t.qcbddocno,qcbd_t.qcbdseq"

   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point

   PREPARE aqct300_01_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aqct300_01_pb

   OPEN b_fill_curs USING g_enterprise, g_site

   CALL g_qcbd_d.clear()


   LET g_cnt = l_ac
   LET l_ac = 1
   ERROR "Searching!"

   FOREACH b_fill_curs INTO g_qcbd_d[l_ac].qcbdsite,g_qcbd_d[l_ac].qcbddocno,g_qcbd_d[l_ac].qcbdseq,g_qcbd_d[l_ac].qcbd001,g_qcbd_d[l_ac].qcbd001_desc,g_qcbd_d[l_ac].qcbd002,g_qcbd_d[l_ac].qcbd009,g_qcbd_d[l_ac].qcbd014,g_qcbd_d[l_ac].qcbd013,g_qcbd_d[l_ac].qcbd015,g_qcbd_d[l_ac].qcbg002_1,g_qcbd_d[l_ac].qcbg002_2,g_qcbd_d[l_ac].qcbg002_3,g_qcbd_d[l_ac].qcbg002_4
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF

      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point

      CALL aqct300_01_detail_show()

      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF

   END FOREACH

   IF l_ac > g_max_rec AND g_error_show = 1 THEN
      CALL cl_err("qcbd_t",9035,1)
   END IF
   LET g_error_show = 0




   CALL g_qcbd_d.deleteElement(g_qcbd_d.getLength())


   #將key欄位填到每個page
   FOR l_ac = 1 TO g_qcbd_d.getLength()

   END FOR

   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point

   LET g_detail_cnt = l_ac - 1
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   LET l_ac = g_cnt
   LET g_cnt = 0

   CLOSE b_fill_curs
   FREE aqct300_01_pb

END FUNCTION]]>
