  <point name="function.aqci040_form_only_fill" cite_std="N" status="u" ver="1" src="s" new="Y" order="3" mark_hard="N">
<![CDATA[
PRIVATE FUNCTION aqci040_form_only_fill(p_imae001)
DEFINE p_imae001   LIKE imae_t.imae001
   SELECT imaal003,imaal004,imaa004,imaf013,imaa009,rtaxl003,imaa006,oocal003 
     INTO g_imae_d[l_ac].imaal003,g_imae_d[l_ac].imaal004,g_imae_d[l_ac].imaa004,g_imae_d[l_ac].imaf013,
          g_imae_d[l_ac].imaa009,g_imae_d[l_ac].rtaxl003,g_imae_d[l_ac].imaa006,g_imae_d[l_ac].imaa006_desc
     FROM imaa_t LEFT OUTER JOIN imaal_t ON imaal001 = imaa001 AND imaalent = imaaent AND imaal002 = g_dlang
                 LEFT OUTER JOIN imaf_t  ON imaf001 = imaa001 AND imafent = imaaent AND imafsite = g_site
                 LEFT OUTER JOIN rtaxl_t ON rtaxl001 = imaa009 AND rtaxlent = imaaent AND rtaxl002 = g_dlang
                 LEFT OUTER JOIN oocal_t ON oocal001 = imaa006 AND oocalent = imaalent AND oocal002 = g_dlang
    WHERE imaaent = g_enterprise AND imaa001 = p_imae001
                 
END FUNCTION]]>
</point>
