  <point name="function.aqci040_imae117_scc" cite_std="N" status="" ver="1" src="s" new="Y" order="2" mark_hard="N">
<![CDATA[
PRIVATE FUNCTION aqci040_imae117_scc()
   CASE WHEN g_imae_d[l_ac].imae116 = '1'
           CALL cl_set_combo_scc_part('imae117','5053','1,2,3')
           
        WHEN g_imae_d[l_ac].imae116 = '2'
           CALL cl_set_combo_scc('imae117','5053')
           
        WHEN g_imae_d[l_ac].imae116 MATCHES '[34]'
           CALL cl_set_combo_scc_part('imae117','5053','1,2,3,4,5,6,7')
   END CASE
END FUNCTION]]>
