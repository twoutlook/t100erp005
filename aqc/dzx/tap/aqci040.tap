   #end add-point 
   LET g_forupd_sql = "SELECT imae001,imae111,imae112,imae113,imae120,imae114,imae115,imae116,imae117, 
       imae001,imaeownid,imaeowndp,imaecrtid,imaecrtdp,imaecrtdt,imaemodid,imaemoddt FROM imae_t WHERE  
       imaeent=? AND imaesite=? AND imae001=? FOR UPDATE"
   #add-point:main段define_sql
   {<point name="main.body.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
