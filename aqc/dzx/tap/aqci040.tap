  <section id="aqci040.b_fill" ver="7" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aqci040_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2    STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE imae001,imae111,imae112,imae113,imae120,imae114,imae115,imae116,imae117, 
       imae001,imaeownid,imaeowndp,imaecrtid,imaecrtdp,imaecrtdt,imaemodid,imaemoddt ,t1.oocql004 ,t2.oofa011 , 
       t4.oocal003 ,t5.oofa011 ,t6.ooefl003 ,t7.oofa011 ,t8.ooefl003 ,t9.oofa011 FROM imae_t",
               "",
                              " LEFT JOIN oocql_t t1 ON t1.oocqlent='"||g_enterprise||"' AND t1.oocql001='205' AND t1.oocql002=imae111 AND t1.oocql003='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t2 ON t2.oofaent='"||g_enterprise||"' AND t2.oofa002='2' AND t2.oofa003=imae112  ",
               " LEFT JOIN oocal_t t4 ON t4.oocalent='"||g_enterprise||"' AND t4.oocal001=imae113 AND t4.oocal002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t5 ON t5.oofaent='"||g_enterprise||"' AND t5.oofa002='2' AND t5.oofa003=imaeownid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent='"||g_enterprise||"' AND t6.ooefl001=imaeowndp AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t7 ON t7.oofaent='"||g_enterprise||"' AND t7.oofa002='2' AND t7.oofa003=imaecrtid  ",
               " LEFT JOIN ooefl_t t8 ON t8.ooeflent='"||g_enterprise||"' AND t8.ooefl001=imaecrtdp AND t8.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oofa_t t9 ON t9.oofaent='"||g_enterprise||"' AND t9.oofa002='2' AND t9.oofa003=imaemodid  ",
 
               " WHERE imaeent= ? AND imaesite= ? AND 1=1 AND ", p_wc2 
    
   LET g_sql = g_sql, cl_sql_add_filter("imae_t"),
                      " ORDER BY imae_t.imae001"
   
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE aqci040_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aqci040_pb
   
   OPEN b_fill_curs USING g_enterprise, g_site
 
   CALL g_imae_d.clear()
   CALL g_imae2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_imae_d[l_ac].imae001,g_imae_d[l_ac].imae111,g_imae_d[l_ac].imae112,g_imae_d[l_ac].imae113, 
       g_imae_d[l_ac].imae120,g_imae_d[l_ac].imae114,g_imae_d[l_ac].imae115,g_imae_d[l_ac].imae116,g_imae_d[l_ac].imae117, 
       g_imae2_d[l_ac].imae001,g_imae2_d[l_ac].imaeownid,g_imae2_d[l_ac].imaeowndp,g_imae2_d[l_ac].imaecrtid, 
       g_imae2_d[l_ac].imaecrtdp,g_imae2_d[l_ac].imaecrtdt,g_imae2_d[l_ac].imaemodid,g_imae2_d[l_ac].imaemoddt, 
       g_imae_d[l_ac].imae111_desc,g_imae_d[l_ac].imae112_desc,g_imae_d[l_ac].imae113_desc,g_imae2_d[l_ac].imaeownid_desc, 
       g_imae2_d[l_ac].imaeowndp_desc,g_imae2_d[l_ac].imaecrtid_desc,g_imae2_d[l_ac].imaecrtdp_desc, 
       g_imae2_d[l_ac].imaemodid_desc
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL aqci040_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec THEN
      IF g_error_show = 1 THEN
         CALL cl_err("imae_t",9035,1)
      END IF
   END IF 
   LET g_error_show = 0
   
 
   
   CALL g_imae_d.deleteElement(g_imae_d.getLength())   
   CALL g_imae2_d.deleteElement(g_imae2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_imae_d.getLength()
      LET g_imae2_d[l_ac].imae001 = g_imae_d[l_ac].imae001 
 
   END FOR
   
   IF g_cnt > g_imae_d.getLength() THEN
      LET l_ac = g_imae_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_imae_d.getLength()
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE aqci040_pb
   
END FUNCTION
]]>
</section>
