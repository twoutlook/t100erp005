  <section id="aqci040.global" ver="5" status="" src="s">
<![CDATA[{<point name="global.memo" />}
 
IMPORT os
IMPORT util
#add-point:增加匯入項目
{<point name="global.import" />}
#end add-point
 
SCHEMA ds
 
GLOBALS "../../cfg/top_global.inc"
 
#add-point:增加匯入變數檔
{<point name="global.inc" />}
#end add-point
 
#單身 type 宣告
PRIVATE TYPE type_g_imae_d RECORD
       imae001 LIKE imae_t.imae001, 
   imaal003 LIKE type_t.chr80, 
   imaal004 LIKE type_t.chr80, 
   imaa004 LIKE type_t.chr80, 
   imaf013 LIKE type_t.chr80, 
   imaa009 LIKE type_t.chr80, 
   rtaxl003 LIKE type_t.chr80, 
   imae111 LIKE imae_t.imae111, 
   imae111_desc LIKE type_t.chr500, 
   imae112 LIKE imae_t.imae112, 
   imae112_desc LIKE type_t.chr500, 
   imaa006 LIKE type_t.chr80, 
   imaa006_desc LIKE type_t.chr500, 
   imae113 LIKE imae_t.imae113, 
   imae113_desc LIKE type_t.chr500, 
   imae120 LIKE imae_t.imae120, 
   imae114 LIKE imae_t.imae114, 
   imae115 LIKE imae_t.imae115, 
   imae116 LIKE imae_t.imae116, 
   imae117 LIKE imae_t.imae117
       END RECORD
PRIVATE TYPE type_g_imae2_d RECORD
       imae001 LIKE imae_t.imae001, 
   imaeownid LIKE imae_t.imaeownid, 
   imaeownid_desc LIKE type_t.chr500, 
   imaeowndp LIKE imae_t.imaeowndp, 
   imaeowndp_desc LIKE type_t.chr500, 
   imaecrtid LIKE imae_t.imaecrtid, 
   imaecrtid_desc LIKE type_t.chr500, 
   imaecrtdp LIKE imae_t.imaecrtdp, 
   imaecrtdp_desc LIKE type_t.chr500, 
   imaecrtdt DATETIME YEAR TO SECOND, 
   imaemodid LIKE imae_t.imaemodid, 
   imaemodid_desc LIKE type_t.chr500, 
   imaemoddt DATETIME YEAR TO SECOND
       END RECORD
 
 
 
#模組變數(Module Variables)
DEFINE g_imae_d          DYNAMIC ARRAY OF type_g_imae_d
DEFINE g_imae_d_t        type_g_imae_d
DEFINE g_imae2_d   DYNAMIC ARRAY OF type_g_imae2_d
DEFINE g_imae2_d_t type_g_imae2_d
 
      
DEFINE g_wc2                STRING
DEFINE g_sql                STRING
