  <section id="aqci002.b_fill" ver="1" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION aqci002_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2           STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE qcabstus,qcab001,qcab002,qcab003,qcab004,qcab005,qcab006,qcab007,qcab001, 
       qcab002,qcab003,qcab004,qcabmodid,'',qcabmoddt,qcabownid,'',qcabowndp,'',qcabcrtid,'',qcabcrtdp, 
       '',qcabcrtdt FROM qcab_t",
               "",
               " WHERE qcabent= ? AND 1=1 AND ", p_wc2
    
   LET g_sql = g_sql, " ORDER BY qcab_t.qcab001,qcab_t.qcab002,qcab_t.qcab003,qcab_t.qcab004"
  
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
  
   PREPARE aqci002_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR aqci002_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_qcab_d.clear()
   CALL g_qcab1_info_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_qcab_d[l_ac].qcabstus,g_qcab_d[l_ac].qcab001,g_qcab_d[l_ac].qcab002,g_qcab_d[l_ac].qcab003, 
       g_qcab_d[l_ac].qcab004,g_qcab_d[l_ac].qcab005,g_qcab_d[l_ac].qcab006,g_qcab_d[l_ac].qcab007,g_qcab1_info_d[l_ac].qcab001, 
       g_qcab1_info_d[l_ac].qcab002,g_qcab1_info_d[l_ac].qcab003,g_qcab1_info_d[l_ac].qcab004,g_qcab1_info_d[l_ac].qcabmodid, 
       g_qcab1_info_d[l_ac].qcabmodid_desc,g_qcab1_info_d[l_ac].qcabmoddt,g_qcab1_info_d[l_ac].qcabownid, 
       g_qcab1_info_d[l_ac].qcabownid_desc,g_qcab1_info_d[l_ac].qcabowndp,g_qcab1_info_d[l_ac].qcabowndp_desc, 
       g_qcab1_info_d[l_ac].qcabcrtid,g_qcab1_info_d[l_ac].qcabcrtid_desc,g_qcab1_info_d[l_ac].qcabcrtdp, 
       g_qcab1_info_d[l_ac].qcabcrtdp_desc,g_qcab1_info_d[l_ac].qcabcrtdt
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL aqci002_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec AND g_error_show = 1 THEN
      CALL cl_err("qcab_t",9035,1)
   END IF 
   LET g_error_show = 0
   
 
  
   
   CALL g_qcab_d.deleteElement(g_qcab_d.getLength())   
   CALL g_qcab1_info_d.deleteElement(g_qcab1_info_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_qcab_d.getLength()
      LET g_qcab1_info_d[l_ac].qcab001 = g_qcab_d[l_ac].qcab001 
      LET g_qcab1_info_d[l_ac].qcab002 = g_qcab_d[l_ac].qcab002 
      LET g_qcab1_info_d[l_ac].qcab003 = g_qcab_d[l_ac].qcab003 
      LET g_qcab1_info_d[l_ac].qcab004 = g_qcab_d[l_ac].qcab004 
 
   END FOR
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = l_ac - 1
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE aqci002_pb
   
END FUNCTION
]]>
  </section>
