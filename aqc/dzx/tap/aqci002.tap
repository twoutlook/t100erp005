         ##   CALL q_common('','qcabpstid',TRUE,FALSE,.qcabpstid) RETURNING ls_return
         ##   DISPLAY ls_return TO qcabpstid
         ##   NEXT FIELD qcabpstid
         
         ##----<<qcabcrtdt>>----
         AFTER FIELD qcabcrtdt
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
         
         #----<<qcabmoddt>>----
         AFTER FIELD qcabmoddt
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
         
         #----<<qcabcnfdt>>----
         #AFTER FIELD qcabcnfdt
         #   CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
         #   IF NOT cl_null(ls_result) THEN
         #      IF NOT cl_chk_date_symbol(ls_result) THEN
         #         LET ls_result = cl_add_date_extra_cond(ls_result)
         #      END IF
         #   END IF
         #   CALL FGL_DIALOG_SETBUFFER(ls_result)
         
         #----<<qcabpstdt>>----
         #AFTER FIELD qcabpstdt
         #   CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
         #   IF NOT cl_null(ls_result) THEN
         #      IF NOT cl_chk_date_symbol(ls_result) THEN
         #         LET ls_result = cl_add_date_extra_cond(ls_result)
         #      END IF
         #   END IF
         #   CALL FGL_DIALOG_SETBUFFER(ls_result)
 
 
      
         #---------------------<  Detail: page1  >---------------------
  
         
         #---------------------<  Detail: page2  >---------------------
         #----<<qcabmoddt>>----
         #此段落由子樣板a01產生
         BEFORE FIELD qcabmoddt
            #add-point:BEFORE FIELD qcabmoddt
            {<point name="query.b.page1_info.qcabmoddt" />}
            #END add-point
 
         #----<<qcabcrtid>>----
         #Ctrlp:construct.c.page1_info.qcabcrtid
         ON ACTION controlp INFIELD qcabcrtid
            #add-point:ON ACTION controlp INFIELD qcabcrtid
            {<point name="construct.c.page1_info.qcabcrtid" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD qcabcrtid
            #add-point:BEFORE FIELD qcabcrtid
