  <point name="function.aqci002_qcab" order="1" ver="2" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[# # 同一級數下，起始批量和截止批量不可有重疊區域
PRIVATE FUNCTION aqci002_qcab(p_qcab001,p_qcab001_t,p_qcab002,p_qcab002_t,p_qcab003,p_qcab003_t)
  DEFINE p_cmd        LIKE type_t.chr1
   DEFINE p_qcab001    LIKE qcab_t.qcab001 
   DEFINE p_qcab001_t  LIKE qcab_t.qcab001
   DEFINE p_qcab002    LIKE qcab_t.qcab002
   DEFINE p_qcab002_t  LIKE qcab_t.qcab002
   DEFINE p_qcab003    LIKE qcab_t.qcab003
   DEFINE p_qcab003_t  LIKE qcab_t.qcab003
   DEFINE l_qcab001    LIKE qcab_t.qcab001
   DEFINE l_qcab002    LIKE qcab_t.qcab002
   DEFINE l_cnt        LIKE type_t.num5
   DEFINE l_sql        STRING

   IF cl_null(p_qcab001) OR cl_null(p_qcab002) OR cl_null(p_qcab003) THEN
      RETURN TRUE
   END IF
   IF p_qcab001_t IS NULL OR
      ( NOT cl_null(p_qcab001) AND
      p_qcab001_t >p_qcab001) THEN
      # check所輸入之數量是否有區間重疊
      LET l_cnt = 0
      SELECT COUNT(*) INTO l_cnt FROM qcab_t
       WHERE ((qcab001 BETWEEN p_qcab001 AND p_qcab002) OR (qcab002 BETWEEN p_qcab001 AND p_qcab002))
         AND qcab003 = p_qcab003
         AND (qcab001 != p_qcab001_t OR qcab002 != p_qcab002_t)
      IF l_cnt > 0 THEN
         RETURN FALSE
      END IF
   END IF
   IF p_qcab002_t IS NULL OR
      ( NOT cl_null(p_qcab002) AND
      p_qcab002_t <p_qcab002) THEN
      # check所輸入之數量是否有區間重疊
      SELECT COUNT(*) INTO l_cnt FROM qcab_t
       WHERE ((qcab001 BETWEEN p_qcab001 AND p_qcab002) OR (qcab002 BETWEEN p_qcab001 AND p_qcab002))
         AND qcab003 = p_qcab003
         AND (qcab001 != p_qcab001_t OR qcab002 != p_qcab002_t)
      IF l_cnt > 0 THEN
         RETURN FALSE
      END IF
   END IF
   
   IF p_qcab003_t IS NULL OR
      ( NOT cl_null(p_qcab003) AND
      p_qcab003_t <> p_qcab003) THEN
      # check所輸入之數量是否有區間重疊
      SELECT COUNT(*) INTO l_cnt FROM qcab_t
       WHERE ((qcab002 BETWEEN p_qcab001 AND p_qcab002) OR (qcab001 BETWEEN p_qcab001 AND p_qcab002)) 
         AND qcab003 = p_qcab003
        #AND (qcab001 = p_qcab001 AND qcab002=p_qcab002 AND qcab003=p_qcab003)
      IF l_cnt > 0 THEN
         RETURN FALSE
      END IF
   END IF
   RETURN TRUE
END FUNCTION]]>
