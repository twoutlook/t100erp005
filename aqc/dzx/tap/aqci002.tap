  <section id="aqci002.modify" ver="1" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION aqci002_modify()
   DEFINE  l_cmd                  LIKE type_t.chr1
   DEFINE  l_ac_t                 LIKE type_t.num5                #未取消的ARRAY CNT 
   DEFINE  l_n                    LIKE type_t.num5                #檢查重複用  
   DEFINE  l_cnt                  LIKE type_t.num5                #檢查重複用  
   DEFINE  l_lock_sw              LIKE type_t.chr1                #單身鎖住否  
   DEFINE  l_allow_insert         LIKE type_t.num5                #可新增否 
   DEFINE  l_allow_delete         LIKE type_t.num5                #可刪除否  
   DEFINE  l_count                LIKE type_t.num5
   DEFINE  l_i                    LIKE type_t.num5
   DEFINE  ls_return              STRING
   DEFINE  l_var_keys             DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys           DYNAMIC ARRAY OF STRING
   DEFINE  l_vars                 DYNAMIC ARRAY OF STRING
   DEFINE  l_fields               DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak         DYNAMIC ARRAY OF STRING
   DEFINE  li_reproduce           LIKE type_t.num5
   DEFINE  li_reproduce_target    LIKE type_t.num5
   DEFINE  lb_reproduce           BOOLEAN
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point 
   
   LET g_action_choice = ""
   
   LET g_qryparam.state = "i"
 
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
 
   #add-point:modify段define_sql
   {<point name="modify.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT qcabstus,qcab001,qcab002,qcab003,qcab004,qcab005,qcab006,qcab007,'','', 
       '','',qcabmodid,'',qcabmoddt,qcabownid,'',qcabowndp,'',qcabcrtid,'',qcabcrtdp,'',qcabcrtdt FROM  
       qcab_t WHERE qcabent=? AND qcab001=? AND qcab002=? AND qcab003=? AND qcab004=? FOR UPDATE"
   #add-point:modify段define_sql
   {<point name="modify.after_define_sql"/>}
   #end add-point 
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)
   DECLARE aqci002_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR
   
 
   
   LET INT_FLAG = FALSE
   LET lb_reproduce = FALSE
 
   #add-point:modify段修改前
   {<point name="modify.before_input"/>}
   #end add-point
 
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #Page1 預設值產生於此處
      INPUT ARRAY g_qcab_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_detail_cnt,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = , 
                  DELETE ROW = ,
                  APPEND ROW = )
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_qcab_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL aqci002_b_fill(g_wc2)
            LET g_detail_cnt = g_qcab_d.getLength()
         
         BEFORE ROW
            #add-point:modify段before row
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            DISPLAY g_qcab_d.getLength() TO FORMONLY.cnt
         
            CALL s_transaction_begin()
            LET g_detail_cnt = g_qcab_d.getLength()
            
            IF g_detail_cnt >= l_ac 
               AND g_qcab_d[l_ac].qcab001 IS NOT NULL
               AND g_qcab_d[l_ac].qcab002 IS NOT NULL
               AND g_qcab_d[l_ac].qcab003 IS NOT NULL
               AND g_qcab_d[l_ac].qcab004 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_qcab_d_t.* = g_qcab_d[l_ac].*  #BACKUP
               IF NOT aqci002_lock_b("qcab_t") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH aqci002_bcl INTO g_qcab_d[l_ac].qcabstus,g_qcab_d[l_ac].qcab001,g_qcab_d[l_ac].qcab002, 
                      g_qcab_d[l_ac].qcab003,g_qcab_d[l_ac].qcab004,g_qcab_d[l_ac].qcab005,g_qcab_d[l_ac].qcab006, 
                      g_qcab_d[l_ac].qcab007,g_qcab1_info_d[l_ac].qcab001,g_qcab1_info_d[l_ac].qcab002, 
                      g_qcab1_info_d[l_ac].qcab003,g_qcab1_info_d[l_ac].qcab004,g_qcab1_info_d[l_ac].qcabmodid, 
                      g_qcab1_info_d[l_ac].qcabmodid_desc,g_qcab1_info_d[l_ac].qcabmoddt,g_qcab1_info_d[l_ac].qcabownid, 
                      g_qcab1_info_d[l_ac].qcabownid_desc,g_qcab1_info_d[l_ac].qcabowndp,g_qcab1_info_d[l_ac].qcabowndp_desc, 
                      g_qcab1_info_d[l_ac].qcabcrtid,g_qcab1_info_d[l_ac].qcabcrtid_desc,g_qcab1_info_d[l_ac].qcabcrtdp, 
                      g_qcab1_info_d[l_ac].qcabcrtdp_desc,g_qcab1_info_d[l_ac].qcabcrtdt
                  IF SQLCA.sqlcode THEN
                     CALL cl_err(g_qcab_d_t.qcab001,SQLCA.sqlcode,1)
                     LET l_lock_sw = "Y"
                  END IF
                  
                  CALL aqci002_detail_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
        
         BEFORE INSERT
            
            CALL s_transaction_begin()
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_qcab_d_t.* TO NULL
            INITIALIZE g_qcab_d[l_ac].* TO NULL 
            
            LET g_qcab_d_t.* = g_qcab_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL aqci002_set_entry_b("a")
            CALL aqci002_set_no_entry_b("a")
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_qcab_d[li_reproduce_target].* = g_qcab_d[li_reproduce].*
               LET g_qcab1_info_d[li_reproduce_target].* = g_qcab1_info_d[li_reproduce].*
 
               LET g_qcab_d[g_qcab_d.getLength()].qcab001 = NULL
               LET g_qcab_d[g_qcab_d.getLength()].qcab002 = NULL
               LET g_qcab_d[g_qcab_d.getLength()].qcab003 = NULL
               LET g_qcab_d[g_qcab_d.getLength()].qcab004 = NULL
 
            END IF
            #公用欄位給值(單身)
            #此段落由子樣板a14產生    
      LET g_qcab1_info_d[l_ac].qcabownid = g_user
      LET g_qcab1_info_d[l_ac].qcabowndp = g_dept
      LET g_qcab1_info_d[l_ac].qcabcrtid = g_user
      LET g_qcab1_info_d[l_ac].qcabcrtdp = g_dept 
      LET g_qcab1_info_d[l_ac].qcabcrtdt = cl_get_current()
      LET g_qcab1_info_d[l_ac].qcabmodid = ""
      LET g_qcab1_info_d[l_ac].qcabmoddt = ""
      #LET g_qcab_d[l_ac].qcabstus = ""
 
 
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM qcab_t 
             WHERE qcabent = g_enterprise AND qcab001 = g_qcab_d[l_ac].qcab001
                                       AND qcab002 = g_qcab_d[l_ac].qcab002
                                       AND qcab003 = g_qcab_d[l_ac].qcab003
                                       AND qcab004 = g_qcab_d[l_ac].qcab004
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前
               {<point name="input.body.b_insert"/>}
               #end add-point
            
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_qcab_d[g_detail_idx].qcab001
               LET gs_keys[2] = g_qcab_d[g_detail_idx].qcab002
               LET gs_keys[3] = g_qcab_d[g_detail_idx].qcab003
               LET gs_keys[4] = g_qcab_d[g_detail_idx].qcab004
               CALL aqci002_insert_b('qcab_t',gs_keys,"'1'")
                           
               #add-point:單身新增後
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_qcab_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("qcab_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL aqci002_b_fill(g_wc2)
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert2"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR 'INSERT O.K'
               LET g_detail_cnt = g_detail_cnt + 1
               
               LET g_wc2 = g_wc2, " OR (qcab001 = '", g_qcab_d[l_ac].qcab001, "' "
                                  ," AND qcab002 = '", g_qcab_d[l_ac].qcab002, "' "
                                  ," AND qcab003 = '", g_qcab_d[l_ac].qcab003, "' "
                                  ," AND qcab004 = '", g_qcab_d[l_ac].qcab004, "' "
 
                                  ,")"
            END IF                
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_qcab_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_qcab_d.deleteElement(l_ac)
               NEXT FIELD qcab001
            END IF
            IF g_qcab_d[l_ac].qcab001 IS NOT NULL
               AND g_qcab_d_t.qcab002 IS NOT NULL
               AND g_qcab_d_t.qcab003 IS NOT NULL
               AND g_qcab_d_t.qcab004 IS NOT NULL
 
               THEN     
            
               #add-point:單身刪除ask前
               {<point name="input.body.b_delete_ask"/>}
               #end add-point   
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete" mark="Y"/>}
               #end add-point   
               
               DELETE FROM qcab_t
                WHERE qcabent = g_enterprise AND 
                      qcab001 = g_qcab_d_t.qcab001
                      AND qcab002 = g_qcab_d_t.qcab002
                      AND qcab003 = g_qcab_d_t.qcab003
                      AND qcab004 = g_qcab_d_t.qcab004
 
                      
               #add-point:單身刪除中
               {<point name="input.body.m_delete"/>}
               #end add-point  
                      
               IF SQLCA.sqlcode THEN
                  CALL cl_err("qcab_t",SQLCA.sqlcode,1)
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               ELSE
                  LET g_detail_cnt = g_detail_cnt-1
                  
                  #add-point:單身刪除後
                  {<point name="input.body.a_delete"/>}
                  #end add-point
                  CALL s_transaction_end('Y','0')
               END IF 
               CLOSE aqci002_bcl
               LET l_count = g_qcab_d.getLength()
            END IF 
                           INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_qcab_d[g_detail_idx].qcab001
               LET gs_keys[2] = g_qcab_d[g_detail_idx].qcab002
               LET gs_keys[3] = g_qcab_d[g_detail_idx].qcab003
               LET gs_keys[4] = g_qcab_d[g_detail_idx].qcab004
 
              
         AFTER DELETE 
            #add-point:單身刪除後2
            {<point name="input.body.after_delete"/>}
            #end add-point
                           CALL aqci002_delete_b('qcab_t',gs_keys,"'1'")
 
         #---------------------<  Detail: page1  >---------------------
 
         #---------------------<  Detail: page1  >---------------------
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_qcab_d[l_ac].* = g_qcab_d_t.*
               CLOSE aqci002_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_qcab_d[l_ac].qcab001,-263,1)
               LET g_qcab_d[l_ac].* = g_qcab_d_t.*
            ELSE
               
               #寫入修改者/修改日期資訊(單身)
               LET g_qcab1_info_d[l_ac].qcabmodid = g_user 
LET g_qcab1_info_d[l_ac].qcabmoddt = cl_get_current()
 
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               UPDATE qcab_t SET (qcabstus,qcab001,qcab002,qcab003,qcab004,qcab005,qcab006,qcab007,qcabmodid, 
                   qcabmoddt,qcabownid,qcabowndp,qcabcrtid,qcabcrtdp,qcabcrtdt) = (g_qcab_d[l_ac].qcabstus, 
                   g_qcab_d[l_ac].qcab001,g_qcab_d[l_ac].qcab002,g_qcab_d[l_ac].qcab003,g_qcab_d[l_ac].qcab004, 
                   g_qcab_d[l_ac].qcab005,g_qcab_d[l_ac].qcab006,g_qcab_d[l_ac].qcab007,g_qcab1_info_d[l_ac].qcabmodid, 
                   g_qcab1_info_d[l_ac].qcabmoddt,g_qcab1_info_d[l_ac].qcabownid,g_qcab1_info_d[l_ac].qcabowndp, 
                   g_qcab1_info_d[l_ac].qcabcrtid,g_qcab1_info_d[l_ac].qcabcrtdp,g_qcab1_info_d[l_ac].qcabcrtdt) 

                WHERE qcabent = g_enterprise AND
                  qcab001 = g_qcab_d_t.qcab001 #項次   
                  AND qcab002 = g_qcab_d_t.qcab002  
                  AND qcab003 = g_qcab_d_t.qcab003  
                  AND qcab004 = g_qcab_d_t.qcab004  
 
                  
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
                  
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("qcab_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                    WHEN SQLCA.sqlcode #其他錯誤
                     CALL cl_err("qcab_t",SQLCA.sqlcode,1)  
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_qcab_d[g_detail_idx].qcab001
               LET gs_keys_bak[1] = g_qcab_d_t.qcab001
               LET gs_keys[2] = g_qcab_d[g_detail_idx].qcab002
               LET gs_keys_bak[2] = g_qcab_d_t.qcab002
               LET gs_keys[3] = g_qcab_d[g_detail_idx].qcab003
               LET gs_keys_bak[3] = g_qcab_d_t.qcab003
               LET gs_keys[4] = g_qcab_d[g_detail_idx].qcab004
               LET gs_keys_bak[4] = g_qcab_d_t.qcab004
               CALL aqci002_update_b('qcab_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
               END CASE
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            CALL aqci002_unlock_b("qcab_t")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock
            
             #add-point:單身after row
            {<point name="input.body.a_row"/>}
            #end add-point
            
         AFTER INPUT
            #add-point:單身input後
            {<point name="input.body.a_input"/>}
            #end add-point
            
         ON ACTION controlo   
            CALL FGL_SET_ARR_CURR(g_qcab_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_qcab_d.getLength()+1
            
      END INPUT
      
 
      
      DISPLAY ARRAY g_qcab1_info_d TO s_detail1_info.*
         ATTRIBUTES(COUNT=g_detail_cnt)  
      
         BEFORE DISPLAY 
            CALL aqci002_b_fill(g_wc2)
            CALL FGL_SET_ARR_CURR(g_detail_idx)
      
         BEFORE ROW
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1_info")
            LET l_ac = g_detail_idx
            LET g_temp_idx = l_ac
            DISPLAY g_detail_idx TO FORMONLY.idx
            CALL cl_show_fld_cont() 
            
         #add-point:page2自定義行為
         {<point name="input.body1_info.action"/>}
         #end add-point
            
      END DISPLAY
 
      
      #add-point:before_more_input
      {<point name="input.more_input"/>}
      #end add-point
      
      BEFORE DIALOG 
         IF g_temp_idx > 0 THEN
            LET l_ac = g_temp_idx
            CALL DIALOG.setCurrentRow("s_detail1",l_ac)
            LET g_temp_idx = 1
         END IF
         LET g_curr_diag = ui.DIALOG.getCurrent()
         #add-point:before_dialog
         {<point name="input.before_dialog"/>}
         #end add-point
         CASE g_aw
            WHEN "s_detail1"
               NEXT FIELD qcabstus
            WHEN "s_detail1_info"
               NEXT FIELD qcab001_2
 
         END CASE
   
      ON ACTION accept
         ACCEPT DIALOG
      
      ON ACTION cancel
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION controlr
         CALL cl_show_req_fields()
 
      ON ACTION controlf
         CALL cl_set_focus_form(ui.Interface.getRootNode()) 
              RETURNING g_fld_name,g_frm_name 
         CALL cl_fldhelp(g_frm_name,g_fld_name,g_lang) 
           
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE DIALOG
   
   END DIALOG 
   
   #add-point:modify段修改後
   {<point name="modify.after_input"/>}
   #end add-point
 
   CLOSE aqci002_bcl
   CALL s_transaction_end('Y','0')
   
END FUNCTION
]]>
