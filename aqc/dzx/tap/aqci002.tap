         #   CALL q_common('','qcabowndp',TRUE,FALSE,g_qcab1_info_d[1].qcabowndp) RETURNING ls_return
         #   DISPLAY ls_return TO qcabowndp
         #   NEXT FIELD qcabowndp
         #
         ##----<<qcabcrtid>>----
         #ON ACTION controlp INFIELD qcabcrtid
         #   CALL q_common('','qcabcrtid',TRUE,FALSE,g_qcab1_info_d[1].qcabcrtid) RETURNING ls_return
         #   DISPLAY ls_return TO qcabcrtid
         #   NEXT FIELD qcabcrtid
         #
         ##----<<qcabcrtdp>>----
         #ON ACTION controlp INFIELD qcabcrtdp
         #   CALL q_common('','qcabcrtdp',TRUE,FALSE,g_qcab1_info_d[1].qcabcrtdp) RETURNING ls_return
         #   DISPLAY ls_return TO qcabcrtdp
         #   NEXT FIELD qcabcrtdp
         #
         ##----<<qcabmodid>>----
         #ON ACTION controlp INFIELD qcabmodid
         #   CALL q_common('','qcabmodid',TRUE,FALSE,g_qcab1_info_d[1].qcabmodid) RETURNING ls_return
         #   DISPLAY ls_return TO qcabmodid
         #   NEXT FIELD qcabmodid
         #
         ##----<<qcabcnfid>>----
         ##ON ACTION controlp INFIELD qcabcnfid
         ##   CALL q_common('','qcabcnfid',TRUE,FALSE,.qcabcnfid) RETURNING ls_return
         ##   DISPLAY ls_return TO qcabcnfid
         ##   NEXT FIELD qcabcnfid
         #
         ##----<<qcabpstid>>----
