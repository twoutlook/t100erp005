  <section id="aqci002.query" ver="1" status="" src="s">
    <![CDATA[#+ QBE資料查詢
PRIVATE FUNCTION aqci002_query()
   DEFINE ls_wc      LIKE type_t.chr500
   DEFINE ls_return  STRING
   DEFINE ls_result  STRING 
   #add-point:query段define
   {<point name="query.define"/>}
   #end add-point 
   
   LET INT_FLAG = 0
   CLEAR FORM
   CALL g_qcab_d.clear()
   
   LET g_qryparam.state = "c"
   
   #wc備份
   LET ls_wc = g_wc2
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      CONSTRUCT g_wc2 ON qcabmoddt,qcabcrtid,qcabcrtdp,qcabcrtdt 
 
         FROM s_detail1_info[1].qcabmoddt,s_detail1_info[1].qcabcrtid,s_detail1_info[1].qcabcrtdp,s_detail1_info[1].qcabcrtdt  

      
         #此段落由子樣板a11產生
         ##----<<qcabownid>>----
         #ON ACTION controlp INFIELD qcabownid
         #   CALL q_common('','qcabownid',TRUE,FALSE,g_qcab1_info_d[1].qcabownid) RETURNING ls_return
         #   DISPLAY ls_return TO qcabownid
         #   NEXT FIELD qcabownid  
         #
         ##----<<qcabowndp>>----
