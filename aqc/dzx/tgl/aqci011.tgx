               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = g_qcah_d[l_ac].qcah004 
               LET g_errparam.code   = -263 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               LET g_qcah_d[l_ac].* = g_qcah_d_t.*
            ELSE
               #寫入修改者/修改日期資訊
               LET g_qcah2_d[l_ac].qcahmodid = g_user 
LET g_qcah2_d[l_ac].qcahmoddt = cl_get_current()
LET g_qcah2_d[l_ac].qcahmodid_desc = cl_get_username(g_qcah2_d[l_ac].qcahmodid)
            
               #add-point:單身修改前 name="input.body.b_update"
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
         
               #將遮罩欄位還原
               CALL aqci011_qcah_t_mask_restore('restore_mask_o')
         
               UPDATE qcah_t SET (qcah001,qcah002,qcah003,qcahstus,qcah004,qcah005,qcah006,qcah007,qcahownid,qcahowndp,qcahcrtid,qcahcrtdp,qcahcrtdt,qcahmodid,qcahmoddt) = (g_qcah_m.qcah001,g_qcah_m.qcah002,g_qcah_m.qcah003,g_qcah_d[l_ac].qcahstus,g_qcah_d[l_ac].qcah004,g_qcah_d[l_ac].qcah005,g_qcah_d[l_ac].qcah006,g_qcah_d[l_ac].qcah007,g_qcah2_d[l_ac].qcahownid,g_qcah2_d[l_ac].qcahowndp,g_qcah2_d[l_ac].qcahcrtid,g_qcah2_d[l_ac].qcahcrtdp,g_qcah2_d[l_ac].qcahcrtdt,g_qcah2_d[l_ac].qcahmodid,g_qcah2_d[l_ac].qcahmoddt)
                WHERE qcahent = g_enterprise AND qcah001 = g_qcah_m.qcah001 
                 AND qcah002 = g_qcah_m.qcah002 
                 AND qcah003 = g_qcah_m.qcah003 

                 AND qcah004 = g_qcah_d_t.qcah004 #項次   
                 AND qcah005 = g_qcah_d_t.qcah005  

                 
               #add-point:單身修改中 name="input.body.m_update"
               {<point name="input.body.m_update"/>}
               #end add-point
               
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL s_transaction_end('N','0')
                     INITIALIZE g_errparam TO NULL 
                     LET g_errparam.extend = "qcah_t" 
                     LET g_errparam.code   = "std-00009" 
                     LET g_errparam.popup  = TRUE 
                     CALL cl_err()
                     
                  #WHEN SQLCA.sqlcode #其他錯誤
                  #   INITIALIZE g_errparam TO NULL 
                  #   LET g_errparam.extend = "qcah_t" 
                  #   LET g_errparam.code   = SQLCA.sqlcode 
                  #   LET g_errparam.popup  = TRUE 
                  #   CALL cl_err()
                  #   CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_qcah_m.qcah001
               LET gs_keys_bak[1] = g_qcah001_t
               LET gs_keys[2] = g_qcah_m.qcah002
               LET gs_keys_bak[2] = g_qcah002_t
               LET gs_keys[3] = g_qcah_m.qcah003
               LET gs_keys_bak[3] = g_qcah003_t
               LET gs_keys[4] = g_qcah_d[g_detail_idx].qcah004
               LET gs_keys_bak[4] = g_qcah_d_t.qcah004
               LET gs_keys[5] = g_qcah_d[g_detail_idx].qcah005
               LET gs_keys_bak[5] = g_qcah_d_t.qcah005
               CALL aqci011_update_b('qcah_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
                     #修改歷程記錄(單身修改)
                     LET g_log1 = util.JSON.stringify(g_qcah_m),util.JSON.stringify(g_qcah_d_t)
                     LET g_log2 = util.JSON.stringify(g_qcah_m),util.JSON.stringify(g_qcah_d[l_ac])
                     IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                        CALL s_transaction_end('N','0')
                     END IF
                     
               END CASE
               
               #將遮罩欄位進行遮蔽
               CALL aqci011_qcah_t_mask_restore('restore_mask_n')
               
               #若Key欄位有變動
               LET ls_keys[01] = g_qcah_m.qcah001
               LET ls_keys[ls_keys.getLength()+1] = g_qcah_m.qcah002
               LET ls_keys[ls_keys.getLength()+1] = g_qcah_m.qcah003

               LET ls_keys[ls_keys.getLength()+1] = g_qcah_d_t.qcah004
               LET ls_keys[ls_keys.getLength()+1] = g_qcah_d_t.qcah005

               CALL aqci011_key_update_b(ls_keys)
               
               #add-point:單身修改後 name="input.body.a_update"
               {<point name="input.body.a_update"/>}
               #end add-point
            END IF
 
         AFTER ROW
            #add-point:input段after row name="input.body.after_row"
            {<point name="input.body.after_row"/>}
            #end add-point  
            LET l_ac = ARR_CURR()
            LET l_ac_t = l_ac
            IF INT_FLAG THEN
               CLOSE aqci011_bcl
               CALL s_transaction_end('N','0')
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               IF l_cmd = 'u' THEN
                  LET g_qcah_d[l_ac].* = g_qcah_d_t.*
               END IF
               EXIT DIALOG 
            END IF
 
            CLOSE aqci011_bcl
            CALL s_transaction_end('Y','0')
 
         AFTER INPUT
            #若單身還沒有輸入資料, 強制切換至單身
            IF g_qcah_d.getLength() = 0 THEN
               NEXT FIELD qcah004
            END IF
            #add-point:input段after input  name="input.body.after_input"
            {<point name="input.body.after_input"/>}
            #end add-point    
            
         ON ACTION controlo   
            IF l_insert THEN
               LET li_reproduce = l_ac_t
               LET li_reproduce_target = l_ac
               LET g_qcah_d[li_reproduce_target].* = g_qcah_d[li_reproduce].*
               LET g_qcah2_d[li_reproduce_target].* = g_qcah2_d[li_reproduce].*

               LET g_qcah_d[li_reproduce_target].qcah004 = NULL
               LET g_qcah_d[li_reproduce_target].qcah005 = NULL

            ELSE
               CALL FGL_SET_ARR_CURR(g_qcah_d.getLength()+1)
               LET lb_reproduce = TRUE
               LET li_reproduce = l_ac
               LET li_reproduce_target = g_qcah_d.getLength()+1
            END IF
            
      END INPUT
 

      
      DISPLAY ARRAY g_qcah2_d TO s_detail2.* ATTRIBUTES(COUNT=g_rec_b)  
      
         BEFORE ROW
            CALL aqci011_b_fill(g_wc2) #test 
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail2")
            LET l_ac = g_detail_idx
            LET g_curr_diag = ui.DIALOG.getCurrent()
            LET g_current_page = 2
