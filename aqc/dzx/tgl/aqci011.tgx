            #add-point:ON ACTION controlp INFIELD qcah006 name="input.c.page1.qcah006"
            {<point name="input.c.page1.qcah006" />}
            #END add-point


         #Ctrlp:input.c.page1.qcah007
         #應用 a03 樣板自動產生(Version:3)
         ON ACTION controlp INFIELD qcah007
            #add-point:ON ACTION controlp INFIELD qcah007 name="input.c.page1.qcah007"
            {<point name="input.c.page1.qcah007" />}
            #END add-point



 
         ON ROW CHANGE
            IF INT_FLAG THEN
               LET INT_FLAG = 0
               LET g_qcah_d[l_ac].* = g_qcah_d_t.*
               CLOSE aqci011_bcl
               CALL s_transaction_end('N','0')
               INITIALIZE g_errparam TO NULL 
