{<section id="aqci011.delete" >}
#+ 資料刪除
PRIVATE FUNCTION aqci011_delete()
   #add-point:delete段define name="delete.define_customerization"
   {<point name="delete.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="delete.define"
   {<point name="delete.define"/>}
   #end add-point     
   
   #add-point:Function前置處理  name="delete.pre_function"
   {<point name="delete.pre_function"/>}
   #end add-point
   
   IF g_qcah_m.qcah001 IS NULL
   OR g_qcah_m.qcah002 IS NULL
   OR g_qcah_m.qcah003 IS NULL

   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   
 
   OPEN aqci011_cl USING g_enterprise,g_qcah_m.qcah001,g_qcah_m.qcah002,g_qcah_m.qcah003
   IF SQLCA.SQLCODE THEN   #(ver:49)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aqci011_cl:" 
      LET g_errparam.code   = SQLCA.SQLCODE   #(ver:49)
      LET g_errparam.popup  = TRUE 
      CLOSE aqci011_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aqci011_master_referesh USING g_qcah_m.qcah001,g_qcah_m.qcah002,g_qcah_m.qcah003 INTO g_qcah_m.qcah001,g_qcah_m.qcah002,g_qcah_m.qcah003,g_qcah_m.qcah001_desc
   
   #遮罩相關處理
   LET g_qcah_m_mask_o.* =  g_qcah_m.*
   CALL aqci011_qcah_t_mask()
   LET g_qcah_m_mask_n.* =  g_qcah_m.*
   
   CALL aqci011_show()
 
   #單頭多語言資料備份
   
   
   IF cl_ask_del_master() THEN              #確認一下
      #應用 a47 樣板自動產生(Version:4)
      #刪除相關文件
      CALL aqci011_set_pk_array()
      #add-point:相關文件刪除前 name="delete.befroe.related_document_remove"
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 


  
 
      #add-point:單身刪除前 name="delete.body.b_delete"
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM qcah_t WHERE qcahent = g_enterprise AND qcah001 = g_qcah_m.qcah001
                                                               AND qcah002 = g_qcah_m.qcah002
                                                               AND qcah003 = g_qcah_m.qcah003

                                                               
      #add-point:單身刪除中 name="delete.body.m_delete"
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "qcah_t:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = FALSE 
         CALL s_transaction_end('N','0')
         CALL cl_err()
      END IF
 
      
  
      #add-point:單身刪除後  name="delete.body.a_delete"
      {<point name="delete.body.a_delete"/>}
      #end add-point
      

      
      #單頭多語言刪除
      
      
      #單身多語言刪除
      


   
      #add-point:多語言刪除 name="delete.lang.delete"
      {<point name="delete.lang.delete"/>}
      #end add-point
      
      #頭先不紀錄
      #IF NOT cl_log_modified_record('','') THEN 
      #   CLOSE aqci011_cl
      #   CALL s_transaction_end('N','0')
      #   RETURN 
      #END IF
      
      CLEAR FORM
      CALL g_qcah_d.clear() 
      CALL g_qcah2_d.clear()       

     
      CALL aqci011_ui_browser_refresh()  
      #CALL aqci011_ui_headershow()  
      #CALL aqci011_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL aqci011_fetch('P')
      ELSE
         #LET g_wc = " 1=1"
         #LET g_wc2 = " 1=1"
         #CALL aqci011_browser_fill("F")
         CLEAR FORM
      END IF
      CALL s_transaction_end('Y','0')
   ELSE
      CALL s_transaction_end('N','0')   
   END IF
 
   CLOSE aqci011_cl
 
   #功能已完成,通報訊息中心
   CALL aqci011_msgcentre_notify('delete')
    
END FUNCTION
{</section>}
 
{<section id="aqci011.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION aqci011_b_fill(p_wc2)
   #add-point:b_fill段define name="b_fill.define_customerization"
   {<point name="b_fill.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE p_wc2      STRING
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   {<point name="b_fill.define"/>}
   #end add-point     
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   {<point name="b_fill.pre_function"/>}
   #end add-point
   
   #先清空單身變數內容
   CALL g_qcah_d.clear()
   CALL g_qcah2_d.clear()

 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  DISTINCT qcahstus,qcah004,qcah005,qcah006,qcah007,qcah004,qcah005,qcahownid,qcahowndp,qcahcrtid,qcahcrtdp,qcahcrtdt,qcahmodid,qcahmoddt,t1.ooag011 ,t2.ooefl003 ,t3.ooag011 ,t4.ooefl003 ,t5.ooag011 FROM qcah_t",   
               "",
               
                              " LEFT JOIN ooag_t t1 ON t1.ooagent="||g_enterprise||" AND t1.ooag001=qcahownid  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent="||g_enterprise||" AND t2.ooefl001=qcahowndp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent="||g_enterprise||" AND t3.ooag001=qcahcrtid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent="||g_enterprise||" AND t4.ooefl001=qcahcrtdp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=qcahmodid  ",

               " WHERE qcahent= ? AND qcah001=? AND qcah002=? AND qcah003=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("qcah_t")
   END IF
   
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF aqci011_fill_chk(1) THEN
      IF (g_action_choice = "query" OR cl_null(g_action_choice))
         #add-point:b_fill段long_sql_if name="b_fill.long_sql_if"
         {<point name="b_fill.long_sql_if"/>}
         #end add-point
      THEN
         LET g_sql = g_sql, " ORDER BY qcah_t.qcah004,qcah_t.qcah005"
         #add-point:b_fill段fill_before name="b_fill.fill_before"
         {<point name="b_fill.fill_before"/>}
         #end add-point
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE aqci011_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR aqci011_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
   #  OPEN b_fill_cs USING g_enterprise,g_qcah_m.qcah001,g_qcah_m.qcah002,g_qcah_m.qcah003   #(ver:49)
                                               
      FOREACH b_fill_cs USING g_enterprise,g_qcah_m.qcah001,g_qcah_m.qcah002,g_qcah_m.qcah003 INTO g_qcah_d[l_ac].qcahstus,g_qcah_d[l_ac].qcah004,g_qcah_d[l_ac].qcah005,g_qcah_d[l_ac].qcah006,g_qcah_d[l_ac].qcah007,g_qcah2_d[l_ac].qcah004,g_qcah2_d[l_ac].qcah005,g_qcah2_d[l_ac].qcahownid,g_qcah2_d[l_ac].qcahowndp,g_qcah2_d[l_ac].qcahcrtid,g_qcah2_d[l_ac].qcahcrtdp,g_qcah2_d[l_ac].qcahcrtdt,g_qcah2_d[l_ac].qcahmodid,g_qcah2_d[l_ac].qcahmoddt,g_qcah2_d[l_ac].qcahownid_desc,g_qcah2_d[l_ac].qcahowndp_desc,g_qcah2_d[l_ac].qcahcrtid_desc,g_qcah2_d[l_ac].qcahcrtdp_desc,g_qcah2_d[l_ac].qcahmodid_desc   #(ver:49)
                             
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充 name="b_fill.fill"
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #應用 a12 樣板自動產生(Version:4)




        
         #add-point:單身資料抓取 name="bfill.foreach"
         {<point name="bfill.foreach"/>}
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF 
            EXIT FOREACH
         END IF
      
         LET l_ac = l_ac + 1
         
      END FOREACH
 
            CALL g_qcah_d.deleteElement(g_qcah_d.getLength())
      CALL g_qcah2_d.deleteElement(g_qcah2_d.getLength())

      
   END IF
   
   #add-point:b_fill段more name="b_fill.more"
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_qcah_d.getLength()
      LET g_qcah_d_mask_o[l_ac].* =  g_qcah_d[l_ac].*
      CALL aqci011_qcah_t_mask()
      LET g_qcah_d_mask_n[l_ac].* =  g_qcah_d[l_ac].*
   END FOR
   
   LET g_qcah2_d_mask_o.* =  g_qcah2_d.*
   FOR l_ac = 1 TO g_qcah2_d.getLength()
      LET g_qcah2_d_mask_o[l_ac].* =  g_qcah2_d[l_ac].*
      CALL aqci011_qcah_t_mask()
      LET g_qcah2_d_mask_n[l_ac].* =  g_qcah2_d[l_ac].*
   END FOR

 
   FREE aqci011_pb   
   
END FUNCTION
