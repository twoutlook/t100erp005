         #該樣板不需此段落      LET g_current_sw = TRUE
         #該樣板不需此段落      
         #該樣板不需此段落      IF g_current_idx > g_browser.getLength() THEN
         #該樣板不需此段落         LET g_current_idx = g_browser.getLength()
         #該樣板不需此段落      END IF 
         #該樣板不需此段落      LET g_curr_diag = ui.DIALOG.getCurrent()
         #該樣板不需此段落      CALL aqci011_idx_chk()
         #該樣板不需此段落      CALL aqci011_fetch('') # reload data
         #該樣板不需此段落      LET g_detail_idx = 1
         #該樣板不需此段落      CALL aqci011_ui_detailshow() #Setting the current row 
         #該樣板不需此段落
         #該樣板不需此段落   ON ACTION qbefield_user   #欄位隱藏設定 
         #該樣板不需此段落      LET g_action_choice="qbefield_user"
         #該樣板不需此段落      CALL cl_ui_qbefield_user()
         #該樣板不需此段落
         #該樣板不需此段落END DISPLAY
        
         DISPLAY ARRAY g_qcah_d TO s_detail1.* ATTRIBUTES(COUNT=g_rec_b) #page1  
         
            BEFORE ROW
               #顯示單身筆數
               CALL aqci011_idx_chk()
               LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
               LET l_ac = g_detail_idx
               CALL aqci011_ui_detailshow()
               
               #add-point:page1自定義行為 name="ui_dialog.body.before_row"
               {<point name="ui_dialog.body.before_row"/>}
               #end add-point
            
            BEFORE DISPLAY 
