#+ 資料複製
PRIVATE FUNCTION aqci011_reproduce()
   #add-point:reproduce段define name="reproduce.define_customerization"
   {<point name="reproduce.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE l_newno     LIKE qcah_t.qcah001 
   DEFINE l_oldno     LIKE qcah_t.qcah001 
   DEFINE l_newno02     LIKE qcah_t.qcah002 
   DEFINE l_oldno02     LIKE qcah_t.qcah002 
   DEFINE l_newno03     LIKE qcah_t.qcah003 
   DEFINE l_oldno03     LIKE qcah_t.qcah003 

   DEFINE l_master    RECORD LIKE qcah_t.* #此變數樣板目前無使用
   DEFINE l_detail    RECORD LIKE qcah_t.* #此變數樣板目前無使用

   DEFINE l_cnt       LIKE type_t.num10
   #add-point:reproduce段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="reproduce.define"
   {<point name="reproduce.define"/>}
   #end add-point
   
   #add-point:Function前置處理  name="reproduce.pre_function"
   {<point name="reproduce.pre_function"/>}
   #end add-point
   
   #切換畫面
   IF g_main_hidden THEN
      CALL gfrm_curr.setElementHidden("mainlayout",0)
      CALL gfrm_curr.setElementHidden("worksheet",1)
      LET g_main_hidden = 0
   END IF     
 
   IF g_qcah_m.qcah001 IS NULL
      OR g_qcah_m.qcah002 IS NULL
      OR g_qcah_m.qcah003 IS NULL

      THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
   
   LET g_qcah001_t = g_qcah_m.qcah001
   LET g_qcah002_t = g_qcah_m.qcah002
   LET g_qcah003_t = g_qcah_m.qcah003

   
   LET g_qcah_m.qcah001 = ""
   LET g_qcah_m.qcah002 = ""
   LET g_qcah_m.qcah003 = ""

   LET g_master_insert = FALSE
   CALL aqci011_set_entry('a')
   CALL aqci011_set_no_entry('a')
   
   CALL cl_set_head_visible("","YES")
   CALL s_transaction_begin()
   
   #add-point:複製輸入前 name="reproduce.head.b_input"
   {<point name="reproduce.head.b_input"/>}
   #end add-point
   
   #清空key欄位的desc
      LET g_qcah_m.qcah001_desc = ''
   DISPLAY BY NAME g_qcah_m.qcah001_desc

   
   CALL aqci011_input("r")
    
   IF INT_FLAG AND NOT g_master_insert THEN
      DISPLAY g_detail_cnt  TO FORMONLY.h_count    #總筆數
      DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
      INITIALIZE g_qcah_m.* TO NULL
      INITIALIZE g_qcah_d TO NULL
      INITIALIZE g_qcah2_d TO NULL

      CALL aqci011_show()
      LET INT_FLAG = 0
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = '' 
      LET g_errparam.code   = 9001 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN 
   END IF
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("modify,modify_detail,delete,reproduce", TRUE)
   CALL aqci011_set_act_visible()
   CALL aqci011_set_act_no_visible()
 
   #將新增的資料併入搜尋條件中
   LET g_state = "insert"
   
   LET g_qcah001_t = g_qcah_m.qcah001
   LET g_qcah002_t = g_qcah_m.qcah002
   LET g_qcah003_t = g_qcah_m.qcah003

   
   #組合新增資料的條件
   LET g_add_browse = " qcahent = " ||g_enterprise|| " AND",
                      " qcah001 = '", g_qcah_m.qcah001, "' "
                      ," AND qcah002 = '", g_qcah_m.qcah002, "' "
                      ," AND qcah003 = '", g_qcah_m.qcah003, "' "

