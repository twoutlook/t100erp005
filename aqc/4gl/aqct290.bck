#檢查料、產品特徵、庫、儲、批、庫存管理特徵、庫存單位是否存在inag_t內
PRIVATE FUNCTION aqct290_chk_inag()
DEFINE r_success   LIKE type_t.num5
DEFINE l_n         LIKE type_t.num5
DEFINE l_inag008   LIKE inag_t.inag008

   LET r_success = TRUE
   
   #产品特征
   IF cl_null(g_qcbn_m.qcbn004) THEN
      LET g_qcbn_m.qcbn004 = ' '
   END IF
   
   #储位
   IF cl_null(g_qcbn_m.qcbn006) THEN
      LET g_qcbn_m.qcbn006 = ' '
   END IF
   
   #批号
   IF cl_null(g_qcbn_m.qcbn007) THEN
      LET g_qcbn_m.qcbn007 = ' '
   END IF
   
   #库存管理特征
   IF cl_null(g_qcbn_m.qcbn008) THEN
      LET g_qcbn_m.qcbn008 = ' '
   END IF
   
   IF cl_null(g_qcbn_m.qcbn003) OR cl_null(g_qcbn_m.qcbn005) OR cl_null(g_qcbn_m.qcbn010) THEN
      RETURN r_success
   END IF
   
   LET l_n = 0
   LET l_inag008 = 0
   
   SELECT COUNT(*),SUM(inag008) INTO l_n , l_inag008 FROM inag_t
      WHERE inagent = g_enterprise AND inagsite = g_site AND inag001 = g_qcbn_m.qcbn003
        AND inag002 = g_qcbn_m.qcbn004 AND inag003 = g_qcbn_m.qcbn008 AND inag004 = g_qcbn_m.qcbn005
        AND inag005 = g_qcbn_m.qcbn006 AND inag006 = g_qcbn_m.qcbn007 AND inag007 = g_qcbn_m.qcbn010
   IF l_n = 0 OR cl_null(l_n) THEN 
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'aqc-00128'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()
      LET r_success = FALSE
      RETURN r_success
   END IF
      
   IF NOT cl_null(g_qcbn_m.qcbn011) THEN
      IF g_qcbn_m.qcbn011 > l_inag008 THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'aqc-00129'
         LET g_errparam.extend = g_qcbn_m.qcbn011
         LET g_errparam.popup = TRUE
         LET g_errparam.replace[1] = l_inag008
         CALL cl_err()
         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF      
   
   RETURN r_success
   
   
END FUNCTION

 
