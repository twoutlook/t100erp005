{<section id="aqct290.fetch" >}
#+ 指定PK後抓取單頭其他資料
PRIVATE FUNCTION aqct290_fetch(p_fl)
   #add-point:fetch段define(客製用) name="fetch.define_customerization"
   
   #end add-point
   DEFINE p_fl       LIKE type_t.chr1
   DEFINE ls_msg     STRING
   #add-point:fetch段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="fetch.define"
   
   #end add-point  
   
   #add-point:Function前置處理  name="fetch.pre_function"
   
   #end add-point
   
   #根據傳入的條件決定抓取的資料
   CASE p_fl
      WHEN "F" 
         LET g_current_idx = 1
      WHEN "P"
         IF g_current_idx > 1 THEN               
            LET g_current_idx = g_current_idx - 1
         END IF 
      WHEN "N"
         IF g_current_idx < g_header_cnt THEN
            LET g_current_idx =  g_current_idx + 1
         END IF        
      WHEN "L" 
         #LET g_current_idx = g_header_cnt        
         LET g_current_idx = g_browser.getLength()    
      WHEN "/"
         #詢問要指定的筆數
         IF (NOT g_no_ask) THEN      
            CALL cl_getmsg("fetch", g_lang) RETURNING ls_msg
            LET INT_FLAG = 0
 
            PROMPT ls_msg CLIPPED,": " FOR g_jump
               #交談指令共用ACTION
               &include "common_action.4gl"
            END PROMPT
            
            IF INT_FLAG THEN
               LET INT_FLAG = 0
               EXIT CASE  
            END IF           
         END IF
         IF g_jump > 0 THEN
            LET g_current_idx = g_jump
         END IF
         LET g_no_ask = FALSE     
   END CASE
 
   #筆數顯示
   LET g_browser_idx = g_current_idx 
   IF g_browser_cnt > 0 THEN
      DISPLAY g_browser_idx TO FORMONLY.b_index #當下筆數
      DISPLAY g_browser_cnt TO FORMONLY.b_count #總筆數
      DISPLAY g_browser_idx TO FORMONLY.h_index #當下筆數
      DISPLAY g_browser_cnt TO FORMONLY.h_count #總筆數
   ELSE
      DISPLAY '' TO FORMONLY.b_index #當下筆數
      DISPLAY '' TO FORMONLY.b_count #總筆數
      DISPLAY '' TO FORMONLY.h_index #當下筆數
      DISPLAY '' TO FORMONLY.h_count #總筆數
   END IF
   
   
   
   #避免超出browser資料筆數上限
   IF g_current_idx > g_browser.getLength() THEN
      LET g_browser_idx = g_browser.getLength()
      LET g_current_idx = g_browser.getLength() 
   END IF
   
   # 設定browse索引
   CALL g_curr_diag.setCurrentRow("s_browse", g_current_idx)
   CALL cl_navigator_setting(g_browser_idx, g_browser_cnt) 
 
   #代表沒有資料, 無需做後續資料撈取之動作
   IF g_current_idx = 0 THEN
      RETURN
   END IF
 
   #根據選定的筆數給予key欄位值
   LET g_qcbn_m.qcbndocno = g_browser[g_current_idx].b_qcbndocno
 
                       
   #讀取單頭所有欄位資料
   EXECUTE aqct290_master_referesh USING g_qcbn_m.qcbndocno INTO g_qcbn_m.qcbndocno,g_qcbn_m.qcbndocdt, 
       g_qcbn_m.qcbn001,g_qcbn_m.qcbn002,g_qcbn_m.qcbnstus,g_qcbn_m.qcbnsite,g_qcbn_m.qcbn003,g_qcbn_m.qcbn004, 
       g_qcbn_m.qcbn005,g_qcbn_m.qcbn006,g_qcbn_m.qcbn007,g_qcbn_m.qcbn008,g_qcbn_m.qcbn009,g_qcbn_m.qcbn010, 
       g_qcbn_m.qcbn011,g_qcbn_m.qcbn012,g_qcbn_m.qcbn013,g_qcbn_m.qcbnownid,g_qcbn_m.qcbnowndp,g_qcbn_m.qcbncrtid, 
       g_qcbn_m.qcbncrtdp,g_qcbn_m.qcbncrtdt,g_qcbn_m.qcbnmodid,g_qcbn_m.qcbnmoddt,g_qcbn_m.qcbncnfid, 
       g_qcbn_m.qcbncnfdt,g_qcbn_m.qcbn001_desc,g_qcbn_m.qcbn002_desc,g_qcbn_m.qcbn005_desc,g_qcbn_m.qcbn006_desc, 
       g_qcbn_m.qcbn009_desc,g_qcbn_m.qcbn010_desc,g_qcbn_m.qcbnownid_desc,g_qcbn_m.qcbnowndp_desc,g_qcbn_m.qcbncrtid_desc, 
       g_qcbn_m.qcbncrtdp_desc,g_qcbn_m.qcbnmodid_desc,g_qcbn_m.qcbncnfid_desc
   
   #遮罩相關處理
   LET g_qcbn_m_mask_o.* =  g_qcbn_m.*
   CALL aqct290_qcbn_t_mask()
   LET g_qcbn_m_mask_n.* =  g_qcbn_m.*
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL aqct290_set_act_visible()
   CALL aqct290_set_act_no_visible()
 
   #add-point:fetch段action控制 name="fetch.action_control"
   
   #end add-point  
   
   
   
   #保存單頭舊值
   LET g_qcbn_m_t.* = g_qcbn_m.*
   LET g_qcbn_m_o.* = g_qcbn_m.*
   
   LET g_data_owner = g_qcbn_m.qcbnownid      
   LET g_data_dept  = g_qcbn_m.qcbnowndp
   
   #重新顯示
   CALL aqct290_show()
   
 
END FUNCTION
 
