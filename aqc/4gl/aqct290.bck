{<section id="aqct290.modify" >}
#+ 資料修改
PRIVATE FUNCTION aqct290_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   
   #end add-point
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   
   #end add-point
   
   #add-point:Function前置處理 name="modify.pre_function"
   
   #end add-point
   
   #先確定key值無遺漏
   IF g_qcbn_m.qcbndocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_qcbndocno_t = g_qcbn_m.qcbndocno
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN aqct290_cl USING g_enterprise,g_qcbn_m.qcbndocno
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aqct290_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CLOSE aqct290_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE aqct290_master_referesh USING g_qcbn_m.qcbndocno INTO g_qcbn_m.qcbndocno,g_qcbn_m.qcbndocdt, 
       g_qcbn_m.qcbn001,g_qcbn_m.qcbn002,g_qcbn_m.qcbnstus,g_qcbn_m.qcbnsite,g_qcbn_m.qcbn003,g_qcbn_m.qcbn004, 
       g_qcbn_m.qcbn005,g_qcbn_m.qcbn006,g_qcbn_m.qcbn007,g_qcbn_m.qcbn008,g_qcbn_m.qcbn009,g_qcbn_m.qcbn010, 
       g_qcbn_m.qcbn011,g_qcbn_m.qcbn012,g_qcbn_m.qcbn013,g_qcbn_m.qcbnownid,g_qcbn_m.qcbnowndp,g_qcbn_m.qcbncrtid, 
       g_qcbn_m.qcbncrtdp,g_qcbn_m.qcbncrtdt,g_qcbn_m.qcbnmodid,g_qcbn_m.qcbnmoddt,g_qcbn_m.qcbncnfid, 
       g_qcbn_m.qcbncnfdt,g_qcbn_m.qcbn001_desc,g_qcbn_m.qcbn002_desc,g_qcbn_m.qcbn005_desc,g_qcbn_m.qcbn006_desc, 
       g_qcbn_m.qcbn009_desc,g_qcbn_m.qcbn010_desc,g_qcbn_m.qcbnownid_desc,g_qcbn_m.qcbnowndp_desc,g_qcbn_m.qcbncrtid_desc, 
       g_qcbn_m.qcbncrtdp_desc,g_qcbn_m.qcbnmodid_desc,g_qcbn_m.qcbncnfid_desc
 
   #檢查是否允許此動作
   IF NOT aqct290_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_qcbn_m_mask_o.* =  g_qcbn_m.*
   CALL aqct290_qcbn_t_mask()
   LET g_qcbn_m_mask_n.* =  g_qcbn_m.*
   
   
 
   #顯示資料
   CALL aqct290_show()
   
   WHILE TRUE
      LET g_qcbn_m.qcbndocno = g_qcbndocno_t
 
      
      #寫入修改者/修改日期資訊
      LET g_qcbn_m.qcbnmodid = g_user 
LET g_qcbn_m.qcbnmoddt = cl_get_current()
LET g_qcbn_m.qcbnmodid_desc = cl_get_username(g_qcbn_m.qcbnmodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      
      #end add-point
 
      #資料輸入
      CALL aqct290_input("u")     
 
      #add-point:modify段修改後 name="modify.after_input"
      
      #end add-point
      
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         LET g_qcbn_m.* = g_qcbn_m_t.*
         CALL aqct290_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE qcbn_t SET (qcbnmodid,qcbnmoddt) = (g_qcbn_m.qcbnmodid,g_qcbn_m.qcbnmoddt)
       WHERE qcbnent = g_enterprise AND qcbndocno = g_qcbndocno_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL aqct290_set_act_visible()
   CALL aqct290_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " qcbnent = " ||g_enterprise|| " AND",
                      " qcbndocno = '", g_qcbn_m.qcbndocno, "' "
 
   #填到對應位置
   CALL aqct290_browser_fill(g_wc,"")
 
   CLOSE aqct290_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aqct290_msgcentre_notify('modify')
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
 
