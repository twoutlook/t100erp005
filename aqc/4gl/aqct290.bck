{<section id="aqct290.action_chk" >}
#+ 修改/刪除前行為檢查(是否可允許此動作), 若有其他行為須管控也可透過此段落
PRIVATE FUNCTION aqct290_action_chk()
   #add-point:action_chk段define(客製用) name="action_chk.define_customerization" 
   
   #end add-point
   #add-point:action_chk段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="action_chk.define"
   
   #end add-point
   
   #add-point:action_chk段action_chk name="action_chk.action_chk"
   #160818-00017#33-s
   SELECT qcbnstus INTO g_qcbn_m.qcbnstus FROM qcbn_t
    WHERE qcbnent = g_enterprise
      AND qcbnsite = g_site
      AND qcbndocno = g_qcbn_m.qcbndocno
   IF (g_action_choice="modify" OR g_action_choice="delete" OR g_action_choice="modify_detail")  THEN
     LET g_errno = NULL
     CASE g_qcbn_m.qcbnstus

        WHEN 'X'
           LET g_errno = 'sub-00229'
        WHEN 'Y'
           LET g_errno = 'sub-00178'

     END CASE

     IF NOT cl_null(g_errno) THEN
        INITIALIZE g_errparam TO NULL
        LET g_errparam.code = g_errno
        LET g_errparam.extend = g_qcbn_m.qcbndocno
        LET g_errparam.popup = TRUE
        CALL cl_err()
        LET g_errno = NULL
        CALL aqct290_set_act_visible()
        CALL aqct290_set_act_no_visible()
        CALL aqct290_show()
        RETURN FALSE
     END IF
   END IF      
   #160818-00017#33-e
   #end add-point
   
   RETURN TRUE
   
END FUNCTION
 
