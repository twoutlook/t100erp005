      #end add-point 
   END IF
   
   #add-point:set_no_entry段欄位控制後 name="set_no_entry.after_control"
   LET l_imaa005 = ''
   SELECT imaa005 INTO l_imaa005 FROM imaa_t 
      WHERE imaaent = g_enterprise AND imaa001 = g_qcbn_m.qcbn003
