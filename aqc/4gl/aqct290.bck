{<section id="aqct290.state_change" >}
   #應用 a09 樣板自動產生(Version:17)
#+ 確認碼變更 
PRIVATE FUNCTION aqct290_statechange()
   #add-point:statechange段define(客製用) name="statechange.define_customerization"
   
   #end add-point  
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="statechange.define"
   DEFINE l_qcbncnfdt  DATETIME YEAR TO SECOND
   DEFINE l_n          LIKE type_t.num5
   #end add-point  
   
   #add-point:Function前置處理 name="statechange.before"
   IF g_qcbn_m.qcbnstus = 'X' THEN
      RETURN
   END IF
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_qcbn_m.qcbndocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN aqct290_cl USING g_enterprise,g_qcbn_m.qcbndocno
   IF STATUS THEN
      CLOSE aqct290_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN aqct290_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE aqct290_master_referesh USING g_qcbn_m.qcbndocno INTO g_qcbn_m.qcbndocno,g_qcbn_m.qcbndocdt, 
       g_qcbn_m.qcbn001,g_qcbn_m.qcbn002,g_qcbn_m.qcbnstus,g_qcbn_m.qcbnsite,g_qcbn_m.qcbn003,g_qcbn_m.qcbn004, 
       g_qcbn_m.qcbn005,g_qcbn_m.qcbn006,g_qcbn_m.qcbn007,g_qcbn_m.qcbn008,g_qcbn_m.qcbn009,g_qcbn_m.qcbn010, 
       g_qcbn_m.qcbn011,g_qcbn_m.qcbn012,g_qcbn_m.qcbn013,g_qcbn_m.qcbnownid,g_qcbn_m.qcbnowndp,g_qcbn_m.qcbncrtid, 
       g_qcbn_m.qcbncrtdp,g_qcbn_m.qcbncrtdt,g_qcbn_m.qcbnmodid,g_qcbn_m.qcbnmoddt,g_qcbn_m.qcbncnfid, 
       g_qcbn_m.qcbncnfdt,g_qcbn_m.qcbn001_desc,g_qcbn_m.qcbn002_desc,g_qcbn_m.qcbn005_desc,g_qcbn_m.qcbn006_desc, 
       g_qcbn_m.qcbn009_desc,g_qcbn_m.qcbn010_desc,g_qcbn_m.qcbnownid_desc,g_qcbn_m.qcbnowndp_desc,g_qcbn_m.qcbncrtid_desc, 
       g_qcbn_m.qcbncrtdp_desc,g_qcbn_m.qcbnmodid_desc,g_qcbn_m.qcbncnfid_desc
   
 
   #檢查是否允許此動作
   IF NOT aqct290_action_chk() THEN
      CLOSE aqct290_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_qcbn_m.qcbndocno,g_qcbn_m.qcbndocno_desc,g_qcbn_m.qcbndocdt,g_qcbn_m.qcbn001,g_qcbn_m.qcbn001_desc, 
       g_qcbn_m.qcbn002,g_qcbn_m.qcbn002_desc,g_qcbn_m.qcbnstus,g_qcbn_m.qcbnsite,g_qcbn_m.qcbn003,g_qcbn_m.qcbn004, 
       g_qcbn_m.qcbn003_desc,g_qcbn_m.qcbn003_desc_1,g_qcbn_m.qcbn005,g_qcbn_m.qcbn005_desc,g_qcbn_m.qcbn006, 
       g_qcbn_m.qcbn006_desc,g_qcbn_m.qcbn007,g_qcbn_m.qcbn008,g_qcbn_m.qcbn009,g_qcbn_m.qcbn009_desc, 
       g_qcbn_m.qcbn010,g_qcbn_m.qcbn010_desc,g_qcbn_m.qcbn011,g_qcbn_m.qcbn012,g_qcbn_m.qcbn013,g_qcbn_m.qcbnownid, 
       g_qcbn_m.qcbnownid_desc,g_qcbn_m.qcbnowndp,g_qcbn_m.qcbnowndp_desc,g_qcbn_m.qcbncrtid,g_qcbn_m.qcbncrtid_desc, 
       g_qcbn_m.qcbncrtdp,g_qcbn_m.qcbncrtdp_desc,g_qcbn_m.qcbncrtdt,g_qcbn_m.qcbnmodid,g_qcbn_m.qcbnmodid_desc, 
       g_qcbn_m.qcbnmoddt,g_qcbn_m.qcbncnfid,g_qcbn_m.qcbncnfid_desc,g_qcbn_m.qcbncnfdt
 
   CASE g_qcbn_m.qcbnstus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
      WHEN "X"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_qcbn_m.qcbnstus
            
            WHEN "N"
               HIDE OPTION "unconfirmed"
            WHEN "Y"
               HIDE OPTION "confirmed"
            WHEN "X"
               HIDE OPTION "invalid"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      CASE g_qcbn_m.qcbnstus
         WHEN "N"
            CALL cl_set_act_visible("unconfirmed",FALSE)

         WHEN "X"
            CALL cl_set_act_visible("unconfirmed,invalid,confirmed",FALSE)

         WHEN "Y"
            CALL cl_set_act_visible("invalid,confirmed",FALSE)

      END CASE
      #end add-point
      
      
	  
      ON ACTION unconfirmed
         IF cl_auth_chk_act("unconfirmed") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.unconfirmed"
            IF NOT cl_ask_confirm('aim-00110') THEN
               CLOSE aqct290_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            
            #已轉QC單的申請單號不可取消確認
            LET l_n = 0
            SELECT COUNT(*) INTO l_n FROM qcba_t
             WHERE qcbaent = g_enterprise
               AND qcba001 = g_qcbn_m.qcbndocno
               AND qcbastus <> 'X'
            IF l_n > 0 THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 'sub-00473'
               LET g_errparam.extend = g_qcbn_m.qcbndocno
               LET g_errparam.popup = TRUE
               CALL cl_err()
               CLOSE aqct290_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            #更新單據狀態碼
            UPDATE qcbn_t
               SET qcbnstus = 'N',
                   qcbncnfid = NULL,
                   qcbncnfdt = NULL
             WHERE qcbnent = g_enterprise
               AND qcbndocno = g_qcbn_m.qcbndocno
            IF SQLCA.sqlcode THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = SQLCA.sqlcode
               LET g_errparam.extend = "UPDATE qcbn_t"
               LET g_errparam.popup = TRUE
               CALL cl_err()
               CLOSE aqct290_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            #end add-point
         END IF
         EXIT MENU
      ON ACTION confirmed
         IF cl_auth_chk_act("confirmed") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.confirmed"
            #更新單據狀態碼
            IF NOT cl_ask_confirm('aim-00108') THEN
               CLOSE aqct290_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            LET l_qcbncnfdt = cl_get_current()
            UPDATE qcbn_t
               SET qcbnstus = 'Y',
                   qcbncnfid = g_user,
                   qcbncnfdt = l_qcbncnfdt
             WHERE qcbnent = g_enterprise
               AND qcbndocno = g_qcbn_m.qcbndocno
            IF SQLCA.sqlcode THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = SQLCA.sqlcode
               LET g_errparam.extend = "UPDATE qcbn_t"
               LET g_errparam.popup = TRUE
               CALL cl_err()
               CLOSE aqct290_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            #end add-point
         END IF
         EXIT MENU
      ON ACTION invalid
         IF cl_auth_chk_act("invalid") THEN
            LET lc_state = "X"
            #add-point:action控制 name="statechange.invalid"
            IF NOT cl_ask_confirm('aim-00109') THEN
               CLOSE aqct290_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      AND lc_state <> "X"
      ) OR 
      g_qcbn_m.qcbnstus = lc_state OR cl_null(lc_state) THEN
      CLOSE aqct290_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   
   #end add-point
   
   LET g_qcbn_m.qcbnmodid = g_user
   LET g_qcbn_m.qcbnmoddt = cl_get_current()
   LET g_qcbn_m.qcbnstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE qcbn_t 
      SET (qcbnstus,qcbnmodid,qcbnmoddt) 
        = (g_qcbn_m.qcbnstus,g_qcbn_m.qcbnmodid,g_qcbn_m.qcbnmoddt)     
    WHERE qcbnent = g_enterprise AND qcbndocno = g_qcbn_m.qcbndocno
 
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/unconfirmed.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/confirmed.png")
         WHEN "X"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/invalid.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE aqct290_master_referesh USING g_qcbn_m.qcbndocno INTO g_qcbn_m.qcbndocno,g_qcbn_m.qcbndocdt, 
          g_qcbn_m.qcbn001,g_qcbn_m.qcbn002,g_qcbn_m.qcbnstus,g_qcbn_m.qcbnsite,g_qcbn_m.qcbn003,g_qcbn_m.qcbn004, 
          g_qcbn_m.qcbn005,g_qcbn_m.qcbn006,g_qcbn_m.qcbn007,g_qcbn_m.qcbn008,g_qcbn_m.qcbn009,g_qcbn_m.qcbn010, 
          g_qcbn_m.qcbn011,g_qcbn_m.qcbn012,g_qcbn_m.qcbn013,g_qcbn_m.qcbnownid,g_qcbn_m.qcbnowndp,g_qcbn_m.qcbncrtid, 
          g_qcbn_m.qcbncrtdp,g_qcbn_m.qcbncrtdt,g_qcbn_m.qcbnmodid,g_qcbn_m.qcbnmoddt,g_qcbn_m.qcbncnfid, 
          g_qcbn_m.qcbncnfdt,g_qcbn_m.qcbn001_desc,g_qcbn_m.qcbn002_desc,g_qcbn_m.qcbn005_desc,g_qcbn_m.qcbn006_desc, 
          g_qcbn_m.qcbn009_desc,g_qcbn_m.qcbn010_desc,g_qcbn_m.qcbnownid_desc,g_qcbn_m.qcbnowndp_desc, 
          g_qcbn_m.qcbncrtid_desc,g_qcbn_m.qcbncrtdp_desc,g_qcbn_m.qcbnmodid_desc,g_qcbn_m.qcbncnfid_desc 
 
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_qcbn_m.qcbndocno,g_qcbn_m.qcbndocno_desc,g_qcbn_m.qcbndocdt,g_qcbn_m.qcbn001, 
          g_qcbn_m.qcbn001_desc,g_qcbn_m.qcbn002,g_qcbn_m.qcbn002_desc,g_qcbn_m.qcbnstus,g_qcbn_m.qcbnsite, 
          g_qcbn_m.qcbn003,g_qcbn_m.qcbn004,g_qcbn_m.qcbn003_desc,g_qcbn_m.qcbn003_desc_1,g_qcbn_m.qcbn005, 
          g_qcbn_m.qcbn005_desc,g_qcbn_m.qcbn006,g_qcbn_m.qcbn006_desc,g_qcbn_m.qcbn007,g_qcbn_m.qcbn008, 
          g_qcbn_m.qcbn009,g_qcbn_m.qcbn009_desc,g_qcbn_m.qcbn010,g_qcbn_m.qcbn010_desc,g_qcbn_m.qcbn011, 
          g_qcbn_m.qcbn012,g_qcbn_m.qcbn013,g_qcbn_m.qcbnownid,g_qcbn_m.qcbnownid_desc,g_qcbn_m.qcbnowndp, 
          g_qcbn_m.qcbnowndp_desc,g_qcbn_m.qcbncrtid,g_qcbn_m.qcbncrtid_desc,g_qcbn_m.qcbncrtdp,g_qcbn_m.qcbncrtdp_desc, 
          g_qcbn_m.qcbncrtdt,g_qcbn_m.qcbnmodid,g_qcbn_m.qcbnmodid_desc,g_qcbn_m.qcbnmoddt,g_qcbn_m.qcbncnfid, 
          g_qcbn_m.qcbncnfid_desc,g_qcbn_m.qcbncnfdt
   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
   
   #end add-point
 
   #add-point:statechange段結束前 name="statechange.after"
   
   #end add-point  
 
   CLOSE aqct290_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL aqct290_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION
 
 
 
 
