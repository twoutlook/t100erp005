################################################################################
# Descriptions...: 獲取資料選項說明
# Memo...........:
# Usage..........: CALL s_aooi150_ins (传入参数)
#                  RETURNING r_desc
# Input parameter: p_class 類型
#                : p_num   系統分類碼
#                : p_target   系統分類碼值
# Return code....: r_desc   將說明傳出

# Date & Author..: 2014/7/21 By zj
# Modify.........:
################################################################################
PRIVATE FUNCTION aqcr310_g01_desc(p_class,p_num,p_target)
   DEFINE p_class  LIKE type_t.chr1
   DEFINE p_num    LIKE type_t.num5
   DEFINE p_target LIKE type_t.chr10
   DEFINE r_desc   LIKE type_t.chr500
   
   CASE p_class
      WHEN '1'
         SELECT gzcbl004 INTO r_desc
           FROM gzcbl_t
          WHERE gzcbl001 = p_num
            AND gzcbl002 = p_target
            AND gzcbl003 = g_dlang
         
      WHEN '2'
         SELECT gzaal003 INTO r_desc
           FROM gzaal_t
           LEFT OUTER JOIN gzaa_t on gzaa_t.gzaa001 = gzaal_t.gzaal001
          WHERE gzaal001 = p_num
            AND gzaa_t.gzaa004 = p_target
            AND gzaal002 = g_dlang
            
      WHEN '3'
         SELECT oocql004 INTO r_desc
           FROM oocql_t
          WHERE oocql001 = p_num
            AND oocql002 = p_target
            AND oocqlent = g_enterprise
            AND oocql003 = g_dlang  
            
   END CASE
   
   RETURN r_desc
END FUNCTION

 
{</section>}
 
