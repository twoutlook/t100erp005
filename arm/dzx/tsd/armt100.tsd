  <field cite_std="N" name="rmaa_t.rmaa003" ver="1" src="s" column="rmaa003" table="rmaa_t" attribute="" type="" req="Y" i_zoom="q_ooea001_3" c_zoom="q_ooea001_3" chk_ref="v_ooea001_3" items="" default="" max_compare="" max="" min_compare="" min="" can_edit="Y" can_query="Y" widget="ButtonEdit" status="">
    <![CDATA[業務部門：
