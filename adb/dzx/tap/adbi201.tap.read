    <![CDATA[#+ 功能選單 
PRIVATE FUNCTION adbi201_ui_dialog()
   DEFINE la_param  RECORD
             prog   STRING,
             param  DYNAMIC ARRAY OF STRING
                    END RECORD
   DEFINE ls_js     STRING
