  <point name="statechange.b_update" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[   CALL s_transaction_begin()
   CALL cl_showmsg_init()
   OPEN adbt590_cl USING g_enterprise,g_xmdk_m.xmdkdocno
 
   IF STATUS THEN
      CALL cl_err("OPEN adbt590_cl:", STATUS, 1)
      CLOSE adbt590_cl
      CALL s_transaction_end('N','0')
      CALL cl_showmsg()
      RETURN
   END IF
   IF lc_state = 'Y' THEN
      CALL s_adbt590_conf_chk(g_xmdk_m.xmdkdocno) RETURNING l_success
      IF NOT l_success THEN
         CALL s_transaction_end('N','0')
         CALL cl_showmsg()
         RETURN
      ELSE
         IF NOT cl_ask_confirm('aim-00108') THEN
            CALL s_transaction_end('N','0')
            CALL cl_showmsg()
            RETURN
         ELSE
            CALL s_transaction_begin()
            IF NOT adbt590_conf_input() THEN   #Input
               CALL s_transaction_end('N','0')
               CALL cl_showmsg()
               RETURN
            END IF
            CALL s_adbt590_conf_upd(g_xmdk_m.xmdkdocno) RETURNING l_success
            IF NOT l_success THEN
               CALL s_transaction_end('N','0')
               CALL cl_showmsg()
               RETURN
            ELSE
               CALL s_transaction_end('Y','0')
               CALL cl_showmsg()
            END IF
         END IF
      END IF
   END IF
           
   IF lc_state = 'N' THEN
      CALL s_adbt590_unconf_chk(g_xmdk_m.xmdkdocno) RETURNING l_success
      IF NOT l_success THEN
         CALL s_transaction_end('N','0')
         CALL cl_showmsg()
         RETURN
      ELSE
         IF NOT cl_ask_confirm('aim-00110') THEN
            CALL s_transaction_end('N','0')
            CALL cl_showmsg()
            RETURN
         ELSE
            CALL s_transaction_begin()
            CALL s_adbt590_unconf_upd(g_xmdk_m.xmdkdocno) RETURNING l_success
            IF NOT l_success THEN
               CALL s_transaction_end('N','0')
               CALL cl_showmsg()
               RETURN
            ELSE
               CALL s_transaction_end('Y','0')
               CALL cl_showmsg()
            END IF
         END IF
      END IF
   END IF  ]]>
  </point>
