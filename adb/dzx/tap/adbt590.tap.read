   LET g_wc = cl_replace_str(g_wc, g_wc_filter_t, '')
 
   #使用DIALOG包住 單頭CONSTRUCT及單身CONSTRUCT
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      #單頭
      CONSTRUCT g_wc_filter ON xmdkdocno,xmdk001,xmdk003,xmdk004,xmdk006,xmdk007,xmdk008,xmdk009
                          FROM s_browse[1].b_xmdkdocno,s_browse[1].b_xmdk001,s_browse[1].b_xmdk003,s_browse[1].b_xmdk004, 
                              s_browse[1].b_xmdk006,s_browse[1].b_xmdk007,s_browse[1].b_xmdk008,s_browse[1].b_xmdk009 

 
         BEFORE CONSTRUCT
               DISPLAY adbt590_filter_parser('xmdkdocno') TO s_browse[1].b_xmdkdocno
            DISPLAY adbt590_filter_parser('xmdk001') TO s_browse[1].b_xmdk001
            DISPLAY adbt590_filter_parser('xmdk003') TO s_browse[1].b_xmdk003
            DISPLAY adbt590_filter_parser('xmdk004') TO s_browse[1].b_xmdk004
            DISPLAY adbt590_filter_parser('xmdk006') TO s_browse[1].b_xmdk006
            DISPLAY adbt590_filter_parser('xmdk007') TO s_browse[1].b_xmdk007
            DISPLAY adbt590_filter_parser('xmdk008') TO s_browse[1].b_xmdk008
            DISPLAY adbt590_filter_parser('xmdk009') TO s_browse[1].b_xmdk009
      
         #add-point:filter段cs_ctrl
         {<point name="filter.cs_ctrl"/>}
         #end add-point
      
      END CONSTRUCT
 
      #add-point:filter段add_cs
      {<point name="filter.add_cs"/>}
      #end add-point
 
      BEFORE DIALOG
         #add-point:filter段b_dialog
         {<point name="filter.b_dialog"/>}
         #end add-point  
      
      ON ACTION accept
         ACCEPT DIALOG
 
      ON ACTION cancel
         LET INT_FLAG = 1
         EXIT DIALOG 
 
      #交談指令共用ACTION
      &include "common_action.4gl" 
         CONTINUE DIALOG
   
   END DIALOG
 
   IF NOT INT_FLAG THEN
      LET g_wc_filter = "   AND   ", g_wc_filter, "   "
      LET g_wc = g_wc , g_wc_filter
   ELSE
      LET g_wc_filter = g_wc_filter_t
      LET g_wc = g_wc_t
   END IF
 
      CALL adbt590_filter_show('xmdkdocno')
   CALL adbt590_filter_show('xmdk001')
   CALL adbt590_filter_show('xmdk003')
   CALL adbt590_filter_show('xmdk004')
