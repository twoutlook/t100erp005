  <point name="fetch.after" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[   CALL cl_set_act_visible("insert,delete,reproduce",FALSE)
   CALL cl_set_act_visible("modify,modify_detail",TRUE)
   CALL cl_set_act_visible("adbt590_change_warehouse",TRUE)
   CALL cl_set_act_visible("open_adbt590_01",TRUE)
   CALL cl_set_act_visible("call_s_pay,call_s_pay_09",TRUE)

   CASE g_xmdk_m.xmdkstus
      WHEN 'N'   #未確認
         
      WHEN 'Y'   #已確認
         CALL cl_set_act_visible("modify,modify_detail",FALSE)
         CALL cl_set_act_visible("adbt590_change_warehouse",FALSE)
         CALL cl_set_act_visible("open_adbt590_01,call_s_pay,call_s_pay_09",FALSE)
      WHEN 'X'   #作廢
         CALL cl_set_act_visible("modify,modify_detail",FALSE)
         CALL cl_set_act_visible("adbt590_change_warehouse",FALSE)
         CALL cl_set_act_visible("open_adbt590_01,call_s_pay,call_s_pay_09",FALSE)
      WHEN 'D'   #抽單
      WHEN 'R'   #已拒絕
      WHEN 'A'   #已核准
         CALL cl_set_act_visible("modify,modify_detail",FALSE)
         CALL cl_set_act_visible("adbt590_change_warehouse",FALSE)
         CALL cl_set_act_visible("open_adbt590_01,call_s_pay,call_s_pay_09",FALSE)
      WHEN 'W'   #送簽中
         CALL cl_set_act_visible("modify,modify_detail",FALSE)
         CALL cl_set_act_visible("adbt590_change_warehouse",FALSE)
         CALL cl_set_act_visible("open_adbt590_01,call_s_pay,call_s_pay_09",FALSE)
   END CASE
   
   LET l_xmdl216 = ''
   SELECT DISTINCT xmdl216
     INTO l_xmdl216
     FROM xmdl_t
    WHERE xmdlent = g_enterprise
      AND xmdldocno = g_xmdk_m.xmdkdocno
   IF l_xmdl216 = '13' THEN
      CALL cl_set_act_visible('call_s_pay,call_s_pay_09',FALSE)
   END IF ]]>
