  <point name="function.adbt590_xmdl014_chk" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION adbt590_xmdl014_chk()
   DEFINE r_success      LIKE type_t.num5
   DEFINE l_xmdl014      LIKE xmdl_t.xmdl014
   DEFINE l_type1        LIKE type_t.chr1
   DEFINE l_type2        LIKE type_t.chr1
   
   LET r_success = TRUE
   
   #庫位
   IF NOT cl_null(g_xmdl_d[l_ac].xmdl014)THEN     
      #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
      INITIALIZE g_chkparam.* TO NULL
               
      #設定g_chkparam.*的參數
      LET g_chkparam.arg1 = g_site
      LET g_chkparam.arg2 = g_xmdl_d[l_ac].xmdl014    #庫位
                        
      #呼叫檢查存在並帶值的library
      IF NOT cl_chk_exist("v_inaa001") THEN
         LET r_success = FALSE
         RETURN r_success
      END IF
      
      #原始出貨單庫位
      LET l_xmdl014 = ''
      SELECT xmdl014 INTO l_xmdl014
        FROM xmdl_t
       WHERE xmdlent = g_enterprise
         AND xmdldocno = g_xmdl_d[l_ac].xmdl001
         AND xmdlseq = g_xmdl_d[l_ac].xmdl002

      #出貨單庫位
      CALL s_adbt540_inag012_chk(g_site,g_xmdl_d[l_ac].xmdl008,g_xmdl_d[l_ac].xmdl009,l_xmdl014,g_xmdl_d[l_ac].xmdl015,g_xmdl_d[l_ac].xmdl016,g_xmdl_d[l_ac].xmdl017)
      RETURNING l_type1
            
      #新輸入庫位
      CALL s_adbt540_inag012_chk(g_site,g_xmdl_d[l_ac].xmdl008,g_xmdl_d[l_ac].xmdl009,g_xmdl_d[l_ac].xmdl014,g_xmdl_d[l_ac].xmdl015,g_xmdl_d[l_ac].xmdl016,g_xmdl_d[l_ac].xmdl017)
      RETURNING l_type2
      
      IF l_type1 <> l_type2 THEN
         CALL cl_err_msg('','axm-00250',l_xmdl014 ||'|'|| g_xmdl_d[l_ac].xmdl014,1)
         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF
   
   RETURN r_success
END FUNCTION]]>
  </point>
  <point name="function.adbt590_xmdl015_chk" order="2" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION adbt590_xmdl015_chk(p_xmdl014,p_xmdl015)
   DEFINE p_xmdl014      LIKE xmdl_t.xmdl014
   DEFINE p_xmdl015      LIKE xmdl_t.xmdl015
   DEFINE r_success      LIKE type_t.num5
   
   LET r_success = TRUE
   
   IF NOT cl_null(p_xmdl015) THEN     #儲位       
   
      IF cl_null(p_xmdl014) THEN         #庫位
         CALL cl_err('','sub-00126',1)
         LET r_success = FALSE
         RETURN r_success   
      END IF
      
      #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
      INITIALIZE g_chkparam.* TO NULL
               
      #設定g_chkparam.*的參數
      LET g_chkparam.arg1 = g_site
      LET g_chkparam.arg2 = p_xmdl014
      LET g_chkparam.arg3 = p_xmdl015
                  
      #呼叫檢查存在並帶值的library
      IF NOT cl_chk_exist("v_inab002") THEN
         LET r_success = FALSE
         RETURN r_success  
      END IF
   END IF
   
   RETURN r_success
END FUNCTION]]>
