                  IF NOT ap_chk_notDup("","SELECT COUNT(*) FROM xmdr_t WHERE "||"xmdrent = '" ||g_enterprise|| "' AND "||"xmdrdocno = '"||g_xmdr_d[g_detail_idx].xmdrdocno ||"' AND "|| "xmdrseq = '"||g_xmdr_d[g_detail_idx].xmdrseq ||"' AND "|| "xmdrseq1 = '"||g_xmdr_d[g_detail_idx].xmdrseq1 ||"' AND "|| "xmdrseq2 = '"||g_xmdr_d[g_detail_idx].xmdrseq2 ||"' AND "|| "xmdr001 = '"||g_xmdr_d[g_detail_idx].xmdr001 ||"' AND "|| "xmdr002 = '"||g_xmdr_d[g_detail_idx].xmdr002 ||"' AND "|| "xmdr003 = '"||g_xmdr_d[g_detail_idx].xmdr003 ||"' AND "|| "xmdr004 = '"||g_xmdr_d[g_detail_idx].xmdr004 ||"' AND "|| "xmdr005 = '"||g_xmdr_d[g_detail_idx].xmdr005 ||"' AND "|| "xmdr006 = '"||g_xmdr_d[g_detail_idx].xmdr006 ||"' AND "|| "xmdr007 = '"||g_xmdr_d[g_detail_idx].xmdr007 ||"'",'std-00004',0) THEN 
