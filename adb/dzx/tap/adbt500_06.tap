<add_points prog="adbt500_06" std_prog="adbt500_06" erpver="1.0" module="ADB" ver="1" env="s" zone="t10dev" booking="Y">
  <point name="function.adbt500_06_fill" cite_std="N" status="u" ver="1" src="s" new="Y" order="1" mark_hard="N">
<![CDATA[
PRIVATE FUNCTION adbt500_06_fill()
DEFINE l_sql      STRING
DEFINE l_ac       LIKE type_t.num5
DEFINE i          LIKE type_t.num5
DEFINE l_cnt      LIKE type_t.num5
DEFINE l_success  LIKE type_t.num5
DEFINE l_inan010  LIKE inan_t.inan010




   LET l_sql = "SELECT xmdr001,xmdr002,'',xmdr008,xmdr009, ",
               "       '',xmdr010,'',xmdr004,'', ",
               "       xmdr005,'',xmdr003,xmdr006,'', ",
               "       xmdr007,'' ",
               "  FROM xmdr_t ",
               "  WHERE xmdrent = '",g_enterprise,"' ",
               "    AND xmdrdocno = '",g_xmdddocno,"' ",
               "    AND xmdrseq = '",g_xmddseq,"' ",
               "    AND xmdrseq1 = '",g_xmddseq1,"' ",
               "    AND xmdrseq2 = '",g_xmddseq2,"' "
   PREPARE adbt500_06_sel FROM l_sql
   DECLARE adbt500_06_per CURSOR FOR adbt500_06_sel 
   
   CALL g_xmdr_d.clear()
   LET l_ac = 1
   
   #先抓取對應的xmdr_t資料
   FOREACH adbt500_06_per INTO g_xmdr_d[l_ac].*
        IF SQLCA.sqlcode THEN
           CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
           EXIT FOREACH
        END IF
        LET g_xmdr_d[l_ac].sel = 'Y'
        
        
        #抓取對應inag_t資料的庫存量(inag008)
        LET g_xmdr_d[l_ac].inag008 = 0
        SELECT inag008 INTO g_xmdr_d[l_ac].inag008
          FROM inag_t 
         WHERE inagent = g_enterprise
           AND inagsite = g_xmddsite
           AND inag001 = g_xmdr_d[l_ac].xmdr001
           AND inag002 = g_xmdr_d[l_ac].xmdr002       
           AND inag003 = g_xmdr_d[l_ac].xmdr003
           AND inag004 = g_xmdr_d[l_ac].xmdr004
           AND inag005 = g_xmdr_d[l_ac].xmdr005
           AND inag006 = g_xmdr_d[l_ac].xmdr006            
           AND inag007 = g_xmdr_d[l_ac].xmdr007
        IF cl_null(g_xmdr_d[l_ac].inag008) THEN LET g_xmdr_d[l_ac].inag008 = 0 END IF
        #已在揀量inan010+已備置量inan010
        LET l_inan010 = 0
        SELECT SUM(inan010)  INTO l_inan010
          FROM inan_t
         WHERE inanent = g_enterprise
           AND inansite = g_xmddsite
           AND inan001 = g_xmdr_d[l_ac].xmdr001
           AND inan002 = g_xmdr_d[l_ac].xmdr002       
           AND inan003 = g_xmdr_d[l_ac].xmdr003
           AND inan004 = g_xmdr_d[l_ac].xmdr004
           AND inan005 = g_xmdr_d[l_ac].xmdr005
           AND inan006 = g_xmdr_d[l_ac].xmdr006            
           AND inan007 = g_xmdr_d[l_ac].xmdr007
        IF cl_null(l_inan010) THEN LET l_inan010 = 0 END IF
        #可備置量 = 庫存量(inag008)-已在揀量inan010-已備置量inan010        
        LET g_xmdr_d[l_ac].yxmdr008 = g_xmdr_d[l_ac].inag008 - l_inan010
        
        #轉換備置單位的數量
        IF NOT cl_null(g_xmdr_d[l_ac].xmdr007) THEN
           CALL s_aooi250_convert_qty(g_xmdr_d[l_ac].xmdr001,g_xmdr_d[l_ac].xmdr007,g_xmdr_d[l_ac].xmdr010,g_xmdr_d[l_ac].yxmdr008)
                RETURNING l_success,g_xmdr_d[l_ac].yxmdr008  
        END IF                
               
        CALL adbt500_06_xmdr004_ref(g_xmdr_d[l_ac].xmdr004) RETURNING g_xmdr_d[l_ac].xmdr004_desc 
        CALL adbt500_06_xmdr005_ref(g_xmdr_d[l_ac].xmdr004,g_xmdr_d[l_ac].xmdr005) RETURNING g_xmdr_d[l_ac].xmdr005_desc 
        CALL adbt500_06_unit_ref(g_xmdr_d[l_ac].xmdr007) RETURNING g_xmdr_d[l_ac].xmdr007_desc         
        CALL adbt500_06_unit_ref(g_xmdr_d[l_ac].xmdr010) RETURNING g_xmdr_d[l_ac].xmdr010_desc
        
        LET l_ac = l_ac + 1
        IF l_ac > g_max_rec THEN
           CALL cl_err( '', 9035, 0 )
           EXIT FOREACH
        END IF        
   END FOREACH
   #再抓取inag_t資料，排除已存在xmdr_t的資料，
   LET l_sql = "SELECT inag001,inag002,'','','', ",
               "       '','','',inag004,'', ",
               "       inag005,'',inag003,inag006,inag008, ",
               "       inag007,'' ",
               "  FROM inag_t ",
               "  WHERE inagent = '",g_enterprise,"' ",
               "    AND inagsite = '",g_xmddsite,"' ",
               "    AND inag001 = '",g_xmdd001,"' ",
               "    AND inag002 = '",g_xmdd002,"' ",
               "    AND inag001||inag002||inag003||inag004||inag005||inag006||inag007 NOT IN ",
               "     (SELECT xmdr001||xmdr002||xmdr003||xmdr004||xmdr005||xmdr006||xmdr007 ",
               "        FROM xmdr_t ",
               "       WHERE xmdrent = '",g_enterprise,"' ",
               "         AND xmdrdocno = '",g_xmdddocno,"' ",
               "         AND xmdrseq = '",g_xmddseq,"' ",
               "         AND xmdrseq1 = '",g_xmddseq1,"' ",
               "         AND xmdrseq2 = '",g_xmddseq2,"') "        
   PREPARE adbt500_inag_sel FROM l_sql
   DECLARE adbt500_inag_per CURSOR FOR adbt500_inag_sel 
   FOREACH adbt500_inag_per INTO g_xmdr_d[l_ac].*
        IF SQLCA.sqlcode THEN
           CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
           EXIT FOREACH
        END IF
        #排除可備置量(庫存量-已在揀量-已備置量)小於等於0的資料
        #已在揀量inan010+已備置量inan010
        LET l_inan010 = 0
        SELECT SUM(inan010)  INTO l_inan010
          FROM inan_t
         WHERE inanent = g_enterprise
           AND inansite = g_xmddsite
           AND inan001 = g_xmdr_d[l_ac].xmdr001
           AND inan002 = g_xmdr_d[l_ac].xmdr002       
           AND inan003 = g_xmdr_d[l_ac].xmdr003
           AND inan004 = g_xmdr_d[l_ac].xmdr004
           AND inan005 = g_xmdr_d[l_ac].xmdr005
           AND inan006 = g_xmdr_d[l_ac].xmdr006            
           AND inan007 = g_xmdr_d[l_ac].xmdr007
        IF cl_null(l_inan010) THEN LET l_inan010 = 0 END IF  
        IF g_xmdr_d[l_ac].inag008 - l_inan010 = 0 THEN
           CONTINUE FOREACH
        END IF        
        LET g_xmdr_d[l_ac].sel = 'N'
        
        #備置量
        LET g_xmdr_d[l_ac].xmdr008 = 0
        #備置已沖銷量
        LET g_xmdr_d[l_ac].xmdr009 = 0
        #備置單位
        LET g_xmdr_d[l_ac].xmdr010 = g_xmdd004
        
        #可備置量 = 庫存量(inag008)-已在揀量inan010-已備置量inan010
        LET g_xmdr_d[l_ac].yxmdr008 = g_xmdr_d[l_ac].inag008 - l_inan010
        #轉換備置單位的數量
        IF NOT cl_null(g_xmdr_d[l_ac].xmdr007) THEN
           CALL s_aooi250_convert_qty(g_xmdr_d[l_ac].xmdr001,g_xmdr_d[l_ac].xmdr007,g_xmdr_d[l_ac].xmdr010,g_xmdr_d[l_ac].yxmdr008)
                RETURNING l_success,g_xmdr_d[l_ac].yxmdr008 
        END IF
        
        CALL adbt500_06_xmdr004_ref(g_xmdr_d[l_ac].xmdr004) RETURNING g_xmdr_d[l_ac].xmdr004_desc 
        CALL adbt500_06_xmdr005_ref(g_xmdr_d[l_ac].xmdr004,g_xmdr_d[l_ac].xmdr005) RETURNING g_xmdr_d[l_ac].xmdr005_desc 
        CALL adbt500_06_unit_ref(g_xmdr_d[l_ac].xmdr007) RETURNING g_xmdr_d[l_ac].xmdr007_desc         
        CALL adbt500_06_unit_ref(g_xmdr_d[l_ac].xmdr010) RETURNING g_xmdr_d[l_ac].xmdr010_desc
        
        LET l_ac = l_ac + 1
        IF l_ac > g_max_rec THEN
           CALL cl_err( '', 9035, 0 )
           EXIT FOREACH
        END IF 
    END FOREACH  
   CALL g_xmdr_d.deleteElement(g_xmdr_d.getLength())
   LET g_rec_b = l_ac - 1     
END FUNCTION]]>
</point>
  <point name="function.adbt500_06_unit_ref" cite_std="N" status="" ver="1" src="s" new="Y" order="2" mark_hard="N">
<![CDATA[#單位名稱顯示
PRIVATE FUNCTION adbt500_06_unit_ref(p_xmdr007)
DEFINE p_xmdr007      LIKE xmdr_t.xmdr007
DEFINE r_xmdr007_desc LIKE oocal_t.oocal003

       INITIALIZE g_ref_fields TO NULL
       LET g_ref_fields[1] = p_xmdr007
       CALL ap_ref_array2(g_ref_fields,"SELECT oocal003 FROM oocal_t WHERE oocalent='"||g_enterprise||"' AND oocal001=? AND oocal002='"||g_dlang||"'","") RETURNING g_rtn_fields
       LET r_xmdr007_desc = '', g_rtn_fields[1] , ''
       RETURN r_xmdr007_desc
END FUNCTION]]>
</point>
  <point name="function.adbt500_06_xmdr004_ref" cite_std="N" status="" ver="1" src="s" new="Y" order="3" mark_hard="N">
<![CDATA[#庫位說明顯示
PRIVATE FUNCTION adbt500_06_xmdr004_ref(p_xmdr004)
DEFINE p_xmdr004      LIKE xmdr_t.xmdr004
DEFINE r_inaa002      LIKE inaa_t.inaa002

       INITIALIZE g_ref_fields TO NULL
       LET g_ref_fields[1] = p_xmdr004
       CALL ap_ref_array2(g_ref_fields,"SELECT inaa002 FROM inaa_t WHERE inaaent='"||g_enterprise||"' AND inaa001=? ","") RETURNING g_rtn_fields
       LET r_inaa002 = '', g_rtn_fields[1] , ''
       RETURN r_inaa002
END FUNCTION]]>
</point>
  <point name="function.adbt500_06_xmdr005_ref" cite_std="N" status="" ver="1" src="s" new="Y" order="4" mark_hard="N">
<![CDATA[#儲位說明顯示
PRIVATE FUNCTION adbt500_06_xmdr005_ref(p_xmdr004,p_xmdr005)
DEFINE p_xmdr004      LIKE xmdr_t.xmdr004
DEFINE p_xmdr005      LIKE xmdr_t.xmdr005
DEFINE r_inab003      LIKE inab_t.inab003

       INITIALIZE g_ref_fields TO NULL
       LET g_ref_fields[1] = p_xmdr005
       CALL ap_ref_array2(g_ref_fields,"SELECT inab003 FROM inab_t WHERE inabent='"||g_enterprise||"' AND inab001='"||p_xmdr004||"' AND inab002=?  ","") RETURNING g_rtn_fields
       LET r_inab003 = '', g_rtn_fields[1] , ''
       RETURN r_inab003
END FUNCTION]]>
</point>
