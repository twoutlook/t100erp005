DEFINE g_xmdr        DYNAMIC ARRAY OF RECORD     #已勾選的備置
       xmdr008          LIKE xmdr_t.xmdr008,
       xmdr009          LIKE xmdr_t.xmdr009,
       xmdr010          LIKE xmdr_t.xmdr010,
       xmdr004          LIKE xmdr_t.xmdr004, 
