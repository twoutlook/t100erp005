         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF  

   #MRP凍結否
   IF p_xmej_d.xmej901 = '3' OR (p_xmej_d.xmej901 = '2' AND
      ((p_xmej_d.xmej006 <> l_xmdf.xmdf006) OR
