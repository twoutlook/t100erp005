            END IF
            SELECT xmee201
              INTO l_xmee201
              FROM xmee_t
             WHERE xmeeent = g_enterprise
               AND xmeedocno = p_xmejdocno
               AND xmee900 = p_xmej900
            IF NOT cl_null(l_xmee201) THEN
               CALL cl_set_comp_required("xmej200,xmej201",TRUE)
            ELSE
               CALL cl_set_comp_required("xmej200,xmej201",FALSE)
