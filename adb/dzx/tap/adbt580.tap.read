  <point name="function.adbt580_conf_input" order="13" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 簽退單確認input
# Memo...........:
# Usage..........: CALL adbt580_conf_input()
#                  RETURNING r_success
# Input parameter: 
#                : 
# Return code....: r_success  執行結果
#                : 
# Date & Author..: 140422 By earl
# Modify.........:
################################################################################
PRIVATE FUNCTION adbt580_conf_input()
   DEFINE r_success       LIKE type_t.num5
   DEFINE l_para_data     LIKE type_t.chr80          #接參數用
   
   LET r_success = TRUE
   
   INPUT BY NAME g_xmdk_m.xmdk001 ATTRIBUTE(WITHOUT DEFAULTS)

      BEFORE INPUT         
         IF cl_null(g_xmdk_m.xmdk001) THEN
            LET g_xmdk_m.xmdk001 = g_today
         END IF
         DISPLAY BY NAME g_xmdk_m.xmdk001

      AFTER FIELD xmdk001
         IF NOT cl_null(g_xmdk_m.xmdk001) THEN
            IF g_xmdk_m.xmdk001 < g_xmdk_m.xmdkdocdt THEN
               CALL cl_err(g_xmdk_m.xmdk001,'axm-00268',1)
               LET g_xmdk_m.xmdk001 = g_xmdk_m_t.xmdk001
               NEXT FIELD CURRENT
            END IF
                  
            CALL cl_get_para(g_enterprise,g_site,'S-MFG-0031') RETURNING l_para_data
            IF g_xmdk_m.xmdk001 < l_para_data THEN
               CALL cl_err(g_xmdk_m.xmdk001,'axm-00077',1)
               LET g_xmdk_m.xmdk001 = g_xmdk_m_t.xmdk001
               NEXT FIELD CURRENT
            END IF
         END IF
            
     AFTER INPUT
         IF INT_FLAG THEN
            EXIT INPUT
         END IF

         ON ACTION accept
            ACCEPT INPUT

         ON ACTION cancel
            LET INT_FLAG = TRUE
            EXIT INPUT

         ON ACTION exit
            LET INT_FLAG = TRUE
            EXIT INPUT

         ON ACTION close
            LET INT_FLAG = TRUE
            EXIT INPUT

   END INPUT

   IF INT_FLAG THEN
      LET INT_FLAG = FALSE
      LET r_success = FALSE
      RETURN r_success
   END IF

   UPDATE xmdk_t
      SET xmdk001 = g_xmdk_m.xmdk001
    WHERE xmdkent = g_enterprise
      AND xmdkdocno = g_xmdk_m.xmdkdocno

   IF SQLCA.sqlcode THEN
      CALL cl_err("UPDATE:",SQLCA.sqlcode,g_error_show)
      LET r_success = FALSE
      RETURN r_success
   END IF
  
   RETURN r_success 
END FUNCTION]]>
  </point>
  <point name="function.adbt580_xmdk007_chk" order="14" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 
# Memo...........:
# Usage..........: CALL adbt580_xmdk007_chk()
#                : RETURNING r_success
# Input parameter: 無
# Return code....: r_success
# Date & Author..: 2014/05/28 By pomelo
# Modify.........:
################################################################################
PUBLIC FUNCTION adbt580_xmdk007_chk()
DEFINE r_success LIKE type_t.num5
DEFINE l_success LIKE type_t.num5
DEFINE l_flag    LIKE type_t.num5

   LET r_success = TRUE
   
   INITIALIZE g_chkparam.* TO NULL
   LET g_chkparam.arg1 = g_xmdk_m.xmdk007
   IF NOT cl_chk_exist("v_pmaa001_3") THEN
      LET r_success = FALSE
      RETURN r_success
   END IF
               
   #檢核輸入的收款客戶是否在單據別限制範圍內，若不在限制內則不允許輸入此客戶編號
   LET l_success = ''
   LET l_flag = ''
   CALL s_control_chk_doc('3',g_xmdk_m.xmdkdocno,g_xmdk_m.xmdk007,'','','','') RETURNING l_success,l_flag
   IF NOT l_success THEN      #處理狀況
      LET r_success = FALSE
      RETURN r_success
   ELSE
      IF NOT l_flag THEN      #是否存在
         CALL cl_err(g_xmdk_m.xmdk007,'apm-00239',1)
         LET r_success = FALSE
         RETURN r_success
      END IF 
   END IF
               
   #判斷輸入的客戶編號是否在銷售控制組限制的供應商範圍內，若不在限制內則不允許跟此客戶訂購  
   LET l_success = ''
   LET l_flag = ''
   CALL s_control_chk_group('1','2',g_user,g_dept,g_xmdk_m.xmdk007,'','','','') RETURNING l_success,l_flag
   IF NOT l_success THEN      #處理狀況
      LET r_success = FALSE
      RETURN r_success
   ELSE
      IF NOT l_flag THEN      #是否存在
         CALL cl_err(g_xmdk_m.xmdk007,'axm-00007',1)
         LET r_success = FALSE
         RETURN r_success
      END IF 
   END IF
   
