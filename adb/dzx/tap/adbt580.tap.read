                  g_xmdl_d[g_detail_idx].xmdl011,g_xmdl_d[g_detail_idx].xmdl012,g_xmdl_d[g_detail_idx].xmdl204, 
                  g_xmdl_d[g_detail_idx].xmdl205,g_xmdl_d[g_detail_idx].xmdl206,g_xmdl_d[g_detail_idx].xmdl017, 
                  g_xmdl_d[g_detail_idx].xmdl018,g_xmdl_d[g_detail_idx].xmdl081,g_xmdl_d[g_detail_idx].xmdl084, 
                  g_xmdl_d[g_detail_idx].xmdl019,g_xmdl_d[g_detail_idx].xmdl020,g_xmdl_d[g_detail_idx].xmdl082, 
                  g_xmdl_d[g_detail_idx].xmdl010,g_xmdl_d[g_detail_idx].xmdl013,g_xmdl_d[g_detail_idx].xmdl014, 
