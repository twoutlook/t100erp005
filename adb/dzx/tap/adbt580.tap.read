  <point name="statechange.b_update" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[   CALL cl_showmsg_init()
   CALL s_transaction_begin()
   OPEN adbt580_cl USING g_enterprise,g_xmdk_m.xmdkdocno
   IF STATUS THEN
      CALL cl_err("OPEN adbt580_cl:", STATUS, 1)
      CLOSE adbt580_cl
      CALL s_transaction_end('N','0')
      CALL cl_showmsg()
      RETURN
   END IF
   IF lc_state = 'Y' THEN
      CALL s_adbt580_conf_chk(g_xmdk_m.xmdkdocno) RETURNING l_success
      IF NOT l_success THEN
         CALL s_transaction_end('N','0')
         CALL cl_showmsg()
         RETURN
      ELSE
         IF NOT cl_ask_confirm('aim-00108') THEN
            CALL s_transaction_end('Y','0')
            CALL cl_showmsg()
            RETURN
         ELSE      
            IF NOT adbt580_conf_input() THEN   #Input
               CALL s_transaction_end('N','0')
               CALL cl_showmsg()
               RETURN
            END IF
            CALL s_adbt580_conf_upd(g_xmdk_m.xmdkdocno) RETURNING l_success
            IF NOT l_success THEN
               CALL s_transaction_end('N','0')
               CALL cl_showmsg()
               RETURN
            ELSE
               CALL s_transaction_end('Y','0')
               CALL cl_showmsg()
            END IF
         END IF
      END IF
   END IF
       
   IF lc_state = 'X' THEN
      CALL s_adbt580_invalid_chk(g_xmdk_m.xmdkdocno) RETURNING l_success
      IF NOT l_success THEN
         CALL s_transaction_end('N','0')
         CALL cl_showmsg()
         RETURN
      ELSE
         IF NOT cl_ask_confirm('aim-00109') THEN
            CALL s_transaction_end('Y','0')
            CALL cl_showmsg()
            RETURN
         ELSE
            CALL s_transaction_begin()
            CALL s_adbt580_invalid_upd(g_xmdk_m.xmdkdocno) RETURNING l_success
            IF NOT l_success THEN
               CALL s_transaction_end('N','0')
               CALL cl_showmsg()
               RETURN
            ELSE
               CALL s_transaction_end('Y','0')
               CALL cl_showmsg()
            END IF
         END IF
      END IF
   END IF  
    
   IF lc_state = 'N' THEN
      CALL s_adbt580_unconf_chk(g_xmdk_m.xmdkdocno) RETURNING l_success
      IF NOT l_success THEN
         CALL s_transaction_end('N','0')
         CALL cl_showmsg()
         RETURN
      ELSE
         IF NOT cl_ask_confirm('aim-00110') THEN
            CALL s_transaction_end('Y','0')
            CALL cl_showmsg()
            RETURN
         ELSE
            CALL s_transaction_begin()
            CALL s_adbt580_unconf_upd(g_xmdk_m.xmdkdocno) RETURNING l_success
            IF NOT l_success THEN
               CALL s_transaction_end('N','0')
               CALL cl_showmsg()
               RETURN
            ELSE
               CALL s_transaction_end('Y','0')
               CALL cl_showmsg()
            END IF
         END IF
      END IF
   END IF  ]]>
  </point>
