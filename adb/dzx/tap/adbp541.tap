  <point name="function.adbp541_set_entry_b" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
PRIVATE FUNCTION adbp541_set_entry_b()
   CALL cl_set_comp_entry("b_xmdh203,b_xmdh019",TRUE)
END FUNCTION]]>
