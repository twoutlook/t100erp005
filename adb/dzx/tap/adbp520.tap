  <point name="function.adbp520_set_no_entry_b" cite_std="N" status="d" ver="" src="s" new="Y" order="3">
<![CDATA[
PRIVATE FUNCTION adbp520_set_no_entry_b(p_cmd)
   DEFINE l_xmdc022      LIKE xmdc_t.xmdc022
   
   LET l_xmdc022 = ''
   SELECT xmdc022 INTO l_xmdc022 FROM xmdc_t
    WHERE xmdcent = g_enterprise
      AND xmdcdocno = g_detail_d[l_ac].xmdadocno
      AND xmdcseq   = g_detail_d[l_ac].xmddseq
   IF l_xmdc022 = 'N' THEN
      CALL cl_set_comp_entry("b_xmdd202",FALSE)
   END IF
END FUNCTION]]>
</point>
  <point name="function.adbp520_chk_xmdd202" cite_std="N" status="d" ver="" src="s" new="Y" order="4">
<![CDATA[#檢查可轉出通數量
#當 p_cmd = '1' 時,僅檢查數量是否在範圍內
#當 p_cmd = '2' 時,則是在b_fill單身時,給預設值的動作
PRIVATE FUNCTION adbp520_chk_xmdd202(p_cmd)
   DEFINE l_num         LIKE xmdd_t.xmdd006
   DEFINE l_num1        LIKE xmdd-t.xmdd006
   DEFINE l_xmdd004     LIKE xmdd_t.xmdd004
   DEFINE l_xmdd006     LIKE xmdd_t.xmdd006
   DEFINE l_xmdd031     LIKE xmdd_t.xmdd031
   DEFINE l_sum        LIKE xmdd_t.xmdd006
   
   LET l_xmdd006 = 0
   LET l_xmdd031 = 0
   SELECT xmdd006,xmdd031 INTO l_xmdd006,l_xmdd031 
     FROM xmdd_t
    WHERE xmddent = g_enterprise
      AND xmdddocno = g_detail_d[l_ac].xmdadocno
      AND xmddseq   = g_detail_d[l_ac].xmddseq
      AND xmddseq1  = g_detail_d[l_ac].xmddseq1
      AND xmddseq2  = g_detail_d[l_ac].xmddseq2
      
   LET l_sum = 0
   SELECT SUM(xmdh016) INTO l_sum
     FROM xmdh_t,xmdg_t
    WHERE xmdhent = xmdgent
      AND xmdhdocno = xmdgdocno
      AND NOT ( xmdgstus = 'Y' OR xmdgstus = 'X' )
      AND xmdhent = g_enterprise
      AND xmdh001 = g_detail_d[l_ac].xmdadocno
      AND xmdh002 = g_detail_d[l_ac].xmddseq
      AND xmdh003 = g_detail_d[l_ac].xmddseq1
      AND xmdh004 = g_detail_d[l_ac].xmddseq2

   LET l_num = l_xmdd006 - l_xmdd031 - l_sum
   CALL 
   IF g_detail_d[l_ac].xmdd202 > l_num THEN
      LET g_errno = 'adb-00122'
   END IF
   
END FUNCTION]]>
</point>
