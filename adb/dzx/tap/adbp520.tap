  <point name="ui_dialog.more_input" cite_std="N" status="u" ver="" src="s" new="Y">
<![CDATA[         INPUT g_xmda005 FROM xmda005
            BEFORE INPUT
               CALL cl_set_act_visible("batch_execute", TRUE)
               CALL cl_set_act_visible("selall,selnone,sel,unsel",FALSE)
               CALL cl_set_act_visible("filter",FALSE)
         END INPUT
         
         INPUT ARRAY g_detail_d FROM s_detail1.*
             ATTRIBUTE(COUNT = g_detail_cnt,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS,
                     INSERT ROW = FALSE,DELETE ROW = FALSE, APPEND ROW = FALSE)
            BEFORE INPUT               
               CALL cl_set_act_visible("batch_execute",FALSE)
               CALL cl_set_act_visible("selall,selnone,sel,unsel",TRUE)  
               CALL cl_set_act_visible("filter",FALSE)
               
            BEFORE ROW
               LET l_ac = DIALOG.getCurrentRow("s_detail1")
               LET g_detail_d_t.* = g_detail_d[l_ac].*
               DISPLAY l_ac TO FORMONLY.h_index
               CALL adbp520_set_entry_b()    
               CALL adbp520_set_no_entry_b()   
               
            AFTER FIELD b_xmdd025         
               IF NOT cl_ap_chk_Range(g_detail_d[l_ac].xmdd025,"0.000","0","","","azz-00079",1) THEN
                  LET g_detail_d[l_ac].xmdd025 = g_detail_d_t.xmdd025
                  NEXT FIELD b_xmdd025
               END IF
               
            AFTER FIELD b_xmdd202    
               IF NOT cl_ap_chk_Range(g_detail_d[l_ac].xmdd202,"0.000","0","","","azz-00079",1) THEN
                  LET g_detail_d[l_ac].xmdd202 = g_detail_d_t.xmdd202
                  NEXT FIELD b_xmdd202
               END IF
               
               IF NOT cl_null(g_detail_d[l_ac].xmdd202) THEN
                  CALL s_adbt520_xmdd006_xmdd031(g_detail_d[l_ac].xmdadocno, g_detail_d[l_ac].xmddseq,g_detail_d[l_ac].xmddseq1,
                                                 g_detail_d[l_ac].xmddseq2 ,0,0) RETURNING l_xmdh016
                  LET l_xmdd004 = ''
                  SELECT xmdd004 INTO l_xmdd004 FROM xmdd_t
                   WHERE xmddent   = g_enterprise
                     AND xmdddocno = g_detail_d[l_ac].xmdadocno
                     AND xmddseq   = g_detail_d[l_ac].xmddseq
                     AND xmddseq1  = g_detail_d[l_ac].xmddseq1
                     AND xmddseq2  = g_detail_d[l_ac].xmddseq2
                  CALL s_aooi250_convert_qty(g_detail_d[l_ac].xmdd001,l_xmdd004,g_detail_d[l_ac].xmdd201,l_xmdh016) RETURNING l_success,l_xmdh016
                  IF l_success THEN
                     IF g_detail_d[l_ac].xmdd202 > l_xmdh016 THEN
                        CALL cl_err(g_detail_d[l_ac].xmdd202,'adb-00122',1)
                        LET g_detail_d[l_ac].xmdd202 = g_detail_d_t.xmdd202
                        NEXT FIELD b_xmdd202
                     END IF                     
                  END IF
               END IF
            
            ON CHANGE sel
               LET l_ac = DIALOG.getCurrentRow("s_detail1")
               CALL adbp520_upd_s_adbp520_temp1()      
               
            ON ROW CHANGE
               LET l_ac = DIALOG.getCurrentRow("s_detail1")
               CALL adbp520_upd_s_adbp520_temp1()
               
            ON ACTION accept 
               ACCEPT DIALOG
               
         END INPUT ]]>
</point>
