  <point name="ui_dialog.more_action" cite_std="N" status="u" ver="" src="s" new="Y">
<![CDATA[         ON ACTION batch_execute 
            LET l_cnt = 0
            SELECT COUNT(*)
              INTO l_cnt
              FROM s_adbp520_temp1
             WHERE sel = 'Y'
            IF l_cnt > 0 THEN
               CALL s_transaction_begin()
               CALL s_adbp520('1','') RETURNING l_success,l_str
               IF l_success THEN
                  CALL s_transaction_end('Y','1')

                  LET la_param.prog = 'adbt520'
                  LET la_param.param[1] = ''
                  LET la_param.param[2] = l_str
                  LET ls_js = util.JSON.stringify(la_param )
                  CALL cl_cmdrun_wait(ls_js)
               ELSE
                  CALL s_transaction_end('N','1')
               END IF
               DELETE FROM s_adbp520_temp2
               CALL adbp520_b_fill()
            ELSE
               CALL cl_err('','adb-00078',1)
               CONTINUE DIALOG
            END IF]]>
</point>
