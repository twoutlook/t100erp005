  <section id="adbi255.detail_reproduce" ver="4" status="" src="s">
<![CDATA[#+ 單身自動複製
PRIVATE FUNCTION adbi255_detail_reproduce()
   DEFINE ls_sql      STRING
   DEFINE ld_date     DATETIME YEAR TO SECOND
   DEFINE l_detail    RECORD LIKE dbaf_t.*
 
 
   #add-point:delete段define
   {<point name="detail_reproduce.define"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   
   LET ld_date = cl_get_current()
   
   DROP TABLE adbi255_detail
   
   #add-point:單身複製前1
   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE adbi255_detail AS ",
                "SELECT * FROM dbaf_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO adbi255_detail SELECT * FROM dbaf_t 
                                         WHERE dbafent = g_enterprise AND dbaf001 = g_dbaf001_t
 
   
   #將key修正為調整後   
   UPDATE adbi255_detail 
      #更新key欄位
      SET dbaf001 = g_dbaf_m.dbaf001
 
      #更新共用欄位
      #此段落由子樣板a13產生
       , dbafownid = g_user
       , dbafowndp = g_dept
       , dbafcrtid = g_user
       , dbafcrtdp = g_dept 
       , dbafcrtdt = ld_date
       , dbafmodid = "" 
       , dbafmoddt = "" 
      #, dbafstus = "Y"
 
 
                                       
  
   #將資料塞回原table   
   INSERT INTO dbaf_t SELECT * FROM adbi255_detail
   
   IF SQLCA.sqlcode THEN
      CALL cl_err("reproduce",SQLCA.sqlcode,1)
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE adbi255_detail
   
   #add-point:單身複製後1
   {<point name="detail_reproduce.body.table1.a_insert"/>}
   #end add-point
 
 
   
 
   
   #多語言複製段落
   
   
   CALL s_transaction_end('Y','0')
   
   #已新增完, 調整資料內容(修改時使用)
   LET g_dbaf001_t = g_dbaf_m.dbaf001
 
   
   DROP TABLE adbi255_detail
   
END FUNCTION
]]>
