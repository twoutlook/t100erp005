            #給予arg
            LET g_qryparam.arg1 = "" #

            
            CALL q_dbad001()                                #呼叫開窗

            LET g_dbaf3_d[l_ac].dbaf0031 = g_qryparam.return1              

            DISPLAY g_dbaf3_d[l_ac].dbaf0031 TO dbaf0031              #

            NEXT FIELD dbaf0031                          #返回原欄位

]]>
