  <point name="b_fill.sql_before" order="" ver="1" cite_std="N" new="N" status="" src="s" mark_hard="N">
    <![CDATA[   DELETE FROM s_adbp540_temp1
   LET g_sql = "SELECT      'N', xmda202,     '',xmdadocno, xmda005,",
               "        xmda004,      '',xmda021,       '', xmdd211,", 
               "             '',xmddunit,     '',  xmdd229, xmddseq,",
               "       xmddseq1,xmddseq2, xmdd001,      '',       '',",
               "        xmdd002, xmdd201,      '', xmdd202,  xmdd024,",
               "             '', xmdd025, xmdd011, xmdd207,       '',",
               "        xmdd208,      '', xmdd209,      '',  xmdd031,",
               "        xmdd014, xmdd015, xmdd016, xmdd017,  xmdd220,",
               "             '', xmdd221, xmdd004, xmdd006,  xmdasite ",
               "  FROM xmda_t,xmdd_t ",
               " WHERE xmdaent = xmddent AND xmdadocno = xmdddocno ",
               "   AND xmddent = ? ",
               "   AND ",g_wc CLIPPED,
               "   AND ",g_wc2 CLIPPED ,
               "   AND xmda005 = '",g_xmda005,"'",
               "   AND xmddunit = '",g_xmddunit,"'",
