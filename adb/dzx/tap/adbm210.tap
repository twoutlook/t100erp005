            IF NOT cl_null(g_mrba_m.mrba010) THEN
               IF p_cmd = 'a' OR (p_cmd = 'u' AND (g_mrba_m.mrba010 != g_mrba_m_t.mrba010 OR g_mrba_m_t.mrba010 IS NULL )) THEN
                  #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
