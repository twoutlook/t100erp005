                  IF NOT cl_chk_exist("v_oocq002_4") THEN
                     LET g_mrba_m.mrba010 = g_mrba_m_t.mrba010
                     CALL s_desc_get_acc_desc('1101',g_mrba_m.mrba010)
                        RETURNING g_mrba_m.mrba010_desc
                     DISPLAY BY NAME g_mrba_m.mrba010_desc
                     NEXT FIELD CURRENT
