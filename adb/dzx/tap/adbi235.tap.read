  <point name="reproduce.head.a_insert" order="" ver="1" cite_std="N" new="N" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[   LET l_success = ''
   LET l_inaa004 = ''
   CALL s_aooi350_ins_oofa('8',l_newno,'') RETURNING l_success,l_inaa004
   IF NOT l_success THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend = "oofa_t"
      LET g_errparam.popup = TRUE
      CALL cl_err()
 
      CALL s_transaction_end('N','0')      
      RETURN
   ELSE
      UPDATE inaa_t SET inaa004 = l_inaa004
       WHERE inaaent = g_enterprise AND  inaasite = g_site AND inaa001 = l_newno
      IF SQLCA.SQLcode  THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "inaa_t"
         LET g_errparam.popup = TRUE
         CALL cl_err()
  
         CALL s_transaction_end('N','0')      
         RETURN
      END IF
   END IF
   #複製聯絡地址
   LET l_n = 0
   SELECT COUNT(*) INTO l_n FROM oofb_t WHERE oofbent = g_enterprise AND oofb002 = l_master.inaa004
   IF l_n > 0 THEN
      CALL adbi235_oofb_insert(l_master.inaa004,l_inaa004) RETURNING l_success
      IF NOT l_success THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "oofb_t"
         LET g_errparam.popup = TRUE
         CALL cl_err()

         CALL s_transaction_end('N','0')
         RETURN
      END IF
   END IF

   #複製通訊方式
   LET l_n = 0
   SELECT COUNT(*) INTO l_n FROM oofc_t WHERE oofcent = g_enterprise AND oofc002 = l_master.inaa004
   IF l_n > 0 THEN
      CALL adbi235_oofc_insert(l_master.inaa004,l_inaa004) RETURNING l_success
      IF NOT l_success THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "oofc_t"
         LET g_errparam.popup = TRUE
         CALL cl_err()

         CALL s_transaction_end('N','0')
         RETURN
      END IF
   END IF
   
   LET g_sql = "SELECT * FROM inac_t WHERE inacent = '" ||g_enterprise|| "' AND inacsite = '" ||g_site|| "' AND ",
               " inac001 = '",g_inaa_m.inaa001,"'"

   DECLARE adbi235_reproduce2 CURSOR FROM g_sql
 
   FOREACH adbi235_reproduce2 INTO l_detai2.*
      LET l_detai2.inac001 = l_newno
      
      INSERT INTO inac_t VALUES (l_detai2.*) #複製單身
      
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = 'Insert error!'
         LET g_errparam.popup = TRUE
         CALL cl_err()

         CALL s_transaction_end('N','0')
         RETURN
      END IF
      
   END FOREACH
   ]]>
  </point>
