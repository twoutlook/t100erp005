                  CALL cl_show_fld_cont()
                  #CALL adbi235_inac_fill()
               END IF
            ELSE
               LET l_cmd='a'
            END IF

         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' THEN
               CALL FGL_SET_ARR_CURR(l_ac2-1)
               CALL g_inab2_d.deleteElement(l_ac2)
               NEXT FIELD inac003
            END IF
            IF NOT cl_null(g_inab2_d[l_ac2].inac003) THEN
