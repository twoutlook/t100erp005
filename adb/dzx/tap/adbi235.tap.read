  <section id="adbi235.modify" ver="24" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION adbi235_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   DEFINE l_wc2_table1          STRING
 
 
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point    
   #add-point:modify段define(客製用)
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point    
   
   #保存單頭舊值
   LET g_inaa_m_t.* = g_inaa_m.*
   LET g_inaa_m_o.* = g_inaa_m.*
   
   IF g_inaa_m.inaasite IS NULL
   OR g_inaa_m.inaa001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_inaasite_t = g_inaa_m.inaasite
   LET g_inaa001_t = g_inaa_m.inaa001
 
   CALL s_transaction_begin()
   
   OPEN adbi235_cl USING g_enterprise,g_inaa_m.inaasite,g_inaa_m.inaa001
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN adbi235_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE adbi235_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE adbi235_master_referesh USING g_inaa_m.inaasite,g_inaa_m.inaa001 INTO g_inaa_m.inaasite,g_inaa_m.inaa001, 
       g_inaa_m.inaa005,g_inaa_m.inaa006,g_inaa_m.inaaunit,g_inaa_m.inaa007,g_inaa_m.inaa008,g_inaa_m.inaa009, 
       g_inaa_m.inaa010,g_inaa_m.inaa014,g_inaa_m.inaa015,g_inaa_m.inaa011,g_inaa_m.inaa012,g_inaa_m.inaa013, 
       g_inaa_m.inaastus,g_inaa_m.inaaownid,g_inaa_m.inaaowndp,g_inaa_m.inaacrtid,g_inaa_m.inaacrtdp, 
       g_inaa_m.inaacrtdt,g_inaa_m.inaamodid,g_inaa_m.inaamoddt,g_inaa_m.inaasite_desc,g_inaa_m.inaa005_desc, 
       g_inaa_m.inaaownid_desc,g_inaa_m.inaaowndp_desc,g_inaa_m.inaacrtid_desc,g_inaa_m.inaacrtdp_desc, 
       g_inaa_m.inaamodid_desc
   
   #檢查是否允許此動作
   IF NOT adbi235_action_chk() THEN
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_inaa_m_mask_o.* =  g_inaa_m.*
   CALL adbi235_inaa_t_mask()
   LET g_inaa_m_mask_n.* =  g_inaa_m.*
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
 
   
   CALL adbi235_show()
   #add-point:modify段show之後
   {<point name="modify.after_show"/>}
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
 
 
    
   WHILE TRUE
      LET g_inaasite_t = g_inaa_m.inaasite
      LET g_inaa001_t = g_inaa_m.inaa001
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_inaa_m.inaamodid = g_user 
LET g_inaa_m.inaamoddt = cl_get_current()
LET g_inaa_m.inaamodid_desc = cl_get_username(g_inaa_m.inaamodid)
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      CALL adbi235_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE inaa_t SET (inaamodid,inaamoddt) = (g_inaa_m.inaamodid,g_inaa_m.inaamoddt)
          WHERE inaaent = g_enterprise AND inaasite = g_inaasite_t
            AND inaa001 = g_inaa001_t
 
      END IF
    
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_inaa_m.* = g_inaa_m_t.*
         CALL adbi235_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_inaa_m.inaasite != g_inaasite_t 
      OR g_inaa_m.inaa001 != g_inaa001_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE inab_t SET inabsite = g_inaa_m.inaasite
                                       ,inab001 = g_inaa_m.inaa001
 
          WHERE inabent = g_enterprise AND inabsite = g_inaasite_t
            AND inab001 = g_inaa001_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "inab_t" 
               LET g_errparam.code   = "std-00009" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "inab_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL adbi235_set_act_visible()   
   CALL adbi235_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " inaaent = '" ||g_enterprise|| "' AND",
                      " inaasite = '", g_inaa_m.inaasite, "' "
                      ," AND inaa001 = '", g_inaa_m.inaa001, "' "
 
   #填到對應位置
   CALL adbi235_browser_fill("")
 
   CLOSE adbi235_cl
   
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL adbi235_msgcentre_notify('')
 
END FUNCTION   
]]>
  </section>
