                  ELSE
                     LET g_flag = FALSE
                     NEXT FIELD inac003
                  END IF
               END IF             
            ELSE
               LET g_inab2_d[l_ac2].inac003 = g_qryparam.return1
               DISPLAY g_inab2_d[l_ac2].inac003 TO inac003              #顯示到畫面上
               CALL adbi235_inac003_ref(g_inab2_d[l_ac2].inac003) RETURNING g_inab2_d[l_ac2].inac003_desc
               DISPLAY BY NAME g_inab2_d[l_ac2].inac003_desc
