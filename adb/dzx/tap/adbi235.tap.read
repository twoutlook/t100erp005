            LET g_qryparam.reqry = FALSE

            LET g_qryparam.default1 = g_inab2_d[l_ac2].inac003             #給予default值

            LET g_qryparam.arg1 = "220" #應用分類

            CALL q_oocq002_1()                                #呼叫開窗

            LET g_qryparam.where = " "
            IF l_cmd = 'a' THEN
