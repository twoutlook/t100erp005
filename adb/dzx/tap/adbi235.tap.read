                  END IF
                  #IF cl_null(g_inab_d[l_ac].inab002) THEN
                  #   LET g_inab_d[l_ac].inab002 = ' '
                  #END IF
                  CALL adbi235_inac_insert(l_inac003,g_inaa_m.inaa001,l_inab002) RETURNING l_success
                  IF l_success THEN
                     CALL adbi235_inac_fill()
                     LET g_flag = TRUE
