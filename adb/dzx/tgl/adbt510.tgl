      CLOSE adbt510_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE adbt510_master_referesh USING g_xmee_m.xmeedocno,g_xmee_m.xmee900 INTO g_xmee_m.xmeesite, 
