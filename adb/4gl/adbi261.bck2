# Modify.........:
################################################################################
PRIVATE FUNCTION adbi261_dbbg003_ref()
   DEFINE l_acc_code   LIKE oocq_t.oocq001
   DEFINE l_imaal004   LIKE imaal_t.imaal004
   
   CASE g_inaa2_d[l_ac].dbbg002
      WHEN '3'   #品牌
         LET l_acc_code = '2002'
      WHEN '4'   #系列
         LET l_acc_code = '2003'
      WHEN '5'   #型別 
         LET l_acc_code = '2004'
      WHEN '6'   #功能 
         LET l_acc_code = '2005'
      WHEN '7'   #價格帶
         LET l_acc_code = '2001'
      WHEN '8'   #其他屬性一
         LET l_acc_code = '2006'
      WHEN '9'   #其他屬性二
         LET l_acc_code = '2007'
      WHEN '10'  #其他屬性三
         LET l_acc_code = '2008'
      WHEN '11'  #其他屬性四
         LET l_acc_code = '2009'
      WHEN '12'  #其他屬性五
         LET l_acc_code = '2010'
      WHEN '13'  #其他屬性六
         LET l_acc_code = '2011'
      WHEN '14'  #其他屬性七
         LET l_acc_code = '2012'
      WHEN '15'  #其他屬性八
         LET l_acc_code = '2013'
      WHEN '16'  #其他屬性九
         LET l_acc_code = '2014'
      WHEN '17'  #其他屬性十
         LET l_acc_code = '2015'                  
   END CASE
   
   CASE g_inaa2_d[l_ac].dbbg002
      WHEN '1'   #產品編號
         CALL s_desc_get_item_desc(g_inaa2_d[l_ac].dbbg003)
            RETURNING g_inaa2_d[l_ac].dbbg003_desc,l_imaal004
      WHEN '2'   #產品品類
         CALL s_desc_get_rtaxl003_desc(g_inaa2_d[l_ac].dbbg003)
            RETURNING g_inaa2_d[l_ac].dbbg003_desc 
      OTHERWISE
         CALL s_desc_get_acc_desc(l_acc_code,g_inaa2_d[l_ac].dbbg003)
            RETURNING g_inaa2_d[l_ac].dbbg003_desc  
   END CASE
   DISPLAY BY NAME g_inaa2_d[l_ac].dbbg003_desc
END FUNCTION

 
