           RETURNING l_success,g_xmdh_d[l_ac].xmdh021
   END IF

   CALL s_tax_ins(g_xmdg_m.xmdgdocno,g_xmdh_d[l_ac].xmdhseq,0,g_xmdh_d[l_ac].xmdhunit,g_xmdh_d[l_ac].xmdh021 * g_xmdh4_d[l_ac].xmdh023,
