   CALL adbt520_detail()
   CALL adbt520_detail4()
   CALL adbt520_detail5()
   LET g_xmdh_d_o.* = g_xmdh_d[l_ac].*
   LET g_xmdh4_d_o.* = g_xmdh4_d[l_ac].*
   LET g_xmdh5_d_o.* = g_xmdh5_d[l_ac].*
