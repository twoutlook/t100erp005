   LET g_xmdh_d[l_ac].xmdh204 = g_xmdh_d[l_ac].xmdh203

   #對出貨數量進行取位
   CALL s_aooi250_take_decimals(g_xmdh_d[l_ac].xmdh202,g_xmdh_d[l_ac].xmdh203)
        RETURNING l_success,g_xmdh_d[l_ac].xmdh203
   #若料號有設置使用參考單位時且出貨單位與參考單位有設置換算率時，則應自動推算參考數量
   IF NOT cl_null(g_xmdh_d[l_ac].xmdh018) THEN
      CALL s_aooi250_convert_qty(g_xmdh_d[l_ac].xmdh006,g_xmdh_d[l_ac].xmdh015,g_xmdh_d[l_ac].xmdh018,g_xmdh_d[l_ac].xmdh016)
