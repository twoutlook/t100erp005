################################################################################
# Descriptions...: 若單據別參數設定需檢核在揀量時，則帳面庫存量-已在揀量小於申請出貨數量時，則不允許輸入此庫位
# Memo...........:
# Usage..........: CALL adbt520_inan_chk()
#                :    RETURNING r_success
# Input parameter: 無
# Return code....: r_success      TRUE/FALSE
# Date & Author..: 2014/07/03 By pomelo
# Modify.........:
################################################################################
PUBLIC FUNCTION adbt520_inan_chk()
DEFINE r_success          LIKE type_t.num5
DEFINE l_success          LIKE type_t.num5
DEFINE l_flag             LIKE type_t.num5
DEFINE l_inaa014          LIKE inaa_t.inaa014   #lori522612  150113  add

   LET r_success = TRUE
   
   #不允許負庫存時檢查在撿量                                                            #lori522612  150113  add
   LET l_inaa014 = s_adb_get_inaa014(g_xmdh_d[l_ac].xmdhunit,g_xmdh_d[l_ac].xmdh012)  #lori522612  150113  add
   CALL s_inventory_check_inan(g_xmdh_d[l_ac].xmdhunit,g_xmdh_d[l_ac].xmdh006,
                               g_xmdh_d[l_ac].xmdh007,' ',g_xmdh_d[l_ac].xmdh012,
                               g_xmdh_d[l_ac].xmdh013,g_xmdh_d[l_ac].xmdh014,
                               g_xmdh_d[l_ac].xmdh015,g_xmdh_d[l_ac].xmdh016,
                               g_xmdg_m.xmdgdocno,g_xmdh_d[l_ac].xmdhseq,'0','','') #160408-00035#9-add-'',''
        RETURNING l_success,l_flag
   IF NOT l_success THEN
      LET r_success = FALSE
   ELSE                                                                           #lori522612  150113  add 
      #IF l_flag = 0 THEN                                                         #lori522612  150113  mark
      IF l_flag = 0 AND l_inaa014 = 'N' THEN                                      #lori522612  150113  add
         LET r_success = FALSE
      END IF
   END IF
   
   RETURN r_success

END FUNCTION

################################################################################
# Descriptions...: 當出貨料號與訂單料號不一樣時，必須檢核是否與訂單料號有替代關係，若沒有則不允許修改
# Memo...........:
# Usage..........: CALL adbt520_xmdh006_xmdh007_chk()
#                :    RETURNING r_success
# Input parameter: 無
# Return code....: r_success True/False
# Date & Author..: 2014/08/08 By pomelo
# Modify.........:
################################################################################
PUBLIC FUNCTION adbt520_xmdh006_xmdh007_chk()
DEFINE r_success LIKE type_t.num5
DEFINE l_xmdd001 LIKE xmdd_t.xmdd001
DEFINE l_xmdd002 LIKE xmdd_t.xmdd002

   LET r_success = TRUE
   LET l_xmdd001 = ''
   LET l_xmdd002 = ''
   SELECT xmdd001,xmdd002
     INTO l_xmdd001,l_xmdd002
     FROM xmdd_t
    WHERE xmddent = g_enterprise
      AND xmdddocno = g_xmdh_d[l_ac].xmdh001
      AND xmddseq = g_xmdh_d[l_ac].xmdh002 
      AND xmddseq1 = g_xmdh_d[l_ac].xmdh003
      AND xmddseq2 = g_xmdh_d[l_ac].xmdh004
   IF l_xmdd001 <> g_xmdh_d[l_ac].xmdh006 OR
      l_xmdd002 <> g_xmdh_d[l_ac].xmdh007 THEN
      IF NOT s_pmaq_chk_replacement(g_xmdg_m.xmdg005,l_xmdd001,g_xmdh_d[l_ac].xmdh006,'3',l_xmdd002,g_xmdh_d[l_ac].xmdh007) THEN
         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF

   RETURN r_success
END FUNCTION

################################################################################
# Descriptions...: 清除單身帶值的欄位
# Memo...........:
# Usage..........: CALL adbt520_clear_detail(p_type)
# Input parameter: p_type   1 訂單單號欄位
#                :          2 項次
#                :          3 項序
#                :          4 分批序
# Return code....: 無
# Date & Author..: 2014/08/11 By pomelo
# Modify.........:
################################################################################
PUBLIC FUNCTION adbt520_clear_detail(p_type)
DEFINE p_type         LIKE type_t.chr1
DEFINE l_xmdhseq      LIKE xmdh_t.xmdhseq
DEFINE l_xmdh001      LIKE xmdh_t.xmdh001
DEFINE l_xmdh002      LIKE xmdh_t.xmdh002
DEFINE l_xmdh003      LIKE xmdh_t.xmdh003

   CASE p_type 
      WHEN '1'
         LET l_xmdhseq = g_xmdh_d[l_ac].xmdhseq
      WHEN '2'
         LET l_xmdhseq = g_xmdh_d[l_ac].xmdhseq
         LET l_xmdh001 = g_xmdh_d[l_ac].xmdh001
      WHEN '3'
         LET l_xmdhseq = g_xmdh_d[l_ac].xmdhseq
         LET l_xmdh001 = g_xmdh_d[l_ac].xmdh001
         LET l_xmdh002 = g_xmdh_d[l_ac].xmdh002
      WHEN '4'
         LET l_xmdhseq = g_xmdh_d[l_ac].xmdhseq
         LET l_xmdh001 = g_xmdh_d[l_ac].xmdh001
         LET l_xmdh002 = g_xmdh_d[l_ac].xmdh002
         LET l_xmdh003 = g_xmdh_d[l_ac].xmdh003
   END CASE
   
   INITIALIZE g_xmdh_d[l_ac] TO NULL 
   
   CASE p_type
      WHEN '1'
         LET g_xmdh_d[l_ac].xmdhseq = l_xmdhseq
      WHEN '2'
         LET g_xmdh_d[l_ac].xmdhseq = l_xmdhseq
         LET g_xmdh_d[l_ac].xmdh001 = l_xmdh001
      WHEN '3'
         LET g_xmdh_d[l_ac].xmdhseq = l_xmdhseq 
         LET g_xmdh_d[l_ac].xmdh001 = l_xmdh001 
         LET g_xmdh_d[l_ac].xmdh002 = l_xmdh002 
      WHEN '4'
         LET g_xmdh_d[l_ac].xmdhseq = l_xmdhseq
         LET g_xmdh_d[l_ac].xmdh001 = l_xmdh001
         LET g_xmdh_d[l_ac].xmdh002 = l_xmdh002
         LET g_xmdh_d[l_ac].xmdh003 = l_xmdh003
   END CASE
   IF NOT cl_null(g_xmdg_m.xmdg004) THEN
      LET g_xmdh_d[l_ac].xmdh001 = g_xmdg_m.xmdg004
   END IF
   LET g_xmdh_d[l_ac].xmdh005 = "1"
   LET g_xmdh_d[l_ac].xmdh011 = "N"
   LET g_xmdh_d[l_ac].xmdh022 = "Y"
   LET g_xmdh_d[l_ac].xmdh036 = "N"
   LET g_xmdh_d[l_ac].xmdh030 = "0"
   LET g_xmdh_d[l_ac].xmdh007 = ' '   #產品特徵
   LET g_xmdh_d[l_ac].xmdh012 = ' '   #限定庫位
   LET g_xmdh_d[l_ac].xmdh013 = ' '   #限定儲位
   LET g_xmdh_d[l_ac].xmdh014 = ' '   #限定批號
END FUNCTION

