         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         RETURN FALSE
      END IF
   END IF
 
   #鎖定整組table
   #LET ls_group = "'6',"
   #僅鎖定自身table
   LET ls_group = "xrcd_t"
   IF ls_group.getIndexOf(ps_table,1) THEN
   
      OPEN adbt520_bcl3 USING g_enterprise,
                                             g_xmdg_m.xmdgdocno,g_xmdh6_d[g_detail_idx].xrcdld,g_xmdh6_d[g_detail_idx].xrcdseq, 
                                                 g_xmdh6_d[g_detail_idx].xrcdseq2,g_xmdh6_d[g_detail_idx].xrcd007 
 
      IF SQLCA.sqlcode THEN
