      INITIALIZE l_xmdh.* TO NULL
      LET l_seq = l_seq + 1
      
   END FOREACH
   
   CALL s_tax_recount(g_xmdg_m.xmdgdocno)
        RETURNING l_xrcd103,l_xrcd104,l_xrcd105,l_xrcd113,l_xrcd114,l_xrcd115
