          g_xmdh_d[l_ac].xmdhunit, g_xmdh_d[l_ac].xmdh221,  g_xmdh_d[l_ac].xmdh222,  g_xmdh_d[l_ac].xmdh223,  g_xmdh_d[l_ac].xmdh224
     FROM  xmja_t,xmdc_t LEFT OUTER JOIN xmdd_t ON xmdcent = xmddent
                                               AND xmdcsite = xmddsite
                                               AND xmdcdocno = xmdddocno
                                               AND xmdcseq = xmddseq
                                               AND xmdc045 = '1'    #未結案
    WHERE xmjaent = xmdcent
      AND xmjadocno = xmdcdocno
      AND xmjaseq = xmdcseq
      AND xmddent = g_enterprise
      AND xmdddocno = g_xmdh_d[l_ac].xmdh001
      AND xmddseq = g_xmdh_d[l_ac].xmdh002
      AND xmddseq1 = g_xmdh_d[l_ac].xmdh003
      AND xmddseq2 = g_xmdh_d[l_ac].xmdh004


   CALL s_adbt520_xmdd006_xmdd031(g_xmdh_d[l_ac].xmdh001,g_xmdh_d[l_ac].xmdh002,g_xmdh_d[l_ac].xmdh003,g_xmdh_d[l_ac].xmdh004,'0',g_xmdg_m.xmdgdocno)
        RETURNING g_xmdh_d[l_ac].xmdh016
   LET g_xmdh_d[l_ac].xmdh011 = 'N'
   LET g_xmdh_d[l_ac].xmdh017 = g_xmdh_d[l_ac].xmdh016
   #對出貨數量進行取位
   CALL s_aooi250_take_decimals(g_xmdh_d[l_ac].xmdh015,g_xmdh_d[l_ac].xmdh016)
        RETURNING l_success,g_xmdh_d[l_ac].xmdh016
   #出貨單位轉包裝單位
   CALL s_aooi250_convert_qty(g_xmdh_d[l_ac].xmdh006,g_xmdh_d[l_ac].xmdh015,g_xmdh_d[l_ac].xmdh202,g_xmdh_d[l_ac].xmdh016)
