{<section id="adbt520.set_no_entry" >}
#+ 單頭欄位關閉設定
PRIVATE FUNCTION adbt520_set_no_entry(p_cmd)
   #add-point:set_no_entry段define(客製用) name="set_no_entry.define_customerization"
   
   #end add-point     
   DEFINE p_cmd   LIKE type_t.chr1   
   #add-point:set_no_entry段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="set_no_entry.define"
   DEFINE l_cnt   LIKE type_t.num5
   #end add-point     
   
   #add-point:Function前置處理  name="set_no_entry.pre_function"
   
   #end add-point
   
   IF p_cmd = 'u' AND g_chkey = 'N' THEN
      CALL cl_set_comp_entry("xmdgdocno",FALSE)
      #根據azzi850使用者身分開關特定欄位
      IF NOT cl_null(g_no_entry) THEN
         CALL cl_set_comp_entry(g_no_entry,FALSE)
      END IF
      #add-point:set_no_entry段欄位控制 name="set_no_entry.field_control"
      CALL cl_set_comp_entry("xmdgdocdt",FALSE)
      #end add-point 
   END IF 
   
   IF p_cmd = 'u' THEN  #docno,ld欄位確認是絕對關閉
      CALL cl_set_comp_entry("xmdgdocno",FALSE)
   END IF 
 
   IF p_cmd = 'u' THEN  #docdt欄位依照設定關閉(FALSE則為設定不同意修正)
      IF NOT cl_chk_update_docdt() THEN
         CALL cl_set_comp_entry("xmdgdocdt",FALSE)
      END IF
   END IF 
   
   #add-point:set_no_entry段欄位控制後 name="set_no_entry.after_control"
   CALL cl_set_comp_entry("xmdg031",FALSE)
   #將訂單對應的相關欄位預設到出通單上後不能維護
   #單身有資料時，單頭欄位不可再輸入
   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt
     FROM xmdh_t
    WHERE xmdhent = g_enterprise
      AND xmdhdocno = g_xmdg_m.xmdgdocno
   IF NOT cl_null(g_xmdg_m.xmdg004) OR l_cnt >=1 THEN
      CALL cl_set_comp_entry("xmdg001",FALSE)     #出貨性質
      CALL cl_set_comp_entry("xmdg004",FALSE)     #訂單單號
      CALL cl_set_comp_entry("xmdg005",FALSE)     #訂單客戶
      CALL cl_set_comp_entry("xmdg006",FALSE)     #收款客戶
      CALL cl_set_comp_entry("xmdg007",FALSE)     #送貨客戶
      CALL cl_set_comp_entry("xmdg008",FALSE)     #收款條件
      CALL cl_set_comp_entry("xmdg009",FALSE)     #交易條件
      CALL cl_set_comp_entry("xmdg010",FALSE)     #稅別
      #lori522612  150119  add ----------------------(S)
      #CALL cl_set_comp_entry("xmdg013",FALSE)     #發票類型
      #lori522612  150119  add ----------------------(E)
      CALL cl_set_comp_entry("xmdg014",FALSE)     #幣別
      CALL cl_set_comp_entry("xmdg017",FALSE)     #送貨地址
      CALL cl_set_comp_entry("xmdg026",FALSE)     #銷售通路
      CALL cl_set_comp_entry("xmdg027",FALSE)     #銷售分類
      CALL cl_set_comp_entry("xmdg031",FALSE)     #留置原因
      CALL cl_set_comp_entry("xmdg200",FALSE)     #調貨經銷商編號
      CALL cl_set_comp_entry("xmdg201",FALSE)     #代送商編號
      CALL cl_set_comp_entry("xmdg202",FALSE)     #發票客戶
      CALL cl_set_comp_entry("xmdgunit",FALSE)    #銷售組織
      CALL cl_set_comp_entry("xmdg026",FALSE)     #銷售渠道
      CALL cl_set_comp_entry("xmdg031",FALSE)     #匯率計算基準
      CALL cl_set_comp_entry("xmdg032",FALSE)     #內外銷
   END IF
   
   #當出貨性質 = 6調貨訂單出貨
   IF g_xmdg_m.xmdg001 = '6' THEN
      CALL cl_set_comp_entry("xmdg201",FALSE)
   ELSE
      CALL cl_set_comp_entry("xmdg200",FALSE)     #調貨經銷商編號
   END IF
   
   #aooi500設定的欄位控卡
   IF NOT s_aooi500_comp_entry(g_prog,'xmdgsite') OR g_site_flag THEN
      CALL cl_set_comp_entry("xmdgsite",FALSE)
   END IF
   IF NOT s_aooi500_comp_entry(g_prog,'xmdgunit') THEN
      CALL cl_set_comp_entry("xmdgunit",FALSE)
   END IF
   #end add-point 
 
END FUNCTION
 
