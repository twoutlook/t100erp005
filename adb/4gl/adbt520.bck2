        xmdastus LIKE xmda_t.xmdastus,   #狀態碼
        xmdasite LIKE xmda_t.xmdasite,   #銷售組織
        xmdaunit LIKE xmda_t.xmdaunit,   #發貨組織
        xmda004  LIKE xmda_t.xmda004,    #客戶編號
        xmda005  LIKE xmda_t.xmda005,    #訂單性質 
        xmda006  LIKE xmda_t.xmda006,    #多角性質
        xmda009  LIKE xmda_t.xmda009,    #收款條件 
        xmda010  LIKE xmda_t.xmda010,    #交易條件 
        xmda011  LIKE xmda_t.xmda011,    #稅別 
        xmda015  LIKE xmda_t.xmda015,    #幣別
        xmda021  LIKE xmda_t.xmda021,    #帳款客戶 
        xmda022  LIKE xmda_t.xmda022,    #送貨客戶 
        xmda023  LIKE xmda_t.xmda023,    #銷售渠道
        xmda024  LIKE xmda_t.xmda024,    #銷售分類
        xmda025  LIKE xmda_t.xmda025,    #送貨地址
        xmda035  LIKE xmda_t.xmda035,    #發票類型
        xmda048  LIKE xmda_t.xmda048,    #內外銷
        xmda049  LIKE xmda_t.xmda049,    #匯率計算基準
        xmda200  LIKE xmda_t.xmda200,    #調貨經銷商
        xmda201  LIKE xmda_t.xmda201,    #代送商
        xmda203  LIKE xmda_t.xmda203,    #發票客戶
        xmda208  LIKE xmda_t.xmda208,    #地區編號
        xmda209  LIKE xmda_t.xmda209,    #縣市編號
        xmda210  LIKE xmda_t.xmda210,    #省區編號
        xmda211  LIKE xmda_t.xmda211     #區域編號
                 END RECORD   
DEFINE l_xmda005 LIKE xmda_t.xmda005
DEFINE l_xmda006 LIKE xmda_t.xmda006
DEFINE l_cnt     LIKE type_t.num5
DEFINE l_xmdh016 LIKE xmdh_t.xmdh016
DEFINE l_success LIKE type_t.num5
DEFINE l_slip    LIKE ooba_t.ooba002

   LET g_errno = ''
   INITIALIZE l_xmda TO NULL

   SELECT xmdastus, xmdasite, xmdaunit, xmda004, xmda005,
          xmda006,  xmda009,  xmda010,  xmda011, xmda015,
          xmda021,  xmda022,  xmda023,  xmda024, xmda025,
          xmda035,  xmda048,  xmda049,  xmda200, xmda201,
          xmda203,  xmda208,  xmda209,  xmda210, xmda211
     INTO l_xmda.xmdastus, l_xmda.xmdasite, l_xmda.xmdaunit, l_xmda.xmda004, l_xmda.xmda005,
          l_xmda.xmda006,  l_xmda.xmda009,  l_xmda.xmda010,  l_xmda.xmda011, l_xmda.xmda015,
          l_xmda.xmda021,  l_xmda.xmda022,  l_xmda.xmda023,  l_xmda.xmda024, l_xmda.xmda025,
          l_xmda.xmda035,  l_xmda.xmda048,  l_xmda.xmda049,  l_xmda.xmda200, l_xmda.xmda201,
          l_xmda.xmda203,  l_xmda.xmda208,  l_xmda.xmda209,  l_xmda.xmda210, l_xmda.xmda211
     FROM xmda_t
     WHERE xmdaent = g_enterprise
       AND xmdadocno = g_xmdh_d[l_ac].xmdh001

   CASE
      WHEN SQLCA.sqlcode = 100    LET g_errno = 'adb-00076'
      #輸入的資料不存在於 訂單單頭檔 中!
      WHEN l_xmda.xmdastus <> 'Y' LET g_errno = 'adb-00077'
      #輸入的資料狀態非"已確認"!
      WHEN NOT cl_null(g_xmdg_m.xmdgunit) AND l_xmda.xmdaunit <> g_xmdg_m.xmdgunit
         LET g_errno = 'adb-00069'    #發貨組織
      WHEN NOT cl_null(g_xmdg_m.xmdg026) AND l_xmda.xmda023 <> g_xmdg_m.xmdg026
         LET g_errno = 'adb-00070'    #銷售渠道
      WHEN l_xmda.xmda005 <> g_xmdg_m.xmdg001 LET g_errno = 'adb-00061' #出貨性質
      WHEN l_xmda.xmda005 = '5'               LET g_errno = 'axm-00237' #此筆資料的單據性質為預先訂單，不可以出貨！
      WHEN l_xmda.xmda004 <> g_xmdg_m.xmdg005 LET g_errno = 'adb-00060' #客戶編號
      WHEN NOT cl_null(g_xmdg_m.xmdg200) AND l_xmda.xmda200 <> g_xmdg_m.xmdg200
         LET g_errno = 'adb-00071'    #調貨經銷商
      WHEN NOT cl_null(g_xmdg_m.xmdg201) AND l_xmda.xmda201 <> g_xmdg_m.xmdg201
         LET g_errno = 'adb-00072'    #代送商
      #WHEN l_xmda.xmda022 <> g_xmdg_m.xmdg007 LET g_errno = 'adb-00067' #送貨客戶
      #WHEN NOT cl_null(l_xmda.xmda025) AND l_xmda.xmda025 <> g_xmdg_m.xmdg017
      #   LET g_errno = 'adb-00073'    #送貨地址
      WHEN l_xmda.xmda021 <> g_xmdg_m.xmdg006 LET g_errno = 'adb-00066' #帳款客戶
      WHEN l_xmda.xmda203 <> g_xmdg_m.xmdg202 LET g_errno = 'adb-00074' #發票客戶
      WHEN l_xmda.xmda009 <> g_xmdg_m.xmdg008 LET g_errno = 'adb-00062' #收款條件
      WHEN l_xmda.xmda010 <> g_xmdg_m.xmdg009 LET g_errno = 'adb-00063' #交易條件
      WHEN l_xmda.xmda035 <> g_xmdg_m.xmdg013 LET g_errno = 'adb-00068' #發票類型
      WHEN l_xmda.xmda048 <> g_xmdg_m.xmdg032 LET g_errno = 'adb-00206' #內外銷
      WHEN l_xmda.xmda049 <> g_xmdg_m.xmdg033 LET g_errno = 'adb-00305' #匯率計算基準
      WHEN l_xmda.xmda015 <> g_xmdg_m.xmdg014 LET g_errno = 'adb-00065' #幣別
      WHEN l_xmda.xmda011 <> g_xmdg_m.xmdg010 LET g_errno = 'adb-00064' #稅別
      WHEN NOT cl_null(g_xmdg_m.xmdg027) AND l_xmda.xmda024 <> g_xmdg_m.xmdg027
         LET g_errno = 'adb-00073'    #銷售分類
      OTHERWISE LET g_errno = SQLCA.sqlcode USING '------'
   END CASE

   IF NOT cl_null(g_errno) THEN
      RETURN
   END IF
   
   #單據別參數
   CALL s_aooi200_get_slip(g_xmdh_d[l_ac].xmdh001) RETURNING l_success,l_slip
   IF cl_get_doc_para(g_enterprise,g_xmdg_m.xmdgsite,l_slip,'D-BAS-0077') = 'N' THEN
      #單據別參數設定該筆訂單不須經過出通流程！
      LET g_errno = 'axm-00349'
   END IF

   #IF cl_null(g_errno) AND NOT cl_null(g_xmdg_m.xmdg004) THEN
   #   LET l_xmda006 = ''
   #   SELECT xmda006 INTO l_xmda006
   #     FROM xmda_t
   #    WHERE xmdaent = g_enterprise
   #      AND xmdasite = g_xmdg_m.xmdgsite
   #      AND xmdadocno = g_xmdg_m.xmdg004
   #   IF l_xmda.xmda006 <> l_xmda006 THEN
   #      #若單頭有指定訂單單號時則單身訂單的多角性質需與單頭一致
   #      LET g_errno = 'axm-00156'
   #      RETURN
   #   END IF
   #END IF

   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt
     FROM xmdg_t,xmdh_t
    WHERE xmdgent = xmdhent
      AND xmdgdocno = xmdhdocno
      AND xmdhent = g_enterprise
      AND xmdh001 = g_xmdh_d[l_ac].xmdh001
      AND xmdh002 = g_xmdh_d[l_ac].xmdh002
      AND xmdh003 = g_xmdh_d[l_ac].xmdh003
      AND xmdh004 = g_xmdh_d[l_ac].xmdh004
      AND xmdgstus <> 'X'
   IF l_cnt >= 1 THEN
      LET g_errno = 'adb-00358'
      RETURN
   END IF

   #則需檢核該訂單分批續是否還有可轉出通量，若沒有則不允許輸入此訂單分批序s
   CALL s_adbt520_xmdd006_xmdd031(g_xmdh_d[l_ac].xmdh001,g_xmdh_d[l_ac].xmdh002,g_xmdh_d[l_ac].xmdh003,g_xmdh_d[l_ac].xmdh004,'0',g_xmdg_m.xmdgdocno)
        RETURNING l_xmdh016
END FUNCTION
################################################################################
# Descriptions...: 若輸入的訂單號+訂單項次的xmdd_t只有對應一個訂單項序時，則自動將訂單項序預設到出通單身[C:訂單項序]上
# Memo...........:
# Usage..........: CALL adbt520_xmdh002_default()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/05/16 By pomelo
# Modify.........:
################################################################################
PRIVATE FUNCTION adbt520_xmdh002_default()
DEFINE l_cnt     LIKE type_t.num5
 
   LET l_cnt = 0
   SELECT COUNT(COUNT(xmddseq1)) INTO l_cnt
     FROM xmdd_t
    WHERE xmddent = g_enterprise
      AND xmdddocno = g_xmdh_d[l_ac].xmdh001
      AND xmddseq = g_xmdh_d[l_ac].xmdh002
    GROUP BY xmddseq1
   IF l_cnt = 1 THEN
      SELECT DISTINCT xmddseq1 INTO g_xmdh_d[l_ac].xmdh003
        FROM xmdd_t
       WHERE xmddent = g_enterprise
         AND xmdddocno = g_xmdh_d[l_ac].xmdh001
         AND xmddseq = g_xmdh_d[l_ac].xmdh002
   END IF
END FUNCTION
################################################################################
# Descriptions...: 訂單項次欄位校驗
# Memo...........:
# Usage..........: CALL adbt520_xmdh002_chk()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/05/16 By pomelo
# Modify.........:
################################################################################
PRIVATE FUNCTION adbt520_xmdh002_chk()
DEFINE l_cnt          LIKE type_t.num5
DEFINE l_xmdh016      LIKE xmdh_t.xmdh016
DEFINE l_xmdc045      LIKE xmdc_t.xmdc045
DEFINE l_xmja025      LIKE xmja_t.xmja025
DEFINE l_xmja026      LIKE xmja_t.xmja026
DEFINE l_xmddseq1     LIKE xmdd_t.xmddseq1
DEFINE l_xmddseq2     LIKE xmdd_t.xmddseq2
 
   LET g_errno = ''
   LET l_cnt = ''
   LET l_xmdc045 = ''
   LET l_xmja025 = ''
   LET l_xmja026 = ''
   
   SELECT xmdc045,xmja025,xmja026
     INTO l_xmdc045,l_xmja025,l_xmja026
     FROM xmdc_t,xmja_t
    WHERE xmdcent = xmjaent
      AND xmdcdocno = xmjadocno
      AND xmdcseq = xmjaseq
      AND xmdcent = g_enterprise
      AND xmdcsite = g_xmdg_m.xmdgsite
      AND xmdcdocno = g_xmdh_d[l_ac].xmdh001
      AND xmdcseq = g_xmdh_d[l_ac].xmdh002
   CASE
      WHEN SQLCA.sqlcode = 100
         #LET g_errno = 'adb-00359' #160318-00005#6 mark
         LET g_errno = 'sub-01323'  #160318-00005#6 add
         RETURN
      WHEN l_xmdc045 <> '1'
         LET g_errno = 'adb-00105'
         RETURN
      #送貨客戶
      WHEN l_xmja025 <> g_xmdg_m.xmdg007
         LET g_errno = 'adb-00276'
         RETURN
      #送貨地址碼
      WHEN l_xmja026 <> g_xmdg_m.xmdg017
         LET g_errno = 'adb-00277'
         RETURN
   END CASE
   
   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt
     FROM xmdd_t
    WHERE xmddent = g_enterprise
      AND xmdddocno = g_xmdh_d[l_ac].xmdh001
      AND xmddseq = g_xmdh_d[l_ac].xmdh002
   
   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt
     FROM xmdg_t,xmdh_t
    WHERE xmdgent = xmdhent
      AND xmdgdocno = xmdhdocno
      AND xmdhent = g_enterprise
      AND xmdh001 = g_xmdh_d[l_ac].xmdh001
      AND xmdh002 = g_xmdh_d[l_ac].xmdh002
      AND xmdh003 = g_xmdh_d[l_ac].xmdh003
      AND xmdh004 = g_xmdh_d[l_ac].xmdh004
      AND xmdgstus <> 'X'
   IF l_cnt >= 1 THEN
      LET g_errno = 'adb-00358'
      RETURN
   END IF
   
   #核該訂單分批續是否還有可轉出通量
   IF l_cnt = 1 THEN
      LET l_xmddseq1 = ''
      LET l_xmddseq2 = ''
      LET l_xmdh016 = 0
      SELECT xmddseq1,xmddseq2 INTO l_xmddseq1,l_xmddseq2
        FROM xmdd_t
       WHERE xmddent = g_enterprise
         AND xmdddocno = g_xmdh_d[l_ac].xmdh001
         AND xmddseq = g_xmdh_d[l_ac].xmdh002
      CALL s_adbt520_xmdd006_xmdd031(g_xmdh_d[l_ac].xmdh001,g_xmdh_d[l_ac].xmdh002,l_xmddseq1,l_xmddseq2,'0',g_xmdg_m.xmdgdocno)
         RETURNING l_xmdh016
      IF cl_null(g_errno) THEN
         LET g_xmdh_d[l_ac].xmdh003 = l_xmddseq1
         LET g_xmdh_d[l_ac].xmdh004 = l_xmddseq2
         LET g_xmdh_d[l_ac].xmdh016 = l_xmdh016
      END IF
   END IF
   
END FUNCTION
################################################################################
# Descriptions...: 若輸入的訂單號+訂單項次的xmdd_t只有對應一個分批序時，則自動將分批序預設到出通單身[C:分批序]上
# Memo...........:
# Usage..........: CALL adbt520_xmdh003_default()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/05/16 By pomelo
# Modify.........:
################################################################################
PRIVATE FUNCTION adbt520_xmdh003_default()
DEFINE l_cnt     LIKE type_t.num5 

   LET l_cnt = 0
   SELECT count(count(xmddseq2)) INTO l_cnt
     FROM xmdd_t
    WHERE xmddent = g_enterprise
      #AND xmddsite = g_xmdg_m.xmdgsite
      AND xmdddocno = g_xmdh_d[l_ac].xmdh001
      AND xmddseq = g_xmdh_d[l_ac].xmdh002
      AND xmddseq1 = g_xmdh_d[l_ac].xmdh003
    GROUP BY xmddseq2
   IF l_cnt = 1 THEN
      SELECT DISTINCT xmddseq2 INTO g_xmdh_d[l_ac].xmdh004
        FROM xmdd_t
       WHERE xmddent = g_enterprise
         #AND xmddsite = g_xmdg_m.xmdgsite
         AND xmdddocno = g_xmdh_d[l_ac].xmdh001
         AND xmddseq = g_xmdh_d[l_ac].xmdh002
         AND xmddseq1 = g_xmdh_d[l_ac].xmdh003
   END IF
END FUNCTION
################################################################################
# Descriptions...: 訂單項序欄位校驗
# Memo...........:
# Usage..........: CALL adbt520_xmdh003_chk()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/05/16 By pomelo
# Modify.........:
################################################################################
PRIVATE FUNCTION adbt520_xmdh003_chk()
DEFINE l_cnt     LIKE type_t.num5
DEFINE l_xmdh016 LIKE xmdh_t.xmdh016

   LET g_errno = ''
   LET l_cnt = ''
    
   SELECT count(*) INTO l_cnt
     FROM xmdd_t
    WHERE xmddent = g_enterprise
      #AND xmddsite = g_xmdg_m.xmdgsite
      AND xmdddocno = g_xmdh_d[l_ac].xmdh001
      AND xmddseq = g_xmdh_d[l_ac].xmdh002
      AND xmddseq1 = g_xmdh_d[l_ac].xmdh003
   IF cl_null(l_cnt) OR l_cnt = 0 THEN
      LET g_errno = 'adb-00359'
   END IF
   
   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt
     FROM xmdg_t,xmdh_t
    WHERE xmdgent = xmdhent
      AND xmdgdocno = xmdhdocno
      AND xmdhent = g_enterprise
      AND xmdh001 = g_xmdh_d[l_ac].xmdh001
      AND xmdh002 = g_xmdh_d[l_ac].xmdh002
      AND xmdh003 = g_xmdh_d[l_ac].xmdh003
      AND xmdh004 = g_xmdh_d[l_ac].xmdh004
      AND xmdgstus <> 'X'
   IF l_cnt >= 1 THEN
      LET g_errno = 'adb-00358'
      RETURN
   END IF
   
   CALL s_adbt520_xmdd006_xmdd031(g_xmdh_d[l_ac].xmdh001,g_xmdh_d[l_ac].xmdh002,g_xmdh_d[l_ac].xmdh003,g_xmdh_d[l_ac].xmdh004,'0',g_xmdg_m.xmdgdocno)
        RETURNING l_xmdh016
END FUNCTION
################################################################################
# Descriptions...: 訂單分批序欄位校驗
# Memo...........:
# Usage..........: CALL adbt520_xmdh004_chk()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/05/16 By pomelo
# Modify.........:
################################################################################
PRIVATE FUNCTION adbt520_xmdh004_chk()
DEFINE l_xmdc045 LIKE xmdc_t.xmdc045
DEFINE l_xmdh016 LIKE xmdh_t.xmdh016
DEFINE l_cnt     LIKE type_t.num5

   LET g_errno = ''
   LET l_xmdc045 = ''
   
   SELECT xmdc045 INTO l_xmdc045
     FROM xmdd_t
     LEFT OUTER JOIN xmdc_t ON xmdcent = xmddent
                           AND xmdcdocno = xmdddocno 
                           AND xmdcseq = xmddseq
    WHERE xmddent = g_enterprise
      AND xmdddocno = g_xmdh_d[l_ac].xmdh001
      AND xmddseq = g_xmdh_d[l_ac].xmdh002
      AND xmddseq1 = g_xmdh_d[l_ac].xmdh003
      AND xmddseq2 = g_xmdh_d[l_ac].xmdh004
      
   CASE
      #WHEN SQLCA.sqlcode = 100 LET g_errno = 'adb-00208' RETURN #160318-00005#6 mark
      WHEN SQLCA.sqlcode = 100 LET g_errno = 'sub-01323' RETURN  #160318-00005#6 add
      #訂單已結案，不可輸入!
      WHEN l_xmdc045 <> '1' LET g_errno = 'adb-00105' RETURN
      
      OTHERWISE LET g_errno = SQLCA.sqlcode USING '------'
   END CASE
      
   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt
     FROM xmdg_t,xmdh_t
    WHERE xmdgent = xmdhent
      AND xmdgdocno = xmdhdocno
      AND xmdhent = g_enterprise
      AND xmdh001 = g_xmdh_d[l_ac].xmdh001
      AND xmdh002 = g_xmdh_d[l_ac].xmdh002
      AND xmdh003 = g_xmdh_d[l_ac].xmdh003
      AND xmdh004 = g_xmdh_d[l_ac].xmdh004
      AND xmdgstus <> 'X'
   IF l_cnt >= 1 THEN
      LET g_errno = 'adb-00358'
   END IF
   
   IF cl_null(g_errno) THEN
      CALL s_adbt520_xmdd006_xmdd031(g_xmdh_d[l_ac].xmdh001,g_xmdh_d[l_ac].xmdh002,g_xmdh_d[l_ac].xmdh003,g_xmdh_d[l_ac].xmdh004,'0',g_xmdg_m.xmdgdocno)
           RETURNING l_xmdh016
      IF cl_null(g_errno) THEN
         LET g_xmdh_d[l_ac].xmdh016 = l_xmdh016
      END IF
   END IF
END FUNCTION
################################################################################
# Descriptions...: 料件編號欄位校驗
# Memo...........:
# Usage..........: CALL adbt520_xmdh006_chk()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/05/16 By pomelo
# Modify.........:
################################################################################
PRIVATE FUNCTION adbt520_xmdh006_chk()
DEFINE l_success LIKE type_t.num5
DEFINE l_flag    LIKE type_t.num5
DEFINE l_xmdd001 LIKE xmdd_t.xmdd001

   #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
   INITIALIZE g_chkparam.* TO NULL 
   
   #設定g_chkparam.*的參數
   LET g_chkparam.arg1 = g_xmdh_d[l_ac].xmdh006

   #呼叫檢查存在的library
   IF NOT cl_chk_exist("v_imaf001_1") THEN
      #檢查失敗時後續處理
      RETURN FALSE
   END IF
   
   #當出貨料號與訂單料號不一樣時，必須檢核是否與訂單料號有替代關係，若沒有則不允許修改
   IF NOT adbt520_xmdh006_xmdh007_chk() THEN
      RETURN FALSE
   END IF

   RETURN TRUE

END FUNCTION
################################################################################
# Descriptions...: 若訂單號、項次、項序、分批序均有值時，則需預設出貨單身相關欄位值
# Memo...........:
# Usage..........: CALL adbt520_xmdh004_default()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/05/16 By pomelo
# Modify.........:
################################################################################
PRIVATE FUNCTION adbt520_xmdh004_default()
DEFINE l_cnt        LIKE type_t.num5
DEFINE l_success    LIKE type_t.num5
DEFINE l_xrcd113    LIKE xrcd_t.xrcd113
DEFINE l_xrcd114    LIKE xrcd_t.xrcd114
