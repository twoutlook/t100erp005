################################################################################
# Descriptions...: 依aooi500設定檢查, 輸入的訂單銷售組織是否符合出通單單頭的銷售組織
# Memo...........:
# Usage..........: CALL adbt520_xmdadocno_chk(p_xmdadocno)
#                  RETURNING r_success
# Input parameter: p_xmdadocno    訂單單號
# Return code....: r_success      檢查結果
# Date & Author..: 2016/08/31 By Lori   #160809-00015#1
# Modify.........:
################################################################################
PRIVATE FUNCTION adbt520_xmdadocno_chk(p_xmdadocno)
   DEFINE p_xmdadocno      LIKE xmda_t.xmdadocno
   DEFINE r_success        LIKE type_t.num5
   DEFINE l_cnt            LIKE type_t.num5
   DEFINE l_sql            STRING
   DEFINE l_where          STRING
   
   LET r_success = TRUE
               
	LET l_where = s_aooi500_q_where('adbt500','xmdasite',g_xmdg_m.xmdgsite,'c')
   LET l_where = cl_str_replace(l_where,"ooef001","xmddsite")
   LET l_sql = "SELECT COUNT(*) FROM xmdd_t ",
               " WHERE xmddent = ",g_enterprise,
               "   AND xmdddocno = '",p_xmdadocno,"' ",
               "   AND ",l_where 
   PREPARE adbt520_xmdadocno_chk FROM l_sql
   EXECUTE adbt520_xmdadocno_chk INTO l_cnt
   
   IF l_cnt = 0 THEN
      LET r_success = FALSE
      
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'adb-00104'
      LET g_errparam.extend = p_xmdadocno
      LET g_errparam.popup = TRUE
      CALL cl_err()      
   END IF
            
   RETURN r_success
END FUNCTION

 
