      AND lc_state <> "R"
      AND lc_state <> "W"
      AND lc_state <> "H"
      AND lc_state <> "UH"
      AND lc_state <> "X"
      ) OR 
      g_xmdg_m.xmdgstus = lc_state OR cl_null(lc_state) THEN
      CLOSE adbt520_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
