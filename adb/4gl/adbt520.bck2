             xmdc005,  xmdd213,  xmdd214, xmdd215,  xmdd004,
             xmdd024,  xmdd026,  xmdc052, xmdd018,  xmdd019,
             xmdd020,  xmdd028,  xmdd029, xmdd030,  xmdd231,
             xmdc036,  xmdc037,  xmdc038, xmdc027,  xmdd008,
             xmdc021,  xmdd200,  xmja002, xmdd201,  xmdd202,
             xmdd203,  xmdd204,  xmdd205, xmdd206,  xmdd207,
             xmja025,  xmja026,  xmdd209, xmdd210,  xmdd216,
             xmdd218,  xmdd219,  xmdd220, xmdd229,
             xmja037,  xmja038,  xmja039, xmja040
        FROM xmja_t,xmdc_t LEFT OUTER JOIN xmdd_t ON xmdcent = xmddent
                                                 AND xmdcsite = xmddsite
                                                 AND xmdcdocno = xmdddocno
                                                 AND xmdcseq = xmddseq
                                                 AND xmdc045 = '1'    #未結案
       WHERE xmddent = g_enterprise
         AND xmdddocno = g_xmdg_m.xmdg004
         AND xmdc045 = '1'
         AND xmjaent = xmdcent
         AND xmjadocno = xmdcdocno
         AND xmjaseq = xmdcseq
       ORDER BY xmddseq,xmddseq1,xmddseq2
       
   INITIALIZE l_xmdh.* TO NULL
   LET r_success = TRUE
   LET l_seq = 1
   
   FOREACH gen_xmdh_cur INTO l_xmdh.xmdhsite,l_xmdh.xmdhunit,l_xmdh.xmdh002, l_xmdh.xmdh003, l_xmdh.xmdh004,
