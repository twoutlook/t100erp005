         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         RETURN FALSE
      END IF
   END IF
                                    
   #鎖定整組table
   #LET ls_group = "'2',"
   #僅鎖定自身table
   LET ls_group = "xmdi_t"
   IF ls_group.getIndexOf(ps_table,1) THEN
   
      OPEN adbt520_bcl2 USING g_enterprise,
                                             g_xmdg_m.xmdgdocno,g_xmdh2_d[g_detail_idx].xmdiseq,g_xmdh2_d[g_detail_idx].xmdiseq1 
 
