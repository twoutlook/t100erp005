                     xmdg051 = l_xrcd105,
                     xmdg052 = l_xrcd104
    WHERE xmdgent = g_enterprise
      AND xmdgdocno = g_xmdg_m.xmdgdocno
      
   IF SQLCA.sqlcode THEN
      LET r_success = FALSE
   END IF
      
   RETURN r_success
   
END FUNCTION

