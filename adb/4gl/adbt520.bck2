################################################################################
# Descriptions...: 檢核訂單所有項次的出貨據點，若沒有任何一筆項次的出貨據點跟g_site一樣則不允許輸入此訂單
# Memo...........:
# Usage..........: CALL adbt520_xmdg004_chk()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/05/16 By pomelo
# Modify.........:
################################################################################
PRIVATE FUNCTION adbt520_xmdg004_chk()
DEFINE l_cnt        LIKE type_t.num5
DEFINE l_xmdaunit   LIKE xmda_t.xmdaunit
DEFINE l_xmda005    LIKE xmda_t.xmda005
DEFINE l_xmda023    LIKE xmda_t.xmda023
DEFINE l_success    LIKE type_t.num5
DEFINE l_slip       LIKE ooba_t.ooba002

   LET g_errno = '' 
   
   LET l_cnt = 0
   LET l_xmdaunit = ''
   LET l_xmda005 = ''
   LET l_xmda023 = ''
   SELECT DISTINCT xmdaunit,xmda005,xmda023
     INTO l_xmdaunit,l_xmda005,l_xmda023
     FROM xmda_t,xmdc_t 
    WHERE xmdaent = xmdcent
      AND xmdadocno = xmdcdocno
      AND xmdcent = g_enterprise 
      AND xmdcdocno = g_xmdg_m.xmdg004
   CASE
      WHEN SQLCA.sqlcode = 100
         #輸入的訂單單號 不存在 訂單單頭檔中！
         LET g_errno = 'adb-00052'
      WHEN NOT cl_null(g_xmdg_m.xmdgunit)
         IF cl_null(l_xmdaunit) THEN
            LET l_cnt = 0
            SELECT COUNT(xmddseq) INTO l_cnt
              FROM xmdd_t 
             WHERE xmddent = g_enterprise
               AND xmdddocno = g_xmdg_m.xmdg004
            IF l_cnt = 0 THEN
               #此訂單單號的銷售組織 與畫面上的銷售組織值不相同！
               LET g_errno = 'adb-00053'
            END IF
         ELSE
            IF g_xmdg_m.xmdgunit <> l_xmdaunit THEN
               #此訂單單號的銷售組織 與畫面上的銷售組織值不相同！
               LET g_errno = 'adb-00053'
            END IF
         END IF
      WHEN NOT cl_null(g_xmdg_m.xmdg023)
         IF g_xmdg_m.xmdg023 <> l_xmda023 OR cl_null(l_xmda023) THEN
            #此訂單單號的銷售渠道 與畫面上的銷售渠道值不相同！
            LET g_errno = 'adb-00054'
         END IF
      WHEN l_xmda005 = '5'
         #此筆資料的單據性質為預先訂單，不可以出貨！
         LET g_errno = 'axm-00237'
   END CASE
   
   IF NOT cl_null(g_errno) THEN
      RETURN
   END IF
   
   #單據別參數
   CALL s_aooi200_get_slip(g_xmdg_m.xmdg004) RETURNING l_success,l_slip
   IF cl_get_doc_para(g_enterprise,g_xmdg_m.xmdgsite,l_slip,'D-BAS-0077') = 'N' THEN
      #單據別參數設定該筆訂單不須經過出通流程！
      LET g_errno = 'axm-00349'
   END IF
   
END FUNCTION
################################################################################
# Descriptions...: 帶出起運地的參考欄位資訊
# Memo...........:
# Usage..........: CALL adbt520_local_ref(p_local)
#                : RETURNING r_local
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2014/05/16 By pomelo
# Modify.........:
################################################################################
PRIVATE FUNCTION adbt520_local_ref(p_local)
DEFINE p_local        LIKE oockl_t.oockl003
DEFINE r_local        LIKE oockl_t.oockl005
DEFINE l_oocq019      LIKE oocq_t.oocq019

   IF NOT cl_null(g_xmdg_m.xmdg018) THEN 
      SELECT oocq019 INTO l_oocq019
        FROM oocq_t
       WHERE oocq001 = '263'
         AND oocq002 = g_xmdg_m.xmdg018
   END IF

   IF NOT cl_null(l_oocq019) THEN
      CASE
         WHEN l_oocq019 ='1' OR l_oocq019 ='4'
           INITIALIZE g_ref_fields TO NULL
           LET g_ref_fields[1] = p_local
           CALL ap_ref_array2(g_ref_fields,"SELECT oockl005 FROM oockl_t WHERE oocklent='"||g_enterprise||"' AND oockl003=? AND oockl004='"||g_dlang||"'","") RETURNING g_rtn_fields
         WHEN l_oocq019 ='2' 
           INITIALIZE g_ref_fields TO NULL
           LET g_ref_fields[1] = p_local
           CALL ap_ref_array2(g_ref_fields,"SELECT oocql004 FROM oocql_t WHERE oocqlent='"||g_enterprise||"' AND oocql002=? AND oocql001 = '262' AND oocql003='"||g_dlang||"'","") RETURNING g_rtn_fields
         WHEN l_oocq019 ='3'
           INITIALIZE g_ref_fields TO NULL
           LET g_ref_fields[1] = p_local
           CALL ap_ref_array2(g_ref_fields,"SELECT oocql004 FROM oocql_t WHERE oocqlent='"||g_enterprise||"' AND oocql002=? AND oocql001 = '276' AND oocql003='"||g_dlang||"'","") RETURNING g_rtn_fields
      END CASE
   ELSE
      INITIALIZE g_ref_fields TO NULL
      LET g_ref_fields[1] = p_local
      CALL ap_ref_array2(g_ref_fields,"SELECT oockl005 FROM oockl_t WHERE oocklent='"||g_enterprise||"' AND oockl003=? AND oocql001 = '262' AND oockl004='"||g_dlang||"'","") RETURNING g_rtn_fields
   END IF
   
   LET r_local = '', g_rtn_fields[1] , ''
   RETURN r_local

END FUNCTION
