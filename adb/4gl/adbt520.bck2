{<section id="adbt520.set_act_no_visible" >}
#+ 單頭權限關閉
PRIVATE FUNCTION adbt520_set_act_no_visible()
   #add-point:set_act_no_visible段define(客製用) name="set_act_no_visible.define_customerization"
   
   #end add-point   
   #add-point:set_act_no_visible段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="set_act_no_visible.define"
 
   #end add-point   
   #add-point:set_act_no_visible段 name="set_act_no_visible.set_act_no_visible"
   
   #提交和抽單一開始先無條件關
   CALL cl_set_act_visible("signing,withdraw",FALSE)
   CASE g_xmdg_m.xmdgstus
      WHEN 'N'   #未確認
         CALL cl_set_act_visible("unconfirmed,closed,unhold,hold",FALSE)   #lori522612  150206  add ---hold
         #需提交至BPM時，則顯示「提交」功能並隱藏「確認」功能
         IF cl_bpm_chk() THEN
            CALL cl_set_act_visible("signing",TRUE)
            CALL cl_set_act_visible("confirmed",FALSE)
         END IF
      WHEN 'Y'   #已確認
         CALL cl_set_act_visible("modify,delete,modify_detail,",FALSE)
         CALL cl_set_act_visible("open_adbt520_01,unhold,invalid",FALSE)
      WHEN 'H'   #留置
         CALL cl_set_act_visible("modify,delete,modify_detail,",FALSE)
         CALL cl_set_act_visible("open_adbt520_01,invalid",FALSE)
         CALL cl_set_act_visible("confirmed,unconfirmed,hold,invalid",FALSE)
      WHEN 'UH'  #取消留置
         CALL cl_set_act_visible("modify,delete,modify_detail,",FALSE)
         CALL cl_set_act_visible("open_adbt520_01,invalid",FALSE)
         CALL cl_set_act_visible("confirmed,unconfirmed,unhold,invalid",FALSE)
      WHEN 'X'   #作廢
         CALL cl_set_act_visible("modify,delete,modify_detail",FALSE)
         CALL cl_set_act_visible("hold,invalid",FALSE)
         CALL cl_set_act_visible("open_adbt520_01",FALSE)
      WHEN 'D'   #抽單
         CALL cl_set_act_visible("confirmed,unconfirmed,hold,invalid",FALSE)
      WHEN 'R'   #已拒絕
         CALL cl_set_act_visible("confirmed,unconfirmed,hold,invalid",FALSE)
      WHEN 'A'   #已核准
         CALL cl_set_act_visible("modify,delete,modify_detail",FALSE)
         CALL cl_set_act_visible("hold,invalid",FALSE)
         CALL cl_set_act_visible("open_adbt520_01",FALSE)
      WHEN 'W'   #送簽中
         CALL cl_set_act_visible("modify,delete,modify_detail",FALSE)
         CALL cl_set_act_visible("hold,invalid",FALSE)
         CALL cl_set_act_visible("open_adbt520_01",FALSE)
   END CASE
   #end add-point   
END FUNCTION
 
