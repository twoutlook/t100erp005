                             l_xmdh.xmdh025, l_xmdh.xmdh026, l_xmdh.xmdh027, l_xmdh.xmdh028, l_xmdh.xmdh029,
                             l_xmdh.xmdh031, l_xmdh.xmdh032, l_xmdh.xmdh033, l_xmdh.xmdh034, l_xmdh.xmdh035,
                             l_xmdh.xmdh036, l_xmdh.xmdh200, l_xmdh.xmdh201, l_xmdh.xmdh202, l_xmdh.xmdh203,
                             l_xmdh.xmdh207, l_xmdh.xmdh208, l_xmdh.xmdh209, l_xmdh.xmdh210, l_xmdh.xmdh211,
                             l_xmdh.xmdh212, l_xmdh.xmdh213, l_xmdh.xmdh214, l_xmdh.xmdh215, l_xmdh.xmdh216,
                             l_xmdh.xmdh217, l_xmdh.xmdh218, l_xmdh.xmdh219, l_xmdh.xmdh220,
                             l_xmdh.xmdh221, l_xmdh.xmdh222, l_xmdh.xmdh223, l_xmdh.xmdh224
      IF SQLCA.SQLCODE THEN
         EXIT FOREACH
      END IF

      #lori522612  150116  add ----------------------(S)
      #產品特徵為NULL時,給空白   
      IF cl_null(l_xmdh.xmdh007) THEN
         LET l_xmdh.xmdh007 = ' '
      END IF
      #lori522612  150116  add ----------------------(E)  

      SELECT xmdd202 INTO l_xmdh.xmdh203
        FROM xmdd_t
       WHERE xmddent = g_enterprise
         AND xmdddocno = g_xmdg_m.xmdg004
         AND xmddseq = l_xmdh.xmdh002
         AND xmddseq1= l_xmdh.xmdh003
         AND xmddseq2= l_xmdh.xmdh004
      #檢查單身的包裝單位必須要有值
      IF cl_null(l_xmdh.xmdh202) THEN
         LET r_success = FALSE
         #訂單單身的包裝單位為空！
         LET g_errno = 'adb-00055'
         EXIT FOREACH
      END IF
      #檢查單身的包裝數量必須要有值
      IF cl_null(l_xmdh.xmdh203) THEN
         LET r_success = FALSE
         #訂單單身的包裝數量為空！
         LET g_errno = 'adb-00056'
         EXIT FOREACH
      END IF
      
      #檢查單身的送貨客戶及送貨地址是否與單頭的相同(當單頭有輸入)
      #送貨客戶
      IF NOT cl_null(g_xmdg_m.xmdg007) THEN
         IF cl_null(l_xmdh.xmdh212) OR l_xmdh.xmdh212 <> g_xmdg_m.xmdg007 THEN
            #LET g_errno = 'adb-00276'
            CONTINUE FOREACH
         END IF
      END IF
      #送貨地址碼
      IF NOT cl_null(g_xmdg_m.xmdg017) THEN
         IF cl_null(l_xmdh.xmdh213) OR l_xmdh.xmdh213 <> g_xmdg_m.xmdg017 THEN
            #LET g_errno = 'adb-00277'
            CONTINUE FOREACH
         END IF
      END IF
      
      #檢核不可大於(xmdd006-xmdd014-已登打出貨未確認的出貨量)
      CALL s_adbt520_xmdd006_xmdd031(g_xmdg_m.xmdg004,l_xmdh.xmdh002,l_xmdh.xmdh003,l_xmdh.xmdh004,'0',g_xmdg_m.xmdgdocno)
           RETURNING l_xmdh.xmdh016
      IF NOT cl_null(g_errno) THEN
         #LET r_success = FALSE
         CONTINUE FOREACH
      END IF
      
      #160720-00004#1 20160725 add by beckxie---S
      #若剩餘出通數量為0,就表示已全數出通,此張單據不需帶出已全數出通的項次至單身
      IF NOT cl_null(l_xmdh.xmdh016) THEN
         IF l_xmdh.xmdh016 = 0 THEN 
            CONTINUE FOREACH
         END IF
      END IF
      #160720-00004#1 20160725 add by beckxie---E
      
      LET l_xmdh.xmdh001 = g_xmdg_m.xmdg004
      LET l_xmdh.xmdhent = g_enterprise
      #LET l_xmdh.xmdhsite = g_xmdg_m.xmdgsite
      #LET l_xmdh.xmdhunit = g_xmdg_m.xmdgunit
      LET l_xmdh.xmdhdocno = g_xmdg_m.xmdgdocno
      LET l_xmdh.xmdhseq = l_seq
      LET l_xmdh.xmdh011 = 'N'
      LET l_xmdh.xmdh017 = l_xmdh.xmdh016
      LET l_xmdh.xmdh030 = 0
      
      #出貨單位轉包裝單位
      CALL s_aooi250_convert_qty(l_xmdh.xmdh006,l_xmdh.xmdh015,l_xmdh.xmdh202,l_xmdh.xmdh016)
         RETURNING l_success,l_xmdh.xmdh203
      #對出貨包裝單位數量進行取位
      CALL s_aooi250_take_decimals(l_xmdh.xmdh202,l_xmdh.xmdh203)
           RETURNING l_success,l_xmdh.xmdh203
      LET l_xmdh.xmdh204 = l_xmdh.xmdh203
      
      #對出貨數量進行取位
      CALL s_aooi250_take_decimals(l_xmdh.xmdh015,l_xmdh.xmdh016)
           RETURNING l_success,l_xmdh.xmdh016
      #若料號有設置使用參考單位時且出貨單位與參考單位有設置換算率時，則應自動推算參考數量
      IF NOT cl_null(l_xmdh.xmdh018) THEN
         CALL s_aooi250_convert_qty(l_xmdh.xmdh006,l_xmdh.xmdh015,l_xmdh.xmdh018,l_xmdh.xmdh016)
              RETURNING l_success,l_xmdh.xmdh019
      END IF
      #若料號有使用銷售計價單位時，則輸入[C:出貨數量]時則應自動推算計價數量
      IF NOT cl_null(l_xmdh.xmdh020) THEN
         CALL s_aooi250_convert_qty(l_xmdh.xmdh006,l_xmdh.xmdh015,l_xmdh.xmdh020,l_xmdh.xmdh016)
              RETURNING l_success,l_xmdh.xmdh021
      END IF

      CALL s_tax_ins(g_xmdg_m.xmdgdocno,l_xmdh.xmdhseq,0,l_xmdh.xmdhunit,l_xmdh.xmdh021 * l_xmdh.xmdh023,
                     l_xmdh.xmdh024,l_xmdh.xmdh021,g_xmdg_m.xmdg014,1,'','','' )
           RETURNING l_xmdh.xmdh026,l_xmdh.xmdh028,l_xmdh.xmdh027,
                     l_xrcd113,l_xrcd114,l_xrcd115,l_xrcd123,l_xrcd124,l_xrcd125,l_xrcd133,l_xrcd134,l_xrcd135
         

      INSERT INTO xmdh_t (xmdhent, xmdhsite, xmdhunit, xmdhdocno, xmdhseq,
                          xmdh001, xmdh002,  xmdh003,  xmdh004,   xmdh005,
                          xmdh006, xmdh007,  xmdh008,  xmdh009,   xmdh010,
                          xmdh011, xmdh012,  xmdh013,  xmdh014,   xmdh015,
                          xmdh016, xmdh017,  xmdh018,  xmdh019,   xmdh020,
                          xmdh021, xmdh022,  xmdh023,  xmdh024,   xmdh025,
                          xmdh026, xmdh027,  xmdh028,  xmdh029,   xmdh031,
                          xmdh032, xmdh033,  xmdh034,  xmdh035,   xmdh036,
                          xmdh200, xmdh201,  xmdh202,  xmdh203,   xmdh204,
                          xmdh207, xmdh208,  xmdh209,  xmdh210,   xmdh211,
                          xmdh212, xmdh213,  xmdh214,  xmdh215,   xmdh216,
                          xmdh217, xmdh218,  xmdh219,  xmdh220,   xmdh221,
                          xmdh222, xmdh223,  xmdh224)
         VALUES (l_xmdh.xmdhent, l_xmdh.xmdhsite, l_xmdh.xmdhunit, l_xmdh.xmdhdocno, l_xmdh.xmdhseq,
                 l_xmdh.xmdh001, l_xmdh.xmdh002,  l_xmdh.xmdh003,  l_xmdh.xmdh004,   l_xmdh.xmdh005,
                 l_xmdh.xmdh006, l_xmdh.xmdh007,  l_xmdh.xmdh008,  l_xmdh.xmdh009,   l_xmdh.xmdh010,
                 l_xmdh.xmdh011, l_xmdh.xmdh012,  l_xmdh.xmdh013,  l_xmdh.xmdh014,   l_xmdh.xmdh015,
                 l_xmdh.xmdh016, l_xmdh.xmdh017,  l_xmdh.xmdh018,  l_xmdh.xmdh019,   l_xmdh.xmdh020,
                 l_xmdh.xmdh021, l_xmdh.xmdh022,  l_xmdh.xmdh023,  l_xmdh.xmdh024,   l_xmdh.xmdh025,
                 l_xmdh.xmdh026, l_xmdh.xmdh027,  l_xmdh.xmdh028,  l_xmdh.xmdh029,   l_xmdh.xmdh031,
                 l_xmdh.xmdh032, l_xmdh.xmdh033,  l_xmdh.xmdh034,  l_xmdh.xmdh035,   l_xmdh.xmdh036,
                 l_xmdh.xmdh200, l_xmdh.xmdh201,  l_xmdh.xmdh202,  l_xmdh.xmdh203,   l_xmdh.xmdh204,
                 l_xmdh.xmdh207, l_xmdh.xmdh208,  l_xmdh.xmdh209,  l_xmdh.xmdh210,   l_xmdh.xmdh211,
                 l_xmdh.xmdh212, l_xmdh.xmdh213,  l_xmdh.xmdh214,  l_xmdh.xmdh215,   l_xmdh.xmdh216,
                 l_xmdh.xmdh217, l_xmdh.xmdh218,  l_xmdh.xmdh219,  l_xmdh.xmdh220,   l_xmdh.xmdh221,
                 l_xmdh.xmdh222, l_xmdh.xmdh223,  l_xmdh.xmdh224)
      IF SQLCA.sqlcode THEN
         LET r_success = FALSE
         EXIT FOREACH
      END IF
      LET l_success = ''
      CALL adbt540_01_xmdm_modify('2',l_xmdh.xmdhseq,g_xmdg_m.xmdgsite,g_xmdg_m.xmdgdocno,
