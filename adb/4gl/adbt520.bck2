################################################################################
PUBLIC FUNCTION adbt520_get_xmdg017()
DEFINE l_address        STRING
DEFINE l_oofb011        LIKE oofb_t.oofb011
DEFINE l_oofb019         LIKE oofb_t.oofb019    #簡要代碼
DEFINE l_oofb022         LIKE oofb_t.oofb022    #送貨站點
DEFINE l_dbaf001         LIKE dbaf_t.dbaf001    #路線編號

   #送貨客戶值取其該客戶主檔設置的連絡地址簡要代碼(地址類型為'3:送貨地址')預設
   #給[C:送貨地址]xmdg017欄位，若有設置多筆時已有勾選主要的那一筆做預設
   #收貨站點編號不為空，至[T:路線順序規劃資料檔]中，抓取[C:類型]dbaf002='1.去程'
   #且[C:站點編號]dbaf003 = xmdg204站點資料的[C:路線編號]dbaf001值給[C:運輸路線編號]xmdg205 
   CALL s_adb_address_default('3',g_xmdg_m.xmdg007)
      RETURNING l_oofb019,l_oofb022,l_dbaf001
 
   LET g_xmdg_m.xmdg017 = l_oofb019   #簡要代碼
   LET g_xmdg_m.xmdg204 = l_oofb022   #送貨站點
   LET g_xmdg_m.xmdg205 = l_dbaf001   #路線編號
   
   LET l_address = '' 
   LET l_oofb011 = ''
   #帶出送貨地址的參考欄位資訊
   CALL s_adb_address_ref('3',g_xmdg_m.xmdg017,g_xmdg_m.xmdg007)
      RETURNING l_oofb011,l_address
   LET g_xmdg_m.xmdg017_desc = l_oofb011
   LET g_xmdg_m.textedit1 = l_address
   DISPLAY BY NAME g_xmdg_m.xmdg017_desc,g_xmdg_m.textedit1
   
   CALL adbt520_dbadl003(g_xmdg_m.xmdg204) RETURNING g_xmdg_m.xmdg204_desc
   DISPLAY BY NAME g_xmdg_m.xmdg204_desc
   CALL s_desc_get_dbab001_desc(g_xmdg_m.xmdg205) RETURNING g_xmdg_m.xmdg205_desc
   DISPLAY BY NAME g_xmdg_m.xmdg205_desc
   
END FUNCTION

