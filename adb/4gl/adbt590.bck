               UPDATE xmdl_t SET (xmdldocno,xmdlsite,xmdlseq,xmdl001,xmdl002,xmdl003,xmdl004,xmdl005, 
                   xmdl006,xmdl007,xmdl226,xmdl008,xmdl009,xmdl033,xmdl011,xmdl012,xmdl204,xmdl206,xmdl017, 
