PRIVATE FUNCTION adbt590_xmdl014_chk()
   DEFINE r_success      LIKE type_t.num5
   DEFINE l_xmdl014      LIKE xmdl_t.xmdl014
   DEFINE l_type1        LIKE type_t.chr1
   DEFINE l_type2        LIKE type_t.chr1
   
   LET r_success = TRUE
   
   #庫位
   IF NOT cl_null(g_xmdl_d[l_ac].xmdl014)THEN     
      #設定g_chkparam.*的參數前，先將其初始化，避免之前設定遺留的參數值造成影響。
      INITIALIZE g_chkparam.* TO NULL
               
      #設定g_chkparam.*的參數
      LET g_chkparam.arg1 = g_site
      LET g_chkparam.arg2 = g_xmdl_d[l_ac].xmdl014    #庫位
      #160318-00025#18  by 07900 --add-str
      LET g_errshow = TRUE #是否開窗                   
      LET g_chkparam.err_str[1] ="aim-00065:sub-01302|aini001|",cl_get_progname("aini001",g_lang,"2"),"|:EXEPROGaini001"
      #160318-00025#18  by 07900 --add-end                 
      #呼叫檢查存在並帶值的library
      IF NOT cl_chk_exist("v_inaa001") THEN
         LET r_success = FALSE
         RETURN r_success
      END IF
      
      #原始出貨單庫位
      LET l_xmdl014 = ''
      
      SELECT xmdl014
        INTO l_xmdl014
        FROM xmdl_t
       WHERE xmdlent = g_enterprise
         AND xmdldocno = g_xmdl_d[l_ac].xmdl001
         AND xmdlseq = g_xmdl_d[l_ac].xmdl002
      
      #150302-00004#14 150305 by lori522612 mod---(S)      
      #出貨單庫位
      CALL s_adb_get_inaa010(g_site,l_xmdl014) RETURNING l_type1                #出貨單庫位 
      CALL s_adb_get_inaa010(g_site,g_xmdl_d[l_ac].xmdl014) RETURNING l_type2   #新輸入庫位
      #150302-00004#14 150305 by lori522612 mod---(E)
      
      
      IF l_type1 <> l_type2 THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'axm-00250'
         LET g_errparam.extend = ''
         LET g_errparam.popup = TRUE
         LET g_errparam.replace[1] = l_xmdl014 
         LET g_errparam.replace[2] =  g_xmdl_d[l_ac].xmdl014
         CALL cl_err()

         LET r_success = FALSE
         RETURN r_success
      END IF
   END IF
   
   RETURN r_success
END FUNCTION

