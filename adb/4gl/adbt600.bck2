     FROM xmdk_t
    WHERE xmdkent = g_enterprise
      AND xmdk000 IN ('1','2','4')
      AND xmdkdocno = g_xmdk_m.xmdk005
         
   IF cl_null(g_xmdk_m.xmdk084) THEN
      LET g_xmdk_m.xmdk084 = '1'
   END IF
   IF cl_null(g_xmdk_m.xmdk042) THEN
