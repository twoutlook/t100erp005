{<section id="adbt600.set_no_entry" >}
#+ 單頭欄位關閉設定
PRIVATE FUNCTION adbt600_set_no_entry(p_cmd)
   #add-point:set_no_entry段define(客製用) name="set_no_entry.define_customerization"
   
   #end add-point     
   DEFINE p_cmd   LIKE type_t.chr1   
   #add-point:set_no_entry段define (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="set_no_entry.define"
   DEFINE l_success  LIKE type_t.num5
   DEFINE l_slip     LIKE ooba_t.ooba001
   #end add-point     
   
   #add-point:Function前置處理  name="set_no_entry.pre_function"
   
   #end add-point
   
   IF p_cmd = 'u' AND g_chkey = 'N' THEN
      CALL cl_set_comp_entry("xmdkdocno",FALSE)
      #根據azzi850使用者身分開關特定欄位
      IF NOT cl_null(g_no_entry) THEN
         CALL cl_set_comp_entry(g_no_entry,FALSE)
      END IF
      #add-point:set_no_entry段欄位控制 name="set_no_entry.field_control"
      CALL cl_set_comp_entry("xmdkdocdt,xmdksite",FALSE)                                                
      #end add-point 
   END IF 
   
   IF p_cmd = 'u' THEN  #docno,ld欄位確認是絕對關閉
      CALL cl_set_comp_entry("xmdkdocno",FALSE)
   END IF 
 
   IF p_cmd = 'u' THEN  #docdt欄位依照設定關閉(FALSE則為設定不同意修正)
      IF NOT cl_chk_update_docdt() THEN
         CALL cl_set_comp_entry("xmdkdocdt",FALSE)
      END IF
   END IF 
   
   #add-point:set_no_entry段欄位控制後 name="set_no_entry.after_control"
   IF p_cmd = 'a' AND g_flag THEN
   #IF (NOT s_aooi500_comp_entry(g_prog,'xmdksite') OR g_flag = 'Y') THEN
      CALL cl_set_comp_entry("xmdksite",FALSE)
   END IF
   #若單據參數設定銷退來源不需對應出貨單時
   CALL s_aooi200_get_slip(g_xmdk_m.xmdkdocno) RETURNING l_success,l_slip
   IF l_success THEN
      IF cl_get_doc_para(g_enterprise,g_xmdk_m.xmdksite,l_slip,'D-BAS-0082') = 'N' THEN
         CALL cl_set_comp_entry("xmdk005",FALSE)
      END IF
   END IF
   IF NOT cl_null(g_xmdk_m.xmdk208) THEN
      CALL cl_set_comp_entry("xmdk007",FALSE)      #客戶編號
      CALL cl_set_comp_entry("xmdk200",FALSE)      #調貨供應商
      CALL cl_set_comp_entry("xmdk201",FALSE)      #代送商
      CALL cl_set_comp_entry("xmdk009",FALSE)      #送貨客戶
      CALL cl_set_comp_entry("xmdk021",FALSE)      #送貨地址
      CALL cl_set_comp_entry("xmdk012",FALSE)      #稅別
      CALL cl_set_comp_entry("xmdk016",FALSE)      #幣別  
      CALL cl_set_comp_entry("xmdk082",FALSE)
      #CALL cl_set_comp_entry("xmdk008",FALSE)      #收款客戶
      #CALL cl_set_comp_entry("xmdk202",FALSE)      #發票客戶
      CALL cl_set_comp_entry("xmdk209,xmdk210,xmdk211,xmdk212",FALSE) 
      CALL cl_set_comp_entry("xmdk042,xmdk043",FALSE)
   ELSE
      #若已輸入來源出貨單
      IF NOT cl_null(g_xmdk_m.xmdk005) THEN
         CALL cl_set_comp_entry("xmdk007",FALSE)      #客戶編號
         CALL cl_set_comp_entry("xmdk200",FALSE)      #調貨供應商
         CALL cl_set_comp_entry("xmdk201",FALSE)      #代送商
         CALL cl_set_comp_entry("xmdk009",FALSE)      #送貨客戶
         CALL cl_set_comp_entry("xmdk021",FALSE)      #送貨地址
         CALL cl_set_comp_entry("xmdk008",FALSE)      #收款客戶
         CALL cl_set_comp_entry("xmdk202",FALSE)      #發票客戶
         #lori522612  150119  add ----------------------(S)
         #CALL cl_set_comp_entry("xmdk015",FALSE)      #發票類型
         #lori522612  150119  add ----------------------(E)
         CALL cl_set_comp_entry("xmdk010",FALSE)      #收款條件
         CALL cl_set_comp_entry("xmdk011",FALSE)      #交易條件
         CALL cl_set_comp_entry("xmdk012",FALSE)      #稅別
         CALL cl_set_comp_entry("xmdk016",FALSE)      #幣別
         CALL cl_set_comp_entry("xmdk209,xmdk210,xmdk211,xmdk212",FALSE)
         CALL cl_set_comp_entry("xmdk042,xmdk043",FALSE)         
      END IF
      #單身有資料時，出貨單號欄位不可再修改
      IF adbt600_xmdl001_count() THEN
         CALL cl_set_comp_entry("xmdk005",FALSE)
         CALL cl_set_comp_entry("xmdk007,xmdk008,xmdk009,xmdk012,xmdk016,xmdk202",FALSE)
         #lori522612  150119  add ----------------------(S)
         #CALL cl_set_comp_entry("xmdk010,xmdk011,xmdk015",FALSE)
         CALL cl_set_comp_entry("xmdk010,xmdk011",FALSE)
         #lori522612  150119  add ----------------------(E)
         CALL cl_set_comp_entry("xmdk209,xmdk210,xmdk211,xmdk212",FALSE) 
      END IF
   END IF
   IF adbt600_xmdl001_count() THEN
      CALL cl_set_comp_entry("xmdk207",FALSE)      #銷售組織
   END IF
   #end add-point 
 
END FUNCTION
 
