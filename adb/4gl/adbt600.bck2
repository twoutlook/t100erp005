      CALL cl_set_comp_entry("xmdl014,xmdl015,xmdl016",FALSE)
   END IF
   
   #儲位控管若為5.不使用儲位控管         
   IF NOT s_adbt600_inaa007_chk(g_xmdl_d[l_ac].xmdl014) THEN
      CALL cl_set_comp_entry("xmdl015",FALSE)
   END IF
   #批號管控
   IF NOT adbt600_imaf061_chk() THEN
      CALL cl_set_comp_entry("xmdl016",FALSE)
   END IF
   
   IF cl_get_para(g_enterprise,g_xmdk_m.xmdksite,'S-BAS-0007') = "N" THEN  #參數使用銷售計價單位
      CALL cl_set_comp_entry("xmdl022",FALSE)
   END IF
   #判斷據點參數若不使用參考單位(據點參數:S-BAS-0028)
   IF cl_get_para(g_enterprise,g_xmdk_m.xmdksite,'S-BAS-0028') = 'N' THEN
      CALL cl_set_comp_entry("xmdl019,xmdl020",FALSE)
   END IF
   
   IF NOT cl_null(g_xmdl_d[l_ac].xmdl001) AND NOT cl_null(g_xmdl_d[l_ac].xmdl002) THEN
      CALL cl_set_comp_entry("xmdl008,xmdl009",FALSE)
   END IF

   IF cl_null(g_xmdl_d[l_ac].xmdl019) THEN
      CALL cl_set_comp_entry("xmdl020",FALSE)
   END IF   
   IF NOT cl_null(g_xmdk_m.xmdk208) THEN
      CALL cl_set_comp_entry("xmdlseq,xmdl001,xmdl002,xmdl008,xmdl009,xmdl050,xmdl205,xmdl020,xmdl022",FALSE)
      CALL cl_set_comp_entry("xmdl048,xmdl049",FALSE)
   END IF
   
   #ken------------------------------------s
   IF NOT cl_null(g_xmdl_d[l_ac].xmdl018)  THEN
