   #匯率
   LET l_xmdk017 = ''
   CALL s_adb_get_exchange(g_xmdk_m.xmdksite,g_xmdk_m.xmdk042,g_xmdk_m.xmdk016) RETURNING l_errno,l_xmdk017
   IF NOT cl_null(l_xmdk017) THEN
      LET g_xmdk_m.xmdk017 = l_xmdk017
