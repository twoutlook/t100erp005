   #+ b_fill2段table資料取得(包含sql組成及資料填充)
#Page2
   CALL g_dbea2_d.clear()
 
   #add-point:陣列清空 name="b_fill2.array_clear"
   LET l_cnt = g_dbea_d.getLength()
   IF l_cnt <= 0 THEN
     RETURN
   END IF
   #end add-point
 
#table2
   #為避免影響執行效能，若是按上下筆就不重組SQL
   IF g_action_choice <> "fetch" OR cl_null(g_action_choice) THEN
      LET g_sql = "SELECT  UNIQUE dbeb001,'',dbeb002,'',dbeb003,dbeb004,dbeb005,'',dbeb007,dbeb006,dbeb008, 
          '' FROM dbeb_t",
                  "",
                  " WHERE dbebent=? AND dbebdocno=?"
  
      IF NOT cl_null(g_wc2_table2) THEN
         LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
      END IF
  
      LET g_sql = g_sql, " ORDER BY dbeb_t.dbeb001"
  
      #add-point:單身填充前 name="b_fill2.before_fill2"
      #150826-00013#2 20150907 s983961--mark and mod(s) 效能調整
      #LET g_sql = "SELECT  UNIQUE dbeb001,t1.dbabl003,dbeb002,t2.dbael003,dbeb003,dbeb004,dbeb005,t3.oocal003,dbeb007,dbeb006,dbeb008,t4.oocal003 ",
      #            "  FROM  dbeb_t",
      #            "  LEFT OUTER JOIN dbabl_t t1 ON dbebent = t1.dbablent AND dbeb001 = t1.dbabl001 AND t1.dbabl002 = '",g_dlang,"' ",  #路線說明
      #            "  LEFT OUTER JOIN dbael_t t2 ON dbebent = t2.dbaelent AND dbeb002 = t2.dbael001 AND t2.dbael002 = '",g_dlang,"' ",  #裝載點說明
      #            "  LEFT OUTER JOIN oocal_t t3 ON dbebent = t3.oocalent AND dbeb005 = t3.oocal001 AND t3.oocal002 = '",g_dlang,"' ",  #承載容積單位說明
      #            "  LEFT OUTER JOIN oocal_t t4 ON dbebent = t4.oocalent AND dbeb008 = t4.oocal001 AND t4.oocal002 = '",g_dlang,"' ",  #承載重量單位說明
      #            " WHERE dbebent=? AND dbebdocno=?"
                  
      LET g_sql = "SELECT  UNIQUE dbeb001, ",
                  "               (SELECT dbabl003 FROM dbabl_t ",
                  "                 WHERE dbablent = dbebent ",
                  "                   AND dbabl001 = dbeb001 ",
                  "                   AND dbabl002 ='"||g_dlang||"') dbeb001_desc, ", #路線說明
                  "               dbeb002, ",
                  "               (SELECT dbael003 FROM dbael_t ",
                  "                 WHERE dbaelent = dbebent ",
                  "                   AND dbael001 = dbeb002 ",
                  "                   AND dbael002 ='"||g_dlang||"') dbeb002_desc, ", #裝載點說明
                  "               dbeb003, ",
                  "               dbeb004, ",
                  "               dbeb005, ",
                  "               (SELECT oocal003 FROM oocal_t ",
                  "                 WHERE oocalent = dbebent ",
                  "                   AND oocal001 = dbeb005 ",
                  "                   AND oocal002 ='"||g_dlang||"') dbeb005_desc, ", #承載容積單位說明
                  "               dbeb007, ",
                  "               dbeb006, ",
                  "               dbeb008, ",
                  "               (SELECT oocal003 FROM oocal_t ",
                  "                 WHERE oocalent = dbebent ",
                  "                   AND oocal001 = dbeb008 ",
                  "                   AND oocal002 ='"||g_dlang||"') dbeb008_desc ", #承載重量單位說明
                  "  FROM  dbeb_t",
                  " WHERE dbebent=? AND dbebdocno=?"                       
      #150826-00013#2 20150907 s983961--mark and mod(e) 效能調整                  
      IF NOT cl_null(g_wc2_table2) THEN
         LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
      END IF

      LET g_sql = g_sql, " ORDER BY dbeb_t.dbeb001"                  
      #end add-point
 
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE adbq700_pb2 FROM g_sql
      DECLARE b_fill_curs2 CURSOR FOR adbq700_pb2
   END IF
 
   OPEN b_fill_curs2 USING g_enterprise,g_dbea_d[g_detail_idx].dbeadocno
 
   LET l_ac = 1
   FOREACH b_fill_curs2 INTO g_dbea2_d[l_ac].dbeb001,g_dbea2_d[l_ac].dbeb001_desc,g_dbea2_d[l_ac].dbeb002, 
       g_dbea2_d[l_ac].dbeb002_desc,g_dbea2_d[l_ac].dbeb003,g_dbea2_d[l_ac].dbeb004,g_dbea2_d[l_ac].dbeb005, 
       g_dbea2_d[l_ac].dbeb005_desc,g_dbea2_d[l_ac].dbeb007,g_dbea2_d[l_ac].dbeb006,g_dbea2_d[l_ac].dbeb008, 
       g_dbea2_d[l_ac].dbeb008_desc
      IF SQLCA.sqlcode THEN
