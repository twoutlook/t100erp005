{<section id="adbq700.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION adbq700_b_fill()
   #add-point:b_fill段define-客製 name="b_fill.define_customerization"
   
   #end add-point
   DEFINE ls_wc           STRING
   DEFINE l_pid           LIKE type_t.chr50
   DEFINE ls_sql_rank     STRING
   #add-point:b_fill段define-標準  (請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   DEFINE l_where           STRING 
   #end add-point
 
   #add-point:b_fill段sql_before name="b_fill.sql_before"
   CALL s_aooi500_sql_where(g_prog,'dbeasite') RETURNING l_where
   #end add-point
 
 
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
 
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
 
   CALL g_dbea_d.clear()
 
   #add-point:陣列清空 name="b_fill.array_clear"
   IF tm.stus != 'ALL' THEN
      LET l_where = l_where CLIPPED," AND dbeastus = '",tm.stus CLIPPED,"'"
   END IF
   
   LET ls_wc = ls_wc CLIPPED," AND ",l_where
   #end add-point
 
   LET g_cnt = l_ac
   IF g_cnt = 0 THEN
      LET g_cnt = 1
   END IF
   LET l_ac = 1
 
   # b_fill段sql組成及FOREACH撰寫
   #應用 qs04 樣板自動產生(Version:9)
   #+ b_fill段資料取得(包含sql組成及FOREACH段撰寫)
   LET ls_sql_rank = "SELECT  UNIQUE '',dbeasite,'',dbeadocno,dbeadocdt,dbea001,dbea002,'',dbea003,'', 
       dbeastus  ,DENSE_RANK() OVER( ORDER BY dbea_t.dbeadocno) AS RANK FROM dbea_t",
 
#table2
                     " LEFT JOIN dbeb_t ON dbebent = dbeaent AND dbeadocno = dbebdocno",
 
                     "",
                     " WHERE dbeaent= ? AND 1=1 AND ", ls_wc
   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("dbea_t"),
                     " ORDER BY dbea_t.dbeadocno"
 
   #add-point:b_fill段rank_sql_after name="b_fill.rank_sql_after"
   #150826-00013#2 20150907 s983961--add(s) 效能調整
   LET ls_sql_rank = "SELECT  UNIQUE 'N',dbeasite, ",
                     "                   (SELECT ooefl003 FROM ooefl_t ",
                     "                     WHERE ooeflent = dbeaent ",
                     "                       AND ooefl001 = dbeasite ",
                     "                       AND ooefl002 ='"||g_dlang||"') dbeasite_desc, ",  #配送組織說明
                     "                   dbeadocno, ",
                     "                   dbeadocdt, ",
                     "                   dbea001,   ",
                     "                   dbea002,   ",
                     "                   (SELECT ooag011 FROM ooag_t ",
                     "                     WHERE ooagent = dbeaent ",
                     "                       AND ooag001 = dbea002) dbea002_desc, ",
                     "                   dbea003,   ",
                     "                   (SELECT ooefl003 FROM ooefl_t ",
                     "                     WHERE ooeflent = dbeaent ",
                     "                       AND ooefl001 = dbea003 ",
                     "                       AND ooefl002 ='"||g_dlang||"') dbea003_desc, ",  #預排部門說明
                     "                   dbeastus,  ",
                     "        DENSE_RANK() OVER( ORDER BY dbea_t.dbeadocno) AS RANK ",
                     "  FROM dbea_t,dbeb_t",      
                     "    WHERE dbeaent = ? ",
                     "      AND dbeaent = dbebent ",
                     "      AND dbeadocno = dbebdocno",   
                     "      AND ", ls_wc ," AND ",l_where        

   LET ls_sql_rank = ls_sql_rank, cl_sql_add_filter("dbea_t"),
                     " ORDER BY dbea_t.dbeadocno"
   #150826-00013#2 20150907 s983961--add(s) 效能調整
   #end add-point
 
   LET g_sql = "SELECT COUNT(1) FROM (",ls_sql_rank,")"
 
   PREPARE b_fill_cnt_pre FROM g_sql  #總筆數
   EXECUTE b_fill_cnt_pre USING g_enterprise INTO g_tot_cnt
   FREE b_fill_cnt_pre
 
   #add-point:b_fill段rank_sql_after_count name="b_fill.rank_sql_after_count"
   
   #end add-point
 
   CASE g_detail_page_action
      WHEN "detail_first"
          LET g_pagestart = 1
 
      WHEN "detail_previous"
          LET g_pagestart = g_pagestart - g_num_in_page
          IF g_pagestart < 1 THEN
              LET g_pagestart = 1
          END IF
 
      WHEN "detail_next"
         LET g_pagestart = g_pagestart + g_num_in_page
         IF g_pagestart > g_tot_cnt THEN
            LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
            WHILE g_pagestart > g_tot_cnt
               LET g_pagestart = g_pagestart - g_num_in_page
            END WHILE
         END IF
 
      WHEN "detail_last"
         LET g_pagestart = g_tot_cnt - (g_tot_cnt mod g_num_in_page) + 1
         WHILE g_pagestart > g_tot_cnt
            LET g_pagestart = g_pagestart - g_num_in_page
         END WHILE
 
      OTHERWISE
         LET g_pagestart = 1
 
   END CASE
 
   LET g_sql = "SELECT '',dbeasite,'',dbeadocno,dbeadocdt,dbea001,dbea002,'',dbea003,'',dbeastus",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
 
   #add-point:b_fill段sql_after name="b_fill.sql_after"
   #150826-00013#2 20150907 s983961--mark and mod(s) 效能調整
   #LET g_sql = "SELECT  UNIQUE 'N',dbeasite,t1.ooefl003,dbeadocno,dbeadocdt, ",
   #            "        dbea001,dbea002,t2.ooag011,dbea003,t3.ooefl003, ",
   #            "        dbeastus ",
   #            "        FROM dbea_t",
   #            "    LEFT OUTER JOIN ooefl_t t1 ON dbeaent = t1.ooeflent AND dbeasite = t1.ooefl001 AND t1.ooefl002 = '",g_dlang,"' ",  #配送組織說明
   #            "    LEFT OUTER JOIN ooag_t  t2 ON dbeaent = t2.ooagent AND dbea002 = t2.ooag001 ",  #預排人員全名
   #            "    LEFT OUTER JOIN ooefl_t t3 ON dbeaent = t3.ooeflent AND dbea003 = t3.ooefl001 AND t3.ooefl002 = '",g_dlang,"' ",  #預排部門說明
   #            "        ,dbeb_t",       
   #            "    WHERE dbeaent = ? ",
   #            "      AND dbeaent = dbebent ",
   #            "      AND dbeadocno = dbebdocno",   
   #            "      AND ", ls_wc ," AND ",l_where        
   #
   #LET g_sql = g_sql, cl_sql_add_filter("dbea_t"),
   #                   " ORDER BY dbea_t.dbeadocno"
   LET g_sql = "SELECT 'N',dbeasite,dbeasite_desc,dbeadocno,dbeadocdt,dbea001,
                          dbea002,dbea002_desc,dbea003,dbea003_desc,dbeastus",
               " FROM (",ls_sql_rank,")",
              " WHERE RANK >= ",g_pagestart,
                " AND RANK < ",g_pagestart + g_num_in_page
   #150826-00013#2 20150907 s983961--mark and mod(e) 效能調整
   #end add-point
 
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE adbq700_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR adbq700_pb
 
   OPEN b_fill_curs USING g_enterprise
 
   FOREACH b_fill_curs INTO g_dbea_d[l_ac].sel,g_dbea_d[l_ac].dbeasite,g_dbea_d[l_ac].dbeasite_desc, 
       g_dbea_d[l_ac].dbeadocno,g_dbea_d[l_ac].dbeadocdt,g_dbea_d[l_ac].dbea001,g_dbea_d[l_ac].dbea002, 
       g_dbea_d[l_ac].dbea002_desc,g_dbea_d[l_ac].dbea003,g_dbea_d[l_ac].dbea003_desc,g_dbea_d[l_ac].dbeastus 
 
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
 
      
 
      #add-point:b_fill段資料填充 name="b_fill.fill"
      
      #end add-point
 
      CALL adbq700_detail_show("'1'")
 
      CALL adbq700_dbea_t_mask()
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
         END IF
         EXIT FOREACH
      END IF
      LET l_ac = l_ac + 1
 
   END FOREACH
 
 
 
 
 
   #應用 qs05 樣板自動產生(Version:3)
   #+ b_fill段其他table資料取得(包含sql組成及資料填充)
 
 
 
 
 
 
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
   
   #end add-point
 
   CALL g_dbea_d.deleteElement(g_dbea_d.getLength())
 
   #add-point:陣列長度調整 name="b_fill.array_deleteElement"
   
   #end add-point
 
   LET g_error_show = 0
 
   LET g_detail_cnt = g_dbea_d.getLength()
   LET l_ac = g_cnt
   LET g_cnt = 0
 
   #應用 qs06 樣板自動產生(Version:3)
   #+ b_fill段CURSOR釋放
   CLOSE b_fill_curs
   FREE adbq700_pb
 
 
 
 
 
 
   #調整單身index指標，避免翻頁後指到空白筆數
   CALL adbq700_detail_index_setting()
 
   #重新計算單身筆數並呈現
   CALL adbq700_detail_action_trans()
 
   LET l_ac = 1
   IF g_dbea_d.getLength() > 0 THEN
      CALL adbq700_b_fill2()
   END IF
 
      CALL adbq700_filter_show('dbeasite','b_dbeasite')
   CALL adbq700_filter_show('dbeadocno','b_dbeadocno')
   CALL adbq700_filter_show('dbeadocdt','b_dbeadocdt')
   CALL adbq700_filter_show('dbea001','b_dbea001')
   CALL adbq700_filter_show('dbea002','b_dbea002')
   CALL adbq700_filter_show('dbea003','b_dbea003')
   CALL adbq700_filter_show('dbeastus','b_dbeastus')
   CALL adbq700_filter_show('dbeb001','b_dbeb001')
   CALL adbq700_filter_show('dbeb002','b_dbeb002')
   CALL adbq700_filter_show('dbeb003','b_dbeb003')
   CALL adbq700_filter_show('dbeb004','b_dbeb004')
   CALL adbq700_filter_show('dbeb005','b_dbeb005')
   CALL adbq700_filter_show('dbeb007','b_dbeb007')
   CALL adbq700_filter_show('dbeb006','b_dbeb006')
   CALL adbq700_filter_show('dbeb008','b_dbeb008')
 
 
END FUNCTION
 
