################################################################################
# Descriptions...: 修改，删除时重新检查状态
# Date & Author..: #160818-00017#5 2016/08/22 By 08172
# Modify.........:
################################################################################
PRIVATE FUNCTION adbm201_action_chk()
   SELECT pmaastus  INTO g_pmaa_m.pmaastus
     FROM pmaa_t
    WHERE pmaaent = g_enterprise
      AND pmaa001 = g_pmaa_m.pmaa001
   LET g_errno = NULL
   IF (g_action_choice="modify" OR g_action_choice="modify_detail" OR g_action_choice="delete")  THEN
     CASE g_pmaa_m.pmaastus
        WHEN 'W'
           LET g_errno = 'sub-00180'
        WHEN 'X'
           LET g_errno = 'sub-00229'
        WHEN 'Y'
           LET g_errno = 'sub-00178'
        WHEN 'S'
           LET g_errno = 'sub-00230'
     END CASE

     IF NOT cl_null(g_errno) THEN
        INITIALIZE g_errparam TO NULL
        LET g_errparam.code = g_errno
        LET g_errparam.extend = g_pmaa_m.pmaa001
        LET g_errparam.popup = TRUE
        CALL cl_err()
        LET g_errno = NULL
        CALL adbm201_show()
        RETURN FALSE
     END IF
   END IF
  
   RETURN TRUE
END FUNCTION

