            CALL cl_err()

            CALL s_transaction_end('N','0')
            RETURN
         END IF
      END IF           {#ADP版次:1#}
      #end add-point



      CLEAR FORM
      CALL adbm201_ui_browser_refresh()
      IF g_browser_cnt > 0 THEN
         CALL adbm201_fetch("P")
      ELSE
         CALL adbm201_browser_fill(" 1=1 ","F")
      END IF

   END IF

   CLOSE adbm201_cl
   CALL s_transaction_end('Y','0')

   #流程通知預埋點-D
   CALL cl_flow_notify(g_pmaa_m.pmaa001,"D")

