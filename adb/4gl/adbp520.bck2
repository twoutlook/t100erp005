PRIVATE FUNCTION adbp520_set_no_entry_b()
   DEFINE l_xmdc022      LIKE xmdc_t.xmdc022
   
   LET l_xmdc022 = ''
   SELECT xmdc022 INTO l_xmdc022 FROM xmdc_t
    WHERE xmdcent = g_enterprise
      AND xmdcdocno = g_detail_d[l_ac].xmdadocno
      AND xmdcseq   = g_detail_d[l_ac].xmddseq
   IF l_xmdc022 = 'N' THEN
      CALL cl_set_comp_entry("b_xmdd202,b_xmdd006",FALSE)
   END IF
   
   IF g_detail_d[l_ac].sel = 'N' THEN
      CALL cl_set_comp_entry("b_xmdd202,b_xmdd006,b_xmdd025",FALSE)
   END IF
   IF NOT cl_null(g_detail_d[l_ac].xmdd006) THEN
      CALL cl_set_comp_entry("b_xmdd202",FALSE)
   END IF
   IF cl_null(g_detail_d[l_ac].xmdd024) THEN
      CALL cl_set_comp_entry("b_xmdd025",FALSE)
   END IF
END FUNCTION
#於整批處理前,校驗當下那筆資料(l_ac)的正確性
PRIVATE FUNCTION adbp520_chk_data()
   DEFINE r_success  LIKE type_t.chr1

   LET r_success = TRUE

   IF g_detail_d[l_ac].sel = 'Y' AND (cl_null(g_detail_d[l_ac].xmdd202) OR cl_null(g_detail_d[l_ac].xmdd006)) THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.extend = ''
      LET g_errparam.code   = 'adb-00282'
      LET g_errparam.popup  = TRUE
      CALL cl_err()
      LET r_success = FALSE
      RETURN r_success
   END IF

   IF NOT cl_ap_chk_Range(g_detail_d[l_ac].xmdd202,"0.000","1","","","azz-00079",1) THEN
      LET g_detail_d[l_ac].xmdd202 = g_detail_d_t.xmdd202
      LET r_success = FALSE
      RETURN r_success
   END IF
   
   IF NOT cl_ap_chk_Range(g_detail_d[l_ac].xmdd006,"0.000","0","","","azz-00079",1) THEN
      LET g_detail_d[l_ac].xmdd006 = g_detail_d_t.xmdd006
      LET r_success = FALSE
      RETURN r_success
   END IF
   
   IF NOT adbp520_chk_xmdd006(g_detail_d[l_ac].xmdd006) THEN
      LET r_success = FALSE
      RETURN r_success
   END IF

   IF NOT cl_ap_chk_Range(g_detail_d[l_ac].xmdd025,"0.000","1","","","azz-00079",1) THEN
      LET g_detail_d[l_ac].xmdd025 = g_detail_d_t.xmdd025
      LET r_success = FALSE
      RETURN r_success
   END IF

   RETURN r_success
END FUNCTION

PRIVATE FUNCTION adbp520_chk_xmdd202()
   DEFINE l_xmdh016   LIKE xmdh_t.xmdh016
   DEFINE l_xmdd004   LIKE xmdd_t.xmdd004
   DEFINE l_xmdd202   LIKE xmdd_t.xmdd202
   DEFINE l_success   LIKE type_t.num5
   DEFINE l_cnt       LIKE type_t.num5
   DEFINE l_str       STRING
   DEFINE r_success   LIKE type_t.num5

   LET r_success = TRUE

   CALL s_aooi250_convert_qty(g_detail_d[l_ac].xmdd001,g_detail_d[l_ac].xmdd201,g_detail_d[l_ac].xmdd004,g_detail_d[l_ac].xmdd202) RETURNING l_success,l_xmdh016
   IF l_success THEN
      IF NOT adbp520_chk_xmdd006(l_xmdh016) THEN
         LET r_success = FALSE
         RETURN r_success
      END IF
   ELSE
      LET r_success = FALSE
      RETURN r_success
   END IF

   RETURN r_success
END FUNCTION

PRIVATE FUNCTION adbp520_chk_xmdd006(p_xmdh016)
   DEFINE p_xmdh016   LIKE xmdh_t.xmdh016
   DEFINE l_xmdh016   LIKE xmdh_t.xmdh016
   DEFINE l_xmdd004   LIKE xmdd_t.xmdd004
   DEFINE l_xmdd202   LIKE xmdd_t.xmdd202
   DEFINE l_success   LIKE type_t.num5
   DEFINE l_cnt       LIKE type_t.num5
   DEFINE l_str       STRING
   DEFINE r_success   LIKE type_t.num5

   LET r_success = TRUE

   CALL s_adbt520_xmdd006_xmdd031(g_detail_d[l_ac].xmdadocno, g_detail_d[l_ac].xmddseq,g_detail_d[l_ac].xmddseq1,
                                  g_detail_d[l_ac].xmddseq2 ,0,0) RETURNING l_xmdh016
                                  
   IF p_xmdh016 > l_xmdh016 THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'adb-00122'
      LET g_errparam.extend = p_xmdh016
      LET g_errparam.popup = TRUE
      CALL cl_err()
      LET r_success = FALSE
      RETURN r_success
   END IF

   RETURN r_success
END FUNCTION

