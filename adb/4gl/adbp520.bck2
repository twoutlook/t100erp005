   g_detail_d[l_ac].xmdd207_desc, g_detail_d[l_ac].xmdd208,      g_detail_d[l_ac].xmdd208_desc, g_detail_d[l_ac].xmdd209,
   g_detail_d[l_ac].xmdd209_desc, g_detail_d[l_ac].xmdd031,      g_detail_d[l_ac].xmdd015,      g_detail_d[l_ac].xmdd016,
   g_detail_d[l_ac].xmdd017,      g_detail_d[l_ac].xmdd220,      g_detail_d[l_ac].xmdd220_desc, g_detail_d[l_ac].xmdd221,
   l_xmda022, l_xmda203, l_xmda200, l_xmda201, l_xmda002, 
   l_xmda003, l_xmda009, l_xmda010, l_xmda015, l_xmdd019, 
   l_xmda035, l_xmda024, l_xmda023, l_xmda048, l_xmda049,
   l_xmdd222, l_xmdd220, l_xmddunit 
   #end add-point
   
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
 
         EXIT FOREACH
      END IF
      
      #add-point:b_fill段資料填充
      #如訂單的單據別參數"訂單需走出通單"D-BAS-0077 為'N' 則表示不允許轉成出貨單通知單(即不符合此作業的核心)
      SELECT xmdasite,xmdastus
        INTO l_xmdasite,l_xmdastus
        FROM xmda_t
       WHERE xmdaent = g_enterprise
         AND xmdadocno = g_detail_d[l_ac].xmdadocno
      IF cl_null(l_xmdasite) THEN
         CONTINUE FOREACH
      END IF
      
      CALL s_aooi200_get_slip(g_detail_d[l_ac].xmdadocno) RETURNING l_success,l_slip
      IF l_success THEN 
         CALL cl_get_doc_para(g_enterprise,l_xmdasite,l_slip,'D-BAS-0077') RETURNING l_ooac004
         
         IF l_ooac004 = 'N' THEN
            CONTINUE FOREACH 
         END IF
      ELSE 
         CONTINUE FOREACH
      END IF      
      
      #檢查 當可轉出通數量已經為0或單位數量轉換出現異常時,則不顯示於畫面上
      LET l_xmdh016 = 0 
      CALL s_adbt520_xmdd006_xmdd031(g_detail_d[l_ac].xmdadocno, g_detail_d[l_ac].xmddseq,g_detail_d[l_ac].xmddseq1,
                                     g_detail_d[l_ac].xmddseq2 ,0,0) RETURNING l_xmdh016
      IF NOT cl_null(g_errno) OR l_xmdh016 <= 0 THEN
         CONTINUE FOREACH 
      ELSE
         LET g_detail_d[l_ac].xmdd006 = l_xmdh016
         CALL s_aooi250_convert_qty(g_detail_d[l_ac].xmdd001,g_detail_d[l_ac].xmdd004,g_detail_d[l_ac].xmdd201,l_xmdh016) RETURNING l_success,l_xmdh016
         IF l_success THEN
            LET g_detail_d[l_ac].xmdd202 = l_xmdh016
         END IF
      END IF          
      IF NOT cl_null(g_detail_d[l_ac].xmdd024) THEN
         LET l_xmdd025 = 0
         CALL s_aooi250_convert_qty(g_detail_d[l_ac].xmdd001,g_detail_d[l_ac].xmdd201,g_detail_d[l_ac].xmdd024,g_detail_d[l_ac].xmdd202) RETURNING l_success,l_xmdd025
         IF l_success THEN
            LET g_detail_d[l_ac].xmdd025 = l_xmdd025
         END IF
      END IF
      #在b_fill時 如果已在暫存的temp_table 中,則將temp_table 的值顯示於畫面
      LET l_cnt = 0
      LET l_xmdd202 = ''
      LET l_xmdd006 = ''
      LET l_xmdd025 = ''
      SELECT COUNT(*),xmdd202,xmdd006,xmdd025
        INTO l_cnt,l_xmdd202,l_xmdd006,l_xmdd025
        FROM s_adbp520_temp2
       WHERE xmddent = g_enterprise
         AND xmdddocno = g_detail_d[l_ac].xmdadocno
         AND xmddseq   = g_detail_d[l_ac].xmddseq
         AND xmddseq1  = g_detail_d[l_ac].xmddseq1
         AND xmddseq2  = g_detail_d[l_ac].xmddseq2
         AND sel = 'Y'
       GROUP BY xmdd202,xmdd006,xmdd025

      IF l_cnt = 1 THEN
         LET g_detail_d[l_ac].sel     = 'Y'
         LET g_detail_d[l_ac].xmdd202 = l_xmdd202
         LET g_detail_d[l_ac].xmdd004 = l_xmdd004
         LET g_detail_d[l_ac].xmdd025 = l_xmdd025
      END IF

      #將這一次b_fill 的值 寫到主要的temp中
      INSERT INTO s_adbp520_temp1
      VALUES(g_detail_d[l_ac].sel,g_enterprise,g_detail_d[l_ac].xmdadocno,
             g_detail_d[l_ac].xmddseq,g_detail_d[l_ac].xmddseq1,g_detail_d[l_ac].xmddseq2,
             g_detail_d[l_ac].xmdd202,g_detail_d[l_ac].xmdd006, g_detail_d[l_ac].xmdd025, g_detail_d[l_ac].xmda004,g_detail_d[l_ac].xmda021,
             l_xmda022, l_xmda203, l_xmda200, l_xmda201, l_xmda002, 
             l_xmda003, l_xmda009, l_xmda010, l_xmda015, l_xmdd019, 
             l_xmda035, l_xmda024, g_detail_d[l_ac].xmda005,l_xmda023,l_xmda048,l_xmda049,
             l_xmdd222, l_xmdd220, l_xmddunit )
      
      
      #end add-point
      
      CALL adbp520_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
 
         END IF
         EXIT FOREACH
      END IF
      
   END FOREACH
   LET g_error_show = 0
   
   #add-point:b_fill段資料填充(其他單身)
   CALL g_detail_d.deleteElement(l_ac)
   #end add-point
    
   LET g_detail_cnt = l_ac - 1 
   DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE adbp520_sel
   
   LET l_ac = 1
   CALL adbp520_fetch()
   #add-point:b_fill段資料填充(其他單身)
   
   #end add-point
 
END FUNCTION
 
