         FOR l_n=g_gcal_d[l_ac].gcal003 TO g_gcal_d[l_ac].gcal002-1
            IF p_gcal009[l_n+1,l_n+1] NOT MATCHES '[0123456789]' THEN
               LET g_errno = "amm-00142"
