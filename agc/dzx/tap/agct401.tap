      AND gcaoent = g_enterprise
   IF l_cnt>0 THEN
      LET g_errno = "agc-00002"
   END IF
   IF cl_null(g_errno) THEN
      IF p_gcal009[1,g_gcal_d[l_ac].gcal003]<>g_gcal_d[l_ac].gcal004 THEN
         LET g_errno = "amm-00141"
