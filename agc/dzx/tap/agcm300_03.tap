   LET ls_group = "gcas_t,"
   IF ls_group.getIndexOf(ps_table,1) > 0 AND ps_table <> 'gcas_t' THEN
   
      #add-point:delete_b段刪除前
      {<point name="delete_b.b_delete" mark="Y"/>}
