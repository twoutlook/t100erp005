      SELECT COUNT(*) INTO l_cnt
        FROM gcaq_t
       WHERE gcaqent = g_enterprise AND gcaqdocno = g_gcaa_m.gcaadocno
      IF l_cnt > 0 THEN
         CASE p_cmd
