      CALL agct300_gcac_t_mask_restore('restore_mask_o')
               
      UPDATE gcac_t 
         SET (gcacdocno,
              gcac002,gcac003
              ,gcacsite,gcacunit,gcac000,gcac001,gcacacti) 
