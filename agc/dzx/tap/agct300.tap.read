             (ps_keys[1],ps_keys[2],ps_keys[3]
              ,g_gcab2_d[g_detail_idx].gcacsite,g_gcab2_d[g_detail_idx].gcacunit,g_gcab2_d[g_detail_idx].gcac000, 
                  g_gcab2_d[g_detail_idx].gcac001,g_gcab2_d[g_detail_idx].gcacacti) 
         WHERE gcacent = g_enterprise AND gcacdocno = ps_keys_bak[1] AND gcac002 = ps_keys_bak[2] AND gcac003 = ps_keys_bak[3]
      #add-point:update_b段修改中
      {<point name="update_b.m_update2"/>}
      #end add-point  
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "gcac_t" 
            LET g_errparam.code   = "std-00009" 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            CALL s_transaction_end('N','0')
         WHEN SQLCA.sqlcode #其他錯誤
