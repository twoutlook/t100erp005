       g_gcaa_m.gcaaownid,g_gcaa_m.gcaaowndp,g_gcaa_m.gcaacrtid,g_gcaa_m.gcaacrtdp,g_gcaa_m.gcaacrtdt, 
       g_gcaa_m.gcaamodid,g_gcaa_m.gcaamoddt,g_gcaa_m.gcaacnfid,g_gcaa_m.gcaacnfdt,g_gcaa_m.gcaa027, 
       g_gcaa_m.gcaa028,g_gcaa_m.gcaa029,g_gcaa_m.gcaa030,g_gcaa_m.gcaasite_desc,g_gcaa_m.gcaa012_desc, 
       g_gcaa_m.gcaa013_desc,g_gcaa_m.gcaa031_desc,g_gcaa_m.gcaaownid_desc,g_gcaa_m.gcaaowndp_desc,g_gcaa_m.gcaacrtid_desc, 
       g_gcaa_m.gcaacrtdp_desc,g_gcaa_m.gcaamodid_desc,g_gcaa_m.gcaacnfid_desc
   
   #檢查是否允許此動作
   IF NOT agct300_action_chk() THEN
