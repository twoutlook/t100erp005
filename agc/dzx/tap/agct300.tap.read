  <section id="agct300.b_fill" ver="17" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION agct300_b_fill()
   DEFINE p_wc2      STRING
   DEFINE li_idx     LIKE type_t.num10
   #add-point:b_fill段define
   {<point name="b_fill.define" edit="s"/>}
   #end add-point     
   #add-point:b_fill段define(客製用)
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point     
 
   CALL g_gcab_d.clear()    #g_gcab_d 單頭及單身 
   CALL g_gcab2_d.clear()
   CALL g_gcab3_d.clear()
   CALL g_gcab4_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF agct300_fill_chk(1) THEN
      #切換上下筆時不重組SQL
      IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
         LET g_sql = "SELECT  UNIQUE gcabsite,gcabunit,gcab000,gcab001,gcab002,gcab003,gcab004,gcab005, 
             gcab006,gcabacti  FROM gcab_t",   
                     " INNER JOIN gcaa_t ON gcaadocno = gcabdocno ",
 
                     #"",
                     
                     "",
                     
                     " WHERE gcabent=? AND gcabdocno=?"
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before
         {<point name="b_fill.body.fill_sql"/>}
         #end add-point
         IF NOT cl_null(g_wc2_table1) THEN
            LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY gcab_t.gcab002"
         
         #add-point:單身填充控制
         {<point name="b_fill.sql"/>}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE agct300_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR agct300_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_gcaa_m.gcaadocno
                                               
      FOREACH b_fill_cs INTO g_gcab_d[l_ac].gcabsite,g_gcab_d[l_ac].gcabunit,g_gcab_d[l_ac].gcab000, 
          g_gcab_d[l_ac].gcab001,g_gcab_d[l_ac].gcab002,g_gcab_d[l_ac].gcab003,g_gcab_d[l_ac].gcab004, 
          g_gcab_d[l_ac].gcab005,g_gcab_d[l_ac].gcab006,g_gcab_d[l_ac].gcabacti
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF
            EXIT FOREACH
         END IF
         
         LET l_ac = l_ac + 1
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
   #判斷是否填充
   IF agct300_fill_chk(2) THEN
      IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
         LET g_sql = "SELECT  UNIQUE gcacsite,gcacunit,gcac000,gcac001,gcac002,gcac003,gcacacti  FROM gcac_t", 
                
                     " INNER JOIN gcaa_t ON gcaadocno = gcacdocno ",
 
                     "",
                     
                     
                     " WHERE gcacent=? AND gcacdocno=?"   
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before
         {<point name="b_fill.body2.fill_sql"/>}
         #end add-point
         IF NOT cl_null(g_wc2_table2) THEN
            LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY gcac_t.gcac002,gcac_t.gcac003"
         
         #add-point:單身填充控制
         {<point name="b_fill.sql2"/>}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE agct300_pb2 FROM g_sql
         DECLARE b_fill_cs2 CURSOR FOR agct300_pb2
      END IF
    
      LET l_ac = 1
      
      OPEN b_fill_cs2 USING g_enterprise,g_gcaa_m.gcaadocno
                                               
      FOREACH b_fill_cs2 INTO g_gcab2_d[l_ac].gcacsite,g_gcab2_d[l_ac].gcacunit,g_gcab2_d[l_ac].gcac000, 
          g_gcab2_d[l_ac].gcac001,g_gcab2_d[l_ac].gcac002,g_gcab2_d[l_ac].gcac003,g_gcab2_d[l_ac].gcacacti 

         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill2.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
         
      END FOREACH
   END IF
 
   #判斷是否填充
   IF agct300_fill_chk(3) THEN
      IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
         LET g_sql = "SELECT  UNIQUE gcadsite,gcadunit,gcad000,gcad001,gcad002,gcad003,gcad004,gcad005, 
             gcad006,gcad007,gcad008,gcadacti  FROM gcad_t",   
                     " INNER JOIN gcaa_t ON gcaadocno = gcaddocno ",
 
                     "",
                     
                     
                     " WHERE gcadent=? AND gcaddocno=?"   
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before
         {<point name="b_fill.body3.fill_sql"/>}
         #end add-point
         IF NOT cl_null(g_wc2_table3) THEN
            LET g_sql = g_sql CLIPPED," AND ",g_wc2_table3 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY gcad_t.gcad002"
         
         #add-point:單身填充控制
         {<point name="b_fill.sql3"/>}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE agct300_pb3 FROM g_sql
         DECLARE b_fill_cs3 CURSOR FOR agct300_pb3
      END IF
    
      LET l_ac = 1
      
      OPEN b_fill_cs3 USING g_enterprise,g_gcaa_m.gcaadocno
                                               
      FOREACH b_fill_cs3 INTO g_gcab3_d[l_ac].gcadsite,g_gcab3_d[l_ac].gcadunit,g_gcab3_d[l_ac].gcad000, 
          g_gcab3_d[l_ac].gcad001,g_gcab3_d[l_ac].gcad002,g_gcab3_d[l_ac].gcad003,g_gcab3_d[l_ac].gcad004, 
          g_gcab3_d[l_ac].gcad005,g_gcab3_d[l_ac].gcad006,g_gcab3_d[l_ac].gcad007,g_gcab3_d[l_ac].gcad008, 
          g_gcab3_d[l_ac].gcadacti
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill3.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
         
      END FOREACH
   END IF
 
   #判斷是否填充
   IF agct300_fill_chk(4) THEN
      IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
         LET g_sql = "SELECT  UNIQUE gcaesite,gcaeunit,gcae000,gcae001,gcae002,gcae003,gcaeacti ,t1.ooefl003 FROM gcae_t", 
                
                     " INNER JOIN gcaa_t ON gcaadocno = gcaedocno ",
 
                     "",
                     
                                    " LEFT JOIN ooefl_t t1 ON t1.ooeflent='"||g_enterprise||"' AND t1.ooefl001=gcae002 AND t1.ooefl002='"||g_dlang||"' ",
 
                     " WHERE gcaeent=? AND gcaedocno=?"   
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         #add-point:b_fill段sql_before
         {<point name="b_fill.body4.fill_sql"/>}
         #end add-point
         IF NOT cl_null(g_wc2_table4) THEN
            LET g_sql = g_sql CLIPPED," AND ",g_wc2_table4 CLIPPED
         END IF
         
         #子單身的WC
         
         
         LET g_sql = g_sql, " ORDER BY gcae_t.gcae002"
         
         #add-point:單身填充控制
         {<point name="b_fill.sql4"/>}
         #end add-point
         
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE agct300_pb4 FROM g_sql
         DECLARE b_fill_cs4 CURSOR FOR agct300_pb4
      END IF
    
      LET l_ac = 1
      
      OPEN b_fill_cs4 USING g_enterprise,g_gcaa_m.gcaadocno
                                               
      FOREACH b_fill_cs4 INTO g_gcab4_d[l_ac].gcaesite,g_gcab4_d[l_ac].gcaeunit,g_gcab4_d[l_ac].gcae000, 
          g_gcab4_d[l_ac].gcae001,g_gcab4_d[l_ac].gcae002,g_gcab4_d[l_ac].gcae003,g_gcab4_d[l_ac].gcaeacti, 
          g_gcab4_d[l_ac].gcae002_desc
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill4.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
         
      END FOREACH
   END IF
 
 
   
   #add-point:browser_fill段其他table處理
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_gcab_d.deleteElement(g_gcab_d.getLength())
   CALL g_gcab2_d.deleteElement(g_gcab2_d.getLength())
   CALL g_gcab3_d.deleteElement(g_gcab3_d.getLength())
   CALL g_gcab4_d.deleteElement(g_gcab4_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE agct300_pb
   FREE agct300_pb2
 
   FREE agct300_pb3
 
   FREE agct300_pb4
 
 
   
   LET li_idx = l_ac
   
   #遮罩相關處理
   FOR l_ac = 1 TO g_gcab_d.getLength()
      LET g_gcab_d_mask_o[l_ac].* =  g_gcab_d[l_ac].*
      CALL agct300_gcab_t_mask()
      LET g_gcab_d_mask_n[l_ac].* =  g_gcab_d[l_ac].*
   END FOR
   
   LET g_gcab2_d_mask_o.* =  g_gcab2_d.*
   FOR l_ac = 1 TO g_gcab2_d.getLength()
      LET g_gcab2_d_mask_o[l_ac].* =  g_gcab2_d[l_ac].*
      CALL agct300_gcac_t_mask()
      LET g_gcab2_d_mask_n[l_ac].* =  g_gcab2_d[l_ac].*
   END FOR
   LET g_gcab3_d_mask_o.* =  g_gcab3_d.*
   FOR l_ac = 1 TO g_gcab3_d.getLength()
      LET g_gcab3_d_mask_o[l_ac].* =  g_gcab3_d[l_ac].*
      CALL agct300_gcad_t_mask()
      LET g_gcab3_d_mask_n[l_ac].* =  g_gcab3_d[l_ac].*
   END FOR
   LET g_gcab4_d_mask_o.* =  g_gcab4_d.*
   FOR l_ac = 1 TO g_gcab4_d.getLength()
      LET g_gcab4_d_mask_o[l_ac].* =  g_gcab4_d[l_ac].*
      CALL agct300_gcae_t_mask()
      LET g_gcab4_d_mask_n[l_ac].* =  g_gcab4_d[l_ac].*
   END FOR
 
   
   LET l_ac = li_idx
   
   CALL cl_ap_performance_next_end()
 
END FUNCTION
]]>
  </section>
