   IF g_gcaa_m.gcaadocno IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
    
   LET g_gcaadocno_t = g_gcaa_m.gcaadocno
 
    
   LET g_gcaa_m.gcaadocno = ""
 
 
   CALL cl_set_head_visible("","YES")
 
   #公用欄位給予預設值
   #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_gcaa_m.gcaaownid = g_user
      LET g_gcaa_m.gcaaowndp = g_dept
      LET g_gcaa_m.gcaacrtid = g_user
      LET g_gcaa_m.gcaacrtdp = g_dept 
      LET g_gcaa_m.gcaacrtdt = cl_get_current()
      LET g_gcaa_m.gcaamodid = g_user
      LET g_gcaa_m.gcaamoddt = cl_get_current()
      LET g_gcaa_m.gcaastus = 'N'
 
 
   
   CALL s_transaction_begin()
   
   #add-point:複製輸入前
   {<point name="reproduce.head.b_input"/>}
   #end add-point
   
   #顯示狀態(stus)圖片
         #應用 a21 樣板自動產生(Version:2)
	  #根據當下狀態碼顯示圖片
      CASE g_gcaa_m.gcaastus 
