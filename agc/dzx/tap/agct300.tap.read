PRIVATE FUNCTION agct300_gcaa017_chk()
DEFINE l_success    LIKE type_t.num5

   LET l_success = TRUE
   INITIALIZE g_errno TO NULL

   #规则 --当[效期控管起算基准gcaa017]为0.无时，[有效期至gcaa018]必须为1.指定日期
   #故若[有效期至gcaa018]为1.指定日期且[效期指定日期]不为空，则[效期控管起算基准gcaa017]更改时要询问是否清空原先设定
   IF g_gcaa_m.gcaa017 <> '0' THEN
      IF NOT cl_null(g_gcaa_m.gcaa019) THEN
         IF NOT cl_ask_confirm('agc-00015') THEN #已设定效期指定日期，是否清空？(Y/N)
            LET l_success = FALSE
            RETURN l_success
         END IF
      END IF
      LET g_gcaa_m.gcaa018 = '2'
      LET g_gcaa_m.gcaa019 = ''
      DISPLAY BY NAME g_gcaa_m.gcaa018,g_gcaa_m.gcaa019
   END IF
   
   #规则 --当[效期控管起算基准gcaa017]为0.无时，[有效期至gcaa018]必须为1.指定日期
   #故若[有效期至gcaa018]为2.指定月份長度或3.指定天數長度，且[效期指定长度]不为空，则[效期控管起算基准gcaa017]更改时要询问是否清空原先设定
   IF g_gcaa_m.gcaa017 = '0' THEN
      IF NOT cl_null(g_gcaa_m.gcaa020) THEN
         IF NOT cl_ask_confirm('agc-00016') THEN #已设定效期指定长度，是否清空？(Y/N)
            LET l_success = FALSE
            RETURN l_success
         END IF
      END IF
      LET g_gcaa_m.gcaa018 = '1'
      LET g_gcaa_m.gcaa020 = ''
      DISPLAY BY NAME g_gcaa_m.gcaa018,g_gcaa_m.gcaa020
   END IF
   
   RETURN l_success
END FUNCTION]]>
  </point>
