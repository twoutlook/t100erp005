       g_gcaa_m.gcaa035,g_gcaa_m.gcaa036,g_gcaa_m.gcaa024,g_gcaa_m.gcaa025,g_gcaa_m.gcaa031,g_gcaa_m.gcaa031_desc, 
       g_gcaa_m.gcaa026,g_gcaa_m.gcaa032,g_gcaa_m.gcaa033,g_gcaa_m.gcaaownid,g_gcaa_m.gcaaownid_desc, 
       g_gcaa_m.gcaaowndp,g_gcaa_m.gcaaowndp_desc,g_gcaa_m.gcaacrtid,g_gcaa_m.gcaacrtid_desc,g_gcaa_m.gcaacrtdp, 
