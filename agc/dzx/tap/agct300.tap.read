  <section id="agct300.insert" ver="32" status="" src="s" readonly="">
    <![CDATA[#+ 資料新增
PRIVATE FUNCTION agct300_insert()
   #add-point:insert段define
   {<point name="insert.define" edit="s"/>}
   #end add-point    
   #add-point:insert段define(客製用)
   {<point name="insert.define_customerization" edit="c"/>}
   #end add-point    
   
   #清畫面欄位內容
   CLEAR FORM                    
   CALL g_gcab_d.clear()   
   CALL g_gcab2_d.clear()  
   CALL g_gcab3_d.clear()  
   CALL g_gcab4_d.clear()  
 
 
   INITIALIZE g_gcaa_m.* LIKE gcaa_t.*             #DEFAULT 設定
   
   LET g_gcaadocno_t = NULL
 
   
   LET g_master_insert = FALSE
   
   #add-point:insert段before
   {<point name="insert.before"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   WHILE TRUE
      #公用欄位給值(單頭)
      #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_gcaa_m.gcaaownid = g_user
      LET g_gcaa_m.gcaaowndp = g_dept
      LET g_gcaa_m.gcaacrtid = g_user
      LET g_gcaa_m.gcaacrtdp = g_dept 
