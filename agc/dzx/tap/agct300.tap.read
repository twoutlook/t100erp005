      RETURN
   END IF
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL agct300_set_act_visible()   
   CALL agct300_set_act_no_visible()
   
   #將新增的資料併入搜尋條件中
   LET g_gcaadocno_t = g_gcaa_m.gcaadocno
 
   
   #組合新增資料的條件
   LET g_add_browse = " gcaaent = '" ||g_enterprise|| "' AND",
                      " gcaadocno = '", g_gcaa_m.gcaadocno, "' "
 
   #填到最後面
   LET g_current_idx = g_browser.getLength() + 1
   CALL agct300_browser_fill("")
   
   DISPLAY g_browser_cnt TO FORMONLY.h_count    #總筆數
   DISPLAY g_current_idx TO FORMONLY.h_index    #當下筆數
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   
   #add-point:完成複製段落後
   {<point name="reproduce.after_reproduce" />}
   #end add-point
   
   CALL agct300_idx_chk()
   
   #功能已完成,通報訊息中心
   CALL agct300_msgcentre_notify('reproduce')
 
END FUNCTION
