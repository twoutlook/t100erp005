      SELECT COUNT(*) INTO l_cnt2
        FROM gcac_t
       WHERE gcacent = g_enterprise AND gcacdocno = g_gcaa_m.gcaadocno
      IF l_cnt > 0 OR l_cnt2 > 0 THEN
