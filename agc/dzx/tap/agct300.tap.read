PRIVATE FUNCTION agct300_gcaa010_chk(p_gcaa010)
DEFINE p_gcaa010   LIKE gcaa_t.gcaa010
DEFINE l_str       STRING
DEFINE l_num       STRING
   
   INITIALIZE g_errno TO NULL
   
   IF NOT cl_null(g_gcaa_m.gcaa006) AND NOT cl_null(g_gcaa_m.gcaa007) AND 
      NOT cl_null(g_gcaa_m.gcaa008) AND NOT cl_null(g_gcaa_m.gcaa009) THEN
      LET l_str = p_gcaa010
      IF g_gcaa_m.gcaa006 != l_str.getLength() THEN
         LET g_errno = 'agc-00009' #该券号不符合当前券种编码规则
         RETURN
      END IF
      IF g_gcaa_m.gcaa008 != l_str.subString(1,g_gcaa_m.gcaa007) THEN
         LET g_errno = 'agc-00009' #该券号不符合当前券种编码规则
         RETURN
      END IF
      IF g_gcaa_m.gcaa009 > 0 THEN
         IF NOT s_chr_alphanumeric(l_str.subString(g_gcaa_m.gcaa007+1,l_str.getLength()),1) THEN
            LET g_errno = 'agc-00009' #该券号不符合当前券种编码规则
            RETURN
         END IF
      END IF
   END IF
   IF NOT cl_null(g_gcaa_m.gcaa010) AND NOT cl_null(g_gcaa_m.gcaa011) THEN
      IF g_gcaa_m.gcaa010 > g_gcaa_m.gcaa011 THEN
         LET g_errno = 'agc-00010' #开始券号不可大于结束券号
         RETURN
      END IF
   END IF
END FUNCTION]]>
