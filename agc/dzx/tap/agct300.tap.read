             (ps_keys[1],ps_keys[2]
              ,g_gcab3_d[g_detail_idx].gcadsite,g_gcab3_d[g_detail_idx].gcadunit,g_gcab3_d[g_detail_idx].gcad000, 
                  g_gcab3_d[g_detail_idx].gcad001,g_gcab3_d[g_detail_idx].gcad003,g_gcab3_d[g_detail_idx].gcad004, 
                  g_gcab3_d[g_detail_idx].gcad005,g_gcab3_d[g_detail_idx].gcad006,g_gcab3_d[g_detail_idx].gcad007, 
                  g_gcab3_d[g_detail_idx].gcad008,g_gcab3_d[g_detail_idx].gcadacti) 
         WHERE gcadent = g_enterprise AND gcaddocno = ps_keys_bak[1] AND gcad002 = ps_keys_bak[2]
      #add-point:update_b段修改中
      {<point name="update_b.m_update3"/>}
