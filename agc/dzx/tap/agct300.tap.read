  <section id="agct300.fetch" ver="22" status="" src="s" readonly="">
    <![CDATA[#+ 指定PK後抓取單頭其他資料
PRIVATE FUNCTION agct300_fetch(p_flag)
   DEFINE p_flag     LIKE type_t.chr1
   DEFINE ls_msg     STRING
   #add-point:fetch段define
   {<point name="fetch.define" edit="s"/>}
   #end add-point    
   #add-point:fetch段define(客製用)
   {<point name="fetch.define_customerization" edit="c"/>}
   #end add-point    
   
   IF g_browser_cnt = 0 THEN
      RETURN
   END IF
 
   CALL cl_ap_performance_next_start()
   CASE p_flag
      WHEN 'F' 
         LET g_current_idx = 1
      WHEN 'L'  
         LET g_current_idx = g_browser.getLength()              
      WHEN 'P'
         IF g_current_idx > 1 THEN               
            LET g_current_idx = g_current_idx - 1
         END IF 
      WHEN 'N'
         IF g_current_idx < g_header_cnt THEN
            LET g_current_idx =  g_current_idx + 1
         END IF        
      WHEN '/'
         IF (NOT g_no_ask) THEN    
            CALL cl_set_act_visible("accept,cancel", TRUE)    
            CALL cl_getmsg('fetch',g_lang) RETURNING ls_msg
            LET INT_FLAG = 0
 
            PROMPT ls_msg CLIPPED,':' FOR g_jump
               #交談指令共用ACTION
               &include "common_action.4gl" 
            END PROMPT
 
            CALL cl_set_act_visible("accept,cancel", FALSE)    
            IF INT_FLAG THEN
                LET INT_FLAG = 0
                EXIT CASE  
            END IF           
         END IF
         
         IF g_jump > 0 AND g_jump <= g_browser.getLength() THEN
             LET g_current_idx = g_jump
         END IF
         LET g_no_ask = FALSE  
   END CASE 
 
   CALL g_curr_diag.setCurrentRow("s_browse", g_current_idx) #設定browse 索引
   
   LET g_current_row = g_current_idx
   LET g_detail_cnt = g_header_cnt                  
   
   #單身總筆數顯示
   IF g_detail_cnt > 0 THEN
      #若單身有資料時, idx至少為1
      IF g_detail_idx <= 0 THEN
         LET g_detail_idx = 1
      END IF
      DISPLAY g_detail_idx TO FORMONLY.idx  
   ELSE
      LET g_detail_idx = 0
      DISPLAY '' TO FORMONLY.idx    
   END IF
   
   #瀏覽頁筆數顯示
   LET g_browser_idx = g_pagestart+g_current_idx-1
   DISPLAY g_browser_idx TO FORMONLY.b_index   #當下筆數
   DISPLAY g_browser_idx TO FORMONLY.h_index   #當下筆數
   
   CALL cl_navigator_setting( g_current_idx, g_browser_cnt )
 
   #代表沒有資料
   IF g_current_idx = 0 OR g_browser.getLength() = 0 THEN
      RETURN
   END IF
   
   #超出範圍
   IF g_current_idx > g_browser.getLength() THEN
      LET g_current_idx = g_browser.getLength()
   END IF
   
   LET g_gcaa_m.gcaadocno = g_browser[g_current_idx].b_gcaadocno
 
   
   #重讀DB,因TEMP有不被更新特性
   EXECUTE agct300_master_referesh USING g_gcaa_m.gcaadocno INTO g_gcaa_m.gcaasite,g_gcaa_m.gcaadocdt, 
       g_gcaa_m.gcaadocno,g_gcaa_m.gcaa000,g_gcaa_m.gcaaunit,g_gcaa_m.gcaa001,g_gcaa_m.gcaa002,g_gcaa_m.gcaa003, 
       g_gcaa_m.gcaa004,g_gcaa_m.gcaaacti,g_gcaa_m.gcaastus,g_gcaa_m.gcaa005,g_gcaa_m.gcaa006,g_gcaa_m.gcaa007, 
       g_gcaa_m.gcaa008,g_gcaa_m.gcaa009,g_gcaa_m.gcaa010,g_gcaa_m.gcaa011,g_gcaa_m.gcaa012,g_gcaa_m.gcaa013, 
       g_gcaa_m.gcaa015,g_gcaa_m.gcaa016,g_gcaa_m.gcaa017,g_gcaa_m.gcaa018,g_gcaa_m.gcaa019,g_gcaa_m.gcaa020, 
       g_gcaa_m.gcaa021,g_gcaa_m.gcaa022,g_gcaa_m.gcaa023,g_gcaa_m.gcaa034,g_gcaa_m.gcaa035,g_gcaa_m.gcaa036, 
       g_gcaa_m.gcaa024,g_gcaa_m.gcaa025,g_gcaa_m.gcaa031,g_gcaa_m.gcaa026,g_gcaa_m.gcaa032,g_gcaa_m.gcaa033, 
       g_gcaa_m.gcaaownid,g_gcaa_m.gcaaowndp,g_gcaa_m.gcaacrtid,g_gcaa_m.gcaacrtdp,g_gcaa_m.gcaacrtdt, 
       g_gcaa_m.gcaamodid,g_gcaa_m.gcaamoddt,g_gcaa_m.gcaacnfid,g_gcaa_m.gcaacnfdt,g_gcaa_m.gcaa027, 
       g_gcaa_m.gcaa028,g_gcaa_m.gcaa029,g_gcaa_m.gcaa030,g_gcaa_m.gcaasite_desc,g_gcaa_m.gcaa012_desc, 
       g_gcaa_m.gcaa013_desc,g_gcaa_m.gcaa031_desc,g_gcaa_m.gcaaownid_desc,g_gcaa_m.gcaaowndp_desc,g_gcaa_m.gcaacrtid_desc, 
       g_gcaa_m.gcaacrtdp_desc,g_gcaa_m.gcaamodid_desc,g_gcaa_m.gcaacnfid_desc
   
   #遮罩相關處理
   LET g_gcaa_m_mask_o.* =  g_gcaa_m.*
   CALL agct300_gcaa_t_mask()
   LET g_gcaa_m_mask_n.* =  g_gcaa_m.*
   
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL agct300_set_act_visible()   
   CALL agct300_set_act_no_visible()
   
   #add-point:fetch段action控制
   {<point name="fetch.action_control"/>}
   #end add-point  
   
   
   
   #add-point:fetch結束前
   {<point name="fetch.after" />}
