    <![CDATA[#+
PRIVATE FUNCTION agct300_gcaa001_chk()
DEFINE l_cnt      LIKE type_t.num5
DEFINE l_gcafunit LIKE gcaf_t.gcafunit

   #初始化
   INITIALIZE g_errno TO NULL
   LET l_cnt = 0
   
   #检查：该券种是否存在未确认单据
   SELECT COUNT(*) INTO l_cnt
     FROM gcaa_t
    WHERE gcaaent = g_enterprise AND gcaa001 = g_gcaa_m.gcaa001 AND gcaastus = 'N'
   IF l_cnt > 0 THEN
      LET g_errno = 'agc-00044' #当前券种仍存在未审核的单据,不允许输入 -- 请至[券种基本资料申请作业agct300]查询后重新输入
      RETURN
   END IF
   
   #检查券种编号
   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt
     FROM gcaf_t
    WHERE gcafent = g_enterprise AND gcaf001 = g_gcaa_m.gcaa001
   CASE g_gcaa_m.gcaa000
      WHEN 'I'
         IF l_cnt > 0 THEN 
            LET g_errno = 'agc-00045' #申请类别为I.新增时,券种编号不可存在于[券种基本资料档]中 -- 请至[券种基本资料维护作业agcm300]中查询后重新输入!
         END IF
      WHEN 'U'
         IF l_cnt = 0 THEN 
            LET g_errno = 'agc-00046' #申请类别为U.修改时,券种编号必须存在于[券种基本资料档]中 -- 请至[券种基本资料维护作业agcm300]中查询后重新输入!
            RETURN
         END IF
         SELECT gcafunit INTO l_gcafunit
           FROM gcaf_t
          WHERE gcafent = g_enterprise AND gcaf001 = g_gcaa_m.gcaa001
         IF cl_null(l_gcafunit) OR l_gcafunit <> g_site THEN
            LET g_errno = '' #该券种制定营运组织非当前组织,不可异动
         END IF
   END CASE
   
END FUNCTION]]>
