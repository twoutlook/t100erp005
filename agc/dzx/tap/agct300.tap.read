   LET g_data_owner = g_gcaa_m.gcaaownid      
   LET g_data_dept  = g_gcaa_m.gcaaowndp
   
   #重新顯示   
   CALL agct300_show()
 
   #應用 a56 樣板自動產生(Version:2)
