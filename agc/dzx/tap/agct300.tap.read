               RETURN r_success
            WHEN 'd'
               DELETE FROM gcab_t WHERE gcabent = g_enterprise AND gcabdocno = g_gcaa_m.gcaadocno
               IF SQLCA.sqlcode THEN
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code = SQLCA.sqlcode
                  LET g_errparam.extend = 'del gcab_t'
