       g_gcaa_m.gcaa015,g_gcaa_m.gcaa016,g_gcaa_m.gcaa017,g_gcaa_m.gcaa018,g_gcaa_m.gcaa019,g_gcaa_m.gcaa020, 
       g_gcaa_m.gcaa021,g_gcaa_m.gcaa022,g_gcaa_m.gcaa023,g_gcaa_m.gcaa034,g_gcaa_m.gcaa035,g_gcaa_m.gcaa036, 
       g_gcaa_m.gcaa024,g_gcaa_m.gcaa025,g_gcaa_m.gcaa031,g_gcaa_m.gcaa026,g_gcaa_m.gcaa032,g_gcaa_m.gcaa033, 
       g_gcaa_m.gcaaownid,g_gcaa_m.gcaaowndp,g_gcaa_m.gcaacrtid,g_gcaa_m.gcaacrtdp,g_gcaa_m.gcaacrtdt, 
       g_gcaa_m.gcaamodid,g_gcaa_m.gcaamoddt,g_gcaa_m.gcaacnfid,g_gcaa_m.gcaacnfdt,g_gcaa_m.gcaa027, 
       g_gcaa_m.gcaa028,g_gcaa_m.gcaa029,g_gcaa_m.gcaa030,g_gcaa_m.gcaasite_desc,g_gcaa_m.gcaa012_desc, 
       g_gcaa_m.gcaa013_desc,g_gcaa_m.gcaa031_desc,g_gcaa_m.gcaaownid_desc,g_gcaa_m.gcaaowndp_desc,g_gcaa_m.gcaacrtid_desc, 
       g_gcaa_m.gcaacrtdp_desc,g_gcaa_m.gcaamodid_desc,g_gcaa_m.gcaacnfid_desc
   
   #遮罩相關處理
   LET g_gcaa_m_mask_o.* =  g_gcaa_m.*
   CALL agct300_gcaa_t_mask()
   LET g_gcaa_m_mask_n.* =  g_gcaa_m.*
   
   #將資料顯示到畫面上
   DISPLAY BY NAME g_gcaa_m.gcaasite,g_gcaa_m.gcaasite_desc,g_gcaa_m.gcaadocdt,g_gcaa_m.gcaadocno,g_gcaa_m.gcaa000, 
       g_gcaa_m.gcaaunit,g_gcaa_m.gcaa001,g_gcaa_m.gcaa002,g_gcaa_m.gcaal002,g_gcaa_m.gcaal003,g_gcaa_m.gcaa003, 
       g_gcaa_m.gcaa004,g_gcaa_m.gcaaacti,g_gcaa_m.gcaastus,g_gcaa_m.gcaa005,g_gcaa_m.gcaa006,g_gcaa_m.gcaa007, 
       g_gcaa_m.gcaa008,g_gcaa_m.gcaa009,g_gcaa_m.gcaa010,g_gcaa_m.gcaa011,g_gcaa_m.gcaa012,g_gcaa_m.gcaa012_desc, 
       g_gcaa_m.gcaa013,g_gcaa_m.gcaa013_desc,g_gcaa_m.gcaa015,g_gcaa_m.gcaa016,g_gcaa_m.gcaa017,g_gcaa_m.gcaa018, 
