            CALL cl_show_fld_cont()
            CALL agcq300_set_entry_b("a")
            CALL agcq300_set_no_entry_b("a")
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_gcao_d[li_reproduce_target].* = g_gcao_d[li_reproduce].*
