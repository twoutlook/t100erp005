                  END IF
               END IF 
               CLOSE agcq300_bcl
               #add-point:單身關閉bcl
               {<point name="input.body.close"/>}
               #end add-point
               LET l_count = g_gcao_d.getLength()
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_gcao_d_t.gcao001
 
