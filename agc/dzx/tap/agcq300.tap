  <section id="agcq300.b_fill" ver="17" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION agcq300_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2    STRING
   #add-point:b_fill段define
   {<point name="b_fill.define" edit="s"/>}
   #end add-point
   #add-point:b_fill段define(客製用)
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE t0.gcao001,t0.gcao002,t0.gcao003,t0.gcao004,t0.gcao005,t0.gcao006,t0.gcao008, 
       t0.gcao009,t0.gcao034,t0.gcao001,t0.gcao011,t0.gcao012,t0.gcao013,t0.gcao014,t0.gcao015,t0.gcao016, 
       t0.gcao017,t0.gcao018,t0.gcao019,t0.gcao020,t0.gcao021,t0.gcao022,t0.gcao023,t0.gcao024,t0.gcao025, 
       t0.gcao026,t0.gcao027,t0.gcao028,t0.gcao029,t0.gcao030,t0.gcao031 ,t1.gcafl003 ,t2.oocql004 , 
       t3.mmaf008 ,t4.ooefl003 ,t5.ooefl003 ,t6.ooefl003 ,t7.ooefl003 ,t8.ooefl003 ,t9.ooefl003 ,t10.ooefl003 , 
       t11.ooefl003 ,t12.ooefl003 ,t13.ooefl003 ,t14.ooefl003 ,t15.ooefl003 ,t16.ooefl003 ,t17.ooefl003 FROM gcao_t t0", 

               "",
                              " LEFT JOIN gcafl_t t1 ON t1.gcaflent='"||g_enterprise||"' AND t1.gcafl001=t0.gcao002 AND t1.gcafl002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t2 ON t2.oocqlent='"||g_enterprise||"' AND t2.oocql001='2071' AND t2.oocql002=t0.gcao003 AND t2.oocql003='"||g_dlang||"' ",
               " LEFT JOIN mmaf_t t3 ON t3.mmafent='"||g_enterprise||"' AND t3.mmaf001=t0.gcao034  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=t0.gcao011 AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=t0.gcao013 AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent='"||g_enterprise||"' AND t6.ooefl001=t0.gcao014 AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t7 ON t7.ooeflent='"||g_enterprise||"' AND t7.ooefl001=t0.gcao016 AND t7.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t8 ON t8.ooeflent='"||g_enterprise||"' AND t8.ooefl001=t0.gcao017 AND t8.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t9 ON t9.ooeflent='"||g_enterprise||"' AND t9.ooefl001=t0.gcao019 AND t9.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t10 ON t10.ooeflent='"||g_enterprise||"' AND t10.ooefl001=t0.gcao020 AND t10.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t11 ON t11.ooeflent='"||g_enterprise||"' AND t11.ooefl001=t0.gcao022 AND t11.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t12 ON t12.ooeflent='"||g_enterprise||"' AND t12.ooefl001=t0.gcao023 AND t12.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t13 ON t13.ooeflent='"||g_enterprise||"' AND t13.ooefl001=t0.gcao025 AND t13.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t14 ON t14.ooeflent='"||g_enterprise||"' AND t14.ooefl001=t0.gcao026 AND t14.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t15 ON t15.ooeflent='"||g_enterprise||"' AND t15.ooefl001=t0.gcao028 AND t15.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t16 ON t16.ooeflent='"||g_enterprise||"' AND t16.ooefl001=t0.gcao029 AND t16.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooefl_t t17 ON t17.ooeflent='"||g_enterprise||"' AND t17.ooefl001=t0.gcao031 AND t17.ooefl002='"||g_dlang||"' ",
 
               " WHERE t0.gcaoent= ?  AND  1=1 AND (", p_wc2, ") " 
   #add-point:b_fill段sql wc
   {<point name="b_fill.sql_wc"/>}
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("gcao_t"),
                      " ORDER BY t0.gcao001"
   
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"gcao_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE agcq300_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR agcq300_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_gcao_d.clear()
   CALL g_gcao2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_gcao_d[l_ac].gcao001,g_gcao_d[l_ac].gcao002,g_gcao_d[l_ac].gcao003,g_gcao_d[l_ac].gcao004, 
       g_gcao_d[l_ac].gcao005,g_gcao_d[l_ac].gcao006,g_gcao_d[l_ac].gcao008,g_gcao_d[l_ac].gcao009,g_gcao_d[l_ac].gcao034, 
       g_gcao2_d[l_ac].gcao001,g_gcao2_d[l_ac].gcao011,g_gcao2_d[l_ac].gcao012,g_gcao2_d[l_ac].gcao013, 
