  <point name="function.agct300_01_mmam003_ref" cite_std="N" status="" ver="1" src="s" new="Y" order="1" mark_hard="N">
<![CDATA[#+
PRIVATE FUNCTION agct300_01_mmam003_ref()
   
   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = g_mmam_d[l_ac].mmam003
   CALL ap_ref_array2(g_ref_fields,"SELECT ooefl003 FROM ooefl_t WHERE ooeflent='"||g_enterprise||"' AND ooefl001=? AND ooefl002='"||g_dlang||"'","") RETURNING g_rtn_fields
   LET g_mmam_d[l_ac].mmam003_desc = '', g_rtn_fields[1] , ''
   DISPLAY BY NAME g_mmam_d[l_ac].mmam003_desc
END FUNCTION]]>
</point>
  <point name="function.agct300_01_mmam003_chk" cite_std="N" status="" ver="1" src="s" new="Y" order="2" mark_hard="N">
<![CDATA[#+
PRIVATE FUNCTION agct300_01_mmam003_chk()
DEFINE l_ooea004   LIKE ooea_t.ooea004
DEFINE l_ooeastus  LIKE ooea_t.ooeastus

   INITIALIZE g_errno TO NULL
   SELECT ooea004,ooeastus INTO l_ooea004,l_ooeastus
     FROM ooea_t
    WHERE ooeaent = g_enterprise AND ooea001 = g_mmam_d[l_ac].mmam003
   CASE
      WHEN SQLCA.sqlcode = 100 LET g_errno = 'amm-00083' #
      WHEN l_ooeastus <> 'Y'   LET g_errno = 'amm-00084' #
      WHEN l_ooea004 <> 'Y'    LET g_errno = 'amm-00085' #
   END CASE
END FUNCTION]]>
</point>
