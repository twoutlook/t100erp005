  <section id="agct300_01.b_fill" ver="4" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION agct300_01_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2    STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE mmamdocno,mmam000,mmam001,mmam002,mmam003,mmam005,mmam004,mmamacti ,t1.ooefl003 FROM mmam_t", 

               "",
                              " LEFT JOIN ooefl_t t1 ON t1.ooeflent='"||g_enterprise||"' AND t1.ooefl001=mmam003 AND t1.ooefl002='"||g_dlang||"' ",
 
               " WHERE mmament= ? AND 1=1 AND ", p_wc2 
    
   LET g_sql = g_sql, cl_sql_add_filter("mmam_t"),
                      " ORDER BY mmam_t.mmamdocno,mmam_t.mmam003"
   
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE agct300_01_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR agct300_01_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_mmam_d.clear()
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_mmam_d[l_ac].mmamdocno,g_mmam_d[l_ac].mmam000,g_mmam_d[l_ac].mmam001,g_mmam_d[l_ac].mmam002, 
       g_mmam_d[l_ac].mmam003,g_mmam_d[l_ac].mmam005,g_mmam_d[l_ac].mmam004,g_mmam_d[l_ac].mmamacti, 
       g_mmam_d[l_ac].mmam003_desc
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL agct300_01_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec THEN
      IF g_error_show = 1 THEN
         CALL cl_err("mmam_t",9035,1)
      END IF
   END IF 
   LET g_error_show = 0
   
 
   
   CALL g_mmam_d.deleteElement(g_mmam_d.getLength())   
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_mmam_d.getLength()
 
   END FOR
   
   IF g_cnt > g_mmam_d.getLength() THEN
      LET l_ac = g_mmam_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_mmam_d.getLength()
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE agct300_01_pb
   
END FUNCTION
]]>
</section>
  <section id="agct300_01.default_search" ver="4" status="" src="s">
<![CDATA[#+ 外部參數搜尋
PRIVATE FUNCTION agct300_01_default_search()
   DEFINE li_idx  LIKE type_t.num5
   DEFINE li_cnt  LIKE type_t.num5
   DEFINE ls_wc   STRING
   #add-point:default_search段define
   {<point name="default_search.define"/>}
   #end add-point  
   
   IF NOT cl_null(g_argv[01]) THEN
      LET ls_wc = ls_wc, " mmamdocno = '", g_argv[01], "' AND "
   END IF
   
   IF NOT cl_null(g_argv[02]) THEN
      LET ls_wc = ls_wc, " mmam003 = ", g_argv[02], " AND "
   END IF
 
   
   #add-point:default_search段after sql
   {<point name="default_search.after_sql"/>}
   #end add-point  
   
   IF NOT cl_null(ls_wc) THEN
      LET ls_wc = ls_wc.subString(1,ls_wc.getLength()-5)
      LET g_wc2 = ls_wc
   ELSE
      LET g_wc2 = " 1=1"
   END IF
 
END FUNCTION
]]>
