         #Ctrlp:input.c.page1.mmam005
         ON ACTION controlp INFIELD mmam005
            #add-point:ON ACTION controlp INFIELD mmam005
            {<point name="input.c.page1.mmam005" />}
            #END add-point
 
         #Ctrlp:input.c.page1.mmam004
         ON ACTION controlp INFIELD mmam004
            #add-point:ON ACTION controlp INFIELD mmam004
            {<point name="input.c.page1.mmam004" />}
            #END add-point
 
         #Ctrlp:input.c.page1.mmamacti
         ON ACTION controlp INFIELD mmamacti
            #add-point:ON ACTION controlp INFIELD mmamacti
            {<point name="input.c.page1.mmamacti" />}
            #END add-point
 
 
 
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_mmam_d[l_ac].* = g_mmam_d_t.*
               CLOSE agct300_01_bcl
               CALL s_transaction_end('N','0')
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(g_mmam_d[l_ac].mmamdocno,-263,1)
               LET g_mmam_d[l_ac].* = g_mmam_d_t.*
            ELSE
               
               #寫入修改者/修改日期資訊(單身)
               
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               UPDATE mmam_t SET (mmamdocno,mmam000,mmam001,mmam002,mmam003,mmam005,mmam004,mmamacti) = (g_mmam_d[l_ac].mmamdocno, 
                   g_mmam_d[l_ac].mmam000,g_mmam_d[l_ac].mmam001,g_mmam_d[l_ac].mmam002,g_mmam_d[l_ac].mmam003, 
                   g_mmam_d[l_ac].mmam005,g_mmam_d[l_ac].mmam004,g_mmam_d[l_ac].mmamacti)
                WHERE mmament = g_enterprise AND
                  mmamdocno = g_mmam_d_t.mmamdocno #項次   
                  AND mmam003 = g_mmam_d_t.mmam003  
 
                  
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
                  
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("mmam_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                    WHEN SQLCA.sqlcode #其他錯誤
                     CALL cl_err("mmam_t",SQLCA.sqlcode,1)  
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_mmam_d[g_detail_idx].mmamdocno
               LET gs_keys_bak[1] = g_mmam_d_t.mmamdocno
               LET gs_keys[2] = g_mmam_d[g_detail_idx].mmam003
               LET gs_keys_bak[2] = g_mmam_d_t.mmam003
               CALL agct300_01_update_b('mmam_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
                     LET g_log1 = util.JSON.stringify(g_mmam_d_t)
                     LET g_log2 = util.JSON.stringify(g_mmam_d[l_ac])
                     IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                        CALL s_transaction_end('N','0')
                     END IF
               END CASE
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            CALL agct300_01_unlock_b("mmam_t")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock
            
             #add-point:單身after row
            {<point name="input.body.a_row"/>}
            #end add-point
            
         AFTER INPUT
            #add-point:單身input後
            {<point name="input.body.a_input"/>}
            #end add-point
            
         ON ACTION controlo   
            CALL FGL_SET_ARR_CURR(g_mmam_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_mmam_d.getLength()+1
            
      END INPUT
      
 
      
 
      
      #add-point:before_more_input
      {<point name="input.more_input"/>}
      #end add-point
      
      BEFORE DIALOG 
         IF g_temp_idx > 0 THEN
            LET l_ac = g_temp_idx
            CALL DIALOG.setCurrentRow("s_detail1",l_ac)
            LET g_temp_idx = 1
         END IF
         LET g_curr_diag = ui.DIALOG.getCurrent()
         #add-point:before_dialog
         {<point name="input.before_dialog"/>}
         #end add-point
         CASE g_aw
            WHEN "s_detail1"
               NEXT FIELD mmamdocno
 
         END CASE
   
      ON ACTION accept
         ACCEPT DIALOG
      
      ON ACTION cancel
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION controlr
         CALL cl_show_req_fields()
 
      ON ACTION controlf
         CALL cl_set_focus_form(ui.Interface.getRootNode()) 
              RETURNING g_fld_name,g_frm_name 
         CALL cl_fldhelp(g_frm_name,g_fld_name,g_lang) 
           
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE DIALOG
   
   END DIALOG 
   
   #add-point:modify段修改後
   {<point name="modify.after_input"/>}
   #end add-point
 
   CLOSE agct300_01_bcl
   CALL s_transaction_end('Y','0')
   
END FUNCTION
]]>
</section>
