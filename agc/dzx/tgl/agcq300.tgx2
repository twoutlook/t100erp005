{<section id="agcq300.update_b" >}
#+ 修改單身後其他table連動
PRIVATE FUNCTION agcq300_update_b(ps_table,ps_keys,ps_keys_bak,ps_page)
   #add-point:update_b段define(客製用) name="update_b.define_customerization"
   {<point name="update_b.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE ps_page          STRING
   DEFINE ps_table         STRING
   DEFINE ps_keys          DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ps_keys_bak      DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ls_group         STRING
   DEFINE li_idx           LIKE type_t.num10
   DEFINE lb_chk           BOOLEAN
   DEFINE l_new_key        DYNAMIC ARRAY OF STRING
   DEFINE l_old_key        DYNAMIC ARRAY OF STRING
   DEFINE l_field_key      DYNAMIC ARRAY OF STRING
   #add-point:update_b段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="update_b.define"
   {<point name="update_b.define"/>}
   #end add-point     
   
   #add-point:Function前置處理  name="update_b.pre_function"
   {<point name="update_b.pre_function"/>}
   #end add-point
   
   #比對新舊值, 判斷key是否有改變
   LET lb_chk = TRUE
   FOR li_idx = 1 TO ps_keys.getLength()
      IF ps_keys[li_idx] <> ps_keys_bak[li_idx] THEN
         LET lb_chk = FALSE
         EXIT FOR
      END IF
   END FOR
   
   #若key無變動, 不需要做處理
   IF lb_chk THEN
      RETURN
   END IF
    
   #若key有變動, 則連動其他table的資料   
   #判斷是否是同一群組的table
   LET ls_group = "gcao_t,"
   IF ls_group.getIndexOf(ps_table,1) > 0 AND ps_table <> "gcao_t" THEN
      #add-point:update_b段修改前 name="update_b.b_update"
      {<point name="update_b.b_update" mark="Y"/>}
      #end add-point     
      UPDATE gcao_t 
         SET (gcao001
              ,gcao002,gcao003,gcao037,gcao004,gcao005,gcao006,gcao008,gcao009,gcao034,gcao038,gcao011,gcao012,gcao013,gcao039,gcao014,gcao015,gcao016,gcao040,gcao017,gcao018,gcao019,gcao041,gcao020,gcao021,gcao022,gcao042,gcao023,gcao024,gcao025,gcao043,gcao026,gcao027,gcao028,gcao044,gcao029,gcao030,gcao031,gcao045) 
              = 
             (ps_keys[1]
              ,g_gcao_d[l_ac].gcao002,g_gcao_d[l_ac].gcao003,g_gcao_d[l_ac].gcao037,g_gcao_d[l_ac].gcao004, 
                  g_gcao_d[l_ac].gcao005,g_gcao_d[l_ac].gcao006,g_gcao_d[l_ac].gcao008,g_gcao_d[l_ac].gcao009, 
                  g_gcao_d[l_ac].gcao034,g_gcao_d[l_ac].gcao038,g_gcao2_d[l_ac].gcao011,g_gcao2_d[l_ac].gcao012, 
                  g_gcao2_d[l_ac].gcao013,g_gcao2_d[l_ac].gcao039,g_gcao2_d[l_ac].gcao014,g_gcao2_d[l_ac].gcao015, 
                  g_gcao2_d[l_ac].gcao016,g_gcao2_d[l_ac].gcao040,g_gcao2_d[l_ac].gcao017,g_gcao2_d[l_ac].gcao018, 
                  g_gcao2_d[l_ac].gcao019,g_gcao2_d[l_ac].gcao041,g_gcao2_d[l_ac].gcao020,g_gcao2_d[l_ac].gcao021, 
                  g_gcao2_d[l_ac].gcao022,g_gcao2_d[l_ac].gcao042,g_gcao2_d[l_ac].gcao023,g_gcao2_d[l_ac].gcao024, 
                  g_gcao2_d[l_ac].gcao025,g_gcao2_d[l_ac].gcao043,g_gcao2_d[l_ac].gcao026,g_gcao2_d[l_ac].gcao027, 
                  g_gcao2_d[l_ac].gcao028,g_gcao2_d[l_ac].gcao044,g_gcao2_d[l_ac].gcao029,g_gcao2_d[l_ac].gcao030, 
                  g_gcao2_d[l_ac].gcao031,g_gcao2_d[l_ac].gcao045) 
         WHERE gcaoent = g_enterprise AND gcao001 = ps_keys_bak[1]
      #add-point:update_b段修改中 name="update_b.m_update"
      {<point name="update_b.m_update"/>}
      #end add-point 
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            CALL s_transaction_end('N','0')
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "gcao_t" 
            LET g_errparam.code = "std-00009" 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
         WHEN SQLCA.SQLCODE #其他錯誤
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "gcao_t:",SQLERRMESSAGE 
            LET g_errparam.code = SQLCA.SQLCODE
            LET g_errparam.popup = TRUE 
            CALL s_transaction_end('N','0')
            CALL cl_err()
         OTHERWISE
            
      END CASE
      #add-point:update_b段修改後 name="update_b.a_update"
      {<point name="update_b.a_update"/>}
      #end add-point 
      RETURN
   END IF
   
 
   
END FUNCTION
{</section>}
 
