      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      AND lc_state <> "A"
      AND lc_state <> "D"
      AND lc_state <> "R"
      AND lc_state <> "W"
      AND lc_state <> "X"
      ) OR 
      g_gcaa_m.gcaastus = lc_state OR cl_null(lc_state) THEN
      CLOSE agct300_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
