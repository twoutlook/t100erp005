   #end add-point   
   
   OPTIONS
   INPUT NO WRAP
   DEFER INTERRUPT
   
   #設定SQL錯誤記錄方式 (模組內定義有效)
   WHENEVER ERROR CALL cl_err_msg_log
       
   #依模組進行系統初始化設定(系統設定)
   CALL cl_ap_init("agc","")
 
   #add-point:作業初始化 name="main.init"
   {<point name="main.init" />}
   #end add-point
   
   
 
   #LOCK CURSOR (identifier)
   #add-point:SQL_define name="main.define_sql"
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT gcaasite,'',gcaadocdt,gcaadocno,gcaa000,gcaaunit,gcaa001,gcaa002,'','', 
       gcaa003,gcaa004,gcaaacti,gcaastus,gcaa005,gcaa006,gcaa007,gcaa008,gcaa009,gcaa010,gcaa011,gcaa012, 
       '',gcaa013,'',gcaa041,'',gcaa015,gcaa043,gcaa037,gcaa038,gcaa039,gcaa016,gcaa017,gcaa042,gcaa018, 
       gcaa019,gcaa020,gcaa021,gcaa022,gcaa023,gcaa034,gcaa035,gcaa036,gcaa024,gcaa025,gcaa031,'','', 
       gcaa026,gcaa032,gcaa033,gcaa044,gcaa045,gcaaownid,'',gcaaowndp,'',gcaacrtid,'',gcaacrtdp,'',gcaacrtdt, 
       gcaamodid,'',gcaamoddt,gcaacnfid,'',gcaacnfdt,gcaa027,gcaa028,gcaa029,gcaa030", 
                      " FROM gcaa_t",
                      " WHERE gcaaent= ? AND gcaadocno=? FOR UPDATE"
   #add-point:SQL_define name="main.after_define_sql"
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE agct300_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.gcaasite,t0.gcaadocdt,t0.gcaadocno,t0.gcaa000,t0.gcaaunit,t0.gcaa001, 
       t0.gcaa002,t0.gcaa003,t0.gcaa004,t0.gcaaacti,t0.gcaastus,t0.gcaa005,t0.gcaa006,t0.gcaa007,t0.gcaa008, 
       t0.gcaa009,t0.gcaa010,t0.gcaa011,t0.gcaa012,t0.gcaa013,t0.gcaa041,t0.gcaa015,t0.gcaa043,t0.gcaa037, 
       t0.gcaa038,t0.gcaa039,t0.gcaa016,t0.gcaa017,t0.gcaa042,t0.gcaa018,t0.gcaa019,t0.gcaa020,t0.gcaa021, 
       t0.gcaa022,t0.gcaa023,t0.gcaa034,t0.gcaa035,t0.gcaa036,t0.gcaa024,t0.gcaa025,t0.gcaa031,t0.gcaa040, 
       t0.gcaa026,t0.gcaa032,t0.gcaa033,t0.gcaa044,t0.gcaa045,t0.gcaaownid,t0.gcaaowndp,t0.gcaacrtid, 
       t0.gcaacrtdp,t0.gcaacrtdt,t0.gcaamodid,t0.gcaamoddt,t0.gcaacnfid,t0.gcaacnfdt,t0.gcaa027,t0.gcaa028, 
       t0.gcaa029,t0.gcaa030,t1.ooefl003 ,t2.oocql004 ,t3.imaal003 ,t4.ooial003 ,t5.ooag011 ,t6.ooefl003 , 
       t7.ooag011 ,t8.ooefl003 ,t9.ooag011 ,t10.ooag011",
               " FROM gcaa_t t0",
                              " LEFT JOIN ooefl_t t1 ON t1.ooeflent="||g_enterprise||" AND t1.ooefl001=t0.gcaasite AND t1.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN oocql_t t2 ON t2.oocqlent="||g_enterprise||" AND t2.oocql001='2071' AND t2.oocql002=t0.gcaa012 AND t2.oocql003='"||g_dlang||"' ",
               " LEFT JOIN imaal_t t3 ON t3.imaalent="||g_enterprise||" AND t3.imaal001=t0.gcaa013 AND t3.imaal002='"||g_dlang||"' ",
               " LEFT JOIN ooial_t t4 ON t4.ooialent="||g_enterprise||" AND t4.ooial001=t0.gcaa031 AND t4.ooial002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=t0.gcaaownid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent="||g_enterprise||" AND t6.ooefl001=t0.gcaaowndp AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent="||g_enterprise||" AND t7.ooag001=t0.gcaacrtid  ",
               " LEFT JOIN ooefl_t t8 ON t8.ooeflent="||g_enterprise||" AND t8.ooefl001=t0.gcaacrtdp AND t8.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t9 ON t9.ooagent="||g_enterprise||" AND t9.ooag001=t0.gcaamodid  ",
               " LEFT JOIN ooag_t t10 ON t10.ooagent="||g_enterprise||" AND t10.ooag001=t0.gcaacnfid  ",
 
               " WHERE t0.gcaaent = " ||g_enterprise|| " AND t0.gcaadocno = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define name="main.after_refresh_sql"
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE agct300_master_referesh FROM g_sql
 
    
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call name="main.servicecall"
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_agct300 WITH FORM cl_ap_formpath("agc",g_code)
   
      #瀏覽頁簽資料初始化
