#+ 功能選單 
PRIVATE FUNCTION azzi090_ui_dialog()
   {<Local define>}
   DEFINE li_idx   LIKE type_t.num5
   {</Local define>}
   #add-point:ui_dialog段define
   {<point name="ui_dialog.define"/>}
   #end add-point 
 
   LET g_action_choice = " "          
 
   CALL cl_set_act_visible("accept,cancel", FALSE)
   
   WHILE TRUE
   
      DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
         DISPLAY ARRAY g_gztd_d TO s_detail1.* ATTRIBUTE(COUNT=g_detail_cnt) 
      
            BEFORE ROW
               LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
               LET l_ac = g_detail_idx
               LET g_temp_idx = l_ac
               IF g_gztd_d[l_ac].gztdstus = "Y" THEN
                  CALL cl_set_act_visible("void", TRUE)
                  CALL cl_set_act_visible("valid", FALSE)
               ELSE
                  CALL cl_set_act_visible("valid", TRUE)
                  CALL cl_set_act_visible("void", FALSE)
               END IF
               DISPLAY g_detail_idx TO FORMONLY.idx
               CALL cl_show_fld_cont() 
         END DISPLAY
      

      
         #add-point:ui_dialog段define
         {<point name="ui_dialog.more_displayarray"/>}
         #end add-point
         
         BEFORE DIALOG
            IF g_temp_idx > 0 THEN
               LET l_ac = g_temp_idx
               CALL DIALOG.setCurrentRow("s_detail1",l_ac)
               LET g_temp_idx = 1
            END IF
            LET g_curr_diag = ui.DIALOG.getCurrent()         
            CALL DIALOG.setSelectionMode("s_detail1", 1)
            NEXT FIELD CURRENT
      
         


         ON ACTION query
            LET g_action_choice="query"
            IF cl_chk_act_auth() THEN 
               CALL azzi090_query()
            #add-point:ON ACTION query
            {<point name="menu.query" />}
            #END add-point

                EXIT DIALOG
            END IF



         ON ACTION modify
            LET g_action_choice="modify"
            IF cl_chk_act_auth() THEN 
               CALL azzi090_modify()
            #add-point:ON ACTION modify
            {<point name="menu.modify" />}
            #END add-point

                EXIT DIALOG
            END IF

      
         # 全選修改狀態
         ON ACTION valid
            CALL azzi090_statechange("Y")
            #資料重讀
            CALL azzi090_b_fill(g_wc2)
            EXIT DIALOG
            
         ON ACTION void
            CALL azzi090_statechange("N")
            #資料重讀
            CALL azzi090_b_fill(g_wc2)
            EXIT DIALOG
      
         ON ACTION close
            LET INT_FLAG=FALSE         
            LET g_action_choice="exit"
            EXIT WHILE
      
         ON ACTION exit
            LET g_action_choice="exit"
            EXIT WHILE
      
         #主選單用ACTION
         &include "main_menu.4gl"
         
         #交談指令共用ACTION
         &include "common_action.4gl"
            CONTINUE DIALOG
      END DIALOG
	  
   END WHILE
 
   CALL cl_set_act_visible("accept,cancel", TRUE)
 
END FUNCTION
 
 
#+ QBE資料查詢
PRIVATE FUNCTION azzi090_query()
   {<Local define>}
   DEFINE ls_wc      LIKE type_t.chr500
   DEFINE ls_return  STRING
   DEFINE ls_result  STRING 
   {</Local define>}
   #add-point:query段define
   {<point name="query.define"/>}
   #end add-point 
   
   LET INT_FLAG = 0
   CLEAR FORM
   CALL g_gztd_d.clear()
   
   #wc備份
   LET ls_wc = g_wc2
   
   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)
 
      CONSTRUCT g_wc2 ON gztdstus,gztd001,gztd002,gztd004,gztd003,gztd005,gztd006,gztd007,gztd008,gztdmodu,gztddate,gztdoriu,gztdorid,gztduser,gztddept,gztdbuid                        
         FROM s_detail1[1].gztdstus,s_detail1[1].gztd001,s_detail1[1].gztd002,s_detail1[1].gztd004,s_detail1[1].gztd003,s_detail1[1].gztd005,s_detail1[1].gztd006,s_detail1[1].gztd007,s_detail1[1].gztd008,s_detail1[1].gztdmodu,s_detail1[1].gztddate,s_detail1[1].gztdoriu,s_detail1[1].gztdorid,s_detail1[1].gztduser,s_detail1[1].gztddept,s_detail1[1].gztdbuid 
      
         #----<<gztdmodu>>----
         ON ACTION controlp INFIELD gztdmodu
            CALL q_common("gztd_t","gztdmodu",TRUE,FALSE,g_gztd_d[l_ac].gztdmodu) RETURNING ls_return
            DISPLAY ls_return TO gztdmodu
            NEXT FIELD gztdmodu
      
         #----<<gztdoriu>>----
         ON ACTION controlp INFIELD gztdoriu
            CALL q_common("gztd_t","gztdoriu",TRUE,FALSE,g_gztd_d[l_ac].gztdoriu) RETURNING ls_return
            DISPLAY ls_return TO gztdoriu
            NEXT FIELD gztdoriu
      
         #----<<gztduser>>----
         ON ACTION controlp INFIELD gztduser
            CALL q_common("gztd_t","gztduser",TRUE,FALSE,g_gztd_d[l_ac].gztduser) RETURNING ls_return
            DISPLAY ls_return TO gztduser
            NEXT FIELD gztduser
      
         #----<<gztdorid>>----
         ON ACTION controlp INFIELD gztdorid
            CALL q_common("gztd_t","gztdorid",TRUE,FALSE,g_gztd_d[l_ac].gztdorid) RETURNING ls_return
            DISPLAY ls_return TO gztdorid
            NEXT FIELD gztdorid
      
         #----<<gztddept>>----
         ON ACTION controlp INFIELD gztddept
            CALL q_common("gztd_t","gztddept",TRUE,FALSE,g_gztd_d[l_ac].gztddept) RETURNING ls_return
            DISPLAY ls_return TO gztddept
            NEXT FIELD gztddept
 
         #----<<gztdbuid>>----
         AFTER FIELD gztdbuid
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
         
         #----<<gztddate>>----
         AFTER FIELD gztddate
            CALL FGL_DIALOG_GETBUFFER() RETURNING ls_result
            IF NOT cl_null(ls_result) THEN
               IF NOT cl_chk_date_symbol(ls_result) THEN
                  LET ls_result = cl_add_date_extra_cond(ls_result)
               END IF
            END IF
            CALL FGL_DIALOG_SETBUFFER(ls_result)
      
         #---------------------<  Detail: page     1  >---------------------
         #----<<gztdstus>>----
         #Ctrlp:query.c.gztdstus
         ON ACTION controlp INFIELD gztdstus
            #add-point:ON ACTION controlp INFIELD gztdstus
            {<point name="query.c.page1.gztdstus" />}
            #END add-point

         #----<<gztd001>>----
         #Ctrlp:query.c.gztd001
         ON ACTION controlp INFIELD gztd001
            #add-point:ON ACTION controlp INFIELD gztd001
            {<point name="query.c.page1.gztd001" />}
            #END add-point

         #----<<gztd002>>----
         #Ctrlp:query.c.gztd002
         ON ACTION controlp INFIELD gztd002
            #add-point:ON ACTION controlp INFIELD gztd002
            {<point name="query.c.page1.gztd002" />}
            #END add-point

         #----<<gztd004>>----
         #Ctrlp:query.c.gztd004
         ON ACTION controlp INFIELD gztd004
            #add-point:ON ACTION controlp INFIELD gztd004
            {<point name="query.c.page1.gztd004" />}
            #END add-point

         #----<<gztd003>>----
         #Ctrlp:query.c.gztd003
         ON ACTION controlp INFIELD gztd003
            #add-point:ON ACTION controlp INFIELD gztd003
            {<point name="query.c.page1.gztd003" />}
            #END add-point

         #----<<gztd005>>----
         #Ctrlp:query.c.gztd005
         ON ACTION controlp INFIELD gztd005
            #add-point:ON ACTION controlp INFIELD gztd005
            {<point name="query.c.page1.gztd005" />}
            #END add-point

         #----<<gztd006>>----
         #Ctrlp:query.c.gztd006
         ON ACTION controlp INFIELD gztd006
            #add-point:ON ACTION controlp INFIELD gztd006
            {<point name="query.c.page1.gztd006" />}
            #END add-point

         #----<<gztd007>>----
         #Ctrlp:query.c.gztd007
         ON ACTION controlp INFIELD gztd007
            #add-point:ON ACTION controlp INFIELD gztd007
            {<point name="query.c.page1.gztd007" />}
            #END add-point

         #----<<gztd008>>----
         #Ctrlp:query.c.gztd008
         ON ACTION controlp INFIELD gztd008
            #add-point:ON ACTION controlp INFIELD gztd008
            {<point name="query.c.page1.gztd008" />}
            #END add-point

         #----<<gztdmodu>>----
         #----<<modu_desc>>----
         #----<<gztddate>>----
         #----<<gztdoriu>>----
         #----<<oriu_desc>>----
         #----<<gztdorid>>----
         #----<<orid_desc>>----
         #----<<gztduser>>----
         #----<<user_desc>>----
         #----<<gztddept>>----
         #----<<dept_desc>>----
         #----<<gztdbuid>>----
  
      
         BEFORE CONSTRUCT
            CALL cl_qbe_init()
      
         ON ACTION qbe_select
            CALL cl_qbe_select()
      
         ON ACTION qbe_save
            CALL cl_qbe_save()
      
      END CONSTRUCT
  
      #add-point:query段more_construct
      {<point name="query.more_construct"/>}
      #end add-point 
	  
	  ON ACTION accept
	     EXIT DIALOG
		 
	  ON ACTION cancel
	     LET INT_FLAG = 1
		 EXIT DIALOG
	  
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE DIALOG 
   END DIALOG
 
   LET g_wc2 = g_wc2 CLIPPED, cl_get_extra_cond("g_gztd_d[l_ac].gztduser", "")
 
   IF INT_FLAG THEN
      LET INT_FLAG = 0
      #還原
      #LET g_wc2 = ls_wc
	  LET g_wc2 = " 1=1"
   END IF
  
   CALL azzi090_b_fill(g_wc2)
   
   IF g_detail_cnt = 0 AND NOT INT_FLAG THEN
      CALL cl_err("",-100,1)
   END IF
   
   LET INT_FLAG = FALSE
   
END FUNCTION
 
 
#+ 資料修改
PRIVATE FUNCTION azzi090_modify()
   {<Local define>}
   DEFINE l_ac_t          LIKE type_t.num5     #未取消的ARRAY CNT      
   DEFINE l_n             LIKE type_t.num5     #檢查重複用             
   DEFINE l_lock_sw       LIKE type_t.chr1     #單身鎖住否            
   DEFINE p_cmd           LIKE type_t.chr1     #處理狀態              
   DEFINE l_allow_insert  LIKE type_t.chr1     #可新增否
   DEFINE l_allow_delete  LIKE type_t.chr1     
   DEFINE l_insert        LIKE type_t.num5     #輸入時判斷是否在inser狀態
   DEFINE ls_return       STRING
   DEFINE l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE l_vars          DYNAMIC ARRAY OF STRING
   DEFINE l_fields        DYNAMIC ARRAY OF STRING
   DEFINE l_var_keys_bak  DYNAMIC ARRAY OF STRING
   {</Local define>}
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point 
   
   LET l_insert = FALSE
   LET g_action_choice = ""
 
   LET l_allow_insert = cl_detail_input_auth("insert")
   LET l_allow_delete = cl_detail_input_auth("delete")
 
   LET g_forupd_sql = "SELECT gztdstus,gztd001,gztd002,gztd004,gztd003,gztd005,gztd006,gztd007,gztd008,gztdmodu,'',gztddate,gztdoriu,'',gztdorid,'',gztduser,'',gztddept,'',gztdbuid FROM gztd_t WHERE gztd001=? FOR UPDATE"
   LET g_forupd_sql = cl_forupd_sql(g_forupd_sql)
   DECLARE azzi090_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR
 
   INPUT ARRAY g_gztd_d FROM s_detail1.*
         ATTRIBUTE (WITHOUT DEFAULTS, UNBUFFERED, FIELD ORDER FORM,
                    COUNT = g_detail_cnt,MAXCOUNT = g_max_rec,
                    INSERT ROW = l_allow_insert, DELETE ROW = l_allow_delete,
                    APPEND ROW = l_allow_insert)
      #自訂ACTION
      
 
      BEFORE INPUT
         IF g_detail_cnt != 0 THEN
            CALL fgl_set_arr_curr(l_ac)
         END IF
 
      BEFORE ROW
         LET p_cmd="" 
         LET l_ac = ARR_CURR()
         LET l_lock_sw = "N"            #DEFAULT
         LET l_n  = ARR_COUNT()
 
         IF g_detail_cnt>=l_ac THEN
            BEGIN WORK
            LET p_cmd="u"
 
            LET g_before_input_done = FALSE                                      
            CALL azzi090_set_entry_b(p_cmd)                                           
            CALL azzi090_set_no_entry_b(p_cmd)                                        
            LET g_before_input_done = TRUE                                       
            
            LET g_gztd_d_t.* = g_gztd_d[l_ac].*  #BACKUP
 
            OPEN azzi090_bcl USING g_gztd_d_t.gztd001

            IF STATUS THEN
               CALL cl_err("OPEN azzi090_bcl:", STATUS, 1)
               LET l_lock_sw = "Y"
            ELSE 
               FETCH azzi090_bcl INTO g_gztd_d[l_ac].gztdstus,g_gztd_d[l_ac].gztd001,g_gztd_d[l_ac].gztd002,g_gztd_d[l_ac].gztd004,g_gztd_d[l_ac].gztd003,g_gztd_d[l_ac].gztd005,g_gztd_d[l_ac].gztd006,g_gztd_d[l_ac].gztd007,g_gztd_d[l_ac].gztd008,g_gztd_d[l_ac].gztdmodu,g_gztd_d[l_ac].modu_desc,g_gztd_d[l_ac].gztddate,g_gztd_d[l_ac].gztdoriu,g_gztd_d[l_ac].oriu_desc,g_gztd_d[l_ac].gztdorid,g_gztd_d[l_ac].orid_desc,g_gztd_d[l_ac].gztduser,g_gztd_d[l_ac].user_desc,g_gztd_d[l_ac].gztddept,g_gztd_d[l_ac].dept_desc,g_gztd_d[l_ac].gztdbuid 
               IF SQLCA.sqlcode THEN
                  CALL cl_err(g_gztd_d_t.gztd001,SQLCA.sqlcode,1)
                  LET l_lock_sw = "Y"
               END IF
            END IF
            
            CALL azzi090_detail_show()      
            CALL cl_show_fld_cont() 
 
            
 
            #add-point:modify段before_row
            {<point name="modify.before_row"/>}
            #end add-point  
         END IF
  
      BEFORE INSERT
         LET l_insert = TRUE
         LET l_n = ARR_COUNT()
         LET p_cmd="a"   
                                                        
         LET g_before_input_done = FALSE                                        
         CALL azzi090_set_entry_b(p_cmd)                                             
         CALL azzi090_set_no_entry_b(p_cmd)                                          
         LET g_before_input_done = TRUE                                         
 
         INITIALIZE g_gztd_d[l_ac].* TO NULL      
 
         LET g_gztd_d_t.* = g_gztd_d[l_ac].*         #新輸入資料
         CALL cl_show_fld_cont()  
         LET g_gztd_d[l_ac].gztdstus = "Y"
         LET g_gztd_d[l_ac].gztdbuid = cl_get_current()
         LET g_gztd_d[l_ac].gztddate = cl_get_current()
         LET g_gztd_d[l_ac].gztdmodu = g_user
         LET g_gztd_d[l_ac].gztduser = g_user
         LET g_gztd_d[l_ac].gztdoriu = g_user
         LET g_gztd_d[l_ac].gztdorid = g_dept
         LET g_gztd_d[l_ac].gztddept = g_dept
         
        
                  
         CALL azzi090_detail_show()  
         #add-point:modify段before insert
         {<point name="modify.before_insert"/>}
         #end add-point  
         NEXT FIELD gztd001
 
      AFTER INSERT
         LET l_insert = FALSE
         IF INT_FLAG THEN
            CALL cl_err("",9001,0)
            LET INT_FLAG = 0
            CLOSE azzi090_bcl
            CANCEL INSERT
         END IF
      
         BEGIN WORK                    
 
         #add-point:單身新增前
         {<point name="modify.b_insert"/>}
         #end add-point
 
         INSERT INTO gztd_t
                (gztdstus,gztd001,gztd002,gztd004,gztd003,gztd005,gztd006,gztd007,gztd008,gztdmodu,gztddate,gztdoriu,gztdorid,gztduser,gztddept,gztdbuid)
         VALUES (g_gztd_d[l_ac].gztdstus,g_gztd_d[l_ac].gztd001,g_gztd_d[l_ac].gztd002,g_gztd_d[l_ac].gztd004,g_gztd_d[l_ac].gztd003,g_gztd_d[l_ac].gztd005,g_gztd_d[l_ac].gztd006,g_gztd_d[l_ac].gztd007,g_gztd_d[l_ac].gztd008,g_gztd_d[l_ac].gztdmodu,g_gztd_d[l_ac].gztddate,g_gztd_d[l_ac].gztdoriu,g_gztd_d[l_ac].gztdorid,g_gztd_d[l_ac].gztduser,g_gztd_d[l_ac].gztddept,g_gztd_d[l_ac].gztdbuid)
                
         IF SQLCA.sqlcode THEN
            CALL cl_err("gztd_t",SQLCA.sqlcode,1)  
            ROLLBACK WORK              
            CANCEL INSERT
         ELSE
            #add-point:after_insert
            {<point name="modify.after_insert"/>}
            #end add-point
            #資料多語言用-增/改
            
            ERROR "INSERT O.K"
            LET g_detail_cnt=g_detail_cnt+1
            DISPLAY g_detail_cnt TO FORMONLY.cnt
            CALL azzi090_detail_show()      
         END IF
         
         #add-point:單身新增後
         {<point name="modify.a_insert"/>}
         #end add-point
 
      BEFORE DELETE                            #是否取消單身
         IF g_gztd_d_t.gztd001 IS NOT NULL THEN
 
            #add-point:單身刪除前
            {<point name="modify.b_delete"/>}
            #end add-point
 
            IF NOT cl_ask_delete() THEN
               ROLLBACK WORK
               CANCEL DELETE
            END IF
 
            IF l_lock_sw = "Y" THEN 
               CALL cl_err("", -263, 1) 
               ROLLBACK WORK
               CANCEL DELETE 
            END IF 
            
            DELETE FROM gztd_t
             WHERE  gztd001 = g_gztd_d_t.gztd001

            IF SQLCA.sqlcode THEN
               CALL cl_err("gztd_t",SQLCA.sqlcode,1) 
               ROLLBACK WORK
               CANCEL DELETE
               EXIT INPUT
            ELSE
               ERROR "DELETE O.K."
               LET g_detail_cnt=g_detail_cnt-1
               DISPLAY g_detail_cnt TO FORMONLY.cnt
            END IF
            
            #add-point:單身刪除後
            {<point name="modify.a_delete"/>}
            #end add-point
            
         END IF
      
      AFTER DELETE 
         #add-point:單身刪除後2
         {<point name="modify.after_delete"/>}
         #end add-point
 
      ON ROW CHANGE
         IF INT_FLAG THEN                 #新增程式段
            CALL cl_err("",9001,0)
            LET INT_FLAG = 0
            LET g_gztd_d[l_ac].* = g_gztd_d_t.*
            CLOSE azzi090_bcl
            ROLLBACK WORK
            EXIT INPUT
         END IF
 
         IF l_lock_sw = "Y" THEN
            CALL cl_err(g_gztd_d[l_ac].gztd001,-263,0)
            LET g_gztd_d[l_ac].* = g_gztd_d_t.*
