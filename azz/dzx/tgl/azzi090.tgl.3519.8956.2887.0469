#+ 單身陣列填充
PRIVATE FUNCTION azzi090_b_fill(p_wc2)              #BODY FILL UP
   {<Local define>}
   DEFINE p_wc2           STRING
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
 
   LET g_sql = "SELECT gztdstus,gztd001,gztd002,gztd004,gztd003,gztd005,gztd006,gztd007,gztd008,gztdmodu,'',gztddate,gztdoriu,'',gztdorid,'',gztduser,'',gztddept,'',gztdbuid FROM gztd_t WHERE 1=1  AND ", p_wc2 CLIPPED,           #單身
               " ORDER BY gztd001" 
  
   PREPARE azzi090_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR azzi090_pb
   
   
 
   CALL g_gztd_d.clear()

 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
   FOREACH b_fill_curs INTO g_gztd_d[l_ac].gztdstus,g_gztd_d[l_ac].gztd001,g_gztd_d[l_ac].gztd002,g_gztd_d[l_ac].gztd004,g_gztd_d[l_ac].gztd003,g_gztd_d[l_ac].gztd005,g_gztd_d[l_ac].gztd006,g_gztd_d[l_ac].gztd007,g_gztd_d[l_ac].gztd008,g_gztd_d[l_ac].gztdmodu,g_gztd_d[l_ac].modu_desc,g_gztd_d[l_ac].gztddate,g_gztd_d[l_ac].gztdoriu,g_gztd_d[l_ac].oriu_desc,g_gztd_d[l_ac].gztdorid,g_gztd_d[l_ac].orid_desc,g_gztd_d[l_ac].gztduser,g_gztd_d[l_ac].user_desc,g_gztd_d[l_ac].gztddept,g_gztd_d[l_ac].dept_desc,g_gztd_d[l_ac].gztdbuid

      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 

      CALL azzi090_detail_show()      
      
      #add-point:單身資料抓取
      {<point name="bfill.foreach"/>}
      #end add-point
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         CALL cl_err( "", 9035, 0 )
         EXIT FOREACH
      END IF
      
   END FOREACH
 
   CALL g_gztd_d.deleteElement(l_ac) 
   ERROR "" 

 
   LET g_detail_cnt = l_ac - 1
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   LET l_ac = g_cnt
   LET g_cnt = 0
   
END FUNCTION
 
