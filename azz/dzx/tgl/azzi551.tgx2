{<section id="azzi551.modify" >}
#+ 資料修改
PRIVATE FUNCTION azzi551_modify()
   #add-point:modify段define name="modify.define_customerization"
   {<point name="modify.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   {<point name="modify.define"/>}
   #end add-point
   
   #add-point:Function前置處理  name="modify.pre_function"
   {<point name="modify.pre_function"/>}
   #end add-point
   
   IF g_gzth_m.gzth001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code = "std-00003" 
      LET g_errparam.popup = FALSE 
      CALL cl_err()
      RETURN
   END IF 
   
   EXECUTE azzi551_master_referesh USING g_gzth_m.gzth001,g_gzth_m.gzth002 INTO g_gzth_m.gzth004,g_gzth_m.gzth002, 
       g_gzth_m.gzth001,g_gzth_m.gzth007,g_gzth_m.gzth003,g_gzth_m.gzth005,g_gzth_m.gzth006,g_gzth_m.gzthstus, 
       g_gzth_m.gzthownid,g_gzth_m.gzthowndp,g_gzth_m.gzthcrtid,g_gzth_m.gzthcrtdp,g_gzth_m.gzthcrtdt, 
       g_gzth_m.gzthmodid,g_gzth_m.gzthmoddt,g_gzth_m.gzth001_desc,g_gzth_m.gzthownid_desc,g_gzth_m.gzthowndp_desc, 
       g_gzth_m.gzthcrtid_desc,g_gzth_m.gzthcrtdp_desc,g_gzth_m.gzthmodid_desc
 
   #檢查是否允許此動作
   IF NOT azzi551_action_chk() THEN
      RETURN
   END IF
  
   LET g_gzth001_t = g_gzth_m.gzth001
   LET g_gzth002_t = g_gzth_m.gzth002
 
   
   CALL s_transaction_begin()
   
   OPEN azzi551_cl USING g_gzth_m.gzth001,g_gzth_m.gzth002
   IF SQLCA.SQLCODE THEN   #(ver:36)
      CLOSE azzi551_cl
      CALL s_transaction_end("N","0")
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN azzi551_cl:" 
      LET g_errparam.code = SQLCA.SQLCODE   #(ver:36)
      LET g_errparam.popup = TRUE 
      CALL cl_err()
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH azzi551_cl INTO g_gzth_m.gzth004,g_gzth_m.gzth002,g_gzth_m.gzth001,g_gzth_m.gzth001_desc,g_gzth_m.gzthl004, 
       g_gzth_m.gzth007,g_gzth_m.gzth003,g_gzth_m.gzth005,g_gzth_m.gzth006,g_gzth_m.gzthstus,g_gzth_m.gzthownid, 
       g_gzth_m.gzthownid_desc,g_gzth_m.gzthowndp,g_gzth_m.gzthowndp_desc,g_gzth_m.gzthcrtid,g_gzth_m.gzthcrtid_desc, 
       g_gzth_m.gzthcrtdp,g_gzth_m.gzthcrtdp_desc,g_gzth_m.gzthcrtdt,g_gzth_m.gzthmodid,g_gzth_m.gzthmodid_desc, 
       g_gzth_m.gzthmoddt
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.SQLCODE THEN
      CLOSE azzi551_cl
      CALL s_transaction_end("N","0")
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = g_gzth_m.gzth001,":",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE 
      LET g_errparam.popup = FALSE 
      CALL cl_err()
      RETURN
   END IF
   #其他table資料備份(確定是否更改用)
   LET g_master_multi_table_t.gzthl001 = g_gzth_m.gzth001
LET g_master_multi_table_t.gzthl002 = g_gzth_m.gzth002
LET g_master_multi_table_t.gzthl004 = g_gzth_m.gzthl004
 
 
   CALL azzi551_show()
   WHILE TRUE
      LET g_gzth_m.gzth001 = g_gzth001_t
      LET g_gzth_m.gzth002 = g_gzth002_t
 
      
      #寫入修改者/修改日期資訊
      LET g_gzth_m.gzthmodid = g_user 
LET g_gzth_m.gzthmoddt = cl_get_current()
LET g_gzth_m.gzthmodid_desc = cl_get_username(g_gzth_m.gzthmodid)
 
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL azzi551_input("u")     #欄位更改
 
      #add-point:modify段修改後 name="modify.after_input"
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_gzth_m.* = g_gzth_m_t.*
         CALL azzi551_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code = 9001 
         LET g_errparam.popup = FALSE 
         CALL cl_err()
 
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE gzth_t SET (gzthmodid,gzthmoddt) = (g_gzth_m.gzthmodid,g_gzth_m.gzthmoddt)
       WHERE  gzth001 = g_gzth001_t
         AND gzth002 = g_gzth002_t
 
      
      EXIT WHILE
  
   END WHILE
 
   CLOSE azzi551_cl
   CALL s_transaction_end("Y","0")
 
   #功能已完成,通報訊息中心
   CALL azzi551_msgcentre_notify('modify')
   
END FUNCTION
{</section>}
 
{<section id="azzi551.check" >}
#+ 避免資料錯誤
PRIVATE FUNCTION azzi551_check(ps_id,ps_pid)
   #add-point:check段define name="check.define_customerization"
   {<point name="check.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE ps_id       STRING
   DEFINE ps_pid      STRING
   DEFINE ps_type     STRING
   DEFINE ls_pid      LIKE type_t.chr100
   DEFINE ls_id       LIKE type_t.chr100
   DEFINE ls_type     LIKE type_t.chr100
   DEFINE ls_return   LIKE type_t.num5
   DEFINE ls_sql      STRING
   DEFINE li_cnt      LIKE type_t.num10
   #add-point:check段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="check.define"
   {<point name="check.define"/>}
   #end add-point
   
   #add-point:Function前置處理  name="check.pre_function"
   {<point name="check.pre_function"/>}
   #end add-point
   
   #從該節點往上檢查, 若出現ID重複的狀況代表會導致無限迴圈
   LET ls_sql = " SELECT gzth001,gzth002 FROM ",
                " (SELECT gzth001,gzth002 FROM gzth_t WHERE  gzth001<>gzth002)",
                " WHERE gzth001 = '",ps_id,"' OR gzth002 = '",ps_id,"'",
                " START WITH gzth001='",ps_pid,"'",
                " CONNECT BY PRIOR gzth002=gzth001 "
 
   PREPARE check_cnt FROM ls_sql
   DECLARE check_cntcur CURSOR FOR check_cnt
   OPEN check_cntcur
   IF SQLCA.SQLCODE THEN   #(ver:36)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN check_cntcur:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE   #(ver:36)
      LET g_errparam.popup = TRUE 
      CALL cl_err()
      RETURN FALSE
   END IF
 
   FETCH check_cntcur INTO li_cnt
 
   IF li_cnt > 0 THEN
      LET ls_return = TRUE
   ELSE
      LET ls_return = FALSE
   END IF
   
   RETURN ls_return 
   
END FUNCTION
{</section>}
 
{<section id="azzi551.reproduce" >}
#+ 資料複製
PRIVATE FUNCTION azzi551_reproduce(l_dialog)
   #add-point:reproduce段define name="reproduce.define_customerization"
   {<point name="reproduce.define_customerization" edit="c" mark="Y"/>}
   #end add-point   
   DEFINE l_dialog      ui.DIALOG
   DEFINE li_addpos     LIKE type_t.num10
   DEFINE l_newno           LIKE gzth_t.gzth001 
   DEFINE l_oldno           LIKE gzth_t.gzth001 
   DEFINE l_newno02     LIKE gzth_t.gzth002 
   DEFINE l_oldno02     LIKE gzth_t.gzth002 
 
   DEFINE l_master          RECORD LIKE gzth_t.*
   DEFINE l_cnt             LIKE type_t.num10  
   DEFINE li_idx            LIKE type_t.num10  
   DEFINE li_cnt            LIKE type_t.num10  
   #add-point:reproduce段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="reproduce.define"
   {<point name="reproduce.define"/>}
   #end add-point
   
   #add-point:Function前置處理  name="reproduce.before_reproduce"
   {<point name="reproduce.before_reproduce"/>}
   #end add-point
 
   #檢查PK欄位是否均有值,若全部為NULL時退出
   IF g_gzth_m.gzth001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code = "std-00003" 
      LET g_errparam.popup = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   EXECUTE azzi551_master_referesh USING g_gzth_m.gzth001,g_gzth_m.gzth002 INTO g_gzth_m.gzth004,g_gzth_m.gzth002, 
       g_gzth_m.gzth001,g_gzth_m.gzth007,g_gzth_m.gzth003,g_gzth_m.gzth005,g_gzth_m.gzth006,g_gzth_m.gzthstus, 
       g_gzth_m.gzthownid,g_gzth_m.gzthowndp,g_gzth_m.gzthcrtid,g_gzth_m.gzthcrtdp,g_gzth_m.gzthcrtdt, 
       g_gzth_m.gzthmodid,g_gzth_m.gzthmoddt,g_gzth_m.gzth001_desc,g_gzth_m.gzthownid_desc,g_gzth_m.gzthowndp_desc, 
       g_gzth_m.gzthcrtid_desc,g_gzth_m.gzthcrtdp_desc,g_gzth_m.gzthmodid_desc
 
   #檢查如果有子節點(hasC=1)則顯示錯誤訊息後退出
 
   ERROR ""
 
   CALL cl_set_head_visible("","YES")
 
      LET g_gzth_m.gzth001_desc = ''
   DISPLAY BY NAME g_gzth_m.gzth001_desc
 
 
   LET g_gzth_m.gzth001 = ""
   LET g_gzth_m.gzth002 = ""
 
   #應用 a14 樣板自動產生(Version:5)    
      #公用欄位新增給值  
      LET g_gzth_m.gzthownid = g_user
      LET g_gzth_m.gzthowndp = g_dept
      LET g_gzth_m.gzthcrtid = g_user
      LET g_gzth_m.gzthcrtdp = g_dept 
      LET g_gzth_m.gzthcrtdt = cl_get_current()
      LET g_gzth_m.gzthmodid = g_user
      LET g_gzth_m.gzthmoddt = cl_get_current()
      LET g_gzth_m.gzthstus = 'Y'
 
 
 
   
   CALL s_transaction_begin()
   
   #其他table資料備份(確定是否更改用)
   LET g_master_multi_table_t.gzthl001 = g_gzth_m.gzth001
LET g_master_multi_table_t.gzthl002 = g_gzth_m.gzth002
LET g_master_multi_table_t.gzthl004 = g_gzth_m.gzthl004
 
 
   #add-point:複製輸入前 name="reproduce.head.b_input"
   {<point name="reproduce.head.b_input"/>}
   #end add-point
   
   #直接呼叫輸入
   CALL azzi551_input("r")
 
   #add-point:完成複製段落後 name="reproduce.after_reproduce"
   {<point name="reproduce.after_reproduce" />}
   #end add-point
 
   IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_gzth_m.* = g_gzth_m_t.*
         CALL azzi551_show()
         INITIALIZE g_errparam TO NULL
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code = 9001 
         LET g_errparam.popup = FALSE 
         CALL cl_err()
      ELSE
         #分兩種狀況-1.根節點, 2.非根節點
         IF g_gzth_m.gzth002 = g_gzth_m.gzth001 THEN
            #為根節點
            LET li_addpos = g_browser.getLength() + 1
            LET g_browser[li_addpos].b_gzth002 = g_gzth_m.gzth002
            LET g_browser[li_addpos].b_gzth001 = g_gzth_m.gzth001
            LET g_browser[li_addpos].b_exp  = FALSE
            LET g_browser[li_addpos].b_hasC = FALSE
            LET g_browser[li_addpos].b_id  = '0.add',g_add_idx USING "<<<"
            LET g_browser[li_addpos].b_pid = '0',g_add_idx USING "<<<"
            LET g_add_idx = g_add_idx + 1
            CALL azzi551_desc_show(li_addpos)
            LET g_current_idx = li_addpos
         ELSE
            #非根節點, 開始搜尋其父節點(已展開才做處理)
            LET li_cnt = g_cnt
            IF g_browser.getLength() > 0 THEN
               FOR li_idx = 1 TO g_browser.getLength()
                  IF g_browser[li_idx].b_gzth001 = g_gzth_m.gzth002 THEN
                     LET li_addpos = l_dialog.appendNode("s_browse",li_idx)
                     LET g_cnt = li_addpos
                     LET g_browser[li_addpos].b_gzth002 = g_gzth_m.gzth002
                     LET g_browser[li_addpos].b_gzth001 = g_gzth_m.gzth001
                     EXECUTE master_refreshcur USING g_browser[li_addpos].b_gzth001
                                                     ,g_browser[li_addpos].b_gzth002
 
                                                INTO g_browser[g_cnt].b_gzth005,g_browser[g_cnt].b_gzthstus, 
                                                    g_browser[g_cnt].b_gzth004,g_browser[g_cnt].b_gzth001, 
                                                    g_browser[g_cnt].b_gzth002,g_browser[g_cnt].b_gzth001_desc, 
                                                    g_browser[g_cnt].b_gzth001_desc_3,g_browser[g_cnt].b_gzth001_desc_4 

                     LET g_browser[li_addpos].b_exp  = FALSE
                     LET g_browser[li_addpos].b_hasC = FALSE
                     LET g_browser[li_addpos].b_id  = g_browser[li_idx].b_id, '.add',g_add_idx USING "<<<"
                     LET g_browser[li_addpos].b_pid = g_browser[li_idx].b_id
                     LET g_add_idx = g_add_idx + 1
                     CALL azzi551_desc_show(li_addpos)
                     #打開父節點
                     LET g_browser[li_idx].b_hasC = TRUE
                     LET g_browser[li_idx].b_exp = TRUE
                     LET g_current_idx = li_addpos
                     EXIT FOR
                  END IF
               END FOR
            END IF
            LET g_cnt = li_cnt 
         END IF
      END IF
 
   #功能已完成,通報訊息中心
   CALL azzi551_msgcentre_notify('reproduce')
 
END FUNCTION
