   DECLARE azzi551_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.gzth004,t0.gzth002,t0.gzth001,t0.gzth007,t0.gzth003,t0.gzth005,t0.gzth006, 
       t0.gzthstus,t0.gzthownid,t0.gzthowndp,t0.gzthcrtid,t0.gzthcrtdp,t0.gzthcrtdt,t0.gzthmodid,t0.gzthmoddt, 
       t1.gzzol003 ,t2.ooag011 ,t3.ooefl003 ,t4.ooag011 ,t5.ooefl003 ,t6.ooag011",
