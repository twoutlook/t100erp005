{<section id="azzi551.browser_fill" >}
#+ 瀏覽頁簽where條件組成
PRIVATE FUNCTION azzi551_browser_fill(p_wc,p_type)
   #add-point:browser_fill段define name="browser_fill.define_customerization"
   {<point name="browser_fill.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE p_wc       STRING 
   DEFINE p_type     LIKE type_t.chr10
   DEFINE l_cnt      LIKE type_t.num10
   DEFINE l_cnt2     LIKE type_t.num10   
   #add-point:browser_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="browser_fill.define"
   {<point name="browser_fill.define"/>}
   #end add-point
   
   #add-point:Function前置處理  name="browser_fill.before_fill"
   {<point name="browser_fill.before_fill"/>}
   #end add-point
 
   CALL g_browser.clear()
   CLEAR FORM
   LET l_cnt = 0
   LET l_cnt2 = 0
   
   DROP TABLE azzi551_tmp
   
   #Create temp table
   CREATE TEMP TABLE azzi551_tmp
   (
         gzth005 VARCHAR(500),
   gzthstus VARCHAR(500),
   gzth004 VARCHAR(500),
   img VARCHAR(500),
   gzth001 VARCHAR(500),
   gzth001_desc VARCHAR(500),
   gzth001_desc_3 VARCHAR(500),
   gzth001_desc_4 VARCHAR(500),
   gzth002 VARCHAR(500),
      #僅含browser的欄位
      exp_code  VARCHAR(5)
   );
 
   #先確定搜尋範圍(若無條件搜尋則只找root出來)
   SELECT COUNT(1) INTO l_cnt FROM gzth_t WHERE  1=1
   
   #取得符合p_wc的所有資料
   LET g_sql = "SELECT COUNT(1)",
               " FROM gzth_t ",
               "  LEFT JOIN gzthl_t ON gzth001 = gzthl001 AND gzth002 = gzthl002 AND gzthl003 = '",g_dlang,"' ",
               " WHERE  ",p_wc ,cl_sql_add_filter("gzth_t")
   #add-point:browser_fill段cnt wc name="browser_fill.cnt_wc"
   {<point name="browser_fill.cnt_wc"/>}
   #end add-point
              
   PREPARE master_cnt FROM g_sql
   DECLARE master_cntcur CURSOR FOR master_cnt
   OPEN master_cntcur
   IF SQLCA.SQLCODE THEN   #(ver:36)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN master_cntcur:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE   #(ver:36)
      LET g_errparam.popup = TRUE 
      CALL cl_err()
      RETURN
   END IF
 
   FETCH master_cntcur INTO l_cnt2
   LET g_root_search = FALSE
   
   IF l_cnt2 = 0 THEN
      #INITIALIZE g_errparam TO NULL 
      #LET g_errparam.extend = "" 
      #LET g_errparam.code = "-100" 
      #LET g_errparam.popup = TRUE 
      #CALL cl_err()
      RETURN
   END IF
   
   IF l_cnt = l_cnt2 THEN
      #未輸入條件時則只查找root節點
      LET p_wc = " gzth001 = gzth002 "
      LET g_root_search = TRUE
   END IF
 
   #取得符合p_wc的所有資料
   LET g_sql = "SELECT gzth005,gzthstus,gzth004,'',gzth001,'','','',gzth002 ",
               " FROM gzth_t",
               "  LEFT JOIN gzthl_t ON gzth001 = gzthl001 AND gzth002 = gzthl002 AND gzthl003 = '",g_dlang,"' ",
               " WHERE  ",p_wc ,cl_sql_add_filter("gzth_t")
   #add-point:browser_fill段sql wc name="browser_fill.sql_wc"
   {<point name="browser_fill.sql_wc"/>}
   #end add-point
   #LET g_sql = cl_sql_add_tabid(g_sql,"gzth_t")             #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料           
   PREPARE master_ext FROM g_sql
   DECLARE master_extcur CURSOR FOR master_ext
   
   #單筆update
   #LET g_sql = "SELECT '','','','','','','',gzth005,gzthstus,gzth004,'',gzth001,'','','',gzth002 ",
   #            " FROM gzth_t",
   #            "  LEFT JOIN gzthl_t ON gzth001 = gzthl001 AND gzth002 = gzthl002 AND gzthl003 = '",g_dlang,"' ",
   #            " WHERE  ",
   #            " gzth001 = ?"
   #            ," gzth002 = ?"
 
                
   LET g_sql = " SELECT t0.gzth005,t0.gzthstus,t0.gzth004,t0.gzth001,t0.gzth002,t1.gzthl004 ,t2.gztel003 , 
       t3.gztel003 ",
               " FROM gzth_t t0",
               "  ",
                              " LEFT JOIN gzthl_t t1 ON t1.gzthl001=gzth001 AND t1.gzthl002=gzth002 AND t1.gzthl003='"||g_lang||"' ",
               " LEFT JOIN gztel_t t2 ON t2.gztel001=gzth001 AND t2.gztel002='"||g_lang||"' ",
               " LEFT JOIN gztel_t t3 ON t3.gztel001=gzth001 AND t3.gztel002='"||g_lang||"' ",
 
               " WHERE  ",
               " gzth001 = ?"
               ," AND gzth002 = ?"
 
 
               
   #add-point:browser_fill段sql wc name="browser_fill.refresh"
   {<point name="browser_fill.refresh"/>}
   #end add-point
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料           
   PREPARE master_refresh FROM g_sql
   DECLARE master_refreshcur CURSOR FOR master_refresh
 
   #搜尋建構樹所需的節點
   CASE p_type
      WHEN "1" #上推
         CALL azzi551_match_node(p_wc,p_type) 
      WHEN "2" #下展
         #CALL azzi551_find_speed_tbl(p_wc,p_type) 
         CALL azzi551_match_node(p_wc,p_type) 
      WHEN "3" #全部
         CALL azzi551_match_node(p_wc,p_type) 
   END CASE
 
   CALL azzi551_browser_create(p_type)
     
END FUNCTION
