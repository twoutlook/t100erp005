   LET g_forupd_sql = " SELECT gzth004,gzth002,gzth001,'','',gzth007,gzth003,gzth005,gzth006,gzthstus, 
       gzthownid,'',gzthowndp,'',gzthcrtid,'',gzthcrtdp,'',gzthcrtdt,gzthmodid,'',gzthmoddt", 
                      " FROM gzth_t",
                      " WHERE gzth001=? AND gzth002=? FOR UPDATE"
