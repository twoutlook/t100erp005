{<section id="azzi551.fetch" >}
#+ 指定PK後抓取單頭其他資料
PRIVATE FUNCTION azzi551_fetch(p_flag)
   #add-point:fetch段define name="fetch.define_customerization"
   {<point name="fetch.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE p_flag     LIKE type_t.chr1
   DEFINE ls_msg     STRING
   DEFINE ls_chk     STRING
   DEFINE li_idx     LIKE type_t.num10
   #add-point:fetch段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="fetch.define"
   {<point name="fetch.define"/>}
   #end add-point
   
   #add-point:Function前置處理  name="fetch.befroe_fetch"
   {<point name="fetch.befroe_fetch"/>}
   #end add-point 
 
 
   #瀏覽頁筆數顯示
   LET li_idx = 1
   FOR li_idx = 1 TO g_browser_root.getLength()
      IF g_browser_root[li_idx] > g_current_idx THEN
       EXIT FOR
      END IF
   END FOR
   LET li_idx = g_current_idx - li_idx + 1
   #DISPLAY li_idx TO FORMONLY.h_index   #當下筆數
 
   IF p_flag = "/" THEN
      IF (NOT g_no_ask) THEN      
         CALL cl_getmsg("fetch",g_lang) RETURNING ls_msg
         LET INT_FLAG = 0
 
         PROMPT ls_msg CLIPPED,": " FOR g_jump
            #交談指令共用ACTION
            &include "common_action.4gl" 
         END PROMPT
 
         IF INT_FLAG THEN
            LET INT_FLAG = 0
         ELSE
            IF g_jump > 0 AND g_jump <= g_browser.getLength() THEN
               LET g_current_idx = g_jump
            END IF
            LET g_no_ask = FALSE  
         END IF           
      END IF
   END IF    
   
   #若無資料則離開
   IF g_current_idx = 0 THEN
      RETURN
   END IF
 
   LET g_gzth_m.gzth001 = g_browser[g_current_idx].b_gzth001
   LET g_gzth_m.gzth002 = g_browser[g_current_idx].b_gzth002
    
   #add-point:fetch段refresh前 name="fetch.before_refresh"
   {<point name="fetch.before_refresh"/>}
   #end add-point
    
   #重讀DB,因TEMP有不被更新特性
   EXECUTE azzi551_master_referesh USING g_gzth_m.gzth001,g_gzth_m.gzth002 INTO g_gzth_m.gzth004,g_gzth_m.gzth002, 
       g_gzth_m.gzth001,g_gzth_m.gzth007,g_gzth_m.gzth003,g_gzth_m.gzth005,g_gzth_m.gzth006,g_gzth_m.gzthstus, 
       g_gzth_m.gzthownid,g_gzth_m.gzthowndp,g_gzth_m.gzthcrtid,g_gzth_m.gzthcrtdp,g_gzth_m.gzthcrtdt, 
       g_gzth_m.gzthmodid,g_gzth_m.gzthmoddt,g_gzth_m.gzth001_desc,g_gzth_m.gzthownid_desc,g_gzth_m.gzthowndp_desc, 
       g_gzth_m.gzthcrtid_desc,g_gzth_m.gzthcrtdp_desc,g_gzth_m.gzthmodid_desc 
   
   IF SQLCA.SQLCODE THEN
       INITIALIZE g_errparam TO NULL 
       LET g_errparam.extend = "gzth_t:",SQLERRMESSAGE 
       LET g_errparam.code = SQLCA.SQLCODE 
       LET g_errparam.popup = TRUE 
       CALL cl_err()
       INITIALIZE g_gzth_m.* TO NULL
       RETURN
   END IF
   
   #若無資料則關閉相關功能
   IF g_browser.getLength() = 0 THEN
      CALL cl_set_act_visible("statechange,modify,delete,reproduce", FALSE)
   ELSE
      CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   END IF
   
   #add-point:fetch段after_fetch name="fetch.after_fetch"
   {<point name="fetch.after_fetch"/>}
   #end add-point
 
   
   
   #保存單頭舊值
   LET g_gzth_m_t.* = g_gzth_m.*
   LET g_gzth_m_o.* = g_gzth_m.*
   
   #重新顯示
   CALL azzi551_show()
   CALL cl_navigator_setting(g_current_idx, g_browser_cnt)
   DISPLAY g_browser.getLength() TO FORMONLY.h_count 
 
   
   
END FUNCTION
{</section>}
 
{<section id="azzi551.insert" >}
#+ 資料新增
PRIVATE FUNCTION azzi551_insert(l_dialog)
   #add-point:insert段define name="insert.define_customerization"
   {<point name="insert.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   DEFINE l_dialog   ui.DIALOG
   DEFINE li_addpos  LIKE type_t.num10 #新增節點時產出的畫面實際位置
   DEFINE li_idx     LIKE type_t.num10
   DEFINE li_cnt     LIKE type_t.num10
   #add-point:insert段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="insert.define"
   {<point name="insert.define"/>}
   #end add-point
   
   #add-point:Function前置處理  name="insert.pre_function"
   {<point name="insert.pre_function"/>}
   #end add-point
   
   LET g_gzth001_t = g_gzth_m.gzth001
   LET g_gzth002_t = g_gzth_m.gzth002
 
   LET g_gzth001_t = g_gzth_m.gzth001
 
   #清畫面欄位內容
   CLEAR g_gzth_m.*
 
 
   #add-point:單頭預設值 name="insert.before_insert"
   {<point name="insert.before_insert"/>}
   #end add-point 
 
   INITIALIZE g_gzth_m.* LIKE gzth_t.*
   DISPLAY BY NAME g_gzth_m.gzth004,g_gzth_m.gzth002,g_gzth_m.gzth001,g_gzth_m.gzth001_desc,g_gzth_m.gzthl004, 
       g_gzth_m.gzth007,g_gzth_m.gzth003,g_gzth_m.gzth005,g_gzth_m.gzth006,g_gzth_m.gzthstus,g_gzth_m.gzthownid, 
       g_gzth_m.gzthownid_desc,g_gzth_m.gzthowndp,g_gzth_m.gzthowndp_desc,g_gzth_m.gzthcrtid,g_gzth_m.gzthcrtid_desc, 
       g_gzth_m.gzthcrtdp,g_gzth_m.gzthcrtdp_desc,g_gzth_m.gzthcrtdt,g_gzth_m.gzthmodid,g_gzth_m.gzthmodid_desc, 
       g_gzth_m.gzthmoddt
   CALL s_transaction_begin()
 
   #其他table資料備份(確定是否更改用)
   LET g_master_multi_table_t.gzthl001 = g_gzth_m.gzth001
LET g_master_multi_table_t.gzthl002 = g_gzth_m.gzth002
LET g_master_multi_table_t.gzthl004 = g_gzth_m.gzthl004
 
 
   WHILE TRUE
      #給予pid,type預設值
      LET g_gzth_m.gzth002 = g_gzth001_t
      #應用 a14 樣板自動產生(Version:5)    
      #公用欄位新增給值  
      LET g_gzth_m.gzthownid = g_user
      LET g_gzth_m.gzthowndp = g_dept
      LET g_gzth_m.gzthcrtid = g_user
      LET g_gzth_m.gzthcrtdp = g_dept 
      LET g_gzth_m.gzthcrtdt = cl_get_current()
      LET g_gzth_m.gzthmodid = g_user
      LET g_gzth_m.gzthmoddt = cl_get_current()
      LET g_gzth_m.gzthstus = 'Y'
 
 
 
      
      CALL gfrm_curr.setElementImage("statechange", "authstatus/valid.png")
  
      
      
      #單頭預設值
            LET g_gzth_m.gzth005 = "0"
 
     
      #add-point:單頭預設值 name="insert.default"
      {<point name="insert.default"/>}
      #end add-point 
 
      CALL azzi551_input("a")
      
      #add-point:單頭輸入後 name="insert.after_insert"
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_gzth_m.* = g_gzth_m_t.*
         CALL azzi551_show()
         INITIALIZE g_errparam TO NULL
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code = 9001 
         LET g_errparam.popup = FALSE 
         CALL cl_err()
         EXIT WHILE
      ELSE
         #分兩種狀況-1.根節點, 2.非根節點
         IF g_gzth_m.gzth002 = g_gzth_m.gzth001 THEN
            #為根節點
            LET li_addpos = g_browser.getLength() + 1
            LET g_browser[li_addpos].b_gzth002 = g_gzth_m.gzth002
            LET g_browser[li_addpos].b_gzth001 = g_gzth_m.gzth001
            LET g_browser[li_addpos].b_exp  = FALSE
            LET g_browser[li_addpos].b_hasC = FALSE
            LET g_browser[li_addpos].b_id  = '0.add',g_add_idx USING "<<<"
            LET g_browser[li_addpos].b_pid = '0',g_add_idx USING "<<<"
            LET g_add_idx = g_add_idx + 1
            CALL azzi551_desc_show(li_addpos)
            LET g_current_idx = li_addpos
         ELSE
            #非根節點, 開始搜尋其父節點(已展開才做處理)
            LET li_cnt = g_cnt
            IF g_browser.getLength() > 0 THEN
               FOR li_idx = 1 TO g_browser.getLength()
                  IF g_browser[li_idx].b_gzth001 = g_gzth_m.gzth002 THEN
                     LET li_addpos = l_dialog.appendNode("s_browse",li_idx)
                     LET g_cnt = li_addpos
                     LET g_browser[li_addpos].b_gzth002 = g_gzth_m.gzth002
                     LET g_browser[li_addpos].b_gzth001 = g_gzth_m.gzth001
                     EXECUTE master_refreshcur USING g_browser[li_addpos].b_gzth001
                                                     ,g_browser[li_addpos].b_gzth002
 
                                                INTO g_browser[g_cnt].b_gzth005,g_browser[g_cnt].b_gzthstus, 
                                                    g_browser[g_cnt].b_gzth004,g_browser[g_cnt].b_gzth001, 
                                                    g_browser[g_cnt].b_gzth002,g_browser[g_cnt].b_gzth001_desc, 
                                                    g_browser[g_cnt].b_gzth001_desc_3,g_browser[g_cnt].b_gzth001_desc_4 

                     LET g_browser[li_addpos].b_exp  = FALSE
                     LET g_browser[li_addpos].b_hasC = FALSE
                     LET g_browser[li_addpos].b_id  = g_browser[li_idx].b_id, '.add',g_add_idx USING "<<<"
                     LET g_browser[li_addpos].b_pid = g_browser[li_idx].b_id
                     LET g_add_idx = g_add_idx + 1
                     CALL azzi551_desc_show(li_addpos)
                     #打開父節點
                     LET g_browser[li_idx].b_hasC = TRUE
                     LET g_browser[li_idx].b_exp = TRUE
                     LET g_current_idx = li_addpos
                     EXIT FOR
                  END IF
               END FOR
            END IF
            LET g_cnt = li_cnt
         END IF
      END IF
      
      LET g_rec_b = 0
      EXIT WHILE
   END WHILE
   
   #撈取異動後的資料(主要是帶出reference)
