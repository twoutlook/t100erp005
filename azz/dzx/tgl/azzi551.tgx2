{<section id="azzi551.state_change" >}
   #應用 a09 樣板自動產生(Version:17)
#+ 確認碼變更 
PRIVATE FUNCTION azzi551_statechange()
   #add-point:statechange段define(客製用) name="statechange.define_customerization"
   {<point name="statechange.define_customerization" edit="c" mark="Y"/>}
   #end add-point  
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="statechange.define"
   {<point name="statechange.define"/>}
   #end add-point  
   
   #add-point:Function前置處理 name="statechange.before"
   {<point name="statechange.before"/>}
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_gzth_m.gzth001 IS NULL
      OR g_gzth_m.gzth002 IS NULL
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN azzi551_cl USING g_gzth_m.gzth001,g_gzth_m.gzth002
   IF STATUS THEN
      CLOSE azzi551_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN azzi551_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE azzi551_master_referesh USING g_gzth_m.gzth001,g_gzth_m.gzth002 INTO g_gzth_m.gzth004,g_gzth_m.gzth002, 
       g_gzth_m.gzth001,g_gzth_m.gzth007,g_gzth_m.gzth003,g_gzth_m.gzth005,g_gzth_m.gzth006,g_gzth_m.gzthstus, 
       g_gzth_m.gzthownid,g_gzth_m.gzthowndp,g_gzth_m.gzthcrtid,g_gzth_m.gzthcrtdp,g_gzth_m.gzthcrtdt, 
       g_gzth_m.gzthmodid,g_gzth_m.gzthmoddt,g_gzth_m.gzth001_desc,g_gzth_m.gzthownid_desc,g_gzth_m.gzthowndp_desc, 
       g_gzth_m.gzthcrtid_desc,g_gzth_m.gzthcrtdp_desc,g_gzth_m.gzthmodid_desc
   
 
   #檢查是否允許此動作
   IF NOT azzi551_action_chk() THEN
      CLOSE azzi551_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_gzth_m.gzth004,g_gzth_m.gzth002,g_gzth_m.gzth001,g_gzth_m.gzth001_desc,g_gzth_m.gzthl004, 
       g_gzth_m.gzth007,g_gzth_m.gzth003,g_gzth_m.gzth005,g_gzth_m.gzth006,g_gzth_m.gzthstus,g_gzth_m.gzthownid, 
       g_gzth_m.gzthownid_desc,g_gzth_m.gzthowndp,g_gzth_m.gzthowndp_desc,g_gzth_m.gzthcrtid,g_gzth_m.gzthcrtid_desc, 
       g_gzth_m.gzthcrtdp,g_gzth_m.gzthcrtdp_desc,g_gzth_m.gzthcrtdt,g_gzth_m.gzthmodid,g_gzth_m.gzthmodid_desc, 
       g_gzth_m.gzthmoddt
 
   CASE g_gzth_m.gzthstus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   {<point name="statechange.after_refresh"/>}
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_gzth_m.gzthstus
            
            WHEN "N"
               HIDE OPTION "inactive"
            WHEN "Y"
               HIDE OPTION "active"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      {<point name="statechange.before_menu"/>}
      #end add-point
      
      
	  
      ON ACTION inactive
         IF cl_auth_chk_act("inactive") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.inactive"
            {<point name="statechange.inactive"/>}
            #end add-point
         END IF
         EXIT MENU
      ON ACTION active
         IF cl_auth_chk_act("active") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.active"
            {<point name="statechange.active"/>}
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      {<point name="statechange.more_control"/>}
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      ) OR 
      g_gzth_m.gzthstus = lc_state OR cl_null(lc_state) THEN
      CLOSE azzi551_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   {<point name="statechange.b_update"/>}
   #end add-point
   
   LET g_gzth_m.gzthmodid = g_user
   LET g_gzth_m.gzthmoddt = cl_get_current()
   LET g_gzth_m.gzthstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE gzth_t 
      SET (gzthstus,gzthmodid,gzthmoddt) 
        = (g_gzth_m.gzthstus,g_gzth_m.gzthmodid,g_gzth_m.gzthmoddt)     
    WHERE  gzth001 = g_gzth_m.gzth001
      AND gzth002 = g_gzth_m.gzth002
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE azzi551_master_referesh USING g_gzth_m.gzth001,g_gzth_m.gzth002 INTO g_gzth_m.gzth004, 
          g_gzth_m.gzth002,g_gzth_m.gzth001,g_gzth_m.gzth007,g_gzth_m.gzth003,g_gzth_m.gzth005,g_gzth_m.gzth006, 
          g_gzth_m.gzthstus,g_gzth_m.gzthownid,g_gzth_m.gzthowndp,g_gzth_m.gzthcrtid,g_gzth_m.gzthcrtdp, 
          g_gzth_m.gzthcrtdt,g_gzth_m.gzthmodid,g_gzth_m.gzthmoddt,g_gzth_m.gzth001_desc,g_gzth_m.gzthownid_desc, 
          g_gzth_m.gzthowndp_desc,g_gzth_m.gzthcrtid_desc,g_gzth_m.gzthcrtdp_desc,g_gzth_m.gzthmodid_desc 

      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_gzth_m.gzth004,g_gzth_m.gzth002,g_gzth_m.gzth001,g_gzth_m.gzth001_desc,g_gzth_m.gzthl004, 
          g_gzth_m.gzth007,g_gzth_m.gzth003,g_gzth_m.gzth005,g_gzth_m.gzth006,g_gzth_m.gzthstus,g_gzth_m.gzthownid, 
