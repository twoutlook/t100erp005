      END IF
   
   END IF
                                    
 
   
   #鎖定整組table
   #LET ls_group = "gzsy_t,"
   #僅鎖定自身table
   LET ls_group = "gzsy_t"
   IF ls_group.getIndexOf(ps_table,1) THEN
      OPEN azzi991_bcl2 USING 
                                             g_master.gzsz001,g_master.gzsz002,
                                             g_gzsz3_d[g_detail_idx2].gzsy004
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "azzi991_bcl2:",SQLERRMESSAGE 
         LET g_errparam.code = SQLCA.SQLCODE 
         LET g_errparam.popup = TRUE 
         CALL cl_err()
         RETURN FALSE
      END IF
   END IF
 
 
   
   RETURN TRUE
 
END FUNCTION

