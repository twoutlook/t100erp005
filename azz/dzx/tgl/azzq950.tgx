#(ver:42) mark --- end ---

   LET l_ac = 1
   FOREACH b_fill_curs2 USING g_enterprise,g_gzpc_d[g_detail_idx].gzpc000   #(ver:42)

        INTO g_gzpc2_d[l_ac].gzpd003,g_gzpc2_d[l_ac].gzpd011,g_gzpc2_d[l_ac].gzpd004,g_gzpc2_d[l_ac].gzpd004_desc,g_gzpc2_d[l_ac].gzpd007,g_gzpc2_d[l_ac].gzpd008,g_gzpc2_d[l_ac].gzpd010,g_gzpc2_d[l_ac].gzpd005,g_gzpc2_d[l_ac].gzpd009,g_gzpc2_d[l_ac].gzpd009_desc,g_gzpc2_d[l_ac].gzpd006,g_gzpc2_d[l_ac].gzpd012,g_gzpc2_d[l_ac].gzpd013,g_gzpc2_d[l_ac].gzpd014   #(ver:42)
      IF SQLCA.SQLCODE THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code = SQLCA.SQLCODE 
         LET g_errparam.popup = TRUE 
