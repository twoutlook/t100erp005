      #end add-point
      
      RETURN
   END IF
   
 
   
   LET ls_group = "gztf_t,"
   #判斷是否是同一群組的table
   IF ls_group.getIndexOf(ps_table,1) > 0 AND ps_table <> "gztf_t" THEN
   
