   #add-point:SQL_define name="main.define_sql"
   {<point name="main.define_sql" />}
   #end add-point
   LET g_forupd_sql = " SELECT gzpa001,gzpa004,gzpa002,gzpa005,gzpa003,gzpastus,gzpa006,gzpa007,gzpa008, 
       '','','','','','','','','','','','',gzpa010,'','','','','',gzpa012,'','','','','','','','','', 
