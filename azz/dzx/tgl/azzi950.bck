                  {<point name="browser_fill.join.gzpe_t1"/>}
                  #end add-point
 
 
                  " ", 
 
                  " ",
 
 
                  
                  " WHERE t0.gzpaent = " ||g_enterprise|| " AND ",l_wc," AND ",l_wc2, cl_sql_add_filter("gzpa_t")
   ELSE
