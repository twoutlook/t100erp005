               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=t0.gzpamodid  ",
 
               " WHERE t0.gzpaent = " ||g_enterprise|| " AND t0.gzpa001 = ?"
