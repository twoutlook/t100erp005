   DECLARE azzi950_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT DISTINCT t0.gzpa001,t0.gzpa004,t0.gzpa002,t0.gzpa005,t0.gzpa003,t0.gzpastus, 
       t0.gzpa006,t0.gzpa007,t0.gzpa008,t0.gzpa010,t0.gzpa012,t0.gzpa015,t0.gzpa016,t0.gzpa017,t0.gzpa018, 
       t0.gzpa019,t0.gzpa020,t0.gzpa021,t0.gzpa022,t0.gzpa023,t0.gzpa024,t0.gzpa025,t0.gzpa026,t0.gzpa027, 
