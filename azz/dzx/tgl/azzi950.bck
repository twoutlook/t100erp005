       gzpa027,gzpa028,gzpa029,gzpa030,gzpa032,gzpa031,gzpa033,gzpaownid,'',gzpaowndp,'',gzpacrtid,'', 
       gzpacrtdp,'',gzpacrtdt,gzpamodid,'',gzpamoddt", 
                      " FROM gzpa_t",
                      " WHERE gzpaent= ? AND gzpa001=? FOR UPDATE"
   #add-point:SQL_define name="main.after_define_sql"
   {<point name="main.after_define_sql"/>}
   #end add-point
