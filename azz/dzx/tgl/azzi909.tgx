{<section id="azzi909.modify" >}
#+ 資料修改
PRIVATE FUNCTION azzi909_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   {<point name="modify.define_customerization" edit="c" mark="Y"/>}
   #end add-point
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   {<point name="modify.define"/>}
   #end add-point
   
   #add-point:Function前置處理 name="modify.pre_function"
   {<point name="modify.pre_function"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_gzza_m.gzza001 IS NULL

   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_gzza001_t = g_gzza_m.gzza001

   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN azzi909_cl USING g_gzza_m.gzza001
   IF SQLCA.SQLCODE THEN    #(ver:49)
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN azzi909_cl:",SQLERRMESSAGE 
      LET g_errparam.code = SQLCA.SQLCODE
      LET g_errparam.popup = TRUE 
      CLOSE azzi909_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE azzi909_master_referesh USING g_gzza_m.gzza001 INTO g_gzza_m.gzza001,g_gzza_m.gzza019,g_gzza_m.gzza020,g_gzza_m.gzzaownid,g_gzza_m.gzzaowndp,g_gzza_m.gzzacrtid,g_gzza_m.gzzacrtdp,g_gzza_m.gzzacrtdt,g_gzza_m.gzzamodid,g_gzza_m.gzzamoddt,g_gzza_m.gzza001_desc,g_gzza_m.gzza019_desc,g_gzza_m.gzzaownid_desc,g_gzza_m.gzzaowndp_desc,g_gzza_m.gzzacrtid_desc,g_gzza_m.gzzacrtdp_desc,g_gzza_m.gzzamodid_desc
 
   #檢查是否允許此動作
   IF NOT azzi909_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_gzza_m_mask_o.* =  g_gzza_m.*
   CALL azzi909_gzza_t_mask()
   LET g_gzza_m_mask_n.* =  g_gzza_m.*
   
   
 
   #顯示資料
   CALL azzi909_show()
   
   WHILE TRUE
      LET g_gzza_m.gzza001 = g_gzza001_t

      
      #寫入修改者/修改日期資訊
      LET g_gzza_m.gzzamodid = g_user 
LET g_gzza_m.gzzamoddt = cl_get_current()
LET g_gzza_m.gzzamodid_desc = cl_get_username(g_gzza_m.gzzamodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      {<point name="modify.before_input"/>}
      #end add-point
 
      #資料輸入
      CALL azzi909_input("u")     
 
      #add-point:modify段修改後 name="modify.after_input"
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         LET g_gzza_m.* = g_gzza_m_t.*
         CALL azzi909_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE gzza_t SET (gzzamodid,gzzamoddt) = (g_gzza_m.gzzamodid,g_gzza_m.gzzamoddt)
       WHERE  gzza001 = g_gzza001_t

 
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL azzi909_set_act_visible()
   CALL azzi909_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " ",
                      " gzza001 = '", g_gzza_m.gzza001, "' "

   #填到對應位置
   CALL azzi909_browser_fill(g_wc,"")
 
   CLOSE azzi909_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL azzi909_msgcentre_notify('modify')
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
