  <section id="azzi930.modify" ver="1" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION azzi930_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_gzwg_m.gzwg001 IS NULL
   OR g_gzwg_m.gzwg002 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   ERROR ""
  
   LET g_gzwg001_t = g_gzwg_m.gzwg001
   LET g_gzwg002_t = g_gzwg_m.gzwg002
 
   CALL s_transaction_begin()
   
   OPEN azzi930_cl USING g_enterprise,g_gzwg_m.gzwg001,g_gzwg_m.gzwg002
   IF STATUS THEN
      CALL cl_err("OPEN azzi930_cl:", STATUS, 1)
      CLOSE azzi930_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE azzi930_master_referesh USING g_gzwg_m.gzwg001,g_gzwg_m.gzwg002 INTO g_gzwg_m.gzwg001,g_gzwg_m.gzwg002, 
       g_gzwg_m.gzwgstus,g_gzwg_m.gzwgownid,g_gzwg_m.gzwgowndp,g_gzwg_m.gzwgcrtid,g_gzwg_m.gzwgcrtdp, 
       g_gzwg_m.gzwgcrtdt,g_gzwg_m.gzwgmodid,g_gzwg_m.gzwgmoddt,g_gzwg_m.gzwg001_desc,g_gzwg_m.gzwg002_desc, 
       g_gzwg_m.gzwgownid_desc,g_gzwg_m.gzwgowndp_desc,g_gzwg_m.gzwgcrtid_desc,g_gzwg_m.gzwgcrtdp_desc, 
       g_gzwg_m.gzwgmodid_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_gzwg_m.gzwg001,SQLCA.sqlcode,0)
      CLOSE azzi930_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   CALL azzi930_show()
   WHILE TRUE
      LET g_gzwg001_t = g_gzwg_m.gzwg001
      LET g_gzwg002_t = g_gzwg_m.gzwg002
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_gzwg_m.gzwgmodid = g_user 
LET g_gzwg_m.gzwgmoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      CALL azzi930_input("u")
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_gzwg_m.* = g_gzwg_m_t.*
         CALL azzi930_show()
         CALL cl_err('',9001,0)
         CALL s_transaction_end('N','0')
         EXIT WHILE
      END IF 
      
      #若有modid跟moddt則進行update
      UPDATE gzwg_t SET (gzwgmodid,gzwgmoddt) = (g_gzwg_m.gzwgmodid,g_gzwg_m.gzwgmoddt)
       WHERE gzwgent = g_enterprise AND gzwg001 = g_gzwg001_t
         AND gzwg002 = g_gzwg002_t
 
                  
      #若單頭key欄位有變更
      IF g_gzwg_m.gzwg001 != g_gzwg001_t 
      OR g_gzwg_m.gzwg002 != g_gzwg002_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE gzwh_t SET gzwh001 = g_gzwg_m.gzwg001
                                       ,gzwh002 = g_gzwg_m.gzwg002
 
          WHERE gzwhent = g_enterprise AND gzwh001 = g_gzwg001_t
            AND gzwh002 = g_gzwg002_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("gzwh_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("gzwh_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_gzwg_m.gzwg001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE azzi930_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_gzwg_m.gzwg001,'U')
 
END FUNCTION   
]]>
