          , gzwh002 = g_gzwg_m.gzwg002
 
      #更新共用欄位
      
                                       
  
   #將資料塞回原table   
   INSERT INTO gzwh_t SELECT * FROM azzi930_detail
   
   IF SQLCA.sqlcode THEN
      CALL cl_err("reproduce",SQLCA.sqlcode,1)
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
   DROP TABLE azzi930_detail
   
