  <section id="azzi930.fetch" ver="1" status="" src="s">
    <![CDATA[#+ 指定PK後抓取單頭其他資料
PRIVATE FUNCTION azzi930_fetch(p_flag)
   DEFINE p_flag     LIKE type_t.chr1
   DEFINE ls_msg     STRING
   #add-point:fetch段define
   {<point name="fetch.define"/>}
   #end add-point    
   
   IF g_browser_cnt = 0 THEN
      RETURN
   END IF
   
   CASE p_flag
      WHEN 'F' LET g_current_idx = 1
      WHEN 'L' LET g_current_idx = g_header_cnt        
      WHEN 'P'
         IF g_current_idx > 1 THEN               
            LET g_current_idx = g_current_idx - 1
         END IF 
      WHEN 'N'
         IF g_current_idx < g_header_cnt THEN
            LET g_current_idx =  g_current_idx + 1
         END IF        
      WHEN '/'
         IF (NOT g_no_ask) THEN    
            CALL cl_set_act_visible("accept,cancel", TRUE)    
            CALL cl_getmsg('fetch',g_lang) RETURNING ls_msg
            LET INT_FLAG = 0
 
            PROMPT ls_msg CLIPPED,':' FOR g_jump
               #交談指令共用ACTION
               &include "common_action.4gl" 
            END PROMPT
 
            CALL cl_set_act_visible("accept,cancel", FALSE)    
            IF INT_FLAG THEN
                LET INT_FLAG = 0
                EXIT CASE  
            END IF           
         END IF
         
         IF g_jump > 0 AND g_jump <= g_browser.getLength() THEN
             LET g_current_idx = g_jump
         END IF
         
         LET g_no_ask = FALSE  
   END CASE 
   
   
   CALL g_curr_diag.setCurrentRow("s_browse", g_current_idx) #設定browse 索引
   
   LET g_detail_cnt = g_header_cnt                  
   
   #單身總筆數顯示
   #LET g_detail_idx = 1
   IF g_detail_cnt > 0 THEN
      #LET g_detail_idx = 1
      DISPLAY g_detail_idx TO FORMONLY.idx  
   ELSE
      LET g_detail_idx = 0
      DISPLAY ' ' TO FORMONLY.idx    
   END IF
   
   #瀏覽頁筆數顯示
   LET g_browser_idx = g_pagestart+g_current_idx-1
   DISPLAY g_browser_idx TO FORMONLY.b_index   #當下筆數
   DISPLAY g_browser_idx TO FORMONLY.h_index   #當下筆數
   
   CALL cl_navigator_setting( g_current_idx, g_browser_cnt )
   
   #代表沒有資料
   IF g_current_idx = 0 OR g_browser.getLength() = 0 THEN
      RETURN
   END IF
   
   #超出範圍
   IF g_current_idx > g_browser.getLength() THEN
      LET g_current_idx = g_browser.getLength()
   END IF
   
   LET g_gzwg_m.gzwg001 = g_browser[g_current_idx].b_gzwg001
   LET g_gzwg_m.gzwg002 = g_browser[g_current_idx].b_gzwg002
 
   
   #重讀DB,因TEMP有不被更新特性
   EXECUTE azzi930_master_referesh USING g_gzwg_m.gzwg001,g_gzwg_m.gzwg002 INTO g_gzwg_m.gzwg001,g_gzwg_m.gzwg002, 
       g_gzwg_m.gzwgstus,g_gzwg_m.gzwgownid,g_gzwg_m.gzwgowndp,g_gzwg_m.gzwgcrtid,g_gzwg_m.gzwgcrtdp, 
       g_gzwg_m.gzwgcrtdt,g_gzwg_m.gzwgmodid,g_gzwg_m.gzwgmoddt,g_gzwg_m.gzwg001_desc,g_gzwg_m.gzwg002_desc, 
       g_gzwg_m.gzwgownid_desc,g_gzwg_m.gzwgowndp_desc,g_gzwg_m.gzwgcrtid_desc,g_gzwg_m.gzwgcrtdp_desc, 
       g_gzwg_m.gzwgmodid_desc
   IF SQLCA.sqlcode THEN
      CALL cl_err("gzwg_t",SQLCA.sqlcode,1)
      INITIALIZE g_gzwg_m.* TO NULL
      RETURN
   END IF
   
   #add-point:fetch段action控制
   {<point name="fetch.action_control"/>}
   #end add-point  
   
   
   
   #add-point:fetch結束前
   {<point name="fetch.after" />}
   #end add-point
   
   #保存單頭舊值
   LET g_gzwg_m_t.* = g_gzwg_m.*
   LET g_gzwg_m_o.* = g_gzwg_m.*
   
   #LET g_data_owner =       
   #LET g_data_group =   
   
   #重新顯示   
   CALL azzi930_show()
 
   
 
END FUNCTION
]]>
  </section>
