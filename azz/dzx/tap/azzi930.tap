  <section id="azzi930.b_fill" ver="1" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION azzi930_b_fill()
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_gzwh_d.clear()    #g_gzwh_d 單頭及單身 
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF azzi930_fill_chk(1) THEN
   
      LET g_sql = "SELECT  UNIQUE gzwh003 ,t1.gzzol003 FROM gzwh_t",   
                  " INNER JOIN gzwg_t ON gzwg001 = gzwh001 ",
                  " AND gzwg002 = gzwh002 ",
 
                  #"",
                  
                  "",
                                 " LEFT JOIN gzzol_t t1 ON t1.gzzol001=gzwh003 AND t1.gzzol002='"||g_lang||"' ",
 
                  " WHERE gzwhent=? AND gzwh001=? AND gzwh002=?"
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      #add-point:b_fill段sql_before
      {<point name="b_fill.body.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table1) THEN
         LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
      END IF
      
      #子單身的WC
      
      
      LET g_sql = g_sql, " ORDER BY gzwh_t.gzwh003"
      
      #add-point:單身填充控制
      {<point name="b_fill.sql"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE azzi930_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR azzi930_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_enterprise,g_gzwg_m.gzwg001,g_gzwg_m.gzwg002
                                               
      FOREACH b_fill_cs INTO g_gzwh_d[l_ac].gzwh003,g_gzwh_d[l_ac].gzwh003_desc
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               CALL cl_err( '', 9035, 1)
            END IF
            EXIT FOREACH
         END IF
         
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
 
   
   #add-point:browser_fill段其他table處理
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_gzwh_d.deleteElement(g_gzwh_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE azzi930_pb
 
   
END FUNCTION
]]>
  </section>
