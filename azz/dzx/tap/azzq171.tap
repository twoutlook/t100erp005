  <section id="azzq171.b_fill" ver="1" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION azzq171_b_fill()
   {<Local define>}
   DEFINE ls_wc           STRING
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   IF cl_null(g_wc_filter) THEN
      LET g_wc_filter = " 1=1"
   END IF
   IF cl_null(g_wc) THEN
      LET g_wc = " 1=1"
   END IF
   IF cl_null(g_wc2) THEN
      LET g_wc2 = " 1=1"
   END IF
   
   LET ls_wc = g_wc, " AND ", g_wc2, " AND ", g_wc_filter
   
   LET g_sql = "SELECT  UNIQUE gzwa001,gzwa002,gzwa003,gzwa005,gzwa006,gzwa007 FROM gzwa_t",
 
               " LEFT JOIN gzwb_t ON gzwa001 = gzwb001 AND gzwa002 = gzwb002",
 
 
               "",
               " WHERE 1=1 AND ", ls_wc
    
   LET g_sql = g_sql, " ORDER BY gzwa_t.gzwa001,gzwa_t.gzwa002"
  
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
  
   PREPARE azzq171_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR azzq171_pb
   
   OPEN b_fill_curs
 
   CALL g_gzwa_d.clear()
   CALL g_gzwa2_d.clear()   
 
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_gzwa_d[l_ac].gzwa001,g_gzwa_d[l_ac].gzwa002,g_gzwa_d[l_ac].gzwa003,g_gzwa_d[l_ac].gzwa005, 
       g_gzwa_d[l_ac].gzwa006,g_gzwa_d[l_ac].gzwa007
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
      
      LET g_gzwa_d[l_ac].sel = "N"
      #LET g_gzwa_d[l_ac].statepic = cl_get_actipic(g_gzwa_d[l_ac].statepic)
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL azzq171_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            CALL cl_err( "", 9035, 1 )
         END IF
         EXIT FOREACH
      END IF
      
   END FOREACH
   LET g_error_show = 0
   
 
   
   CALL g_gzwa_d.deleteElement(g_gzwa_d.getLength())   
   CALL g_gzwa2_d.deleteElement(g_gzwa2_d.getLength())
 
 
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
    
   LET g_detail_cnt = l_ac - 1 
   DISPLAY g_detail_cnt TO FORMONLY.h_count
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE azzq171_pb
   
   LET l_ac = 1
   CALL azzq171_fetch()
   
      CALL azzq171_filter_show('gzwa001','b_gzwa001')
   CALL azzq171_filter_show('gzwa002','b_gzwa002')
   CALL azzq171_filter_show('gzwa003','b_gzwa003')
   CALL azzq171_filter_show('gzwa005','b_gzwa005')
   CALL azzq171_filter_show('gzwa006','b_gzwa006')
   CALL azzq171_filter_show('gzwa007','b_gzwa007')
   CALL azzq171_filter_show('gzwb006','b_gzwb006')
   CALL azzq171_filter_show('gzwb007','b_gzwb007')
   CALL azzq171_filter_show('gzwb008','b_gzwb008')
   CALL azzq171_filter_show('gzwb009','b_gzwb009')
   CALL azzq171_filter_show('gzwb010','b_gzwb010')
 
   
END FUNCTION
]]>
</section>
  <section id="azzq171.b_fill2" ver="1" status="" src="s">
<![CDATA[#+ 單身陣列填充2
PRIVATE FUNCTION azzq171_b_fill2()
   {<Local define>}
   DEFINE li_ac           LIKE type_t.num5
   {</Local define>}
   #add-point:b_fill2段define
   {<point name="b_fill2.define"/>}
   #end add-point
 
   CALL g_gzwa2_d.clear()
 
 
 
   LET li_ac = l_ac
 
   #單身組成
      #+ 此段落由子樣板qs07產生
   #+ b_fill2段table資料取得(包含sql組成及資料填充)
#table2
   LET g_sql = "SELECT  UNIQUE gzwb006,gzwb007,gzwb008,gzwb009,gzwb010 FROM gzwb_t",
               "",
               " WHERE gzwb001=? AND gzwb002=?"
 
   IF NOT cl_null(g_wc2_table2) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
   END IF
 
   LET g_sql = g_sql, " ORDER BY gzwb_t.gzwb006,gzwb_t.gzwb007"
 
   #add-point:單身填充前
   {<point name="b_fill2.before_fill2" />}
   #end add-point
 
   PREPARE azzq171_pb2 FROM g_sql
   DECLARE b_fill_curs2 CURSOR FOR azzq171_pb2
 
   OPEN b_fill_curs2 USING g_gzwa_d[g_detail_idx].gzwa001
                                  ,g_gzwa_d[g_detail_idx].gzwa002
 
 
   LET l_ac = 1
   FOREACH b_fill_curs2 INTO g_gzwa2_d[l_ac].gzwb006,g_gzwa2_d[l_ac].gzwb007,g_gzwa2_d[l_ac].gzwb008, 
       g_gzwa2_d[l_ac].gzwb009,g_gzwa2_d[l_ac].gzwb010
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
 
      
 
      #add-point:b_fill2段資料填充
      {<point name="b_fill2.fill2"/>}
      #end add-point
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
 
   END FOREACH
 
   LET li_ac = l_ac - 1
   DISPLAY li_ac TO FORMONLY.cnt
   LET g_detail_idx2 = 1
   DISPLAY g_detail_idx2 TO FORMONLY.idx
 
 
 
 
   #add-point:單身填充後
   {<point name="b_fill2.after_fill" />}
   #end add-point
 
   CALL g_gzwa2_d.deleteElement(g_gzwa2_d.getLength())
 
 
 
   LET l_ac = li_ac
 
END FUNCTION
]]>
</section>
