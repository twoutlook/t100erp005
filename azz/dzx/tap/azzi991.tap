  <point name="function.azzi991_check_gzsz002" order="4" ver="4" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#+ 檢核gzsz002
#+ D開頭-gzsz011-xxxx  (標準)
#+ D開頭-gzsz011Cxxxx  (客製)
PRIVATE FUNCTION azzi991_check_gzsz002(ps_cmd)
   DEFINE ps_cmd    LIKE type_t.chr1
   DEFINE l_dgenv   LIKE type_t.chr1     #客製標示:s-標準, c-客製
   DEFINE l_flag    LIKE type_t.chr1     #連接符
   DEFINE ls_str    STRING
   DEFINE ls_num    STRING

   LET l_dgenv = FGL_GETENV("DGENV")     #"s":標準; "c":客製
   LET ls_str = g_gzsz_d[l_ac].gzsz002

   CASE l_dgenv
      WHEN "s"
         LET l_flag = "-"   #s:標準的格式為D-XXX-9999
         IF ls_str.getIndexOf("-",3) THEN   
            IF cl_chk_num(ls_str.subString(ls_str.getIndexOf("-",3)+1,ls_str.getLength()),"N") THEN
               LET ls_num =  ls_str.subString(ls_str.getIndexOf("-",3)+1,ls_str.getLength())
            END IF
         END IF
      WHEN "c"
         LET l_flag = "C"   #c:客製的格式為D-XXXC9999
         IF ls_str.getIndexOf("C",5) THEN   
            IF cl_chk_num(ls_str.subString(ls_str.getIndexOf("C",5)+1,ls_str.getLength()),"N") THEN
               LET ls_num =  ls_str.subString(ls_str.getIndexOf("C",5)+1,ls_str.getLength())
            END IF
         END IF
   END CASE

   #客製環境應該要可以輸入符合客製命名規則的參數編號(例：D-MFGC0001)
   #DISPLAY "ls_num:",ls_num
   IF ps_cmd = 'a' AND cl_null(g_gzsz_d[l_ac].gzsz011) THEN
      LET ls_str = "D-"
   ELSE
     #LET ls_str = "D-",g_gzsz_d[l_ac].gzsz011,"-",ls_num
      LET ls_str = "D-",g_gzsz_d[l_ac].gzsz011,l_flag,ls_num
   END IF

   LET g_gzsz_d[l_ac].gzsz002 = ls_str
   
END FUNCTION]]>
  </point>
