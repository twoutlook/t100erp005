   LET g_qryparam.state = "i"
 
   LET l_allow_insert = cl_auth_detail_input("insert")
   LET l_allow_delete = cl_auth_detail_input("delete")
 
   #add-point:input段define_sql
   {<point name="input.define_sql" mark="Y"/>}
   #end add-point 
   LET g_forupd_sql = "SELECT gzszstus,gzsz001,gzsz011,gzsz002,gzsz003,gzsz004,gzsz005,gzsz009,gzsz016, 
       gzsz015,gzsz013,gzsz014,gzsz008,gzsz001,gzsz002,gzszownid,gzszowndp,gzszcrtid,gzszcrtdp,gzszcrtdt, 
       gzszmodid,gzszmoddt FROM gzsz_t WHERE gzsz001=? AND gzsz002=? FOR UPDATE"
