  <point name="function.azzi991_gzcb_b_fill" order="1" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[#+ 抓取參數值單身資料
PRIVATE FUNCTION azzi991_gzcb_b_fill()
DEFINE l_cnt     LIKE type_t.num5
DEFINE l_sql     STRING
DEFINE l_gzcb012 LIKE gzcb_t.gzcb012

   CALL g_gzcb_d.clear()
   
   IF cl_null(g_detail_idx) OR g_detail_idx <= 0 THEN
      RETURN
   END IF

   LET l_sql = "SELECT UNIQUE gzcb012,gzcb002,gzcbl004 FROM gzcb_t ",
               " LEFT OUTER JOIN gzcbl_t ON gzcbl001 = gzcb001 AND gzcbl002 = gzcb002 AND gzcbl003 = '",g_dlang,"'",
               " WHERE gzcb001 = ? "
   LET l_sql = l_sql," ORDER BY gzcb012 "            
   PREPARE azzi991_gzcb_pb FROM l_sql
   DECLARE azzi991_gzcb_cs CURSOR FOR azzi991_gzcb_pb

   OPEN azzi991_gzcb_cs USING g_gzsz_d[g_detail_idx].gzsz016
   LET l_cnt = 1
   FOREACH azzi991_gzcb_cs INTO l_gzcb012,g_gzcb_d[l_cnt].gzcb002,g_gzcb_d[l_cnt].gzcbl004
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "FOREACH gzcb:"
         LET g_errparam.popup = TRUE
         CALL cl_err()

         EXIT FOREACH
      END IF

      LET l_cnt = l_cnt + 1
      IF l_cnt > g_max_rec THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code =  9035
         LET g_errparam.extend =  ''
         LET g_errparam.popup = FALSE
         CALL cl_err()

         EXIT FOREACH
      END IF
      
   END FOREACH

   LET g_gzcb_cnt = l_cnt - 1
   CALL g_gzcb_d.deleteElement(g_gzcb_d.getLength())
   
   DISPLAY ARRAY g_gzcb_d TO s_detail4.* ATTRIBUTE(COUNT=g_gzcb_cnt)
      BEFORE DISPLAY 
         EXIT DISPLAY
   END DISPLAY
END FUNCTION
]]>
  </point>
