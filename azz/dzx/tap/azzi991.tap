   #判斷key是否有改變
   LET lb_chk = TRUE
   
   IF g_master.gzsz001 <> g_master_t.gzsz001 THEN
      LET lb_chk = FALSE
   END IF
   IF g_master.gzsz002 <> g_master_t.gzsz002 THEN
      LET lb_chk = FALSE
   END IF
 
   #不需要做處理
   IF lb_chk THEN
      RETURN
   END IF
    
   #add-point:update_b段修改前
