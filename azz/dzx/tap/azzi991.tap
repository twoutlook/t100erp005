  <section id="azzi991.b_fill" ver="16" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION azzi991_b_fill(p_wc2)
   DEFINE p_wc2           STRING
   DEFINE l_ac_t          LIKE type_t.num10
   #add-point:b_fill段define
   {<point name="b_fill.define" edit="s"/>}
   #end add-point
   #add-point:b_fill段define(客製用)
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE t0.gzszstus,t0.gzsz001,t0.gzsz011,t0.gzsz002,t0.gzsz003,t0.gzsz004,t0.gzsz005, 
       t0.gzsz009,t0.gzsz016,t0.gzsz015,t0.gzsz013,t0.gzsz014,t0.gzsz008,t0.gzsz001,t0.gzsz002,t0.gzszownid, 
       t0.gzszowndp,t0.gzszcrtid,t0.gzszcrtdp,t0.gzszcrtdt,t0.gzszmodid,t0.gzszmoddt ,t1.ooag011 ,t2.ooefl003 , 
       t3.ooag011 ,t4.ooefl003 ,t5.ooag011 FROM gzsz_t t0",
 
               " LEFT JOIN gzsy_t ON gzsz001 = gzsy001 AND gzsz002 = gzsy002",
 
 
               " LEFT JOIN gzszl_t ON gzsz001 = gzszl001 AND gzsz002 = gzszl002 AND gzszl003 = '",g_dlang,"'",
                              " LEFT JOIN ooag_t t1 ON t1.ooagent='"||g_enterprise||"' AND t1.ooag001=t0.gzszownid  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent='"||g_enterprise||"' AND t2.ooefl001=t0.gzszowndp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent='"||g_enterprise||"' AND t3.ooag001=t0.gzszcrtid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=t0.gzszcrtdp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent='"||g_enterprise||"' AND t5.ooag001=t0.gzszmodid  ",
 
               " WHERE 1=1 AND (", p_wc2, ") "
   #add-point:b_fill段sql_wc
   {<point name="b_fill.sql_wc"/>}
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("gzsz_t"),
                      " ORDER BY t0.gzsz001,t0.gzsz002"
  
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
  
   #LET g_sql = cl_sql_add_tabid(g_sql,"gzsz_t")             #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料  
   PREPARE azzi991_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR azzi991_pb
   
   OPEN b_fill_curs
 
   CALL g_gzsz_d.clear()
   CALL g_gzsz2_d.clear()   
   CALL g_gzsz3_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_gzsz_d[l_ac].gzszstus,g_gzsz_d[l_ac].gzsz001,g_gzsz_d[l_ac].gzsz011,g_gzsz_d[l_ac].gzsz002, 
       g_gzsz_d[l_ac].gzsz003,g_gzsz_d[l_ac].gzsz004,g_gzsz_d[l_ac].gzsz005,g_gzsz_d[l_ac].gzsz009,g_gzsz_d[l_ac].gzsz016, 
       g_gzsz_d[l_ac].gzsz015,g_gzsz_d[l_ac].gzsz013,g_gzsz_d[l_ac].gzsz014,g_gzsz_d[l_ac].gzsz008,g_gzsz2_d[l_ac].gzsz001, 
       g_gzsz2_d[l_ac].gzsz002,g_gzsz2_d[l_ac].gzszownid,g_gzsz2_d[l_ac].gzszowndp,g_gzsz2_d[l_ac].gzszcrtid, 
       g_gzsz2_d[l_ac].gzszcrtdp,g_gzsz2_d[l_ac].gzszcrtdt,g_gzsz2_d[l_ac].gzszmodid,g_gzsz2_d[l_ac].gzszmoddt, 
       g_gzsz2_d[l_ac].gzszownid_desc,g_gzsz2_d[l_ac].gzszowndp_desc,g_gzsz2_d[l_ac].gzszcrtid_desc, 
       g_gzsz2_d[l_ac].gzszcrtdp_desc,g_gzsz2_d[l_ac].gzszmodid_desc
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend =  "" 
            LET g_errparam.code   =  9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
      
      LET l_ac = l_ac + 1
      
   END FOREACH
   LET g_error_show = 0
   
 
   CALL g_gzsz_d.deleteElement(g_gzsz_d.getLength())   
   CALL g_gzsz2_d.deleteElement(g_gzsz2_d.getLength())
   CALL g_gzsz3_d.deleteElement(g_gzsz3_d.getLength())
 
   
   #確定指標無超過上限, 超過則指到最後一筆
   IF g_detail_idx > g_gzsz_d.getLength() THEN
      LET g_detail_idx = g_gzsz_d.getLength()
   END IF
   
   #將key欄位填到每個page
   LET l_ac_t = g_detail_idx
   FOR g_detail_idx = 1 TO g_gzsz_d.getLength()
      LET g_gzsz2_d[g_detail_idx].gzsz001 = g_gzsz_d[g_detail_idx].gzsz001 
      LET g_gzsz2_d[g_detail_idx].gzsz002 = g_gzsz_d[g_detail_idx].gzsz002 
      #LET g_gzsz3_d[g_detail_idx2].gzsy004 = g_gzsz_d[g_detail_idx].gzsz001 
 
   END FOR
   LET g_detail_idx = l_ac_t
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
    
   LET g_detail_cnt = l_ac - 1
   IF g_detail_cnt > 0 THEN
      DISPLAY g_detail_cnt TO FORMONLY.h_count
   END IF
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE azzi991_pb
   
   LET g_loc = 'm'
   CALL azzi991_detail_show() 
   
   LET l_ac = 1
   IF g_gzsz_d.getLength() > 0 THEN
      CALL azzi991_fetch()
   END IF
   
   #遮罩相關處理
   FOR l_ac = 1 TO g_gzsz_d.getLength()
      LET g_gzsz_d_mask_o[l_ac].* =  g_gzsz_d[l_ac].*
      CALL azzi991_gzsz_t_mask()
      LET g_gzsz_d_mask_n[l_ac].* =  g_gzsz_d[l_ac].*
   END FOR
   
   LET g_gzsz2_d_mask_o.* =  g_gzsz2_d.*
   FOR l_ac = 1 TO g_gzsz2_d.getLength()
      LET g_gzsz2_d_mask_o[l_ac].* =  g_gzsz2_d[l_ac].*
      CALL azzi991_gzsz_t_mask()
      LET g_gzsz2_d_mask_n[l_ac].* =  g_gzsz2_d[l_ac].*
   END FOR
   LET g_gzsz3_d_mask_o.* =  g_gzsz3_d.*
   FOR l_ac = 1 TO g_gzsz3_d.getLength()
      LET g_gzsz3_d_mask_o[l_ac].* =  g_gzsz3_d[l_ac].*
      CALL azzi991_gzsy_t_mask()
      LET g_gzsz3_d_mask_n[l_ac].* =  g_gzsz3_d[l_ac].*
   END FOR
 
   
   ERROR "" 
   
END FUNCTION
]]>
  </section>
