                       g_gzsz_d[g_detail_idx].gzsz014,g_gzsz_d[g_detail_idx].gzsz008,g_gzsz2_d[g_detail_idx].gzszownid, 
                       g_gzsz2_d[g_detail_idx].gzszowndp,g_gzsz2_d[g_detail_idx].gzszcrtid,g_gzsz2_d[g_detail_idx].gzszcrtdp, 
                       g_gzsz2_d[g_detail_idx].gzszcrtdt,g_gzsz2_d[g_detail_idx].gzszmodid,g_gzsz2_d[g_detail_idx].gzszmoddt) 

