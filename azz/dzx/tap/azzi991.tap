  <point name="function.azzi991_multi_ins" order="7" ver="4" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 開窗多選新增到單身
# Memo...........:
# Usage..........: CALL azzi991_multi_ins (p_str)
# Input parameter: p_str          多選字串
# Return code....: 無
# Date & Author..: 131204 By stellar
# Modify.........:
################################################################################
PRIVATE FUNCTION azzi991_multi_ins(p_str)
DEFINE p_str     STRING
DEFINE l_token   base.StringTokenizer
DEFINE l_gzsy    RECORD LIKE gzsy_t.*

   INITIALIZE l_gzsy.* TO NULL
   LET l_gzsy.gzsy001 = g_gzsz_d[g_detail_idx].gzsz001
   LET l_gzsy.gzsy002 = g_gzsz_d[g_detail_idx].gzsz002 
   LET l_gzsy.gzsy005 = 'N'   
   
   LET l_token = base.StringTokenizer.create(p_str, "|")

   CALL s_transaction_begin()

   WHILE l_token.hasMoreTokens()
      LET l_gzsy.gzsy004 = l_token.nextToken()
      LET l_gzsy.gzsy003 = UPSHIFT(l_gzsy.gzsy004[1,3])
      
      INSERT INTO gzsy_t (gzsy001,gzsy002,gzsy003,gzsy004,gzsy005)
             VALUES (l_gzsy.gzsy001,l_gzsy.gzsy002,l_gzsy.gzsy003,
                     l_gzsy.gzsy004,l_gzsy.gzsy005)
      IF SQLCA.sqlcode OR SQLCA.sqlerrd[3] = 0 THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = 'INSERT gzsy_t'
         LET g_errparam.popup = TRUE
         CALL cl_err()

         CALL s_transaction_end('N','0')
         EXIT WHILE
      END IF
   END WHILE

   CALL s_transaction_end('Y','0')
   
END FUNCTION]]>
  </point>
  <point name="function.azzi991_check_gzsz008" order="8" ver="4" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 檢查輸入值是否正確
# Memo...........:
# Usage..........: CALL azzi991_check_gzsz008 ()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2013/12/04 By stellar
# Modify.........:
################################################################################
PRIVATE FUNCTION azzi991_check_gzsz008()
DEFINE l_cnt     LIKE type_t.num5
DEFINE l_date    LIKE type_t.dat
DEFINE l_str     LIKE gzsz_t.gzsz015

   LET g_errno = ''
   CASE g_gzsz_d[l_ac].gzsz003
      WHEN '1'
           IF g_gzsz_d[l_ac].gzsz008 NOT MATCHES '[YN]' THEN
              LET g_errno = '-25777'
           END IF
      WHEN '4'
           IF NOT cl_null(g_gzsz_d[l_ac].gzsz016) THEN
              LET l_cnt = 0
              SELECT COUNT(*) INTO l_cnt FROM gzcb_t
               WHERE gzcb001 = g_gzsz_d[l_ac].gzsz016
                 AND gzcb002 = g_gzsz_d[l_ac].gzsz008
              IF cl_null(l_cnt) OR l_cnt = 0 THEN
                 LET g_errno = 'azz-00023'
              END IF
           END IF
      WHEN '5'
           LET l_str = cl_replace_str(g_gzsz_d[l_ac].gzsz015,'YYYY','YY')
           SELECT TO_DATE(g_gzsz_d[l_ac].gzsz008,l_str) 
             INTO l_date
             FROM dual
           IF SQLCA.sqlcode THEN
              LET g_errno = '-1218'
           ELSE
              LET g_gzsz_d[l_ac].gzsz008 = l_date USING g_gzsz_d[l_ac].gzsz015
           END IF
   END CASE
END FUNCTION]]>
  </point>
  <point name="function.azzi991_open_azzi991_01" order="9" ver="4" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 檢查是否需要執行產生單據別參數的子程式
################################################################################
PRIVATE FUNCTION azzi991_open_azzi991_01(p_type)
DEFINE p_type    LIKE type_t.chr1
DEFINE l_cnt     LIKE type_t.num5

   #檢查是否有不存在於ooac_t的參數編號
   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt FROM gzsz_t
    WHERE gzsz001 = 'ooac_t'
      AND NOT EXISTS (SELECT * FROM ooac_t WHERE ooac003 = gzsz002)
   IF NOT cl_null(l_cnt) AND l_cnt > 0 THEN
      IF p_type = '2' THEN
         IF cl_ask_confirm('azz-00130') THEN
            CALL azzi991_01()
         END IF
      ELSE
         CALL azzi991_01()
      END IF
   END IF
   
   #檢查單據性質是否有未拋轉的資料，有則自動拋轉
   CALL azzi991_auto_ins_ooac()
   
END FUNCTION]]>
  </point>
  <point name="function.azzi991_auto_ins_ooac" order="10" ver="4" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 若單據性質有未拋轉的資料時，自動拋到ooac_t
################################################################################
PRIVATE FUNCTION azzi991_auto_ins_ooac()
DEFINE l_gzsy             RECORD LIKE gzsy_t.*
DEFINE l_gzsz008          LIKE gzsz_t.gzsz008
DEFINE l_cnt             LIKE type_t.num5
DEFINE l_success         LIKE type_t.num5

   CALL s_transaction_begin()
   LET l_success = TRUE
   
   #將未拋轉的參數資料拋到ooac_t
   DECLARE sel_gzsy_cur CURSOR FOR
      SELECT gzsy_t.*,gzsz008
        FROM gzsy_t INNER JOIN gzsz_t ON gzsy001=gzsz001 AND gzsy002=gzsz002
       WHERE gzsz001 = 'ooac_t'
         AND gzszstus= 'Y'
#        AND gzsy005 = 'N'
   FOREACH sel_gzsy_cur INTO l_gzsy.*,l_gzsz008
      CALL s_azzi991_process_ooac(l_gzsy.*,l_gzsz008)
           RETURNING l_success
      IF NOT l_success THEN
         EXIT FOREACH
      END IF      
#      CALL s_azzi991_insert_ooac(l_gzsy.*,l_gzsz.gzsz008)
#           RETURNING l_success
#      IF NOT l_success THEN
#         EXIT FOREACH
#      END IF
   END FOREACH
   
   #若ooac_t的資料不存在gzsy_t，則將該資料刪除
   DELETE FROM ooac_t
    WHERE NOT EXISTS (SELECT gzsy002 FROM ooba_t,gzsy_t,oobx_t
                       WHERE gzsy004 = oobx003
                         AND gzsy001 = 'ooac_t'
                         AND gzsy002 = ooac003
                         AND oobaent = ooacent
                         AND ooba001 = ooac001
                         AND ooba002 = ooac002
                         AND oobxent = oobaent
                         AND oobx001 = ooba002)
      AND ooacent = g_enterprise
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend = "gzsz_t"
      LET g_errparam.popup = TRUE
      CALL cl_err()
      LET l_success = FALSE
   END IF
   
   IF l_success THEN
      CALL s_transaction_end('Y',0)
   ELSE
      CALL s_transaction_end('N',0)
   END IF
      
END FUNCTION]]>
  </point>
  <point name="function.azzi991_check_gzsz009" order="11" ver="4" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 檢核gzsz009值域範圍
# Memo...........:
# Usage..........: CALL azzi991_check_gzsz009()
#                  RETURNING 回传参数
# Input parameter: 
#                : 
# Return code....: 
#                : 
# Date & Author..: 2014/02/05 By stellar
# Modify.........:
################################################################################
PRIVATE FUNCTION azzi991_check_gzsz009()
DEFINE ls_gzsz009   STRING
DEFINE li_index     LIKE type_t.num5
DEFINE li_chk       LIKE type_t.num5

   LET ls_gzsz009 = g_gzsz_d[l_ac].gzsz009
   LET li_chk = FALSE
   LET g_errno = NULL
   
   #輸入資料型態是整數選項:2或範圍選項:3 必須檢核
   #可以數字區間
   #可以>= > < <=
   #離散性的資料，可以用逗號分隔，如：1,8,9,20,21,99 只有正數選項才有
   IF g_gzsz_d[l_ac].gzsz003 = '2' OR g_gzsz_d[l_ac].gzsz003 = '3' THEN
      IF cl_chk_num(ls_gzsz009.subString(1,1),'N') THEN
         IF ls_gzsz009.getIndexOf("-",1) THEN
            CASE
               WHEN ls_gzsz009.getIndexOf("-",1)
                    LET li_index = ls_gzsz009.getIndexOf("-",1)
            END CASE
            IF NOT cl_chk_num(ls_gzsz009.subString(1,li_index-1),"N") OR NOT cl_chk_num(ls_gzsz009.subString(li_index+1,ls_gzsz009.getLength()),"N") THEN
               LET g_errno = "azz-00139"            #值域範圍須整數數字 
            END IF
         ELSE
            CASE
               # 整數選項:2  可以有離散性的資料
