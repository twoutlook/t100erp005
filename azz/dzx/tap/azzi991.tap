  <section id="azzi991.fetch" ver="14" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充2
PRIVATE FUNCTION azzi991_fetch()
   DEFINE li_ac           LIKE type_t.num10
   #add-point:fetch段define
   {<point name="fetch.define" edit="s"/>}
   #end add-point
   #add-point:fetch段define(客製用)
   {<point name="fetch.define_customerization" edit="c"/>}
   #end add-point
   
   #判定單頭是否有資料
   IF g_detail_idx <= 0 OR g_gzsz_d.getLength() = 0 THEN
      RETURN
   END IF
   
   #CALL g_gzsz2_d.clear()
   CALL g_gzsz3_d.clear()
 
   
   LET li_ac = l_ac 
    
   IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
      LET g_sql = "SELECT  UNIQUE t0.gzsy003,t0.gzsy004,t0.gzsy005 ,t6.gzzol003 ,t7.gzcbl004 FROM gzsy_t t0", 
              
                  "",
                                 " LEFT JOIN gzzol_t t6 ON t6.gzzol001=t0.gzsy003 AND t6.gzzol002='"||g_lang||"' ",
               " LEFT JOIN gzcbl_t t7 ON t7.gzcbl001='24' AND t7.gzcbl002=t0.gzsy004 AND t7.gzcbl003='"||g_lang||"' ",
 
                  " WHERE t0.gzsy001=?  AND t0. gzsy002=?"
      #add-point:單身sql wc
      {<point name="fetch.sql_wc" />}
      #end add-point
      IF NOT cl_null(g_wc2_table2) THEN
         LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
      END IF
      
      LET g_sql = g_sql, " ORDER BY t0.gzsy004" 
                         
      #add-point:單身填充前
      {<point name="fetch.before_fill" />}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料    
      PREPARE azzi991_pb2 FROM g_sql
      DECLARE b_fill_curs2 CURSOR FOR azzi991_pb2
   END IF
   
   LET l_ac = g_detail_idx
   OPEN b_fill_curs2 USING g_gzsz_d[l_ac].gzsz001,g_gzsz_d[l_ac].gzsz002
   
   LET l_ac = 1
   FOREACH b_fill_curs2 INTO g_gzsz3_d[l_ac].gzsy003,g_gzsz3_d[l_ac].gzsy004,g_gzsz3_d[l_ac].gzsy005, 
       g_gzsz3_d[l_ac].gzsy003_desc,g_gzsz3_d[l_ac].gzsy004_desc
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
      
      #add-point:b_fill段資料填充
      {<point name="fetch.fill2"/>}
      #end add-point
      
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
      LET l_ac = l_ac + 1
      
   END FOREACH
 
 
 
   #add-point:單身填充後
   {<point name="fetch.after_fill" />}
   #end add-point
   
   #CALL g_gzsz2_d.deleteElement(g_gzsz2_d.getLength())   
   CALL g_gzsz3_d.deleteElement(g_gzsz3_d.getLength())   
 
   
   LET g_gzsz3_d_mask_o.* =  g_gzsz3_d.*
   FOR l_ac = 1 TO g_gzsz3_d.getLength()
      LET g_gzsz3_d_mask_o[l_ac].* =  g_gzsz3_d[l_ac].*
      CALL azzi991_gzsy_t_mask()
      LET g_gzsz3_d_mask_n[l_ac].* =  g_gzsz3_d[l_ac].*
   END FOR
 
   
   DISPLAY g_gzsz3_d.getLength() TO FORMONLY.cnt
   #LET g_loc = 'd'
   CALL azzi991_detail_show()
   
   LET l_ac = li_ac
   
END FUNCTION
]]>
