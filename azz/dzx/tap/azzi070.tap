            SELECT COUNT(*) INTO l_count FROM gzzn_t 
             WHERE  gzzn001 = g_gzzj_m.gzzj001
 
               AND gzzn002 = g_gzzn_d[l_ac].gzzn002
 
                
            #資料未重複, 插入新增資料
