                  CANCEL DELETE
               END IF
    
               #刪除下層單身
               IF NOT azzi070_key_delete_b(gs_keys,'gzzn_t') THEN
                  CALL s_transaction_end('N','0')
                  CLOSE azzi070_bcl
                  CANCEL DELETE
               END IF
               
               #刪除多語言
               
               
               #add-point:單身刪除中
               {<point name="input.body.m_delete"/>}
               #end add-point 
               
