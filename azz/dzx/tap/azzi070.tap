      CALL cl_err()
      RETURN
   END IF
    
   LET g_gzzj001_t = g_gzzj_m.gzzj001
 
    
   LET g_gzzj_m.gzzj001 = ""
 
 
   CALL cl_set_head_visible("","YES")
 
   #公用欄位給予預設值
   #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
