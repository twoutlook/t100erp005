                      " WHERE gzzz001=? FOR UPDATE"
   #add-point:SQL_define
   {<point name="main.after_define_sql"/>}
   #end add-point
   LET g_forupd_sql = cl_sql_forupd(g_forupd_sql)                #轉換不同資料庫語法
   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE azzi910_cl CURSOR FROM g_forupd_sql                 # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE gzzz001,gzzz002,gzzz003,gzzz004,gzzz005,gzzz006,gzzzstus,gzzz007,gzzz008, 
       gzzzownid,gzzzowndp,gzzzcrtid,gzzzcrtdt,gzzzcrtdp,gzzzmodid,gzzzmoddt,t1.gzzal003 ,t2.gzzk004 , 
       t3.ooag011 ,t4.ooefl003 ,t5.ooag011 ,t6.ooefl003 ,t7.ooag011",
               " FROM gzzz_t",
                              " LEFT JOIN gzzal_t t1 ON t1.gzzal001=gzzz002 AND t1.gzzal002='"||g_lang||"' ",
               " LEFT JOIN gzzk_t t2 ON t2.gzzk001=gzzz002 AND t2.gzzk002=gzzz003  ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent='"||g_enterprise||"' AND t3.ooag001=gzzzownid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=gzzzowndp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent='"||g_enterprise||"' AND t5.ooag001=gzzzcrtid  ",
               " LEFT JOIN ooefl_t t6 ON t6.ooeflent='"||g_enterprise||"' AND t6.ooefl001=gzzzcrtdp AND t6.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent='"||g_enterprise||"' AND t7.ooag001=gzzzmodid  ",
 
               " WHERE  gzzz001 = ?"
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   #add-point:SQL_define
   {<point name="main.after_refresh_sql"/>}
   #end add-point
   PREPARE azzi910_master_referesh FROM g_sql
 
 
 
   
   IF g_bgjob = "Y" THEN
      #add-point:Service Call
      {<point name="main.servicecall" />}
      #end add-point
   ELSE
      #畫面開啟 (identifier)
      OPEN WINDOW w_azzi910 WITH FORM cl_ap_formpath("azz",g_code)
   
      #瀏覽頁簽資料初始化
      CALL cl_ui_init()
   
      #程式初始化
      CALL azzi910_init()   
 
      #進入選單 Menu (="N")
      CALL azzi910_ui_dialog() 
      
      #add-point:畫面關閉前
      {<point name="main.before_close" />}
      #end add-point
 
      #畫面關閉
      CLOSE WINDOW w_azzi910
      
   END IF 
   
   CLOSE azzi910_cl
   
   
 
   #add-point:作業離開前
   {<point name="main.exit" />}
   #end add-point
 
   #離開作業
   CALL cl_ap_exitprogram("0")
   
END MAIN
 
 
]]>
</section>
  <section id="azzi910.modify" ver="9" status="" src="s">
<![CDATA[#+ 資料修改
PRIVATE FUNCTION azzi910_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_gzzz_m.gzzz001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_gzzz001_t = g_gzzz_m.gzzz001
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN azzi910_cl USING g_gzzz_m.gzzz001
   IF STATUS THEN
      CALL cl_err("OPEN azzi910_cl:", STATUS, 1)
      CLOSE azzi910_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE azzi910_master_referesh USING g_gzzz_m.gzzz001 INTO g_gzzz_m.gzzz001,g_gzzz_m.gzzz002,g_gzzz_m.gzzz003, 
       g_gzzz_m.gzzz004,g_gzzz_m.gzzz005,g_gzzz_m.gzzz006,g_gzzz_m.gzzzstus,g_gzzz_m.gzzz007,g_gzzz_m.gzzz008, 
       g_gzzz_m.gzzzownid,g_gzzz_m.gzzzowndp,g_gzzz_m.gzzzcrtid,g_gzzz_m.gzzzcrtdt,g_gzzz_m.gzzzcrtdp, 
       g_gzzz_m.gzzzmodid,g_gzzz_m.gzzzmoddt,g_gzzz_m.gzzz002_desc,g_gzzz_m.gzzz003_desc,g_gzzz_m.gzzzownid_desc, 
       g_gzzz_m.gzzzowndp_desc,g_gzzz_m.gzzzcrtid_desc,g_gzzz_m.gzzzcrtdp_desc,g_gzzz_m.gzzzmodid_desc 

 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err("gzzz_t",SQLCA.sqlcode,0)
      CLOSE azzi910_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
 
   #顯示資料
   CALL azzi910_show()
   
   WHILE TRUE
      LET g_gzzz_m.gzzz001 = g_gzzz001_t
 
      
      #寫入修改者/修改日期資訊
      LET g_gzzz_m.gzzzmodid = g_user 
LET g_gzzz_m.gzzzmoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
 
      #資料輸入
      CALL azzi910_input("u")     
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_gzzz_m.* = g_gzzz_m_t.*
         CALL azzi910_show()
         CALL cl_err("",9001,0)
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE gzzz_t SET (gzzzmodid,gzzzmoddt) = (g_gzzz_m.gzzzmodid,g_gzzz_m.gzzzmoddt)
       WHERE  gzzz001 = g_gzzz001_t
 
 
      EXIT WHILE
      
   END WHILE
   
   CLOSE azzi910_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U(暫時無用)
   #CALL cl_flow_notify(g_gzzz_m.gzzz001,"U")
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
]]>
</section>
