  <point name="function.azzi910_chk_naming_rule" cite_std="N" status="" ver="1" src="s" new="Y" order="1" mark_hard="N">
<![CDATA[#+ 檢核命名規則
#+ xxxxxxx 要符合命名規則(3碼模組名稱+1碼程式類別+3碼流水號)
#+ xxxxxxx_xx 要符合命名規則(3碼模組名稱+1碼程式類別+3碼流水號+_2碼行業別代碼)
PRIVATE FUNCTION azzi910_chk_naming_rule()
 DEFINE ls_prog STRING 
   DEFINE ls_module STRING 
   DEFINE ls_progtype STRING 
   DEFINE ls_serial_num STRING 
   DEFINE li_chk LIKE type_t.num5
   
   LET ls_prog = g_gzzz_m.gzzz002
   IF  ls_prog.getLength() != 7 THEN 
       IF ls_prog.getLength() < 7 THEN
          #程式編號要符合命名規則(3碼模組名稱+1碼程式類別+3碼流水號)
          CALL cl_err("","azz-00001",0)
          RETURN FALSE
       ELSE
          #程式編號 > 7個字元
          IF ls_prog.subString(8,8) = "_" THEN  
             #作業名稱要符合命名規則(3碼模組名稱+1碼程式類別+3碼流水號+_2碼行業別代碼)
             IF cl_chk_industry(ls_prog.subString(9,ls_prog.getLength())) THEN 
                CALL cl_err("","azz-00022",0)
                RETURN FALSE
             END IF
          ELSE 
             CALL cl_err("","azz-00001",0)
             RETURN FALSE
          END IF 
       END IF  
       
   END IF 
   
   #檢核模組名稱
   LET ls_module =  ls_prog.subString(1,3)
   IF NOT s_azzi900_chk_gzzj(UPSHIFT(ls_module)) THEN
      LET li_chk = FALSE 
      IF ls_prog.subString(1,1) = 'b' THEN 
         LET ls_module =  'a',ls_prog.subString(2,3)
         LET li_chk = s_azzi900_chk_gzzj(UPSHIFT(ls_module))  
       
      END IF 
      IF NOT li_chk THEN 
         CALL cl_err("","azz-00004",0)
          RETURN FALSE 
      END IF 
   END IF
   
   #LET ls_module =  ls_prog.subString(1,3)
   #IF cl_chk_module(UPSHIFT(ls_module)) THEN
   #   CALL cl_err("","azz-00004",0)
   #   RETURN FALSE 
   #END IF 
   LET g_gzzz_m.gzzz005 = UPSHIFT(ls_module)
   #檢核程式類別
   LET ls_progtype = ls_prog.subString(4,4)
   IF cl_chk_progtype(UPSHIFT(ls_progtype)) THEN
      CALL cl_err("","azz-00001",0)
      RETURN FALSE 
   END IF 
   #檢核程式編號後3碼流水號
   LET ls_serial_num = ls_prog.subString(5,7)  
   IF NOT ls_serial_num MATCHES '[0-9][0-9][0-9]' THEN
      CALL cl_err("","azz-00001",0)
      RETURN FALSE 
   END IF  
   RETURN TRUE
END FUNCTION]]>
</point>
  <point name="function.azzi910_combox_gzzz003" cite_std="N" status="" ver="1" src="s" new="Y" order="2" mark_hard="N">
<![CDATA[#+組應用參數組編號下拉式選單
PRIVATE FUNCTION azzi910_combox_gzzz003()
 DEFINE ls_sql STRING
   DEFINE lc_gzzk002 LIKE gzzk_t.gzzk002
   DEFINE lc_gzzk003 LIKE gzzk_t.gzzk003
   DEFINE ls_gzzk002 STRING 
   DEFINE ls_values  STRING 
   DEFINE ls_items   STRING 
   DEFINE li_cnt LIKE type_t.num5 
   DEFINE lc_gzzd005 LIKE gzzd_t.gzzd005 

   SELECT  gzzd005 INTO lc_gzzd005
      FROM  gzzd_t
      WHERE  gzzd001 = 'azzi910'
      AND  gzzd002 = g_dlang
      AND  gzzd003 = 'cbo_param.0'
      AND  gzzd004 = 's'
   
   #T100 一開始先給 0 不管有沒有值 
   LET ls_values = ls_values, "0", ',' #值
   LET ls_items = ls_items, "0", ':', lc_gzzd005 CLIPPED, ',' #items 
   
   LET ls_sql = "SELECT gzzk002,gzzk003 FROM gzzk_t ", 
                    "WHERE gzzk001= ? "
   PREPARE azzi910_combox_pre FROM ls_sql
   DECLARE azzi910_combox_cs CURSOR FOR azzi910_combox_pre
   OPEN azzi910_combox_cs USING  g_gzzz_m.gzzz002
   #T6
   FOREACH azzi910_combox_cs INTO lc_gzzk002,lc_gzzk003
      LET ls_gzzk002 = lc_gzzk002 #項目
      LET ls_values = ls_values, ls_gzzk002, ',' #值
      LET ls_items = ls_items, ls_gzzk002, ':', lc_gzzk003 CLIPPED, ',' #items
   END FOREACH

   LET ls_values = ls_values.subString(1, ls_values.getLength() - 1)
   LET ls_items = ls_items.subString(1, ls_items.getLength() - 1)

   CALL cl_set_combo_items("gzzz003", ls_values, ls_items)
   FREE azzi910_combox_pre
   CLOSE azzi910_combox_cs
END FUNCTION]]>
</point>
