  <section id="azzi085.insert" ver="18" status="" src="s" readonly="">
    <![CDATA[#+ 資料新增
PRIVATE FUNCTION azzi085_insert()
   #add-point:insert段define
   {<point name="insert.define" edit="s"/>}
   #end add-point    
   #add-point:insert段define(客製用)
   {<point name="insert.define_customerization" edit="c"/>}
   #end add-point
   
   CLEAR FORM #清畫面欄位內容
   INITIALIZE g_gzda_m.* LIKE gzda_t.*             #DEFAULT 設定
   LET g_gzda001_t = NULL
 
   
   #add-point:insert段before
   {<point name="insert.before"/>}
   #end add-point    
   
   CALL s_transaction_begin()
   
   WHILE TRUE
      
      #公用欄位給值
      #應用 a14 樣板自動產生(Version:4)    
      #公用欄位新增給值  
      LET g_gzda_m.gzdaownid = g_user
      LET g_gzda_m.gzdaowndp = g_dept
      LET g_gzda_m.gzdacrtid = g_user
      LET g_gzda_m.gzdacrtdp = g_dept 
      LET g_gzda_m.gzdacrtdt = cl_get_current()
      LET g_gzda_m.gzdamodid = g_user
      LET g_gzda_m.gzdamoddt = cl_get_current()
      LET g_gzda_m.gzdastus = 'Y'
 
 
 
      #append欄位給值
      
     
      #一般欄位給值
            LET g_gzda_m.gzdastus = "Y"
 
 
      #add-point:單頭預設值
      {<point name="insert.default"/>}
      #end add-point   
     
      #顯示狀態(stus)圖片
            #應用 a21 樣板自動產生(Version:2)
	  #根據當下狀態碼顯示圖片
      CASE g_gzda_m.gzdastus 
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
         
      END CASE
 
 
     
      #資料輸入
      CALL azzi085_input("a")
      
      #add-point:單頭輸入後
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG THEN
         #取消
         LET INT_FLAG = 0
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         DISPLAY g_current_cnt TO FORMONLY.h_count     #總筆數
