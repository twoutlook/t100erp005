  <section id="azzi085.modify" ver="14" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION azzi085_modify()
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point
   #add-point:modify段define(客製用)
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_gzda_m.gzda001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_gzda001_t = g_gzda_m.gzda001
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN azzi085_cl USING g_gzda_m.gzda001
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN azzi085_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE azzi085_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE azzi085_master_referesh USING g_gzda_m.gzda001 INTO g_gzda_m.gzda001,g_gzda_m.gzda002,g_gzda_m.gzda003, 
       g_gzda_m.gzda004,g_gzda_m.gzda005,g_gzda_m.gzda006,g_gzda_m.gzda007,g_gzda_m.gzda008,g_gzda_m.gzdastus, 
       g_gzda_m.gzdaownid,g_gzda_m.gzdaowndp,g_gzda_m.gzdacrtid,g_gzda_m.gzdacrtdp,g_gzda_m.gzdacrtdt, 
       g_gzda_m.gzdamodid,g_gzda_m.gzdamoddt,g_gzda_m.gzdaownid_desc,g_gzda_m.gzdaowndp_desc,g_gzda_m.gzdacrtid_desc, 
       g_gzda_m.gzdacrtdp_desc,g_gzda_m.gzdamodid_desc
 
   #檢查是否允許此動作
   IF NOT azzi085_action_chk() THEN
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_gzda_m_mask_o.* =  g_gzda_m.*
   CALL azzi085_gzda_t_mask()
   LET g_gzda_m_mask_n.* =  g_gzda_m.*
   
   
 
   #顯示資料
   CALL azzi085_show()
   
   WHILE TRUE
      LET g_gzda_m.gzda001 = g_gzda001_t
 
      
      #寫入修改者/修改日期資訊
      LET g_gzda_m.gzdamodid = g_user 
LET g_gzda_m.gzdamoddt = cl_get_current()
LET g_gzda_m.gzdamodid_desc = cl_get_username(g_gzda_m.gzdamodid)
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
 
      #資料輸入
      CALL azzi085_input("u")     
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_gzda_m.* = g_gzda_m_t.*
         CALL azzi085_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE gzda_t SET (gzdamodid,gzdamoddt) = (g_gzda_m.gzdamodid,g_gzda_m.gzdamoddt)
       WHERE  gzda001 = g_gzda001_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL azzi085_set_act_visible()
   CALL azzi085_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " ",
                      " gzda001 = '", g_gzda_m.gzda001, "' "
 
   #填到對應位置
   CALL azzi085_browser_fill(g_wc,"")
 
   CLOSE azzi085_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL azzi085_msgcentre_notify('modify')
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
]]>
  </section>
