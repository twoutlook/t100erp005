  <section id="azzi085.delete" ver="17" status="" src="s" readonly="">
    <![CDATA[#+ 資料刪除 
PRIVATE FUNCTION azzi085_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define" edit="s"/>}
   #end add-point  
   #add-point:delete段define(客製用)
   {<point name="delete.define_customerization" edit="c"/>}
   #end add-point
   
   #先確定key值無遺漏
   IF g_gzda_m.gzda001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
    
   LET g_gzda001_t = g_gzda_m.gzda001
 
   
   
 
   OPEN azzi085_cl USING g_gzda_m.gzda001
 
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN azzi085_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE azzi085_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE azzi085_master_referesh USING g_gzda_m.gzda001 INTO g_gzda_m.gzda001,g_gzda_m.gzda002,g_gzda_m.gzda003, 
       g_gzda_m.gzda004,g_gzda_m.gzda005,g_gzda_m.gzda006,g_gzda_m.gzda007,g_gzda_m.gzda008,g_gzda_m.gzdastus, 
       g_gzda_m.gzdaownid,g_gzda_m.gzdaowndp,g_gzda_m.gzdacrtid,g_gzda_m.gzdacrtdp,g_gzda_m.gzdacrtdt, 
       g_gzda_m.gzdamodid,g_gzda_m.gzdamoddt,g_gzda_m.gzdaownid_desc,g_gzda_m.gzdaowndp_desc,g_gzda_m.gzdacrtid_desc, 
       g_gzda_m.gzdacrtdp_desc,g_gzda_m.gzdamodid_desc
   
   #檢查是否允許此動作
   IF NOT azzi085_action_chk() THEN
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_gzda_m_mask_o.* =  g_gzda_m.*
   CALL azzi085_gzda_t_mask()
   LET g_gzda_m_mask_n.* =  g_gzda_m.*
   
   #將最新資料顯示到畫面上
   CALL azzi085_show()
   
   IF cl_ask_delete() THEN
 
      #add-point:單頭刪除前
      {<point name="delete.head.b_delete" mark="Y"/>}
      #end add-point
 
      #應用 a47 樣板自動產生(Version:2)
      #刪除相關文件
      CALL azzi085_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
 
      DELETE FROM gzda_t 
       WHERE  gzda001 = g_gzda_m.gzda001 
 
 
      #add-point:單頭刪除中
      {<point name="delete.head.m_delete"/>}
      #end add-point
         
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "gzda_t" 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
      END IF
  
      
      
      #add-point:單頭刪除後
      {<point name="delete.head.a_delete"/>}
      #end add-point
      
       
 
      #修改歷程記錄(刪除)
      IF NOT cl_log_modified_record('','') THEN 
         CLOSE azzi085_cl
         CALL s_transaction_end('N','0')
         RETURN
      END IF
      
      CLEAR FORM
      CALL azzi085_ui_browser_refresh()
      
      #確保畫面上保有資料
      IF g_browser_cnt > 0 THEN
         #CALL azzi085_browser_fill(g_wc,"")
         CALL azzi085_fetch("P")
      ELSE
         CLEAR FORM
      END IF
      
   END IF
 
   CLOSE azzi085_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL azzi085_msgcentre_notify('delete')
 
END FUNCTION
