      #更新key欄位
      SET gzxh001 = g_gzxg_m.gzxg001
          , gzxh002 = g_gzxg_m.gzxg002
 
      #更新共用欄位
      
                                       
  
   #將資料塞回原table   
   INSERT INTO gzxh_t SELECT * FROM azzi800_03_detail
   
   IF SQLCA.sqlcode THEN
      CALL cl_err("reproduce",SQLCA.sqlcode,1)
      RETURN
   END IF
   
   #add-point:單身複製中1
   {<point name="detail_reproduce.body.table1.m_insert"/>}
   #end add-point
   
   #刪除TEMP TABLE
