   LET g_forupd_sql = cl_sql_add_mask(g_forupd_sql)              #遮蔽特定資料
   DECLARE azzi800_03_cl CURSOR FROM g_forupd_sql     # LOCK CURSOR
 
   LET g_sql = " SELECT UNIQUE gzxg001,gzxg002,gzxgstus,gzxgownid,gzxgowndp,gzxgcrtid,gzxgcrtdp,gzxgcrtdt, 
