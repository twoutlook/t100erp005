  <section id="azzi800_03.modify" ver="5" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION azzi800_03_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_gzxg_m.gzxg001 IS NULL
   OR g_gzxg_m.gzxg002 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   ERROR ""
  
   LET g_gzxg001_t = g_gzxg_m.gzxg001
   LET g_gzxg002_t = g_gzxg_m.gzxg002
 
   CALL s_transaction_begin()
   
   OPEN azzi800_03_cl USING g_enterprise,g_gzxg_m.gzxg001,g_gzxg_m.gzxg002
   IF STATUS THEN
      CALL cl_err("OPEN azzi800_03_cl:", STATUS, 1)
      CLOSE azzi800_03_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE azzi800_03_master_referesh USING g_gzxg_m.gzxg001,g_gzxg_m.gzxg002 INTO g_gzxg_m.gzxg001, 
       g_gzxg_m.gzxg002,g_gzxg_m.gzxgstus,g_gzxg_m.gzxgownid,g_gzxg_m.gzxgowndp,g_gzxg_m.gzxgcrtid,g_gzxg_m.gzxgcrtdp, 
       g_gzxg_m.gzxgcrtdt,g_gzxg_m.gzxgmodid,g_gzxg_m.gzxgmoddt,g_gzxg_m.gzxgownid_desc,g_gzxg_m.gzxgowndp_desc, 
       g_gzxg_m.gzxgcrtid_desc,g_gzxg_m.gzxgcrtdp_desc,g_gzxg_m.gzxgmodid_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_gzxg_m.gzxg001,SQLCA.sqlcode,0)
      CLOSE azzi800_03_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   CALL azzi800_03_show()
   WHILE TRUE
      LET g_gzxg001_t = g_gzxg_m.gzxg001
      LET g_gzxg002_t = g_gzxg_m.gzxg002
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_gzxg_m.gzxgmodid = g_user 
LET g_gzxg_m.gzxgmoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      CALL azzi800_03_input("u")
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_gzxg_m.* = g_gzxg_m_t.*
         CALL azzi800_03_show()
         CALL cl_err('',9001,0)
         CALL s_transaction_end('N','0')
         EXIT WHILE
      END IF 
      
      #若有modid跟moddt則進行update
      UPDATE gzxg_t SET (gzxgmodid,gzxgmoddt) = (g_gzxg_m.gzxgmodid,g_gzxg_m.gzxgmoddt)
       WHERE gzxgent = g_enterprise AND gzxg001 = g_gzxg001_t
         AND gzxg002 = g_gzxg002_t
 
                  
      #若單頭key欄位有變更
      IF g_gzxg_m.gzxg001 != g_gzxg001_t 
      OR g_gzxg_m.gzxg002 != g_gzxg002_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE gzxh_t SET gzxh001 = g_gzxg_m.gzxg001
                                       ,gzxh002 = g_gzxg_m.gzxg002
 
          WHERE gzxhent = g_enterprise AND gzxh001 = g_gzxg001_t
            AND gzxh002 = g_gzxg002_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("gzxh_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("gzxh_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
 
         
         #UPDATE 多語言table key值
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_gzxg_m.gzxg001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE azzi800_03_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_gzxg_m.gzxg001,'U')
 
END FUNCTION   
]]>
