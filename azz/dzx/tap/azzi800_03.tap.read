   EXECUTE azzi800_03_master_referesh USING g_gzxg_m.gzxg001,g_gzxg_m.gzxg002 INTO g_gzxg_m.gzxg001, 
       g_gzxg_m.gzxg002,g_gzxg_m.gzxgstus,g_gzxg_m.gzxgownid,g_gzxg_m.gzxgowndp,g_gzxg_m.gzxgcrtid,g_gzxg_m.gzxgcrtdp, 
       g_gzxg_m.gzxgcrtdt,g_gzxg_m.gzxgmodid,g_gzxg_m.gzxgmoddt,g_gzxg_m.gzxgownid_desc,g_gzxg_m.gzxgowndp_desc, 
       g_gzxg_m.gzxgcrtid_desc,g_gzxg_m.gzxgcrtdp_desc,g_gzxg_m.gzxgmodid_desc
 
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_gzxg_m.gzxg001,SQLCA.sqlcode,0)          #資料被他人LOCK
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:delete段before ask
   {<point name="delete.before_ask"/>}
   #end add-point 
 
   IF cl_ask_del_master() THEN              #確認一下
   
      #add-point:單頭刪除前
