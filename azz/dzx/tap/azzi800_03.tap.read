  <section id="azzi800_03.input.body" ver="3" status="" src="s">
    <![CDATA[   
      #Page1 預設值產生於此處
      INPUT ARRAY g_gzxh_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = FALSE, 
                  DELETE ROW = FALSE,
                  APPEND ROW = FALSE)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_gzxh_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL azzi800_03_b_fill()
            LET g_rec_b = g_gzxh_d.getLength()
            #add-point:資料輸入前
            {<point name="input.d.before_input"/>}
            #end add-point
         
         BEFORE ROW
            #add-point:modify段before row2
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET l_insert = FALSE
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET g_detail_idx = l_ac
            
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
         
            CALL s_transaction_begin()
            OPEN azzi800_03_cl USING g_enterprise,g_gzxg_m.gzxg001,g_gzxg_m.gzxg002
            IF STATUS THEN
               CALL cl_err("OPEN azzi800_03_cl:", STATUS, 1)
               CLOSE azzi800_03_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            
            LET g_rec_b = g_gzxh_d.getLength()
            
            IF g_rec_b >= l_ac 
               AND g_gzxh_d[l_ac].gzxh003 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_gzxh_d_t.* = g_gzxh_d[l_ac].*  #BACKUP
               LET g_gzxh_d_o.* = g_gzxh_d[l_ac].*  #BACKUP
               CALL azzi800_03_set_entry_b(l_cmd)
               #add-point:modify段after_set_entry_b
               {<point name="input.body.after_set_entry_b"/>}
               #end add-point  
               CALL azzi800_03_set_no_entry_b(l_cmd)
               IF NOT azzi800_03_lock_b("gzxh_t","'1'") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH azzi800_03_bcl INTO g_gzxh_d[l_ac].gzxh003,g_gzxh_d[l_ac].gzxh004,g_gzxh_d[l_ac].gzxh005, 
                      g_gzxh_d[l_ac].gzxh006
                  IF SQLCA.sqlcode THEN
                     CALL cl_err(g_gzxh_d_t.gzxh003,SQLCA.sqlcode,1)
                     LET l_lock_sw = "Y"
                  END IF
                  
                  LET g_bfill = "N"
                  CALL azzi800_03_show()
                  LET g_bfill = "Y"
                  
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
        
         BEFORE INSERT
            
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_gzxh_d[l_ac].* TO NULL 
			INITIALIZE g_gzxh_d_t.* TO NULL 
            INITIALIZE g_gzxh_d_o.* TO NULL 
            
            LET g_gzxh_d_t.* = g_gzxh_d[l_ac].*     #新輸入資料
            LET g_gzxh_d_o.* = g_gzxh_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL azzi800_03_set_entry_b(l_cmd)
            #add-point:modify段after_set_entry_b
            {<point name="input.body.insert.after_set_entry_b"/>}
            #end add-point
            CALL azzi800_03_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_gzxh_d[li_reproduce_target].* = g_gzxh_d[li_reproduce].*
 
               LET g_gzxh_d[li_reproduce_target].gzxh003 = NULL
 
