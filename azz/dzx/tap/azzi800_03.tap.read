   {<point name="detail_reproduce.body.table1.b_insert" mark="Y"/>}
   #end add-point
   
   #CREATE TEMP TABLE
   LET ls_sql = "CREATE GLOBAL TEMPORARY TABLE azzi800_03_detail AS ",
                "SELECT * FROM gzxh_t "
   PREPARE repro_tbl FROM ls_sql
   EXECUTE repro_tbl
   FREE repro_tbl
                
   #將符合條件的資料丟入TEMP TABLE
   INSERT INTO azzi800_03_detail SELECT * FROM gzxh_t 
                                         WHERE gzxhent = g_enterprise AND gzxh001 = g_gzxg001_t
                                         AND gzxh002 = g_gzxg002_t
 
   
   #將key修正為調整後   
