  <section id="azzi800_03.insert" ver="3" status="" src="s">
    <![CDATA[#+ 資料新增
PRIVATE FUNCTION azzi800_03_insert()
   #add-point:insert段define
   {<point name="insert.define"/>}
   #end add-point    
   
   #清畫面欄位內容
   CLEAR FORM                    
   CALL g_gzxh_d.clear()   
 
 
   INITIALIZE g_gzxg_m.* LIKE gzxg_t.*             #DEFAULT 設定
   
   LET g_gzxg001_t = NULL
   LET g_gzxg002_t = NULL
 
   
   CALL s_transaction_begin()
   WHILE TRUE
      #公用欄位給值(單頭)
      #此段落由子樣板a14產生    
      LET g_gzxg_m.gzxgownid = g_user
      LET g_gzxg_m.gzxgowndp = g_dept
      LET g_gzxg_m.gzxgcrtid = g_user
      LET g_gzxg_m.gzxgcrtdp = g_dept 
      LET g_gzxg_m.gzxgcrtdt = cl_get_current()
      LET g_gzxg_m.gzxgmodid = ""
      LET g_gzxg_m.gzxgmoddt = ""
      LET g_gzxg_m.gzxgstus = "N"
 
 
 
      #append欄位給值
      
     
      #一般欄位給值
      
  
      #add-point:單頭預設值
      {<point name="insert.default"/>}
      #end add-point 
     
      CALL azzi800_03_input("a")
      
      #add-point:單頭輸入後
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_gzxg_m.* = g_gzxg_m_t.*
         CALL azzi800_03_show()
         CALL cl_err('',9001,0)
         CALL s_transaction_end('N','0')
         EXIT WHILE
      END IF
      
      CALL g_gzxh_d.clear()
 
 
      LET g_rec_b = 0
      CALL s_transaction_end('Y','0')
      EXIT WHILE
        
   END WHILE
   
