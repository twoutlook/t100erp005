  <section id="azzi030.input.body" ver="4" status="" src="s">
    <![CDATA[   
      #Page1 預設值產生於此處
      INPUT ARRAY g_gzhc_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert, 
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂ACTION(detail_input,page_1)
         
         
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_gzhc_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL azzi030_b_fill()
            LET g_rec_b = g_gzhc_d.getLength()
            #add-point:資料輸入前
            {<point name="input.d.before_input"/>}
            #end add-point
         
         BEFORE ROW
            #add-point:modify段before row2
            {<point name="input.body.before_row2"/>}
            #end add-point  
            LET l_insert = FALSE
            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET g_detail_idx = l_ac
            
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
         
            CALL s_transaction_begin()
            OPEN azzi030_cl USING g_gzha_m.gzha001
            IF STATUS THEN
               CALL cl_err("OPEN azzi030_cl:", STATUS, 1)
               CLOSE azzi030_cl
               CALL s_transaction_end('N','0')
               RETURN
            END IF
            
            LET g_rec_b = g_gzhc_d.getLength()
            
            IF g_rec_b >= l_ac 
               AND g_gzhc_d[l_ac].gzhc002 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_gzhc_d_t.* = g_gzhc_d[l_ac].*  #BACKUP
               LET g_gzhc_d_o.* = g_gzhc_d[l_ac].*  #BACKUP
               CALL azzi030_set_entry_b(l_cmd)
               #add-point:modify段after_set_entry_b
               {<point name="input.body.after_set_entry_b"/>}
               #end add-point  
               CALL azzi030_set_no_entry_b(l_cmd)
               IF NOT azzi030_lock_b("gzhc_t","'1'") THEN
                  LET l_lock_sw='Y'
               ELSE
                  FETCH azzi030_bcl INTO g_gzhc_d[l_ac].gzhc002
                  IF SQLCA.sqlcode THEN
                     CALL cl_err(g_gzhc_d_t.gzhc002,SQLCA.sqlcode,1)
                     LET l_lock_sw = "Y"
                  END IF
                  
                  LET g_bfill = "N"
                  CALL azzi030_show()
                  LET g_bfill = "Y"
                  
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            #其他table資料備份(確定是否更改用)
            
            #其他table進行lock
            
        
         BEFORE INSERT
            
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_gzhc_d[l_ac].* TO NULL 
            INITIALIZE g_gzhc_d_t.* TO NULL 
            INITIALIZE g_gzhc_d_o.* TO NULL 
            
            LET g_gzhc_d_t.* = g_gzhc_d[l_ac].*     #新輸入資料
            LET g_gzhc_d_o.* = g_gzhc_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL azzi030_set_entry_b(l_cmd)
            #add-point:modify段after_set_entry_b
            {<point name="input.body.insert.after_set_entry_b"/>}
            #end add-point
            CALL azzi030_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_gzhc_d[li_reproduce_target].* = g_gzhc_d[li_reproduce].*
 
               LET g_gzhc_d[li_reproduce_target].gzhc002 = NULL
 
            END IF
            #公用欄位給值(單身)
            
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
  
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            #add-point:單身新增
            {<point name="input.body.b_a_insert"/>}
            #end add-point
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM gzhc_t 
             WHERE  gzhc001 = g_gzha_m.gzha001
 
               AND gzhc002 = g_gzhc_d[l_ac].gzhc002
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               #add-point:單身新增前
               {<point name="input.body.b_insert"/>}
               #end add-point
            
               #同步新增到同層的table
                              INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_gzha_m.gzha001
               LET gs_keys[2] = g_gzhc_d[g_detail_idx].gzhc002
               CALL azzi030_insert_b('gzhc_t',gs_keys,"'1'")
                           
               #add-point:單身新增後
               {<point name="input.body.a_insert"/>}
               #end add-point
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_gzhc_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("gzhc_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #先刷新資料
               #CALL azzi030_b_fill()
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert2"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR 'INSERT O.K'
               LET g_rec_b = g_rec_b + 1
            END IF
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_gzhc_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_gzhc_d.deleteElement(l_ac)
               NEXT FIELD gzhc002
            END IF
         
            IF g_gzhc_d[l_ac].gzhc002 IS NOT NULL
 
               THEN 
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete" mark="Y"/>}
               #end add-point 
               
               DELETE FROM gzhc_t
                WHERE  gzhc001 = g_gzha_m.gzha001 AND
 
                      gzhc002 = g_gzhc_d_t.gzhc002
 
                  
               #add-point:單身刪除中
               {<point name="input.body.m_delete"/>}
               #end add-point 
               
               IF SQLCA.sqlcode THEN
                  CALL cl_err("gzhc_t",SQLCA.sqlcode,1)
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               ELSE
                  LET g_rec_b = g_rec_b-1
                  
                  #add-point:單身刪除後
                  {<point name="input.body.a_delete"/>}
                  #end add-point
                  CALL s_transaction_end('Y','0')
               END IF 
               CLOSE azzi030_bcl
               LET l_count = g_gzhc_d.getLength()
            END IF 
            
                           INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_gzha_m.gzha001
               LET gs_keys[2] = g_gzhc_d[g_detail_idx].gzhc002
 
              
         AFTER DELETE 
            #add-point:單身刪除後2
            {<point name="input.body.after_delete"/>}
            #end add-point
                           CALL azzi030_delete_b('gzhc_t',gs_keys,"'1'")
 
                  #此段落由子樣板a01產生
         BEFORE FIELD gzhc002
            #add-point:BEFORE FIELD gzhc002
            {<point name="input.b.page1.gzhc002" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD gzhc002
            
            #add-point:AFTER FIELD gzhc002
            {<point name="input.a.page1.gzhc002" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE gzhc002
            #add-point:ON CHANGE gzhc002
