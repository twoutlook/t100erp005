  <section id="azzi030.modify" ver="6" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION azzi030_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_gzha_m.gzha001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   ERROR ""
  
   LET g_gzha001_t = g_gzha_m.gzha001
 
   CALL s_transaction_begin()
   
   OPEN azzi030_cl USING g_gzha_m.gzha001
   IF STATUS THEN
      CALL cl_err("OPEN azzi030_cl:", STATUS, 1)
      CLOSE azzi030_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE azzi030_master_referesh USING g_gzha_m.gzha001 INTO g_gzha_m.gzha001,g_gzha_m.gzhastus,g_gzha_m.gzhaownid, 
       g_gzha_m.gzhaowndp,g_gzha_m.gzhacrtid,g_gzha_m.gzhacrtdp,g_gzha_m.gzhacrtdt,g_gzha_m.gzhamodid, 
       g_gzha_m.gzhamoddt,g_gzha_m.gzhaownid_desc,g_gzha_m.gzhaowndp_desc,g_gzha_m.gzhacrtid_desc,g_gzha_m.gzhacrtdp_desc, 
       g_gzha_m.gzhamodid_desc
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_gzha_m.gzha001,SQLCA.sqlcode,0)
      CLOSE azzi030_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   CALL azzi030_show()
   WHILE TRUE
      LET g_gzha001_t = g_gzha_m.gzha001
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_gzha_m.gzhamodid = g_user 
LET g_gzha_m.gzhamoddt = cl_get_current()
 
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      CALL azzi030_input("u")
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_gzha_m.* = g_gzha_m_t.*
         CALL azzi030_show()
         CALL cl_err('',9001,0)
         CALL s_transaction_end('N','0')
         EXIT WHILE
      END IF 
      
      #若有modid跟moddt則進行update
      UPDATE gzha_t SET (gzhamodid,gzhamoddt) = (g_gzha_m.gzhamodid,g_gzha_m.gzhamoddt)
       WHERE  gzha001 = g_gzha001_t
 
                  
      #若單頭key欄位有變更
      IF g_gzha_m.gzha001 != g_gzha001_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE gzhc_t SET gzhc001 = g_gzha_m.gzha001
 
          WHERE  gzhc001 = g_gzha001_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
