  <point name="function.azzi030_file_chk_gzhc" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 檢查GWC佈景色調的檔案是否存在
# Memo...........:
# Usage..........: CALL azzi030_file_chk_gzhc(p_gzha001)
#                  RETURNING l_not_exist
# Input parameter: p_gzha001   佈景編號
#                  p_idx        檢查單筆的index
# Return code....: l_not_exist   不存在的檔案
# Date & Author..: 2014/04/08 By tsai_yen
# Modify.........:
################################################################################
PRIVATE FUNCTION azzi030_file_chk_gzhc(p_gzha001,p_idx)
   DEFINE p_gzha001     LIKE gzha_t.gzha001   #佈景編號
   DEFINE p_idx         LIKE type_t.num10     #檢查單筆的index
   DEFINE l_not_exist   STRING                #不存在的檔案
   DEFINE l_file        STRING   
   DEFINE l_i           LIKE type_t.num10
   DEFINE l_start       LIKE type_t.num10
   DEFINE l_end         LIKE type_t.num10

   IF g_gzhc_d.getLength() > 0 THEN
      IF p_idx = 0 THEN
         LET l_start = 1
         LET l_end = g_gzhc_d.getLength()
         CALL g_gzhc_d_att.clear()
      ELSE
         LET l_start = p_idx
         LET l_end = p_idx
      END IF
   
      FOR l_i = l_start TO l_end
         LET l_file = FGL_GETENV("TOP"),"/www/tplinc/",p_gzha001 CLIPPED,'_',g_gzhc_d[l_i].gzhc002 CLIPPED      
         IF os.Path.exists(l_file) THEN
            LET g_gzhc_d_att[l_i].gzhc002 = ""
         ELSE
            LET g_gzhc_d_att[l_i].gzhc002 = "red"
            LET l_not_exist = l_not_exist CLIPPED,",",l_file CLIPPED
         END IF
      END FOR

      IF NOT cl_null(l_not_exist) THEN
         LET l_not_exist = l_not_exist.subString(2,l_not_exist.getLength())
      END IF
   END IF
   
   RETURN l_not_exist
END FUNCTION]]>
  </point>
  <point name="function.azzi030_file_chk_gzhb" order="2" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[
################################################################################
# Descriptions...: 檢查GWC佈景樣板的檔案是否存在
# Memo...........:
# Usage..........: CALL azzi030_file_chk_gzhb(p_gzha001)
#                  RETURNING l_not_exist
# Input parameter: p_gzha001    佈景編號
#                  p_idx        檢查單筆的index
# Return code....: l_not_exist  不存在的檔案
# Date & Author..: 2014/04/08 By tsai_yen
# Modify.........:
################################################################################
PRIVATE FUNCTION azzi030_file_chk_gzhb(p_gzha001,p_idx)
   DEFINE p_gzha001     LIKE gzha_t.gzha001   #佈景編號
   DEFINE p_idx         LIKE type_t.num10     #檢查單筆的index
   DEFINE l_not_exist   STRING                #不存在的檔案
   DEFINE l_file        STRING   
   DEFINE l_i           LIKE type_t.num10
   DEFINE l_start       LIKE type_t.num10
   DEFINE l_end         LIKE type_t.num10

   IF g_gzhc2_d.getLength() > 0 THEN
      IF p_idx = 0 THEN
         LET l_start = 1
         LET l_end = g_gzhc2_d.getLength()
         CALL g_gzhc2_d_att.clear()
      ELSE
         LET l_start = p_idx
         LET l_end = p_idx
      END IF
      
      FOR l_i = l_start TO l_end
         LET l_file = FGL_GETENV("TOP"),"/erp/cfg/tpl/",p_gzha001 CLIPPED,'/',g_gzhc2_d[l_i].gzhb005 CLIPPED      
         LET g_gzhc2_d_att[l_i].gzhb002 = "" 
         LET g_gzhc2_d_att[l_i].gzhb003 = "" 
         LET g_gzhc2_d_att[l_i].gzhb004 = "" 
         IF os.Path.exists(l_file) THEN
            LET g_gzhc2_d_att[l_i].gzhb005 = ""
         ELSE
            LET g_gzhc2_d_att[l_i].gzhb005 = "red"
            LET l_not_exist = l_not_exist CLIPPED,",",l_file CLIPPED
         END IF
      END FOR

      IF NOT cl_null(l_not_exist) THEN
         LET l_not_exist = l_not_exist.subString(2,l_not_exist.getLength())
      END IF
   END IF

   RETURN l_not_exist
END FUNCTION]]>
