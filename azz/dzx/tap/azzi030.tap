  <section id="azzi030.b_fill" ver="4" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION azzi030_b_fill()
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point     
 
   CALL g_gzhc_d.clear()    #g_gzhc_d 單頭及單身 
   CALL g_gzhc2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   #判斷是否填充
   IF azzi030_fill_chk(1) THEN
   
      LET g_sql = "SELECT  UNIQUE gzhc002  FROM gzhc_t",   
                  " INNER JOIN gzha_t ON gzha001 = gzhc001 ",
 
                  #"",
                  
                  "",
                  
                  " WHERE gzhc001=?"
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      #add-point:b_fill段sql_before
      {<point name="b_fill.body.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table1) THEN
         LET g_sql = g_sql CLIPPED, " AND ", g_wc2_table1 CLIPPED
      END IF
      
      #子單身的WC
      
      
      LET g_sql = g_sql, " ORDER BY gzhc_t.gzhc002"
      
      #add-point:單身填充控制
      {<point name="b_fill.sql"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE azzi030_pb FROM g_sql
      DECLARE b_fill_cs CURSOR FOR azzi030_pb
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_gzha_m.gzha001
                                               
      FOREACH b_fill_cs INTO g_gzhc_d[l_ac].gzhc002
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               CALL cl_err( '', 9035, 1)
            END IF
            EXIT FOREACH
         END IF
         
      END FOREACH
      LET g_error_show = 0
   
   END IF
   
   #判斷是否填充
   IF azzi030_fill_chk(2) THEN
      LET g_sql = "SELECT  UNIQUE gzhb002,gzhb003,gzhb004,gzhb005  FROM gzhb_t",   
                  " INNER JOIN gzha_t ON gzha001 = gzhb001 ",
 
                  "",
                  
                  
                  " WHERE gzhb001=?"   
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      #add-point:b_fill段sql_before
      {<point name="b_fill.body2.fill_sql"/>}
      #end add-point
      IF NOT cl_null(g_wc2_table2) THEN
         LET g_sql = g_sql CLIPPED," AND ",g_wc2_table2 CLIPPED
      END IF
      
      #子單身的WC
      
      
      LET g_sql = g_sql, " ORDER BY gzhb_t.gzhb002,gzhb_t.gzhb003,gzhb_t.gzhb004"
      
      #add-point:單身填充控制
      {<point name="b_fill.sql2"/>}
      #end add-point
      
      LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
      PREPARE azzi030_pb2 FROM g_sql
      DECLARE b_fill_cs2 CURSOR FOR azzi030_pb2
      
      LET l_ac = 1
      
      OPEN b_fill_cs2 USING g_gzha_m.gzha001
                                               
      FOREACH b_fill_cs2 INTO g_gzhc2_d[l_ac].gzhb002,g_gzhc2_d[l_ac].gzhb003,g_gzhc2_d[l_ac].gzhb004, 
          g_gzhc2_d[l_ac].gzhb005
         IF SQLCA.sqlcode THEN
            CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
        
         #add-point:b_fill段資料填充
         {<point name="b_fill2.fill"/>}
         #end add-point
      
         LET l_ac = l_ac + 1
         IF l_ac > g_max_rec THEN
            CALL cl_err( '', 9035, 0 )
            EXIT FOREACH
         END IF
         
      END FOREACH
   END IF
 
 
   
   #add-point:browser_fill段其他table處理
   {<point name="browser_fill.other_fill"/>}
   #end add-point
   
   CALL g_gzhc_d.deleteElement(g_gzhc_d.getLength())
   CALL g_gzhc2_d.deleteElement(g_gzhc2_d.getLength())
 
   
 
   LET l_ac = g_cnt
   LET g_cnt = 0  
   
   FREE azzi030_pb
   FREE azzi030_pb2
 
 
   
END FUNCTION
]]>
  </section>
