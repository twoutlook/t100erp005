  <section id="azzi988.modify" ver="9" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION azzi988_modify()
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point  
   #add-point:modify段define
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point    
   
   IF g_gzah_m.gzah001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
   LET g_gzah001_t = g_gzah_m.gzah001
 
   CALL s_transaction_begin()
   
   OPEN azzi988_cl USING g_gzah_m.gzah001
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN azzi988_cl:" 
      LET g_errparam.code   =  STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE azzi988_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE azzi988_master_referesh USING g_gzah_m.gzah001 INTO g_gzah_m.gzah001,g_gzah_m.gzah001_desc 

   
   #遮罩相關處理
   LET g_gzah_m_mask_o.* =  g_gzah_m.*
   CALL azzi988_gzah_t_mask()
   LET g_gzah_m_mask_n.* =  g_gzah_m.*
   
   CALL s_transaction_end('Y','0')
 
   CALL azzi988_show()
   WHILE TRUE
      LET g_gzah001_t = g_gzah_m.gzah001
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL azzi988_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_gzah_m.* = g_gzah_m_t.*
         CALL azzi988_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更(更新單身table的key欄位值)
      IF g_gzah_m.gzah001 != g_gzah001_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("modify,modify_detail,delete,reproduce", TRUE)
   CALL azzi988_set_act_visible()
   CALL azzi988_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " ",
                      " gzah001 = '", g_gzah_m.gzah001, "' "
 
   #填到對應位置
   CALL azzi988_browser_fill("")
 
   CALL azzi988_idx_chk()
 
   CLOSE azzi988_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL azzi988_msgcentre_notify('modify')
   
END FUNCTION   
]]>
  </section>
