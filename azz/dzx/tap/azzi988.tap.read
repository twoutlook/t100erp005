  <section id="azzi988.b_fill" ver="11" status="" src="s" readonly="">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION azzi988_b_fill(p_wc2)
   DEFINE p_wc2      STRING
   #add-point:b_fill段define
   {<point name="b_fill.define" edit="s"/>}
   #end add-point     
   #add-point:b_fill段define
   {<point name="b_fill.define_customerization" edit="c"/>}
   #end add-point
   
   #先清空單身變數內容
   CALL g_gzah_d.clear()
   CALL g_gzah2_d.clear()
 
 
   #add-point:b_fill段sql_before
   {<point name="b_fill.sql_before"/>}
   #end add-point
   
   LET g_sql = "SELECT  UNIQUE gzah005,gzah006,gzah002,gzah003,gzah007,gzah004,gzahstus,gzahownid,gzahowndp, 
       gzahcrtid,gzahcrtdp,gzahcrtdt,gzahmodid,gzahmoddt,gzahcnfid,gzahcnfdt,gzah005,t1.gzdel003 ,t2.ooag011 , 
       t3.ooefl003 ,t4.ooag011 ,t5.ooefl003 ,t6.ooag011 ,t7.ooag011 FROM gzah_t",   
               " LEFT JOIN gzahl_t ON gzah001 = gzahl001 AND gzah005 = gzahl002 AND gzahl003 = '",g_dlang,"'",
               
                              " LEFT JOIN gzdel_t t1 ON t1.gzdel001=gzah002 AND t1.gzdel002='"||g_lang||"' ",
               " LEFT JOIN ooag_t t2 ON t2.ooagent='"||g_enterprise||"' AND t2.ooag001=gzahownid  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent='"||g_enterprise||"' AND t3.ooefl001=gzahowndp AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t4 ON t4.ooagent='"||g_enterprise||"' AND t4.ooag001=gzahcrtid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=gzahcrtdp AND t5.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t6 ON t6.ooagent='"||g_enterprise||"' AND t6.ooag001=gzahmodid  ",
               " LEFT JOIN ooag_t t7 ON t7.ooagent='"||g_enterprise||"' AND t7.ooag001=gzahcnfid  ",
 
               " WHERE gzah001=?"  
 
   IF NOT cl_null(g_wc2_table1) THEN
      LET g_sql = g_sql CLIPPED," AND ",g_wc2_table1 CLIPPED, cl_sql_add_filter("gzah_t")
   END IF
   
   #add-point:b_fill段sql_after
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   #子單身的WC
   
   
   #判斷是否填充
   IF azzi988_fill_chk(1) THEN
      IF g_action_choice <> 'fetch' OR cl_null(g_action_choice) THEN
         LET g_sql = g_sql, " ORDER BY gzah_t.gzah005"
         #add-point:b_fill段fill_before
         {<point name="b_fill.fill_before"/>}
         #end add-point
         LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
         PREPARE azzi988_pb FROM g_sql
         DECLARE b_fill_cs CURSOR FOR azzi988_pb
      END IF
      
      LET g_cnt = l_ac
      LET l_ac = 1
      
      OPEN b_fill_cs USING g_gzah_m.gzah001
                                               
      FOREACH b_fill_cs INTO g_gzah_d[l_ac].gzah005,g_gzah_d[l_ac].gzah006,g_gzah_d[l_ac].gzah002,g_gzah_d[l_ac].gzah003, 
          g_gzah_d[l_ac].gzah007,g_gzah_d[l_ac].gzah004,g_gzah_d[l_ac].gzahstus,g_gzah2_d[l_ac].gzahownid, 
          g_gzah2_d[l_ac].gzahowndp,g_gzah2_d[l_ac].gzahcrtid,g_gzah2_d[l_ac].gzahcrtdp,g_gzah2_d[l_ac].gzahcrtdt, 
          g_gzah2_d[l_ac].gzahmodid,g_gzah2_d[l_ac].gzahmoddt,g_gzah2_d[l_ac].gzahcnfid,g_gzah2_d[l_ac].gzahcnfdt, 
          g_gzah2_d[l_ac].gzah005,g_gzah_d[l_ac].gzah002_desc,g_gzah2_d[l_ac].gzahownid_desc,g_gzah2_d[l_ac].gzahowndp_desc, 
          g_gzah2_d[l_ac].gzahcrtid_desc,g_gzah2_d[l_ac].gzahcrtdp_desc,g_gzah2_d[l_ac].gzahmodid_desc, 
          g_gzah2_d[l_ac].gzahcnfid_desc
                             
         IF SQLCA.sqlcode THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "FOREACH:" 
            LET g_errparam.code   = SQLCA.sqlcode 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
            EXIT FOREACH
         END IF
                           
         #add-point:b_fill段資料填充
         {<point name="b_fill.fill"/>}
         #end add-point
      
         #帶出公用欄位reference值
         
         
         #應用 a12 樣板自動產生(Version:3)
 
 
 
        
         #add-point:單身資料抓取
         {<point name="bfill.foreach"/>}
         #end add-point
      
         IF l_ac > g_max_rec THEN
            IF g_error_show = 1 THEN
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = l_ac
               LET g_errparam.code   = 9035 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
            END IF 
            EXIT FOREACH
         END IF
      
         LET l_ac = l_ac + 1
         
      END FOREACH
 
            CALL g_gzah_d.deleteElement(g_gzah_d.getLength())
      CALL g_gzah2_d.deleteElement(g_gzah2_d.getLength())
 
      
   END IF
   
   #add-point:b_fill段more
   {<point name="b_fill.more"/>}
   #end add-point
   
   LET g_rec_b=l_ac-1
   DISPLAY g_rec_b TO FORMONLY.cnt  
   LET l_ac = g_cnt
   LET g_cnt = 0 
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_gzah_d.getLength()
      LET g_gzah_d_mask_o[l_ac].* =  g_gzah_d[l_ac].*
      CALL azzi988_gzah_t_mask()
      LET g_gzah_d_mask_n[l_ac].* =  g_gzah_d[l_ac].*
   END FOR
   
   LET g_gzah2_d_mask_o.* =  g_gzah2_d.*
   FOR l_ac = 1 TO g_gzah2_d.getLength()
      LET g_gzah2_d_mask_o[l_ac].* =  g_gzah2_d[l_ac].*
      CALL azzi988_gzah_t_mask()
      LET g_gzah2_d_mask_n[l_ac].* =  g_gzah2_d[l_ac].*
   END FOR
 
 
   FREE azzi988_pb   
   
END FUNCTION
]]>
