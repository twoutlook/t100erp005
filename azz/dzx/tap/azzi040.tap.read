  <section id="azzi040.b_fill" ver="1" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION azzi040_b_fill(p_wc2)              #BODY FILL UP
   {<Local define>}
   DEFINE p_wc2           STRING
   {</Local define>}
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE gzotstus,gzot001,gzot002,gzot003,'',gzotmodid,'',gzotmoddt,gzotownid, 
       '',gzotowndp,'',gzotcrtid,'',gzotcrtdp,'',gzotcrtdt FROM gzot_t",
               "",
               " WHERE 1=1 AND ", p_wc2
    
   LET g_sql = g_sql, " ORDER BY gzot_t.gzot001"
  
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
  
   PREPARE azzi040_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR azzi040_pb
   
   OPEN b_fill_curs
 
   CALL g_gzot_d.clear()
   CALL g_gzot1_info_d.clear()   
 
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_gzot_d[l_ac].gzotstus,g_gzot_d[l_ac].gzot001,g_gzot_d[l_ac].gzot002,g_gzot_d[l_ac].gzot003, 
       g_gzot1_info_d[l_ac].gzot001,g_gzot1_info_d[l_ac].gzotmodid,g_gzot1_info_d[l_ac].gzotmodid_desc, 
       g_gzot1_info_d[l_ac].gzotmoddt,g_gzot1_info_d[l_ac].gzotownid,g_gzot1_info_d[l_ac].gzotownid_desc, 
       g_gzot1_info_d[l_ac].gzotowndp,g_gzot1_info_d[l_ac].gzotowndp_desc,g_gzot1_info_d[l_ac].gzotcrtid, 
       g_gzot1_info_d[l_ac].gzotcrtid_desc,g_gzot1_info_d[l_ac].gzotcrtdp,g_gzot1_info_d[l_ac].gzotcrtdp_desc, 
       g_gzot1_info_d[l_ac].gzotcrtdt
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL azzi040_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec AND g_error_show = 1 THEN
      CALL cl_err("gzot_t",9035,1)
   END IF 
   LET g_error_show = 0
   
 
  
   
   CALL g_gzot_d.deleteElement(g_gzot_d.getLength())   
   CALL g_gzot1_info_d.deleteElement(g_gzot1_info_d.getLength())
 
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_gzot_d.getLength()
      LET g_gzot1_info_d[l_ac].gzot001 = g_gzot_d[l_ac].gzot001 
 
 
   END FOR
   
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
 
   LET g_detail_cnt = l_ac - 1
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   LET l_ac = g_cnt
   LET g_cnt = 0
   
   CLOSE b_fill_curs
   FREE azzi040_pb
   
END FUNCTION
]]>
  </section>
