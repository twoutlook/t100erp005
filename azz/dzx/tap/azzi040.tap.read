  <point name="function.azzi040_check_gzot002" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[# 檢核gzot002 必須符合 GMT+N 或 GMT-N  N表示0 - 12的整數
PRIVATE FUNCTION azzi040_check_gzot002()
  DEFINE ls_str STRING 
   DEFINE ls_temp STRING 

   LET g_errno = NULL 
   LET ls_str  = g_gzot_d[l_ac].gzot002
   LET ls_str = ls_str.trim()
   LET ls_temp = ls_str.subString(1,3)
   
    IF ls_temp <> "GMT" THEN 
      LET g_errno = "azz-00037"  #符合 GMT+N 或 GMT-N  N表示0 - 12的整數
      RETURN 
   END IF 

   IF ls_str.getLength() < 4 OR ls_str.getLength() > 6 THEN 
      LET g_errno = "azz-00037"  
      RETURN 
   END IF 

   IF ls_str.getIndexOf("-",1) OR ls_str.getIndexOf("+",1) THEN 
      LET ls_temp = ls_str.subString(5,ls_str.getLength()) 
      #ls_temp 要符合數字
      IF NOT cl_chk_num(ls_temp,"N") THEN  
         LET g_errno = "azz-00005"
         RETURN
      END IF
      #ls_temp > 12 要符合 N表示0 - 12的整數
      IF ls_temp > 12 THEN 
         LET g_errno = "azz-00005"
         RETURN
      END IF 
   ELSE
      #要符合 GMT+N 或 GMT-N 
      LET g_errno = "azz-00037"
      RETURN
   END IF
END FUNCTION]]>
