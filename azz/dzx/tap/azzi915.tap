               LET g_gzxn_d_t.* = g_gzxn_d[l_ac3].*  #BACKUP
               OPEN azzi915_bcl3 USING g_enterprise, g_gzxo_d[g_detail_idx4].gp_id, g_gzxm_m.gzxm002, g_gzxm_m.gzxm003,  g_gzxn_d_t.gzxn004
               IF STATUS THEN
