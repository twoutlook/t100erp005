            END IF
            
            #add-point:單身新增後

            #end add-point
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' THEN
               CALL FGL_SET_ARR_CURR(l_ac3-1)
               CALL g_gzxn_d.deleteElement(l_ac3)
               NEXT FIELD gzxn001
            END IF
            IF NOT cl_null(g_gzxn_d_t.gzxn004) THEN
               #add-point:單身刪除前

               #end add-point
               
               IF NOT cl_ask_del_detail() THEN
