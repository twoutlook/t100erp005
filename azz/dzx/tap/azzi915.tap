################################################################################
PRIVATE FUNCTION azzi915_set_allrows_default(pc_gzxm001)
   DEFINE   pc_gzxm001   LIKE gzxm_t.gzxm001
   DEFINE   li_i         LIKE type_t.num5

   #除了傳入的查詢方案本身外, 其他皆改為非預設
   UPDATE gzxm_t SET gzxm004 = "N"
