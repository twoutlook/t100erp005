         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code = 9001
               LET g_errparam.extend = ''
               LET g_errparam.popup = FALSE
               CALL cl_err()

               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM gzxn_t 
             WHERE gzxnent = g_enterprise AND gzxn001 = g_gzxm_d[g_detail_idx].gzxm001
               AND gzxn002 = g_gzxm_m.gzxm002 AND gzxn003 = g_gzxm_m.gzxm003
               AND gzxn004 = g_gzxn_d[l_ac3].gzxn004
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               CALL s_transaction_begin()

               #add-point:單身新增前

               #end add-point
               
               INSERT INTO gzxn_t
                           (gzxnent,gzxn001,gzxn002,gzxn003,gzxn004,gzxn005,gzxn006,gzxn007,gzxn008,gzxn009) 
                     VALUES(g_enterprise,g_gzxm_d[g_detail_idx].gzxm001,
                            g_gzxm_m.gzxm002,g_gzxm_m.gzxm003,
                       g_gzxn_d[l_ac3].gzxn004,g_gzxn_d[l_ac3].gzxn005,g_gzxn_d[l_ac3].gzxn006,g_gzxn_d[l_ac3].gzxn007,g_gzxn_d[l_ac3].gzxn008,g_gzxn_d[l_ac3].gzxn009)
