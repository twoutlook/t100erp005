               LET g_where_rs = cl_qbe_get_condition(g_gzxo_d[g_detail_idx4].gp_id, g_gzxm_m.gzxm002, g_gzxm_m.gzxm003)
               LET g_where_rs = azzi915_combine_condition(g_where_rs)
               DISPLAY g_where_rs TO FORMONLY.where_result
            END IF

