               CALL cl_qbe_get_operator_param(g_gzxn_d[l_ac3].gzxn009)
               LET g_gzxn_d[l_ac3].gzxn006 = g_operator_sel.gzxn006
               IF g_gzxn_d[l_ac3].gzxn009 != g_gzxn_d[l_ac3].gzxn006 THEN
