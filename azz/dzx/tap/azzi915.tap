                  LET g_errparam.popup = TRUE
                  CALL cl_err()

                  CANCEL DELETE
               END IF
               DELETE FROM gzxn_t
                WHERE gzxnent = g_enterprise AND gzxn001 = g_gzxm_d[g_detail_idx].gzxm001
                  AND gzxn002 = g_gzxm_m.gzxm002 AND gzxn003 = g_gzxm_m.gzxm003 
                  AND gzxn004 = g_gzxn_d_t.gzxn004

              #add-point:單筆刪除中

              #end add-point
   
              IF SQLCA.sqlcode THEN
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE
               ELSE 
                  CALL s_transaction_end('Y','0')
               END IF 
               CLOSE azzi915_bcl3
               LET l_count = g_gzxn_d.getLength()
               LET g_where_rs = cl_qbe_get_condition(g_gzxm_d[g_detail_idx].gzxm001, g_gzxm_m.gzxm002, g_gzxm_m.gzxm003)
               DISPLAY g_where_rs TO FORMONLY.where_result
            END IF 
            
            #add-point:單身刪除後

