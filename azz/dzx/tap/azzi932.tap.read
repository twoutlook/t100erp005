            #add-point:AFTER FIELD fileimg
            {<point name="input.a.page1.fileimg" />}
            #END add-point
            
 
         #應用 a04 樣板自動產生(Version:2)
         ON CHANGE fileimg
            #add-point:ON CHANGE fileimg
            {<point name="input.g.page1.fileimg" />}
            #END add-point 
 
 
                  #Ctrlp:input.c.page1.gzwj002
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD gzwj002
            #add-point:ON ACTION controlp INFIELD gzwj002
            {<point name="input.c.page1.gzwj002" />}
            #END add-point
 
         #Ctrlp:input.c.page1.gzwj004
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD gzwj004
            #add-point:ON ACTION controlp INFIELD gzwj004
