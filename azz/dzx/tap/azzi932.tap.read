               LET gs_keys_bak[1] = g_gzwi001_t
               LET gs_keys[2] = g_gzwj_d[g_detail_idx].gzwj002
               LET gs_keys_bak[2] = g_gzwj_d_t.gzwj002
               CALL azzi932_update_b('gzwj_t',gs_keys,gs_keys_bak,"'1'")
