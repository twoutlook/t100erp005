            #add-point:BEFORE FIELD gzwj005
            {<point name="input.b.page1.gzwj005" />}
            #END add-point
 
         #應用 a02 樣板自動產生(Version:1)
         AFTER FIELD gzwj005
            
            #add-point:AFTER FIELD gzwj005
            {<point name="input.a.page1.gzwj005" />}
            #END add-point
            
 
         #應用 a04 樣板自動產生(Version:2)
         ON CHANGE gzwj005
            #add-point:ON CHANGE gzwj005
            {<point name="input.g.page1.gzwj005" />}
            #END add-point 
 
         #應用 a01 樣板自動產生(Version:1)
         BEFORE FIELD fileimg
            #add-point:BEFORE FIELD fileimg
            {<point name="input.b.page1.fileimg" />}
            #END add-point
 
         #應用 a02 樣板自動產生(Version:1)
