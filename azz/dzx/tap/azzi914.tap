  <section id="azzi914.input.body" ver="2" status="" src="s">
    <![CDATA[      #Page1 預設值產生於此處
      INPUT ARRAY g_gzxk_d FROM s_detail1.*
          ATTRIBUTE(COUNT = g_rec_b,MAXCOUNT = g_max_rec,WITHOUT DEFAULTS, 
                  INSERT ROW = l_allow_insert,
                  DELETE ROW = l_allow_delete,
                  APPEND ROW = l_allow_insert)
 
         #自訂單身ACTION
         
 
         BEFORE INPUT
            IF g_insert = 'Y' AND NOT cl_null(g_insert) THEN 
              CALL FGL_SET_ARR_CURR(g_gzxk_d.getLength()+1) 
              LET g_insert = 'N' 
           END IF 
 
            CALL azzi914_b_fill(g_wc2) 
            IF g_rec_b != 0 THEN
               CALL fgl_set_arr_curr(l_ac)
            END IF
            #add-point:資料輸入前
            {<point name="input.body.before_input"/>}
            #end add-point
         
         BEFORE ROW
            LET l_insert = FALSE
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail1")
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_ac TO FORMONLY.idx
            
         
            CALL s_transaction_begin()
            
            #判定新增或修改
            IF l_cmd = 'u' THEN
               OPEN azzi914_cl USING g_enterprise,
                                               g_gzxk_m.gzxk001
                                               ,g_gzxk_m.gzxk002
                                               ,g_gzxk_m.gzxk003
 
                                               
               IF STATUS THEN
                  CALL cl_err("OPEN azzi914_cl:", STATUS, 1)
                  CLOSE azzi914_cl
                  CALL s_transaction_end('N','0')
                  RETURN
               END IF
            END IF
            
            LET l_cmd = ''
            
            IF g_rec_b >= l_ac 
               AND g_gzxk_d[l_ac].gzxk004 IS NOT NULL
 
            THEN
               LET l_cmd='u'
               LET g_gzxk_d_t.* = g_gzxk_d[l_ac].*  #BACKUP
               LET g_gzxk_d_o.* = g_gzxk_d[l_ac].*  #BACKUP
               CALL azzi914_set_entry_b(l_cmd)
               #add-point:set_entry_b後
               {<point name="input.body.before_row.set_entry_b"/>}
               #end add-point
               CALL azzi914_set_no_entry_b(l_cmd)
               OPEN azzi914_bcl USING g_enterprise,g_gzxk_m.gzxk001,
                                                g_gzxk_m.gzxk002,
                                                g_gzxk_m.gzxk003,
 
                                                g_gzxk_d_t.gzxk004
 
               IF STATUS THEN
                  CALL cl_err("OPEN azzi914_bcl:", STATUS, 1)
                  LET l_lock_sw='Y'
               ELSE
                  FETCH azzi914_bcl INTO g_gzxk_d[l_ac].gzxkstus,g_gzxk_d[l_ac].gzxk005,g_gzxk_d[l_ac].gzxk004, 
                      g_gzxk_d[l_ac].gzxk006,g_gzxk2_d[l_ac].gzxk004,g_gzxk2_d[l_ac].gzxkownid,g_gzxk2_d[l_ac].gzxkowndp, 
                      g_gzxk2_d[l_ac].gzxkcrtid,g_gzxk2_d[l_ac].gzxkcrtdp,g_gzxk2_d[l_ac].gzxkcrtdt, 
                      g_gzxk2_d[l_ac].gzxkmodid,g_gzxk2_d[l_ac].gzxkmoddt
                  IF SQLCA.sqlcode THEN
                      CALL cl_err(g_gzxk_d_t.gzxk004,SQLCA.sqlcode,1)
                      LET l_lock_sw = "Y"
                  END IF
                  
                  CALL azzi914_ref_show()
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF
            #add-point:modify段before row
            {<point name="input.body.before_row"/>}
            #end add-point  
            
        
         BEFORE INSERT
            
            INITIALIZE g_gzxk_d_t.* TO NULL
            INITIALIZE g_gzxk_d_o.* TO NULL
            LET l_insert = TRUE
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_gzxk_d[l_ac].* TO NULL
            
            #公用欄位預設值
            #此段落由子樣板a14產生    
      LET g_gzxk2_d[l_ac].gzxkownid = g_user
      LET g_gzxk2_d[l_ac].gzxkowndp = g_dept
      LET g_gzxk2_d[l_ac].gzxkcrtid = g_user
      LET g_gzxk2_d[l_ac].gzxkcrtdp = g_dept 
      LET g_gzxk2_d[l_ac].gzxkcrtdt = cl_get_current()
      LET g_gzxk2_d[l_ac].gzxkmodid = ""
      LET g_gzxk2_d[l_ac].gzxkmoddt = ""
      LET g_gzxk_d[l_ac].gzxkstus = ""
 
  
            
            #一般欄位預設值
            
            
            
            LET g_gzxk_d_t.* = g_gzxk_d[l_ac].*     #新輸入資料
            LET g_gzxk_d_o.* = g_gzxk_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL azzi914_set_entry_b(l_cmd)
            #add-point:set_entry_b後
            {<point name="input.body.before_insert.set_entry_b"/>}
            #end add-point
            CALL azzi914_set_no_entry_b(l_cmd)
            IF lb_reproduce THEN
               LET lb_reproduce = FALSE
               LET g_gzxk_d[li_reproduce_target].* = g_gzxk_d[li_reproduce].*
               LET g_gzxk2_d[li_reproduce_target].* = g_gzxk2_d[li_reproduce].*
 
               LET g_gzxk_d[g_gzxk_d.getLength()].gzxk004 = NULL
 
            END IF
            
            #add-point:modify段before insert
            {<point name="input.body.before_insert"/>}
            #end add-point  
 
         AFTER INSERT
            LET l_insert = FALSE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1  
            SELECT COUNT(*) INTO l_count FROM gzxk_t 
             WHERE gzxkent = g_enterprise AND gzxk001 = g_gzxk_m.gzxk001
               AND gzxk002 = g_gzxk_m.gzxk002
               AND gzxk003 = g_gzxk_m.gzxk003
 
               AND gzxk004 = g_gzxk_d[l_ac].gzxk004
 
                
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               CALL s_transaction_begin()
               #add-point:單身新增前
               {<point name="input.body.b_insert" mark="Y"/>}
               #end add-point
               INSERT INTO gzxk_t
                           (gzxkent,
                            gzxk001,gzxk002,gzxk003,
                            gzxk004
                            ,gzxkstus,gzxk005,gzxk006,gzxkownid,gzxkowndp,gzxkcrtid,gzxkcrtdp,gzxkcrtdt,gzxkmodid,gzxkmoddt) 
                     VALUES(g_enterprise,
                            g_gzxk_m.gzxk001,g_gzxk_m.gzxk002,g_gzxk_m.gzxk003,
                            g_gzxk_d[l_ac].gzxk004
                            ,g_gzxk_d[l_ac].gzxkstus,g_gzxk_d[l_ac].gzxk005,g_gzxk_d[l_ac].gzxk006,g_gzxk2_d[l_ac].gzxkownid, 
                                g_gzxk2_d[l_ac].gzxkowndp,g_gzxk2_d[l_ac].gzxkcrtid,g_gzxk2_d[l_ac].gzxkcrtdp, 
                                g_gzxk2_d[l_ac].gzxkcrtdt,g_gzxk2_d[l_ac].gzxkmodid,g_gzxk2_d[l_ac].gzxkmoddt) 

               #add-point:單身新增中
               {<point name="input.body.m_insert"/>}
               #end add-point
               LET p_cmd = 'u'
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_gzxk_d[l_ac].* TO NULL
               CALL s_transaction_end('N','0')
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("gzxk_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')                    
               CANCEL INSERT
            ELSE
               #資料多語言用-增/改
               
               #add-point:input段-after_insert
               {<point name="input.body.a_insert"/>}
               #end add-point
               CALL s_transaction_end('Y','0')
               ERROR "INSERT O.K"
               LET g_rec_b=g_rec_b+1
               DISPLAY g_rec_b TO FORMONLY.cnt
            END IF
            
            #add-point:單身新增後
            {<point name="input.body.after_insert"/>}
            #end add-point
              
         BEFORE DELETE                            #是否取消單身
            IF l_cmd = 'a' AND g_gzxk_d.getLength() < l_ac THEN
               CALL FGL_SET_ARR_CURR(l_ac-1)
               CALL g_gzxk_d.deleteElement(l_ac)
               NEXT FIELD gzxk004
            END IF
            IF g_gzxk_d_t.gzxk004 IS NOT NULL
 
               THEN
               
               #add-point:單身刪除前
               {<point name="input.body.b_delete"/>}
               #end add-point
               
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               IF azzi914_before_delete() THEN 
                  CALL s_transaction_end('Y','0')
               ELSE 
                  CALL s_transaction_end('N','0')
                  CANCEL DELETE   
               END IF 
               CLOSE azzi914_bcl
               LET l_count = g_gzxk_d.getLength()
            END IF 
            
            #add-point:單身刪除後
            {<point name="input.body.a_delete"/>}
            #end add-point
              
         AFTER DELETE 
            #add-point:單身AFTER DELETE 
            {<point name="input.body.after_delete"/>}
            #end add-point
 
                  #此段落由子樣板a01產生
         BEFORE FIELD gzxkstus
            #add-point:BEFORE FIELD gzxkstus
            {<point name="input.b.page1.gzxkstus" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD gzxkstus
            
            #add-point:AFTER FIELD gzxkstus
            {<point name="input.a.page1.gzxkstus" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE gzxkstus
            #add-point:ON CHANGE gzxkstus
            {<point name="input.g.page1.gzxkstus" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD gzxk005
            #add-point:BEFORE FIELD gzxk005
            {<point name="input.b.page1.gzxk005" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD gzxk005
            
            #add-point:AFTER FIELD gzxk005
            {<point name="input.a.page1.gzxk005" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE gzxk005
            #add-point:ON CHANGE gzxk005
            {<point name="input.g.page1.gzxk005" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD gzxk004
            
            #add-point:AFTER FIELD gzxk004
            {<point name="input.a.page1.gzxk004" />}
            #END add-point
            
 
         #此段落由子樣板a01產生
         BEFORE FIELD gzxk004
            #add-point:BEFORE FIELD gzxk004
            {<point name="input.b.page1.gzxk004" />}
            #END add-point
 
         #此段落由子樣板a04產生
         ON CHANGE gzxk004
            #add-point:ON CHANGE gzxk004
            {<point name="input.g.page1.gzxk004" />}
            #END add-point
 
         #此段落由子樣板a01產生
         BEFORE FIELD gzxk006
            #add-point:BEFORE FIELD gzxk006
            {<point name="input.b.page1.gzxk006" />}
            #END add-point
 
         #此段落由子樣板a02產生
         AFTER FIELD gzxk006
            
            #add-point:AFTER FIELD gzxk006
            {<point name="input.a.page1.gzxk006" />}
            #END add-point
            
 
         #此段落由子樣板a04產生
         ON CHANGE gzxk006
            #add-point:ON CHANGE gzxk006
            {<point name="input.g.page1.gzxk006" />}
            #END add-point
 
 
                  #Ctrlp:input.c.page1.gzxkstus
         ON ACTION controlp INFIELD gzxkstus
            #add-point:ON ACTION controlp INFIELD gzxkstus
            {<point name="input.c.page1.gzxkstus" />}
            #END add-point
 
