  <section id="azzi914.delete" ver="1" status="" src="s">
    <![CDATA[#+ 資料刪除
PRIVATE FUNCTION azzi914_delete()
   DEFINE  l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE  l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE  l_vars          DYNAMIC ARRAY OF STRING
   DEFINE  l_fields        DYNAMIC ARRAY OF STRING
   DEFINE  l_var_keys_bak  DYNAMIC ARRAY OF STRING
   #add-point:delete段define
   {<point name="delete.define"/>}
   #end add-point     
   
   IF g_gzxk_m.gzxk001 IS NULL
   OR g_gzxk_m.gzxk002 IS NULL
   OR g_gzxk_m.gzxk003 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
 
   EXECUTE azzi914_master_referesh USING g_gzxk_m.gzxk001,g_gzxk_m.gzxk002,g_gzxk_m.gzxk003 INTO g_gzxk_m.gzxk001, 
       g_gzxk_m.gzxk002,g_gzxk_m.gzxk003,g_gzxk_m.gzxk002_desc
   
   CALL azzi914_show()
   
   CALL s_transaction_begin()
   
   
 
   OPEN azzi914_cl USING g_enterprise,g_gzxk_m.gzxk001
                                                       ,g_gzxk_m.gzxk002
                                                       ,g_gzxk_m.gzxk003
 
   IF STATUS THEN
      CALL cl_err("OPEN azzi914_cl:", STATUS, 1)
      CLOSE azzi914_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH azzi914_cl INTO g_gzxk_m.gzxk001,g_gzxk_m.gzxk002,g_gzxk_m.gzxk002_desc,g_gzxk_m.gzxk003
   
   #若資料已被他人LOCK
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_gzxk_m.gzxk001,SQLCA.sqlcode,0)
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #IF NOT cl_ask_delete() THEN             #確認一下
   IF cl_ask_del_master() THEN              #確認一下
      #+ 此段落由子樣板a47產生
      #刪除相關文件
      CALL azzi914_set_pk_array()
      #add-point:相關文件刪除前
      {<point name="delete.befroe.related_document_remove"/>}
      #end add-point   
      CALL cl_doc_remove()  
 
  
 
      #add-point:單身刪除前
      {<point name="delete.body.b_delete" mark="Y"/>}
      #end add-point
      
      DELETE FROM gzxk_t WHERE gzxkent = g_enterprise AND gzxk001 = g_gzxk_m.gzxk001
                                                               AND gzxk002 = g_gzxk_m.gzxk002
                                                               AND gzxk003 = g_gzxk_m.gzxk003
 
                                                               
      #add-point:單身刪除中
      {<point name="delete.body.m_delete"/>}
      #end add-point
      
      IF SQLCA.sqlcode THEN
         CALL cl_err("gzxk_t",SQLCA.sqlcode,0)
         CALL s_transaction_end('N','0')
      END IF
 
      
  
      #add-point:單身刪除後 
      {<point name="delete.body.a_delete"/>}
      #end add-point
      
 
      
      CLEAR FORM
      CALL g_gzxk_d.clear() 
      CALL g_gzxk2_d.clear()       
 
     
      CALL azzi914_ui_browser_refresh()  
      CALL azzi914_ui_headershow()  
      CALL azzi914_ui_detailshow()
       
      IF g_browser_cnt > 0 THEN 
         CALL azzi914_fetch('P')
      ELSE
         LET g_wc = " 1=1"
         LET g_wc2 = " 1=1"
         CALL azzi914_browser_fill("F")
      END IF
       
   END IF
 
   CLOSE azzi914_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-D
   CALL cl_flow_notify(g_gzxk_m.gzxk001,'D')
    
END FUNCTION
