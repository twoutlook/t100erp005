  <section id="azzi914.insert" ver="1" status="" src="s">
    <![CDATA[#+ 資料新增
PRIVATE FUNCTION azzi914_insert()
   #add-point:insert段define
   {<point name="insert.define"/>}
   #end add-point    
   
   #add-point:insert段before
   {<point name="insert.before"/>}
   #end add-point    
   
   #清除相關資料
   CLEAR FORM                    
   CALL g_gzxk_d.clear()
   CALL g_gzxk2_d.clear()
 
 
   INITIALIZE g_gzxk_m.* LIKE gzxk_t.*             #DEFAULT 設定
   LET g_gzxk001_t = NULL
   LET g_gzxk002_t = NULL
   LET g_gzxk003_t = NULL
 
   CALL s_transaction_begin()
   WHILE TRUE
     
      #單頭預設值
      
     
      #add-point:單頭預設值
      {<point name="insert.default"/>}
      #end add-point 
 
      CALL azzi914_input("a")
      
      #add-point:單頭輸入後
      {<point name="insert.after_insert"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_gzxk_m.* = g_gzxk_m_t.*
         CALL azzi914_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      CALL g_gzxk_d.clear()
      CALL g_gzxk2_d.clear()
 
      
      #add-point:單頭輸入後2
      {<point name="insert.after_insert2"/>}
      #end add-point
 
      LET g_rec_b = 0
