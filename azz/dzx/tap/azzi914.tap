  <section id="azzi914.modify" ver="1" status="" src="s">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION azzi914_modify()
   #add-point:modify段define
   {<point name="modify.define"/>}
   #end add-point    
   
   IF g_gzxk_m.gzxk001 IS NULL
   OR g_gzxk_m.gzxk002 IS NULL
   OR g_gzxk_m.gzxk003 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF
   
   EXECUTE azzi914_master_referesh USING g_gzxk_m.gzxk001,g_gzxk_m.gzxk002,g_gzxk_m.gzxk003 INTO g_gzxk_m.gzxk001, 
       g_gzxk_m.gzxk002,g_gzxk_m.gzxk003,g_gzxk_m.gzxk002_desc
 
   ERROR ""
  
   LET g_gzxk001_t = g_gzxk_m.gzxk001
   LET g_gzxk002_t = g_gzxk_m.gzxk002
   LET g_gzxk003_t = g_gzxk_m.gzxk003
 
   CALL s_transaction_begin()
   
   OPEN azzi914_cl USING g_enterprise,g_gzxk_m.gzxk001
                                                       ,g_gzxk_m.gzxk002
                                                       ,g_gzxk_m.gzxk003
 
   IF STATUS THEN
      CALL cl_err("OPEN azzi914_cl:", STATUS, 1)
      CLOSE azzi914_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH azzi914_cl INTO g_gzxk_m.gzxk001,g_gzxk_m.gzxk002,g_gzxk_m.gzxk002_desc,g_gzxk_m.gzxk003
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err(g_gzxk_m.gzxk001,SQLCA.sqlcode,0)
      CLOSE azzi914_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   CALL s_transaction_end('Y','0')
 
   CALL azzi914_show()
   WHILE TRUE
      LET g_gzxk001_t = g_gzxk_m.gzxk001
      LET g_gzxk002_t = g_gzxk_m.gzxk002
      LET g_gzxk003_t = g_gzxk_m.gzxk003
 
 
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      CALL azzi914_input("u")     #欄位更改
      
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_gzxk_m.* = g_gzxk_m_t.*
         CALL azzi914_show()
         CALL cl_err('',9001,0)
         EXIT WHILE
      END IF
      
      #若單頭key欄位有變更
      IF g_gzxk_m.gzxk001 != g_gzxk001_t 
      OR g_gzxk_m.gzxk002 != g_gzxk002_t 
      OR g_gzxk_m.gzxk003 != g_gzxk003_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單頭(偽)修改前
         {<point name="modify.b_key_update" mark="Y"/>}
         #end add-point
         
         #更新單頭key值
         UPDATE gzxk_t SET gzxk001 = g_gzxk_m.gzxk001
                                       , gzxk002 = g_gzxk_m.gzxk002
                                       , gzxk003 = g_gzxk_m.gzxk003
 
          WHERE gzxkent = g_enterprise AND gzxk001 = g_gzxk001_t
            AND gzxk002 = g_gzxk002_t
            AND gzxk003 = g_gzxk003_t
 
         #add-point:單頭(偽)修改中
         {<point name="modify.m_key_update"/>}
         #end add-point
         
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               CALL cl_err("gzxk_t","std-00009",1)
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               CALL cl_err("gzxk_t",SQLCA.sqlcode,1)  
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
 
 
         
         #add-point:單頭(偽)修改後
         {<point name="modify.a_key_update"/>}
         #end add-point
         
      END IF
      
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   #IF NOT cl_log_modified_record(g_gzxk_m.gzxk001,g_site) THEN 
   #   CALL s_transaction_end('N','0')
   #END IF
 
   CLOSE azzi914_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_gzxk_m.gzxk001,'U')
 
   CALL azzi914_b_fill("1=1")
   
END FUNCTION   
]]>
  </section>
