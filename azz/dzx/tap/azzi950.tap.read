PRIVATE FUNCTION azzi950_init_exec_time_info()
   #月份
    CALL azzi950_init_month_info("N","0")
    #第幾週
    CALL azzi950_init_week_info("N","0")
    #星期日-星期六
    CALL azzi950_init_weekday_info("N")
    #日 1-31
    CALL azzi950_init_day_info("N","0")

    #指定執行時間  
    CALL azzi950_init_assign_exec_info("1",NULL,"N")
END FUNCTION]]>
