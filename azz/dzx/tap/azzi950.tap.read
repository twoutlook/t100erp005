  <point name="function.azzi950_extra_process_assign_exec" order="4" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 額外處理時段一(sc1) 時段二(sc2) 時段三(sc3)
# Memo...........:
# Usage..........: CALL azzi950_extra_process_assign_exec()
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION azzi950_extra_process_assign_exec()
    DEFINE li_i LIKE type_t.num5
   
   FOR li_i =  1 TO 3

       IF li_i = 1 THEN 
          IF NOT cl_null(g_gzpa_m.gzpa017) OR NOT cl_null(g_gzpa_m.gzpa018) OR 
               NOT cl_null(g_gzpa_m.gzpa019) OR NOT cl_null(g_gzpa_m.gzpa020) THEN 
            
             LET g_gzpa_m.sc1 = "Y"
             LET g_gzpa_m.gzpa015 = "2"   
          END IF 
                
       END IF 

       IF li_i = 2 THEN 

           IF NOT cl_null(g_gzpa_m.gzpa021) OR NOT cl_null(g_gzpa_m.gzpa022) OR 
                    NOT cl_null(g_gzpa_m.gzpa023) OR NOT cl_null(g_gzpa_m.gzpa024) THEN  

                    LET g_gzpa_m.sc2 = "Y"
                    LET g_gzpa_m.gzpa015 = "2"     
           END IF
       END IF

       IF li_i = 3 THEN 
             IF NOT cl_null(g_gzpa_m.gzpa025) OR NOT cl_null(g_gzpa_m.gzpa026) OR 
                 NOT cl_null(g_gzpa_m.gzpa027) OR NOT cl_null(g_gzpa_m.gzpa028) THEN 
               
                LET g_gzpa_m.sc3 = "Y" 
                LET g_gzpa_m.gzpa015 = "2"    
             END IF
       END IF 
             
    END FOR
END FUNCTION]]>
  </point>
  <point name="function.azzi950_init_info" order="5" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 一開始初始資料
# Memo...........:
# Usage..........: CALL azzi950_init_info(ps_part,ps_value)
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION azzi950_init_info(ps_part,ps_value)
   DEFINE ps_value STRING  
   DEFINE ps_part  STRING 

    
   IF ps_part = "all" THEN 
      CLEAR FORM
      INITIALIZE g_schedule.* TO NULL
      CALL g_gzpb2_d.clear()  
   END IF 
   
   LET g_gzpa_m.gzpa003 = ps_value
   
   #指定時間執行
   CALL azzi950_init_assign_time_info()
   #執行時間設定  
   CALL azzi950_init_exec_time_info()
   #特殊指定 
   CALL azzi950_init_special_info("N","N")
END FUNCTION]]>
  </point>
  <point name="function.azzi950_init_assign_time_info" order="6" ver="1" cite_std="N" new="Y" status="" src="s" readonly="" mark_hard="N">
    <![CDATA[################################################################################
# Descriptions...: 指定時間執行
# Memo...........:
# Usage..........: CALL azzi950_init_assign_time_info()
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
#                : 传入参数变量2   传入参数变量说明2
# Return code....: 回传参数变量1   回传参数变量说明1
#                : 回传参数变量2   回传参数变量说明2
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION azzi950_init_assign_time_info()
   
   IF g_gzpa_m.gzpa003 = "2" THEN 
      LET g_gzpa_m.gzpa006 = cl_get_current()
      LET g_gzpa_m.gzpa007 = TIME(CURRENT) 
   ELSE
      INITIALIZE g_gzpa_m.gzpa006 TO NULL
      INITIALIZE g_gzpa_m.gzpa007 TO NULL 
   END IF
END FUNCTION]]>
