      IF g_detail_idx = 0 AND g_gzpb_d.getLength() <> 0 THEN
         LET g_detail_idx = 1
      END IF
      DISPLAY g_detail_idx TO FORMONLY.idx
      DISPLAY g_gzpb_d.getLength() TO FORMONLY.cnt
