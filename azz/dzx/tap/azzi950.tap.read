  <section id="azzi950.modify" ver="24" status="" src="s" readonly="">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION azzi950_modify()
   DEFINE l_new_key    DYNAMIC ARRAY OF STRING
   DEFINE l_old_key    DYNAMIC ARRAY OF STRING
   DEFINE l_field_key  DYNAMIC ARRAY OF STRING
   DEFINE l_wc2_table1          STRING
 
   DEFINE l_wc2_table2   STRING
 
 
   #add-point:modify段define
   {<point name="modify.define" edit="s"/>}
   #end add-point    
   #add-point:modify段define(客製用)
   {<point name="modify.define_customerization" edit="c"/>}
   #end add-point    
   
   #保存單頭舊值
   LET g_gzpa_m_t.* = g_gzpa_m.*
   LET g_gzpa_m_o.* = g_gzpa_m.*
   
   IF g_gzpa_m.gzpa001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   ERROR ""
  
   LET g_gzpa001_t = g_gzpa_m.gzpa001
 
   CALL s_transaction_begin()
   
   OPEN azzi950_cl USING g_enterprise,g_gzpa_m.gzpa001
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN azzi950_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      CLOSE azzi950_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE azzi950_master_referesh USING g_gzpa_m.gzpa001 INTO g_gzpa_m.gzpa001,g_gzpa_m.gzpa004,g_gzpa_m.gzpa002, 
       g_gzpa_m.gzpa005,g_gzpa_m.gzpa003,g_gzpa_m.gzpastus,g_gzpa_m.gzpa006,g_gzpa_m.gzpa007,g_gzpa_m.gzpa008, 
       g_gzpa_m.gzpa010,g_gzpa_m.gzpa012,g_gzpa_m.gzpa015,g_gzpa_m.gzpa016,g_gzpa_m.gzpa017,g_gzpa_m.gzpa018, 
       g_gzpa_m.gzpa019,g_gzpa_m.gzpa020,g_gzpa_m.gzpa021,g_gzpa_m.gzpa022,g_gzpa_m.gzpa023,g_gzpa_m.gzpa024, 
       g_gzpa_m.gzpa025,g_gzpa_m.gzpa026,g_gzpa_m.gzpa027,g_gzpa_m.gzpa028,g_gzpa_m.gzpa029,g_gzpa_m.gzpa030, 
       g_gzpa_m.gzpa031,g_gzpa_m.gzpaownid,g_gzpa_m.gzpaowndp,g_gzpa_m.gzpacrtid,g_gzpa_m.gzpacrtdp, 
       g_gzpa_m.gzpacrtdt,g_gzpa_m.gzpamodid,g_gzpa_m.gzpamoddt,g_gzpa_m.gzpaownid_desc,g_gzpa_m.gzpaowndp_desc, 
       g_gzpa_m.gzpacrtid_desc,g_gzpa_m.gzpacrtdp_desc,g_gzpa_m.gzpamodid_desc
   
   #檢查是否允許此動作
   IF NOT azzi950_action_chk() THEN
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_gzpa_m_mask_o.* =  g_gzpa_m.*
   CALL azzi950_gzpa_t_mask()
   LET g_gzpa_m_mask_n.* =  g_gzpa_m.*
   
   
   
   #add-point:modify段show之前
   {<point name="modify.before_show"/>}
   #end add-point  
   
   #LET l_wc2_table1 = g_wc2_table1
   #LET g_wc2_table1 = " 1=1"
 
   #LET l_wc2_table2 = g_wc2_table2
   #LET l_wc2_table2 = " 1=1"
 
 
   
   CALL azzi950_show()
   #add-point:modify段show之後
   {<point name="modify.after_show"/>}
   #end add-point
   
   #LET g_wc2_table1 = l_wc2_table1
 
   #LET  g_wc2_table2 = l_wc2_table2 
 
 
    
   WHILE TRUE
      LET g_gzpa001_t = g_gzpa_m.gzpa001
 
      
      #寫入修改者/修改日期資訊(單頭)
      LET g_gzpa_m.gzpamodid = g_user 
LET g_gzpa_m.gzpamoddt = cl_get_current()
LET g_gzpa_m.gzpamodid_desc = cl_get_username(g_gzpa_m.gzpamodid)
      
      #add-point:modify段修改前
      {<point name="modify.before_input"/>}
      #end add-point
      
      #欄位更改
      LET g_loc = 'n'
      LET g_update = FALSE
      CALL azzi950_input("u")
      LET g_loc = 'n'
 
      #add-point:modify段修改後
      {<point name="modify.after_input"/>}
      #end add-point
      
      IF g_update OR NOT INT_FLAG THEN
         #若有modid跟moddt則進行update
         UPDATE gzpa_t SET (gzpamodid,gzpamoddt) = (g_gzpa_m.gzpamodid,g_gzpa_m.gzpamoddt)
          WHERE gzpaent = g_enterprise AND gzpa001 = g_gzpa001_t
 
      END IF
    
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_gzpa_m.* = g_gzpa_m_t.*
         CALL azzi950_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = '' 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         CALL s_transaction_end('N','0')
         RETURN
      END IF 
                  
      #若單頭key欄位有變更
      IF g_gzpa_m.gzpa001 != g_gzpa001_t 
 
      THEN
         CALL s_transaction_begin()
         
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update" mark="Y"/>}
         #end add-point
         
         #更新單身key值
         UPDATE gzpb_t SET gzpb001 = g_gzpa_m.gzpa001
 
          WHERE gzpbent = g_enterprise AND gzpb001 = g_gzpa001_t
 
            
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update"/>}
         #end add-point
 
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "gzpb_t" 
               LET g_errparam.code   = "std-00009" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "gzpb_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update"/>}
         #end add-point
         
 
         
         #更新單身key值
         #add-point:單身fk修改前
         {<point name="modify.body.b_fk_update2" mark="Y"/>}
         #end add-point
         UPDATE gzpe_t
            SET gzpe001 = g_gzpa_m.gzpa001
 
          WHERE gzpeent = g_enterprise AND
                gzpe001 = g_gzpa001_t
 
         #add-point:單身fk修改中
         {<point name="modify.body.m_fk_update2"/>}
         #end add-point
         CASE
            WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "gzpe_t" 
               LET g_errparam.code   = "std-00009" 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
            WHEN SQLCA.sqlcode #其他錯誤
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = "gzpe_t" 
               LET g_errparam.code   = SQLCA.sqlcode 
               LET g_errparam.popup  = TRUE 
               CALL cl_err()
               CALL s_transaction_end('N','0')
               CONTINUE WHILE
         END CASE
         #add-point:單身fk修改後
         {<point name="modify.body.a_fk_update2"/>}
         #end add-point
 
 
         
         #UPDATE 多語言table key值
         
         
 
         CALL s_transaction_end('Y','0')
      END IF
    
      EXIT WHILE
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,modify_detail,delete,reproduce", TRUE)
   CALL azzi950_set_act_visible()   
   CALL azzi950_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " gzpaent = '" ||g_enterprise|| "' AND",
                      " gzpa001 = '", g_gzpa_m.gzpa001, "' "
 
   #填到對應位置
   CALL azzi950_browser_fill("")
 
   CLOSE azzi950_cl
   
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL azzi950_msgcentre_notify('')
 
END FUNCTION   
]]>
