  <section id="azzi840.update_b" ver="2" status="" src="s">
<![CDATA[#+ 修改單身後其他table連動
PRIVATE FUNCTION azzi840_update_b(ps_table,ps_keys,ps_keys_bak,ps_page)
   DEFINE ps_page          STRING
   DEFINE ps_table         STRING
   DEFINE ps_keys          DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ps_keys_bak      DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ls_group         STRING
   DEFINE li_idx           LIKE type_t.num5 
   DEFINE lb_chk           BOOLEAN
   DEFINE l_new_key        DYNAMIC ARRAY OF STRING
   DEFINE l_old_key        DYNAMIC ARRAY OF STRING
   DEFINE l_field_key      DYNAMIC ARRAY OF STRING
   #add-point:update_b段define
   {<point name="update_b.define"/>}
   #end add-point     
   
   #判斷key是否有改變
   LET lb_chk = TRUE
   FOR li_idx = 1 TO ps_keys.getLength()
      IF ps_keys[li_idx] <> ps_keys_bak[li_idx] THEN
         LET lb_chk = FALSE
         EXIT FOR
      END IF
   END FOR
   
   #不需要做處理
   IF lb_chk THEN
      RETURN
   END IF
    
   #判斷是否是同一群組的table
   LET ls_group = "gzye_t,"
   IF ls_group.getIndexOf(ps_table,1) > 0 AND ps_table <> "gzye_t" THEN
      #add-point:update_b段修改前
      {<point name="update_b.b_update" mark="Y"/>}
      #end add-point     
      UPDATE gzye_t 
         SET (gzye001
              ,gzyestus,gzye002,gzye003,gzye004,gzye005,gzyeownid,gzyeowndp,gzyecrtid,gzyecrtdp,gzyecrtdt,gzyemodid,gzyemoddt) 
              = 
             (ps_keys[1]
              ,g_gzye_d[l_ac].gzyestus,g_gzye_d[l_ac].gzye002,g_gzye_d[l_ac].gzye003,g_gzye_d[l_ac].gzye004, 
                  g_gzye_d[l_ac].gzye005,g_gzye2_d[l_ac].gzyeownid,g_gzye2_d[l_ac].gzyeowndp,g_gzye2_d[l_ac].gzyecrtid, 
                  g_gzye2_d[l_ac].gzyecrtdp,g_gzye2_d[l_ac].gzyecrtdt,g_gzye2_d[l_ac].gzyemodid,g_gzye2_d[l_ac].gzyemoddt)  

         WHERE gzye001 = ps_keys_bak[1]
      #add-point:update_b段修改中
      {<point name="update_b.m_update"/>}
      #end add-point 
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            CALL cl_err("gzye_t","std-00009",1)
            CALL s_transaction_end('N','0')
         WHEN SQLCA.sqlcode #其他錯誤
            CALL cl_err("gzye_t",SQLCA.sqlcode,1)  
            CALL s_transaction_end('N','0')
         OTHERWISE
            
      END CASE
      #add-point:update_b段修改後
      {<point name="update_b.a_update"/>}
      #end add-point 
      RETURN
   END IF
   
 
   
END FUNCTION
]]>
