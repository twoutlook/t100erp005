PRIVATE FUNCTION azzi840_insert_b(ps_table,ps_keys,ps_page)
   DEFINE ps_table    STRING
   DEFINE ps_page     STRING
   DEFINE ps_keys     DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ls_group    STRING
   #add-point:insert_b段define
   {<point name="insert_b.define"/>}
   #end add-point     
 
   #判斷是否是同一群組的table
   LET ls_group = "gzye_t,"
   #IF ls_group.getIndexOf(ps_table,1) > 0 THEN
      
      #add-point:insert_b段新增前
      {<point name="insert_b.b_insert" mark="Y"/>}
      #end add-point    
      INSERT INTO gzye_t
                  (gzyeent,
                   gzye001
                   ,gzyestus,gzye002,gzye003,gzye004,gzye005,gzyeownid,gzyeowndp,gzyecrtid,gzyecrtdp,gzyecrtdt,gzyemodid,gzyemoddt) 
            VALUES(g_enterprise,
                   ps_keys[1]
                   ,g_gzye_d[l_ac].gzyestus,g_gzye_d[l_ac].gzye002,g_gzye_d[l_ac].gzye003,g_gzye_d[l_ac].gzye004, 
                       g_gzye_d[l_ac].gzye005,g_gzye2_d[l_ac].gzyeownid,g_gzye2_d[l_ac].gzyeowndp,g_gzye2_d[l_ac].gzyecrtid, 
                       g_gzye2_d[l_ac].gzyecrtdp,g_gzye2_d[l_ac].gzyecrtdt,g_gzye2_d[l_ac].gzyemodid, 
                       g_gzye2_d[l_ac].gzyemoddt)
      #add-point:insert_b段新增中
      {<point name="insert_b.m_insert"/>}
      #end add-point    
      IF SQLCA.sqlcode THEN
         CALL cl_err("gzye_t",SQLCA.sqlcode,0)
      END IF
      #add-point:insert_b段新增後
      {<point name="insert_b.a_insert"/>}
      #end add-point    
   #END IF
   
 
   
END FUNCTION
]]>
