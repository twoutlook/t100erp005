  <section id="azzi840.b_fill" ver="3" status="" src="s">
<![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION azzi840_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2    STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE gzyestus,gzye001,gzye002,gzye003,gzye004,gzye005,gzye001,gzyeownid,gzyeowndp, 
       gzyecrtid,gzyecrtdp,gzyecrtdt,gzyemodid,gzyemoddt ,t1.ooag011 ,t2.ooefl003 ,t3.ooag011 ,t4.ooefl003 , 
       t5.ooag011 FROM gzye_t",
               "",
                              " LEFT JOIN ooag_t t1 ON t1.ooagent='"||g_enterprise||"' AND t1.ooag001=gzyeownid  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent='"||g_enterprise||"' AND t2.ooefl001=gzyeowndp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent='"||g_enterprise||"' AND t3.ooag001=gzyecrtid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent='"||g_enterprise||"' AND t4.ooefl001=gzyecrtdp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent='"||g_enterprise||"' AND t5.ooag001=gzyemodid  ",
 
               " WHERE gzyeent= ? AND 1=1 AND ", p_wc2 
    
   LET g_sql = g_sql, cl_sql_add_filter("gzye_t"),
                      " ORDER BY gzye_t.gzye001"
   
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE azzi840_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR azzi840_pb
   
   OPEN b_fill_curs USING g_enterprise
 
   CALL g_gzye_d.clear()
   CALL g_gzye2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_gzye_d[l_ac].gzyestus,g_gzye_d[l_ac].gzye001,g_gzye_d[l_ac].gzye002,g_gzye_d[l_ac].gzye003, 
       g_gzye_d[l_ac].gzye004,g_gzye_d[l_ac].gzye005,g_gzye2_d[l_ac].gzye001,g_gzye2_d[l_ac].gzyeownid, 
       g_gzye2_d[l_ac].gzyeowndp,g_gzye2_d[l_ac].gzyecrtid,g_gzye2_d[l_ac].gzyecrtdp,g_gzye2_d[l_ac].gzyecrtdt, 
       g_gzye2_d[l_ac].gzyemodid,g_gzye2_d[l_ac].gzyemoddt,g_gzye2_d[l_ac].gzyeownid_desc,g_gzye2_d[l_ac].gzyeowndp_desc, 
       g_gzye2_d[l_ac].gzyecrtid_desc,g_gzye2_d[l_ac].gzyecrtdp_desc,g_gzye2_d[l_ac].gzyemodid_desc
      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL azzi840_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec THEN
      IF g_error_show = 1 THEN
         CALL cl_err("gzye_t",9035,1)
      END IF
   END IF 
   LET g_error_show = 0
   
 
   
   CALL g_gzye_d.deleteElement(g_gzye_d.getLength())   
   CALL g_gzye2_d.deleteElement(g_gzye2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_gzye_d.getLength()
      LET g_gzye2_d[l_ac].gzye001 = g_gzye_d[l_ac].gzye001 
 
   END FOR
   
   IF g_cnt > g_gzye_d.getLength() THEN
      LET l_ac = g_gzye_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_gzye_d.getLength()
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE azzi840_pb
   
END FUNCTION
]]>
</section>
