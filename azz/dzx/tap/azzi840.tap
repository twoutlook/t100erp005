LET g_gzye2_d[l_ac].gzyemoddt = cl_get_current()
 
            
               #add-point:單身修改前
               {<point name="input.body.b_update" mark="Y"/>}
               #end add-point
               
               UPDATE gzye_t SET (gzyestus,gzye001,gzye002,gzye003,gzye004,gzye005,gzyeownid,gzyeowndp, 
                   gzyecrtid,gzyecrtdp,gzyecrtdt,gzyemodid,gzyemoddt) = (g_gzye_d[l_ac].gzyestus,g_gzye_d[l_ac].gzye001, 
                   g_gzye_d[l_ac].gzye002,g_gzye_d[l_ac].gzye003,g_gzye_d[l_ac].gzye004,g_gzye_d[l_ac].gzye005, 
                   g_gzye2_d[l_ac].gzyeownid,g_gzye2_d[l_ac].gzyeowndp,g_gzye2_d[l_ac].gzyecrtid,g_gzye2_d[l_ac].gzyecrtdp, 
                   g_gzye2_d[l_ac].gzyecrtdt,g_gzye2_d[l_ac].gzyemodid,g_gzye2_d[l_ac].gzyemoddt)
                WHERE gzyeent = g_enterprise AND
                  gzye001 = g_gzye_d_t.gzye001 #項次   
 
                  
               #add-point:單身修改中
               {<point name="input.body.m_update"/>}
               #end add-point
                  
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL cl_err("gzye_t","std-00009",1)
                     CALL s_transaction_end('N','0')
                    WHEN SQLCA.sqlcode #其他錯誤
                     CALL cl_err("gzye_t",SQLCA.sqlcode,1)  
                     CALL s_transaction_end('N','0')
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_gzye_d[g_detail_idx].gzye001
               LET gs_keys_bak[1] = g_gzye_d_t.gzye001
               CALL azzi840_update_b('gzye_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
                     LET g_log1 = util.JSON.stringify(g_gzye_d_t)
                     LET g_log2 = util.JSON.stringify(g_gzye_d[l_ac])
                     IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                        CALL s_transaction_end('N','0')
                     END IF
               END CASE
               
               #add-point:單身修改後
               {<point name="input.body.a_update"/>}
               #end add-point
 
            END IF
            
         AFTER ROW
            CALL azzi840_unlock_b("gzye_t")
            CALL s_transaction_end('Y','0')
            #其他table進行unlock
            
             #add-point:單身after row
            {<point name="input.body.a_row"/>}
            #end add-point
            
         AFTER INPUT
            #add-point:單身input後
            {<point name="input.body.a_input"/>}
            #end add-point
            
         ON ACTION controlo   
            CALL FGL_SET_ARR_CURR(g_gzye_d.getLength()+1)
            LET lb_reproduce = TRUE
            LET li_reproduce = l_ac
            LET li_reproduce_target = g_gzye_d.getLength()+1
            
      END INPUT
      
 
      
      DISPLAY ARRAY g_gzye2_d TO s_detail2.*
         ATTRIBUTES(COUNT=g_detail_cnt)  
      
         BEFORE DISPLAY 
            CALL azzi840_b_fill(g_wc2)
            CALL FGL_SET_ARR_CURR(g_detail_idx)
      
         BEFORE ROW
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail2")
            LET l_ac = g_detail_idx
            LET g_temp_idx = l_ac
            DISPLAY g_detail_idx TO FORMONLY.idx
            CALL cl_show_fld_cont() 
            
         #add-point:page2自定義行為
         {<point name="input.body2.action"/>}
         #end add-point
            
      END DISPLAY
 
      
      #add-point:before_more_input
      {<point name="input.more_input"/>}
      #end add-point
      
      BEFORE DIALOG 
         IF g_temp_idx > 0 THEN
            LET l_ac = g_temp_idx
            CALL DIALOG.setCurrentRow("s_detail1",l_ac)
            LET g_temp_idx = 1
         END IF
         LET g_curr_diag = ui.DIALOG.getCurrent()
         #add-point:before_dialog
         {<point name="input.before_dialog"/>}
         #end add-point
         CASE g_aw
            WHEN "s_detail1"
               NEXT FIELD gzyestus
            WHEN "s_detail2"
               NEXT FIELD gzye001_2
 
         END CASE
   
      ON ACTION accept
         ACCEPT DIALOG
      
      ON ACTION cancel
         LET INT_FLAG = TRUE 
         EXIT DIALOG
 
      ON ACTION controlr
         CALL cl_show_req_fields()
 
      ON ACTION controlf
         CALL cl_set_focus_form(ui.Interface.getRootNode()) 
              RETURNING g_fld_name,g_frm_name 
         CALL cl_fldhelp(g_frm_name,g_fld_name,g_lang) 
           
      #交談指令共用ACTION
      &include "common_action.4gl"
         CONTINUE DIALOG
   
   END DIALOG 
   
   #add-point:modify段修改後
   {<point name="modify.after_input"/>}
   #end add-point
 
   CLOSE azzi840_bcl
   CALL s_transaction_end('Y','0')
   
END FUNCTION
]]>
</section>
