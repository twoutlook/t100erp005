  <point name="function.azzi840_chk_gzye005" cite_std="N" status="" ver="1" src="s" new="Y" order="1" mark_hard="N">
<![CDATA[
################################################################################
# Descriptions...: 檢核遮罩方法限制遮罩起始位置
# Memo...........:
# Usage..........: CALL azzi840_chk_gzye005()
#                  RETURNING 
# Input parameter: 
#                : 
# Return code....: 
#                : 
# Date & Author..: 日期 By 作者
# Modify.........:
################################################################################
PRIVATE FUNCTION azzi840_chk_gzye005()

   IF g_gzye_d[g_detail_idx].gzye004 = 'L' OR g_gzye_d[g_detail_idx].gzye004 = 'R' OR 
      g_gzye_d[g_detail_idx].gzye004 = 'M' THEN
      IF NOT cl_chk_num(g_gzye_d[g_detail_idx].gzye005,'N') THEN 
         CALL cl_err('',"lib-00128",1)
         RETURN FALSE  
      END IF  
      IF g_gzye_d[g_detail_idx].gzye004 = 'L' OR g_gzye_d[g_detail_idx].gzye004 = 'R' THEN 
         #L R 至少 遮罩字元 > 1
         IF g_gzye_d[g_detail_idx].gzye005 < 2 THEN 
             CALL cl_err_msg("","azz-00216","1",1)
             RETURN FALSE
         END IF  
      ELSE 
         #M 至少 遮罩字元 >0 遮罩字數至少>(%)
         IF g_gzye_d[g_detail_idx].gzye005 <= 0 THEN 
             CALL cl_err_msg("","azz-00216","0",1)
             RETURN FALSE
         END IF  
      END IF 
   ELSE 
     #C 不管控
   END IF 
   RETURN TRUE 
END FUNCTION]]>
