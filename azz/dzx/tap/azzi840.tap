  <section id="azzi840.lock_b" ver="1" status="" src="s">
<![CDATA[#+ 連動lock其他單身table資料
PRIVATE FUNCTION azzi840_lock_b(ps_table)
 
   DEFINE ps_table STRING
   DEFINE ls_group STRING
   #add-point:lock_b段define
   {<point name="lock_b.define"/>}
   #end add-point   
   
   #先刷新資料
   #CALL azzi840_b_fill(g_wc2)
   
   #鎖定整組table
   #LET ls_group = ""
   #僅鎖定自身table
   LET ls_group = "gzye_t"
   
   IF ls_group.getIndexOf(ps_table,1) THEN
   
      OPEN azzi840_bcl USING g_enterprise,
                                       g_gzye_d[g_detail_idx].gzye001
                                       
      IF SQLCA.sqlcode THEN
         CALL cl_err("azzi840_bcl",SQLCA.sqlcode,1)
         RETURN FALSE
      END IF
   
   END IF
                                    
 
   
   RETURN TRUE
 
END FUNCTION
]]>
