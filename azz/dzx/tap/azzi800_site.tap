  <point name="function.azzi800_site_b_fill" cite_std="N" status="u" env="" ver="" src="" new="Y">
<![CDATA[
PRIVATE FUNCTION azzi800_site_b_fill(pc_gzxb001)
DEFINE pc_gzxb001    LIKE gzxb_t.gzxb001
DEFINE ls_sql           STRING
DEFINE l_ac          LIKE type_t.num5
         
   LET ls_sql = " SELECT gzxbstus,gzxb002,gzxb003,",
                " gzxb004,gzxb005, " ,
                " gzxb002 ,gzxb003, "  ,
                " gzxbownid, ", 
                " gzxbowndp, ",
                " gzxbcrtid, ",
                " gzxbcrtdp, ",
                " gzxbcrtdt,", 
                " gzxbmodid, ",
                " gzxbmoddt ",
                " FROM gzxb_t ", 
                " WHERE gzxbent=",g_enterprise," AND gzxb001 = '", pc_gzxb001,"'" 
    
   LET ls_sql = ls_sql, " ORDER BY gzxb_t.gzxb001"

   #DISPLAY "ls_sql:",ls_sql
   PREPARE azzi800_site_pb FROM ls_sql
   DECLARE b_fill_curs CURSOR FOR azzi800_site_pb
   
   CALL g_gzxb_d.clear()
   CALL g_gzxb2_d.clear()
   
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 

   FOREACH b_fill_curs INTO g_gzxb_d[l_ac].gzxbstus,g_gzxb_d[l_ac].gzxb002,g_gzxb_d[l_ac].gzxb003,
           g_gzxb_d[l_ac].gzxb004,g_gzxb_d[l_ac].gzxb005,
           g_gzxb2_d[l_ac].gzxb002,
           g_gzxb2_d[l_ac].gzxb003,
           g_gzxb2_d[l_ac].gzxbownid,
           g_gzxb2_d[l_ac].gzxbowndp,
           g_gzxb2_d[l_ac].gzxbcrtid,
           g_gzxb2_d[l_ac].gzxbcrtdp,
           g_gzxb2_d[l_ac].gzxbcrtdt,
           g_gzxb2_d[l_ac].gzxbmodid,
           g_gzxb2_d[l_ac].gzxbmoddt

      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF

      #CALL azzi800_site_detail_show()      
      LET l_ac = l_ac + 1
      IF g_cnt > g_max_rec THEN
         CALL cl_err( "", 9035, 0 )
         EXIT FOREACH
      END IF
      
   END FOREACH
   CALL g_gzxb_d.deleteElement(g_gzxb_d.getLength())   
   CALL g_gzxb2_d.deleteElement(g_gzxb2_d.getLength())  
 
   LET g_detail_cnt = l_ac - 1
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   DISPLAY g_detail_cnt TO FORMONLY.idx
   LET l_ac = g_cnt
   LET g_cnt = 0
   CALL azzi800_site_show()
   CLOSE b_fill_curs
   FREE azzi800_site_pb

END FUNCTION]]>
</point>
