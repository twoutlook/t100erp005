  <point name="input.define" cite_std="N" status="u" env="" ver="1" src="" new="Y">
<![CDATA[   DEFINE  pc_gzxb001      LIKE gzxb_t.gzxb001
   DEFINE  l_n             LIKE type_t.num5    #檢查重複用  
   DEFINE  l_cnt           LIKE type_t.num5    #檢查重複用  
   DEFINE  l_lock_sw       LIKE type_t.chr1    #單身鎖住否  
   DEFINE  l_i             LIKE type_t.num5
   DEFINE  ls_return       STRING
   DEFINE  l_gzxa002       LIKE gzxa_t.gzxa002
   DEFINE  l_gzxa003       LIKE gzxa_t.gzxa003
   DEFINE  l_oofa011       LIKE oofa_t.oofa011]]>
</point>
  <point name="input.pre_input" cite_std="N" status="u" env="" ver="1" src="" new="Y">
<![CDATA[    LET g_forupd_sql = "SELECT gzxbstus,gzxb002,gzxb003,gzxb004,gzxb005
                      ,gzxbmodid,gzxbmoddt,gzxbownid,gzxbowndp,gzxbcrtid, 
                      gzxbcrtdp,gzxbcrtdt 
                      FROM gzxb_t WHERE gzxbent=? AND gzxb001=? 
                      AND gzxb002=? AND gzxb003=? FOR UPDATE"
   LET g_forupd_sql = cl_forupd_sql(g_forupd_sql)
   DECLARE azzi800_site_bcl CURSOR FROM g_forupd_sql      # LOCK CURSOR
   
   LET gc_gzxb001 = pc_gzxb001
   SELECT gzxa002,gzxa003 INTO l_gzxa002,l_gzxa003 FROM gzxa_t WHERE gzxaent=g_enterprise AND gzxa001=pc_gzxb001
   SELECT oofa011 INTO l_oofa011 FROM oofa_t WHERE oofa002=l_gzxa002 AND oofa003=l_gzxa003
   CALL azzi800_site_b_fill(pc_gzxb001)
 #  DISPLAY pc_gzxb001,l_oofa011 TO gzxb001,gzxb001_desc
   DISPLAY pc_gzxb001 TO gzxb001]]>
</point>
  <point name="input.action" cite_std="N" status="" env="" ver="1" src="" new="Y">
<![CDATA[]]>
</point>
  <point name="input.before_input" cite_std="N" status="u" env="" ver="1" src="" new="Y">
<![CDATA[        BEFORE ROW

            LET l_cmd = ''
            LET l_ac = ARR_CURR()
            LET l_lock_sw = 'N'            #DEFAULT
            LET l_n = ARR_COUNT()
            DISPLAY l_n TO FORMONLY.cnt    #共幾項
            
            CALL s_transaction_begin()
            
            LET g_detail_cnt = g_gzxb_d.getLength()
            DISPLAY g_detail_cnt TO FORMONLY.idx
    
            #DISPLAY "l_ac:",l_ac ,"g_detail_cnt:",g_detail_cnt
            IF g_detail_cnt >= l_ac  AND NOT cl_null(g_gzxb_d[l_ac].gzxb002) AND NOT cl_null(g_gzxb_d[l_ac].gzxb003)  THEN
               LET l_cmd='u'
               LET g_gzxb_d_t.* = g_gzxb_d[l_ac].*  #BACKUP
               IF NOT azzi800_site_lock_b(l_ac) THEN
                  LET l_lock_sw='Y'
               ELSE  
                  FETCH azzi800_site_bcl INTO 
                  g_gzxb_d[l_ac].gzxbstus,g_gzxb_d[l_ac].gzxb002,g_gzxb_d[l_ac].gzxb003,g_gzxb_d[l_ac].gzxb004,g_gzxb_d[l_ac].gzxb005,
                  g_gzxb2_d[l_ac].gzxbmodid,g_gzxb2_d[l_ac].gzxbmoddt,g_gzxb2_d[l_ac].gzxbownid,g_gzxb2_d[l_ac].gzxbowndp,g_gzxb2_d[l_ac].gzxbcrtid,
                  g_gzxb2_d[l_ac].gzxbcrtdp,g_gzxb2_d[l_ac].gzxbcrtdt
                  IF SQLCA.sqlcode THEN
                     CALL cl_err(g_gzxb_d_t.gzxb002,SQLCA.sqlcode,1)
                     LET l_lock_sw = "Y"
                  END IF
                  CALL cl_show_fld_cont()
               END IF
            ELSE
               LET l_cmd='a'
            END IF


         BEFORE INSERT
            CALL s_transaction_begin()
            LET l_n = ARR_COUNT()
            LET l_cmd = 'a'
            INITIALIZE g_gzxb_d_t.* TO NULL
            INITIALIZE g_gzxb_d[l_ac].* TO NULL 
            
            LET g_gzxb_d_t.* = g_gzxb_d[l_ac].*     #新輸入資料
            CALL cl_show_fld_cont()
            CALL azzi800_site_set_entry_b("a")
            CALL azzi800_site_set_no_entry_b("a")
            #公用欄位給值(單身)
            LET g_gzxb_d[l_ac].gzxbstus = "Y"
            LET g_gzxb2_d[l_ac].gzxbownid = g_user
            LET g_gzxb2_d[l_ac].gzxbowndp = g_dept
            LET g_gzxb2_d[l_ac].gzxbcrtid = g_user
            LET g_gzxb2_d[l_ac].gzxbcrtdp = g_dept 
            LET g_gzxb2_d[l_ac].gzxbcrtdt = cl_get_current()
            LET g_gzxb2_d[l_ac].gzxbmodid = g_user
            LET g_gzxb2_d[l_ac].gzxbmoddt = cl_get_current()

         AFTER INSERT
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               CANCEL INSERT
            END IF
               
            LET l_count = 1 

            SELECT COUNT(*) INTO l_count FROM gzxb_t 
             WHERE  gzxbent = g_enterprise AND gzxb001 = pc_gzxb001
             AND gzxb002 = g_gzxb_d[l_ac].gzxb002 AND gzxb003 =g_gzxb_d[l_ac].gzxb003
      
            #資料未重複, 插入新增資料
            IF l_count = 0 THEN 
               INSERT INTO gzxb_t
                  (
                   gzxbent,
                   gzxb001,gzxbstus,
                   gzxb002,gzxb003,
                   gzxb004,gzxb005,
                   gzxbownid,gzxbowndp,
                   gzxbcrtid,gzxbcrtdp,gzxbcrtdt,
                   gzxbmodid,gzxbmoddt
                   
                   ) 
            VALUES(
                   g_enterprise, 
                   gc_gzxb001,g_gzxb_d[l_ac].gzxbstus,
                   g_gzxb_d[l_ac].gzxb002,g_gzxb_d[l_ac].gzxb003,
                   g_gzxb_d[l_ac].gzxb004,g_gzxb_d[l_ac].gzxb005,
                   g_gzxb2_d[l_ac].gzxbownid,g_gzxb2_d[l_ac].gzxbowndp,
                   g_gzxb2_d[l_ac].gzxbcrtid,g_gzxb2_d[l_ac].gzxbcrtdp,g_gzxb2_d[l_ac].gzxbcrtdt,
                   g_gzxb2_d[l_ac].gzxbmodid,g_gzxb2_d[l_ac].gzxbmoddt
                    )
                   DISPLAY "11 SQLCA.sqlcode:",SQLCA.sqlcode
               IF SQLCA.sqlcode THEN
                  CALL cl_err("gzxb_t",SQLCA.sqlcode,0)
               END IF                           
            ELSE    
               CALL cl_err('INSERT',"std-00006",1)
               INITIALIZE g_gzxb_d[l_ac].* TO NULL
               CALL s_transaction_end('N',0)
               CANCEL INSERT
            END IF
 
            IF SQLCA.SQLcode  THEN
               CALL cl_err("gzxb_t",SQLCA.sqlcode,1) 
               CALL s_transaction_end('N',0)
               CANCEL INSERT
            ELSE
               CALL s_transaction_end('Y',0)
               ERROR 'INSERT O.K'
               LET g_detail_cnt = g_detail_cnt + 1
            END IF
              
         BEFORE DELETE                            #是否取消單身
            IF NOT cl_null(pc_gzxb001) THEN
                          
               IF NOT cl_ask_del_detail() THEN
                  CANCEL DELETE
               END IF
               IF l_lock_sw = "Y" THEN
                  CALL cl_err("", -263, 1)
                  CANCEL DELETE
               END IF
               DELETE FROM gzxb_t
                  WHERE gzxbent = g_enterprise
                      AND gzxb001 = pc_gzxb001
                      AND gzxb002 = g_gzxb_d_t.gzxb002 
                      AND gzxb003 = g_gzxb_d_t.gzxb003 
                      
               IF SQLCA.sqlcode THEN
                  CALL cl_err("gzxb_t",SQLCA.sqlcode,1)
                  CALL s_transaction_end('N',0)
                  CANCEL DELETE   
               ELSE
                  LET g_detail_cnt = g_detail_cnt-1
                  CALL s_transaction_end('Y',0)
               END IF 
               CLOSE azzi800_site_bcl
               LET l_count = g_gzxb_d.getLength()
            END IF 
              
         ON ROW CHANGE
            IF INT_FLAG THEN
               CALL cl_err('',9001,0)
               LET INT_FLAG = 0
               LET g_gzxb_d[l_ac].* = g_gzxb_d_t.*
               CLOSE azzi800_site_bcl
               CALL s_transaction_end('N',0)
               EXIT DIALOG 
            END IF
              
            IF l_lock_sw = 'Y' THEN
               CALL cl_err(pc_gzxb001,-263,1)
               LET g_gzxb_d[l_ac].* = g_gzxb_d_t.*
            ELSE

               
               #寫入修改者/修改日期資訊(單身)
               LET g_gzxb2_d[l_ac].gzxbmodid = g_user
               LET g_gzxb2_d[l_ac].gzxbowndp = g_dept
               LET g_gzxb2_d[l_ac].gzxbcrtid = g_user
               LET g_gzxb2_d[l_ac].gzxbcrtdp = g_dept 
               LET g_gzxb2_d[l_ac].gzxbcrtdt = cl_get_current()
               LET g_gzxb2_d[l_ac].gzxbmodid = g_user
               LET g_gzxb2_d[l_ac].gzxbmoddt = cl_get_current()

      
               UPDATE gzxb_t SET (gzxbstus,gzxb001,gzxb002,gzxb003,gzxbmodid,gzxbmoddt,gzxbownid,gzxbowndp,gzxbcrtid,gzxbcrtdp,gzxbcrtdt) = (g_gzxb_d[l_ac].gzxbstus,pc_gzxb001,g_gzxb_d[l_ac].gzxb002,g_gzxb_d[l_ac].gzxb003,g_gzxb2_d[l_ac].gzxbmodid,g_gzxb2_d[l_ac].gzxbmoddt,g_gzxb2_d[l_ac].gzxbownid,g_gzxb2_d[l_ac].gzxbowndp,g_gzxb2_d[l_ac].gzxbcrtid,g_gzxb2_d[l_ac].gzxbcrtdp,g_gzxb2_d[l_ac].gzxbcrtdt)
                WHERE 
                      gzxbent = g_enterprise
                      AND gzxb001 = pc_gzxb001
                      AND gzxb002 = g_gzxb_d_t.gzxb002 
                      AND gzxb003 = g_gzxb_d_t.gzxb003
 
               IF SQLCA.sqlcode THEN
                  CALL cl_err("gzxb_t",SQLCA.sqlcode,1)   
                  LET g_gzxb_d[l_ac].* = g_gzxb_d_t.* 
                  CALL s_transaction_end('N',0)
               ELSE
                  CALL s_transaction_end('Y',0)
               END IF

            END IF
            
         AFTER ROW
            CALL azzi800_site_b_fill(pc_gzxb001)
            CALL azzi800_site_unlock_b()
            #CALL s_transaction_end('Y',0)]]>
</point>
