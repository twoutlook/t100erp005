  <section id="azzi050.b_fill" ver="5" status="" src="s">
    <![CDATA[#+ 單身陣列填充
PRIVATE FUNCTION azzi050_b_fill(p_wc2)              #BODY FILL UP
   DEFINE p_wc2    STRING
   #add-point:b_fill段define
   {<point name="b_fill.define"/>}
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前
   {<point name="b_fill.sql_before"/>}
   #end add-point
 
   LET g_sql = "SELECT  UNIQUE gzoistus,gzoi001,gzoi002,gzoi001,gzoimodid,gzoimoddt,gzoiownid,gzoiowndp, 
       gzoicrtid,gzoicrtdp,gzoicrtdt ,t1.ooag011 ,t2.ooag011 ,t3.ooefl003 ,t4.ooag011 ,t5.ooefl003 FROM gzoi_t", 

               "",
                              " LEFT JOIN ooag_t t1 ON t1.ooagent='"||g_enterprise||"' AND t1.ooag001=gzoimodid  ",
               " LEFT JOIN ooag_t t2 ON t2.ooagent='"||g_enterprise||"' AND t2.ooag001=gzoiownid  ",
               " LEFT JOIN ooefl_t t3 ON t3.ooeflent='"||g_enterprise||"' AND t3.ooefl001=gzoiowndp AND t3.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t4 ON t4.ooagent='"||g_enterprise||"' AND t4.ooag001=gzoicrtid  ",
               " LEFT JOIN ooefl_t t5 ON t5.ooeflent='"||g_enterprise||"' AND t5.ooefl001=gzoicrtdp AND t5.ooefl002='"||g_dlang||"' ",
 
               " WHERE 1=1 AND ", p_wc2 
    
   LET g_sql = g_sql, cl_sql_add_filter("gzoi_t"),
                      " ORDER BY gzoi_t.gzoi001"
   
   #add-point:b_fill段sql之後
   {<point name="b_fill.sql_after"/>}
   #end add-point
   
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE azzi050_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR azzi050_pb
   
   OPEN b_fill_curs
 
   CALL g_gzoi_d.clear()
   CALL g_gzoi1_info_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_gzoi_d[l_ac].gzoistus,g_gzoi_d[l_ac].gzoi001,g_gzoi_d[l_ac].gzoi002,g_gzoi1_info_d[l_ac].gzoi001, 
       g_gzoi1_info_d[l_ac].gzoimodid,g_gzoi1_info_d[l_ac].gzoimoddt,g_gzoi1_info_d[l_ac].gzoiownid, 
       g_gzoi1_info_d[l_ac].gzoiowndp,g_gzoi1_info_d[l_ac].gzoicrtid,g_gzoi1_info_d[l_ac].gzoicrtdp, 
       g_gzoi1_info_d[l_ac].gzoicrtdt,g_gzoi1_info_d[l_ac].gzoimodid_desc,g_gzoi1_info_d[l_ac].gzoiownid_desc, 
       g_gzoi1_info_d[l_ac].gzoiowndp_desc,g_gzoi1_info_d[l_ac].gzoicrtid_desc,g_gzoi1_info_d[l_ac].gzoicrtdp_desc 

      IF SQLCA.sqlcode THEN
         CALL cl_err("FOREACH:",SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充
      {<point name="b_fill.fill"/>}
      #end add-point
      
      CALL azzi050_detail_show()      
 
      LET l_ac = l_ac + 1
      IF l_ac > g_max_rec THEN
         EXIT FOREACH
      END IF
      
   END FOREACH
   
   IF l_ac > g_max_rec THEN
      IF g_error_show = 1 THEN
         CALL cl_err("gzoi_t",9035,1)
      END IF
   END IF 
   LET g_error_show = 0
   
 
   
   CALL g_gzoi_d.deleteElement(g_gzoi_d.getLength())   
   CALL g_gzoi1_info_d.deleteElement(g_gzoi1_info_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_gzoi_d.getLength()
      LET g_gzoi1_info_d[l_ac].gzoi001 = g_gzoi_d[l_ac].gzoi001 
 
   END FOR
   
   IF g_cnt > g_gzoi_d.getLength() THEN
      LET l_ac = g_gzoi_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #add-point:b_fill段資料填充(其他單身)
   {<point name="b_fill.others.fill"/>}
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_gzoi_d.getLength()
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE azzi050_pb
   
END FUNCTION
