  <point name="function.azzi050_check_gzoi001" order="1" ver="1" cite_std="N" new="Y" status="" src="s" mark_hard="N">
    <![CDATA[#+ 檢核gzoi001 
#+ 限制必須為兩碼英文小寫字符，不可含有英文字元以外的字符，輸入不合規格時報錯
#+ sd 是標準行業別 不可修改及刪除
#+ 行業代碼 有出現在azzi900 gzza015(歸屬行業別) 或 azzi070 gzzo007(歸屬行業編號)
#+ 代碼 uc/ud 為系統使用，不可成為行業代碼
PRIVATE FUNCTION azzi050_check_gzoi001(p_cmd)
     DEFINE p_cmd LIKE type_t.chr1 
   DEFINE ls_str   STRING
   DEFINE li_len   LIKE type_t.num5
   DEFINE li_cnt   LIKE type_t.num5
   
   LET g_errno = NULL
   LET ls_str = g_gzoi_d[l_ac].gzoi001
   

   IF p_cmd = 'u' OR p_cmd = 'a' THEN 
      IF NOT cl_chk_num(g_gzoi_d[l_ac].gzoi001,"L") THEN  
         LET g_errno = "azz-00036" #不可以有數字或其他特別字符
         RETURN
       END IF
   
      IF ls_str.getLength() <>  2 THEN 
         LET g_errno = "azz-00007"  #必須為兩碼英文小寫字符
         RETURN 
      END IF

      IF ls_str = "uc" OR ls_str = "ud" THEN 
         LET g_errno = "azz-00169"  #代碼 uc/ud 為系統使用，不可成為行業代碼
         RETURN 
      END IF

      IF p_cmd = 'u' THEN 
         IF g_gzoi_d_t.gzoi001 = "sd" AND ls_str <> "sd" THEN 
            LET g_errno = "azz-00168"  #sd 是標準行業別，不能修改及刪除 
            RETURN
         END IF 
      END IF 

      
   ELSE 
      #刪除時 sd不能修改及刪除 
      IF g_gzoi_d_t.gzoi001 = "sd" THEN 
         LET g_errno = "azz-00168"  #sd 是標準行業別，不能修改及刪除 
         RETURN
      END IF
      #有出現在azzi900 gzza015(歸屬行業別)
      SELECT COUNT(gzza015) INTO li_cnt
         FROM gzza_t
         WHERE gzza015 = g_gzoi_d_t.gzoi001

      IF li_cnt > 0 THEN
         LET g_errno = "azz-00190" 
         CALL cl_err_msg("","azz-00190",g_gzoi_d_t.gzoi001||'|'||"azzi900(gzza_t)",1)
         RETURN 
      END IF  
      #azzi070 gzzo007(歸屬行業編號)
      SELECT COUNT(gzzo007) INTO li_cnt
         FROM gzzo_t
         WHERE gzzo007 = g_gzoi_d_t.gzoi001

      IF li_cnt > 0 THEN 
         LET g_errno = "azz-00190" 
         CALL cl_err_msg("","azz-00190",g_gzoi_d_t.gzoi001||'|'||"azzi070(gzza_t)",1)
         RETURN 
      END IF
      
   END IF
      
END FUNCTION]]>
