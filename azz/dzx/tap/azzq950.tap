            DISPLAY azzq950_filter_parser('gzpc004') TO s_detail1[1].b_gzpc004
            DISPLAY azzq950_filter_parser('gzpc006') TO s_detail1[1].b_gzpc006
 
 
         #單身公用欄位開窗相關處理
         
           
         #單身一般欄位開窗相關處理
                  #----<<sel>>----
         #----<<b_gzpc002>>----
         #Ctrlp:construct.c.filter.page1.b_gzpc002
