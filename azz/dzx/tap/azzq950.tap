            #add-point:ON ACTION controlp INFIELD b_gzpc002
            {<point name="construct.c.filter.page1.b_gzpc002" />}
            #END add-point
 
         #----<<b_gzpc000>>----
         #Ctrlp:construct.c.filter.page1.b_gzpc000
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD b_gzpc000
            #add-point:ON ACTION controlp INFIELD b_gzpc000
            {<point name="construct.c.filter.page1.b_gzpc000" />}
            #END add-point
 
         #----<<b_gzpc001>>----
         #Ctrlp:construct.c.filter.page1.b_gzpc001
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD b_gzpc001
            #add-point:ON ACTION controlp INFIELD b_gzpc001
            {<point name="construct.c.filter.page1.b_gzpc001" />}
            #END add-point
 
         #----<<b_gzpa002>>----
         #Ctrlp:construct.c.filter.page1.b_gzpa002
