            #add-point:ON ACTION controlp INFIELD b_gzpa002
            {<point name="construct.c.filter.page1.b_gzpa002" />}
            #END add-point
 
         #----<<b_gzpc003>>----
         #Ctrlp:construct.c.filter.page1.b_gzpc003
         #應用 a03 樣板自動產生(Version:2)
         ON ACTION controlp INFIELD b_gzpc003
            #add-point:ON ACTION controlp INFIELD b_gzpc003
            {<point name="construct.c.filter.page1.b_gzpc003" />}
            #END add-point
 
         #----<<b_gzpc004>>----
         #Ctrlp:construct.c.filter.page1.b_gzpc004
