  <section id="azzi700.modify" ver="3" status="" src="m">
    <![CDATA[#+ 資料修改
PRIVATE FUNCTION azzi700_modify()
   #add-point:modify段define
{<point name="modify.define"/>}
   #end add-point
   
   IF g_gzja_m.gzja001 IS NULL
 
   THEN
      CALL cl_err("","std-00003",0)
      RETURN
   END IF 
   
   EXECUTE azzi700_master_referesh USING g_gzja_m.gzja001 INTO g_gzja_m.gzja001,g_gzja_m.gzja002,g_gzja_m.gzja004, 
       g_gzja_m.gzja003,g_gzja_m.gzja005,g_gzja_m.gzja006,g_gzja_m.gzjastus,g_gzja_m.gzjaownid,g_gzja_m.gzjaowndp,g_gzja_m.gzjacrtid,g_gzja_m.gzjacrtdp, 
       g_gzja_m.gzjacrtdt,g_gzja_m.gzjamodid,g_gzja_m.gzjamoddt
 
   ERROR ""
  
   LET g_gzja001_t = g_gzja_m.gzja001
 
   
   CALL s_transaction_begin()
   
   OPEN azzi700_cl USING g_gzja_m.gzja001
   IF STATUS THEN
      CALL cl_err("OPEN azzi700_cl:", STATUS, 1)
      CLOSE azzi700_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #鎖住將被更改或取消的資料
   FETCH azzi700_cl INTO g_gzja_m.gzja001,g_gzja_m.gzjal003,g_gzja_m.gzja002,g_gzja_m.gzja002_desc,g_gzja_m.gzja004, 
       g_gzja_m.gzja003,g_gzja_m.gzja005,g_gzja_m.gzja006,g_gzja_m.gzja006_desc,g_gzja_m.gzjastus,g_gzja_m.gzjaownid,g_gzja_m.gzjaownid_desc,g_gzja_m.gzjaowndp, 
       g_gzja_m.gzjaowndp_desc,g_gzja_m.gzjacrtid,g_gzja_m.gzjacrtid_desc,g_gzja_m.gzjacrtdp,g_gzja_m.gzjacrtdp_desc, 
       g_gzja_m.gzjacrtdt,g_gzja_m.gzjamodid,g_gzja_m.gzjamodid_desc,g_gzja_m.gzjamoddt
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CALL cl_err("gzja_t",SQLCA.sqlcode,0)
      CLOSE azzi700_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   
 
   CALL azzi700_show()
   
   WHILE TRUE
      LET g_gzja_m.gzja001 = g_gzja001_t
 
      
      #寫入修改者/修改日期資訊
      LET g_gzja_m.gzjamodid = g_user 
      LET g_gzja_m.gzjamoddt = cl_get_current()
 
      
      #add-point:modify段修改前
{<point name="modify.before_input"/>}
      #end add-point
 
      CALL azzi700_input("u")     #欄位更改
 
      #add-point:modify段修改後
{<point name="modify.after_input"/>}
      #end add-point
      
      IF INT_FLAG THEN
         LET INT_FLAG = 0
         LET g_gzja_m.* = g_gzja_m_t.*
         CALL azzi700_show()
         CALL cl_err("",9001,0)
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE gzja_t SET (gzjamodid,gzjamoddt) = (g_gzja_m.gzjamodid,g_gzja_m.gzjamoddt)
       WHERE  gzja001 = g_gzja001_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #修改歷程記錄
   IF NOT cl_log_modified_record(g_gzja_m.gzja001,g_site) THEN 
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE azzi700_cl
   CALL s_transaction_end('Y','0')
 
   #流程通知預埋點-U
   CALL cl_flow_notify(g_gzja_m.gzja001,"U")
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
]]>
