{<section id="azzi991.update_b" >}
#+ 修改單身後其他table連動
PRIVATE FUNCTION azzi991_update_b(ps_table,ps_keys,ps_keys_bak,ps_page)
   #add-point:update_b段define(客製用) name="update_b.define_customerization"
   
   #end add-point     
   DEFINE ps_table         STRING
   DEFINE ps_page          STRING
   DEFINE ps_keys          DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ps_keys_bak      DYNAMIC ARRAY OF VARCHAR(500)
   DEFINE ls_group         STRING
   DEFINE li_idx           LIKE type_t.num10 
   DEFINE lb_chk           BOOLEAN
   DEFINE l_new_key        DYNAMIC ARRAY OF STRING
   DEFINE l_old_key        DYNAMIC ARRAY OF STRING
   DEFINE l_field_key      DYNAMIC ARRAY OF STRING
   #add-point:update_b段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="update_b.define"
   
   #end add-point     
  
   #add-point:Function前置處理  name="update_b.pre_function"
   
   #end add-point
   
   #判斷key是否有改變
   LET lb_chk = TRUE
   FOR li_idx = 1 TO ps_keys.getLength()
      IF ps_keys[li_idx] <> ps_keys_bak[li_idx] THEN
         LET lb_chk = FALSE
         EXIT FOR
      END IF
   END FOR
   
   #不需要做處理
   IF lb_chk THEN
      RETURN
   END IF
   
   #判斷是否是同一群組的table
   LET ls_group = "gzsz_t,"
   IF ls_group.getIndexOf(ps_table,1) > 0 AND ps_table <> "gzsz_t" THEN
   
      #add-point:update_b段修改前 name="update_b.before_update"
      
      #end add-point     
   
      #將遮罩欄位還原
      CALL azzi991_gzsz_t_mask_restore('restore_mask_o')
               
      UPDATE gzsz_t 
         SET (gzsz001,gzsz002
              ,gzszstus,gzsz011,gzsz003,gzsz004,gzsz005,gzsz009,gzsz016,gzsz015,gzsz013,gzsz014,gzsz008,gzszownid,gzszowndp,gzszcrtid,gzszcrtdp,gzszcrtdt,gzszmodid,gzszmoddt) 
              = 
             (ps_keys[1],ps_keys[2]
              ,g_gzsz_d[g_detail_idx].gzszstus,g_gzsz_d[g_detail_idx].gzsz011,g_gzsz_d[g_detail_idx].gzsz003, 
                  g_gzsz_d[g_detail_idx].gzsz004,g_gzsz_d[g_detail_idx].gzsz005,g_gzsz_d[g_detail_idx].gzsz009, 
                  g_gzsz_d[g_detail_idx].gzsz016,g_gzsz_d[g_detail_idx].gzsz015,g_gzsz_d[g_detail_idx].gzsz013, 
                  g_gzsz_d[g_detail_idx].gzsz014,g_gzsz_d[g_detail_idx].gzsz008,g_gzsz2_d[g_detail_idx].gzszownid, 
                  g_gzsz2_d[g_detail_idx].gzszowndp,g_gzsz2_d[g_detail_idx].gzszcrtid,g_gzsz2_d[g_detail_idx].gzszcrtdp, 
                  g_gzsz2_d[g_detail_idx].gzszcrtdt,g_gzsz2_d[g_detail_idx].gzszmodid,g_gzsz2_d[g_detail_idx].gzszmoddt)  
 
         WHERE 
               gzsz001 = ps_keys_bak[1] AND gzsz002 = ps_keys_bak[2]
 
      #add-point:update_b段修改中 name="update_b.m_update"
      
      #end add-point 
         
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            CALL s_transaction_end('N','0')
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "gzsz_t" 
            LET g_errparam.code = "std-00009" 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
            
         WHEN SQLCA.SQLCODE #其他錯誤
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "gzsz_t:",SQLERRMESSAGE  
            LET g_errparam.code = SQLCA.SQLCODE 
            LET g_errparam.popup = TRUE 
            CALL s_transaction_end('N','0')
            CALL cl_err()
            
         OTHERWISE
            LET l_new_key[01] = ps_keys[1] 
LET l_old_key[01] = ps_keys_bak[1] 
LET l_field_key[01] = 'gzszl001'
LET l_new_key[02] = ps_keys[2] 
LET l_old_key[02] = ps_keys_bak[2] 
LET l_field_key[02] = 'gzszl002'
LET l_new_key[03] = g_dlang 
LET l_old_key[03] = g_dlang 
LET l_field_key[03] = 'gzszl003'
CALL cl_multitable_key_upd(l_new_key, l_old_key, l_field_key, 'gzszl_t')
      END CASE
 
      #將遮罩欄位進行遮蔽
      CALL azzi991_gzsz_t_mask_restore('restore_mask_n')
               
      #add-point:update_b段修改後 name="update_b.after_update"
      
      #end add-point
      
      RETURN
   END IF
   
 
   
   LET ls_group = "gzsy_t,"
   #判斷是否是同一群組的table
   IF ls_group.getIndexOf(ps_table,1) > 0 AND ps_table <> "gzsy_t" THEN
   
      #add-point:update_b段修改前 name="update_b.before_update2"
      
      #end add-point    
      
      #將遮罩欄位還原
      CALL azzi991_gzsy_t_mask_restore('restore_mask_o')
      
      UPDATE gzsy_t 
         SET (gzsy001,gzsy002,gzsy004
              ,gzsy003,gzsy005) 
              = 
             (ps_keys[1],ps_keys[2],ps_keys[3]
              ,g_gzsz3_d[g_detail_idx2].gzsy003,g_gzsz3_d[g_detail_idx2].gzsy005) 
         WHERE 
               gzsy001 = ps_keys_bak[1] AND gzsy002 = ps_keys_bak[2] AND gzsy004 = ps_keys_bak[3]
 
      #add-point:update_b段修改中 name="update_b.m_update2"
      
      #end add-point 
         
      CASE
         WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
            CALL s_transaction_end('N','0')
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "gzsy_t" 
            LET g_errparam.code = "std-00009" 
            LET g_errparam.popup = TRUE 
            CALL cl_err()
            
         WHEN SQLCA.SQLCODE #其他錯誤
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = "gzsy_t:",SQLERRMESSAGE 
            LET g_errparam.code = SQLCA.SQLCODE 
            LET g_errparam.popup = TRUE 
            CALL s_transaction_end('N','0')
            CALL cl_err()
            
         OTHERWISE
            
      END CASE
      
      #將遮罩欄位進行遮蔽
      CALL azzi991_gzsy_t_mask_restore('restore_mask_n')
               
      #add-point:update_b段修改後 name="update_b.after_update2"
      
      #end add-point 
      
      RETURN
   END IF
 
 
   
