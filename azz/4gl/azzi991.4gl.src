DEFINE ls_gzze003_b   LIKE gzze_t.gzze003        #170119-00041#1
DEFINE ls_gzze003_c   LIKE gzze_t.gzze003        #170119-00041#1
DEFINE ls_gzze003_d   LIKE gzze_t.gzze003        #170119-00041#1 

   #170119-00041#1 start
   SELECT gzzj003 INTO lc_gzsy003 FROM gzzj_t WHERE gzzj001 = p_gzsy003  

   INITIALIZE g_ref_fields TO NULL
   LET g_ref_fields[1] = lc_gzsy003 
   CALL ap_ref_array2(g_ref_fields,"SELECT gzzol003 FROM gzzol_t WHERE gzzol001=? AND gzzol002='"||g_dlang||"'","") RETURNING g_rtn_fields
    
   SELECT gzze003 INTO ls_gzze003_b FROM gzze_t
    WHERE gzze001 = "lib-488"
      AND gzze002 = g_lang

   SELECT gzze003 INTO ls_gzze003_c FROM gzze_t
    WHERE gzze001 = "lib-486"
      AND gzze002 = g_lang

   SELECT gzze003 INTO ls_gzze003_d FROM gzze_t
    WHERE gzze001 = "lib-487"
      AND gzze002 = g_lang

    LET p_gzsy003 = UPSHIFT(p_gzsy003)
    CASE
         WHEN p_gzsy003[1,1] = "B"   # 行業共用模組
            LET r_gzsy003_desc = ls_gzze003_b,g_rtn_fields[1] 

         WHEN p_gzsy003[1,1]  = "C"   # 客製標準模組
            LET r_gzsy003_desc = ls_gzze003_c,g_rtn_fields[1] 

         WHEN p_gzsy003[1,1] = "D"   # 行業別客製模組
            LET r_gzsy003_desc = ls_gzze003_d,g_rtn_fields[1]

         OTHERWISE   # 註冊模組
            LET r_gzsy003_desc = g_rtn_fields[1] 
      END CASE

   #INITIALIZE g_ref_fields TO NULL
   #LET g_ref_fields[1] = p_gzsy003
   #LET g_ref_fields[1] = lc_gzsy003 
   #CALL ap_ref_array2(g_ref_fields,"SELECT gzzol003 FROM gzzol_t WHERE gzzol001=? AND gzzol002='"||g_dlang||"'","") RETURNING g_rtn_fields
   #LET r_gzsy003_desc = g_rtn_fields[1]
   #170119-00041#1 end
   RETURN r_gzsy003_desc
   
   
END FUNCTION
