               " LEFT JOIN gzsy_t ON gzsz001 = gzsy001 AND gzsz002 = gzsy002",
 
 
               " LEFT JOIN gzszl_t ON gzsz001 = gzszl001 AND gzsz002 = gzszl002 AND gzszl003 = '",g_dlang,"'",
