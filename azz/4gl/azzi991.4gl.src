################################################################################
# Descriptions...: 開窗多選新增到單身
# Memo...........:
# Usage..........: CALL azzi991_multi_ins (p_str)
# Input parameter: p_str          多選字串
# Return code....: 無
# Date & Author..: 131204 By stellar
# Modify.........:
################################################################################
PRIVATE FUNCTION azzi991_multi_ins(p_str)
DEFINE p_str     STRING
DEFINE l_token   base.StringTokenizer
DEFINE l_gzsy    RECORD LIKE gzsy_t.*

   INITIALIZE l_gzsy.* TO NULL
   LET l_gzsy.gzsy001 = g_gzsz_d[g_detail_idx].gzsz001
   LET l_gzsy.gzsy002 = g_gzsz_d[g_detail_idx].gzsz002 
   LET l_gzsy.gzsy005 = 'N'   
   
   LET l_token = base.StringTokenizer.create(p_str, "|")

   CALL s_transaction_begin()

   WHILE l_token.hasMoreTokens()
      LET l_gzsy.gzsy004 = l_token.nextToken()
      LET l_gzsy.gzsy003 = UPSHIFT(l_gzsy.gzsy004[1,3])
      
      INSERT INTO gzsy_t (gzsy001,gzsy002,gzsy003,gzsy004,gzsy005)
             VALUES (l_gzsy.gzsy001,l_gzsy.gzsy002,l_gzsy.gzsy003,
                     l_gzsy.gzsy004,l_gzsy.gzsy005)
      IF SQLCA.sqlcode OR SQLCA.sqlerrd[3] = 0 THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = 'INSERT gzsy_t'
         LET g_errparam.popup = TRUE
         CALL cl_err()

         CALL s_transaction_end('N','0')
         EXIT WHILE
      END IF
   END WHILE

   CALL s_transaction_end('Y','0')
   
END FUNCTION
################################################################################
# Descriptions...: 檢查輸入值是否正確
# Memo...........:
# Usage..........: CALL azzi991_check_gzsz008 ()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 2013/12/04 By stellar
# Modify.........:
################################################################################
PRIVATE FUNCTION azzi991_check_gzsz008()
DEFINE l_cnt     LIKE type_t.num5
DEFINE l_date    LIKE type_t.dat
DEFINE l_str     LIKE gzsz_t.gzsz015

   LET g_errno = ''
   CASE g_gzsz_d[l_ac].gzsz003
      WHEN '1'
           IF g_gzsz_d[l_ac].gzsz008 NOT MATCHES '[YN]' THEN
              LET g_errno = '-25777'
           END IF
      WHEN '4'
           IF NOT cl_null(g_gzsz_d[l_ac].gzsz016) THEN
              LET l_cnt = 0
              SELECT COUNT(*) INTO l_cnt FROM gzcb_t
               WHERE gzcb001 = g_gzsz_d[l_ac].gzsz016
                 AND gzcb002 = g_gzsz_d[l_ac].gzsz008
              IF cl_null(l_cnt) OR l_cnt = 0 THEN
                 LET g_errno = 'azz-00023'
              END IF
           END IF
      WHEN '5'
           LET l_str = cl_replace_str(g_gzsz_d[l_ac].gzsz015,'YYYY','YY')
           SELECT TO_DATE(g_gzsz_d[l_ac].gzsz008,l_str) 
             INTO l_date
             FROM dual
           IF SQLCA.sqlcode THEN
              LET g_errno = '-1218'
           ELSE
              LET g_gzsz_d[l_ac].gzsz008 = l_date USING g_gzsz_d[l_ac].gzsz015
           END IF
   END CASE
END FUNCTION
################################################################################
# Descriptions...: 檢查是否需要執行產生單據別參數的子程式
################################################################################
PRIVATE FUNCTION azzi991_open_azzi991_01(p_type)
DEFINE p_type    LIKE type_t.chr1
DEFINE l_cnt     LIKE type_t.num5

   #檢查是否有不存在於ooac_t的參數編號
   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt FROM gzsz_t
    WHERE gzsz001 = 'ooac_t'
      AND NOT EXISTS (SELECT * FROM ooac_t WHERE ooac003 = gzsz002)
   IF NOT cl_null(l_cnt) AND l_cnt > 0 THEN
      IF p_type = '2' THEN
         IF cl_ask_confirm('azz-00130') THEN
            CALL azzi991_01()
         END IF
      ELSE
         CALL azzi991_01()
      END IF
   END IF
   
   #檢查單據性質是否有未拋轉的資料，有則自動拋轉
   CALL azzi991_auto_ins_ooac()
   
END FUNCTION
################################################################################
# Descriptions...: 若單據性質有未拋轉的資料時，自動拋到ooac_t
################################################################################
PRIVATE FUNCTION azzi991_auto_ins_ooac()
DEFINE l_gzsy             RECORD LIKE gzsy_t.*
DEFINE l_gzsz008          LIKE gzsz_t.gzsz008
DEFINE l_cnt             LIKE type_t.num5
DEFINE l_success         LIKE type_t.num5

   CALL s_transaction_begin()
   LET l_success = TRUE
   
   #將未拋轉的參數資料拋到ooac_t
   DECLARE sel_gzsy_cur CURSOR FOR
      SELECT gzsy_t.*,gzsz008
        FROM gzsy_t INNER JOIN gzsz_t ON gzsy001=gzsz001 AND gzsy002=gzsz002
       WHERE gzsz001 = 'ooac_t'
         AND gzszstus= 'Y'
#        AND gzsy005 = 'N'
   FOREACH sel_gzsy_cur INTO l_gzsy.*,l_gzsz008
      CALL s_azzi991_process_ooac(l_gzsy.*,l_gzsz008)
           RETURNING l_success
      IF NOT l_success THEN
         EXIT FOREACH
      END IF      
#      CALL s_azzi991_insert_ooac(l_gzsy.*,l_gzsz.gzsz008)
#           RETURNING l_success
#      IF NOT l_success THEN
#         EXIT FOREACH
#      END IF
   END FOREACH
   
   #若ooac_t的資料不存在gzsy_t，則將該資料刪除
   DELETE FROM ooac_t
    WHERE NOT EXISTS (SELECT gzsy002 FROM ooba_t,gzsy_t,oobx_t
                       WHERE gzsy004 = oobx003
                         AND gzsy001 = 'ooac_t'
                         AND gzsy002 = ooac003
                         AND oobaent = ooacent
                         AND ooba001 = ooac001
                         AND ooba002 = ooac002
                         AND oobxent = oobaent
                         AND oobx001 = ooba002)
      AND ooacent = g_enterprise
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend = "gzsz_t"
      LET g_errparam.popup = TRUE
      CALL cl_err()
      LET l_success = FALSE
   END IF
   
   IF l_success THEN
      CALL s_transaction_end('Y',0)
   ELSE
      CALL s_transaction_end('N',0)
   END IF
      
END FUNCTION
################################################################################
# Descriptions...: 檢核gzsz009值域範圍
# Memo...........:
# Usage..........: CALL azzi991_check_gzsz009()
#                  RETURNING 回传参数
# Input parameter: 
#                : 
# Return code....: 
#                : 
# Date & Author..: 2014/02/05 By stellar
# Modify.........: 160316-00036#1 mod(將azzi990的FUNCTION完整搬過來)
################################################################################
PRIVATE FUNCTION azzi991_check_gzsz009()
DEFINE ls_gzsz009   STRING
DEFINE li_index     LIKE type_t.num5
DEFINE li_chk       LIKE type_t.num5

   LET ls_gzsz009 = g_gzsz_d[l_ac].gzsz009
   LET li_chk = FALSE
   LET g_errno = NULL 

   #輸入資料型態是整數選項:2或範圍選項:3 必須檢核
   #可以數字區間
   #可以>= > < <=
   #離散性的資料，可以用逗號分隔，如：1,8,9,20,21,99 只有正數選項才有
   IF g_gzsz_d[l_ac].gzsz003 = "2" OR g_gzsz_d[l_ac].gzsz003 = "3" THEN 
      #確認第1個位置是否數字
      IF cl_chk_num(ls_gzsz009.subString(1,1),"N") THEN  
         IF ls_gzsz009.getIndexOf("-",1) THEN  #表示數字區間

            CASE 
               WHEN ls_gzsz009.getIndexOf("-",1)
                    LET li_index = ls_gzsz009.getIndexOf("-",1)
            END CASE 
            #整數選項:2 
            IF g_gzsz_d[l_ac].gzsz003 = "2" THEN 
               IF NOT cl_chk_num(ls_gzsz009.subString(1,li_index-1),"N") OR NOT cl_chk_num(ls_gzsz009.subString(li_index+1,ls_gzsz009.getLength()),"N") THEN 
                  LET g_errno = "azz-00139"            #值域範圍須數字 
               END IF
            ELSE
               #範圍選項:3 檢核小數資料
               IF azzi991_chk_decimal_data(ls_gzsz009.subString(1,li_index-1)) OR 
                  azzi991_chk_decimal_data(ls_gzsz009.subString(li_index+1,ls_gzsz009.getLength())) THEN 
                  LET g_errno = "azz-00139" 
              END IF   
            END IF 
         ELSE 
            CASE
               # 整數選項:2  可以有離散性的資料
               #WHEN g_gzsz_d[l_ac].gzsz003 = "2" AND ls_gzsz009.getIndexOf(",",1) 
                    #CALL azzi991_chk_gzsz009_discrete_data(ls_gzsz009)
               WHEN ls_gzsz009.getIndexOf(",",1) 
                  CALL azzi991_chk_gzsz009_discrete_data(g_gzsz_d[l_ac].gzsz003,ls_gzsz009)

               OTHERWISE 
                  LET g_errno = "azz-00140"          #值域範圍要有區間 
            END CASE                  
         END IF
      ELSE
         CASE 
            WHEN ls_gzsz009.getIndexOf(">",1) OR ls_gzsz009.getIndexOf("<",1)
                 CASE 
                     WHEN ls_gzsz009.getIndexOf(">",1)
                        LET li_index = ls_gzsz009.getIndexOf(">",1)
  
                     WHEN ls_gzsz009.getIndexOf("<",1)
                        LET li_index = ls_gzsz009.getIndexOf("<",1)
                 END CASE

                    IF ls_gzsz009.getIndexOf("=",li_index+1) THEN 
                       IF g_gzsz_d[l_ac].gzsz003 = "2" THEN  
                          IF NOT cl_chk_num(ls_gzsz009.subString(li_index+2,ls_gzsz009.getLength()),"N") THEN 
                             LET g_errno = "azz-00139"  #值域範圍須數字
                          END IF
                       ELSE
                          # 範圍選項:3  
                          IF azzi991_chk_decimal_data(ls_gzsz009.subString(li_index+2,ls_gzsz009.getLength())) THEN 
                             LET g_errno = "azz-00139"  #值域範圍須數字
                          END IF 
                       END IF  
                    ELSE
                       IF g_gzsz_d[l_ac].gzsz003 = "2" THEN  
                          IF NOT cl_chk_num(ls_gzsz009.subString(li_index+1,ls_gzsz009.getLength()),"N") THEN 
                             LET g_errno = "azz-00139"  #值域範圍須數字
                          END IF
                       ELSE 
                          # 範圍選項:3  
                          IF azzi991_chk_decimal_data(ls_gzsz009.subString(li_index+1,ls_gzsz009.getLength())) THEN 
                             LET g_errno = "azz-00139"  #值域範圍須數字
                          END IF 
                       END IF  

                    END IF

            OTHERWISE
              LET li_chk = FALSE 
              LET g_errno = "azz-00141"           #值域範圍要有>= > < <= 符號開頭
         END CASE       
      END IF 
   END IF
   
END FUNCTION
################################################################################
# Descriptions...: 值域範圍離散型資料檢查
# Memo...........:
# Usage..........: CALL azzi991_chk_gzsz009_discrete_data(ps_gzsz003,ps_gzsz009)
# Input parameter: ps_gzsz003  輸入限制資料型態
#                : ps_gzsz009  值域範圍值
# Return code....: 無
# Date & Author..: 2014/02/05 By stellar
# Modify.........: 160316-00036#1 mod(將azzi990的FUNCTION完整搬過來)
################################################################################
PRIVATE FUNCTION azzi991_chk_gzsz009_discrete_data(ps_gzsz003,ps_gzsz009)
   DEFINE ps_gzsz009    STRING 
   DEFINE l_token       base.StringTokenizer
   DEFINE l_token2      base.StringTokenizer
   DEFINE ls_next       STRING
   DEFINE ls_next2      STRING
   DEFINE ps_gzsz003    STRING 
   DEFINE li_chk        LIKE type_t.num5

   LET l_token = base.StringTokenizer.create(ps_gzsz009, ",")
   LET li_chk = FALSE 
   WHILE l_token.hasMoreTokens()
      LET ls_next = l_token.nextToken()
      LET ls_next = ls_next.trim()
      IF NOT cl_chk_num(ls_next,"N") THEN 
         #資料型態是整數選項:2 必須是整數 
         IF ps_gzsz003 = "2" THEN 
            LET g_errno = "azz-00139"
            EXIT WHILE 

         ELSE 
            IF ls_next.getIndexOf(".",1) THEN 
               LET l_token2 = base.StringTokenizer.create(ls_next, ".")
               WHILE l_token2.hasMoreTokens()
                  LET ls_next2 = l_token2.nextToken()
                  LET ls_next2 = ls_next2.trim()

                  IF NOT cl_chk_num(ls_next2,"N") THEN 
                     LET g_errno = "azz-00139"
                     LET li_chk = TRUE 
                     EXIT WHILE 
                  END IF 
               END WHILE
               IF li_chk THEN 
                  EXIT WHILE 
               END IF  
            ELSE
               LET g_errno = "azz-00139"               #小數點
               EXIT WHILE 
            END IF 
         END IF 
         #LET g_errno = "azz-00139"                 #整數數字 
      END IF  
   END WHILE 
   
END FUNCTION

################################################################################
# Descriptions...: 檢核小數資料
# Memo...........:
# Usage..........: CALL azzi991_chk_decimal_data(ps_data)
#                  RETURNING 回传参数
# Input parameter: ps_data   檢查值
# Return code....: li_chk    檢查結果(True:是小數/False:不是小數)
# Date & Author..: 16/03/16 By Sarah
# Modify.........: 160316-00036#1 add(將azzi990的FUNCTION完整搬過來)
################################################################################
PRIVATE FUNCTION azzi991_chk_decimal_data(ps_data)
   DEFINE ps_data STRING 
   DEFINE ps_gzsz009    STRING 
   DEFINE l_token       base.StringTokenizer
   DEFINE ls_next       STRING
   DEFINE li_chk        LIKE type_t.num5

   LET li_chk = FALSE 
   LET l_token = base.StringTokenizer.create(ps_data, ".")
   WHILE l_token.hasMoreTokens()
      LET ls_next = l_token.nextToken()
      LET ls_next = ls_next.trim()
      IF NOT cl_chk_num(ls_next,"N") THEN 
         LET li_chk = TRUE 
         EXIT WHILE 
      END IF  
   END WHILE 

   RETURN li_chk
   
END FUNCTION

 
