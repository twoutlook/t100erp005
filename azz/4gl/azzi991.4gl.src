   OPEN b_fill_curs
 
   CALL g_gzsz_d.clear()
   CALL g_gzsz2_d.clear()   
   CALL g_gzsz3_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
