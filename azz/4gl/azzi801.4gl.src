   DEFINE l_insert        LIKE type_t.num5
   DEFINE ls_return       STRING
   DEFINE l_var_keys      DYNAMIC ARRAY OF STRING
   DEFINE l_var_keys_bak  DYNAMIC ARRAY OF STRING
   DEFINE l_field_keys    DYNAMIC ARRAY OF STRING
   DEFINE l_vars          DYNAMIC ARRAY OF STRING
   DEFINE l_fields        DYNAMIC ARRAY OF STRING
   DEFINE lc_gzxd002      LIKE gzxd_t.gzxd002    #使用者密碼
   DEFINE l_gzxz002  LIKE gzxz_t.gzxz002         #上次密碼更新時間 

   CALL cl_set_head_visible("","YES")

   IF p_cmd = 'r' THEN
      #此段落的r動作等同於a
      LET p_cmd = 'a'
   END IF

   LET l_insert = FALSE
   LET g_action_choice = ""
   LET g_qryparam.state = "i"

   #控制key欄位可否輸入
   CALL azzi801_set_entry(p_cmd)
   CALL azzi801_set_no_entry(p_cmd)

   DISPLAY BY NAME g_gzxd_m.gzxd001,g_gzxd_m.gzxd002,g_gzxd_m.passwd,g_gzxd_m.gzxd004

   DIALOG ATTRIBUTES(UNBUFFERED,FIELD ORDER FORM)

      #單頭段
      INPUT BY NAME g_gzxd_m.gzxd001,g_gzxd_m.gzxd002,g_gzxd_m.passwd,g_gzxd_m.gzxd004
         ATTRIBUTE(WITHOUT DEFAULTS)

         BEFORE INPUT
            IF s_transaction_chk("N",0) THEN
               CALL s_transaction_begin()
            END IF
            #其他table資料備份(確定是否更改用)

         AFTER FIELD gzxd001
            IF  NOT cl_null(g_gzxd_m.gzxd001) THEN
               IF p_cmd = 'a' OR ( p_cmd = 'u' AND (g_gzxd_m.gzxd001 != g_gzxd001_t )) THEN
                  IF NOT ap_chk_notDup("","SELECT COUNT(*) FROM gzxd_t WHERE "||"gzxdent = '" ||g_enterprise|| "' AND "||"gzxd001 = '"||g_gzxd_m.gzxd001 ||"'",'std-00004',0) THEN
                     NEXT FIELD CURRENT
                  END IF
               END IF
            END IF

         AFTER INPUT
            IF INT_FLAG THEN
               EXIT DIALOG
            END IF
            
            IF g_gzxd_m.gzxd002 IS NULL THEN
               INITIALIZE g_errparam TO NULL
               LET g_errparam.code =  "azz-00110"
               LET g_errparam.extend = "g_gzxd_m.gzxd001"
               LET g_errparam.popup = FALSE
               CALL cl_err()
               EXIT DIALOG
            END IF

            IF p_cmd <> "u" THEN
               LET l_count = 1

               SELECT COUNT(*) INTO l_count FROM gzxd_t
                WHERE gzxdent = g_enterprise AND gzxd001 = g_gzxd_m.gzxd001

               IF l_count = 0 THEN
                  #密碼加密
                  #161220-00012#1 -start   #161228-00003#1
                  IF cl_get_para(g_enterprise,"","E-SYS-2010") IS NOT NULL AND cl_get_para(g_enterprise,"","E-SYS-2010") = "Y" THEN
                     UPDATE gzxs_t SET gzxs002 = g_gzxd_m.gzxd002
                      WHERE gzxsent = g_enterprise AND gzxs001 = g_gzxd_m.gzxd002
                     IF SQLCA.SQLCODE THEN
                        DISPLAY "upd gzxs_t:",SQLCA.SQLCODE,":",SQLERRMESSAGE
                     END IF
                  END IF
                  #161220-00012#1 -end
                  LET lc_gzxd002 = cl_hashkey_user_encode(g_gzxd_m.gzxd002)
                  IF g_gzxd_m.gzxd004 IS NULL THEN LET g_gzxd_m.gzxd004 = "N" END IF
                  
                  INSERT INTO gzxd_t (gzxdent,gzxd001,gzxd002,gzxd004,gzxdstus)
                  VALUES (g_enterprise,g_gzxd_m.gzxd001,lc_gzxd002,g_gzxd_m.gzxd004,"Y")

                  IF SQLCA.sqlcode THEN
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code = SQLCA.sqlcode
                     LET g_errparam.extend = "gzxd_t"
                     LET g_errparam.popup = TRUE
                     CALL cl_err()
                     CONTINUE DIALOG
                  END IF

                  CALL s_transaction_end('Y','0')
               ELSE
                  CALL s_transaction_end('N','0')    #161220-00012#1
                  INITIALIZE g_errparam TO NULL
                  LET g_errparam.code =  "std-00006"
                  LET g_errparam.extend =  "g_gzxd_m.gzxd001"
                  LET g_errparam.popup = FALSE
                  CALL cl_err()
               END IF
            ELSE
               #add-point:單頭修改前
               #密碼加密
               #161220-00012#1 -start   #161228-00003#1
               IF cl_get_para(g_enterprise,"","E-SYS-2010") IS NOT NULL AND cl_get_para(g_enterprise,"","E-SYS-2010") = "Y" THEN
                  UPDATE gzxs_t SET gzxs002 = g_gzxd_m.gzxd002
                   WHERE gzxsent = g_enterprise AND gzxs001 = g_gzxd_m.gzxd001
                  IF SQLCA.SQLCODE THEN
                     DISPLAY "ins gzxs_t:",SQLCA.SQLCODE,":",SQLERRMESSAGE
                  END IF
               END IF
               #161220-00012#1 -end
               LET lc_gzxd002 = cl_hashkey_user_encode(g_gzxd_m.gzxd002)
               #end add-point
               UPDATE gzxd_t SET gzxd001 = g_gzxd_m.gzxd001,
                                 gzxd002 = lc_gzxd002,
                                 gzxd004 = g_gzxd_m.gzxd004,
                                 gzxdstus = "Y"
                WHERE gzxdent = g_enterprise AND gzxd001 = g_gzxd001_t
               
               CASE
                  WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                     CALL s_transaction_end('N','0')    #161220-00012#1
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code = "std-00009"
                     LET g_errparam.extend = "gzxd_t"
                     LET g_errparam.popup = TRUE
                     CALL cl_err()

                  WHEN SQLCA.sqlcode #其他錯誤
                     CALL s_transaction_end('N','0')    #161220-00012#1
                     INITIALIZE g_errparam TO NULL
                     LET g_errparam.code = SQLCA.sqlcode
                     LET g_errparam.extend = "gzxd_t"
                     LET g_errparam.popup = TRUE
                     CALL cl_err()

                  OTHERWISE
                     #161020-00003 #1 start
                      LET l_count = 0                    
                      #160802-00042 #1 start
                      #SELECT MAX(gzxz003)+1 INTO l_count FROM gzxz_t
                      # WHERE gzxzent = g_enterprise 
                      #  AND  gzxz001 = g_gzxd001_t

                      #LET l_count = NVL(l_count,1)
                     #161020-00003 #1 end 
                     LET l_gzxz002 = cl_get_today()

                     UPDATE gzxz_t SET gzxz002 = l_gzxz002,
                                       gzxz003 = l_count
                      WHERE gzxzent = g_enterprise
                        AND gzxz001 = g_gzxd001_t                

                     IF SQLCA.sqlerrd[3] OR SQLCA.sqlcode THEN 
                        CASE
                           WHEN SQLCA.sqlerrd[3] = 0  #更新不到的處理
                              CALL s_transaction_end('N','0')    #161220-00012#1
                              INITIALIZE g_errparam TO NULL
                              LET g_errparam.code = "std-00009"
                              LET g_errparam.extend = "gzxz_t"
                              LET g_errparam.popup = TRUE
                              CALL cl_err()

                           WHEN SQLCA.sqlcode #其他錯誤
                              CALL s_transaction_end('N','0')    #161220-00012#1
                              INITIALIZE g_errparam TO NULL
                              LET g_errparam.code = SQLCA.sqlcode
                              LET g_errparam.extend = "gzxz_t"
                              LET g_errparam.popup = TRUE
                              CALL cl_err()
                           END CASE 
                        ELSE
                           CALL s_transaction_end('Y','0')
                           # 修改OS層user password #161206-00028
                           CALL s_azzi800_01_passwd_os_user(g_gzxd_m.gzxd001, g_gzxd_m.gzxd002)
                        END IF 
                     #CALL s_transaction_end('Y','0')
                     #160802-00042 #1 end 
                                          
               END CASE
            END IF
           #controlp
      END INPUT

      ON ACTION controlf
         CALL cl_set_focus_form(ui.Interface.getRootNode()) RETURNING g_fld_name,g_frm_name
         CALL cl_fldhelp(g_frm_name, g_fld_name, g_lang)

      ON ACTION controlr
