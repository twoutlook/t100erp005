PRIVATE FUNCTION azzi801_init_chk()

   DEFINE ls_sql STRING
   DEFINE li_cnt LIKE type_t.num5

   IF cl_null(g_argv[1]) THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = "azz-00230"
      LET g_errparam.extend = ""
      LET g_errparam.popup = TRUE
      CALL cl_err()
      RETURN FALSE
   END IF

   LET ls_sql = " SELECT gzxa001 FROM gzxa_t ",
                 " WHERE gzxaent = ",g_enterprise,
                   " AND gzxa001 = '",g_argv[1] CLIPPED,"'"
   PREPARE azzi801_init_chk FROM ls_sql
   EXECUTE azzi801_init_chk

   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = "azz-00230"
      LET g_errparam.extend = ""
      LET g_errparam.popup = TRUE
      CALL cl_err()
      RETURN FALSE
   END IF
   FREE azzi801_init_chk

   RETURN TRUE
END FUNCTION
################################################################################
# Usage..........: CALL azzi801_chk_lock()
#                  RETURNING TRUE/FALSE
################################################################################
PRIVATE FUNCTION azzi801_chk_lock()
   LET g_gzxd001_t = g_gzxd_m.gzxd001
 
   CALL s_transaction_begin()
   
   OPEN azzi801_cl USING g_enterprise,g_gzxd_m.gzxd001
   IF STATUS THEN
      CLOSE azzi801_cl
      CALL s_transaction_end('N','0')   #161220-00012#1
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code =  STATUS
      LET g_errparam.extend = "OPEN azzi801_cl:"
      LET g_errparam.popup = TRUE
      CALL cl_err()
      RETURN TRUE  
   END IF

   #鎖住將被更改或取消的資料
   FETCH azzi801_cl INTO g_gzxd_m.gzxd001,g_gzxd_m.gzxd002,g_gzxd_m.passwd,g_gzxd_m.gzxd004
 
   #資料被他人LOCK, 或是sql執行時出現錯誤
   IF SQLCA.sqlcode THEN
      CLOSE azzi801_cl
      CALL s_transaction_end('N','0')   #161220-00012#1
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend = "gzxd_t"
      LET g_errparam.popup = FALSE
      CALL cl_err()
      RETURN TRUE  
   END IF
   RETURN FALSE  
END FUNCTION

