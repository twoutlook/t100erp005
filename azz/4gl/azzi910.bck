       g_gzzz_m.gzzzownid_desc,g_gzzz_m.gzzzowndp_desc,g_gzzz_m.gzzzcrtid_desc,g_gzzz_m.gzzzcrtdp_desc, 
       g_gzzz_m.gzzzmodid_desc
   
   
   #檢查是否允許此動作
   IF NOT azzi910_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #遮罩相關處理
   LET g_gzzz_m_mask_o.* =  g_gzzz_m.*
   CALL azzi910_gzzz_t_mask()
   LET g_gzzz_m_mask_n.* =  g_gzzz_m.*
   
   #將最新資料顯示到畫面上
   CALL azzi910_show()
   
   IF cl_ask_delete() THEN
 
      #add-point:單頭刪除前 name="delete.head.b_delete"
      
      #end add-point
 
      #應用 a47 樣板自動產生(Version:4)
      #刪除相關文件
      CALL azzi910_set_pk_array()
      #add-point:相關文件刪除前 name="delete.befroe.related_document_remove"
      
      #end add-point   
      CALL cl_doc_remove()  
 
 
 
 
 
      DELETE FROM gzzz_t 
       WHERE  gzzz001 = g_gzzz_m.gzzz001 
 
 
      #add-point:單頭刪除中 name="delete.head.m_delete"
    
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = SQLCA.sqlcode
         LET g_errparam.extend = "gzzz_t"
         LET g_errparam.popup = FALSE
         CALL cl_err()

         CALL s_transaction_end('N','0')
      END IF
  #這支作業名稱 = 程式名稱, 則多語言不可刪除 如果不相等，才可以刪除    
  IF g_gzzz_m.gzzz001 <> g_gzzz_m.gzzz002 THEN     
      #end add-point
         
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "gzzz_t:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = FALSE 
         CALL s_transaction_end('N','0')
         CALL cl_err()
      END IF
  
      INITIALIZE l_var_keys_bak TO NULL 
   INITIALIZE l_field_keys   TO NULL 
   LET l_var_keys_bak[01] = g_master_multi_table_t.gzzal001
   LET l_field_keys[01] = 'gzzal001'
   CALL cl_multitable_delete(l_field_keys,l_var_keys_bak,'gzzal_t')
 
      
      #add-point:單頭刪除後 name="delete.head.a_delete"
 END IF 
 
   #160720-00010 #1 start
   #刪除azzi800 設定的作業功能權限
   DELETE FROM gzxb_t 
    WHERE gzxb003 = g_gzzz_m.gzzz001  

   #刪除azzi800 設定的作業功能權限的可拜訪據點
   DELETE FROM gzxc_t 
    WHERE  gzxc003 = g_gzzz_m.gzzz001 

   #刪除azzi850 設定的職能角色權限的可使用的作業
   DELETE FROM gzyb_t 
    WHERE gzyb002 = g_gzzz_m.gzzz001  

   #刪除azzi850 設定的職能角色權限的可使用功能及資料授權
   DELETE FROM gzyc_t 
    WHERE gzyc002 = g_gzzz_m.gzzz001
   #160720-00010 #1 end   
      #end add-point
      
       
 
      #修改歷程記錄(刪除)
      IF NOT cl_log_modified_record('','') THEN 
         CLOSE azzi910_cl
         CALL s_transaction_end('N','0')
         RETURN
      END IF
      
      CLEAR FORM
      CALL azzi910_ui_browser_refresh()
      
      #確保畫面上保有資料
      IF g_browser_cnt > 0 THEN
         #CALL azzi910_browser_fill(g_wc,"")
         CALL azzi910_fetch("P")
      ELSE
         CLEAR FORM
      END IF
      CALL s_transaction_end('Y','0')
   ELSE    
      CALL s_transaction_end('N','0')
   END IF
 
   CLOSE azzi910_cl
 
   #功能已完成,通報訊息中心
   CALL azzi910_msgcentre_notify('delete')
 
   #add-point:單頭刪除完成後 name="delete.a_delete"
   
   #end add-point
 
