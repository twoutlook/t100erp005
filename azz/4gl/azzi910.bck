# Descriptions...: 重產42s
# Memo...........:
# Usage..........: CALL azzi910_regen_42s()
# Input parameter: 
# Return code....: 
# Date & Author..: 2014/12/27 By jrg542
# Modify.........:
################################################################################
PRIVATE FUNCTION azzi910_regen_42s()
   DEFINE ld_lng_type   DYNAMIC ARRAY OF RECORD
      gzzy001 LIKE gzzy_t.gzzy001
             END RECORD 
   DEFINE la_param      RECORD
             prog       STRING,
             actionid   STRING,
             param      DYNAMIC ARRAY OF STRING
             END RECORD
   DEFINE li_cnt        LIKE type_t.num5
   DEFINE ls_js         STRING
   DEFINE ls_sql        STRING 
   DEFINE li_count      LIKE type_t.num5

   IF cl_ask_confirm("azz-00099") THEN
      LET ld_lng_type = s_azzi070_get_gzzy()
      CALL cl_progress_bar(ld_lng_type.getLength())
      FOR li_cnt = 1 TO  ld_lng_type.getLength() 
          CALL cl_progress_ing("Building:"||ld_lng_type[li_cnt].gzzy001 || " str、42s")
          LET la_param.prog = "azzp191"
          LET la_param.param[1] = g_gzzz_m.gzzz001
          LET la_param.param[2] = ld_lng_type[li_cnt].gzzy001
          LET ls_js = util.JSON.stringify(la_param)
          CALL cl_cmdrun_wait(ls_js)
      END FOR  
   END IF
END FUNCTION

################################################################################
# Descriptions...: 批次更新職能角色權限
# Memo...........:
# Usage..........: CALL azzi910_upd_auth_all()
#                  RETURNING 回传参数
# Input parameter: 
# Return code....: 
# Date & Author..: 2015/03/16 By jrg542
# Modify.........:
################################################################################
PRIVATE FUNCTION azzi910_upd_auth_all()
   DEFINE ls_auth  STRING
   DEFINE l_token  base.StringTokenizer
   DEFINE ls_token STRING
   DEFINE li_cnt   LIKE type_t.num5

   LET ls_auth = g_qryparam.return1

   LET l_token = base.StringTokenizer.create(ls_auth, "|")
   LET li_cnt = l_token.countTokens()
   IF li_cnt <> 0  THEN
      CALL cl_progress_bar(li_cnt)
      WHILE l_token.hasMoreTokens()
         LET ls_token = l_token.nextToken()
         CALL cl_progress_ing("r.r azzp185 " || ls_token || " " || g_gzzz_m.gzzz001)
         RUN "r.r azzp185 " || ls_token || " " || g_gzzz_m.gzzz001
      END WHILE

      INITIALIZE g_errparam TO NULL
      LET g_errparam.code   = "azz-00278" #本支程式%1，加入到這職能群組%2
      LET g_errparam.popup  = TRUE
      LET g_errparam.replace[1] = g_gzzz_m.gzzz001
      LET g_errparam.replace[2] = ls_auth
      CALL cl_err()
   END IF
END FUNCTION

