   IF p_cmd = 'u' AND g_chkey = 'N' THEN
      CALL cl_set_comp_entry("gzzz001",FALSE)
      #根據azzi850使用者身分開關特定欄位
      IF NOT cl_null(g_no_entry) THEN
         CALL cl_set_comp_entry(g_no_entry,FALSE)
      END IF
      #add-point:set_no_entry段欄位控制 name="set_no_entry.field_control"
      IF NOT cl_null(g_gzzz_m.gzzz002) OR NOT cl_null(g_gzzz_m.gzzz001) THEN 
         #作業編號 = 程式編號 不能修改
         IF g_gzzz_m.gzzz001 = g_gzzz_m.gzzz002 THEN 
            CALL cl_set_comp_entry("gzzz003,gzzal003,gzzal004",FALSE)
            LET g_gzzz_m.gzzz003 = "0"
         ELSE 
            CALL cl_set_comp_entry("gzzz003,gzzal003,gzzal004",TRUE)
         END IF 
      END IF
      #end add-point 
   END IF
   
   #add-point:set_no_entry段欄位控制後 name="set_no_entry.after_control"
   
   #end add-point 
 
