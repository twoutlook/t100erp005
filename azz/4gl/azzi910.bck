PRIVATE FUNCTION azzi910_cnt_gzza001()
   DEFINE li_cnt LIKE type_t.num5
   #輸入後檢查，如果 gzzz001存在於 gzza001中，則幫忙把 gzzz002 填入 (值等同於gzzz001)
   SELECT COUNT(*) INTO li_cnt FROM gzza_t
    WHERE gzza001 = g_gzzz_m.gzzz001
   IF li_cnt > 0 THEN 
      RETURN TRUE 
   END IF 
   RETURN FALSE 
END FUNCTION

 
