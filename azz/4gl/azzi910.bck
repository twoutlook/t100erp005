               " LEFT JOIN gzzk_t t2 ON t2.gzzk001=t0.gzzz002 AND t2.gzzk002=t0.gzzz003  ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent="||g_enterprise||" AND t3.ooag001=t0.gzzzownid  ",
