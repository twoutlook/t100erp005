{<section id="azzi910.state_change" >}
   #應用 a09 樣板自動產生(Version:17)
#+ 確認碼變更 
PRIVATE FUNCTION azzi910_statechange()
   #add-point:statechange段define(客製用) name="statechange.define_customerization"
   
   #end add-point  
   DEFINE lc_state LIKE type_t.chr5
   #add-point:statechange段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="statechange.define"
   
   #end add-point  
   
   #add-point:Function前置處理 name="statechange.before"
   
   #end add-point  
   
   ERROR ""     #清空畫面右下側ERROR區塊
 
   IF g_gzzz_m.gzzz001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF
 
   CALL s_transaction_begin()
   
   OPEN azzi910_cl USING g_gzzz_m.gzzz001
   IF STATUS THEN
      CLOSE azzi910_cl
      CALL s_transaction_end('N','0')
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN azzi910_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CALL cl_err()
      RETURN
   END IF
   
   #顯示最新的資料
   EXECUTE azzi910_master_referesh USING g_gzzz_m.gzzz001 INTO g_gzzz_m.gzzz001,g_gzzz_m.gzzz002,g_gzzz_m.gzzz003, 
       g_gzzz_m.gzzz004,g_gzzz_m.gzzz005,g_gzzz_m.gzzz010,g_gzzz_m.gzzz006,g_gzzz_m.gzzzstus,g_gzzz_m.gzzz007, 
       g_gzzz_m.gzzz008,g_gzzz_m.gzzzownid,g_gzzz_m.gzzzowndp,g_gzzz_m.gzzzcrtid,g_gzzz_m.gzzzcrtdp, 
       g_gzzz_m.gzzzcrtdt,g_gzzz_m.gzzzmodid,g_gzzz_m.gzzzmoddt,g_gzzz_m.gzzz002_desc,g_gzzz_m.gzzz003_desc, 
       g_gzzz_m.gzzzownid_desc,g_gzzz_m.gzzzowndp_desc,g_gzzz_m.gzzzcrtid_desc,g_gzzz_m.gzzzcrtdp_desc, 
       g_gzzz_m.gzzzmodid_desc
   
 
   #檢查是否允許此動作
   IF NOT azzi910_action_chk() THEN
      CLOSE azzi910_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #將資料顯示到畫面上
   DISPLAY BY NAME g_gzzz_m.gzzz001,g_gzzz_m.gzzz002,g_gzzz_m.gzzz002_desc,g_gzzz_m.gzzal003,g_gzzz_m.gzzal004, 
       g_gzzz_m.gzzz003,g_gzzz_m.gzzz003_desc,g_gzzz_m.gzzz004,g_gzzz_m.gzzz005,g_gzzz_m.gzzz010,g_gzzz_m.gzzz006, 
       g_gzzz_m.gzzzstus,g_gzzz_m.gzzz007,g_gzzz_m.gzzz008,g_gzzz_m.gzzzownid,g_gzzz_m.gzzzownid_desc, 
       g_gzzz_m.gzzzowndp,g_gzzz_m.gzzzowndp_desc,g_gzzz_m.gzzzcrtid,g_gzzz_m.gzzzcrtid_desc,g_gzzz_m.gzzzcrtdp, 
       g_gzzz_m.gzzzcrtdp_desc,g_gzzz_m.gzzzcrtdt,g_gzzz_m.gzzzmodid,g_gzzz_m.gzzzmodid_desc,g_gzzz_m.gzzzmoddt 
 
 
   CASE g_gzzz_m.gzzzstus
      WHEN "N"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
      WHEN "Y"
         CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
      
   END CASE
 
   #add-point:資料刷新後 name="statechange.after_refresh"
   
   #end add-point
 
   MENU "" ATTRIBUTES (STYLE="popup")
      BEFORE MENU
         HIDE OPTION "approved"
         HIDE OPTION "rejection"
         CASE g_gzzz_m.gzzzstus
            
            WHEN "N"
               HIDE OPTION "inactive"
            WHEN "Y"
               HIDE OPTION "active"
         END CASE
     
      #add-point:menu前 name="statechange.before_menu"
      
      #end add-point
      
      
	  
      ON ACTION inactive
         IF cl_auth_chk_act("inactive") THEN
            LET lc_state = "N"
            #add-point:action控制 name="statechange.inactive"
            
            #end add-point
         END IF
         EXIT MENU
      ON ACTION active
         IF cl_auth_chk_act("active") THEN
            LET lc_state = "Y"
            #add-point:action控制 name="statechange.active"
            
            #end add-point
         END IF
         EXIT MENU
 
      #add-point:stus控制 name="statechange.more_control"
      
      #end add-point
      
   END MENU
   
   #確認被選取的狀態碼在清單中
   IF (lc_state <> "N" 
      AND lc_state <> "Y"
      ) OR 
      g_gzzz_m.gzzzstus = lc_state OR cl_null(lc_state) THEN
      CLOSE azzi910_cl
      CALL s_transaction_end('N','0')
      RETURN
   END IF
   
   #add-point:stus修改前 name="statechange.b_update"
   
   #end add-point
   
   LET g_gzzz_m.gzzzmodid = g_user
   LET g_gzzz_m.gzzzmoddt = cl_get_current()
   LET g_gzzz_m.gzzzstus = lc_state
   
   #異動狀態碼欄位/修改人/修改日期
   UPDATE gzzz_t 
      SET (gzzzstus,gzzzmodid,gzzzmoddt) 
        = (g_gzzz_m.gzzzstus,g_gzzz_m.gzzzmodid,g_gzzz_m.gzzzmoddt)     
    WHERE  gzzz001 = g_gzzz_m.gzzz001
 
    
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = SQLCA.sqlcode 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
   ELSE
      CASE lc_state
         WHEN "N"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/inactive.png")
         WHEN "Y"
            CALL gfrm_curr.setElementImage("statechange", "stus/32/active.png")
         
      END CASE
    
      #撈取異動後的資料
      EXECUTE azzi910_master_referesh USING g_gzzz_m.gzzz001 INTO g_gzzz_m.gzzz001,g_gzzz_m.gzzz002, 
          g_gzzz_m.gzzz003,g_gzzz_m.gzzz004,g_gzzz_m.gzzz005,g_gzzz_m.gzzz010,g_gzzz_m.gzzz006,g_gzzz_m.gzzzstus, 
          g_gzzz_m.gzzz007,g_gzzz_m.gzzz008,g_gzzz_m.gzzzownid,g_gzzz_m.gzzzowndp,g_gzzz_m.gzzzcrtid, 
          g_gzzz_m.gzzzcrtdp,g_gzzz_m.gzzzcrtdt,g_gzzz_m.gzzzmodid,g_gzzz_m.gzzzmoddt,g_gzzz_m.gzzz002_desc, 
          g_gzzz_m.gzzz003_desc,g_gzzz_m.gzzzownid_desc,g_gzzz_m.gzzzowndp_desc,g_gzzz_m.gzzzcrtid_desc, 
          g_gzzz_m.gzzzcrtdp_desc,g_gzzz_m.gzzzmodid_desc
      
      #將資料顯示到畫面上
      DISPLAY BY NAME g_gzzz_m.gzzz001,g_gzzz_m.gzzz002,g_gzzz_m.gzzz002_desc,g_gzzz_m.gzzal003,g_gzzz_m.gzzal004, 
          g_gzzz_m.gzzz003,g_gzzz_m.gzzz003_desc,g_gzzz_m.gzzz004,g_gzzz_m.gzzz005,g_gzzz_m.gzzz010, 
          g_gzzz_m.gzzz006,g_gzzz_m.gzzzstus,g_gzzz_m.gzzz007,g_gzzz_m.gzzz008,g_gzzz_m.gzzzownid,g_gzzz_m.gzzzownid_desc, 
          g_gzzz_m.gzzzowndp,g_gzzz_m.gzzzowndp_desc,g_gzzz_m.gzzzcrtid,g_gzzz_m.gzzzcrtid_desc,g_gzzz_m.gzzzcrtdp, 
          g_gzzz_m.gzzzcrtdp_desc,g_gzzz_m.gzzzcrtdt,g_gzzz_m.gzzzmodid,g_gzzz_m.gzzzmodid_desc,g_gzzz_m.gzzzmoddt 
 
   END IF
 
   #add-point:stus修改後 name="statechange.a_update"
   
   #end add-point
 
   #add-point:statechange段結束前 name="statechange.after"
   
   #end add-point  
 
   CLOSE azzi910_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL azzi910_msgcentre_notify('statechange:'||lc_state)
   
END FUNCTION
 
 
 
 
