{<section id="azzi910.modify" >}
#+ 資料修改
PRIVATE FUNCTION azzi910_modify()
   #add-point:modify段define(客製用) name="modify.define_customerization"
   
   #end add-point
   #add-point:modify段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="modify.define"
   
   #end add-point
   
   #add-point:Function前置處理 name="modify.pre_function"
   
   #end add-point
   
   #先確定key值無遺漏
   IF g_gzzz_m.gzzz001 IS NULL
 
   THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "" 
      LET g_errparam.code   = "std-00003" 
      LET g_errparam.popup  = FALSE 
      CALL cl_err()
      RETURN
   END IF 
 
   ERROR ""
  
   #備份key值
   LET g_gzzz001_t = g_gzzz_m.gzzz001
 
   
   CALL s_transaction_begin()
   
   #先lock資料
   OPEN azzi910_cl USING g_gzzz_m.gzzz001
   IF STATUS THEN
      INITIALIZE g_errparam TO NULL 
      LET g_errparam.extend = "OPEN azzi910_cl:" 
      LET g_errparam.code   = STATUS 
      LET g_errparam.popup  = TRUE 
      CLOSE azzi910_cl
      CALL s_transaction_end('N','0')
      CALL cl_err()
      RETURN
   END IF
 
   #顯示最新的資料
   EXECUTE azzi910_master_referesh USING g_gzzz_m.gzzz001 INTO g_gzzz_m.gzzz001,g_gzzz_m.gzzz002,g_gzzz_m.gzzz003, 
       g_gzzz_m.gzzz004,g_gzzz_m.gzzz005,g_gzzz_m.gzzz010,g_gzzz_m.gzzz006,g_gzzz_m.gzzzstus,g_gzzz_m.gzzz007, 
       g_gzzz_m.gzzz008,g_gzzz_m.gzzzownid,g_gzzz_m.gzzzowndp,g_gzzz_m.gzzzcrtid,g_gzzz_m.gzzzcrtdp, 
       g_gzzz_m.gzzzcrtdt,g_gzzz_m.gzzzmodid,g_gzzz_m.gzzzmoddt,g_gzzz_m.gzzz002_desc,g_gzzz_m.gzzz003_desc, 
       g_gzzz_m.gzzzownid_desc,g_gzzz_m.gzzzowndp_desc,g_gzzz_m.gzzzcrtid_desc,g_gzzz_m.gzzzcrtdp_desc, 
       g_gzzz_m.gzzzmodid_desc
 
   #檢查是否允許此動作
   IF NOT azzi910_action_chk() THEN
      CALL s_transaction_end('N','0')
      RETURN
   END IF
 
   #遮罩相關處理
   LET g_gzzz_m_mask_o.* =  g_gzzz_m.*
   CALL azzi910_gzzz_t_mask()
   LET g_gzzz_m_mask_n.* =  g_gzzz_m.*
   
   
 
   #顯示資料
   CALL azzi910_show()
   
   WHILE TRUE
      LET g_gzzz_m.gzzz001 = g_gzzz001_t
 
      
      #寫入修改者/修改日期資訊
      LET g_gzzz_m.gzzzmodid = g_user 
LET g_gzzz_m.gzzzmoddt = cl_get_current()
LET g_gzzz_m.gzzzmodid_desc = cl_get_username(g_gzzz_m.gzzzmodid)
      
      #add-point:modify段修改前 name="modify.before_input"
      
      #end add-point
 
      #資料輸入
      CALL azzi910_input("u")     
 
      #add-point:modify段修改後 name="modify.after_input"
      
      #end add-point
      
      IF INT_FLAG THEN
         CALL s_transaction_end('N','0')
         LET INT_FLAG = 0
         LET g_gzzz_m.* = g_gzzz_m_t.*
         CALL azzi910_show()
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "" 
         LET g_errparam.code   = 9001 
         LET g_errparam.popup  = FALSE 
         CALL cl_err()
         EXIT WHILE
      END IF
 
      #若有modid跟moddt則進行update
      UPDATE gzzz_t SET (gzzzmodid,gzzzmoddt) = (g_gzzz_m.gzzzmodid,g_gzzz_m.gzzzmoddt)
       WHERE  gzzz001 = g_gzzz001_t
 
 
      EXIT WHILE
      
   END WHILE
 
   #根據資料狀態切換action狀態
   CALL cl_set_act_visible("statechange,modify,delete,reproduce", TRUE)
   CALL azzi910_set_act_visible()
   CALL azzi910_set_act_no_visible()
 
   #組合新增資料的條件
   LET g_add_browse = " ",
                      " gzzz001 = '", g_gzzz_m.gzzz001, "' "
 
   #填到對應位置
   CALL azzi910_browser_fill(g_wc,"")
 
   CLOSE azzi910_cl
   CALL s_transaction_end('Y','0')
 
   #功能已完成,通報訊息中心
   CALL azzi910_msgcentre_notify('modify')
   
   LET g_worksheet_hidden = 0
   
END FUNCTION   
 
