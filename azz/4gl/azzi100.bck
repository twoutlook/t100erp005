################################################################################
PRIVATE FUNCTION azzi100_chk_gzzd(lc_type)
   DEFINE lc_gzzd001      LIKE gzzd_t.gzzd001  #畫面編號
   DEFINE lc_gzzd003      LIKE gzzd_t.gzzd003  #待轉標籤
   DEFINE lc_gzzd004      LIKE gzzd_t.gzzd004  #使用標示
   DEFINE lc_gzzd002_old  LIKE gzzd_t.gzzd002  #指定原始語言別
   DEFINE lc_gzzd005_old  LIKE gzzd_t.gzzd005  #
   DEFINE lc_gzzd006_old  LIKE gzzd_t.gzzd006  #
   DEFINE lc_gzzd005_new  LIKE gzzd_t.gzzd005  #
   DEFINE lc_gzzd006_new  LIKE gzzd_t.gzzd006  #
   DEFINE lc_gzzd002_ref  LIKE gzzd_t.gzzd002  #指定參考語言別
   DEFINE lc_gzzd005_ref  LIKE gzzd_t.gzzd005  #指定新語詞
   DEFINE lc_gzzd006_ref  LIKE gzzd_t.gzzd006  #指定新語詞
   DEFINE lc_find         LIKE gzzd_t.gzzd005  #
   DEFINE lc_match        LIKE gzzd_t.gzzd005  #
   DEFINE ls_sql          STRING
   DEFINE ls_temp         STRING
   DEFINE lc_type         LIKE type_t.chr1
   DEFINE lc_gzzd005_upd  LIKE type_t.chr1
   DEFINE lc_gzzd006_upd  LIKE type_t.chr1
   DEFINE lc_ch            base.Channel
   DEFINE lc_begin         STRING
   DEFINE lc_gzzd_file     STRING
   DEFINE lc_str           STRING

   LET lc_gzzd002_old = g_gzoy_d[g_detail_idx].gzoy002   #調整語系
   LET lc_find        = g_gzoy_d[g_detail_idx].gzoy003    #調整前語詞

   #參考的部分不一定會有值
   LET lc_gzzd002_ref = g_gzoy_d[g_detail_idx].gzoy005   #參考語系
   LET lc_match       = g_gzoy_d[g_detail_idx].gzoy006    #參考語詞

   #先找出指定詞在指定語系內的標籤
   LET ls_sql = "SELECT gzzd001,gzzd003,gzzd004,gzzd005,gzzd006 FROM gzzd_t ",
                " WHERE gzzd002 = '",lc_gzzd002_old CLIPPED,"' "   #原有語言別

   #是否為整詞的處理
   IF g_gzoy_d[g_detail_idx].gzoy004 = "Y" THEN  #整詞
      LET ls_sql = ls_sql, " AND ((gzzd005 = '",lc_find CLIPPED,"') ",   #待查字詞
                                " OR (gzzd006 = '",lc_find CLIPPED,"'))"
   ELSE
      LET ls_sql = ls_sql, " AND ((gzzd005 LIKE '%",lc_find CLIPPED,"%') ",   #待查字詞
                                 " OR (gzzd006 LIKE '%",lc_find CLIPPED,"%'))"
   END IF

   #準備寫入檔案
   LET lc_begin = cl_get_today()
   LET lc_ch = base.Channel.create()
   LET lc_gzzd_file = "azzi100_gzzd_",lc_begin CLIPPED ,".txt"
   LET lc_gzzd_file = os.Path.join(FGL_GETENV("TEMPDIR"),lc_gzzd_file)
   IF os.Path.delete(lc_gzzd_file) THEN   #刪除舊檔
   END IF

   DECLARE azzi100_gzzd_cs CURSOR FROM ls_sql
   FOREACH azzi100_gzzd_cs INTO lc_gzzd001,lc_gzzd003,lc_gzzd004,lc_gzzd005_old,lc_gzzd006_old  #找出舊詞
      LET lc_gzzd005_upd = "N"
      LET lc_gzzd006_upd = "N"

      #如果有填參考的話，做個檢驗
      IF NOT cl_null(lc_gzzd002_ref) AND NOT cl_null(lc_match) THEN
         SELECT gzzd005,gzzd006 INTO lc_gzzd005_ref,lc_gzzd006_ref
           FROM gzzd_t
          WHERE gzzd001 = lc_gzzd001 AND gzzd002 = lc_gzzd002_ref
            AND gzzd003 = lc_gzzd003 AND gzzd004 = lc_gzzd004
         IF lc_match IS NOT NULL THEN
            IF g_gzoy_d[g_detail_idx].gzoy004 = "Y" THEN  #整詞
               IF lc_gzzd005_ref = lc_match THEN   #待轉標籤文字
                  LET lc_gzzd005_upd = "Y"
               END IF

               IF lc_gzzd006_ref = lc_match THEN   #待轉註解
                  LET lc_gzzd006_upd = "Y"
               END IF
            ELSE  #部分詞
               LET ls_temp = lc_gzzd005_ref   #待轉標籤文字
               IF ls_temp.getIndexOf(lc_match,1) THEN
                  LET lc_gzzd005_upd = "Y"
               END IF

               LET ls_temp = lc_gzzd006_ref   #待轉註解
               IF ls_temp.getIndexOf(lc_match,1) THEN
                  LET lc_gzzd006_upd = "Y"
               END IF
            END IF
         END IF
      END IF

      IF lc_type = "u" THEN   
         #組出字詞更新語法
         IF lc_gzzd005_upd = "Y" THEN   #待轉標籤文字
            LET lc_gzzd005_new = cl_replace_str(lc_gzzd005_old,
                                                 g_gzoy_d[g_detail_idx].gzoy003 CLIPPED,
                                                 g_gzoy_d[g_detail_idx].gzoy008 CLIPPED)

            LET lc_str = "UPDATE gzzd_t SET gzzd005='",lc_gzzd005_new CLIPPED,"' ",
                          "WHERE gzzd001='",lc_gzzd001 CLIPPED,"' ",
                            "AND gzzd002='",lc_gzzd002_old CLIPPED,"' ",
                            "AND gzzd003='",lc_gzzd003 CLIPPED,"' ",
                            "AND gzzd004='",lc_gzzd004 CLIPPED,"' ",
                            "AND gzzd005='",lc_gzzd005_old CLIPPED,"' "

            DISPLAY "UPDATE gzzd_t SET gzzd005='",lc_gzzd005_new CLIPPED,"' ",
