################################################################################
# Descriptions...: 執行更新字典檔資料(gzoz_t),且更新為繁體時,新增多語言用詞調整暫存表gzos_t
# Memo...........:
# Usage..........: CALL azzi100_upd_gzoz() RETURNING r_success
# Input parameter: 無
# Return code....: r_success
# Date & Author..: 2016/03/09 By sakura  #160302-00017#3
# Modify.........:
################################################################################
PRIVATE FUNCTION azzi100_upd_gzoz()
DEFINE r_success        LIKE type_t.num5
DEFINE lc_gzoz000       LIKE gzoz_t.gzoz000
DEFINE lc_gzoz501       LIKE gzoz_t.gzoz501
DEFINE lc_gzoz000_old   LIKE gzoz_t.gzoz000    #調整語詞
DEFINE lc_gzoz000_new   LIKE gzoz_t.gzoz000    #調整語詞
DEFINE lc_gzoz000_ref   LIKE gzoz_t.gzoz000    #調整語詞
DEFINE lc_fieldid       LIKE type_t.chr10
DEFINE lc_find          LIKE gzoz_t.gzoz000
DEFINE lc_match         LIKE gzoz_t.gzoz000
DEFINE ls_sql           STRING
DEFINE ls_temp          STRING
DEFINE lc_str           STRING
DEFINE li_idx           LIKE type_t.num10
DEFINE l_cnt            LIKE type_t.num10
DEFINE l_time           DATETIME YEAR TO FRACTION(5)
   
   LET r_success = TRUE
   
   LET l_time = cl_get_time()
   #確認需要的資料有在
   IF cl_null(g_gzoy_d[g_detail_idx].gzoy002) OR
      cl_null(g_gzoy_d[g_detail_idx].gzoy003) THEN
      RETURN
   END IF

   IF g_detail_idx >= 1 AND NOT cl_null(g_detail_idx) THEN
      #判斷選取的筆數
      FOR li_idx = 1 TO g_gzoy_d.getLength()
          IF g_gzoy_d[li_idx].l_sel = 'Y' AND g_gzoy_d[li_idx].gzoystus = 'Y' THEN
             LET g_detail_idx = li_idx
			    LET lc_fieldid = azzi100_get_gzoz_fieldid(g_gzoy_d[g_detail_idx].gzoy002) #取回gzoz_t的多語言欄位
             LET lc_find = g_gzoy_d[g_detail_idx].gzoy003    #調整前語詞
             
             #參考的部分不一定會有值
             LET lc_match = g_gzoy_d[g_detail_idx].gzoy006   #參考語詞
             
             #先找出指定詞在指定語系內的標籤
             LET ls_sql = "SELECT gzoz000,gzoz501,",lc_fieldid CLIPPED
             
             #如果有參考語詞的話
             IF NOT cl_null(g_gzoy_d[g_detail_idx].gzoy005) AND NOT cl_null(lc_match) THEN
                LET ls_sql = ls_sql,",",azzi100_get_gzoz_fieldid(g_gzoy_d[g_detail_idx].gzoy005) CLIPPED
             ELSE
                LET ls_sql = ls_sql,",''"
             END IF
             
             #補其他的SQL
             LET ls_sql = ls_sql," FROM gzoz_t WHERE ",lc_fieldid CLIPPED
             
             #是否為整詞的處理
             IF g_gzoy_d[g_detail_idx].gzoy002 = "Y" THEN  #整詞
                LET ls_sql = ls_sql, " LIKE '%",lc_find CLIPPED,"%' "   #待查字詞
             ELSE
                LET ls_sql = ls_sql, " LIKE '%",lc_find CLIPPED,"%' "   #待查字詞
             END IF 
             
             DECLARE azzi100_gzoz_cs CURSOR FROM ls_sql
             FOREACH azzi100_gzoz_cs INTO lc_gzoz000,lc_gzoz501,lc_gzoz000_old,lc_gzoz000_ref  #找出舊詞
             
                #如果有填參考的話，做個檢驗
                IF NOT cl_null(g_gzoy_d[g_detail_idx].gzoy005) AND NOT cl_null(lc_match) THEN
                   IF g_gzoy_d[g_detail_idx].gzoy002 = "Y" THEN  #整詞
                      IF lc_gzoz000_ref = lc_match THEN
                         CONTINUE FOREACH
                      END IF
                   ELSE  #部分詞
                      LET ls_temp = lc_gzoz000_ref
                      IF NOT ls_temp.getIndexOf(lc_match,1) THEN
                         CONTINUE FOREACH
                      END IF
                   END IF
                END IF
                
                LET lc_gzoz000_new = cl_replace_str(lc_gzoz000_old,
                                                    g_gzoy_d[g_detail_idx].gzoy003 CLIPPED,
                                                    g_gzoy_d[g_detail_idx].gzoy008 CLIPPED)
                                                    
                LET lc_str = "UPDATE gzoz_t SET ",lc_fieldid CLIPPED,"='",lc_gzoz000_new CLIPPED,"'",
                             " WHERE gzoz000='",lc_gzoz000 CLIPPED,"' ",
                             "   AND ",lc_fieldid CLIPPED,"='",lc_gzoz000_old CLIPPED,"'"
                                     
                #DISPLAY "UPDATE gzoz_t SET ",lc_fieldid CLIPPED,"='",lc_gzoz000_new CLIPPED,"'",              
                #        " WHERE gzoz000='",lc_gzoz000 CLIPPED,"' ",
                #        "   AND ",lc_fieldid CLIPPED,"='",lc_gzoz000_old CLIPPED,"'"                        

                IF NOT cl_null(lc_str) THEN
                   #執行update
                   LET g_sql = "UPDATE gzoz_t SET ",lc_fieldid CLIPPED," = ? ",
                               "              ,gzozmodid = '",g_user,"'",
                               "              ,gzozmoddt = ? ",
                               " WHERE gzoz000= ? ",
                               "   AND ",lc_fieldid CLIPPED," = ? "
                   PREPARE azzi100_gzoz_upd_p1 FROM g_sql
                   EXECUTE azzi100_gzoz_upd_p1 USING lc_gzoz000_new,l_time,lc_gzoz000,lc_gzoz000_old
                   IF SQLCA.sqlcode THEN
                      CASE SQLCA.sqlcode
                        WHEN -268 
                         DELETE FROM gzoz_t
                          WHERE gzoz000 = lc_gzoz000
                         DISPLAY "DELETE :UPDATE gzoz_t SET ",lc_fieldid CLIPPED,"='",lc_gzoz000_new CLIPPED,"'",              
                                 " WHERE gzoz000='",lc_gzoz000 CLIPPED,"' ",
                                 "   AND ",lc_fieldid CLIPPED,"='",lc_gzoz000_old CLIPPED,"'"                            
                        WHEN -201
                         DISPLAY "UPDATE gzoz_t SET ",lc_fieldid CLIPPED,"='",lc_gzoz000_new CLIPPED,"'",              
                                 " WHERE gzoz000='",lc_gzoz000 CLIPPED,"' ",
                                 "   AND ",lc_fieldid CLIPPED,"='",lc_gzoz000_old CLIPPED,"'"                            
                        OTHERWISE
                         INITIALIZE g_errparam TO NULL
                         LET g_errparam.code = SQLCA.sqlcode
                         LET g_errparam.extend = 'EXECUTE azzi100_gzoz_upd_p1'
                         LET g_errparam.popup = TRUE
                         CALL cl_err()
                         LET r_success = FALSE
                         RETURN r_success                        
                      END CASE
                   END IF
                   #異動繁體時，更新多語言用詞調整暫存表gzos_t
                   IF lc_fieldid = 'gzoz000' THEN #正體中文
                      LET l_cnt = NULL
                      SELECT COUNT(*) INTO l_cnt
                        FROM gzos_t
                       WHERE gzos001 = 'zh_TW'
                         AND gzos002 = lc_gzoz000_old
                      #檢查是否已存在gzos_t                       
                      IF l_cnt = 0 THEN
                                            #調整語言別#調整前資料#調整後資料
                         INSERT INTO gzos_t (gzos001,gzos002,gzos003)
                                     VALUES ('zh_TW',lc_gzoz000_old,lc_gzoz000_new)
                         IF SQLCA.sqlcode THEN
                            INITIALIZE g_errparam TO NULL 
                            LET g_errparam.extend = "insert gzos_t" 
                            LET g_errparam.code   = SQLCA.sqlcode 
                            LET g_errparam.popup  = TRUE 
                            CALL cl_err()
                            LET r_success = FALSE
                            RETURN r_success                         
                         END IF
                      END IF
                   END IF                   
                END IF
             END FOREACH
          END IF
      END FOR
   END IF
   
   RETURN r_success
END FUNCTION

################################################################################
# Descriptions...: 描述说明
# Memo...........:
################################################################################
PRIVATE FUNCTION azzi100_get_gzoz_fieldid(lc_langid)
   DEFINE lc_langid  LIKE type_t.chr10
   DEFINE lc_fieldid LIKE type_t.chr10
   CASE
      WHEN lc_langid = "zh_TW" LET lc_fieldid = "gzoz000"
      WHEN lc_langid = "en_US" LET lc_fieldid = "gzoz001"
      WHEN lc_langid = "zh_CN" LET lc_fieldid = "gzoz002"
      WHEN lc_langid = "ja_JP" LET lc_fieldid = "gzoz003"
      WHEN lc_langid = "vi_VN" LET lc_fieldid = "gzoz004"
      WHEN lc_langid = "th_TH" LET lc_fieldid = "gzoz005"
      WHEN lc_langid = "ko_KR" LET lc_fieldid = "gzoz006"
   END CASE
   RETURN lc_fieldid
END FUNCTION

################################################################################
# Descriptions...: 組出需要更新gzoz_t的SQL資料
# Memo...........: 
# Usage..........: CALL azzi100_chk_gzoz(lc_type)
# Input parameter: lc_type        型態  u:update  s:查詢
# Date & Author..: 日期 By 作者
# Modify.........: 
################################################################################
PRIVATE FUNCTION azzi100_chk_gzoz(lc_type)
   DEFINE lc_gzoz000       LIKE gzoz_t.gzoz000
   DEFINE lc_gzoz501       LIKE gzoz_t.gzoz501
   DEFINE lc_gzoz000_old   LIKE gzoz_t.gzoz000    #調整語詞
   DEFINE lc_gzoz000_new   LIKE gzoz_t.gzoz000    #調整語詞
   DEFINE lc_gzoz000_ref   LIKE gzoz_t.gzoz000    #調整語詞
   DEFINE lc_find          LIKE gzoz_t.gzoz000  #
   DEFINE lc_match         LIKE gzoz_t.gzoz000  #
   DEFINE ls_sql           STRING
   DEFINE ls_temp          STRING
   DEFINE lc_type          LIKE type_t.chr1
      
   LET lc_find = g_gzoy_d[g_detail_idx].gzoy003    #調整前語詞
   
   #參考的部分不一定會有值
   LET lc_match = g_gzoy_d[g_detail_idx].gzoy006   #參考語詞
   
   #先找出指定詞在指定語系內的標籤
   LET ls_sql = "SELECT gzoz000,gzoz501,",azzi100_get_gzoz_fieldid(g_gzoy_d[g_detail_idx].gzoy002) CLIPPED
   
   #如果有參考語詞的話
   IF NOT cl_null(g_gzoy_d[g_detail_idx].gzoy005) AND NOT cl_null(lc_match) THEN
      LET ls_sql = ls_sql,",",azzi100_get_gzoz_fieldid(g_gzoy_d[g_detail_idx].gzoy005) CLIPPED
   ELSE
      LET ls_sql = ls_sql,",''"
   END IF
   
   #補其他的SQL
   LET ls_sql = ls_sql," FROM gzoz_t WHERE ",azzi100_get_gzoz_fieldid(g_gzoy_d[g_detail_idx].gzoy002) CLIPPED
   
   #是否為整詞的處理
   IF g_gzoy_d[g_detail_idx].gzoy002 = "Y" THEN  #整詞
      LET ls_sql = ls_sql, " LIKE '%",lc_find CLIPPED,"%' "   #待查字詞
   ELSE
      LET ls_sql = ls_sql, " LIKE '%",lc_find CLIPPED,"%' "   #待查字詞
   END IF 
   
   DECLARE azzp140_gzoz_cs CURSOR FROM ls_sql
   FOREACH azzp140_gzoz_cs INTO lc_gzoz000,lc_gzoz501,lc_gzoz000_old,lc_gzoz000_ref  #找出舊詞
   
      #如果有填參考的話，做個檢驗
      IF NOT cl_null(g_gzoy_d[g_detail_idx].gzoy005) AND NOT cl_null(lc_match) THEN
         IF g_gzoy_d[g_detail_idx].gzoy002 = "Y" THEN  #整詞
            IF lc_gzoz000_ref = lc_match THEN
               CONTINUE FOREACH
            END IF
         ELSE  #部分詞
            LET ls_temp = lc_gzoz000_ref
            IF NOT ls_temp.getIndexOf(lc_match,1) THEN
               CONTINUE FOREACH
            END IF
         END IF
      END IF
      
      IF lc_type = "u" THEN
         LET lc_gzoz000_new = cl_replace_str(lc_gzoz000_old,
                                             g_gzoy_d[g_detail_idx].gzoy003 CLIPPED,
                                             g_gzoy_d[g_detail_idx].gzoy008 CLIPPED)
                              
         DISPLAY "UPDATE gzoz_t SET ",azzi100_get_gzoz_fieldid(g_gzoy_d[g_detail_idx].gzoy002) CLIPPED,
                         "='",lc_gzoz000_new CLIPPED,"'",
                 " WHERE gzoz000='",lc_gzoz000 CLIPPED,"' ",
                 "   AND gzoz501='",lc_gzoz501 CLIPPED,"' ",
                 "   AND ",azzi100_get_gzoz_fieldid(g_gzoy_d[g_detail_idx].gzoy002) CLIPPED,
                         "='",lc_gzoz000_old CLIPPED,"';"
      ELSE
         #秀出查到的資料
         DISPLAY lc_gzoz000,"\t",lc_gzoz501,"\t",lc_gzoz000_old,"\t",lc_gzoz000_ref
         
      END IF
   END FOREACH
    
   FREE azzp140_gzoz_cs
END FUNCTION

