   DEFINE lc_gzszl006_ref  LIKE gzszl_t.gzszl006  #指定新語詞
   DEFINE lc_gzszl007_ref  LIKE gzszl_t.gzszl007  #指定新語詞
   DEFINE lc_find          LIKE gzszl_t.gzszl004  #
   DEFINE lc_match         LIKE gzszl_t.gzszl004  #
   DEFINE ls_sql           STRING
   DEFINE ls_temp          STRING
   DEFINE lc_type          LIKE type_t.chr1
   DEFINE lc_gzszl004_upd  LIKE type_t.chr1
   DEFINE lc_gzszl005_upd  LIKE type_t.chr1
   DEFINE lc_gzszl006_upd  LIKE type_t.chr1
   DEFINE lc_gzszl007_upd  LIKE type_t.chr1
   DEFINE lc_ch            base.Channel
   DEFINE lc_begin         STRING
   DEFINE lc_gzszl_file    STRING
   DEFINE lc_str           STRING


   LET lc_gzszl003_old = g_gzoy_d[g_detail_idx].gzoy002   #調整語系
   LET lc_find         = g_gzoy_d[g_detail_idx].gzoy003   #調整前語詞

   #參考的部分不一定會有值
   LET lc_gzszl003_ref = g_gzoy_d[g_detail_idx].gzoy005   #參考語系
   LET lc_match        = g_gzoy_d[g_detail_idx].gzoy006   #參考語詞

   #先找出指定詞在指定語系內的標籤
   LET ls_sql = "SELECT gzszl001,gzszl002,gzszl004,gzszl005,gzszl006,gzszl007 FROM gzszl_t ",
                " WHERE gzszl003 = '",lc_gzszl003_old CLIPPED,"' "   #原有語言別

   #是否為整詞的處理
   IF g_gzoy_d[g_detail_idx].gzoy004 = "Y" THEN  #整詞
      LET ls_sql = ls_sql, " AND ((gzszl004 = '",lc_find CLIPPED,"') ",   #待查字詞
                                " OR (gzszl005 = '",lc_find CLIPPED,"') ",
                                " OR (gzszl006 = '",lc_find CLIPPED,"') ",
                                " OR (gzszl007 = '",lc_find CLIPPED,"')) "
   ELSE
      LET ls_sql = ls_sql, " AND ((gzszl004 LIKE '%",lc_find CLIPPED,"%') ",   #待查字詞
                                 " OR (gzszl005 LIKE '%",lc_find CLIPPED,"%') ",
                                 " OR (gzszl006 LIKE '%",lc_find CLIPPED,"%') ",
                                 " OR (gzszl007 LIKE '%",lc_find CLIPPED,"%'))"
   END IF

   #準備寫入檔案
   LET lc_begin = cl_get_today()
   LET lc_ch = base.Channel.create()
   LET lc_gzszl_file = "azzi100_gzszl_",lc_begin CLIPPED ,".txt"
   LET lc_gzszl_file = os.Path.join(FGL_GETENV("TEMPDIR"),lc_gzszl_file)
   IF os.Path.delete(lc_gzszl_file) THEN   #刪除舊檔
   END IF

   DECLARE azzi100_gzszl_cs CURSOR FROM ls_sql
   FOREACH azzi100_gzszl_cs INTO lc_gzszl001,lc_gzszl002,lc_gzszl004_old,lc_gzszl005_old,lc_gzszl006_old,lc_gzszl007_old  #找出舊詞
      LET lc_gzszl004_upd = "N"
      LET lc_gzszl005_upd = "N"
      LET lc_gzszl006_upd = "N"
      LET lc_gzszl007_upd = "N"

      #如果有填參考的話，做個檢驗
      IF NOT cl_null(lc_gzszl003_ref) AND NOT cl_null(lc_match) THEN
         SELECT gzszl004,gzszl005,gzszl006,gzszl007 INTO lc_gzszl004_ref,lc_gzszl005_ref,lc_gzszl006_ref,lc_gzszl007_ref
           FROM gzszl_t
          WHERE gzszl001 = lc_gzszl001 AND gzszl002 = lc_gzszl002
            AND gzszl003 = lc_gzszl003_ref
         IF lc_match IS NOT NULL THEN
            IF g_gzoy_d[g_detail_idx].gzoy004 = "Y" THEN  #整詞
               IF lc_gzszl004_ref = lc_match THEN   #說明
                  LET lc_gzszl004_upd = "Y"
               END IF

               IF lc_gzszl005_ref = lc_match THEN   #使用說明
                  LET lc_gzszl005_upd = "Y"
               END IF

               IF lc_gzszl006_ref = lc_match THEN   #應用建議
                  LET lc_gzszl006_upd = "Y"
               END IF

               IF lc_gzszl007_ref = lc_match THEN   #個案應用說明
                  LET lc_gzszl007_upd = "Y"
               END IF
            ELSE  #部分詞
               LET ls_temp = lc_gzszl004_ref   #說明
               IF ls_temp.getIndexOf(lc_match,1) THEN
                  LET lc_gzszl004_upd = "Y"
               END IF

               LET ls_temp = lc_gzszl005_ref   #使用說明
               IF ls_temp.getIndexOf(lc_match,1) THEN
                  LET lc_gzszl005_upd = "Y"
               END IF

               LET ls_temp = lc_gzszl006_ref   #應用建議
               IF ls_temp.getIndexOf(lc_match,1) THEN
                  LET lc_gzszl006_upd = "Y"
               END IF

               LET ls_temp = lc_gzszl007_ref   #個案應用說明
               IF ls_temp.getIndexOf(lc_match,1) THEN
                  LET lc_gzszl007_upd = "Y"
               END IF
            END IF
         END IF
      END IF

      IF lc_type = "u" THEN
         #組出字詞更新語法
         IF lc_gzszl004_upd = "Y" THEN   #說明
            LET lc_gzszl004_new = cl_replace_str(lc_gzszl004_old,
                                                 g_gzoy_d[g_detail_idx].gzoy003 CLIPPED,
                                                 g_gzoy_d[g_detail_idx].gzoy008 CLIPPED)

            LET lc_str = "UPDATE gzszl_t SET gzszl004='",lc_gzszl004_new CLIPPED,"' ",
                          "WHERE gzszl001='",lc_gzszl001 CLIPPED,"' ",
                            "AND gzszl002='",lc_gzszl002 CLIPPED,"' ",
                            "AND gzszl003='",lc_gzszl003_old CLIPPED,"' ",
                            "AND gzszl004='",lc_gzszl004_old CLIPPED,"' "

            DISPLAY "UPDATE gzszl_t SET gzszl004='",lc_gzszl004_new CLIPPED,"' ",
                     "WHERE gzszl001='",lc_gzszl001 CLIPPED,"' ",
                       "AND gzszl002='",lc_gzszl002 CLIPPED,"' ",
                       "AND gzszl003='",lc_gzszl003_old CLIPPED,"' ",
                       "AND gzszl004='",lc_gzszl004_old CLIPPED,"' "

            IF NOT os.Path.exists(lc_gzszl_file) THEN
               CALL lc_ch.openFile(lc_gzszl_file, "w")
            ELSE
               CALL lc_ch.openFile(lc_gzszl_file, "a")
            END IF
            CALL lc_ch.write(lc_str)
         END IF

         IF lc_gzszl005_upd = "Y" THEN   #使用說明
            LET lc_gzszl005_new = cl_replace_str(lc_gzszl005_old,
                                                 g_gzoy_d[g_detail_idx].gzoy003 CLIPPED,
                                                 g_gzoy_d[g_detail_idx].gzoy008 CLIPPED)

            LET lc_str = "UPDATE gzszl_t SET gzszl005='",lc_gzszl005_new CLIPPED,"' ",
                          "WHERE gzszl001='",lc_gzszl001 CLIPPED,"' ",
                            "AND gzszl002='",lc_gzszl002 CLIPPED,"' ",
                            "AND gzszl003='",lc_gzszl003_old CLIPPED,"' ",
                            "AND gzszl005='",lc_gzszl005_old CLIPPED,"' "

            DISPLAY "UPDATE gzszl_t SET gzszl005='",lc_gzszl005_new CLIPPED,"' ",
                     "WHERE gzszl001='",lc_gzszl001 CLIPPED,"' ",
                       "AND gzszl002='",lc_gzszl002 CLIPPED,"' ",
                       "AND gzszl003='",lc_gzszl003_old CLIPPED,"' ",
                       "AND gzszl005='",lc_gzszl005_old CLIPPED,"' "

            IF NOT os.Path.exists(lc_gzszl_file) THEN
               CALL lc_ch.openFile(lc_gzszl_file, "w")
            ELSE
               CALL lc_ch.openFile(lc_gzszl_file, "a")
            END IF
            CALL lc_ch.write(lc_str)
         END IF

         IF lc_gzszl006_upd = "Y" THEN   #應用建議
            LET lc_gzszl006_new = cl_replace_str(lc_gzszl006_old,
                                                 g_gzoy_d[g_detail_idx].gzoy003 CLIPPED,
                                                 g_gzoy_d[g_detail_idx].gzoy008 CLIPPED)

            LET lc_str = "UPDATE gzszl_t SET gzszl006='",lc_gzszl006_new CLIPPED,"' ",
                          "WHERE gzszl001='",lc_gzszl001 CLIPPED,"' ",
                            "AND gzszl002='",lc_gzszl002 CLIPPED,"' ",
                            "AND gzszl003='",lc_gzszl003_old CLIPPED,"' ",
                            "AND gzszl006='",lc_gzszl006_old CLIPPED,"' "

            DISPLAY "UPDATE gzszl_t SET gzszl006='",lc_gzszl006_new CLIPPED,"' ",
                     "WHERE gzszl001='",lc_gzszl001 CLIPPED,"' ",
                       "AND gzszl002='",lc_gzszl002 CLIPPED,"' ",
                       "AND gzszl003='",lc_gzszl003_old CLIPPED,"' ",
                       "AND gzszl006='",lc_gzszl006_old CLIPPED,"' "

            IF NOT os.Path.exists(lc_gzszl_file) THEN
               CALL lc_ch.openFile(lc_gzszl_file, "w")
            ELSE
               CALL lc_ch.openFile(lc_gzszl_file, "a")
            END IF
            CALL lc_ch.write(lc_str)
         END IF

         IF lc_gzszl007_upd = "Y" THEN   #個案應用說明
            LET lc_gzszl007_new = cl_replace_str(lc_gzszl007_old,
                                                 g_gzoy_d[g_detail_idx].gzoy003 CLIPPED,
                                                 g_gzoy_d[g_detail_idx].gzoy008 CLIPPED)

            LET lc_str = "UPDATE gzszl_t SET gzszl007='",lc_gzszl007_new CLIPPED,"' ",
                          "WHERE gzszl001='",lc_gzszl001 CLIPPED,"' ",
                            "AND gzszl002='",lc_gzszl002 CLIPPED,"' ",
                            "AND gzszl003='",lc_gzszl003_old CLIPPED,"' ",
                            "AND gzszl007='",lc_gzszl007_old CLIPPED,"' "

            DISPLAY "UPDATE gzszl_t SET gzszl007='",lc_gzszl007_new CLIPPED,"' ",
                     "WHERE gzszl001='",lc_gzszl001 CLIPPED,"' ",
                       "AND gzszl002='",lc_gzszl002 CLIPPED,"' ",
                       "AND gzszl003='",lc_gzszl003_old CLIPPED,"' ",
                       "AND gzszl007='",lc_gzszl007_old CLIPPED,"' "

            IF NOT os.Path.exists(lc_gzszl_file) THEN
               CALL lc_ch.openFile(lc_gzszl_file, "w")
            ELSE
               CALL lc_ch.openFile(lc_gzszl_file, "a")
            END IF
            CALL lc_ch.write(lc_str)
         END IF
      ELSE
         #秀出查到的資料
         IF lc_gzszl004_upd = "Y" THEN   #說明
            LET lc_str = lc_gzszl001,"\t",lc_gzszl002,"\t",
                         lc_gzszl003_old,"\t",lc_gzszl004_old,"\t",lc_gzszl003_ref,"\t",lc_gzszl004_ref
            DISPLAY lc_gzszl001,"\t",lc_gzszl002,"\t",
                    lc_gzszl003_old,"\t",lc_gzszl004_old,"\t",lc_gzszl003_ref,"\t",lc_gzszl004_ref
            IF NOT os.Path.exists(lc_gzszl_file) THEN
               CALL lc_ch.openFile(lc_gzszl_file, "w")
            ELSE
               CALL lc_ch.openFile(lc_gzszl_file, "a")
            END IF
            CALL lc_ch.write(lc_str)
         END IF

         IF lc_gzszl005_upd = "Y" THEN   #使用說明
            LET lc_str = lc_gzszl001,"\t",lc_gzszl002,"\t",
                         lc_gzszl003_old,"\t",lc_gzszl005_old,"\t",lc_gzszl003_ref,"\t",lc_gzszl005_ref
            DISPLAY lc_gzszl001,"\t",lc_gzszl002,"\t",
                    lc_gzszl003_old,"\t",lc_gzszl005_old,"\t",lc_gzszl003_ref,"\t",lc_gzszl005_ref
            IF NOT os.Path.exists(lc_gzszl_file) THEN
               CALL lc_ch.openFile(lc_gzszl_file, "w")
            ELSE
               CALL lc_ch.openFile(lc_gzszl_file, "a")
            END IF
            CALL lc_ch.write(lc_str)
         END IF

         IF lc_gzszl006_upd = "Y" THEN   #應用建議
            LET lc_str = lc_gzszl001,"\t",lc_gzszl002,"\t",
                         lc_gzszl003_old,"\t",lc_gzszl006_old,"\t",lc_gzszl003_ref,"\t",lc_gzszl006_ref
            DISPLAY lc_gzszl001,"\t",lc_gzszl002,"\t",
                    lc_gzszl003_old,"\t",lc_gzszl006_old,"\t",lc_gzszl003_ref,"\t",lc_gzszl006_ref
            IF NOT os.Path.exists(lc_gzszl_file) THEN
               CALL lc_ch.openFile(lc_gzszl_file, "w")
            ELSE
               CALL lc_ch.openFile(lc_gzszl_file, "a")
            END IF
            CALL lc_ch.write(lc_str)
         END IF

         IF lc_gzszl007_upd = "Y" THEN   #個案應用說明
            LET lc_str = lc_gzszl001,"\t",lc_gzszl002,"\t",
                         lc_gzszl003_old,"\t",lc_gzszl007_old,"\t",lc_gzszl003_ref,"\t",lc_gzszl007_ref
            DISPLAY lc_gzszl001,"\t",lc_gzszl002,"\t",
                    lc_gzszl003_old,"\t",lc_gzszl007_old,"\t",lc_gzszl003_ref,"\t",lc_gzszl007_ref
            IF NOT os.Path.exists(lc_gzszl_file) THEN
               CALL lc_ch.openFile(lc_gzszl_file, "w")
            ELSE
               CALL lc_ch.openFile(lc_gzszl_file, "a")
            END IF
            CALL lc_ch.write(lc_str)
         END IF
      END IF
   END FOREACH
   CALL lc_ch.close()

   #後續顯示檔案位置用
   IF lc_gzszl004_upd = "Y" OR lc_gzszl005_upd = "Y" OR lc_gzszl006_upd = "Y" OR lc_gzszl007_upd = "Y" THEN
      IF cl_null(g_show_file) THEN
         LET g_show_file = lc_gzszl_file
      ELSE
         LET g_show_file = g_show_file,"\n",lc_gzszl_file
      END IF    
   END IF
END FUNCTION

################################################################################
# Descriptions...: 調整編號
# Memo...........:
# Usage..........: CALL azzi100_gzoy001_get_sn()
#                  RETURNING 回传参数
# Input parameter: 传入参数变量1   传入参数变量说明1
# Return code....: 回传参数变量1   回传参数变量说明1
# Date & Author..: 2015/01/06 By jrg542
# Modify.........:
################################################################################
PRIVATE FUNCTION azzi100_gzoy001_get_sn()
   DEFINE li_no LIKE type_t.num5

   SELECT MAX(gzoy001) + 1 INTO li_no FROM gzoy_t

   LET g_gzoy_d[l_ac].gzoy001 = NVL(li_no,1) 
END FUNCTION

 
