################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL azzi100_chk_dzebl(lc_type)
# Input parameter: lc_type        型態  u:update  s:查詢
# Date & Author..: 日期 By 作者
# Modify.........: 
################################################################################
PRIVATE FUNCTION azzi100_chk_dzebl(lc_type)
   DEFINE lc_dzebl001      LIKE dzebl_t.dzebl001
   DEFINE lc_dzebl002_old  LIKE dzebl_t.dzebl002  #指定原始語言別
   DEFINE lc_dzebl003_old  LIKE dzebl_t.dzebl003  #
   DEFINE lc_dzebl004_old  LIKE dzebl_t.dzebl004  #
   DEFINE lc_dzebl003_new  LIKE dzebl_t.dzebl003  #
   DEFINE lc_dzebl004_new  LIKE dzebl_t.dzebl004  #
   DEFINE lc_dzebl002_ref  LIKE dzebl_t.dzebl002  #指定參考語言別
   DEFINE lc_dzebl003_ref  LIKE dzebl_t.dzebl003  #指定新語詞
   DEFINE lc_dzebl004_ref  LIKE dzebl_t.dzebl004  #指定新語詞
   DEFINE lc_find          LIKE dzebl_t.dzebl003  #
   DEFINE lc_match         LIKE dzebl_t.dzebl003  #
   DEFINE ls_sql           STRING
   DEFINE ls_temp          STRING
   DEFINE lc_type          LIKE type_t.chr1
   DEFINE lc_dzebl003_upd  LIKE type_t.chr1
   DEFINE lc_dzebl004_upd  LIKE type_t.chr1
   DEFINE lc_ch            base.Channel
   DEFINE lc_begin         STRING
   DEFINE lc_dzebl_file    STRING
   DEFINE lc_str           STRING

   LET lc_dzebl002_old = g_gzoy_d[g_detail_idx].gzoy002   #調整語系
   LET lc_find         = g_gzoy_d[g_detail_idx].gzoy003    #調整前語詞

   #參考的部分不一定會有值
   LET lc_dzebl002_ref = g_gzoy_d[g_detail_idx].gzoy005   #參考語系
   LET lc_match        = g_gzoy_d[g_detail_idx].gzoy006    #參考語詞

   #先找出指定詞在指定語系內的標籤
   LET ls_sql = "SELECT dzebl001,dzebl003,dzebl004 FROM dzebl_t ",
                " WHERE dzebl002 = '",lc_dzebl002_old CLIPPED,"' "   #原有語言別

   #是否為整詞的處理
   IF g_gzoy_d[g_detail_idx].gzoy004 = "Y" THEN  #整詞
   #  LET ls_sql = ls_sql, " AND dzebl003 = '",lc_find CLIPPED,"' "   #待查字詞
      LET ls_sql = ls_sql, " AND ((dzebl003 = '",lc_find CLIPPED,"') ",   #待查字詞
                                " OR (dzebl004 = '",lc_find CLIPPED,"')) "
   ELSE
   #  LET ls_sql = ls_sql, " AND dzebl003 LIKE '%",lc_find CLIPPED,"%' "   #待查字詞
      LET ls_sql = ls_sql, " AND ((dzebl003 LIKE '%",lc_find CLIPPED,"%') ",   #待查字詞
                                 " OR (dzebl004 LIKE '%",lc_find CLIPPED,"%'))"
   END IF

   #準備寫入檔案
   LET lc_begin = cl_get_today()
   LET lc_ch = base.Channel.create()
   LET lc_dzebl_file = "azzi100_dzebl_",lc_begin CLIPPED ,".txt"
   LET lc_dzebl_file = os.Path.join(FGL_GETENV("TEMPDIR"),lc_dzebl_file)
   IF os.Path.delete(lc_dzebl_file) THEN   #刪除舊檔
   END IF

   DECLARE azzi100_dzebl_cs CURSOR FROM ls_sql
   FOREACH azzi100_dzebl_cs INTO lc_dzebl001,lc_dzebl003_old,lc_dzebl004_old
      LET lc_dzebl003_upd = "N"
      LET lc_dzebl004_upd = "N"

      #如果有填參考的話，做個檢驗
      IF NOT cl_null(lc_dzebl002_ref) AND NOT cl_null(lc_match) THEN
         SELECT dzebl003,dzebl004 INTO lc_dzebl003_ref,lc_dzebl004_ref
           FROM dzebl_t
          WHERE dzebl001 = lc_dzebl001 AND dzebl002 = lc_dzebl002_ref
         IF lc_match IS NOT NULL THEN
            IF g_gzoy_d[g_detail_idx].gzoy004 = "Y" THEN  #整詞
               IF lc_dzebl003_ref = lc_match THEN   #欄位名稱
                  LET lc_dzebl003_upd = "Y"
               END IF

               IF lc_dzebl004_ref = lc_match THEN   #欄位說明
                  LET lc_dzebl004_upd = "Y"
               END IF
            ELSE  #部分詞
               LET ls_temp = lc_dzebl003_ref   #欄位名稱
               IF ls_temp.getIndexOf(lc_match,1) THEN
                  LET lc_dzebl003_upd = "Y"
               END IF

               LET ls_temp = lc_dzebl004_ref   #欄位說明
               IF ls_temp.getIndexOf(lc_match,1) THEN
                  LET lc_dzebl004_upd = "Y"
               END IF
            END IF
         END IF
      END IF

      IF lc_type = "u" THEN
         IF lc_dzebl003_upd = "Y" THEN   #欄位名稱
            LET lc_dzebl003_new = cl_replace_str(lc_dzebl003_old,
                                                 g_gzoy_d[g_detail_idx].gzoy003 CLIPPED,
                                                 g_gzoy_d[g_detail_idx].gzoy008 CLIPPED)

            LET lc_str = "UPDATE dzebl_t SET dzebl003='",lc_dzebl003_new CLIPPED,"' ",
                             "WHERE dzebl001='",lc_dzebl001 CLIPPED,"' ",
                               "AND dzebl002='",lc_dzebl002_old CLIPPED,"' ",
                               "AND dzebl003='",lc_dzebl003_old CLIPPED,"' "

            DISPLAY "UPDATE dzebl_t SET dzebl003='",lc_dzebl003_new CLIPPED,"' ",
                     "WHERE dzebl001='",lc_dzebl001 CLIPPED,"' ",
                       "AND dzebl002='",lc_dzebl002_old CLIPPED,"' ",
                       "AND dzebl003='",lc_dzebl003_old CLIPPED,"' "

            IF NOT os.Path.exists(lc_dzebl_file) THEN
               CALL lc_ch.openFile(lc_dzebl_file, "w")
            ELSE
               CALL lc_ch.openFile(lc_dzebl_file, "a")
            END IF
            CALL lc_ch.write(lc_str)
         END IF

         IF lc_dzebl004_upd = "Y" THEN   #欄位說明
            LET lc_dzebl004_new = cl_replace_str(lc_dzebl004_old,
                                                 g_gzoy_d[g_detail_idx].gzoy003 CLIPPED,
                                                 g_gzoy_d[g_detail_idx].gzoy008 CLIPPED)

            LET lc_str = "UPDATE dzebl_t SET dzebl004='",lc_dzebl004_new CLIPPED,"' ",
                             "WHERE dzebl001='",lc_dzebl001 CLIPPED,"' ",
                               "AND dzebl002='",lc_dzebl002_old CLIPPED,"' ",
                               "AND dzebl004='",lc_dzebl004_old CLIPPED,"' "

            DISPLAY "UPDATE dzebl_t SET dzebl004='",lc_dzebl004_new CLIPPED,"' ",
                     "WHERE dzebl001='",lc_dzebl001 CLIPPED,"' ",
                       "AND dzebl002='",lc_dzebl002_old CLIPPED,"' ",
                       "AND dzebl004='",lc_dzebl004_old CLIPPED,"' "

            IF NOT os.Path.exists(lc_dzebl_file) THEN
               CALL lc_ch.openFile(lc_dzebl_file, "w")
            ELSE
               CALL lc_ch.openFile(lc_dzebl_file, "a")
            END IF
            CALL lc_ch.write(lc_str)
         END IF
      ELSE
         #秀出查到的資料
         IF lc_dzebl003_upd = "Y" THEN   #欄位名稱
            LET lc_str = lc_dzebl001,"\t",lc_dzebl002_old,"\t",lc_dzebl003_old,"\t",lc_dzebl002_ref,"\t",lc_dzebl003_ref
            DISPLAY lc_dzebl001,"\t",lc_dzebl002_old,"\t",lc_dzebl003_old,"\t",lc_dzebl002_ref,"\t",lc_dzebl003_ref
            IF NOT os.Path.exists(lc_dzebl_file) THEN
               CALL lc_ch.openFile(lc_dzebl_file, "w")
            ELSE
               CALL lc_ch.openFile(lc_dzebl_file, "a")
            END IF
            CALL lc_ch.write(lc_str)
         END IF

         IF lc_dzebl004_upd = "Y" THEN   #欄位說明
            LET lc_str = lc_dzebl001,"\t",lc_dzebl002_old,"\t",lc_dzebl004_old,"\t",lc_dzebl002_ref,"\t",lc_dzebl004_ref
            DISPLAY lc_dzebl001,"\t",lc_dzebl002_old,"\t",lc_dzebl004_old,"\t",lc_dzebl002_ref,"\t",lc_dzebl004_ref
            IF NOT os.Path.exists(lc_dzebl_file) THEN
               CALL lc_ch.openFile(lc_dzebl_file, "w")
            ELSE
               CALL lc_ch.openFile(lc_dzebl_file, "a")
            END IF
            CALL lc_ch.write(lc_str)
         END IF
      END IF
   END FOREACH
   CALL lc_ch.close()

   #後續顯示檔案位置用
   IF lc_dzebl003_upd = "Y" OR lc_dzebl004_upd = "Y" THEN
      IF cl_null(g_show_file) THEN
         LET g_show_file = lc_dzebl_file
      ELSE
         LET g_show_file = g_show_file,"\n",lc_dzebl_file
      END IF     
   END IF
END FUNCTION

################################################################################
# Descriptions...: 描述说明
# Memo...........:
# Usage..........: CALL azzi100_chk_gzzd (lc_type)
# Input parameter: lc_type        型態  u:update  s:查詢
