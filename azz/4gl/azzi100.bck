                     LET g_errparam.extend = "gzoy_t:",SQLERRMESSAGE 
                     LET g_errparam.code   = SQLCA.sqlcode 
                     LET g_errparam.popup  = TRUE 
                     CALL s_transaction_end('N','0')
                     CALL cl_err()
                  OTHERWISE
                                    INITIALIZE gs_keys TO NULL 
               LET gs_keys[1] = g_gzoy_d[g_detail_idx].gzoy001
               LET gs_keys_bak[1] = g_gzoy_d_t.gzoy001
               CALL azzi100_update_b('gzoy_t',gs_keys,gs_keys_bak,"'1'")
                     #資料多語言用-增/改
                     
                     #修改歷程記錄(修改)
                     LET g_log1 = util.JSON.stringify(g_gzoy_d_t)
                     LET g_log2 = util.JSON.stringify(g_gzoy_d[l_ac])
                     IF NOT cl_log_modified_record(g_log1,g_log2) THEN 
                        CALL s_transaction_end('N','0')
                     END IF
               END CASE
               
               #將遮罩欄位進行遮蔽
               CALL azzi100_gzoy_t_mask_restore('restore_mask_n')
               
               #add-point:單身修改後 name="input.body.a_update"
               
               #end add-point
 
            END IF
            
         AFTER ROW
            CALL azzi100_unlock_b("gzoy_t")
            IF INT_FLAG THEN
               CALL s_transaction_end('N','0')
               INITIALIZE g_errparam TO NULL 
               LET g_errparam.extend = '' 
               LET g_errparam.code   = 9001 
               LET g_errparam.popup  = FALSE 
               CALL cl_err()
               LET INT_FLAG = 0
               IF l_cmd = 'u' THEN
                  LET g_gzoy_d[l_ac].* = g_gzoy_d_t.*
               END IF
               #add-point:單身after row name="input.body.a_close"
               
               #end add-point
            ELSE
               CALL s_transaction_end('Y','0')
            END IF
            #其他table進行unlock
            
             #add-point:單身after row name="input.body.a_row"
            
            #end add-point
            
         AFTER INPUT
            #add-point:單身input後 name="input.body.a_input"
            FOR li_idx = 1 TO g_gzoy_d.getLength()
                LET g_sel[li_idx].sel = g_gzoy_d[li_idx].l_sel
                LET g_sel[li_idx].gzoy001 = g_gzoy_d[li_idx].gzoy001            
            END FOR

            #end add-point
            #錯誤訊息統整顯示
            #CALL cl_err_collect_show()
            #CALL cl_showmsg()
            
         ON ACTION controlo   
            IF l_insert THEN
               LET li_reproduce = l_ac_t
               LET li_reproduce_target = l_ac
               LET g_gzoy_d[li_reproduce_target].* = g_gzoy_d[li_reproduce].*
               LET g_gzoy2_d[li_reproduce_target].* = g_gzoy2_d[li_reproduce].*
 
               LET g_gzoy_d[li_reproduce_target].gzoy001 = NULL
 
            ELSE
               CALL FGL_SET_ARR_CURR(g_gzoy_d.getLength()+1)
               LET lb_reproduce = TRUE
               LET li_reproduce = l_ac
               LET li_reproduce_target = g_gzoy_d.getLength()+1
            END IF
            
      END INPUT
      
 
      
      DISPLAY ARRAY g_gzoy2_d TO s_detail2.*
         ATTRIBUTES(COUNT=g_detail_cnt)  
      
         BEFORE DISPLAY 
            CALL azzi100_b_fill(g_wc2)
            CALL FGL_SET_ARR_CURR(g_detail_idx)
      
         BEFORE ROW
            LET g_detail_idx = DIALOG.getCurrentRow("s_detail2")
