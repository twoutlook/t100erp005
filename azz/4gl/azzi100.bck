{<section id="azzi100.b_fill" >}
#+ 單身陣列填充
PRIVATE FUNCTION azzi100_b_fill(p_wc2)              #BODY FILL UP
   #add-point:b_fill段define(客製用) name="b_fill.define_customerization"
   
   #end add-point
   DEFINE p_wc2    STRING
   #add-point:b_fill段define(請盡量不要在客製環境修改此段落內容, 否則將後續patch的調整需人工處理) name="b_fill.define"
   
   #end add-point
   
   #add-point:Function前置處理  name="b_fill.pre_function"
   
   #end add-point
   
   IF cl_null(p_wc2) THEN
      LET p_wc2 = " 1=1"
   END IF
   
   #add-point:b_fill段sql之前 name="b_fill.sql_before"
   
   #end add-point
 
   LET g_sql = "SELECT  DISTINCT t0.gzoystus,t0.gzoy001,t0.gzoy002,t0.gzoy003,t0.gzoy008,t0.gzoy004, 
       t0.gzoy007,t0.gzoy005,t0.gzoy006,t0.gzoy009,t0.gzoy001,t0.gzoyownid,t0.gzoyowndp,t0.gzoycrtid, 
       t0.gzoycrtdp,t0.gzoycrtdt,t0.gzoymodid,t0.gzoymoddt ,t1.ooag011 ,t2.ooefl003 ,t3.ooag011 ,t4.ooefl003 , 
       t5.ooag011 FROM gzoy_t t0",
               "",
                              " LEFT JOIN ooag_t t1 ON t1.ooagent="||g_enterprise||" AND t1.ooag001=t0.gzoyownid  ",
               " LEFT JOIN ooefl_t t2 ON t2.ooeflent="||g_enterprise||" AND t2.ooefl001=t0.gzoyowndp AND t2.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t3 ON t3.ooagent="||g_enterprise||" AND t3.ooag001=t0.gzoycrtid  ",
               " LEFT JOIN ooefl_t t4 ON t4.ooeflent="||g_enterprise||" AND t4.ooefl001=t0.gzoycrtdp AND t4.ooefl002='"||g_dlang||"' ",
               " LEFT JOIN ooag_t t5 ON t5.ooagent="||g_enterprise||" AND t5.ooag001=t0.gzoymodid  ",
 
               " WHERE 1=1 AND (", p_wc2, ") " 
   #add-point:b_fill段sql wc name="b_fill.sql_wc"
   
   #end add-point
   LET g_sql = g_sql, cl_sql_add_filter("gzoy_t"),
                      " ORDER BY t0.gzoy001"
   
   #add-point:b_fill段sql之後 name="b_fill.sql_after"
   
   #end add-point
   
   #LET g_sql = cl_sql_add_tabid(g_sql,"gzoy_t")            #WC重組
   LET g_sql = cl_sql_add_mask(g_sql)              #遮蔽特定資料
   PREPARE azzi100_pb FROM g_sql
   DECLARE b_fill_curs CURSOR FOR azzi100_pb
   
   OPEN b_fill_curs
 
   CALL g_gzoy_d.clear()
   CALL g_gzoy2_d.clear()   
 
 
   LET g_cnt = l_ac
   LET l_ac = 1   
   ERROR "Searching!" 
 
   FOREACH b_fill_curs INTO g_gzoy_d[l_ac].gzoystus,g_gzoy_d[l_ac].gzoy001,g_gzoy_d[l_ac].gzoy002,g_gzoy_d[l_ac].gzoy003, 
       g_gzoy_d[l_ac].gzoy008,g_gzoy_d[l_ac].gzoy004,g_gzoy_d[l_ac].gzoy007,g_gzoy_d[l_ac].gzoy005,g_gzoy_d[l_ac].gzoy006, 
       g_gzoy_d[l_ac].gzoy009,g_gzoy2_d[l_ac].gzoy001,g_gzoy2_d[l_ac].gzoyownid,g_gzoy2_d[l_ac].gzoyowndp, 
       g_gzoy2_d[l_ac].gzoycrtid,g_gzoy2_d[l_ac].gzoycrtdp,g_gzoy2_d[l_ac].gzoycrtdt,g_gzoy2_d[l_ac].gzoymodid, 
       g_gzoy2_d[l_ac].gzoymoddt,g_gzoy2_d[l_ac].gzoyownid_desc,g_gzoy2_d[l_ac].gzoyowndp_desc,g_gzoy2_d[l_ac].gzoycrtid_desc, 
       g_gzoy2_d[l_ac].gzoycrtdp_desc,g_gzoy2_d[l_ac].gzoymodid_desc
      IF SQLCA.sqlcode THEN
         INITIALIZE g_errparam TO NULL 
         LET g_errparam.extend = "FOREACH:",SQLERRMESSAGE 
         LET g_errparam.code   = SQLCA.sqlcode 
         LET g_errparam.popup  = TRUE 
         CALL cl_err()
         EXIT FOREACH
      END IF
  
      #add-point:b_fill段資料填充 name="b_fill.fill"
      #160302-00017#3 add(S)
      IF cl_null(g_sel[l_ac].sel) THEN
         LET g_gzoy_d[l_ac].l_sel = 'N'
      ELSE
         IF g_sel[l_ac].gzoy001 = g_gzoy_d[l_ac].gzoy001 THEN #調整編號
            LET g_gzoy_d[l_ac].l_sel = g_sel[l_ac].sel
         END IF         
      END IF
      #160302-00017#3 add(E)
      #end add-point
      
      CALL azzi100_detail_show()      
 
      IF l_ac > g_max_rec THEN
         IF g_error_show = 1 THEN
            INITIALIZE g_errparam TO NULL 
            LET g_errparam.extend = l_ac
            LET g_errparam.code   = 9035 
            LET g_errparam.popup  = TRUE 
            CALL cl_err()
         END IF
         EXIT FOREACH
      END IF
 
      LET l_ac = l_ac + 1
      
   END FOREACH
 
   LET g_error_show = 0
   
 
   
   CALL g_gzoy_d.deleteElement(g_gzoy_d.getLength())   
   CALL g_gzoy2_d.deleteElement(g_gzoy2_d.getLength())
 
   
   #將key欄位填到每個page
   FOR l_ac = 1 TO g_gzoy_d.getLength()
      LET g_gzoy2_d[l_ac].gzoy001 = g_gzoy_d[l_ac].gzoy001 
 
      #add-point:b_fill段key值相關欄位 name="b_fill.keys.fill"
      
      #end add-point
   END FOR
   
   IF g_cnt > g_gzoy_d.getLength() THEN
      LET l_ac = g_gzoy_d.getLength()
   ELSE
      LET l_ac = g_cnt
   END IF
   LET g_cnt = l_ac
 
   #遮罩相關處理
   FOR l_ac = 1 TO g_gzoy_d.getLength()
      LET g_gzoy_d_mask_o[l_ac].* =  g_gzoy_d[l_ac].*
      CALL azzi100_gzoy_t_mask()
      LET g_gzoy_d_mask_n[l_ac].* =  g_gzoy_d[l_ac].*
   END FOR
   
   LET g_gzoy2_d_mask_o.* =  g_gzoy2_d.*
   FOR l_ac = 1 TO g_gzoy2_d.getLength()
      LET g_gzoy2_d_mask_o[l_ac].* =  g_gzoy2_d[l_ac].*
      CALL azzi100_gzoy_t_mask()
      LET g_gzoy2_d_mask_n[l_ac].* =  g_gzoy2_d[l_ac].*
   END FOR
 
   
   LET l_ac = g_cnt
 
   #add-point:b_fill段資料填充(其他單身) name="b_fill.others.fill"
 
   #end add-point
   
   ERROR "" 
 
   LET g_detail_cnt = g_gzoy_d.getLength()
   DISPLAY g_detail_idx TO FORMONLY.idx
   DISPLAY g_detail_cnt TO FORMONLY.cnt
   
   CLOSE b_fill_curs
   FREE azzi100_pb
   
END FUNCTION
 
